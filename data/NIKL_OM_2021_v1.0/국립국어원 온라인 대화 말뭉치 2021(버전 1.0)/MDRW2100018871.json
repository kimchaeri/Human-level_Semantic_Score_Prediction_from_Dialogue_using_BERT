      "utterance": [
        {
          "id": "MDRW2100018871.1.1",
          "form": "원래 반려동물이 없었던 거 같은데",
          "original_form": "원래 반려동물이 없었던 거 같은데",
          "speaker_id": "1",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100018871.1.2",
          "form": "새로 생긴 거 같죠",
          "original_form": "새로 생긴 거 같죠",
          "speaker_id": "1",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100018871.1.3",
          "form": "있었다",
          "original_form": "있었다",
          "speaker_id": "2",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100018871.1.4",
          "form": "고양이 얘기 몇 번을 했거든",
          "original_form": "고양이 얘기 몇 번을 했거든",
          "speaker_id": "2",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100018871.1.5",
          "form": "니가 반려동물에 관심이 없어서 그래",
          "original_form": "니가 반려동물에 관심이 없어서 그래",
          "speaker_id": "2",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100018871.1.6",
          "form": "내가 너무 관심이 없었나 봐요",
          "original_form": "내가 너무 관심이 없었나 봐요",
          "speaker_id": "1",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100018871.1.7",
          "form": "반려동물도 키우는 사람이 왜 관심이 없니??",
          "original_form": "반려동물도 키우는 사람이 왜 관심이 없니??",
          "speaker_id": "2",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100018871.1.8",
          "form": "아 맞다 햄 찌 얘기도 한 번 했었네요",
          "original_form": "아 맞다 햄 찌 얘기도 한 번 했었네요",
          "speaker_id": "1",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100018871.1.9",
          "form": "정말 매일 아니고 는다 관심 없네요",
          "original_form": "정말 매일 아니고 는다 관심 없네요",
          "speaker_id": "1",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100018871.1.10",
          "form": "그래 했잖아",
          "original_form": "그래 했잖아",
          "speaker_id": "2",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100018871.1.11",
          "form": "내가 햄스터 이름 뭐냐고 도 물어봤는데",
          "original_form": "내가 햄스터 이름 뭐냐고 도 물어봤는데",
          "speaker_id": "2",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100018871.1.12",
          "form": "정신 안차릴래??",
          "original_form": "정신 안차릴래??",
          "speaker_id": "2",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018871.1.13",
          "form": "근데 반려동물은 키우는 사람만 계속 키우는 듯",
          "original_form": "근데 반려동물은 키우는 사람만 계속 키우는 듯",
          "speaker_id": "1",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018871.1.14",
          "form": "그렇다 채윤이는 앵무새 죽었다던데",
          "original_form": "그렇다 채윤이는 앵무새 죽었다던데",
          "speaker_id": "2",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018871.1.15",
          "form": "아 정말요ㅠ",
          "original_form": "아 정말요ㅠ",
          "speaker_id": "1",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018871.1.16",
          "form": "아 그런 건 너무 슬프다 정말",
          "original_form": "아 그런 건 너무 슬프다 정말",
          "speaker_id": "1",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018871.1.17",
          "form": "우리 투가 name3이한테 언니 앵무새 어디 있어??",
          "original_form": "우리 투가 &name3&이한테 언니 앵무새 어디 있어??",
          "speaker_id": "2",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018871.1.18",
          "form": "라고 물었는데",
          "original_form": "라고 물었는데",
          "speaker_id": "2",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018871.1.19",
          "form": "name4이가 죽었어라고 대답했음",
          "original_form": "&name4&이가 죽었어라고 대답했음",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018871.1.20",
          "form": "원래 하늘로 갔어 구름다리를 건넜어라고 하지 않나요",
          "original_form": "원래 하늘로 갔어 구름다리를 건넜어라고 하지 않나요",
          "speaker_id": "1",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018871.1.21",
          "form": "투놀랬을 듯",
          "original_form": "투놀랬을 듯",
          "speaker_id": "1",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018871.1.22",
          "form": "그래 사랑이 없었던 거지",
          "original_form": "그래 사랑이 없었던 거지",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018871.1.23",
          "form": "그냥 죽었다고",
          "original_form": "그냥 죽었다고",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018871.1.24",
          "form": "그쵸 애들은 잘 모르니",
          "original_form": "그쵸 애들은 잘 모르니",
          "speaker_id": "1",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018871.1.25",
          "form": "아직 투가 죽음을 받아들일 나이는 아니라서",
          "original_form": "아직 투가 죽음을 받아들일 나이는 아니라서",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018871.1.26",
          "form": "그냥 그냥 대충 잘 넘어갔음",
          "original_form": "그냥 그냥 대충 잘 넘어갔음",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018871.1.27",
          "form": "저는 첫째한테",
          "original_form": "저는 첫째한테",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018871.1.28",
          "form": "미라 언니는 앵무새 죽고 나서",
          "original_form": "미라 언니는 앵무새 죽고 나서",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018871.1.29",
          "form": "엄청 많이 울었다던데",
          "original_form": "엄청 많이 울었다던데",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018871.1.30",
          "form": "언젠가는 죽으니",
          "original_form": "언젠가는 죽으니",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018871.1.31",
          "form": "울고불고 하면 다신 안 키운다고 얘기했음",
          "original_form": "울고불고 하면 다신 안 키운다고 얘기했음",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018871.1.32",
          "form": "미라 언니가 더 울었을지",
          "original_form": "미라 언니가 더 울었을지",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018871.1.33",
          "form": "앵무새한테 위로를 많이 받았잖아요",
          "original_form": "앵무새한테 위로를 많이 받았잖아요",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018871.1.34",
          "form": "아 니 진짜",
          "original_form": "아 니 진짜",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018871.1.35",
          "form": "언젠가는 죽다니\r\nㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "언젠가는 죽다니\r\nㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018871.1.36",
          "form": "미치겠다",
          "original_form": "미치겠다",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018871.1.37",
          "form": "첫 만남 전에 이별을 준비했어요",
          "original_form": "첫 만남 전에 이별을 준비했어요",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018871.1.38",
          "form": "에라이 정없는 인간아",
          "original_form": "에라이 정없는 인간아",
