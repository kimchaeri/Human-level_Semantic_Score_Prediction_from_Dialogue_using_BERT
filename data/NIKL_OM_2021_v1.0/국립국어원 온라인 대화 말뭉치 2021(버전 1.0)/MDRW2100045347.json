      "utterance": [
        {
          "id": "MDRW2100045347.1.1",
          "form": "사랑과 애정으로 보듬어주는 관계라고하나",
          "original_form": "사랑과 애정으로 보듬어주는 관계라고하나",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045347.1.2",
          "form": "서로한테",
          "original_form": "서로한테",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045347.1.3",
          "form": "사실 그 이츠학 얘기가 자존심상할 이야기를",
          "original_form": "사실 그 이츠학 얘기가 자존심상할 이야기를",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045347.1.4",
          "form": "손이 많이 가네..",
          "original_form": "손이 많이 가네..",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045347.1.5",
          "form": "관객한테 하는거락",
          "original_form": "관객한테 하는거락",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045347.1.6",
          "form": "ㅇ츠학이 상처받는 부분인데",
          "original_form": "ㅇ츠학이 상처받는 부분인데",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045347.1.7",
          "form": "여기서는",
          "original_form": "여기서는",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045347.1.8",
          "form": "하고 서로 끌어안고",
          "original_form": "하고 서로 끌어안고",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045347.1.9",
          "form": "토닥토닥해주고",
          "original_form": "토닥토닥해주고",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045347.1.10",
          "form": "약간 사랑은 아니고 전우애같은?",
          "original_form": "약간 사랑은 아니고 전우애같은?",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045347.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045347.1.12",
          "form": "알거같아",
          "original_form": "알거같아",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045347.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래서 혼자노선다르다고 호불호많이갈린대",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래서 혼자노선다르다고 호불호많이갈린대",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045347.1.14",
          "form": "근본을 좋아하면 아무래도",
          "original_form": "근본을 좋아하면 아무래도",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045347.1.15",
          "form": "전 인성이 팡팡 한거보단",
          "original_form": "전 인성이 팡팡 한거보단",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045347.1.16",
          "form": "캐붕난 느낌 들겠지...",
          "original_form": "캐붕난 느낌 들겠지...",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045347.1.17",
          "form": "이게 취향이긴했어 ㅋㅋㅋㅋ",
          "original_form": "이게 취향이긴했어 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045347.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ저도",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045347.1.19",
          "form": "대신 이렇게 가면",
          "original_form": "대신 이렇게 가면",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045347.1.20",
          "form": "얘기만 들으면",
          "original_form": "얘기만 들으면",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045347.1.21",
          "form": "이츠학이 얼마나",
          "original_form": "이츠학이 얼마나",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045347.1.22",
          "form": "조 노선 괜찮은데",
          "original_form": "조 노선 괜찮은데",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045347.1.23",
          "form": "드랙퀸을 그리워하는지가",
          "original_form": "드랙퀸을 그리워하는지가",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045347.1.24",
          "form": "안나와서",
          "original_form": "안나와서",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045347.1.25",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045347.1.26",
          "form": "그게 막 그 혐관으로",
          "original_form": "그게 막 그 혐관으로",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045347.1.27",
          "form": "그리워하는걸 인성질로 막고 ㅓ로 증오하고",
          "original_form": "그리워하는걸 인성질로 막고 ㅓ로 증오하고",
          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045347.1.28",
          "form": "이러다가",
          "original_form": "이러다가",
          "speaker_id": "2",
          "time": "20210908 02:01"
        }
      ]
    }
  ]
