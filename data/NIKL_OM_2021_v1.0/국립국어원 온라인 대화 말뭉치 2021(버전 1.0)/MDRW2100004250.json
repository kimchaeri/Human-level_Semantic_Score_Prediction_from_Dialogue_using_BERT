      "utterance": [
        {
          "id": "MDRW2100004250.1.1",
          "form": "아르바이트 해보신 적 있으세요?",
          "original_form": "아르바이트 해보신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004250.1.2",
          "form": "네 영화관알바해봤어요",
          "original_form": "네 영화관알바해봤어요",
          "speaker_id": "2",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004250.1.3",
          "form": "name1님은 있으신가요?",
          "original_form": "&name1&님은 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004250.1.4",
          "form": "오 영화관 어때요?",
          "original_form": "오 영화관 어때요?",
          "speaker_id": "1",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004250.1.5",
          "form": "저는 배스킨 해봤어요!",
          "original_form": "저는 배스킨 해봤어요!",
          "speaker_id": "1",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004250.1.6",
          "form": "헐 배스킨 완전좋아",
          "original_form": "헐 배스킨 완전좋아",
          "speaker_id": "2",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004250.1.7",
          "form": "영화관은 평일은 되게 한가한데 주말이나 공휴일은 엄청 빡세요",
          "original_form": "영화관은 평일은 되게 한가한데 주말이나 공휴일은 엄청 빡세요",
          "speaker_id": "2",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004250.1.8",
          "form": "매점이랑 매표일 해봤어요",
          "original_form": "매점이랑 매표일 해봤어요",
          "speaker_id": "2",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004250.1.9",
          "form": "그쵸 그때 완전 헬일 느낌..!",
          "original_form": "그쵸 그때 완전 헬일 느낌..!",
          "speaker_id": "1",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004250.1.10",
          "form": "네고왕에서 영화관 알바 편 봤었거든요",
          "original_form": "네고왕에서 영화관 알바 편 봤었거든요",
          "speaker_id": "1",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004250.1.11",
          "form": "거기도 진상손님들 많죠??ㅠ",
          "original_form": "거기도 진상손님들 많죠??ㅠ",
          "speaker_id": "1",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004250.1.12",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004250.1.13",
          "form": "매점일 할 때 팝콘이 짜다고 환불해달라던 인간도있었고",
          "original_form": "매점일 할 때 팝콘이 짜다고 환불해달라던 인간도있었고",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004250.1.14",
          "form": "영화가 재미없다고 환불해달라던 인간도 있었어요",
          "original_form": "영화가 재미없다고 환불해달라던 인간도 있었어요",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004250.1.15",
          "form": "진상이죠",
          "original_form": "진상이죠",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004250.1.16",
          "form": "배라는 어때요?",
          "original_form": "배라는 어때요?",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004250.1.17",
          "form": "헉 팝콘이 짜다고;;;",
          "original_form": "헉 팝콘이 짜다고;;;",
          "speaker_id": "1",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004250.1.18",
          "form": "진짜 대응메뉴얼 많겠어유ㅠㅠ",
          "original_form": "진짜 대응메뉴얼 많겠어유ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004250.1.19",
          "form": "그냥 그런 손님들한테는",
          "original_form": "그냥 그런 손님들한테는",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004250.1.20",
          "form": "저흰 젤 유명한 더더더 달라는 손님이죵,, 전 거의 마감이라 술취하신분도 많이 왔구",
          "original_form": "저흰 젤 유명한 더더더 달라는 손님이죵,, 전 거의 마감이라 술취하신분도 많이 왔구",
          "speaker_id": "1",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004250.1.21",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004250.1.22",
          "form": "점장님 부르라고 하셔서ㅋㅋ",
          "original_form": "점장님 부르라고 하셔서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004250.1.23",
          "form": "헐 술취한사람이요?",
          "original_form": "헐 술취한사람이요?",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004250.1.24",
          "form": "위험하지않나요",
          "original_form": "위험하지않나요",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004250.1.25",
          "form": "술병 들고오신 분도 봤는데 그분은 문제 없었구 거의 술취한 사람들은 고성방가나 언어모욕ㅠ..",
          "original_form": "술병 들고오신 분도 봤는데 그분은 문제 없었구 거의 술취한 사람들은 고성방가나 언어모욕ㅠ..",
          "speaker_id": "1",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004250.1.26",
          "form": "아저씨들이 거의 그랬던 것 같아요 그래서 남자손님들 오면 긴장부터 됐다는..",
          "original_form": "아저씨들이 거의 그랬던 것 같아요 그래서 남자손님들 오면 긴장부터 됐다는..",
          "speaker_id": "1",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004250.1.27",
          "form": "헐.. 진짜 진상이다",
          "original_form": "헐.. 진짜 진상이다",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004250.1.28",
          "form": "너무싫었겠어요ㅠㅠ",
          "original_form": "너무싫었겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004250.1.29",
          "form": "진짜 듣기만 했는데도 너무 무섭고 그러네요",
          "original_form": "진짜 듣기만 했는데도 너무 무섭고 그러네요",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004250.1.30",
          "form": "맞아요.. 그래도 중간중간 아이스크림 먹고 겨울엔 꿀이랍니다ㅎㅎ",
          "original_form": "맞아요.. 그래도 중간중간 아이스크림 먹고 겨울엔 꿀이랍니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004250.1.31",
          "form": "영화관은 영화 무료인가요?!",
          "original_form": "영화관은 영화 무료인가요?!",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004250.1.32",
          "form": "네 알바는 영화무료였어요",
          "original_form": "네 알바는 영화무료였어요",
          "speaker_id": "2",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004250.1.33",
          "form": "매점은 유료였는데 영화는 무료여서 열심히 봤었어요",
          "original_form": "매점은 유료였는데 영화는 무료여서 열심히 봤었어요",
          "speaker_id": "2",
          "time": "20210821 15:56"
        }
      ]
