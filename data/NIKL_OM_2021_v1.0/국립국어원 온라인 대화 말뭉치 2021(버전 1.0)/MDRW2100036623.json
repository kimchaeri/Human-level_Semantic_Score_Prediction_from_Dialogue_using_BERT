      "utterance": [
        {
          "id": "MDRW2100036623.1.1",
          "form": "내일까지",
          "original_form": "내일까지",
          "speaker_id": "1",
          "time": "20210831 02:31"
        },
        {
          "id": "MDRW2100036623.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210831 02:32"
        },
        {
          "id": "MDRW2100036623.1.3",
          "form": "농협 하나로마트에서",
          "original_form": "농협 하나로마트에서",
          "speaker_id": "1",
          "time": "20210831 02:32"
        },
        {
          "id": "MDRW2100036623.1.4",
          "form": "계란 한판에",
          "original_form": "계란 한판에",
          "speaker_id": "1",
          "time": "20210831 02:32"
        },
        {
          "id": "MDRW2100036623.1.5",
          "form": "하나로마트세일?",
          "original_form": "하나로마트세일?",
          "speaker_id": "2",
          "time": "20210831 02:32"
        },
        {
          "id": "MDRW2100036623.1.6",
          "form": "4천원대 네네!",
          "original_form": "4천원대 네네!",
          "speaker_id": "1",
          "time": "20210831 02:32"
        },
        {
          "id": "MDRW2100036623.1.7",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210831 02:32"
        },
        {
          "id": "MDRW2100036623.1.8",
          "form": "진짜싸네요",
          "original_form": "진짜싸네요",
          "speaker_id": "2",
          "time": "20210831 02:32"
        },
        {
          "id": "MDRW2100036623.1.9",
          "form": "아 아니다 열두시 지났으니까 오늘까지네요",
          "original_form": "아 아니다 열두시 지났으니까 오늘까지네요",
          "speaker_id": "1",
          "time": "20210831 02:32"
        },
        {
          "id": "MDRW2100036623.1.10",
          "form": "전국세일인가요?",
          "original_form": "전국세일인가요?",
          "speaker_id": "2",
          "time": "20210831 02:32"
        },
        {
          "id": "MDRW2100036623.1.11",
          "form": "오픈을 보통 9시에 한다고해서",
          "original_form": "오픈을 보통 9시에 한다고해서",
          "speaker_id": "1",
          "time": "20210831 02:32"
        },
        {
          "id": "MDRW2100036623.1.12",
          "form": "아침 일찍 달려가려구요ㅋㅋㅋ",
          "original_form": "아침 일찍 달려가려구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:32"
        },
        {
          "id": "MDRW2100036623.1.13",
          "form": "전국 하나로마트중에",
          "original_form": "전국 하나로마트중에",
          "speaker_id": "1",
          "time": "20210831 02:32"
        },
        {
          "id": "MDRW2100036623.1.14",
          "form": "38개의 지점에서 진행한대요",
          "original_form": "38개의 지점에서 진행한대요",
          "speaker_id": "1",
          "time": "20210831 02:32"
        },
        {
          "id": "MDRW2100036623.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ아 저희동네함가봐야겠어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 저희동네함가봐야겠어요",
          "speaker_id": "2",
          "time": "20210831 02:33"
        },
        {
          "id": "MDRW2100036623.1.16",
          "form": "근데 그런빅세일이면",
          "original_form": "근데 그런빅세일이면",
          "speaker_id": "2",
          "time": "20210831 02:33"
        },
        {
          "id": "MDRW2100036623.1.17",
          "form": "문자가오는데",
          "original_form": "문자가오는데",
          "speaker_id": "2",
          "time": "20210831 02:33"
        },
        {
          "id": "MDRW2100036623.1.18",
          "form": "아마 규모가 좀 크면 할 가능성이 높아요!",
          "original_form": "아마 규모가 좀 크면 할 가능성이 높아요!",
          "speaker_id": "1",
          "time": "20210831 02:33"
        },
        {
          "id": "MDRW2100036623.1.19",
          "form": "안오는거보면....",
          "original_form": "안오는거보면....",
          "speaker_id": "2",
          "time": "20210831 02:33"
        },
        {
          "id": "MDRW2100036623.1.20",
          "form": "생필품은 주로 어디서 구매하세요?",
          "original_form": "생필품은 주로 어디서 구매하세요?",
          "speaker_id": "1",
          "time": "20210831 02:33"
        },
        {
          "id": "MDRW2100036623.1.21",
          "form": "그래도 낼나가는김에 함들렀다가봐야겠네요",
          "original_form": "그래도 낼나가는김에 함들렀다가봐야겠네요",
          "speaker_id": "2",
          "time": "20210831 02:33"
        }
      ]
