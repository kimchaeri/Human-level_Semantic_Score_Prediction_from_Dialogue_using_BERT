        {
          "id": "MDRW2100004388.1.7",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004388.1.8",
          "form": "윈윈하고 샤오쥔도 다니긴 하지 않았나?",
          "original_form": "윈윈하고 샤오쥔도 다니긴 하지 않았나?",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004388.1.9",
          "form": "돕바",
          "original_form": "돕바",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004388.1.10",
          "form": "있자나",
          "original_form": "있자나",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004388.1.11",
          "form": "그런가",
          "original_form": "그런가",
          "speaker_id": "1",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004388.1.12",
          "form": "쿤은 정식 데뷔 전에 졸업한 사진 있더라구",
          "original_form": "쿤은 정식 데뷔 전에 졸업한 사진 있더라구",
          "speaker_id": "1",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004388.1.13",
          "form": "해찬이랑 쟈니 가방에 달린 키링 귀여워",
          "original_form": "해찬이랑 쟈니 가방에 달린 키링 귀여워",
          "speaker_id": "1",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004388.1.14",
          "form": "내가 좋아하는 아이돌이 가지고 있는건 따라사고싶어져",
          "original_form": "내가 좋아하는 아이돌이 가지고 있는건 따라사고싶어져",
          "speaker_id": "1",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004388.1.15",
          "form": "맞아 인형 넘 귀여워",
          "original_form": "맞아 인형 넘 귀여워",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004388.1.16",
          "form": "그치그치 손민수하고 싶잖아ㅠㅜ",
          "original_form": "그치그치 손민수하고 싶잖아ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004388.1.17",
          "form": "재밌게 봤다는거 보면 나도 궁금해지더라",
          "original_form": "재밌게 봤다는거 보면 나도 궁금해지더라",
          "speaker_id": "1",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004388.1.18",
          "form": "넷플릭스 해보고싶긴한데",
          "original_form": "넷플릭스 해보고싶긴한데",
          "speaker_id": "1",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004388.1.19",
          "form": "인볼까봐... 돈아까워 질 것 같아서 미루는중",
          "original_form": "인볼까봐... 돈아까워 질 것 같아서 미루는중",
          "speaker_id": "1",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004388.1.20",
          "form": "맞아 약간 넷플릭스 그거잖아",
