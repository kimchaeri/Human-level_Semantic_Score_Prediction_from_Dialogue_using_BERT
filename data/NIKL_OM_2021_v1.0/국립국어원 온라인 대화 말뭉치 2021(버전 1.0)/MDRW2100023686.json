      "utterance": [
        {
          "id": "MDRW2100023686.1.1",
          "form": "안녕하세요~ 점심 드셨나요?",
          "original_form": "안녕하세요~ 점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100023686.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100023686.1.3",
          "form": "네 먹었습니다 ㅎㅎ 도시락 싸온거 먹었어요!",
          "original_form": "네 먹었습니다 ㅎㅎ 도시락 싸온거 먹었어요!",
          "speaker_id": "2",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100023686.1.4",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100023686.1.5",
          "form": "앗 저도 집에서 먹었어요~",
          "original_form": "앗 저도 집에서 먹었어요~",
          "speaker_id": "1",
          "time": "20210830 14:43"
        },
        {
          "id": "MDRW2100023686.1.6",
          "form": "평소에 도시락 챙겨 다니는건가요?",
          "original_form": "평소에 도시락 챙겨 다니는건가요?",
          "speaker_id": "1",
          "time": "20210830 14:43"
        },
        {
          "id": "MDRW2100023686.1.7",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210830 14:43"
        },
        {
          "id": "MDRW2100023686.1.8",
          "form": "tmi지만 ... 제가 대학원생인데,,",
          "original_form": "tmi지만 ... 제가 대학원생인데,,",
          "speaker_id": "2",
          "time": "20210830 14:44"
        },
        {
          "id": "MDRW2100023686.1.9",
          "form": "매일 사먹기에는 돈도 없고",
          "original_form": "매일 사먹기에는 돈도 없고",
          "speaker_id": "2",
          "time": "20210830 14:44"
        },
        {
          "id": "MDRW2100023686.1.10",
          "form": "건강에도 부담될 것 같더라구요 ㅎㅎ",
          "original_form": "건강에도 부담될 것 같더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:44"
        },
        {
          "id": "MDRW2100023686.1.11",
          "form": "아하 그죠그죠",
          "original_form": "아하 그죠그죠",
          "speaker_id": "1",
          "time": "20210830 14:45"
        },
        {
          "id": "MDRW2100023686.1.12",
          "form": "그래도 되게 성실하시네요 도시락 싸들고 다니는 것도 쉬운 일 아닐텐데ㅜㅜ",
          "original_form": "그래도 되게 성실하시네요 도시락 싸들고 다니는 것도 쉬운 일 아닐텐데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 14:45"
        },
        {
          "id": "MDRW2100023686.1.13",
          "form": "보통 어떤 음식으로 챙겨다니시나요?",
          "original_form": "보통 어떤 음식으로 챙겨다니시나요?",
          "speaker_id": "1",
          "time": "20210830 14:45"
        },
        {
          "id": "MDRW2100023686.1.14",
          "form": "아이고 감사합니다 ㅠㅠ",
          "original_form": "아이고 감사합니다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100023686.1.15",
          "form": "근데 도시락이래봤자 별건 없구",
          "original_form": "근데 도시락이래봤자 별건 없구",
          "speaker_id": "2",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023686.1.16",
          "form": "그냥 밥 싸갖고 다니면서",
          "original_form": "그냥 밥 싸갖고 다니면서",
          "speaker_id": "2",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023686.1.17",
          "form": "냉장고에 반찬을 가득 넣어놔요 ㅎㅎㅎ",
          "original_form": "냉장고에 반찬을 가득 넣어놔요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023686.1.18",
          "form": "name1은 평소에도 집에서 드시나요?",
          "original_form": "&name1&은 평소에도 집에서 드시나요?",
          "speaker_id": "2",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100023686.1.19",
          "form": "네 저는 아직 학생이어서 별일없으면 거의 집에서 먹어요!",
          "original_form": "네 저는 아직 학생이어서 별일없으면 거의 집에서 먹어요!",
          "speaker_id": "1",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100023686.1.20",
          "form": "아아 ~ 이제 비대면이라서",
          "original_form": "아아 ~ 이제 비대면이라서",
          "speaker_id": "2",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023686.1.21",
          "form": "친구들이랑 식당 갈 일이",
          "original_form": "친구들이랑 식당 갈 일이",
          "speaker_id": "2",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023686.1.22",
          "form": "일부러 약속잡지 않으면 없겠네요ㅠㅠㅠ",
          "original_form": "일부러 약속잡지 않으면 없겠네요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023686.1.23",
          "form": "요리하는 걸 좋아하기도 하고~ 요즘 시국도 이렇다보니까 집에서 밥 먹는 일이 더 많은 거 같애요",
          "original_form": "요리하는 걸 좋아하기도 하고~ 요즘 시국도 이렇다보니까 집에서 밥 먹는 일이 더 많은 거 같애요",
          "speaker_id": "1",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023686.1.24",
          "form": "네네 그죠ㅜㅜ 그래서 불필요한 약속은 잘 안 잡게 되더라구요",
          "original_form": "네네 그죠ㅜㅜ 그래서 불필요한 약속은 잘 안 잡게 되더라구요",
          "speaker_id": "1",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023686.1.25",
          "form": "우와 요리하는거 좋아하시는구나~!",
          "original_form": "우와 요리하는거 좋아하시는구나~!",
          "speaker_id": "2",
          "time": "20210830 15:36"
        },
        {
          "id": "MDRW2100023686.1.26",
          "form": "어떤 요리를 주로 하시나요?",
          "original_form": "어떤 요리를 주로 하시나요?",
          "speaker_id": "2",
          "time": "20210830 15:36"
        },
        {
          "id": "MDRW2100023686.1.27",
          "form": "그러면 집밥 드시는 게 즐거우실 것 같아요!",
          "original_form": "그러면 집밥 드시는 게 즐거우실 것 같아요!",
          "speaker_id": "2",
          "time": "20210830 15:36"
        },
        {
          "id": "MDRW2100023686.1.28",
          "form": "저는 된장찌개나 마파두부처럼 두부들어간 요리 이런 거 좋아하구 가끔씩 밥에 질리면 샌드위치 해먹거나~하는 편이에요!",
          "original_form": "저는 된장찌개나 마파두부처럼 두부들어간 요리 이런 거 좋아하구 가끔씩 밥에 질리면 샌드위치 해먹거나~하는 편이에요!",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100023686.1.29",
          "form": "name2은 따로 요리는 자주 안 하시나요?",
          "original_form": "&name2&은 따로 요리는 자주 안 하시나요?",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100023686.1.30",
          "form": "우와 맛있는 거 많이 하실 줄 아시네요!!",
          "original_form": "우와 맛있는 거 많이 하실 줄 아시네요!!",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100023686.1.31",
          "form": "저는 자주 할 시간은 없는데",
          "original_form": "저는 자주 할 시간은 없는데",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100023686.1.32",
          "form": "하게 되면...",
          "original_form": "하게 되면...",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100023686.1.33",
          "form": "애호박전을 부쳐먹어요!",
          "original_form": "애호박전을 부쳐먹어요!",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100023686.1.34",
          "form": "근데 밀가루 없는 애호박전이에요 ㅎㅎ",
          "original_form": "근데 밀가루 없는 애호박전이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100023686.1.35",
          "form": "약간 야매요리긴 한데",
          "original_form": "약간 야매요리긴 한데",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100023686.1.36",
          "form": "애호박 얇게 잘라서 계란물만 입히는거예요 ㅎㅎ",
          "original_form": "애호박 얇게 잘라서 계란물만 입히는거예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100023686.1.37",
          "form": "헉 맛있겠네요~ 명절에 필수아닌가요!",
          "original_form": "헉 맛있겠네요~ 명절에 필수아닌가요!",
          "speaker_id": "1",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100023686.1.38",
          "form": "저도 애호박 좋아해요!ㅎㅎ",
          "original_form": "저도 애호박 좋아해요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100023686.1.39",
          "form": "맞아요 만만한게 애호박인거같아요 ㅎㅎ",
          "original_form": "맞아요 만만한게 애호박인거같아요 ㅎㅎ",
