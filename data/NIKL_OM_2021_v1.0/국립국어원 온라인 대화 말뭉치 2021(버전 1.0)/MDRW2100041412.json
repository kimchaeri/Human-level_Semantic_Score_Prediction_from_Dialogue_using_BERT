      "utterance": [
        {
          "id": "MDRW2100041412.1.1",
          "form": "네 저는 감자 류의 짭짤한 과자 좋아합니다",
          "original_form": "네 저는 감자 류의 짭짤한 과자 좋아합니다",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041412.1.2",
          "form": "오감자 예감",
          "original_form": "오감자 예감",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041412.1.3",
          "form": "달달한 것도 한 번씩 먹는데 벤츠 라던가 아니면",
          "original_form": "달달한 것도 한 번씩 먹는데 벤츠 라던가 아니면",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041412.1.4",
          "form": "프링글스",
          "original_form": "프링글스",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041412.1.5",
          "form": "포테토칩",
          "original_form": "포테토칩",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041412.1.6",
          "form": "안 그래도 예감 좋아해서 두 박스 사 왔어요",
          "original_form": "안 그래도 예감 좋아해서 두 박스 사 왔어요",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041412.1.7",
          "form": "달달한 건 허니버터칩 정도요",
          "original_form": "달달한 건 허니버터칩 정도요",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041412.1.8",
          "form": "예감 너무 맛있어요 치즈 예감 특히",
          "original_form": "예감 너무 맛있어요 치즈 예감 특히",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041412.1.9",
          "form": "감자로 만든 과자를 좋아하네요",
          "original_form": "감자로 만든 과자를 좋아하네요",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041412.1.10",
          "form": "그 감자 중에 어 뭐지 그 노란색깔 긴 통에 얇은 감자",
          "original_form": "그 감자 중에 어 뭐지 그 노란색깔 긴 통에 얇은 감자",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041412.1.11",
          "form": "치즈도 맛있고 기본도 맛있고 정말 맛있어요",
          "original_form": "치즈도 맛있고 기본도 맛있고 정말 맛있어요",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041412.1.12",
          "form": "포카칩은 어니언이 진리입니다",
          "original_form": "포카칩은 어니언이 진리입니다",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041412.1.13",
          "form": "좀 엔에서 똑똑하고 불어뜨리는 맛이 있는",
          "original_form": "좀 엔에서 똑똑하고 불어뜨리는 맛이 있는",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041412.1.14",
          "form": "그게 뭘까요",
          "original_form": "그게 뭘까요",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041412.1.15",
          "form": "다각형 모양의 통인데",
          "original_form": "다각형 모양의 통인데",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041412.1.16",
          "form": "길다 라구요 노란 통이에요",
          "original_form": "길다 라구요 노란 통이에요",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041412.1.17",
          "form": "아 이름은 기억이 안 나는데 그것도 감자 과자 지요",
          "original_form": "아 이름은 기억이 안 나는데 그것도 감자 과자 지요",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041412.1.18",
          "form": "알 것 같네요",
          "original_form": "알 것 같네요",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041412.1.19",
          "form": "생일을 자주 하지요",
          "original_form": "생일을 자주 하지요",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041412.1.20",
          "form": "이제 11월이 되면 빼빼로데이가 다가오는데 빼빼로 사 먹게 됐네 되겠네요",
          "original_form": "이제 11월이 되면 빼빼로데이가 다가오는데 빼빼로 사 먹게 됐네 되겠네요",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041412.1.21",
          "form": "세일",
          "original_form": "세일",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041412.1.22",
          "form": "저는 오징어 집도 좋아합니다",
          "original_form": "저는 오징어 집도 좋아합니다",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041412.1.23",
          "form": "ㅋㅋㅋ빼빼로는 싫어요ㅜ",
          "original_form": "ㅋㅋㅋ빼빼로는 싫어요ㅜ",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041412.1.24",
          "form": "포카칩 어니언맛 굉장히 좋아합니다",
          "original_form": "포카칩 어니언맛 굉장히 좋아합니다",
