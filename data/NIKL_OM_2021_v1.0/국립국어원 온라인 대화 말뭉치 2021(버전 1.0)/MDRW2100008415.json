      "utterance": [
        {
          "id": "MDRW2100008415.1.1",
          "form": "AI 좋아,, 진짜 과학이란 너무 매력적인것",
          "original_form": "AI 좋아,, 진짜 과학이란 너무 매력적인것",
          "speaker_id": "1",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008415.1.2",
          "form": "AI?",
          "original_form": "AI?",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008415.1.3",
          "form": "재미있지",
          "original_form": "재미있지",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008415.1.4",
          "form": "어려울뿐..",
          "original_form": "어려울뿐..",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008415.1.5",
          "form": "AI배우고있어? 나는 배우는건 포기해서",
          "original_form": "AI배우고있어? 나는 배우는건 포기해서",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008415.1.6",
          "form": "우리학교 그 과도 AI관련과 이고",
          "original_form": "우리학교 그 과도 AI관련과 이고",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008415.1.7",
          "form": "대세 맞는듯 AI",
          "original_form": "대세 맞는듯 AI",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008415.1.8",
          "form": "배우고있다고 해야하나? 발만 걸치고있어",
          "original_form": "배우고있다고 해야하나? 발만 걸치고있어",
          "speaker_id": "2",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008415.1.9",
          "form": "어떻게 배우는데??",
          "original_form": "어떻게 배우는데??",
          "speaker_id": "1",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008415.1.10",
          "form": "독학이지 뭐 나름 언어를 얇고 많이 배워놔서",
          "original_form": "독학이지 뭐 나름 언어를 얇고 많이 배워놔서",
          "speaker_id": "2",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008415.1.11",
          "form": "AI가 독학이 가능한가?",
          "original_form": "AI가 독학이 가능한가?",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008415.1.12",
          "form": "학과 에서 배워야하지않나",
          "original_form": "학과 에서 배워야하지않나",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008415.1.13",
          "form": "어차피 머신러닝 딥러닝 쪽이잖아",
          "original_form": "어차피 머신러닝 딥러닝 쪽이잖아",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008415.1.14",
          "form": "그니까 완벽하게 배운다고는 말 못하고 그냥 발만 걸치고 있다고 하는거지 뭐..",
          "original_form": "그니까 완벽하게 배운다고는 말 못하고 그냥 발만 걸치고 있다고 하는거지 뭐..",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008415.1.15",
          "form": "웅웅 뭐만든 프젝 있어?",
          "original_form": "웅웅 뭐만든 프젝 있어?",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008415.1.16",
          "form": "아니 ㅋㅋㅋㅋ있으면 프프젝 고민 안하지",
          "original_form": "아니 ㅋㅋㅋㅋ있으면 프프젝 고민 안하지",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008415.1.17",
          "form": "그냥 코드 던져주면 대강 이해할 정도",
          "original_form": "그냥 코드 던져주면 대강 이해할 정도",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008415.1.18",
          "form": "걸쳤다니까ㅎ",
          "original_form": "걸쳤다니까ㅎ",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008415.1.19",
          "form": "프프는 AI랑 완전다르자낰ㅋㅋㅋ근데",
          "original_form": "프프는 AI랑 완전다르자낰ㅋㅋㅋ근데",
          "speaker_id": "1",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100008415.1.20",
          "form": "요즘 AI넘나 똑똑쓰",
          "original_form": "요즘 AI넘나 똑똑쓰",
          "speaker_id": "1",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100008415.1.21",
          "form": "자동차도AI가 대세던데",
          "original_form": "자동차도AI가 대세던데",
          "speaker_id": "1",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100008415.1.22",
          "form": "대학다시간다면 AI학과 넣어볼듯ㅋㅋㅋㅋㅋㅋ",
          "original_form": "대학다시간다면 AI학과 넣어볼듯ㅋㅋㅋㅋㅋㅋ",
