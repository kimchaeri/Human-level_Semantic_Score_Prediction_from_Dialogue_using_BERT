      "utterance": [
        {
          "id": "MDRW2100009854.1.1",
          "form": "안녕하세요! 게임하시나요?",
          "original_form": "안녕하세요! 게임하시나요?",
          "speaker_id": "1",
          "time": "20210823 10:57"
        },
        {
          "id": "MDRW2100009854.1.2",
          "form": "최근에 쿠키런하다가 접었어요ㅎㅎ 게임하세요?",
          "original_form": "최근에 쿠키런하다가 접었어요ㅎㅎ 게임하세요?",
          "speaker_id": "2",
          "time": "20210823 10:57"
        },
        {
          "id": "MDRW2100009854.1.3",
          "form": "저도 쿠키런하다가 접고 롤을 배우고 있어요",
          "original_form": "저도 쿠키런하다가 접고 롤을 배우고 있어요",
          "speaker_id": "1",
          "time": "20210823 10:58"
        },
        {
          "id": "MDRW2100009854.1.4",
          "form": "아 어려워보이던데 그런 겜은 못하겠어요ㅠ",
          "original_form": "아 어려워보이던데 그런 겜은 못하겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210823 10:58"
        },
        {
          "id": "MDRW2100009854.1.5",
          "form": "맞아요 롤 어렵죠... 저도 잘은 못한답니다!",
          "original_form": "맞아요 롤 어렵죠... 저도 잘은 못한답니다!",
          "speaker_id": "1",
          "time": "20210823 10:59"
        },
        {
          "id": "MDRW2100009854.1.6",
          "form": "좋아하는 사람은 엄청 좋아하던데 엄두가 안나는ㅎㅎ",
          "original_form": "좋아하는 사람은 엄청 좋아하던데 엄두가 안나는ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 10:59"
        },
        {
          "id": "MDRW2100009854.1.7",
          "form": "맞아요..ㅎㅎ 쿠키런은 얼마 정도 하다가 접으셨나요?",
          "original_form": "맞아요..ㅎㅎ 쿠키런은 얼마 정도 하다가 접으셨나요?",
          "speaker_id": "1",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009854.1.8",
          "form": "처음나왔을때부터 하다가 세달하구 접은거같아요ㅠㅠ",
          "original_form": "처음나왔을때부터 하다가 세달하구 접은거같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 11:01"
        },
        {
          "id": "MDRW2100009854.1.9",
          "form": "현질안하면 못따라가겠더라구요",
          "original_form": "현질안하면 못따라가겠더라구요",
          "speaker_id": "2",
          "time": "20210823 11:01"
        },
        {
          "id": "MDRW2100009854.1.10",
          "form": "헐 그럼 진짜 얼마 안하셨네요...! 전 쿠키런 킹덤도 하다가 말았어요",
          "original_form": "헐 그럼 진짜 얼마 안하셨네요...! 전 쿠키런 킹덤도 하다가 말았어요",
          "speaker_id": "1",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009854.1.11",
          "form": "아 저 킹덤말한거였어요ㅎㅎ",
          "original_form": "아 저 킹덤말한거였어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009854.1.12",
          "form": "아 그렇군요 쿠킹덤은 진짜 현질이 제일 중요하죠...",
          "original_form": "아 그렇군요 쿠킹덤은 진짜 현질이 제일 중요하죠...",
          "speaker_id": "1",
          "time": "20210823 11:06"
        },
        {
          "id": "MDRW2100009854.1.13",
          "form": "네 재미로 시작한건데 뭔가 현타가ㅎㅎ",
          "original_form": "네 재미로 시작한건데 뭔가 현타가ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 11:07"
        },
        {
          "id": "MDRW2100009854.1.14",
          "form": "맞아요 쿠킹덤에는 돈 뿐만 아니라 시간도 엄청 써야 해서 힘들죠...",
          "original_form": "맞아요 쿠킹덤에는 돈 뿐만 아니라 시간도 엄청 써야 해서 힘들죠...",
          "speaker_id": "1",
          "time": "20210823 11:12"
        }
      ]
