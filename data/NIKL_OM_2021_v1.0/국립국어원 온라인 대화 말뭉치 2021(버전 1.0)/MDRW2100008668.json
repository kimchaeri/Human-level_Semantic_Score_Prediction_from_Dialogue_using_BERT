      "utterance": [
        {
          "id": "MDRW2100008668.1.1",
          "form": "집에서 운동하세요?",
          "original_form": "집에서 운동하세요?",
          "speaker_id": "1",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008668.1.2",
          "form": "자주는 아니지만",
          "original_form": "자주는 아니지만",
          "speaker_id": "2",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008668.1.3",
          "form": "간간이 조금 하는 정도에요",
          "original_form": "간간이 조금 하는 정도에요",
          "speaker_id": "2",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008668.1.4",
          "form": "홈트??",
          "original_form": "홈트??",
          "speaker_id": "1",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008668.1.5",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008668.1.6",
          "form": "그냥 요가 정도해요",
          "original_form": "그냥 요가 정도해요",
          "speaker_id": "2",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008668.1.7",
          "form": "저도 집에서 하고싶은데",
          "original_form": "저도 집에서 하고싶은데",
          "speaker_id": "1",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008668.1.8",
          "form": "의지가 잘 안 생기더라구요",
          "original_form": "의지가 잘 안 생기더라구요",
          "speaker_id": "1",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008668.1.9",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008668.1.10",
          "form": "아무래도 혼자서는 힘들죠",
          "original_form": "아무래도 혼자서는 힘들죠",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008668.1.11",
          "form": "네 그래서 핏블리? 가 하는 그런 온라인 홈트 패키지 이런것도 생각중이긴 해요",
          "original_form": "네 그래서 핏블리? 가 하는 그런 온라인 홈트 패키지 이런것도 생각중이긴 해요",
          "speaker_id": "1",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008668.1.12",
          "form": "그런 것도 있군요",
          "original_form": "그런 것도 있군요",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008668.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008668.1.14",
          "form": "집에서 보고 따라하는 거라 가격이 저렴하더라구요",
          "original_form": "집에서 보고 따라하는 거라 가격이 저렴하더라구요",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008668.1.15",
          "form": "그러면 해볼 만 하겠어요",
          "original_form": "그러면 해볼 만 하겠어요",
          "speaker_id": "2",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008668.1.16",
          "form": "네 근데 역시 강제하는 사람이 없으면",
          "original_form": "네 근데 역시 강제하는 사람이 없으면",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008668.1.17",
          "form": "안 하게 될 것 같아서 ㅋㅋㅋ",
          "original_form": "안 하게 될 것 같아서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008668.1.18",
          "form": "아직 고민중입니다",
          "original_form": "아직 고민중입니다",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008668.1.19",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008668.1.20",
          "form": "헬스장도 잘 안가게 되는데",
          "original_form": "헬스장도 잘 안가게 되는데",
          "speaker_id": "2",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008668.1.21",
          "form": "네.. 근데 오프라인 가도",
          "original_form": "네.. 근데 오프라인 가도",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008668.1.22",
          "form": "사람들 많으면",
          "original_form": "사람들 많으면",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008668.1.23",
          "form": "자세가 잘 안보여서",
          "original_form": "자세가 잘 안보여서",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008668.1.24",
          "form": "온라인 강의가 더 나은가도 싶고..",
          "original_form": "온라인 강의가 더 나은가도 싶고..",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008668.1.25",
          "form": "맞아요 속도 따라가기가 좀 버거울 때가 있어요",
          "original_form": "맞아요 속도 따라가기가 좀 버거울 때가 있어요",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008668.1.26",
          "form": "네 주변엔 아직 온라인 피트니스는 이용하는 사람이 없어서",
          "original_form": "네 주변엔 아직 온라인 피트니스는 이용하는 사람이 없어서",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008668.1.27",
          "form": "아직 잘 모르겠네요 ㅎㅎ",
          "original_form": "아직 잘 모르겠네요 ㅎㅎ",
