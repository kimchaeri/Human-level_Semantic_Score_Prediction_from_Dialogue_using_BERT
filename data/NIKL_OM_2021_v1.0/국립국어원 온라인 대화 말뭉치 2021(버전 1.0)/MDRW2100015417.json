      "utterance": [
        {
          "id": "MDRW2100015417.1.1",
          "form": "저 최근에 다시봤는데 다시봐도 재밌더라구요",
          "original_form": "저 최근에 다시봤는데 다시봐도 재밌더라구요",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015417.1.2",
          "form": "헉 어쩌다 최근에 다시보셨나요",
          "original_form": "헉 어쩌다 최근에 다시보셨나요",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015417.1.3",
          "form": "ㄹㅇ 지브리거는 언제봐도 명작이에여",
          "original_form": "ㄹㅇ 지브리거는 언제봐도 명작이에여",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015417.1.4",
          "form": "방구석 1열인가",
          "original_form": "방구석 1열인가",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015417.1.5",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015417.1.6",
          "form": "그거 지브리편보다가",
          "original_form": "그거 지브리편보다가",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015417.1.7",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015417.1.8",
          "form": "보고싶어져서",
          "original_form": "보고싶어져서",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015417.1.9",
          "form": "좋아하는 캐릭터있나요",
          "original_form": "좋아하는 캐릭터있나요",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015417.1.10",
          "form": "그게 그 나이대 아이들에게",
          "original_form": "그게 그 나이대 아이들에게",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015417.1.11",
          "form": "자아 찾는거라더라고요",
          "original_form": "자아 찾는거라더라고요",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015417.1.12",
          "form": "저는 드래곤 길들이기 아세요?",
          "original_form": "저는 드래곤 길들이기 아세요?",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015417.1.13",
          "form": "그런거같아요",
          "original_form": "그런거같아요",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015417.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015417.1.15",
          "form": "거기 투슬리스요",
          "original_form": "거기 투슬리스요",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015417.1.16",
          "form": "용.",
          "original_form": "용.",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015417.1.17",
          "form": "너어어엉무 귀엽죠",
          "original_form": "너어어엉무 귀엽죠",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015417.1.18",
          "form": "디즈니의 유일한 미남나오는 애니짏아요",
          "original_form": "디즈니의 유일한 미남나오는 애니짏아요",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015417.1.19",
          "form": "저 투슬리스 사랑해요",
          "original_form": "저 투슬리스 사랑해요",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015417.1.20",
          "form": "아 그 용 알아요",
          "original_form": "아 그 용 알아요",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015417.1.21",
          "form": "잭프로스트요?",
          "original_form": "잭프로스트요?",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015417.1.22",
          "form": "너무 귀.여.워",
          "original_form": "너무 귀.여.워",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015417.1.23",
          "form": "잭도 잘생겼는데",
          "original_form": "잭도 잘생겼는데",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015417.1.24",
          "form": "귀여운게 최고",
          "original_form": "귀여운게 최고",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015417.1.25",
          "form": "또 좋아하는 디즈니나 픽사영화있어요ㅡ",
          "original_form": "또 좋아하는 디즈니나 픽사영화있어요ㅡ",
          "speaker_id": "1",
          "time": "20210825 23:04"
        }
      ]
