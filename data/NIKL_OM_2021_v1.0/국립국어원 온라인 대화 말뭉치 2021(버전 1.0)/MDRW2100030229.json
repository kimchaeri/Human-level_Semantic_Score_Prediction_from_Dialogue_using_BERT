      "utterance": [
        {
          "id": "MDRW2100030229.1.1",
          "form": "게임 좋아하세요? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "게임 좋아하세요? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030229.1.2",
          "form": "어몽어스요즘빠졌어요",
          "original_form": "어몽어스요즘빠졌어요",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030229.1.3",
          "form": "저번주에",
          "original_form": "저번주에",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030229.1.4",
          "form": "꼬심당해서 했는데",
          "original_form": "꼬심당해서 했는데",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030229.1.5",
          "form": "너무존잼이더라구요",
          "original_form": "너무존잼이더라구요",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030229.1.6",
          "form": "저도 해보고 싶었는데 게임 시작하기도 귀찮아서",
          "original_form": "저도 해보고 싶었는데 게임 시작하기도 귀찮아서",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030229.1.7",
          "form": "말았어요ㅋㅋㅋ",
          "original_form": "말았어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030229.1.8",
          "form": "정확히 어떻게 하는 게임이에요?",
          "original_form": "정확히 어떻게 하는 게임이에요?",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030229.1.9",
          "form": "어..",
          "original_form": "어..",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030229.1.10",
          "form": "그 뭐조",
          "original_form": "그 뭐조",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030229.1.11",
          "form": "그그그",
          "original_form": "그그그",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030229.1.12",
          "form": "아 찾고다시옵니다",
          "original_form": "아 찾고다시옵니다",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030229.1.13",
          "form": "ㅋㅋㅋㅋ 괜찮아요",
          "original_form": "ㅋㅋㅋㅋ 괜찮아요",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030229.1.14",
          "form": "마피아",
          "original_form": "마피아",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030229.1.15",
          "form": "랑 비슷해요",
          "original_form": "랑 비슷해요",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030229.1.16",
          "form": "사람죽이고다니는 임포스터 잡는?",
          "original_form": "사람죽이고다니는 임포스터 잡는?",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100030229.1.17",
          "form": "오 재미있을거 같은데",
          "original_form": "오 재미있을거 같은데",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030229.1.18",
          "form": "금방 죽을거 같아욬ㅋㅋㅋ",
          "original_form": "금방 죽을거 같아욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030229.1.19",
          "form": "저도...",
          "original_form": "저도...",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030229.1.20",
          "form": "그래서 친구가",
          "original_form": "그래서 친구가",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030229.1.21",
          "form": "개복치라고 놀렸습니다...",
          "original_form": "개복치라고 놀렸습니다...",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030229.1.22",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030229.1.23",
          "form": "ㅋㅋㅋㅋㅋ 저도 한다면 아마 개복치행",
          "original_form": "ㅋㅋㅋㅋㅋ 저도 한다면 아마 개복치행",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030229.1.24",
          "form": "ㅋㅋㅋㅋ근데다들 처음했는데 잘하더라구요",
          "original_form": "ㅋㅋㅋㅋ근데다들 처음했는데 잘하더라구요",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030229.1.25",
          "form": "이번주도 하기로했는데",
          "original_form": "이번주도 하기로했는데",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030229.1.26",
          "form": "발전가능할지 ㅠ",
          "original_form": "발전가능할지 ㅠ",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030229.1.27",
          "form": "ㅋㅋㅋㅋ 그것도 하면 늘거 같은데요?",
          "original_form": "ㅋㅋㅋㅋ 그것도 하면 늘거 같은데요?",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030229.1.28",
          "form": "주에 한번밖에안하니 ㅎㅎㅎㅎㅎㅎ",
          "original_form": "주에 한번밖에안하니 ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030229.1.29",
          "form": "개복치 벗어나긴 글러먹은거같아요",
          "original_form": "개복치 벗어나긴 글러먹은거같아요",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030229.1.30",
          "form": "주에 한번이어도 쬐금 나아지지 않을까요ㅋㅋㅋ",
          "original_form": "주에 한번이어도 쬐금 나아지지 않을까요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:26"
        }
      ]
    }
