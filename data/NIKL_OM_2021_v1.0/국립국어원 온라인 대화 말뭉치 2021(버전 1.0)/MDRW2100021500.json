      "utterance": [
        {
          "id": "MDRW2100021500.1.1",
          "form": "티켓팅 잘하시나요?",
          "original_form": "티켓팅 잘하시나요?",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100021500.1.2",
          "form": "잘 못해요 ㅎㅎ...",
          "original_form": "잘 못해요 ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100021500.1.3",
          "form": "예전엔 그래도 잘 잡았는데",
          "original_form": "예전엔 그래도 잘 잡았는데",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100021500.1.4",
          "form": "코로나 터지면서 너무 빡세져서..",
          "original_form": "코로나 터지면서 너무 빡세져서..",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100021500.1.5",
          "form": "코로나..ㅂㄷㅂㄷ",
          "original_form": "코로나..ㅂㄷㅂㄷ",
          "speaker_id": "1",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100021500.1.6",
          "form": "원래도 빡쎈게 티켓팅인데 말이죠..",
          "original_form": "원래도 빡쎈게 티켓팅인데 말이죠..",
          "speaker_id": "1",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100021500.1.7",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100021500.1.8",
          "form": "자리도 적어지고",
          "original_form": "자리도 적어지고",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100021500.1.9",
          "form": "매크로나 대리티켓팅도 너무 늘었어요",
          "original_form": "매크로나 대리티켓팅도 너무 늘었어요",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100021500.1.10",
          "form": "좋아하는 영화있으세요?",
          "original_form": "좋아하는 영화있으세요?",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100021500.1.11",
          "form": "맞아요 매크로 진짜 화나죠",
          "original_form": "맞아요 매크로 진짜 화나죠",
          "speaker_id": "1",
          "time": "20210828 20:04"
        },
        {
          "id": "MDRW2100021500.1.12",
          "form": "저는 죽은 시인의 사회 좋아해요!",
          "original_form": "저는 죽은 시인의 사회 좋아해요!",
          "speaker_id": "1",
          "time": "20210828 20:04"
        },
        {
          "id": "MDRW2100021500.1.13",
          "form": "헉 대박",
          "original_form": "헉 대박",
          "speaker_id": "2",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100021500.1.14",
          "form": "저도 그 영화 정말 감명깊게 봤어요",
          "original_form": "저도 그 영화 정말 감명깊게 봤어요",
          "speaker_id": "2",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100021500.1.15",
          "form": "대박대박",
          "original_form": "대박대박",
          "speaker_id": "1",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100021500.1.16",
          "form": "캡틴. ㅠ",
          "original_form": "캡틴. ㅠ",
          "speaker_id": "2",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100021500.1.17",
          "form": "저 처음봤을때 그거 이해안돼서 3번보고 이해했었어요",
          "original_form": "저 처음봤을때 그거 이해안돼서 3번보고 이해했었어요",
          "speaker_id": "1",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100021500.1.18",
          "form": "오 마이 캡틴ㅠ",
          "original_form": "오 마이 캡틴ㅠ",
          "speaker_id": "1",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100021500.1.19",
          "form": "저는 한번밖에 못봤어요",
          "original_form": "저는 한번밖에 못봤어요",
          "speaker_id": "2",
          "time": "20210828 20:10"
        },
        {
          "id": "MDRW2100021500.1.20",
          "form": "올해안에 한번 더 봐야겠다",
          "original_form": "올해안에 한번 더 봐야겠다",
          "speaker_id": "2",
          "time": "20210828 20:10"
        },
        {
          "id": "MDRW2100021500.1.21",
          "form": "아 저도 다시 봐야겠네요",
          "original_form": "아 저도 다시 봐야겠네요",
          "speaker_id": "1",
          "time": "20210828 20:14"
        },
        {
          "id": "MDRW2100021500.1.22",
          "form": "이야기하니 캡틴이 보고싶네요",
          "original_form": "이야기하니 캡틴이 보고싶네요",
          "speaker_id": "1",
          "time": "20210828 20:15"
        },
        {
          "id": "MDRW2100021500.1.23",
          "form": "저는 최근에 봤던 것 중에",
          "original_form": "저는 최근에 봤던 것 중에",
          "speaker_id": "2",
          "time": "20210828 20:15"
        },
        {
          "id": "MDRW2100021500.1.24",
          "form": "메기 정말 잘 봤었어요",
          "original_form": "메기 정말 잘 봤었어요",
          "speaker_id": "2",
          "time": "20210828 20:15"
        },
        {
          "id": "MDRW2100021500.1.25",
          "form": "한국영화인가요?",
          "original_form": "한국영화인가요?",
          "speaker_id": "1",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100021500.1.26",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100021500.1.27",
          "form": "이주영 구교환 배우 나왔던",
          "original_form": "이주영 구교환 배우 나왔던",
          "speaker_id": "2",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100021500.1.28",
          "form": "옹 한번 찾아봐야겠어요",
          "original_form": "옹 한번 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 20:26"
        }
      ]
    }
  ]
