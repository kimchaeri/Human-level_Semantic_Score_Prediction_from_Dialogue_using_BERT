        },
        {
          "id": "MDRW2100003927.1.2",
          "form": "결혼하신지 얼마나 되셨어요?",
          "original_form": "결혼하신지 얼마나 되셨어요?",
          "speaker_id": "2",
          "time": "20210821 13:57"
        },
        {
          "id": "MDRW2100003927.1.3",
          "form": "9년이요",
          "original_form": "9년이요",
          "speaker_id": "1",
          "time": "20210821 13:58"
        },
        {
          "id": "MDRW2100003927.1.4",
          "form": "오 오래전에 하셨군요ㅎ",
          "original_form": "오 오래전에 하셨군요ㅎ",
          "speaker_id": "2",
          "time": "20210821 13:59"
        },
        {
          "id": "MDRW2100003927.1.5",
          "form": "연애결혼?",
          "original_form": "연애결혼?",
          "speaker_id": "2",
          "time": "20210821 13:59"
        },
        {
          "id": "MDRW2100003927.1.6",
          "form": "네 1년",
          "original_form": "네 1년",
          "speaker_id": "1",
          "time": "20210821 14:00"
        },
        {
          "id": "MDRW2100003927.1.7",
          "form": "결혼생활에 만족하시나요?",
          "original_form": "결혼생활에 만족하시나요?",
          "speaker_id": "2",
          "time": "20210821 14:01"
        },
        {
          "id": "MDRW2100003927.1.8",
          "form": "네 행복해요",
          "original_form": "네 행복해요",
          "speaker_id": "1",
          "time": "20210821 14:02"
        },
        {
          "id": "MDRW2100003927.1.9",
          "form": "결혼하는걸 추천하세요?",
          "original_form": "결혼하는걸 추천하세요?",
          "speaker_id": "2",
          "time": "20210821 14:03"
        },
        {
          "id": "MDRW2100003927.1.10",
          "form": "한번은 경험이",
          "original_form": "한번은 경험이",
          "speaker_id": "1",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100003927.1.11",
          "form": "그렇군요 저도 하는게 더좋다고 생각해요",
          "original_form": "그렇군요 저도 하는게 더좋다고 생각해요",
          "speaker_id": "2",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100003927.1.12",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100003927.1.13",
          "form": "좀 길게얘기하시면 안되나요ㅎ",
          "original_form": "좀 길게얘기하시면 안되나요ㅎ",
          "speaker_id": "2",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100003927.1.14",
          "form": "결혼하면 항상 함께라서 좋아요",
          "original_form": "결혼하면 항상 함께라서 좋아요",
          "speaker_id": "1",
          "time": "20210821 16:21"
