      "utterance": [
        {
          "id": "MDRW2100012685.1.1",
          "form": "안녕하세요! name2은 게임 좋아하세요 :^ ?",
          "original_form": "안녕하세요! &name2&은 게임 좋아하세요 :^ ?",
          "speaker_id": "1",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100012685.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100012685.1.3",
          "form": "저는 아이패드로 모바일게임 종종해요!!",
          "original_form": "저는 아이패드로 모바일게임 종종해요!!",
          "speaker_id": "2",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100012685.1.4",
          "form": "어떤 게임 하세요~?!",
          "original_form": "어떤 게임 하세요~?!",
          "speaker_id": "1",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100012685.1.5",
          "form": "마이리틀셰프라고 타이쿤같은 게임이요!",
          "original_form": "마이리틀셰프라고 타이쿤같은 게임이요!",
          "speaker_id": "2",
          "time": "20210825 00:00"
        },
        {
          "id": "MDRW2100012685.1.6",
          "form": "name1은요??",
          "original_form": "&name1&은요??",
          "speaker_id": "2",
          "time": "20210825 00:00"
        },
        {
          "id": "MDRW2100012685.1.7",
          "form": "저는 쿠킹덤 진짜 열심히 해요...",
          "original_form": "저는 쿠킹덤 진짜 열심히 해요...",
          "speaker_id": "1",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100012685.1.8",
          "form": "앗 그리고 스도쿠도요 ㅎㅎ!",
          "original_form": "앗 그리고 스도쿠도요 ㅎㅎ!",
          "speaker_id": "1",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100012685.1.9",
          "form": "아 쿠킹덤!해보고 싶은데 엄두가안나요 ㅎㅎ",
          "original_form": "아 쿠킹덤!해보고 싶은데 엄두가안나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100012685.1.10",
          "form": "뭔가 엄청 공략적으로 해야할거같아서ㅜㅜ 안어렵나요??",
          "original_form": "뭔가 엄청 공략적으로 해야할거같아서ㅜㅜ 안어렵나요??",
          "speaker_id": "2",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100012685.1.11",
          "form": "헉 전 되게 아무 생각없이 해요!!!!!!!",
          "original_form": "헉 전 되게 아무 생각없이 해요!!!!!!!",
          "speaker_id": "1",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100012685.1.12",
          "form": "약간 노동게임에 가까워요....",
          "original_form": "약간 노동게임에 가까워요....",
          "speaker_id": "1",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100012685.1.13",
          "form": "열심히 게임하고 쿠키 키우는!",
          "original_form": "열심히 게임하고 쿠키 키우는!",
          "speaker_id": "1",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100012685.1.14",
          "form": "해보시면 진짜 재밌을 거예요 희희",
          "original_form": "해보시면 진짜 재밌을 거예요 희희",
          "speaker_id": "1",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100012685.1.15",
          "form": "하 제가 한번빠지면 답도없어서 나중에 깔아야겠네요ㅠㅠㅎㅎ",
          "original_form": "하 제가 한번빠지면 답도없어서 나중에 깔아야겠네요ㅠㅠㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100012685.1.16",
          "form": "캐릭터가 넘 귀엽더라구요",
          "original_form": "캐릭터가 넘 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100012685.1.17",
          "form": "헉 그럼 큰일일걸요 저 정말 이주내내",
          "original_form": "헉 그럼 큰일일걸요 저 정말 이주내내",
          "speaker_id": "1",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100012685.1.18",
          "form": "잠 줄여가며 게임했어요",
          "original_form": "잠 줄여가며 게임했어요",
          "speaker_id": "1",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100012685.1.19",
          "form": "자랑은 아니지만 ㅎ.ㅎ",
          "original_form": "자랑은 아니지만 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100012685.1.20",
          "form": "그럼 진짜 나중에 시간 많을때 깔아야겠서요..ㅜㅜ",
          "original_form": "그럼 진짜 나중에 시간 많을때 깔아야겠서요..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012685.1.21",
          "form": "또 다른게임 스도쿠말고 하시능거 있나요??",
          "original_form": "또 다른게임 스도쿠말고 하시능거 있나요??",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012685.1.22",
          "form": "그 2080인가요 그",
          "original_form": "그 2080인가요 그",
          "speaker_id": "1",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012685.1.23",
          "form": "2배수로 블럭 쌓는 거요 ㅎㅎ!",
          "original_form": "2배수로 블럭 쌓는 거요 ㅎㅎ!",
          "speaker_id": "1",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012685.1.24",
          "form": "아아 알져 그것두 중독성있는..!!!",
          "original_form": "아아 알져 그것두 중독성있는..!!!",
          "speaker_id": "2",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012685.1.25",
          "form": "맞아요... 전 약간 중독성 있는 것만 하는 거 같아요..",
          "original_form": "맞아요... 전 약간 중독성 있는 것만 하는 거 같아요..",
          "speaker_id": "1",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012685.1.26",
          "form": "그래서 자꾸 잠이 줄오드는..",
          "original_form": "그래서 자꾸 잠이 줄오드는..",
          "speaker_id": "1",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012685.1.27",
          "form": "마자요.. 그럼 프로젝트런웨이라고 아세요??",
          "original_form": "마자요.. 그럼 프로젝트런웨이라고 아세요??",
          "speaker_id": "2",
          "time": "20210825 00:18"
        },
        {
          "id": "MDRW2100012685.1.28",
          "form": "저는 무슨 옷입히기같은건줄 알고 받았는데",
          "original_form": "저는 무슨 옷입히기같은건줄 알고 받았는데",
          "speaker_id": "2",
          "time": "20210825 00:18"
        },
        {
          "id": "MDRW2100012685.1.29",
          "form": "완전 즁독성있는 퍼즐이에여..심심할때 해보세여..",
          "original_form": "완전 즁독성있는 퍼즐이에여..심심할때 해보세여..",
          "speaker_id": "2",
          "time": "20210825 00:18"
        },
        {
          "id": "MDRW2100012685.1.30",
          "form": "헐!!! 아뇨!!!!!?",
          "original_form": "헐!!! 아뇨!!!!!?",
          "speaker_id": "1",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100012685.1.31",
          "form": "해봐야겠네요! 옷을 퍼즐로 입히는 건가요!?",
          "original_form": "해봐야겠네요! 옷을 퍼즐로 입히는 건가요!?",
          "speaker_id": "1",
          "time": "20210825 12:09"
        },
        {
          "id": "MDRW2100012685.1.32",
          "form": "아뇨아뇽ㅎㅎ 퍼즐게임해서 깨면 돈을주는데",
          "original_form": "아뇨아뇽ㅎㅎ 퍼즐게임해서 깨면 돈을주는데",
          "speaker_id": "2",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100012685.1.33",
          "form": "그돈모아서 옷사주는거에요!!근데 꾸미는건 진짜 조금이고 퍼즐이99퍼에요 ㅋㅋ",
          "original_form": "그돈모아서 옷사주는거에요!!근데 꾸미는건 진짜 조금이고 퍼즐이99퍼에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100012685.1.34",
          "form": "헐 재밌겠다! 저 그거랑 비슷하게",
          "original_form": "헐 재밌겠다! 저 그거랑 비슷하게",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100012685.1.35",
          "form": "꿈의 집 아시나요?",
          "original_form": "꿈의 집 아시나요?",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100012685.1.36",
          "form": "그것도 쟈밌게 했거든요!",
          "original_form": "그것도 쟈밌게 했거든요!",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100012685.1.37",
          "form": "앗 그거랑 비슷하게 꿈의 집이란 퍼즐게임",
          "original_form": "앗 그거랑 비슷하게 꿈의 집이란 퍼즐게임",
