      "utterance": [
        {
          "id": "MDRW2100018741.1.1",
          "form": "나중에 재밌는 책 있으면 추천해주세요 ㅎ",
          "original_form": "나중에 재밌는 책 있으면 추천해주세요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100018741.1.2",
          "form": "네 그럴게요ㅋㅋ",
          "original_form": "네 그럴게요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100018741.1.3",
          "form": "사실 저보다 저희 오빠가 책을 더 좋아해서",
          "original_form": "사실 저보다 저희 오빠가 책을 더 좋아해서",
          "speaker_id": "1",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100018741.1.4",
          "form": "아 오빠 있으세요?",
          "original_form": "아 오빠 있으세요?",
          "speaker_id": "2",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100018741.1.5",
          "form": "네 저는 오빠 2명이나 있어요 ㅎㅎ",
          "original_form": "네 저는 오빠 2명이나 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100018741.1.6",
          "form": "작은 오빠는 지금 군대 가있구요",
          "original_form": "작은 오빠는 지금 군대 가있구요",
          "speaker_id": "1",
          "time": "20210827 11:57"
        },
        {
          "id": "MDRW2100018741.1.7",
          "form": "아~ㅠ 군대에 가있군요",
          "original_form": "아~ㅠ 군대에 가있군요",
          "speaker_id": "2",
          "time": "20210827 11:57"
        },
        {
          "id": "MDRW2100018741.1.8",
          "form": "많이 걱정되시겠다",
          "original_form": "많이 걱정되시겠다",
          "speaker_id": "2",
          "time": "20210827 11:57"
        },
        {
          "id": "MDRW2100018741.1.9",
          "form": "네... 군생활 하다 혹시나 다치지는 않을지 걱정되네요",
          "original_form": "네... 군생활 하다 혹시나 다치지는 않을지 걱정되네요",
          "speaker_id": "1",
          "time": "20210827 11:57"
        },
        {
          "id": "MDRW2100018741.1.10",
          "form": "육군으로 갔어요?",
          "original_form": "육군으로 갔어요?",
          "speaker_id": "2",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100018741.1.11",
          "form": "네 육군이에요",
          "original_form": "네 육군이에요",
          "speaker_id": "1",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100018741.1.12",
          "form": "지금은 철산에 있대요",
          "original_form": "지금은 철산에 있대요",
          "speaker_id": "1",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100018741.1.13",
          "form": "아이고 멀리 갔네요",
          "original_form": "아이고 멀리 갔네요",
          "speaker_id": "2",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100018741.1.14",
          "form": "군 생활은 할만 하다던가요?",
          "original_form": "군 생활은 할만 하다던가요?",
          "speaker_id": "2",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100018741.1.15",
          "form": "네 다향히 그럭저럭이래요ㅋㅋㅋㅋ",
          "original_form": "네 다향히 그럭저럭이래요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100018741.1.16",
          "form": "동기들도 다들 좋고 선임들도 착하다네요",
          "original_form": "동기들도 다들 좋고 선임들도 착하다네요",
          "speaker_id": "1",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100018741.1.17",
          "form": "아 그럼 안심이네요",
          "original_form": "아 그럼 안심이네요",
          "speaker_id": "2",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100018741.1.18",
          "form": "선임들이 후임들 많이 갈구잖아요",
          "original_form": "선임들이 후임들 많이 갈구잖아요",
          "speaker_id": "2",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100018741.1.19",
          "form": "제 아들도 군대 갔었는데 그거 땜에 고생 좀 했어요",
          "original_form": "제 아들도 군대 갔었는데 그거 땜에 고생 좀 했어요",
          "speaker_id": "2",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100018741.1.20",
          "form": "아하ㅠㅠ 안타깝네요",
          "original_form": "아하ㅠㅠ 안타깝네요",
          "speaker_id": "1",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100018741.1.21",
          "form": "군대 내 부조리가 많이 없어졌다곤 하지만",
          "original_form": "군대 내 부조리가 많이 없어졌다곤 하지만",
          "speaker_id": "1",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100018741.1.22",
          "form": "아직도 많은거 같아요 솔직히",
          "original_form": "아직도 많은거 같아요 솔직히",
          "speaker_id": "1",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100018741.1.23",
          "form": "맞아요 그래서 부모된 입장으로서는",
          "original_form": "맞아요 그래서 부모된 입장으로서는",
          "speaker_id": "2",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100018741.1.24",
          "form": "넘 걱정되지요",
          "original_form": "넘 걱정되지요",
          "speaker_id": "2",
          "time": "20210827 12:02"
        },
        {
          "id": "MDRW2100018741.1.25",
          "form": "우리 오빠도 무사히 잘 전역했으면 좋겠네요",
          "original_form": "우리 오빠도 무사히 잘 전역했으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210827 12:02"
        }
      ]
    }
  ]
