      "utterance": [
        {
          "id": "MDRW2100045531.1.1",
          "form": "저희 눈오리 눈누사람 눈하트 애기들 눈싸움 얘기햌ㅅ어요",
          "original_form": "저희 눈오리 눈누사람 눈하트 애기들 눈싸움 얘기햌ㅅ어요",
          "speaker_id": "1",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100045531.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100045531.1.3",
          "form": "이거 너무 재밌어요 생각보다",
          "original_form": "이거 너무 재밌어요 생각보다",
          "speaker_id": "2",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100045531.1.4",
          "form": "맞아요 근데 문제는 오늘 오전에 끝난다는거죠",
          "original_form": "맞아요 근데 문제는 오늘 오전에 끝난다는거죠",
          "speaker_id": "1",
          "time": "20210908 03:17"
        },
        {
          "id": "MDRW2100045531.1.5",
          "form": "너무 힘들어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무 힘들어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:17"
        },
        {
          "id": "MDRW2100045531.1.6",
          "form": "7시에 일어나야하는데",
          "original_form": "7시에 일어나야하는데",
          "speaker_id": "1",
          "time": "20210908 03:17"
        },
        {
          "id": "MDRW2100045531.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100045531.1.8",
          "form": "조금빨리 시작할걸!",
          "original_form": "조금빨리 시작할걸!",
          "speaker_id": "2",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100045531.1.9",
          "form": "내일은 점심은 돈가스 먹을거에요...",
          "original_form": "내일은 점심은 돈가스 먹을거에요...",
          "speaker_id": "2",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100045531.1.10",
          "form": "무슨 돈까슨가용",
          "original_form": "무슨 돈까슨가용",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100045531.1.11",
          "form": "저희 집 앞에 완전 맛있는 돈까스집이 있거든요!",
          "original_form": "저희 집 앞에 완전 맛있는 돈까스집이 있거든요!",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045531.1.12",
          "form": "할",
          "original_form": "할",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045531.1.13",
          "form": "어디에요?",
          "original_form": "어디에요?",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045531.1.14",
          "form": "뭔지는 잘 모르는데 매운소스가 있는게 정말 맛있어요",
          "original_form": "뭔지는 잘 모르는데 매운소스가 있는게 정말 맛있어요",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045531.1.15",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045531.1.16",
          "form": "패스요",
          "original_form": "패스요",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045531.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045531.1.18",
          "form": "매운거 못먹어요 ㅠ",
          "original_form": "매운거 못먹어요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045531.1.19",
          "form": "매운거 잘드시나보네요!?",
          "original_form": "매운거 잘드시나보네요!?",
          "speaker_id": "1",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045531.1.20",
          "form": "앗!",
          "original_form": "앗!",
          "speaker_id": "2",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045531.1.21",
          "form": "저는 매운거 정말 좋아해요",
          "original_form": "저는 매운거 정말 좋아해요",
          "speaker_id": "2",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045531.1.22",
          "form": "근데 잘먹는다고 하기엔 조금 부족한편?",
          "original_form": "근데 잘먹는다고 하기엔 조금 부족한편?",
          "speaker_id": "2",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045531.1.23",
          "form": "마라같은것도 정말 좋아하는데",
          "original_form": "마라같은것도 정말 좋아하는데",
          "speaker_id": "2",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045531.1.24",
          "form": "뭐든 .. 매운건 너무 매워요... 너무",
          "original_form": "뭐든 .. 매운건 너무 매워요... 너무",
          "speaker_id": "2",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045531.1.25",
          "form": "ㅋㅋㅋㅋ 매운건 통각이래요 알고 계세요?",
          "original_form": "ㅋㅋㅋㅋ 매운건 통각이래요 알고 계세요?",
          "speaker_id": "1",
          "time": "20210908 03:23"
        },
        {
          "id": "MDRW2100045531.1.26",
          "form": "알아요.. 그래서 매운걸 먹고나면 다음날 고통스러운걸까요?..",
          "original_form": "알아요.. 그래서 매운걸 먹고나면 다음날 고통스러운걸까요?..",
          "speaker_id": "2",
          "time": "20210908 03:24"
        },
        {
          "id": "MDRW2100045531.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아악",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아악",
          "speaker_id": "1",
          "time": "20210908 03:24"
        },
        {
          "id": "MDRW2100045531.1.28",
          "form": "돈고를 소중히 하십시오",
          "original_form": "돈고를 소중히 하십시오",
          "speaker_id": "1",
          "time": "20210908 03:24"
        },
        {
          "id": "MDRW2100045531.1.29",
          "form": "name2님 배고프지 않으세요?",
          "original_form": "&name2&님 배고프지 않으세요?",
