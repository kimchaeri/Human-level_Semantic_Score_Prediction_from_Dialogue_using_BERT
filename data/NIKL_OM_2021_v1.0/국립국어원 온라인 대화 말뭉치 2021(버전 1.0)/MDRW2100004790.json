      "utterance": [
        {
          "id": "MDRW2100004790.1.1",
          "form": "스트레스 받을 때마다 자꾸 사고싶어져",
          "original_form": "스트레스 받을 때마다 자꾸 사고싶어져",
          "speaker_id": "2",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004790.1.2",
          "form": "이게 쇼핑중독일까?",
          "original_form": "이게 쇼핑중독일까?",
          "speaker_id": "2",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004790.1.3",
          "form": "편의점 천원상품권있으면 뭐 사먹을거야?",
          "original_form": "편의점 천원상품권있으면 뭐 사먹을거야?",
          "speaker_id": "1",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004790.1.4",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004790.1.5",
          "form": "아ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004790.1.6",
          "form": "라면!",
          "original_form": "라면!",
          "speaker_id": "2",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004790.1.7",
          "form": "근데 천원으로 살수있는거ㅣ많이없어ㅠ",
          "original_form": "근데 천원으로 살수있는거ㅣ많이없어ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004790.1.8",
          "form": "겨우 한끼 해결함ㅠ",
          "original_form": "겨우 한끼 해결함ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004790.1.9",
          "form": "맞아. 탄산수도 1200원인데!",
          "original_form": "맞아. 탄산수도 1200원인데!",
          "speaker_id": "2",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004790.1.10",
          "form": "심지어 김밥도 이천원대라구",
          "original_form": "심지어 김밥도 이천원대라구",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004790.1.11",
          "form": "물가 너무 비싸",
          "original_form": "물가 너무 비싸",
          "speaker_id": "2",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004790.1.12",
          "form": "슬픔ㅠ",
          "original_form": "슬픔ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004790.1.13",
          "form": "문구류도 맘 놓고 못사겠어",
          "original_form": "문구류도 맘 놓고 못사겠어",
          "speaker_id": "2",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004790.1.14",
          "form": "근데 나도 스트레스받으면 막 삼ㅠ",
          "original_form": "근데 나도 스트레스받으면 막 삼ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004790.1.15",
          "form": "사고 먹고ㅠ",
          "original_form": "사고 먹고ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004790.1.16",
          "form": "최근에 스트레스 받아서 산 거 있어?",
          "original_form": "최근에 스트레스 받아서 산 거 있어?",
          "speaker_id": "2",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004790.1.17",
          "form": "어제",
          "original_form": "어제",
          "speaker_id": "1",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004790.1.18",
          "form": "치킨",
          "original_form": "치킨",
          "speaker_id": "1",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004790.1.19",
          "form": "치킨은 인정이지",
          "original_form": "치킨은 인정이지",
          "speaker_id": "2",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004790.1.20",
          "form": "아니 이게 왜 나쁜말이냐",
          "original_form": "아니 이게 왜 나쁜말이냐",
          "speaker_id": "1",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004790.1.21",
          "form": "어제 치킨",
          "original_form": "어제 치킨",
          "speaker_id": "1",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004790.1.22",
          "form": "아니 대체 아까부터....",
          "original_form": "아니 대체 아까부터....",
          "speaker_id": "2",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004790.1.23",
          "form": "그것말고도",
          "original_form": "그것말고도",
          "speaker_id": "1",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004790.1.24",
          "form": "뭔가 힘들고 지치면 편의점에 들어감ㅋㅋㅋ",
          "original_form": "뭔가 힘들고 지치면 편의점에 들어감ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004790.1.25",
          "form": "마트나ㅋㅋ",
          "original_form": "마트나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004790.1.26",
          "form": "일단 들어감ㅋㅋ",
          "original_form": "일단 들어감ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004790.1.27",
          "form": "난 예전에",
          "original_form": "난 예전에",
          "speaker_id": "2",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004790.1.28",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004790.1.29",
          "form": "화나면 자꾸 펜을 샀어",
          "original_form": "화나면 자꾸 펜을 샀어",
          "speaker_id": "2",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004790.1.30",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004790.1.31",
          "form": "니가 전에 추천해준 휘발펜 너무 잘쓰고있어",
          "original_form": "니가 전에 추천해준 휘발펜 너무 잘쓰고있어",
          "speaker_id": "1",
