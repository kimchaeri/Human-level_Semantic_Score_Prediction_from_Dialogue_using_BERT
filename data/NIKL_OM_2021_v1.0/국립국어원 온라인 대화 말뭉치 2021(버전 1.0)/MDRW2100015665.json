      "utterance": [
        {
          "id": "MDRW2100015665.1.1",
          "form": "어 완전 많아",
          "original_form": "어 완전 많아",
          "speaker_id": "2",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015665.1.2",
          "form": "얼마나?",
          "original_form": "얼마나?",
          "speaker_id": "1",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015665.1.3",
          "form": "맨날 새벽 3 4시까지 과제해야해",
          "original_form": "맨날 새벽 3 4시까지 과제해야해",
          "speaker_id": "2",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015665.1.4",
          "form": "정말? 너 1교시 없어?",
          "original_form": "정말? 너 1교시 없어?",
          "speaker_id": "1",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015665.1.5",
          "form": "이번에 1교시 짱 많아ㅠ",
          "original_form": "이번에 1교시 짱 많아ㅠ",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015665.1.6",
          "form": "그러면 몇시에 일어나야해?",
          "original_form": "그러면 몇시에 일어나야해?",
          "speaker_id": "1",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015665.1.7",
          "form": "비대면이면 8시 50분에 일어나ㅋㅋㅋ",
          "original_form": "비대면이면 8시 50분에 일어나ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015665.1.8",
          "form": "헐.. 그러면 잠 별로 못자는거 아니야?",
          "original_form": "헐.. 그러면 잠 별로 못자는거 아니야?",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015665.1.9",
          "form": "ㅎ항상 에너지 음료를 달고살아..",
          "original_form": "ㅎ항상 에너지 음료를 달고살아..",
          "speaker_id": "2",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015665.1.10",
          "form": "매일 그러는건 아니지?",
          "original_form": "매일 그러는건 아니지?",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015665.1.11",
          "form": "맨날 그러는 건 아니구 일주일에 5일 정도!?",
          "original_form": "맨날 그러는 건 아니구 일주일에 5일 정도!?",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015665.1.12",
          "form": "그럼 학교가는 날은 매일인거아냐..?",
          "original_form": "그럼 학교가는 날은 매일인거아냐..?",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015665.1.13",
          "form": "과제전 할때는 학교에서 밤 새",
          "original_form": "과제전 할때는 학교에서 밤 새",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015665.1.14",
          "form": "과제전이 뭐야?",
          "original_form": "과제전이 뭐야?",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015665.1.15",
          "form": "중간이랑 기말고사때 자기가 한 작품 전시하는거얌",
          "original_form": "중간이랑 기말고사때 자기가 한 작품 전시하는거얌",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015665.1.16",
          "form": "오 바쁘겠다 재밌어?",
          "original_form": "오 바쁘겠다 재밌어?",
          "speaker_id": "1",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015665.1.17",
          "form": "재밌냐고!?!?!?!?",
          "original_form": "재밌냐고!?!?!?!?",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015665.1.18",
          "form": "재미있어?",
          "original_form": "재미있어?",
          "speaker_id": "1",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015665.1.19",
          "form": "아니!!!!!",
          "original_form": "아니!!!!!",
          "speaker_id": "2",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015665.1.20",
          "form": "왜..?",
          "original_form": "왜..?",
          "speaker_id": "1",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015665.1.21",
          "form": "과제한 거 남들한테 보여주는 거잖앙..",
          "original_form": "과제한 거 남들한테 보여주는 거잖앙..",
          "speaker_id": "2",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015665.1.22",
          "form": "창피하다궁ㅠ",
          "original_form": "창피하다궁ㅠ",
          "speaker_id": "2",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015665.1.23",
          "form": "아 부끄러워?",
          "original_form": "아 부끄러워?",
