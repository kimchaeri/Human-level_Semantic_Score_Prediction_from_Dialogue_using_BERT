        {
          "id": "MDRW2100010887.1.1",
          "form": "눈썹 문신 다시 하고시퍼",
          "original_form": "눈썹 문신 다시 하고시퍼",
          "speaker_id": "2",
          "time": "20210823 22:40"
        },
        {
          "id": "MDRW2100010887.1.2",
          "form": "하면 되지",
          "original_form": "하면 되지",
          "speaker_id": "1",
          "time": "20210823 22:40"
        },
        {
          "id": "MDRW2100010887.1.3",
          "form": "근데 그때 서울가서 한거라",
          "original_form": "근데 그때 서울가서 한거라",
          "speaker_id": "2",
          "time": "20210823 22:40"
        },
        {
          "id": "MDRW2100010887.1.4",
          "form": "아 주변엔 없나",
          "original_form": "아 주변엔 없나",
          "speaker_id": "1",
          "time": "20210823 22:40"
        },
        {
          "id": "MDRW2100010887.1.5",
          "form": "있긴있겠지만 맘에 안들거 같아서",
          "original_form": "있긴있겠지만 맘에 안들거 같아서",
          "speaker_id": "2",
          "time": "20210823 22:40"
        },
        {
          "id": "MDRW2100010887.1.6",
          "form": "얼마였어?",
          "original_form": "얼마였어?",
          "speaker_id": "1",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100010887.1.7",
          "form": "그때 거의 5년전인데 30이었음",
          "original_form": "그때 거의 5년전인데 30이었음",
          "speaker_id": "2",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100010887.1.8",
          "form": "짱 비싸다.......",
          "original_form": "짱 비싸다.......",
          "speaker_id": "1",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100010887.1.9",
          "form": "지금은 더 올랐겠지?",
          "original_form": "지금은 더 올랐겠지?",
          "speaker_id": "2",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100010887.1.10",
          "form": "그러게 막 50하는거 아님????",
          "original_form": "그러게 막 50하는거 아님????",
          "speaker_id": "1",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100010887.1.11",
          "form": "그래도 그냥 했으면 좋겠어",
          "original_form": "그래도 그냥 했으면 좋겠어",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100010887.1.12",
          "form": "짱 편한데",
          "original_form": "짱 편한데",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100010887.1.13",
          "form": "난 이마라인",
          "original_form": "난 이마라인",
          "speaker_id": "1",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100010887.1.14",
          "form": "이마라인도 하면 예쁘지",
          "original_form": "이마라인도 하면 예쁘지",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100010887.1.15",
          "form": "근데 계속해야하지 않나",
          "original_form": "근데 계속해야하지 않나",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100010887.1.16",
