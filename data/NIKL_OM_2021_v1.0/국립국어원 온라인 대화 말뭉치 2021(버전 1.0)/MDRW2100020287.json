      "utterance": [
        {
          "id": "MDRW2100020287.1.1",
          "form": "딱딱하실거같았는데 ㅋㅋㅋ",
          "original_form": "딱딱하실거같았는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020287.1.2",
          "form": "그래서 나중에 헬스장 가게되신다면",
          "original_form": "그래서 나중에 헬스장 가게되신다면",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020287.1.3",
          "form": "피티한번 받아보시는걸 추천해요!!",
          "original_form": "피티한번 받아보시는걸 추천해요!!",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020287.1.4",
          "form": "건강해지는 느낌도 있고",
          "original_form": "건강해지는 느낌도 있고",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020287.1.5",
          "form": "저도 나중에 돈모아서 해봐야겠어요 ㅋㅋㅋ",
          "original_form": "저도 나중에 돈모아서 해봐야겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020287.1.6",
          "form": "제친구는 나중에 거기서 배운거로 혼자 해도 된다더라구요",
          "original_form": "제친구는 나중에 거기서 배운거로 혼자 해도 된다더라구요",
          "speaker_id": "1",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020287.1.7",
          "form": "맞아요!!저도 까먹기전에",
          "original_form": "맞아요!!저도 까먹기전에",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020287.1.8",
          "form": "헬스장 자주가서 해야하는데...",
          "original_form": "헬스장 자주가서 해야하는데...",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020287.1.9",
          "form": "다까먹게생겼어요 ㅋㅋ",
          "original_form": "다까먹게생겼어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020287.1.10",
          "form": "ㅠㅠ 얼른 나아져야할텐데 걱정이에요",
          "original_form": "ㅠㅠ 얼른 나아져야할텐데 걱정이에요",
          "speaker_id": "1",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020287.1.11",
          "form": "ㅠㅠ운동하신거 있으세요??",
          "original_form": "ㅠㅠ운동하신거 있으세요??",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020287.1.12",
          "form": "저는 집에서 실내자전거만 매일 타요 ㅌㅋㅋ",
          "original_form": "저는 집에서 실내자전거만 매일 타요 ㅌㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020287.1.13",
          "form": "홈트같은것도 안하시나용??",
          "original_form": "홈트같은것도 안하시나용??",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020287.1.14",
          "form": "전에는 땅끄부부보면서 했는데",
          "original_form": "전에는 땅끄부부보면서 했는데",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020287.1.15",
          "form": "너무 힘들고 효과도모르겠어서..ㅋㅋㅋ 그냥 자전거만 타요 ㅋㅋㅋ",
          "original_form": "너무 힘들고 효과도모르겠어서..ㅋㅋㅋ 그냥 자전거만 타요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020287.1.16",
          "form": "홈트계의 대통령이시죠",
          "original_form": "홈트계의 대통령이시죠",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020287.1.17",
          "form": "근데저도 땅끄부부",
          "original_form": "근데저도 땅끄부부",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020287.1.18",
          "form": "넵ㅋㅋㅋ",
          "original_form": "넵ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020287.1.19",
          "form": "해봤는데 효과모르겠어요 ㅋㅋㅋㅋㅋ",
          "original_form": "해봤는데 효과모르겠어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020287.1.20",
          "form": "오 저만 그런게아니었군요 ㅋㅋㅋ",
          "original_form": "오 저만 그런게아니었군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020287.1.21",
          "form": "ㅋㅋㅋㅋ저희언니가 땅끄부부아저씨 뱃살보라고",
          "original_form": "ㅋㅋㅋㅋ저희언니가 땅끄부부아저씨 뱃살보라고",
