      "utterance": [
        {
          "id": "MDRW2100044544.1.1",
          "form": "저녁머드셨나여",
          "original_form": "저녁머드셨나여",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044544.1.2",
          "form": "제육이요",
          "original_form": "제육이요",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044544.1.3",
          "form": "근데또배고프네요",
          "original_form": "근데또배고프네요",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044544.1.4",
          "form": "와 제육은사랑입니다",
          "original_form": "와 제육은사랑입니다",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044544.1.5",
          "form": "ㅋㅋㅋㅋ 님은요?",
          "original_form": "ㅋㅋㅋㅋ 님은요?",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044544.1.6",
          "form": "전 빠른배달로 찜닭이요ㅋㅋ 1년만에 먹어본듯요",
          "original_form": "전 빠른배달로 찜닭이요ㅋㅋ 1년만에 먹어본듯요",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044544.1.7",
          "form": "우와찜닭",
          "original_form": "우와찜닭",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044544.1.8",
          "form": "무슨맛이요",
          "original_form": "무슨맛이요",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044544.1.9",
          "form": "아기때문에 간장순살이요ㅋㅋ\r\n치킨은 자주시키는데 찜닭은 진짜올만에 먹어봣어요ㅋ",
          "original_form": "아기때문에 간장순살이요ㅋㅋ\r\n치킨은 자주시키는데 찜닭은 진짜올만에 먹어봣어요ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044544.1.10",
          "form": "아항 간장",
          "original_form": "아항 간장",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044544.1.11",
          "form": "당면도 넣어서요?",
          "original_form": "당면도 넣어서요?",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044544.1.12",
          "form": "당면추가~~~예~~~~ㅋㅋㅋㅋ",
          "original_form": "당면추가~~~예~~~~ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044544.1.13",
          "form": "맛을 아시는분이네...",
          "original_form": "맛을 아시는분이네...",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044544.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ 먹고싶슴다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 먹고싶슴다",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044544.1.15",
          "form": "저도먹은지오래돼서..",
          "original_form": "저도먹은지오래돼서..",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044544.1.16",
          "form": "제육은 매콤한게 맛잇는데 그쳐",
          "original_form": "제육은 매콤한게 맛잇는데 그쳐",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044544.1.17",
          "form": "그죵 청양고추도 넣어서 먹엇어요",
          "original_form": "그죵 청양고추도 넣어서 먹엇어요",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044544.1.18",
          "form": "크... 최고네요\r\n최애음식있으세요?",
          "original_form": "크... 최고네요\r\n최애음식있으세요?",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044544.1.19",
          "form": "저요 파스타요",
          "original_form": "저요 파스타요",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044544.1.20",
