        },
        {
          "id": "MDRW2100015897.1.4",
          "form": "네 요새 모바일게임 엄청 열심히 하고 있어요",
          "original_form": "네 요새 모바일게임 엄청 열심히 하고 있어요",
          "speaker_id": "1",
          "time": "20210826 02:44"
        },
        {
          "id": "MDRW2100015897.1.5",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "2",
          "time": "20210826 02:46"
        },
        {
          "id": "MDRW2100015897.1.6",
          "form": "쿠키런킹덤 이요ㅎㅎㅎ",
          "original_form": "쿠키런킹덤 이요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 02:47"
        },
        {
          "id": "MDRW2100015897.1.7",
          "form": "혹시 게임 하세요?",
          "original_form": "혹시 게임 하세요?",
          "speaker_id": "1",
          "time": "20210826 02:48"
        },
        {
          "id": "MDRW2100015897.1.8",
          "form": "예전에요ㅎㅎ",
          "original_form": "예전에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 02:49"
        },
        {
          "id": "MDRW2100015897.1.9",
          "form": "예전엔 뭐 하셨어요?",
          "original_form": "예전엔 뭐 하셨어요?",
          "speaker_id": "1",
          "time": "20210826 02:49"
        },
        {
          "id": "MDRW2100015897.1.10",
          "form": "데바데 했어요",
          "original_form": "데바데 했어요",
          "speaker_id": "2",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100015897.1.11",
          "form": "아 저는 첨 들어보는 게임이네요",
          "original_form": "아 저는 첨 들어보는 게임이네요",
          "speaker_id": "1",
          "time": "20210826 02:52"
        },
        {
          "id": "MDRW2100015897.1.12",
          "form": "메이플 해보셨어요?",
          "original_form": "메이플 해보셨어요?",
          "speaker_id": "2",
          "time": "20210826 02:54"
        },
        {
          "id": "MDRW2100015897.1.13",
          "form": "아뇨..",
