      "utterance": [
        {
          "id": "MDRW2100035562.1.1",
          "form": "후식 드셔용?",
          "original_form": "후식 드셔용?",
          "speaker_id": "1",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100035562.1.2",
          "form": "네 아메리키노 좋아해요",
          "original_form": "네 아메리키노 좋아해요",
          "speaker_id": "2",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100035562.1.3",
          "form": "크으 쿠키나 케이크는요?",
          "original_form": "크으 쿠키나 케이크는요?",
          "speaker_id": "1",
          "time": "20210829 20:48"
        },
        {
          "id": "MDRW2100035562.1.4",
          "form": "평소엔 잘안먹는데 회사에서는 왜이렇게 맛있는지 모르겠네요ㅋㅋㅋ",
          "original_form": "평소엔 잘안먹는데 회사에서는 왜이렇게 맛있는지 모르겠네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:57"
        },
        {
          "id": "MDRW2100035562.1.5",
          "form": "앗ㅋㅋㅋㅋ 나가면 더 맛있는걸요",
          "original_form": "앗ㅋㅋㅋㅋ 나가면 더 맛있는걸요",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100035562.1.6",
          "form": "ㅋㅋㅋ그렇죠 나눠먹는게 맛있어서 그런걸까요",
          "original_form": "ㅋㅋㅋ그렇죠 나눠먹는게 맛있어서 그런걸까요",
          "speaker_id": "2",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100035562.1.7",
          "form": "아마 그럴거에요!",
          "original_form": "아마 그럴거에요!",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100035562.1.8",
          "form": "먹으면서 잠깐 대화까지 곁들이면 최고인걸용",
          "original_form": "먹으면서 잠깐 대화까지 곁들이면 최고인걸용",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100035562.1.9",
          "form": "일하는시간에 꿀같은 휴식이라 그런걸수도요ㅋㅋㅋㅋ 배달음식 잘시키세요?",
          "original_form": "일하는시간에 꿀같은 휴식이라 그런걸수도요ㅋㅋㅋㅋ 배달음식 잘시키세요?",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035562.1.10",
          "form": "그럼 최고긴하죠!",
          "original_form": "그럼 최고긴하죠!",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035562.1.11",
          "form": "한동안 너무 먹어서 요즘엔 자제하려고해요ㅋㅋㅠ",
          "original_form": "한동안 너무 먹어서 요즘엔 자제하려고해요ㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035562.1.12",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035562.1.13",
          "form": "저도 일주일에 1회안넘어가려고 노력해요~",
          "original_form": "저도 일주일에 1회안넘어가려고 노력해요~",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035562.1.14",
          "form": "근대지금못참고 야식을...",
          "original_form": "근대지금못참고 야식을...",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035562.1.15",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035562.1.16",
          "form": "어떤거 드셔용",
          "original_form": "어떤거 드셔용",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035562.1.17",
          "form": "치킨시켰어요ㅋㅋㅋ",
          "original_form": "치킨시켰어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035562.1.18",
          "form": "맥주!",
          "original_form": "맥주!",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035562.1.19",
          "form": "크으!",
          "original_form": "크으!",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035562.1.20",
          "form": "일욜을 깔끔하게 마무리하시려고 하시는군욥",
          "original_form": "일욜을 깔끔하게 마무리하시려고 하시는군욥",
          "speaker_id": "1",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100035562.1.21",
          "form": "ㅋㅋㅋㅋ한주의 마무리라고 해야될까요 배달너무 많이시키면 식비가 너무 많이 들지만 끊을순없내요",
          "original_form": "ㅋㅋㅋㅋ한주의 마무리라고 해야될까요 배달너무 많이시키면 식비가 너무 많이 들지만 끊을순없내요",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100035562.1.22",
          "form": "맞아요 삶의 일부인걸오?!",
          "original_form": "맞아요 삶의 일부인걸오?!",
          "speaker_id": "1",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100035562.1.23",
          "form": "저 지웠다가 다시 까는거보고 지우는게 시간낭비라는걸 깨달았어요ㅋㅋ",
          "original_form": "저 지웠다가 다시 까는거보고 지우는게 시간낭비라는걸 깨달았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100035562.1.24",
          "form": "ㅋㅋㅋㅋ작년에 지웠던거같은데 지금은 절대안그래요",
          "original_form": "ㅋㅋㅋㅋ작년에 지웠던거같은데 지금은 절대안그래요",
          "speaker_id": "2",
          "time": "20210829 22:51"
        }
      ]
    }
