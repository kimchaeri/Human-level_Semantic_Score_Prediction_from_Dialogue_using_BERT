      "utterance": [
        {
          "id": "MDRW2100006777.1.1",
          "form": "큰일이네 약 먹었어?",
          "original_form": "큰일이네 약 먹었어?",
          "speaker_id": "2",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100006777.1.2",
          "form": "아니 아직 안먹었어",
          "original_form": "아니 아직 안먹었어",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100006777.1.3",
          "form": "먹으며누하루가 힘들어",
          "original_form": "먹으며누하루가 힘들어",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100006777.1.4",
          "form": "날씨는 진짜 너무 좋은데",
          "original_form": "날씨는 진짜 너무 좋은데",
          "speaker_id": "2",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100006777.1.5",
          "form": "그르니까유",
          "original_form": "그르니까유",
          "speaker_id": "1",
          "time": "20210908 09:35"
        },
        {
          "id": "MDRW2100006777.1.6",
          "form": "지금도 콧물 주르륵",
          "original_form": "지금도 콧물 주르륵",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100006777.1.7",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100006777.1.8",
          "form": "많애는 아니고..",
          "original_form": "많애는 아니고..",
          "speaker_id": "1",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100006777.1.9",
          "form": "근데 많이들 훌쩍거리네",
          "original_form": "근데 많이들 훌쩍거리네",
          "speaker_id": "1",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100006777.1.10",
          "form": "비욤약좀 개발되면좋겠다 완치약",
          "original_form": "비욤약좀 개발되면좋겠다 완치약",
          "speaker_id": "1",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100006777.1.11",
          "form": "그거 개발되면 진짜 최고겠다",
          "original_form": "그거 개발되면 진짜 최고겠다",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100006777.1.12",
          "form": "왜 개발이 안 되지",
          "original_form": "왜 개발이 안 되지",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100006777.1.13",
          "form": "응 그러니까",
          "original_form": "응 그러니까",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100006777.1.14",
          "form": "이게 알라지성이라 그런가뱌",
          "original_form": "이게 알라지성이라 그런가뱌",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100006777.1.15",
          "form": "알러지 정복은 어렵나보오",
          "original_form": "알러지 정복은 어렵나보오",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100006777.1.16",
          "form": "그르니께유",
          "original_form": "그르니께유",
          "speaker_id": "1",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100006777.1.17",
          "form": "환절기만되면 흐느적",
          "original_form": "환절기만되면 흐느적",
          "speaker_id": "1",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100006777.1.18",
          "form": "게다가 마스크를 쓰니까ㅠ",
          "original_form": "게다가 마스크를 쓰니까ㅠ",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100006777.1.19",
          "form": "아냐",
          "original_form": "아냐",
          "speaker_id": "1",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100006777.1.20",
          "form": "마스크가ㅜ있어서 그나마 나아",
          "original_form": "마스크가ㅜ있어서 그나마 나아",
          "speaker_id": "1",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100006777.1.21",
          "form": "아 그런건가",
          "original_form": "아 그런건가",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100006777.1.22",
          "form": "가려지니까?",
          "original_form": "가려지니까?",
          "speaker_id": "2",
