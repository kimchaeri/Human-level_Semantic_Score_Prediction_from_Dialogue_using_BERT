      "utterance": [
        {
          "id": "MDRW2100002496.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002496.1.2",
          "form": "안녕하세요ㅎ 남자분이신가봐요ㅎ 군대다녀오셨어요?",
          "original_form": "안녕하세요ㅎ 남자분이신가봐요ㅎ 군대다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002496.1.3",
          "form": "저 여자입니다ㅎㅎㅎ주제가",
          "original_form": "저 여자입니다ㅎㅎㅎ주제가",
          "speaker_id": "1",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002496.1.4",
          "form": "저렇게되어있네요",
          "original_form": "저렇게되어있네요",
          "speaker_id": "1",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002496.1.5",
          "form": "군대는안가봤어요!",
          "original_form": "군대는안가봤어요!",
          "speaker_id": "1",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002496.1.6",
          "form": "군대 요즘 뉴스에 넘 많이 나와서 익숙하긴 하죠",
          "original_form": "군대 요즘 뉴스에 넘 많이 나와서 익숙하긴 하죠",
          "speaker_id": "2",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002496.1.7",
          "form": "맞아요 요즘은 군대에서",
          "original_form": "맞아요 요즘은 군대에서",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002496.1.8",
          "form": "핸드폰사용도 자유롭다고하더라구요..!",
          "original_form": "핸드폰사용도 자유롭다고하더라구요..!",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002496.1.9",
          "form": "세상좋아졌죠 ㅎ 보니까 월급도 많이주더라구요",
          "original_form": "세상좋아졌죠 ㅎ 보니까 월급도 많이주더라구요",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002496.1.10",
          "form": "오 얼마정도",
          "original_form": "오 얼마정도",
          "speaker_id": "1",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002496.1.11",
          "form": "준다나요?",
          "original_form": "준다나요?",
          "speaker_id": "1",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002496.1.12",
          "form": "정확히는 모르겠는데 40만원 넘지않나요?ㅎ",
          "original_form": "정확히는 모르겠는데 40만원 넘지않나요?ㅎ",
          "speaker_id": "2",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002496.1.13",
          "form": "예전 대학동기들 군대갔을땐 3만원 5만원 이랬던거같은데",
          "original_form": "예전 대학동기들 군대갔을땐 3만원 5만원 이랬던거같은데",
          "speaker_id": "2",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002496.1.14",
          "form": "헐 대박이네요 3만원",
          "original_form": "헐 대박이네요 3만원",
          "speaker_id": "1",
          "time": "20210820 13:13"
        },
        {
          "id": "MDRW2100002496.1.15",
          "form": "거의 10배가올랐군요..!",
          "original_form": "거의 10배가올랐군요..!",
          "speaker_id": "1",
          "time": "20210820 13:13"
        },
        {
          "id": "MDRW2100002496.1.16",
          "form": "ㅋㅋ 저도 정확히는 잘몰라요 군대안가봐서 ㅋ",
          "original_form": "ㅋㅋ 저도 정확히는 잘몰라요 군대안가봐서 ㅋ",
          "speaker_id": "2",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100002496.1.17",
          "form": "ㅋㅋㅋ 그러네요ㅋㅋ  저도군대는",
          "original_form": "ㅋㅋㅋ 그러네요ㅋㅋ  저도군대는",
          "speaker_id": "1",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100002496.1.18",
          "form": "티비프로그램으로만 봐서리",
          "original_form": "티비프로그램으로만 봐서리",
          "speaker_id": "1",
          "time": "20210820 13:15"
        },
        {
          "id": "MDRW2100002496.1.19",
          "form": "그래도 확실히 총들고  훈련하고하니까 군기는 잡나보더라구요 위험하니",
          "original_form": "그래도 확실히 총들고  훈련하고하니까 군기는 잡나보더라구요 위험하니",
          "speaker_id": "2",
          "time": "20210820 13:15"
        },
        {
          "id": "MDRW2100002496.1.20",
          "form": "훈련하는거보면 힘들거 같긴해요 무거운 짐들고 행군도 하고",
          "original_form": "훈련하는거보면 힘들거 같긴해요 무거운 짐들고 행군도 하고",
          "speaker_id": "2",
          "time": "20210820 13:15"
        },
        {
          "id": "MDRW2100002496.1.21",
          "form": "맞아요 행군이 진짜",
          "original_form": "맞아요 행군이 진짜",
          "speaker_id": "1",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002496.1.22",
          "form": "여름행군은 너무더울거같아요..",
          "original_form": "여름행군은 너무더울거같아요..",
          "speaker_id": "1",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002496.1.23",
          "form": "가을은 날이좋다쳐도",
          "original_form": "가을은 날이좋다쳐도",
          "speaker_id": "1",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002496.1.24",
          "form": "군인들은 다싫을듯요ㅋㅋ 어떤계절이여도",
          "original_form": "군인들은 다싫을듯요ㅋㅋ 어떤계절이여도",
