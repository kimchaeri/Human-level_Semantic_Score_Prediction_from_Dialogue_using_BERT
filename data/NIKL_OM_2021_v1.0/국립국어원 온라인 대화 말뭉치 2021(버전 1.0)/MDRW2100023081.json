      "utterance": [
        {
          "id": "MDRW2100023081.1.1",
          "form": "가을옷 사셨어요",
          "original_form": "가을옷 사셨어요",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023081.1.2",
          "form": "아니용",
          "original_form": "아니용",
          "speaker_id": "2",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023081.1.3",
          "form": "전 고민중이에요",
          "original_form": "전 고민중이에요",
          "speaker_id": "2",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023081.1.4",
          "form": "사실건가요",
          "original_form": "사실건가요",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023081.1.5",
          "form": "고민중!",
          "original_form": "고민중!",
          "speaker_id": "2",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023081.1.6",
          "form": "셔츠나 가디건 고민중이에요",
          "original_form": "셔츠나 가디건 고민중이에요",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023081.1.7",
          "form": "바로 겨울 될 거같은데",
          "original_form": "바로 겨울 될 거같은데",
          "speaker_id": "2",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023081.1.8",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023081.1.9",
          "form": "이번엔 코트 오래입고싶네요",
          "original_form": "이번엔 코트 오래입고싶네요",
          "speaker_id": "2",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023081.1.10",
          "form": "바로 패딩이겠죠",
          "original_form": "바로 패딩이겠죠",
          "speaker_id": "1",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023081.1.11",
          "form": "후",
          "original_form": "후",
          "speaker_id": "2",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023081.1.12",
          "form": "껴입더라도 코트입으실건가요",
          "original_form": "껴입더라도 코트입으실건가요",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100023081.1.13",
          "form": "네 이번엔 보이핏바지랑 니트류",
          "original_form": "네 이번엔 보이핏바지랑 니트류",
          "speaker_id": "2",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100023081.1.14",
          "form": "전 스키니랑 베스트 입고 돌아다니고싶네요",
          "original_form": "전 스키니랑 베스트 입고 돌아다니고싶네요",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100023081.1.15",
          "form": "와우 스키니...",
          "original_form": "와우 스키니...",
          "speaker_id": "2",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100023081.1.16",
          "form": "운동 열심히해서 덜렁살 없애려구요",
          "original_form": "운동 열심히해서 덜렁살 없애려구요",
          "speaker_id": "1",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100023081.1.17",
          "form": "아주 지방 빠짝 태우시려고요?",
          "original_form": "아주 지방 빠짝 태우시려고요?",
          "speaker_id": "2",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100023081.1.18",
          "form": "넹ㅋㅋ",
          "original_form": "넹ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100023081.1.19",
          "form": "응원할게요",
          "original_form": "응원할게요",
          "speaker_id": "2",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100023081.1.20",
          "form": "패딩은 많아서 니트류만 좀 더 사면 되겠어요",
          "original_form": "패딩은 많아서 니트류만 좀 더 사면 되겠어요",
          "speaker_id": "1",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100023081.1.21",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "2",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100023081.1.22",
          "form": "니트는 작년에 다 처분했어요",
          "original_form": "니트는 작년에 다 처분했어요",
          "speaker_id": "1",
          "time": "20210829 23:11"
        },
        {
          "id": "MDRW2100023081.1.23",
          "form": "좋은 거 사세용",
          "original_form": "좋은 거 사세용",
          "speaker_id": "2",
          "time": "20210829 23:11"
        },
        {
          "id": "MDRW2100023081.1.24",
          "form": "어떤 가디건 브랜드가 예쁠까요",
          "original_form": "어떤 가디건 브랜드가 예쁠까요",
          "speaker_id": "1",
          "time": "20210830 13:16"
        }
      ]
