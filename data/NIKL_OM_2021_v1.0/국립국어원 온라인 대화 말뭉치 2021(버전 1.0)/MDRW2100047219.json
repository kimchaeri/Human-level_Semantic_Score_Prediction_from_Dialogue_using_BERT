      "utterance": [
        {
          "id": "MDRW2100047219.1.1",
          "form": "아직도 밖에 비오나요?",
          "original_form": "아직도 밖에 비오나요?",
          "speaker_id": "1",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100047219.1.2",
          "form": "여긴그친거같아요",
          "original_form": "여긴그친거같아요",
          "speaker_id": "2",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100047219.1.3",
          "form": "습하고 덥네요",
          "original_form": "습하고 덥네요",
          "speaker_id": "2",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100047219.1.4",
          "form": "앗 진짜요?",
          "original_form": "앗 진짜요?",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100047219.1.5",
          "form": "여기도 그친 것같은데",
          "original_form": "여기도 그친 것같은데",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100047219.1.6",
          "form": "비와서 그런지 시원해요",
          "original_form": "비와서 그런지 시원해요",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100047219.1.7",
          "form": "집은 에어컨켰어요 선풍기도ㅎ",
          "original_form": "집은 에어컨켰어요 선풍기도ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100047219.1.8",
          "form": "오 완전 시원하겠어요",
          "original_form": "오 완전 시원하겠어요",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100047219.1.9",
          "form": "계절상 가을이지만",
          "original_form": "계절상 가을이지만",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100047219.1.10",
          "form": "진짜 가을 날씨가 얼른 왔으면 좋겠네요",
          "original_form": "진짜 가을 날씨가 얼른 왔으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100047219.1.11",
          "form": "비그치고남 선선해지겠죠?",
          "original_form": "비그치고남 선선해지겠죠?",
          "speaker_id": "2",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100047219.1.12",
          "form": "그러겠죠? 그랬으면 좋겠네요ㅎㅎ",
          "original_form": "그러겠죠? 그랬으면 좋겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100047219.1.13",
          "form": "name2님은 제일 좋아하는 계절이 무엇인가요?",
          "original_form": "&name2&님은 제일 좋아하는 계절이 무엇인가요?",
          "speaker_id": "1",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100047219.1.14",
          "form": "전 가을 봄 두계절이요",
          "original_form": "전 가을 봄 두계절이요",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100047219.1.15",
          "form": "name1님은?",
          "original_form": "&name1&님은?",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100047219.1.16",
          "form": "전 가을 겨울이요ㅎㅎ",
          "original_form": "전 가을 겨울이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100047219.1.17",
          "form": "가을이랑 봄은 짧아서 아쉬운 것같아요",
          "original_form": "가을이랑 봄은 짧아서 아쉬운 것같아요",
          "speaker_id": "1",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100047219.1.18",
          "form": "올해도 유독짧을것같은 예감이^^",
          "original_form": "올해도 유독짧을것같은 예감이^^",
          "speaker_id": "2",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100047219.1.19",
          "form": "갈수록 짧아지는 것같더라구요ㅠ",
          "original_form": "갈수록 짧아지는 것같더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100047219.1.20",
          "form": "비염은 없으세요? 그때가 간절기라 비염 있으신 분들이 힘들어하더라고요",
          "original_form": "비염은 없으세요? 그때가 간절기라 비염 있으신 분들이 힘들어하더라고요",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100047219.1.21",
          "form": "네 전 다행히 건강체질이여서 잔병치레는 안하더라구요",
          "original_form": "네 전 다행히 건강체질이여서 잔병치레는 안하더라구요",
          "speaker_id": "2",
          "time": "20210826 00:42"
        },
        {
          "id": "MDRW2100047219.1.22",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210826 00:43"
        },
        {
          "id": "MDRW2100047219.1.23",
          "form": "내일은 비 안온다던데",
          "original_form": "내일은 비 안온다던데",
          "speaker_id": "1",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100047219.1.24",
          "form": "비염있으신가요?",
          "original_form": "비염있으신가요?",
          "speaker_id": "2",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100047219.1.25",
          "form": "그랬으면 좋게썽요",
          "original_form": "그랬으면 좋게썽요",
          "speaker_id": "1",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100047219.1.26",
          "form": "전 없어요!",
          "original_form": "전 없어요!",
          "speaker_id": "1",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100047219.1.27",
          "form": "다행ㅎ 담주까진 비비비 던데요",
          "original_form": "다행ㅎ 담주까진 비비비 던데요",
          "speaker_id": "2",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100047219.1.28",
          "form": "비오는거 지긋지긋하네요",
          "original_form": "비오는거 지긋지긋하네요",
          "speaker_id": "2",
          "time": "20210826 00:45"
        },
        {
          "id": "MDRW2100047219.1.29",
          "form": "헐 태풍 때문이겠죠ㅠ?",
          "original_form": "헐 태풍 때문이겠죠ㅠ?",
          "speaker_id": "1",
          "time": "20210826 00:48"
        },
        {
          "id": "MDRW2100047219.1.30",
          "form": "그래도 이번 여름엔 장마가 짧아서 좋긴 했지만..ㅎㅎ",
          "original_form": "그래도 이번 여름엔 장마가 짧아서 좋긴 했지만..ㅎㅎ",
          "speaker_id": "1",
