      "utterance": [
        {
          "id": "MDRW2100018907.1.1",
          "form": "회사 다닐땐 점심 뭐나오나 기대 했었는데",
          "original_form": "회사 다닐땐 점심 뭐나오나 기대 했었는데",
          "speaker_id": "2",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018907.1.2",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018907.1.3",
          "form": "저희 회사 연령대가 다양해서ㅋㅋ",
          "original_form": "저희 회사 연령대가 다양해서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018907.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018907.1.5",
          "form": "나오는것도",
          "original_form": "나오는것도",
          "speaker_id": "2",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018907.1.6",
          "form": "다양하겠어요",
          "original_form": "다양하겠어요",
          "speaker_id": "2",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018907.1.7",
          "form": "그래서 샌드위치 나왔나봐요",
          "original_form": "그래서 샌드위치 나왔나봐요",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018907.1.8",
          "form": "의견이 분분하죠ㅋㅋ",
          "original_form": "의견이 분분하죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018907.1.9",
          "form": "끝나면 불금 ㅋㅋ",
          "original_form": "끝나면 불금 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018907.1.10",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018907.1.11",
          "form": "샌드위치는 거의 처음",
          "original_form": "샌드위치는 거의 처음",
          "speaker_id": "1",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018907.1.12",
          "form": "아하 직장의 꽃 불금",
          "original_form": "아하 직장의 꽃 불금",
          "speaker_id": "1",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018907.1.13",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018907.1.14",
          "form": "달리시나요?",
          "original_form": "달리시나요?",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018907.1.15",
          "form": "근데 요즘 회식 재미가 없어요",
          "original_form": "근데 요즘 회식 재미가 없어요",
          "speaker_id": "1",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018907.1.16",
          "form": "배달가나요?",
          "original_form": "배달가나요?",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018907.1.17",
          "form": "ㅋㅋ그렇죠",
          "original_form": "ㅋㅋ그렇죠",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018907.1.18",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018907.1.19",
          "form": "그냥 배달가야죠 ㅜㅜ",
          "original_form": "그냥 배달가야죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018907.1.20",
          "form": "회식하며 으쌰으쌰해야하는데",
          "original_form": "회식하며 으쌰으쌰해야하는데",
          "speaker_id": "1",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018907.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018907.1.22",
          "form": "전 모임 좋아하거든요ㅋ",
          "original_form": "전 모임 좋아하거든요ㅋ",
          "speaker_id": "1",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018907.1.23",
          "form": "저도 그랬어요",
          "original_form": "저도 그랬어요",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018907.1.24",
          "form": "코로나로 뚝 ㅡㅡ",
          "original_form": "코로나로 뚝 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018907.1.25",
          "form": "사라지고 ㅜㅜ",
          "original_form": "사라지고 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018907.1.26",
          "form": "활동적이신가봐요",
          "original_form": "활동적이신가봐요",
          "speaker_id": "1",
          "time": "20210827 13:32"
        }
      ]
