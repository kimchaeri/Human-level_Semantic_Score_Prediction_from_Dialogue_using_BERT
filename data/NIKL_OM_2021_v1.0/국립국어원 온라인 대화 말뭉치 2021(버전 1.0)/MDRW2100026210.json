      "utterance": [
        {
          "id": "MDRW2100026210.1.1",
          "form": "육퇴는하셨어요,?",
          "original_form": "육퇴는하셨어요,?",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026210.1.2",
          "form": "네네 ㅎㅎ 이미 햇죠",
          "original_form": "네네 ㅎㅎ 이미 햇죠",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026210.1.3",
          "form": "우와우와",
          "original_form": "우와우와",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026210.1.4",
          "form": "브럽네요ㅠㅠㅠ",
          "original_form": "브럽네요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026210.1.5",
          "form": "지금 애기가 물총놀이하고있어요ㅠㅠ 오늘은 언제잘련지ㅠㅠ",
          "original_form": "지금 애기가 물총놀이하고있어요ㅠㅠ 오늘은 언제잘련지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026210.1.6",
          "form": "노하우좀알려줘요ㅎㅎ",
          "original_form": "노하우좀알려줘요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026210.1.7",
          "form": "ㅎㅎㅎㅎ 귀엽네요 ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ 귀엽네요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026210.1.8",
          "form": "전 루틴데로 어릴때부터 ㅎㅎ",
          "original_form": "전 루틴데로 어릴때부터 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026210.1.9",
          "form": "그러니깐 습관이 중요한데",
          "original_form": "그러니깐 습관이 중요한데",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026210.1.10",
          "form": "8시되면 전체 소등이여 ㅎㅎ",
          "original_form": "8시되면 전체 소등이여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026210.1.11",
          "form": "우리애는 노는게 습관이에요ㅎㅎ",
          "original_form": "우리애는 노는게 습관이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026210.1.12",
          "form": "우와우와",
          "original_form": "우와우와",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026210.1.13",
          "form": "부럽당ㅍㅍ",
          "original_form": "부럽당ㅍㅍ",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026210.1.14",
          "form": "첨에 불꺼도 놀더니",
          "original_form": "첨에 불꺼도 놀더니",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026210.1.15",
          "form": "저희도 소등한번해야하나?",
          "original_form": "저희도 소등한번해야하나?",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026210.1.16",
          "form": "네네 분위기가 중요해요 ㅎㅎ",
          "original_form": "네네 분위기가 중요해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026210.1.17",
          "form": "아 그렇구나ㅠ",
          "original_form": "아 그렇구나ㅠ",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026210.1.18",
          "form": "자는척 하면 놀다 자고",
          "original_form": "자는척 하면 놀다 자고",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026210.1.19",
          "form": "저는 늦게자니깐",
          "original_form": "저는 늦게자니깐",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026210.1.20",
          "form": "근데 지금은 커서 점 힘들수도잇어여 ㅋ",
          "original_form": "근데 지금은 커서 점 힘들수도잇어여 ㅋ",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026210.1.21",
          "form": "애기가 영향이 있나봐요",
          "original_form": "애기가 영향이 있나봐요",
          "speaker_id": "2",
          "time": "20210901 20:54"
        }
      ]
    }
