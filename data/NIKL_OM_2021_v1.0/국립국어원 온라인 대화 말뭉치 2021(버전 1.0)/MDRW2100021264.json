      "utterance": [
        {
          "id": "MDRW2100021264.1.1",
          "form": "빅터?",
          "original_form": "빅터?",
          "speaker_id": "2",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021264.1.2",
          "form": "나는 이건명으로 함 보고싶어",
          "original_form": "나는 이건명으로 함 보고싶어",
          "speaker_id": "2",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021264.1.3",
          "form": "이건명두 노래잘해?",
          "original_form": "이건명두 노래잘해?",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021264.1.4",
          "form": "사실 그렇게 잘하는 거 같지는 않은데 그냥 한 번 그 사람의 연기를 보고싶어",
          "original_form": "사실 그렇게 잘하는 거 같지는 않은데 그냥 한 번 그 사람의 연기를 보고싶어",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021264.1.5",
          "form": "아하... 난 전동석 박은태로 보고싶어",
          "original_form": "아하... 난 전동석 박은태로 보고싶어",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021264.1.6",
          "form": "난 전동석 좀 느끼햌ㅋㅋ",
          "original_form": "난 전동석 좀 느끼햌ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100021264.1.7",
          "form": "왜냐면 그렇게 아니면 내돈주고 전동석 한번도 안볼 거 같아서..",
          "original_form": "왜냐면 그렇게 아니면 내돈주고 전동석 한번도 안볼 거 같아서..",
          "speaker_id": "1",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100021264.1.8",
          "form": "그렇긴하지...",
          "original_form": "그렇긴하지...",
          "speaker_id": "2",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021264.1.9",
          "form": "느끼하댘ㅋㅋ뭔지 알 거 같아ㅋㅋㅋ",
          "original_form": "느끼하댘ㅋㅋ뭔지 알 거 같아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021264.1.10",
          "form": "차라리 팬텀에서 은에릭에 전동석샹동이었으면 봤을거같지않아?",
          "original_form": "차라리 팬텀에서 은에릭에 전동석샹동이었으면 봤을거같지않아?",
          "speaker_id": "2",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021264.1.11",
          "form": "전동석도 성량이 좋대서 진짠지 한번 들어 보고싶어",
          "original_form": "전동석도 성량이 좋대서 진짠지 한번 들어 보고싶어",
          "speaker_id": "1",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021264.1.12",
          "form": "오 그거 괜찮다.. 근데 안하겠지..그렇게는..",
          "original_form": "오 그거 괜찮다.. 근데 안하겠지..그렇게는..",
          "speaker_id": "1",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021264.1.13",
          "form": "동석샹동이 캐릭터 더 좋았을거같아. 성악과라 성량 좋을걸?",
          "original_form": "동석샹동이 캐릭터 더 좋았을거같아. 성악과라 성량 좋을걸?",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100021264.1.14",
          "form": "전동석이 샹동안하려고할듯 ㅠ",
          "original_form": "전동석이 샹동안하려고할듯 ㅠ",
          "speaker_id": "1",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100021264.1.15",
          "form": "전동석 지가 몬데..",
          "original_form": "전동석 지가 몬데..",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100021264.1.16",
          "form": "주인공요..",
          "original_form": "주인공요..",
          "speaker_id": "1",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100021264.1.17",
          "form": "지가 몬데 에릭이람...",
          "original_form": "지가 몬데 에릭이람...",
          "speaker_id": "2",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100021264.1.18",
          "form": "근데 지킬도햇는데 샹동할리가 없다궄ㅋㅋ",
          "original_form": "근데 지킬도햇는데 샹동할리가 없다궄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:12"
        }
