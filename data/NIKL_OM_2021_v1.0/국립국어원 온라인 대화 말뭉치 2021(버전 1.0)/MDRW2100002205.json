      "utterance": [
        {
          "id": "MDRW2100002205.1.1",
          "form": "종교있으세요? 전 무교에요 ㅎㅎ",
          "original_form": "종교있으세요? 전 무교에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002205.1.2",
          "form": "저도 무교요",
          "original_form": "저도 무교요",
          "speaker_id": "2",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002205.1.3",
          "form": "전 종교는 안갖게 되더라구요",
          "original_form": "전 종교는 안갖게 되더라구요",
          "speaker_id": "2",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002205.1.4",
          "form": "저도 어릴때 교회다녀보긴했는데",
          "original_form": "저도 어릴때 교회다녀보긴했는데",
          "speaker_id": "1",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002205.1.5",
          "form": "간식줘서 갔지 뜻이있어서 간건아니었어요 ㅋㅋㅋㅋ",
          "original_form": "간식줘서 갔지 뜻이있어서 간건아니었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002205.1.6",
          "form": "저도요 뭐 많이 주길래 갖죠 ㅋㅋ",
          "original_form": "저도요 뭐 많이 주길래 갖죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002205.1.7",
          "form": "맞아요 다들 그럴걸요",
          "original_form": "맞아요 다들 그럴걸요",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002205.1.8",
          "form": "ㅋㅋㅋㅋㅋ부처님오신날에는 절가서 절밥먹고",
          "original_form": "ㅋㅋㅋㅋㅋ부처님오신날에는 절가서 절밥먹고",
          "speaker_id": "1",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002205.1.9",
          "form": "크리스마스엔 교회가서 간식먹고",
          "original_form": "크리스마스엔 교회가서 간식먹고",
          "speaker_id": "1",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002205.1.10",
          "form": "맞아요 절 비빔밥 맛나요",
          "original_form": "맞아요 절 비빔밥 맛나요",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002205.1.11",
          "form": "교회 크리스마스 최고죠",
          "original_form": "교회 크리스마스 최고죠",
          "speaker_id": "2",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002205.1.12",
          "form": "ㅋㅋㅋㅋㅋ네 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ네 맞아요",
          "speaker_id": "1",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002205.1.13",
          "form": "성경책도 어디 있었는데",
          "original_form": "성경책도 어디 있었는데",
          "speaker_id": "2",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002205.1.14",
          "form": "특히 절이그렇게 맛집이에여",
          "original_form": "특히 절이그렇게 맛집이에여",
          "speaker_id": "1",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002205.1.15",
          "form": "저도 ㅋㅋㅋㅋㅋ저 꽤 다니긴 오래다녔어요 애기때",
          "original_form": "저도 ㅋㅋㅋㅋㅋ저 꽤 다니긴 오래다녔어요 애기때",
          "speaker_id": "1",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002205.1.16",
          "form": "맞아요 그날 등도 이뿌고 맘도 배도 부르고",
          "original_form": "맞아요 그날 등도 이뿌고 맘도 배도 부르고",
          "speaker_id": "2",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002205.1.17",
          "form": "저도 쫌 다녔어요 유치원이 교회였어요",
          "original_form": "저도 쫌 다녔어요 유치원이 교회였어요",
          "speaker_id": "2",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002205.1.18",
          "form": "지금은 그냥 안다니는게 편해요",
          "original_form": "지금은 그냥 안다니는게 편해요",
          "speaker_id": "2",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002205.1.19",
          "form": "아 그럼 진짜 참여뭐 많이하잖아요 ㅎㅎ강제로",
          "original_form": "아 그럼 진짜 참여뭐 많이하잖아요 ㅎㅎ강제로",
          "speaker_id": "1",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002205.1.20",
          "form": "맞아요 지금은 힐링하러 절에는 가끔가요",
          "original_form": "맞아요 지금은 힐링하러 절에는 가끔가요",
          "speaker_id": "1",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002205.1.21",
          "form": "그렇죠 연극에 성경 외우기",
          "original_form": "그렇죠 연극에 성경 외우기",
          "speaker_id": "2",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002205.1.22",
          "form": "저도요 절에는가요",
          "original_form": "저도요 절에는가요",
          "speaker_id": "2",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002205.1.23",
          "form": "절은 좀 개방되어있는 관광지느낌이라 ㅎㅎ",
          "original_form": "절은 좀 개방되어있는 관광지느낌이라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002205.1.24",
          "form": "템플스테이도 한번해봤는데 넘좋더라구여",
          "original_form": "템플스테이도 한번해봤는데 넘좋더라구여",
          "speaker_id": "1",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002205.1.25",
          "form": "백팔배아무도안시켰는데 나서서 또 하게되고 ㅋㅋ",
          "original_form": "백팔배아무도안시켰는데 나서서 또 하게되고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002205.1.26",
          "form": "맞아요 편하게 다가가고 템플 해보고 싶네요",
          "original_form": "맞아요 편하게 다가가고 템플 해보고 싶네요",
          "speaker_id": "2",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002205.1.27",
          "form": "엥 108배요 대단해요",
          "original_form": "엥 108배요 대단해요",
          "speaker_id": "2",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002205.1.28",
          "form": "네 ㅋㅋㅋㅋㅋ근데 옆에하시는분따라하니까 금방끝나더라구요",
          "original_form": "네 ㅋㅋㅋㅋㅋ근데 옆에하시는분따라하니까 금방끝나더라구요",
          "speaker_id": "1",
          "time": "20210820 00:38"
        },
        {
          "id": "MDRW2100002205.1.29",
          "form": "얼마안걸렸어요",
          "original_form": "얼마안걸렸어요",
          "speaker_id": "1",
          "time": "20210820 00:38"
        },
        {
          "id": "MDRW2100002205.1.30",
          "form": "불경음악?맞춰서하니까 ㅋㅋㅋ",
          "original_form": "불경음악?맞춰서하니까 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:38"
        },
        {
          "id": "MDRW2100002205.1.31",
          "form": "전 못할거예요  음악이 어디든 힘나게해요",
          "original_form": "전 못할거예요  음악이 어디든 힘나게해요",
          "speaker_id": "2",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002205.1.32",
          "form": "저도 못할줄알았는데 새벽에 되더라구여 신기했어요 ㅋㅋㅋ",
          "original_form": "저도 못할줄알았는데 새벽에 되더라구여 신기했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:40"
        }
