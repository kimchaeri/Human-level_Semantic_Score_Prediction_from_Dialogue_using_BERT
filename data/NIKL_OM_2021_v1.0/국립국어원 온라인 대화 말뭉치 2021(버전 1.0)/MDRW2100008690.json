      "utterance": [
        {
          "id": "MDRW2100008690.1.1",
          "form": "한드는 잘 안보세요?",
          "original_form": "한드는 잘 안보세요?",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008690.1.2",
          "form": "네 아무래도 티비가 없어서 잘 못봐요 제일 최근에본게 미스터션샤인이예요",
          "original_form": "네 아무래도 티비가 없어서 잘 못봐요 제일 최근에본게 미스터션샤인이예요",
          "speaker_id": "2",
          "time": "20210822 17:33"
        },
        {
          "id": "MDRW2100008690.1.3",
          "form": "name1님은 뭐 보셨어요?",
          "original_form": "&name1&님은 뭐 보셨어요?",
          "speaker_id": "2",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008690.1.4",
          "form": "미스터 션샤인 재밋죠 ㅠㅠ",
          "original_form": "미스터 션샤인 재밋죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 17:35"
        },
        {
          "id": "MDRW2100008690.1.5",
          "form": "저는 마지막화 떨랴서 아직도 못봤어요",
          "original_form": "저는 마지막화 떨랴서 아직도 못봤어요",
          "speaker_id": "1",
          "time": "20210822 17:35"
        },
        {
          "id": "MDRW2100008690.1.6",
          "form": "최근 로스쿨 쟈밌개 봤네요!!",
          "original_form": "최근 로스쿨 쟈밌개 봤네요!!",
          "speaker_id": "1",
          "time": "20210822 17:35"
        },
        {
          "id": "MDRW2100008690.1.7",
          "form": "헉 아직 못보셨구나 저는 개인적으로 휘몰아치는거 좋아해서 좋았어요",
          "original_form": "헉 아직 못보셨구나 저는 개인적으로 휘몰아치는거 좋아해서 좋았어요",
          "speaker_id": "2",
          "time": "20210822 17:44"
        },
        {
          "id": "MDRW2100008690.1.8",
          "form": "로스쿨! 대사량 엄청많은 움짤은 봤었는데",
          "original_form": "로스쿨! 대사량 엄청많은 움짤은 봤었는데",
          "speaker_id": "2",
          "time": "20210822 17:44"
        },
        {
          "id": "MDRW2100008690.1.9",
          "form": "나중에 시간나면 봐야겠네요",
          "original_form": "나중에 시간나면 봐야겠네요",
          "speaker_id": "2",
          "time": "20210822 17:44"
        },
        {
          "id": "MDRW2100008690.1.10",
          "form": "제가 고통스러운걸 잘 못보거든요",
          "original_form": "제가 고통스러운걸 잘 못보거든요",
          "speaker_id": "1",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008690.1.11",
          "form": "해피엔딩을 무적 사랑하죠..",
          "original_form": "해피엔딩을 무적 사랑하죠..",
          "speaker_id": "1",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008690.1.12",
          "form": "슬의도 되게 재밌게 봤어요",
          "original_form": "슬의도 되게 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008690.1.13",
          "form": "노래는 뭐 좋아하시나요?",
          "original_form": "노래는 뭐 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008690.1.14",
          "form": "최근에는 창귀라는 안예은씨 노래를 듣고있어요",
          "original_form": "최근에는 창귀라는 안예은씨 노래를 듣고있어요",
          "speaker_id": "2",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008690.1.15",
          "form": "오 안예은씨 곡 좋아하는데 새로나온 노래인가요?",
          "original_form": "오 안예은씨 곡 좋아하는데 새로나온 노래인가요?",
          "speaker_id": "1",
          "time": "20210822 18:41"
        },
        {
          "id": "MDRW2100008690.1.16",
          "form": "네 근데 좀 무서운곳이라서 깜놀은 없지만 주의해서 들어야해요",
          "original_form": "네 근데 좀 무서운곳이라서 깜놀은 없지만 주의해서 들어야해요",
          "speaker_id": "2",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008690.1.17",
          "form": "곳이래 곡이라",
          "original_form": "곳이래 곡이라",
          "speaker_id": "2",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008690.1.18",
          "form": "오오 더 흥미롭네요",
          "original_form": "오오 더 흥미롭네요",
          "speaker_id": "1",
          "time": "20210822 19:08"
        },
        {
          "id": "MDRW2100008690.1.19",
          "form": "노래가 무섭다니 뭔가 독특할것같아요!",
          "original_form": "노래가 무섭다니 뭔가 독특할것같아요!",
          "speaker_id": "1",
          "time": "20210822 19:08"
        },
        {
          "id": "MDRW2100008690.1.20",
          "form": "저는 악뮤 신곡 열심히 들어요",
          "original_form": "저는 악뮤 신곡 열심히 들어요",
          "speaker_id": "1",
          "time": "20210822 19:08"
        },
        {
          "id": "MDRW2100008690.1.21",
          "form": "악뮤 언제나 좋죠!",
          "original_form": "악뮤 언제나 좋죠!",
          "speaker_id": "2",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008690.1.22",
          "form": "낙화말씀하시는건가요? 이번껀 안들어봤는데",
          "original_form": "낙화말씀하시는건가요? 이번껀 안들어봤는데",
          "speaker_id": "2",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008690.1.23",
          "form": "낙하요!",
          "original_form": "낙하요!",
          "speaker_id": "1",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008690.1.24",
          "form": "어우 중독성이 장난아니에요",
          "original_form": "어우 중독성이 장난아니에요",
          "speaker_id": "1",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008690.1.25",
          "form": "지금도 제 머릿속애서 핫둘셋 낙하~ 우우~",
          "original_form": "지금도 제 머릿속애서 핫둘셋 낙하~ 우우~",
          "speaker_id": "1",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008690.1.26",
          "form": "이러고있어요",
          "original_form": "이러고있어요",
          "speaker_id": "1",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008690.1.27",
          "form": "앜ㅋㅋㅋㅋ악뮤노래 중독성 장난없죠",
          "original_form": "앜ㅋㅋㅋㅋ악뮤노래 중독성 장난없죠",
          "speaker_id": "2",
          "time": "20210822 19:59"
        },
        {
          "id": "MDRW2100008690.1.28",
          "form": "한번 들어보셔요 ㅋㅋㅋ",
          "original_form": "한번 들어보셔요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:29"
        }
      ]
