        {
          "id": "MDRW2100032518.1.1",
          "form": "name2님 안녕하세용",
          "original_form": "&name2&님 안녕하세용",
          "speaker_id": "1",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100032518.1.2",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "2",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100032518.1.3",
          "form": "최근에 뭐 산거 있으세요?",
          "original_form": "최근에 뭐 산거 있으세요?",
          "speaker_id": "1",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100032518.1.4",
          "form": "오옹 갤럭시워치4를 샀어요!",
          "original_form": "오옹 갤럭시워치4를 샀어요!",
          "speaker_id": "2",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100032518.1.5",
          "form": "예약판매할때 냅다 구입했죵",
          "original_form": "예약판매할때 냅다 구입했죵",
          "speaker_id": "2",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100032518.1.6",
          "form": "너무 잘쓰고잇는데",
          "original_form": "너무 잘쓰고잇는데",
          "speaker_id": "2",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100032518.1.7",
          "form": "오!!! 저도 눈여겨봤었는데 어때요?",
          "original_form": "오!!! 저도 눈여겨봤었는데 어때요?",
          "speaker_id": "1",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100032518.1.8",
          "form": "필름도사야할까봐요",
          "original_form": "필름도사야할까봐요",
          "speaker_id": "2",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100032518.1.9",
          "form": "너무 좋아용 맨날 끼고자면",
          "original_form": "너무 좋아용 맨날 끼고자면",
          "speaker_id": "2",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100032518.1.10",
          "form": "다음날 수면패턴도 알려주고",
          "original_form": "다음날 수면패턴도 알려주고",
          "speaker_id": "2",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100032518.1.11",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100032518.1.12",
          "form": "혈압이랑 인바디도잴수있고",
          "original_form": "혈압이랑 인바디도잴수있고",
          "speaker_id": "2",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100032518.1.13",
          "form": "짱좋아요",
          "original_form": "짱좋아요",
          "speaker_id": "2",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100032518.1.14",
          "form": "차고 자면 조금 불편하지 않으세요?",
          "original_form": "차고 자면 조금 불편하지 않으세요?",
          "speaker_id": "1",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100032518.1.15",
          "form": "허걱 아뇨괜찮아요",
          "original_form": "허걱 아뇨괜찮아요",
          "speaker_id": "2",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100032518.1.16",
          "form": "그냥 스마트폰을 옆에끼고자는느낌이죵",
          "original_form": "그냥 스마트폰을 옆에끼고자는느낌이죵",
          "speaker_id": "2",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100032518.1.17",
          "form": "오오 신기하네용",
          "original_form": "오오 신기하네용",
          "speaker_id": "1",
          "time": "20210905 20:04"
        },
        {
          "id": "MDRW2100032518.1.18",
          "form": "필름은 액정필름 말하시는거죠?",
          "original_form": "필름은 액정필름 말하시는거죠?",
          "speaker_id": "1",
          "time": "20210905 20:04"
        },
        {
          "id": "MDRW2100032518.1.19",
          "form": "워치도 필름 있어요?",
          "original_form": "워치도 필름 있어요?",
          "speaker_id": "1",
          "time": "20210905 20:04"
        },
        {
          "id": "MDRW2100032518.1.20",
          "form": "옹네네네",
          "original_form": "옹네네네",
          "speaker_id": "2",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100032518.1.21",
          "form": "딱 그 동그란화면만",
          "original_form": "딱 그 동그란화면만",
          "speaker_id": "2",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100032518.1.22",
          "form": "가리는 동그란필름을팔고있을거에요",
          "original_form": "가리는 동그란필름을팔고있을거에요",
          "speaker_id": "2",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100032518.1.23",
          "form": "그거꼭사세요..",
          "original_form": "그거꼭사세요..",
          "speaker_id": "2",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100032518.1.24",
          "form": "생활기스가 너무 심하다보니까...",
          "original_form": "생활기스가 너무 심하다보니까...",
          "speaker_id": "2",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100032518.1.25",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:06"
        },
        {
          "id": "MDRW2100032518.1.26",
          "form": "사게되면 꼭 필름도 살게요! ㅋㅋㅋㅋ",
          "original_form": "사게되면 꼭 필름도 살게요! ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:07"
        },
        {
          "id": "MDRW2100032518.1.27",
          "form": "저는 사실 제가 가지고 싶기도 했지만 원래 출시 전에 혈당 측정된다는 얘기 있었거든요",
          "original_form": "저는 사실 제가 가지고 싶기도 했지만 원래 출시 전에 혈당 측정된다는 얘기 있었거든요",
          "speaker_id": "1",
          "time": "20210905 20:07"
        },
        {
          "id": "MDRW2100032518.1.28",
          "form": "그래서 아빠 사드리려고 했는데 그게 빠져서 그냥 흐지부지 됐어요",
          "original_form": "그래서 아빠 사드리려고 했는데 그게 빠져서 그냥 흐지부지 됐어요",
          "speaker_id": "1",
          "time": "20210905 20:07"
        },
        {
          "id": "MDRW2100032518.1.29",
          "form": "아앗 그쵸 혈당측정까지된다면너무좋았겠는데ㅠㅠㅠ",
          "original_form": "아앗 그쵸 혈당측정까지된다면너무좋았겠는데ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:09"
        },
        {
          "id": "MDRW2100032518.1.30",
          "form": "근데조그만버늘이있지않는이상",
          "original_form": "근데조그만버늘이있지않는이상",
          "speaker_id": "2",
          "time": "20210905 20:09"
        },
        {
          "id": "MDRW2100032518.1.31",
          "form": "조그만바늘이 있지않는 이상 측정하기어려울거같던데",
          "original_form": "조그만바늘이 있지않는 이상 측정하기어려울거같던데",
          "speaker_id": "2",
          "time": "20210905 20:10"
        },
        {
          "id": "MDRW2100032518.1.32",
          "form": "너무신기하네요 와중에 혈압측정은되는게",
          "original_form": "너무신기하네요 와중에 혈압측정은되는게",
          "speaker_id": "2",
          "time": "20210905 20:10"
        },
        {
          "id": "MDRW2100032518.1.33",
          "form": "근데 혈압측정도 처음에 혈압기로 3번을 먼저 잰다음에 그걸바탕으로 측정하는거같더라구요",
          "original_form": "근데 혈압측정도 처음에 혈압기로 3번을 먼저 잰다음에 그걸바탕으로 측정하는거같더라구요",
          "speaker_id": "2",
          "time": "20210905 20:10"
        },
        {
          "id": "MDRW2100032518.1.34",
          "form": "아 그런게 있어요? 그건 전혀 몰랐는데",
          "original_form": "아 그런게 있어요? 그건 전혀 몰랐는데",
          "speaker_id": "1",
          "time": "20210905 20:11"
        },
        {
          "id": "MDRW2100032518.1.35",
          "form": "생각보다 까다롭네요",
          "original_form": "생각보다 까다롭네요",
          "speaker_id": "1",
          "time": "20210905 20:11"
        },
        {
          "id": "MDRW2100032518.1.36",
          "form": "그래도 수치만 정확하면",
          "original_form": "그래도 수치만 정확하면",
          "speaker_id": "1",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100032518.1.37",
          "form": "가치는 충분하죵",
          "original_form": "가치는 충분하죵",
          "speaker_id": "1",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100032518.1.38",
          "form": "옹네네",
          "original_form": "옹네네",
          "speaker_id": "2",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100032518.1.39",
          "form": "그춍",
          "original_form": "그춍",
          "speaker_id": "2",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100032518.1.40",
          "form": "인바디체크하는건",
          "original_form": "인바디체크하는건",
          "speaker_id": "2",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100032518.1.41",
          "form": "신기하더라구요",
          "original_form": "신기하더라구요",
          "speaker_id": "2",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100032518.1.42",
          "form": "매일매일 아침마다해보는데",
          "original_form": "매일매일 아침마다해보는데",
          "speaker_id": "2",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100032518.1.43",
          "form": "딱딱잘나와서 은근 잘써먹고잇네요 ㅎㅎ",
          "original_form": "딱딱잘나와서 은근 잘써먹고잇네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:12"
        }
      ]
