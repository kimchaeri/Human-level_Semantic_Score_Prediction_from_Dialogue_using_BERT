      "utterance": [
        {
          "id": "MDRW2100006551.1.1",
          "form": "집안일하시나요",
          "original_form": "집안일하시나요",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100006551.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100006551.1.3",
          "form": "집안일 자주 하시나요?",
          "original_form": "집안일 자주 하시나요?",
          "speaker_id": "2",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100006551.1.4",
          "form": "네 매일하죠 호두님은요",
          "original_form": "네 매일하죠 호두님은요",
          "speaker_id": "1",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100006551.1.5",
          "form": "저도 혼자 살아서 매일 하고 있어요",
          "original_form": "저도 혼자 살아서 매일 하고 있어요",
          "speaker_id": "2",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100006551.1.6",
          "form": "그러시군요 가장 하기싫은건요?",
          "original_form": "그러시군요 가장 하기싫은건요?",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100006551.1.7",
          "form": "음식물 쓰레기요 ㅠ",
          "original_form": "음식물 쓰레기요 ㅠ",
          "speaker_id": "2",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100006551.1.8",
          "form": "아..귀찮죠 ..",
          "original_form": "아..귀찮죠 ..",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100006551.1.9",
          "form": "그리고 냄새가 너무 비위상해요 ㅠ",
          "original_form": "그리고 냄새가 너무 비위상해요 ㅠ",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100006551.1.10",
          "form": "맞아요 여름철엔 특히 더 그렇죠",
          "original_form": "맞아요 여름철엔 특히 더 그렇죠",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100006551.1.11",
          "form": "네 ㅠㅠ 그래서 냉동실에 넣어두는데",
          "original_form": "네 ㅠㅠ 그래서 냉동실에 넣어두는데",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100006551.1.12",
          "form": "그것도 찝찝해요",
          "original_form": "그것도 찝찝해요",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100006551.1.13",
          "form": "맞아요 공감 물기없이 바로바로 버리는게 그나마..",
          "original_form": "맞아요 공감 물기없이 바로바로 버리는게 그나마..",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100006551.1.14",
          "form": "그쵸 ㅠ",
          "original_form": "그쵸 ㅠ",
          "speaker_id": "2",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100006551.1.15",
          "form": "쓰레기 생기는 날이 불규칙해서",
          "original_form": "쓰레기 생기는 날이 불규칙해서",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100006551.1.16",
          "form": "이사가시면 음식물처리기 설치해보세요",
          "original_form": "이사가시면 음식물처리기 설치해보세요",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100006551.1.17",
          "form": "젤 큰 고민이에요",
          "original_form": "젤 큰 고민이에요",
          "speaker_id": "2",
          "time": "20210908 02:17"
        }
      ]
