      "utterance": [
        {
          "id": "MDRW2100040002.1.1",
          "form": "헛 저도 비긴어게인 보ㅏㅆ어요!",
          "original_form": "헛 저도 비긴어게인 보ㅏㅆ어요!",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100040002.1.2",
          "form": "전 보고 자우림한테 빠져버렸잖아욥!",
          "original_form": "전 보고 자우림한테 빠져버렸잖아욥!",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100040002.1.3",
          "form": "자우림도 멋있으시더라구요ㅜㅜㅜ",
          "original_form": "자우림도 멋있으시더라구요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100040002.1.4",
          "form": "비긴어게인 은근 재밌어요ㅋㅋㅋ",
          "original_form": "비긴어게인 은근 재밌어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100040002.1.5",
          "form": "클립으로 많이봤는데 정승환이 부른 도망가자도 엄청 좋아해요...",
          "original_form": "클립으로 많이봤는데 정승환이 부른 도망가자도 엄청 좋아해요...",
          "speaker_id": "1",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100040002.1.6",
          "form": "헛 저도 그거 좋아래요!",
          "original_form": "헛 저도 그거 좋아래요!",
          "speaker_id": "2",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100040002.1.7",
          "form": "그거랑 태연 노래랑 그 그",
          "original_form": "그거랑 태연 노래랑 그 그",
          "speaker_id": "2",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100040002.1.8",
          "form": "김필이 뷰른 너무 아픈 사랑은 어쩌고",
          "original_form": "김필이 뷰른 너무 아픈 사랑은 어쩌고",
          "speaker_id": "2",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100040002.1.9",
          "form": "그 노래도 좋더라고요!",
          "original_form": "그 노래도 좋더라고요!",
          "speaker_id": "2",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100040002.1.10",
          "form": "맞아요!! 비긴어게인은 분위기랑 색감도 예뻐서 보는 맛이 있어요ㅎㅎ",
          "original_form": "맞아요!! 비긴어게인은 분위기랑 색감도 예뻐서 보는 맛이 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100040002.1.11",
          "form": "그니까요! 그래서 그 바라는 바다?",
          "original_form": "그니까요! 그래서 그 바라는 바다?",
          "speaker_id": "2",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100040002.1.12",
          "form": "인가 그것도 비슷할까 했는데 ㅠ.^ 전 역시 비긴어게인 감성이",
          "original_form": "인가 그것도 비슷할까 했는데 ㅠ.^ 전 역시 비긴어게인 감성이",
          "speaker_id": "2",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100040002.1.13",
          "form": "좀 더 맞더라거요",
          "original_form": "좀 더 맞더라거요",
          "speaker_id": "2",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100040002.1.14",
          "form": "저두요ㅋㅋㅋ 그냥 노래에만 집중할 수 있는게 더 나은 것 같아요",
          "original_form": "저두요ㅋㅋㅋ 그냥 노래에만 집중할 수 있는게 더 나은 것 같아요",
          "speaker_id": "1",
          "time": "20210904 12:59"
        },
        {
          "id": "MDRW2100040002.1.15",
          "form": "좋아하는 프로그램 또 있으세요??",
          "original_form": "좋아하는 프로그램 또 있으세요??",
          "speaker_id": "1",
          "time": "20210904 13:00"
        },
        {
          "id": "MDRW2100040002.1.16",
          "form": "저는 예전 예능은 크라임씬! 요즘은 여고추리반이랑 대탈출 좋아해여!",
          "original_form": "저는 예전 예능은 크라임씬! 요즘은 여고추리반이랑 대탈출 좋아해여!",
          "speaker_id": "2",
          "time": "20210904 13:02"
        },
        {
          "id": "MDRW2100040002.1.17",
          "form": "아시나요!?",
          "original_form": "아시나요!?",
          "speaker_id": "2",
          "time": "20210904 13:02"
        },
        {
          "id": "MDRW2100040002.1.18",
          "form": "네네 여추반도 좀 무섭다해서 좀 나중에 보려고 하고 크라임씬 저도 몇개 봤어요!",
          "original_form": "네네 여추반도 좀 무섭다해서 좀 나중에 보려고 하고 크라임씬 저도 몇개 봤어요!",
          "speaker_id": "1",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100040002.1.19",
          "form": "크라임씬 재밌더라구요 컨셉도 좋구ㅋㅋㅋ 작가님들이 정성이라는 생각밖에 안들어요ㅋㅋㅋ",
          "original_form": "크라임씬 재밌더라구요 컨셉도 좋구ㅋㅋㅋ 작가님들이 정성이라는 생각밖에 안들어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100040002.1.20",
          "form": "밎아요!!!!! 어떤 편 재밌게 보셨어요!?!?",
          "original_form": "밎아요!!!!! 어떤 편 재밌게 보셨어요!?!?",
          "speaker_id": "2",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100040002.1.21",
          "form": "아무래도 박미녀가 제일 기억에 남는 것 같아요ㅋㅋㅋㅋ",
          "original_form": "아무래도 박미녀가 제일 기억에 남는 것 같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100040002.1.22",
          "form": "정말 알아낸 것도 신기한ㅋㅋㅋ",
          "original_form": "정말 알아낸 것도 신기한ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:06"
        },
        {
          "id": "MDRW2100040002.1.23",
          "form": "우왁 그니까요!!!!",
          "original_form": "우왁 그니까요!!!!",
          "speaker_id": "2",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100040002.1.24",
          "form": "장진감독 진짜 대단한 것 같아요",
          "original_form": "장진감독 진짜 대단한 것 같아요",
          "speaker_id": "2",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100040002.1.25",
          "form": "전 그 숙다방 살인사건 좋아래요!",
          "original_form": "전 그 숙다방 살인사건 좋아래요!",
          "speaker_id": "2",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100040002.1.26",
          "form": "보셨나요!?",
          "original_form": "보셨나요!?",
          "speaker_id": "2",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100040002.1.27",
          "form": "앗 그거 추천받고 아직 못봤네여ㅜㅠ 그게 제일 박진감 넘쳐요??",
          "original_form": "앗 그거 추천받고 아직 못봤네여ㅜㅠ 그게 제일 박진감 넘쳐요??",
          "speaker_id": "1",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100040002.1.28",
          "form": "그렇다기보단 다 너무 반전 캐릭터인?",
          "original_form": "그렇다기보단 다 너무 반전 캐릭터인?",
          "speaker_id": "2",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100040002.1.29",
          "form": "그리고 좀 많이 웃겨요 ㅋㅋㅋㅋ",
          "original_form": "그리고 좀 많이 웃겨요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100040002.1.30",
          "form": "앜ㅋㅋㅋㅋㅋ 꼭 볼게요 사실 크라임씬도 좀 무서울때가 있어서 웃긴게 나오면 과몰입 안 할 수 있어서 좋더라구욬ㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ 꼭 볼게요 사실 크라임씬도 좀 무서울때가 있어서 웃긴게 나오면 과몰입 안 할 수 있어서 좋더라구욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:17"
        },
        {
          "id": "MDRW2100040002.1.31",
          "form": "맞아요 그 김지훈? 배우?",
          "original_form": "맞아요 그 김지훈? 배우?",
          "speaker_id": "2",
          "time": "20210904 13:19"
        }
      ]
