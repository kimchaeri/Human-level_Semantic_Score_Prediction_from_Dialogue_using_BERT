          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044782.1.4",
          "form": "혼자요",
          "original_form": "혼자요",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044782.1.5",
          "form": "혼자사시나요?",
          "original_form": "혼자사시나요?",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044782.1.6",
          "form": "전 가족이랑함께살아요",
          "original_form": "전 가족이랑함께살아요",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044782.1.7",
          "form": "혼자사신지 얼마나되셨어요",
          "original_form": "혼자사신지 얼마나되셨어요",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044782.1.8",
          "form": "쫌 된 거 같아요",
          "original_form": "쫌 된 거 같아요",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044782.1.9",
          "form": "집안일 분담 어떻게하세요?",
          "original_form": "집안일 분담 어떻게하세요?",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044782.1.10",
          "form": "전 제가 지금쉬고있어서 평일엔 제가거의다하고 주말엔 아빠가 엄마는 중간중간 시간날때하셔요 ㅋㅋㅋㅋ",
          "original_form": "전 제가 지금쉬고있어서 평일엔 제가거의다하고 주말엔 아빠가 엄마는 중간중간 시간날때하셔요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044782.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044782.1.12",
          "form": "외동이세요?",
          "original_form": "외동이세요?",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044782.1.13",
          "form": "아녀 동생들있는데 나가살아요 ㅎㅎㅎ저만 낑겨삽니다",
          "original_form": "아녀 동생들있는데 나가살아요 ㅎㅎㅎ저만 낑겨삽니다",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044782.1.14",
          "form": "name2님은 외동이세요?",
          "original_form": "&name2&님은 외동이세요?",
