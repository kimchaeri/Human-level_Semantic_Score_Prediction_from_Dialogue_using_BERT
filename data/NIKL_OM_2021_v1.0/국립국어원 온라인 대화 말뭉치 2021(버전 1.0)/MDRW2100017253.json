      "utterance": [
        {
          "id": "MDRW2100017253.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017253.1.2",
          "form": "안녕하세요%!",
          "original_form": "안녕하세요%!",
          "speaker_id": "2",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017253.1.3",
          "form": "배민등급이 무엇인가요?",
          "original_form": "배민등급이 무엇인가요?",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017253.1.4",
          "form": "등급보려고 다녀왔어요 ㅋㅋ",
          "original_form": "등급보려고 다녀왔어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017253.1.5",
          "form": "저는 고마운분밖에 안되네요ㅠㅠ",
          "original_form": "저는 고마운분밖에 안되네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017253.1.6",
          "form": "등급이 어떤거세요?",
          "original_form": "등급이 어떤거세요?",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017253.1.7",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017253.1.8",
          "form": "전 뭘까요?",
          "original_form": "전 뭘까요?",
          "speaker_id": "1",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017253.1.9",
          "form": "뭔가 높은 등급이실거같아요 ㅋㅋ",
          "original_form": "뭔가 높은 등급이실거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017253.1.10",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017253.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017253.1.12",
          "form": "어떤거에요?",
          "original_form": "어떤거에요?",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017253.1.13",
          "form": "보통 뭐 많이 시켜드세요?",
          "original_form": "보통 뭐 많이 시켜드세요?",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017253.1.14",
          "form": "저는 초밥이나 커피 많이 시켜요 ㅎㅎ",
          "original_form": "저는 초밥이나 커피 많이 시켜요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017253.1.15",
          "form": "ㅋㅋㅋ저랑같네요",
          "original_form": "ㅋㅋㅋ저랑같네요",
          "speaker_id": "1",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017253.1.16",
          "form": "전 초밥이요!",
          "original_form": "전 초밥이요!",
          "speaker_id": "1",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017253.1.17",
          "form": "초밥 너무 맛있어용 ㅠㅠ",
          "original_form": "초밥 너무 맛있어용 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017253.1.18",
          "form": "음료는 어떤 거 좋아하세요?",
          "original_form": "음료는 어떤 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017253.1.19",
          "form": "자바칩이요!",
          "original_form": "자바칩이요!",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100017253.1.20",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100017253.1.21",
          "form": "저는 공차 자몽그린티 좋아해요!",
          "original_form": "저는 공차 자몽그린티 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017253.1.22",
          "form": "오 맛있나요?",
          "original_form": "오 맛있나요?",
          "speaker_id": "1",
          "time": "20210826 18:51"
        },
        {
          "id": "MDRW2100017253.1.23",
          "form": "네 스벅 자허블 드셔보셨어요?",
          "original_form": "네 스벅 자허블 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017253.1.24",
          "form": "그거랑 비슷한데 좀 덜 달아요!",
          "original_form": "그거랑 비슷한데 좀 덜 달아요!",
          "speaker_id": "2",
          "time": "20210826 20:12"
        }
      ]
