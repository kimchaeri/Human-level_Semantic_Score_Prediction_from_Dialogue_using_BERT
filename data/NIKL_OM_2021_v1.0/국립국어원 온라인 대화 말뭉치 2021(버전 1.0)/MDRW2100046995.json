      "utterance": [
        {
          "id": "MDRW2100046995.1.1",
          "form": "막 이뻐보이다가 장바구니넣어두고",
          "original_form": "막 이뻐보이다가 장바구니넣어두고",
          "speaker_id": "2",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100046995.1.2",
          "form": "계속보니까좀",
          "original_form": "계속보니까좀",
          "speaker_id": "2",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100046995.1.3",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100046995.1.4",
          "form": "저도 예뻐보이는데",
          "original_form": "저도 예뻐보이는데",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100046995.1.5",
          "form": "사진 않았어요",
          "original_form": "사진 않았어요",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100046995.1.6",
          "form": "그래도 알록달록 타이다이말고",
          "original_form": "그래도 알록달록 타이다이말고",
          "speaker_id": "2",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100046995.1.7",
          "form": "왜냐면 구린 타이다이도 많이 봐서",
          "original_form": "왜냐면 구린 타이다이도 많이 봐서",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100046995.1.8",
          "form": "좀 중화가 돼서...",
          "original_form": "좀 중화가 돼서...",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100046995.1.9",
          "form": "맞이요 ㅋㅋ",
          "original_form": "맞이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100046995.1.10",
          "form": "날 현혹시키는 이놈도",
          "original_form": "날 현혹시키는 이놈도",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100046995.1.11",
          "form": "사실 구린 걸지도 모른다는",
          "original_form": "사실 구린 걸지도 모른다는",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100046995.1.12",
          "form": "전 타이다이가방살뻔 ㅡㅡ",
          "original_form": "전 타이다이가방살뻔 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100046995.1.13",
          "form": "결제까지했다가 취소했어요",
          "original_form": "결제까지했다가 취소했어요",
          "speaker_id": "2",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100046995.1.14",
          "form": "헐 근데 가방은 또",
          "original_form": "헐 근데 가방은 또",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100046995.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100046995.1.16",
          "form": "예쁠 것 같긴해요",
          "original_form": "예쁠 것 같긴해요",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100046995.1.17",
          "form": "근데 유행 탈 거 같아서",
          "original_form": "근데 유행 탈 거 같아서",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100046995.1.18",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100046995.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100046995.1.20",
          "form": "저도 pvc 가방도 한참 유행할 때",
          "original_form": "저도 pvc 가방도 한참 유행할 때",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100046995.1.21",
          "form": "그것도맞아서 취소했어요 ㅋㅋ유행땜시",
          "original_form": "그것도맞아서 취소했어요 ㅋㅋ유행땜시",
          "speaker_id": "2",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100046995.1.22",
          "form": "장바구니에 가득 담아놨다가",
          "original_form": "장바구니에 가득 담아놨다가",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100046995.1.23",
          "form": "변색이랑 유행때문에ㅜㅜ",
          "original_form": "변색이랑 유행때문에ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100046995.1.24",
          "form": "변색 너무 싫어요ㅋㅋㅋ",
          "original_form": "변색 너무 싫어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100046995.1.25",
          "form": "ㅠㅜ그래서이번에가방산거는",
          "original_form": "ㅠㅜ그래서이번에가방산거는",
          "speaker_id": "2",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100046995.1.26",
          "form": "그리고 한철 입을게 뻔하면 못사겠어요",
          "original_form": "그리고 한철 입을게 뻔하면 못사겠어요",
