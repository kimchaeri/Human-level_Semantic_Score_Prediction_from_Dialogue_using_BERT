      "utterance": [
        {
          "id": "MDRW2100038595.1.1",
          "form": "여행가고싶네요",
          "original_form": "여행가고싶네요",
          "speaker_id": "2",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100038595.1.2",
          "form": "저두요 여행가고싶네요",
          "original_form": "저두요 여행가고싶네요",
          "speaker_id": "1",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100038595.1.3",
          "form": "그나마 최근에 어디 가셨어요?",
          "original_form": "그나마 최근에 어디 가셨어요?",
          "speaker_id": "1",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100038595.1.4",
          "form": "최근에음...",
          "original_form": "최근에음...",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100038595.1.5",
          "form": "울산??",
          "original_form": "울산??",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100038595.1.6",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100038595.1.7",
          "form": "울산에 어디가셨어요?",
          "original_form": "울산에 어디가셨어요?",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100038595.1.8",
          "form": "울산공원이요!",
          "original_form": "울산공원이요!",
          "speaker_id": "2",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100038595.1.9",
          "form": "어떠셨어요?",
          "original_form": "어떠셨어요?",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100038595.1.10",
          "form": "또가보고싶으세요?",
          "original_form": "또가보고싶으세요?",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100038595.1.11",
          "form": "음그냥 산챡하기 좋은곳이였어요!!",
          "original_form": "음그냥 산챡하기 좋은곳이였어요!!",
          "speaker_id": "2",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100038595.1.12",
          "form": "가보신 여행지중에서 제일 좋았던곳은 어디예요?",
          "original_form": "가보신 여행지중에서 제일 좋았던곳은 어디예요?",
          "speaker_id": "1",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100038595.1.13",
          "form": "저는 일본도 좋았고대그도 좋았어요",
          "original_form": "저는 일본도 좋았고대그도 좋았어요",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100038595.1.14",
          "form": "대구는 어떤점이 좋으셨어요?",
          "original_form": "대구는 어떤점이 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100038595.1.15",
          "form": "그냥 익숙하지않은공간이다 보니깐",
          "original_form": "그냥 익숙하지않은공간이다 보니깐",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100038595.1.16",
          "form": "즐거웠던거같아요 ㅋㅋㅋㅋ",
          "original_form": "즐거웠던거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100038595.1.17",
          "form": "저는 대구 어쩌다보니 자주 갔었는데요 디저트가 맛있었어요 유독 디저트를 잘하는거 같아요",
          "original_form": "저는 대구 어쩌다보니 자주 갔었는데요 디저트가 맛있었어요 유독 디저트를 잘하는거 같아요",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100038595.1.18",
          "form": "아진짜요??",
          "original_form": "아진짜요??",
          "speaker_id": "2",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100038595.1.19",
          "form": "카페 이런데는 안가본거같아요",
          "original_form": "카페 이런데는 안가본거같아요",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100038595.1.20",
          "form": "케이크 종류가 다 맛있었어요",
          "original_form": "케이크 종류가 다 맛있었어요",
          "speaker_id": "1",
          "time": "20210902 23:15"
        }
      ]
