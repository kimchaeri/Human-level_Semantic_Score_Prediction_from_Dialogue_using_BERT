        },
        {
          "id": "MDRW2100004854.1.2",
          "form": "그렇겠다 난 이거 끝나고 놀토 볼려고",
          "original_form": "그렇겠다 난 이거 끝나고 놀토 볼려고",
          "speaker_id": "1",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004854.1.3",
          "form": "난 편먹고골프",
          "original_form": "난 편먹고골프",
          "speaker_id": "2",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004854.1.4",
          "form": "이승기 나오는거 말하지",
          "original_form": "이승기 나오는거 말하지",
          "speaker_id": "1",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004854.1.5",
          "form": "엉  맞아  우리 승기",
          "original_form": "엉  맞아  우리 승기",
          "speaker_id": "2",
          "time": "20210821 18:54"
        },
        {
          "id": "MDRW2100004854.1.6",
          "form": "승기 요번에는 누구랑 편인데",
          "original_form": "승기 요번에는 누구랑 편인데",
          "speaker_id": "1",
          "time": "20210821 18:54"
        },
        {
          "id": "MDRW2100004854.1.7",
          "form": "경규옹 승엽 승기",
          "original_form": "경규옹 승엽 승기",
          "speaker_id": "2",
          "time": "20210821 18:54"
        },
        {
          "id": "MDRW2100004854.1.8",
          "form": "이번에는 3명이서 편인갑네",
          "original_form": "이번에는 3명이서 편인갑네",
          "speaker_id": "1",
          "time": "20210821 18:54"
        },
        {
          "id": "MDRW2100004854.1.9",
          "form": "응 3:3인가봐",
          "original_form": "응 3:3인가봐",
          "speaker_id": "2",
          "time": "20210821 18:55"
        },
        {
          "id": "MDRW2100004854.1.10",
          "form": "경규옹 골프 잘 치나",
          "original_form": "경규옹 골프 잘 치나",
          "speaker_id": "1",
          "time": "20210821 18:55"
        },
        {
          "id": "MDRW2100004854.1.11",
          "form": "승기보다 잘함",
          "original_form": "승기보다 잘함",
          "speaker_id": "2",
          "time": "20210821 18:55"
        },
        {
          "id": "MDRW2100004854.1.12",
          "form": "나도 지금 보고 있는데 아닌거 같은데",
          "original_form": "나도 지금 보고 있는데 아닌거 같은데",
          "speaker_id": "1",
          "time": "20210821 18:55"
        },
        {
          "id": "MDRW2100004854.1.13",
          "form": "가끔씩 실수가 있다",
          "original_form": "가끔씩 실수가 있다",
          "speaker_id": "2",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004854.1.14",
          "form": "선영 저분은 완전 운동 능력이 장난이 아니던데",
          "original_form": "선영 저분은 완전 운동 능력이 장난이 아니던데",
          "speaker_id": "1",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004854.1.15",
          "form": "골 때 리는 그녀들 거기서 완전 최고",
          "original_form": "골 때 리는 그녀들 거기서 완전 최고",
          "speaker_id": "2",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004854.1.16",
          "form": "맞아 나도 거기서 봤는데 완전 추구 선수인거 같더라",
