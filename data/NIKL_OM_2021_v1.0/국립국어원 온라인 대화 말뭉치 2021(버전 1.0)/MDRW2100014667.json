      "utterance": [
        {
          "id": "MDRW2100014667.1.1",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014667.1.2",
          "form": "이번에는 저 주제구나",
          "original_form": "이번에는 저 주제구나",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014667.1.3",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014667.1.4",
          "form": "저녁은 뭐 먹었니",
          "original_form": "저녁은 뭐 먹었니",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014667.1.5",
          "form": "요새 할아버지는 건강하시니",
          "original_form": "요새 할아버지는 건강하시니",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014667.1.6",
          "form": "할머니는",
          "original_form": "할머니는",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014667.1.7",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014667.1.8",
          "form": "아마 괜찮은것 같아",
          "original_form": "아마 괜찮은것 같아",
          "speaker_id": "2",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014667.1.9",
          "form": "저녁은 어머니가 차려주셔서 먹었어",
          "original_form": "저녁은 어머니가 차려주셔서 먹었어",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014667.1.10",
          "form": "아마?",
          "original_form": "아마?",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014667.1.11",
          "form": "그럼 다행이지",
          "original_form": "그럼 다행이지",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014667.1.12",
          "form": "잘 모르겠어",
          "original_form": "잘 모르겠어",
          "speaker_id": "2",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014667.1.13",
          "form": "그럴 수 있지",
          "original_form": "그럴 수 있지",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014667.1.14",
          "form": "name3은 건강하니",
          "original_form": "&name3&은 건강하니",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014667.1.15",
          "form": "귀여운 푸들",
          "original_form": "귀여운 푸들",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014667.1.16",
          "form": "name3은 건강하지",
          "original_form": "&name3&은 건강하지",
          "speaker_id": "2",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014667.1.17",
          "form": "지금 아빠랑 놀고있네",
          "original_form": "지금 아빠랑 놀고있네",
          "speaker_id": "2",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014667.1.18",
          "form": "더 건강하길 바라",
          "original_form": "더 건강하길 바라",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014667.1.19",
          "form": "어머 아버님이 놀아주시나봐",
          "original_form": "어머 아버님이 놀아주시나봐",
          "speaker_id": "1",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014667.1.20",
          "form": "정말 참견이 많은 강아지야",
          "original_form": "정말 참견이 많은 강아지야",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014667.1.21",
          "form": "참견이 많구나 하하",
          "original_form": "참견이 많구나 하하",
          "speaker_id": "1",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014667.1.22",
          "form": "강아지는 소중한 가족이지",
          "original_form": "강아지는 소중한 가족이지",
          "speaker_id": "1",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014667.1.23",
          "form": "오늘 김치 담았는데",
          "original_form": "오늘 김치 담았는데",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014667.1.24",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014667.1.25",
          "form": "옆에서",
          "original_form": "옆에서",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014667.1.26",
          "form": "뭐 달라고 난리야",
          "original_form": "뭐 달라고 난리야",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014667.1.27",
          "form": "사과 깎으면 사과 달라고 난리고",
          "original_form": "사과 깎으면 사과 달라고 난리고",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014667.1.28",
          "form": "무 깎으면 무 달라고 난리야",
          "original_form": "무 깎으면 무 달라고 난리야",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014667.1.29",
          "form": "강아지가 배고팠나봐",
          "original_form": "강아지가 배고팠나봐",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014667.1.30",
          "form": "정말 자기주장 확실해",
          "original_form": "정말 자기주장 확실해",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014667.1.31",
          "form": "널 닮았을까",
          "original_form": "널 닮았을까",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014667.1.32",
          "form": "배 안고파도 달라고 그래",
          "original_form": "배 안고파도 달라고 그래",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014667.1.33",
          "form": "내가 자기주장이 확실한 편이였나",
          "original_form": "내가 자기주장이 확실한 편이였나",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014667.1.34",
          "form": "처음 안 사실이다",
          "original_form": "처음 안 사실이다",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014667.1.35",
          "form": "그건 강아지가 심심했나봐",
          "original_form": "그건 강아지가 심심했나봐",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014667.1.36",
          "form": "그럼 확실한 쪽이지",
          "original_form": "그럼 확실한 쪽이지",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014667.1.37",
          "form": "그랬구나",
          "original_form": "그랬구나",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014667.1.38",
          "form": "이건 나중에 얘기해줄게",
          "original_form": "이건 나중에 얘기해줄게",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014667.1.39",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014667.1.40",
          "form": "가사가 아니니깐 ㅎㅎ",
          "original_form": "가사가 아니니깐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014667.1.41",
          "form": "요즘 언니는 잘 지내시니?",
          "original_form": "요즘 언니는 잘 지내시니?",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014667.1.42",
          "form": "또 끝났어",
          "original_form": "또 끝났어",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014667.1.43",
          "form": "언니 얘긴 들을수 없게 되었어",
          "original_form": "언니 얘긴 들을수 없게 되었어",
          "speaker_id": "1",
          "time": "20210825 20:17"
        }
      ]
    }
