      "utterance": [
        {
          "id": "MDRW2100016336.1.1",
          "form": "보는 티비프로그램같은거 있어?",
          "original_form": "보는 티비프로그램같은거 있어?",
          "speaker_id": "1",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100016336.1.2",
          "form": "요즘 티비는 잘안봐ㅜ",
          "original_form": "요즘 티비는 잘안봐ㅜ",
          "speaker_id": "2",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100016336.1.3",
          "form": "자취라 집에 티비가없어",
          "original_form": "자취라 집에 티비가없어",
          "speaker_id": "2",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100016336.1.4",
          "form": "대신유툽을보지ㅎㅎ",
          "original_form": "대신유툽을보지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100016336.1.5",
          "form": "유튜브 나도많이봐",
          "original_form": "유튜브 나도많이봐",
          "speaker_id": "1",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016336.1.6",
          "form": "누구거봐?",
          "original_form": "누구거봐?",
          "speaker_id": "1",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016336.1.7",
          "form": "요즘은",
          "original_form": "요즘은",
          "speaker_id": "2",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016336.1.8",
          "form": "미야옹철의냥냥펀치",
          "original_form": "미야옹철의냥냥펀치",
          "speaker_id": "2",
          "time": "20210826 12:08"
        },
        {
          "id": "MDRW2100016336.1.9",
          "form": "ㅋㅋㅋ혹시알아??",
          "original_form": "ㅋㅋㅋ혹시알아??",
          "speaker_id": "2",
          "time": "20210826 12:08"
        },
        {
          "id": "MDRW2100016336.1.10",
          "form": "이름이 재밌네 ㅋㅋㅋ 첨들어봐",
          "original_form": "이름이 재밌네 ㅋㅋㅋ 첨들어봐",
          "speaker_id": "1",
          "time": "20210826 12:08"
        },
        {
          "id": "MDRW2100016336.1.11",
          "form": "고양이유툽인데",
          "original_form": "고양이유툽인데",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016336.1.12",
          "form": "수의사가하는 유툽이야!",
          "original_form": "수의사가하는 유툽이야!",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016336.1.13",
          "form": "고양이유툽 좀 봐??",
          "original_form": "고양이유툽 좀 봐??",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016336.1.14",
          "form": "수의사라니 특이하다",
          "original_form": "수의사라니 특이하다",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016336.1.15",
          "form": "동물 유튜브는 잘 안 봐 ㅠㅠ",
          "original_form": "동물 유튜브는 잘 안 봐 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016336.1.16",
          "form": "ㅇㅇ그지 특이해",
          "original_form": "ㅇㅇ그지 특이해",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016336.1.17",
          "form": "세상좋다",
          "original_form": "세상좋다",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016336.1.18",
          "form": "ㅋㅋㅋ나도 잘안보는데 이건 수의사가",
          "original_form": "ㅋㅋㅋ나도 잘안보는데 이건 수의사가",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016336.1.19",
          "form": "안키워도 실컷 볼 수 있다니",
          "original_form": "안키워도 실컷 볼 수 있다니",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016336.1.20",
          "form": "고양이특징?같은거 정리해줘서 재밌어",
          "original_form": "고양이특징?같은거 정리해줘서 재밌어",
          "speaker_id": "2",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016336.1.21",
          "form": "의사답다 ㅋㅋㅋ",
          "original_form": "의사답다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:11"
        }
      ]
