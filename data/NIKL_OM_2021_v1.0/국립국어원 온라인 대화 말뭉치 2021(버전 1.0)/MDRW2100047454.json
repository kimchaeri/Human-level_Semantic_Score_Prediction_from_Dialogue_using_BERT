        },
        {
          "id": "MDRW2100047454.1.25",
          "form": "앜ㅋㅋㄱㅋㅋㅋㅋ완전반대네용",
          "original_form": "앜ㅋㅋㄱㅋㅋㅋㅋ완전반대네용",
