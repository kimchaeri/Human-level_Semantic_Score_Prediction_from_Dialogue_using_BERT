      "utterance": [
        {
          "id": "MDRW2100047454.1.1",
          "form": "게임좋아하시나용?",
          "original_form": "게임좋아하시나용?",
          "speaker_id": "2",
          "time": "20210826 16:39"
        },
        {
          "id": "MDRW2100047454.1.2",
          "form": "넹! littlesong 님은 게임 좋아하세요?",
          "original_form": "넹! littlesong 님은 게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 16:40"
        },
        {
          "id": "MDRW2100047454.1.3",
          "form": "저는 막 미쳐서 하진 않는데 종종하는편이에요!!",
          "original_form": "저는 막 미쳐서 하진 않는데 종종하는편이에요!!",
          "speaker_id": "2",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100047454.1.4",
          "form": "무슨게임 좋아하세요?",
          "original_form": "무슨게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100047454.1.5",
          "form": "저는 fps 좋아해서 롤이랑 쿠키런 해요! littlesong 님은요?",
          "original_form": "저는 fps 좋아해서 롤이랑 쿠키런 해요! littlesong 님은요?",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100047454.1.6",
          "form": "저는 컴퓨터게임은 한지 오래된것같고 요즘은 꿈의집 하고있어요!!",
          "original_form": "저는 컴퓨터게임은 한지 오래된것같고 요즘은 꿈의집 하고있어요!!",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100047454.1.7",
          "form": "롤 어려워보이던데!ㅋㅋㅋ재밌나봐용",
          "original_form": "롤 어려워보이던데!ㅋㅋㅋ재밌나봐용",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100047454.1.8",
          "form": "요즘도 인기있나요",
          "original_form": "요즘도 인기있나요",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100047454.1.9",
          "form": "요즘도 인기 있어요! 하는 사람들 많더라구요",
          "original_form": "요즘도 인기 있어요! 하는 사람들 많더라구요",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100047454.1.10",
          "form": "꿈의 집이 폰게임인가요??",
          "original_form": "꿈의 집이 폰게임인가요??",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100047454.1.11",
          "form": "오 요즘도 인기있구나ㅎㅎㅎ",
          "original_form": "오 요즘도 인기있구나ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100047454.1.12",
          "form": "네!! 약간 캔디크러쉬 느낌이에요!!ㅋㅋ기",
          "original_form": "네!! 약간 캔디크러쉬 느낌이에요!!ㅋㅋ기",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100047454.1.13",
          "form": "그럼 오버워치도 해보셨나용?",
          "original_form": "그럼 오버워치도 해보셨나용?",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100047454.1.14",
          "form": "네 오버워치 했었죠! little 님도 해보셨나요?",
          "original_form": "네 오버워치 했었죠! little 님도 해보셨나요?",
          "speaker_id": "1",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100047454.1.15",
          "form": "오 전 안해봤어요!!",
          "original_form": "오 전 안해봤어요!!",
          "speaker_id": "2",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100047454.1.16",
          "form": "유튜버들이 하는것만 구경해봤어용ㅋㄱㅋ",
          "original_form": "유튜버들이 하는것만 구경해봤어용ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100047454.1.17",
          "form": "왠지 그런거하면 팀원들한테 폐끼치고 욕먹는거 싫어서 안하게되더라구요",
          "original_form": "왠지 그런거하면 팀원들한테 폐끼치고 욕먹는거 싫어서 안하게되더라구요",
          "speaker_id": "2",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100047454.1.18",
          "form": "아ㅋㅋㅋ 맞아요 폐끼치는 느낌이 들긴 하죠 못하면",
          "original_form": "아ㅋㅋㅋ 맞아요 폐끼치는 느낌이 들긴 하죠 못하면",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100047454.1.19",
          "form": "그래서 전 채팅 다 끄고 해요ㅎㅎㅋㅋㅋ",
          "original_form": "그래서 전 채팅 다 끄고 해요ㅎㅎㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100047454.1.20",
          "form": "앜ㅋㅋㅋㅋㅋㅋ그런방법이 있었네요!!크크",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ그런방법이 있었네요!!크크",
          "speaker_id": "2",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100047454.1.21",
          "form": "막 통화하면서도 하던데",
          "original_form": "막 통화하면서도 하던데",
          "speaker_id": "2",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100047454.1.22",
          "form": "그런것보단 전 혼자하는게임이 재밌어요ㅎㅎ",
          "original_form": "그런것보단 전 혼자하는게임이 재밌어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047454.1.23",
          "form": "오오 그렇군요ㅋㅋㅋㅋ저는 혼자하는 게임은 잘 안 맞더라고요",
          "original_form": "오오 그렇군요ㅋㅋㅋㅋ저는 혼자하는 게임은 잘 안 맞더라고요",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100047454.1.24",
          "form": "전화하면서 안해도 무조건 스릴있는게 좋아요ㅎ",
          "original_form": "전화하면서 안해도 무조건 스릴있는게 좋아요ㅎ",
          "speaker_id": "1",
