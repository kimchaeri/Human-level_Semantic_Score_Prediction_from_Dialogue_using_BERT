      "utterance": [
        {
          "id": "MDRW2100027292.1.1",
          "form": "우리다큐아직찍고있다던데",
          "original_form": "우리다큐아직찍고있다던데",
          "speaker_id": "2",
          "time": "20210902 11:09"
        },
        {
          "id": "MDRW2100027292.1.2",
          "form": "아 진짜???",
          "original_form": "아 진짜???",
          "speaker_id": "1",
          "time": "20210902 11:09"
        },
        {
          "id": "MDRW2100027292.1.3",
          "form": "그건언제줄거야비덥....?",
          "original_form": "그건언제줄거야비덥....?",
          "speaker_id": "2",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100027292.1.4",
          "form": "잉 난이미다찍은줄",
          "original_form": "잉 난이미다찍은줄",
          "speaker_id": "1",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100027292.1.5",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100027292.1.6",
          "form": "연말에나오겄네",
          "original_form": "연말에나오겄네",
          "speaker_id": "1",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100027292.1.7",
          "form": "난콘까지찍고나올거라생각했는데",
          "original_form": "난콘까지찍고나올거라생각했는데",
          "speaker_id": "2",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100027292.1.8",
          "form": "뭔7주년프로젝트를1년내내할기세",
          "original_form": "뭔7주년프로젝트를1년내내할기세",
          "speaker_id": "2",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100027292.1.9",
          "form": "근데딱히",
          "original_form": "근데딱히",
          "speaker_id": "1",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100027292.1.10",
          "form": "그프로젝트 많이한것도없어보이는데",
          "original_form": "그프로젝트 많이한것도없어보이는데",
          "speaker_id": "1",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100027292.1.11",
          "form": "힘준줄알았는데이제쫌모르겠어",
          "original_form": "힘준줄알았는데이제쫌모르겠어",
          "speaker_id": "2",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100027292.1.12",
          "form": "앨범 콘 베스드앨범 다큐",
          "original_form": "앨범 콘 베스드앨범 다큐",
          "speaker_id": "1",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100027292.1.13",
          "form": "다큐도제대로된계획없어보이고",
          "original_form": "다큐도제대로된계획없어보이고",
          "speaker_id": "2",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100027292.1.14",
          "form": "말만들음 커보이긴한데",
          "original_form": "말만들음 커보이긴한데",
          "speaker_id": "1",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100027292.1.15",
          "form": "앨범은콘밀리면서급조고",
          "original_form": "앨범은콘밀리면서급조고",
          "speaker_id": "2",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100027292.1.16",
          "form": "콘은괜찮긴했는데",
          "original_form": "콘은괜찮긴했는데",
          "speaker_id": "2",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100027292.1.17",
          "form": "ㅋㅋㅋㅋㅋ으음..",
          "original_form": "ㅋㅋㅋㅋㅋ으음..",
          "speaker_id": "1",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100027292.1.18",
          "form": "다큐에대한기대가점점줄고있음....",
          "original_form": "다큐에대한기대가점점줄고있음....",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100027292.1.19",
          "form": "얘네지들도다큐방향설정못하고있는거아닌가생각도들고",
          "original_form": "얘네지들도다큐방향설정못하고있는거아닌가생각도들고",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100027292.1.20",
          "form": "아니...근데진짜",
          "original_form": "아니...근데진짜",
          "speaker_id": "1",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100027292.1.21",
          "form": "다큐를 지금껏찍음",
          "original_form": "다큐를 지금껏찍음",
          "speaker_id": "1",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100027292.1.22",
