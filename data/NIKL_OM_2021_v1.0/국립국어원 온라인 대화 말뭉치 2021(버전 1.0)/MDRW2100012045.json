      "utterance": [
        {
          "id": "MDRW2100012045.1.1",
          "form": "네! ㅎ.ㅎ 치킨 좋아해요",
          "original_form": "네! ㅎ.ㅎ 치킨 좋아해요",
          "speaker_id": "2",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012045.1.2",
          "form": "저녁으로 지금 치킨 살짝 고민했어요",
          "original_form": "저녁으로 지금 치킨 살짝 고민했어요",
          "speaker_id": "2",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012045.1.3",
          "form": "치킨 어디꺼 드세요?",
          "original_form": "치킨 어디꺼 드세요?",
          "speaker_id": "2",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012045.1.4",
          "form": "치킨좋죠ㅜㅜ",
          "original_form": "치킨좋죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012045.1.5",
          "form": "저 교촌허니콤보가 최애에요",
          "original_form": "저 교촌허니콤보가 최애에요",
          "speaker_id": "1",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012045.1.6",
          "form": "뚜또님은요",
          "original_form": "뚜또님은요",
          "speaker_id": "1",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012045.1.7",
          "form": "저는 교촌 레드콤보요!",
          "original_form": "저는 교촌 레드콤보요!",
          "speaker_id": "2",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012045.1.8",
          "form": "ㅋㅋㅋㅋ허니콤보도 근데 좋아해요 레드소스시켜서",
          "original_form": "ㅋㅋㅋㅋ허니콤보도 근데 좋아해요 레드소스시켜서",
          "speaker_id": "2",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012045.1.9",
          "form": "맞아요 레드소스 추가해야돼요ㅜ",
          "original_form": "맞아요 레드소스 추가해야돼요ㅜ",
          "speaker_id": "1",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012045.1.10",
          "form": "마요네즈랑 먹음 맛있어서",
          "original_form": "마요네즈랑 먹음 맛있어서",
          "speaker_id": "2",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012045.1.11",
          "form": "그쵸!",
          "original_form": "그쵸!",
          "speaker_id": "2",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012045.1.12",
          "form": "근데 레드소스가 옛날이랑 좀 다른것같지않나요ㅜ",
          "original_form": "근데 레드소스가 옛날이랑 좀 다른것같지않나요ㅜ",
          "speaker_id": "1",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012045.1.13",
          "form": "다리랑 날개중 어떤걸 더조아하세요?",
          "original_form": "다리랑 날개중 어떤걸 더조아하세요?",
          "speaker_id": "2",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012045.1.14",
          "form": "맞아여 전보다 덜해진느낌",
          "original_form": "맞아여 전보다 덜해진느낌",
          "speaker_id": "2",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012045.1.15",
          "form": "딸기잼맛이 좀더 강해졌어요ㅜ",
          "original_form": "딸기잼맛이 좀더 강해졌어요ㅜ",
          "speaker_id": "1",
          "time": "20210824 19:30"
        },
        {
          "id": "MDRW2100012045.1.16",
          "form": "전 날개요",
          "original_form": "전 날개요",
          "speaker_id": "1",
          "time": "20210824 19:30"
        },
        {
          "id": "MDRW2100012045.1.17",
          "form": "날개랑 윙봉을 더좋아해요",
          "original_form": "날개랑 윙봉을 더좋아해요",
          "speaker_id": "1",
          "time": "20210824 19:30"
        },
        {
          "id": "MDRW2100012045.1.18",
          "form": "뚜또님은요",
          "original_form": "뚜또님은요",
          "speaker_id": "1",
          "time": "20210824 19:30"
        },
        {
          "id": "MDRW2100012045.1.19",
          "form": "저도 날개랑 윙봉 조아해요! 다리는 맛있긴한데",
          "original_form": "저도 날개랑 윙봉 조아해요! 다리는 맛있긴한데",
          "speaker_id": "2",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100012045.1.20",
          "form": "날개만큼은 아니예요 ㅎ.ㅎ..",
          "original_form": "날개만큼은 아니예요 ㅎ.ㅎ..",
          "speaker_id": "2",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100012045.1.21",
          "form": "맞아요 날개살이 좀더 부드럽고 맛있어요 ㅎㅎㅎ",
          "original_form": "맞아요 날개살이 좀더 부드럽고 맛있어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 19:32"
        },
        {
          "id": "MDRW2100012045.1.22",
          "form": "교촌말고 또 좋아하는브랜드있으시나요",
          "original_form": "교촌말고 또 좋아하는브랜드있으시나요",
          "speaker_id": "1",
          "time": "20210824 19:32"
        },
        {
          "id": "MDRW2100012045.1.23",
          "form": "푸라닭 마요?갈릭마요 그거 맛있더라구요 ?",
          "original_form": "푸라닭 마요?갈릭마요 그거 맛있더라구요 ?",
          "speaker_id": "2",
          "time": "20210824 19:33"
        },
        {
          "id": "MDRW2100012045.1.24",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210824 19:33"
        },
        {
          "id": "MDRW2100012045.1.25",
          "form": "청양마요일까여?",
          "original_form": "청양마요일까여?",
          "speaker_id": "1",
          "time": "20210824 19:33"
        },
        {
          "id": "MDRW2100012045.1.26",
          "form": "아 그거인거같아요!",
          "original_form": "아 그거인거같아요!",
          "speaker_id": "2",
          "time": "20210824 19:33"
        },
        {
          "id": "MDRW2100012045.1.27",
          "form": "블랙알리오먹어봤어요! 청양마요도 먹어본거같긴한데",
          "original_form": "블랙알리오먹어봤어요! 청양마요도 먹어본거같긴한데",
          "speaker_id": "1",
          "time": "20210824 19:33"
        },
        {
          "id": "MDRW2100012045.1.28",
          "form": "ㅋㅋㅋㅋㅋ블랙알리오예요..",
          "original_form": "ㅋㅋㅋㅋㅋ블랙알리오예요..",
          "speaker_id": "2",
          "time": "20210824 19:33"
        },
        {
          "id": "MDRW2100012045.1.29",
          "form": "이름을 제대로 못 기억했네요ㅠㅠ",
          "original_form": "이름을 제대로 못 기억했네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 19:34"
        },
        {
          "id": "MDRW2100012045.1.30",
          "form": "전 블랙알리오가 더 제취향이더라구요 전매운걸 잘못먹어서 ㅋㅋㅋ",
          "original_form": "전 블랙알리오가 더 제취향이더라구요 전매운걸 잘못먹어서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:34"
        }
      ]
    }
