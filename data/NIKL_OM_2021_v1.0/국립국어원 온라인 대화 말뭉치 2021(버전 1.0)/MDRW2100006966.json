      "utterance": [
        {
          "id": "MDRW2100006966.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006966.1.2",
          "form": "혹시 옛날에 싸이월드 써보셨나요?",
          "original_form": "혹시 옛날에 싸이월드 써보셨나요?",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006966.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006966.1.4",
          "form": "싸이월드했죠 ㅎㅎ",
          "original_form": "싸이월드했죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006966.1.5",
          "form": "근데 열심히 한기억이 안나서 어떤게 남아있을지모르겠어요",
          "original_form": "근데 열심히 한기억이 안나서 어떤게 남아있을지모르겠어요",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006966.1.6",
          "form": "저는 초등 고학년 때 잠시 했는데",
          "original_form": "저는 초등 고학년 때 잠시 했는데",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006966.1.7",
          "form": "지금도 아주 가끔 이불 차요",
          "original_form": "지금도 아주 가끔 이불 차요",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006966.1.8",
          "form": "그래도 딱히 부활해도 남들도 안 들어갈 것 같아서",
          "original_form": "그래도 딱히 부활해도 남들도 안 들어갈 것 같아서",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006966.1.9",
          "form": "별로 신경쓰이진 않지만요 ㅎㅎ",
          "original_form": "별로 신경쓰이진 않지만요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006966.1.10",
          "form": "그렇긴해요 저도 별기대안되요",
          "original_form": "그렇긴해요 저도 별기대안되요",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006966.1.11",
          "form": "근데 폭로전이 될꺼라는사람들도 있더라구요",
          "original_form": "근데 폭로전이 될꺼라는사람들도 있더라구요",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006966.1.12",
          "form": "앗 그럴 수도 있겠어요",
          "original_form": "앗 그럴 수도 있겠어요",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006966.1.13",
          "form": "특히 사진 같은 거",
          "original_form": "특히 사진 같은 거",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006966.1.14",
          "form": "네 사진 폭로전이 될꺼라고",
          "original_form": "네 사진 폭로전이 될꺼라고",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006966.1.15",
          "form": "후기 에피소드 정도만 궁금해요 ㅎㅎ",
          "original_form": "후기 에피소드 정도만 궁금해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006966.1.16",
          "form": "혹시 써이 시절이 그립단 생각이 드시는 적은 없나요?",
          "original_form": "혹시 써이 시절이 그립단 생각이 드시는 적은 없나요?",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006966.1.17",
          "form": "저는 90년대 생인데 가뜸 그런 생각 해요",
          "original_form": "저는 90년대 생인데 가뜸 그런 생각 해요",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006966.1.18",
          "form": "2000년대 초중반의 감성이 꽤 자주 그리워요 ㅎㅎ",
          "original_form": "2000년대 초중반의 감성이 꽤 자주 그리워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006966.1.19",
          "form": "그래요? 저는 80년대인데 ㅎ",
          "original_form": "그래요? 저는 80년대인데 ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006966.1.20",
          "form": "그립지는 않아요 ㅎㅎ",
          "original_form": "그립지는 않아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006966.1.21",
          "form": "오 그러시구나",
          "original_form": "오 그러시구나",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100006966.1.22",
          "form": "그럼 더욱이 싸이 부활엔 큰 감흥 없으시겠어요 ㅎㅎ",
          "original_form": "그럼 더욱이 싸이 부활엔 큰 감흥 없으시겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006966.1.23",
          "form": "네 맞아요 그래서 실은 그냥 저냥 그래요 ㅎ",
          "original_form": "네 맞아요 그래서 실은 그냥 저냥 그래요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006966.1.24",
          "form": "그리우시면 기다려지시나요?",
          "original_form": "그리우시면 기다려지시나요?",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100006966.1.25",
          "form": "아니요 저도 그렇지는 않은데",
          "original_form": "아니요 저도 그렇지는 않은데",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006966.1.26",
          "form": "싸이 사측에서는 무슨 이득을 기대해서",
          "original_form": "싸이 사측에서는 무슨 이득을 기대해서",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006966.1.27",
          "form": "그런 사업을 추진하는지 궁금은 해요",
          "original_form": "그런 사업을 추진하는지 궁금은 해요",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006966.1.28",
          "form": "딱히 반응이 긍정적이지도 않은 것 같은데",
          "original_form": "딱히 반응이 긍정적이지도 않은 것 같은데",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100006966.1.29",
          "form": "추억팔이아닐까요?ㅎ",
          "original_form": "추억팔이아닐까요?ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100006966.1.30",
          "form": "그럴 수도 있겠네요 누가 되게 그리운가봐요 사내에서 ㅎㅎㅎ",
          "original_form": "그럴 수도 있겠네요 누가 되게 그리운가봐요 사내에서 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100006966.1.31",
          "form": "ㅎㅎ 싸이월드로 동창을 다시 찾고싶은사람일수도 있구요",
          "original_form": "ㅎㅎ 싸이월드로 동창을 다시 찾고싶은사람일수도 있구요",
          "speaker_id": "2",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100006966.1.32",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100006966.1.33",
          "form": "저는 이사를 자주 다녀서 학창시절 친구들 근황을 거의 몰라서 아쉬워요",
          "original_form": "저는 이사를 자주 다녀서 학창시절 친구들 근황을 거의 몰라서 아쉬워요",
          "speaker_id": "1",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100006966.1.34",
          "form": "요즘 어린이들처럼 내 폰 있으면 안 그랬을 텐데 ㅎㅎ",
          "original_form": "요즘 어린이들처럼 내 폰 있으면 안 그랬을 텐데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:39"
        }
      ]
