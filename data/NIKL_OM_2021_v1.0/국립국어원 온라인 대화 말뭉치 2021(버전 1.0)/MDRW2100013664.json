        },
        {
          "id": "MDRW2100013664.1.3",
          "form": "name1씨 내려와바유",
          "original_form": "&name1&씨 내려와바유",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013664.1.4",
          "form": "절교해요",
          "original_form": "절교해요",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013664.1.5",
          "form": "배추를 고르다니",
          "original_form": "배추를 고르다니",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013664.1.6",
          "form": "와 name2씨",
          "original_form": "와 &name2&씨",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013664.1.7",
          "form": "애호박 적폐잔아요",
          "original_form": "애호박 적폐잔아요",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013664.1.8",
          "form": "어떻게 호박을 두고",
          "original_form": "어떻게 호박을 두고",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013664.1.9",
          "form": "ㅈㅅ",
          "original_form": "ㅈㅅ",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013664.1.10",
          "form": "배추를 와",
          "original_form": "배추를 와",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013664.1.11",
          "form": "빠른 사과 인정",
          "original_form": "빠른 사과 인정",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013664.1.12",
          "form": "당신 추석때 우리집 와바유",
          "original_form": "당신 추석때 우리집 와바유",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013664.1.13",
          "form": "배추전 천국의 맛을 보여줄라니까",
          "original_form": "배추전 천국의 맛을 보여줄라니까",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013664.1.14",
          "form": "가면 뭐줄거에유",
          "original_form": "가면 뭐줄거에유",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013664.1.15",
          "form": "와 배추전맛집",
          "original_form": "와 배추전맛집",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013664.1.16",
          "form": "뭐원해유",
          "original_form": "뭐원해유",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013664.1.17",
          "form": "저 자신감",
          "original_form": "저 자신감",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013664.1.18",
          "form": "대단하다",
          "original_form": "대단하다",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013664.1.19",
          "form": "저 산적 좋아해요",
          "original_form": "저 산적 좋아해요",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013664.1.20",
          "form": "산적 잘해요?",
