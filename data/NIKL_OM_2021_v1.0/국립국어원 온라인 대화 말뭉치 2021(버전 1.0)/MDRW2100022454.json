      "utterance": [
        {
          "id": "MDRW2100022454.1.1",
          "form": "개한테 물려보셨어요?",
          "original_form": "개한테 물려보셨어요?",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022454.1.2",
          "form": "한성격 하는구나ㅋㅋ 저는 본가에 진돗개 키워요",
          "original_form": "한성격 하는구나ㅋㅋ 저는 본가에 진돗개 키워요",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022454.1.3",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022454.1.4",
          "form": "안물려봤어요",
          "original_form": "안물려봤어요",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022454.1.5",
          "form": "ㅋㅋㅋ우와 진돗개 영리하잖아요 ㅠ",
          "original_form": "ㅋㅋㅋ우와 진돗개 영리하잖아요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022454.1.6",
          "form": "저 예뻐해주다가 물려봤어요",
          "original_form": "저 예뻐해주다가 물려봤어요",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022454.1.7",
          "form": "고양이한테는 물려봤어요",
          "original_form": "고양이한테는 물려봤어요",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022454.1.8",
          "form": "고양이 ㅋㅋㅋㅋ너무 무서워요",
          "original_form": "고양이 ㅋㅋㅋㅋ너무 무서워요",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022454.1.9",
          "form": "헐 이뻐해주는데 물다니",
          "original_form": "헐 이뻐해주는데 물다니",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022454.1.10",
          "form": "아나 왜안쳐져",
          "original_form": "아나 왜안쳐져",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022454.1.11",
          "form": "고양이.. 왜요? 괜찮던데",
          "original_form": "고양이.. 왜요? 괜찮던데",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022454.1.12",
          "form": ";;",
          "original_form": ";;",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022454.1.13",
          "form": "고양이가 막",
          "original_form": "고양이가 막",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022454.1.14",
          "form": "하-악 거리잖아요",
          "original_form": "하-악 거리잖아요",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022454.1.15",
          "form": "그거 경고하는거라면서요..",
          "original_form": "그거 경고하는거라면서요..",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022454.1.16",
          "form": "경계가 심해서 그렇긴 한데",
          "original_form": "경계가 심해서 그렇긴 한데",
          "speaker_id": "1",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022454.1.17",
          "form": "뭐 개도 비슷하지않나요ㅋㅋ",
          "original_form": "뭐 개도 비슷하지않나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022454.1.18",
          "form": "ㅠㅠ고양이한테 할큄 여러번 당해봤어요 ㅠ",
          "original_form": "ㅠㅠ고양이한테 할큄 여러번 당해봤어요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022454.1.19",
          "form": "할퀴는게 더 아프더라구요 전",
          "original_form": "할퀴는게 더 아프더라구요 전",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022454.1.20",
          "form": "어쩌다가요",
          "original_form": "어쩌다가요",
          "speaker_id": "1",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022454.1.21",
          "form": "고양이가 장롱위로 도망갔는데",
          "original_form": "고양이가 장롱위로 도망갔는데",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022454.1.22",
          "form": "간식 주면 좋아한대서 간식 주다가",
          "original_form": "간식 주면 좋아한대서 간식 주다가",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022454.1.23",
          "form": "할큄 당했어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "할큄 당했어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:22"
        }
