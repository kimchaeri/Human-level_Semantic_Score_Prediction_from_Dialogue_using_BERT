      "utterance": [
        {
          "id": "MDRW2100034914.1.1",
          "form": "전 롯데리아가 집근처에 있었어서",
          "original_form": "전 롯데리아가 집근처에 있었어서",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100034914.1.2",
          "form": "거기를 기준으로 많이 먹어서 그런가봐요",
          "original_form": "거기를 기준으로 많이 먹어서 그런가봐요",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100034914.1.3",
          "form": "오 그렇군여",
          "original_form": "오 그렇군여",
          "speaker_id": "2",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100034914.1.4",
          "form": "치킨좋아하시나요",
          "original_form": "치킨좋아하시나요",
          "speaker_id": "2",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100034914.1.5",
          "form": "좋아는 하는데 배달은 한달에 한두번 시켜요",
          "original_form": "좋아는 하는데 배달은 한달에 한두번 시켜요",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100034914.1.6",
          "form": "주로 교촌에서요 님은요?",
          "original_form": "주로 교촌에서요 님은요?",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100034914.1.7",
          "form": "앗 저는 치킨은 한달에3~4번 배달은 10회이상먹는듯요 ㅋㅋㅋㅋ",
          "original_form": "앗 저는 치킨은 한달에3~4번 배달은 10회이상먹는듯요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100034914.1.8",
          "form": "저도 교촌자주먹어요",
          "original_form": "저도 교촌자주먹어요",
          "speaker_id": "2",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100034914.1.9",
          "form": "교촌어떤치킨 좋아하세요",
          "original_form": "교촌어떤치킨 좋아하세요",
          "speaker_id": "2",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100034914.1.10",
          "form": "저도 닭배달건만 한달에 두건인거고다른것도 배달을 좀ㅋㅋ",
          "original_form": "저도 닭배달건만 한달에 두건인거고다른것도 배달을 좀ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100034914.1.11",
          "form": "레드콤보요",
          "original_form": "레드콤보요",
          "speaker_id": "1",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100034914.1.12",
          "form": "아ㅋㅋㅋㅋㅋ전 허니콤보좋아해요",
          "original_form": "아ㅋㅋㅋㅋㅋ전 허니콤보좋아해요",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100034914.1.13",
          "form": "자주시키는배달음식은 뭐예요",
          "original_form": "자주시키는배달음식은 뭐예요",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100034914.1.14",
          "form": "닭이요 ㅋㅋㅋ  밖에서 여럿이 배달해 먹는건 빼고 집에서 먹는것만요",
          "original_form": "닭이요 ㅋㅋㅋ  밖에서 여럿이 배달해 먹는건 빼고 집에서 먹는것만요",
          "speaker_id": "1",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100034914.1.15",
          "form": "아 그렇군여 그럼 순살보다 뼈닭선호하시나요",
          "original_form": "아 그렇군여 그럼 순살보다 뼈닭선호하시나요",
          "speaker_id": "2",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100034914.1.16",
          "form": "교촌은 콤보를 먹어요 교촌 순살은 너무 말라비틀어졌어요",
          "original_form": "교촌은 콤보를 먹어요 교촌 순살은 너무 말라비틀어졌어요",
          "speaker_id": "1",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100034914.1.17",
          "form": "맞아요ㅜㅜ그리고 일반뼈닭보다 콤보용닭크기가 더큰거같더라구여",
          "original_form": "맞아요ㅜㅜ그리고 일반뼈닭보다 콤보용닭크기가 더큰거같더라구여",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100034914.1.18",
          "form": "네 그래서 다들 콤보를 많이 사먹더라구요",
          "original_form": "네 그래서 다들 콤보를 많이 사먹더라구요",
          "speaker_id": "1",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100034914.1.19",
          "form": "네 저도 교촌은 무조건콤보로먹어요",
          "original_form": "네 저도 교촌은 무조건콤보로먹어요",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100034914.1.20",
          "form": "다른 치킨집은 뭐드세요",
          "original_form": "다른 치킨집은 뭐드세요",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100034914.1.21",
          "form": "교촌과 유사한 호식이랑요 ㅋㅋ",
          "original_form": "교촌과 유사한 호식이랑요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100034914.1.22",
          "form": "닭 본연이 먹고싶다 하면 비에이치씨랑 또래오래요",
          "original_form": "닭 본연이 먹고싶다 하면 비에이치씨랑 또래오래요",
          "speaker_id": "1",
          "time": "20210828 22:19"
        }
      ]
    }
