      "utterance": [
        {
          "id": "MDRW2100043816.1.1",
          "form": "ㅋㅋㅋ 그래도 대충 아실거 같은데요",
          "original_form": "ㅋㅋㅋ 그래도 대충 아실거 같은데요",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043816.1.2",
          "form": "아니 일이 힘들었어서 아예 까먹고 사셨나봐요",
          "original_form": "아니 일이 힘들었어서 아예 까먹고 사셨나봐요",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043816.1.3",
          "form": "아 힘들기도 힘들었죠 제가 거기 간 이유가",
          "original_form": "아 힘들기도 힘들었죠 제가 거기 간 이유가",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043816.1.4",
          "form": "시급이 높아서였거든요 ㅋㅋㅋ",
          "original_form": "시급이 높아서였거든요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043816.1.5",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043816.1.6",
          "form": "그럴거 같았어요",
          "original_form": "그럴거 같았어요",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043816.1.7",
          "form": "노동강도가 센일",
          "original_form": "노동강도가 센일",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043816.1.8",
          "form": "네..ㅋㅋㅋㅋ",
          "original_form": "네..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043816.1.9",
          "form": "힘들어요 ㅠㅠ",
          "original_form": "힘들어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043816.1.10",
          "form": "그래도 재밌었어요",
          "original_form": "그래도 재밌었어요",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043816.1.11",
          "form": "그리고 밥을 엄청 잘 주셔서",
          "original_form": "그리고 밥을 엄청 잘 주셔서",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043816.1.12",
          "form": "행복했어요 ㅋㅋㅋㅋ",
          "original_form": "행복했어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043816.1.13",
          "form": "칼국수 좋아하거든요 그거 먹어도 되고 냉면이나 떡먼둣국도 그냥 메뉴에 있는거 다 먹어도 되고",
          "original_form": "칼국수 좋아하거든요 그거 먹어도 되고 냉면이나 떡먼둣국도 그냥 메뉴에 있는거 다 먹어도 되고",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043816.1.14",
          "form": "ㅋㅋㅋ 밥 잘나오는 곳 좋은 곳이네요",
          "original_form": "ㅋㅋㅋ 밥 잘나오는 곳 좋은 곳이네요",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043816.1.15",
          "form": "아니면 밥이랑 반찬 찌개 해놓으셔서 그거 먹어도 되구요",
          "original_form": "아니면 밥이랑 반찬 찌개 해놓으셔서 그거 먹어도 되구요",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043816.1.16",
          "form": "와 그래도 먹을건 잘 주셔서 다행이었네요",
          "original_form": "와 그래도 먹을건 잘 주셔서 다행이었네요",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043816.1.17",
          "form": "그리고 사장님이 출출할때되면 백화점가서 빵이랑 아이스크림 이런거 잔뜩 사다주샸어욬ㅋㅋ",
          "original_form": "그리고 사장님이 출출할때되면 백화점가서 빵이랑 아이스크림 이런거 잔뜩 사다주샸어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043816.1.18",
          "form": "와 그건 좋았겠어요",
          "original_form": "와 그건 좋았겠어요",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043816.1.19",
          "form": "하지만 일이 너무 힘든게 단점이었겠어요",
          "original_form": "하지만 일이 너무 힘든게 단점이었겠어요",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043816.1.20",
          "form": "네 힘든만큼 좋았어요 ㅋㅋㅋㅋ 그리고 사람 많은날엔 한시간이나 두시간 더 쳐서 주셨어요",
          "original_form": "네 힘든만큼 좋았어요 ㅋㅋㅋㅋ 그리고 사람 많은날엔 한시간이나 두시간 더 쳐서 주셨어요",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043816.1.21",
          "form": "네...ㅋㅋㅋ",
          "original_form": "네...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043816.1.22",
          "form": "하루종일 서있어서 다리가 항상 아팠어욮",
          "original_form": "하루종일 서있어서 다리가 항상 아팠어욮",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043816.1.23",
          "form": "와 그래도 인심좋은 분들이네여",
          "original_form": "와 그래도 인심좋은 분들이네여",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043816.1.24",
          "form": "그쵸 ㅋㅋㅋㅋㅋ 암튼 칼국수 짱 만두 짱",
          "original_form": "그쵸 ㅋㅋㅋㅋㅋ 암튼 칼국수 짱 만두 짱",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043816.1.25",
          "form": "제가 좋아해요",
          "original_form": "제가 좋아해요",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043816.1.26",
          "form": "name2님 뭐 좋아하세요?",
          "original_form": "&name2&님 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043816.1.27",
          "form": "그만두고도 가서 먹을생각 있을거 같은 느낌이었어요",
          "original_form": "그만두고도 가서 먹을생각 있을거 같은 느낌이었어요",
          "speaker_id": "2",
          "time": "20210907 20:33"
        },
        {
