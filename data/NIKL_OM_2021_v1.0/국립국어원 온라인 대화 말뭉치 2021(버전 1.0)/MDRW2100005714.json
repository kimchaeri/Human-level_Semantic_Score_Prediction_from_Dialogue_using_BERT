      "utterance": [
        {
          "id": "MDRW2100005714.1.1",
          "form": "시는 돈이 돼요...!",
          "original_form": "시는 돈이 돼요...!",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005714.1.2",
          "form": "와 투고같은 거 해보셨나요?",
          "original_form": "와 투고같은 거 해보셨나요?",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005714.1.3",
          "form": "대회 있으면 할 수 있는 만큼 써서 내는 편이에요",
          "original_form": "대회 있으면 할 수 있는 만큼 써서 내는 편이에요",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005714.1.4",
          "form": "딱히 대단한 수상을 하진 않았지만 돈이 되는 취미는 소중하죠",
          "original_form": "딱히 대단한 수상을 하진 않았지만 돈이 되는 취미는 소중하죠{emoji:♡}",
          "speaker_id": "2",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005714.1.5",
          "form": "이런 사이드잡 좋네요",
          "original_form": "이런 사이드잡 좋네요",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005714.1.6",
          "form": "시 읽는 것도 쓰는 것도 좋은 취미네요",
          "original_form": "시 읽는 것도 쓰는 것도 좋은 취미네요",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005714.1.7",
          "form": "평소에 책 구매도 많이 하시나요?",
          "original_form": "평소에 책 구매도 많이 하시나요?",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005714.1.8",
          "form": "예전엔 많이 했는데 요즘엔 도서관에서 읽으려고 해요",
          "original_form": "예전엔 많이 했는데 요즘엔 도서관에서 읽으려고 해요",
          "speaker_id": "2",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005714.1.9",
          "form": "저도 주로 그렇게 해요",
          "original_form": "저도 주로 그렇게 해요",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100005714.1.10",
          "form": "희망도서 신청해서 빌려 읽으니 좋더라구요",
          "original_form": "희망도서 신청해서 빌려 읽으니 좋더라구요",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100005714.1.11",
          "form": "그쵸 최근에 도서관에 사랑의 은어 신청해놔서 개강하면 읽을 거예요",
          "original_form": "그쵸 최근에 도서관에 사랑의 은어 신청해놔서 개강하면 읽을 거예요",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100005714.1.12",
          "form": "혹시 책 제목 들어보셨어요?",
          "original_form": "혹시 책 제목 들어보셨어요?",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100005714.1.13",
          "form": "어 인스타에서 봤던 것 같아요",
          "original_form": "어 인스타에서 봤던 것 같아요",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100005714.1.14",
          "form": "작가님 이름이 기억 안나는네",
          "original_form": "작가님 이름이 기억 안나는네",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100005714.1.15",
          "form": "피리부? 작가진 아니였나요?",
          "original_form": "피리부? 작가진 아니였나요?",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100005714.1.16",
          "form": "헐 맞아요",
          "original_form": "헐 맞아요",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100005714.1.17",
          "form": "서한나 님이요",
          "original_form": "서한나 님이요",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100005714.1.18",
          "form": "전 피리부도 아직 못 읽어봤어요 이럴수가",
          "original_form": "전 피리부도 아직 못 읽어봤어요 이럴수가",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100005714.1.19",
          "form": "피리부는 울프가서 읽었거든요 ㅎㅎ",
          "original_form": "피리부는 울프가서 읽었거든요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100005714.1.20",
          "form": "8월 희망도서 고민했었는데 사랑의 은어 추가해야 겠어요",
          "original_form": "8월 희망도서 고민했었는데 사랑의 은어 추가해야 겠어요",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100005714.1.21",
          "form": "세상에 울프가서 여성주의 책 읽기 제 로망이에요",
          "original_form": "세상에 울프가서 여성주의 책 읽기 제 로망이에요",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100005714.1.22",
          "form": "울프 너무 멀어서 아쉬워요",
          "original_form": "울프 너무 멀어서 아쉬워요",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100005714.1.23",
          "form": "저는 친구랑 각잡고 간거라",
          "original_form": "저는 친구랑 각잡고 간거라",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100005714.1.24",
          "form": "코로나만 아니면 또 가보고 싶네요",
          "original_form": "코로나만 아니면 또 가보고 싶네요",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100005714.1.25",
          "form": "진짜 좋은 책 많아요!",
          "original_form": "진짜 좋은 책 많아요!",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100005714.1.26",
          "form": "래디쉬도 1호부터 있어요",
          "original_form": "래디쉬도 1호부터 있어요",
          "speaker_id": "1",
          "time": "20210822 00:17"
        }
      ]
