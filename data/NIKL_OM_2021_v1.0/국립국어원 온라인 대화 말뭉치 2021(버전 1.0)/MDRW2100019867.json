      "utterance": [
        {
          "id": "MDRW2100019867.1.1",
          "form": "남자들은 군대 이야기 제일 좋아하져 ㅎㅎ?",
          "original_form": "남자들은 군대 이야기 제일 좋아하져 ㅎㅎ?",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019867.1.2",
          "form": "아...ㅎㅎ",
          "original_form": "아...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019867.1.3",
          "form": "전 4주밖에 안갔다와서ㅠ",
          "original_form": "전 4주밖에 안갔다와서ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019867.1.4",
          "form": "앜 ㅋㅋ 맞다 ㅋㅋ",
          "original_form": "앜 ㅋㅋ 맞다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019867.1.5",
          "form": "전에 말했었죠ㅎ 배타서ㅎㅎ",
          "original_form": "전에 말했었죠ㅎ 배타서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019867.1.6",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019867.1.7",
          "form": "항해사",
          "original_form": "항해사",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019867.1.8",
          "form": "승선근무예비역이라고ㅎ 3년간 항해사일하면 면제ㅎㅎ",
          "original_form": "승선근무예비역이라고ㅎ 3년간 항해사일하면 면제ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019867.1.9",
          "form": "아 저는 집안에 군인이 많아서 ㅋㅋ",
          "original_form": "아 저는 집안에 군인이 많아서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019867.1.10",
          "form": "군대 이야기가  안 어색하거든요",
          "original_form": "군대 이야기가  안 어색하거든요",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019867.1.11",
          "form": "저 기억력 꽝인가봐요 ㅋㅋㅋㅋ\r\n이야기 들어놓고 기억을 못해 ㅜㅜㅋ",
          "original_form": "저 기억력 꽝인가봐요 ㅋㅋㅋㅋ\r\n이야기 들어놓고 기억을 못해 ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019867.1.12",
          "form": "아아ㅎㅎ",
          "original_form": "아아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019867.1.13",
          "form": "뭐가요ㅎㅎ 까먹는게 당연한거죠ㅎㅎ",
          "original_form": "뭐가요ㅎㅎ 까먹는게 당연한거죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019867.1.14",
          "form": "예전엔 이런거 참 기억 잘했는데 ㅋㅋㅋ",
          "original_form": "예전엔 이런거 참 기억 잘했는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019867.1.15",
          "form": "이런걸 왜기억해요~ㅎㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "이런걸 왜기억해요~ㅎㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019867.1.16",
          "form": "군 동기도 아니고ㅎㅎㅎ",
          "original_form": "군 동기도 아니고ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019867.1.17",
          "form": "선임과 후임도 아닌뎅ㅎㅎㅎ",
          "original_form": "선임과 후임도 아닌뎅ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019867.1.18",
          "form": "그냥 소소한거 기억 잘해요 ㅋㅋ",
          "original_form": "그냥 소소한거 기억 잘해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019867.1.19",
          "form": "지금은 아닌듯 ㅋㅋ",
          "original_form": "지금은 아닌듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019867.1.20",
          "form": "소소한게 좋죠ㅎㅎ",
          "original_form": "소소한게 좋죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019867.1.21",
          "form": "그냥 꾸미지 않는 자연스러움ㅎㅎ",
          "original_form": "그냥 꾸미지 않는 자연스러움ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019867.1.22",
          "form": "군대 주제 정하면 와우  말할게 좀 잇겠다 했는데 ㅋㅋㅋ",
          "original_form": "군대 주제 정하면 와우  말할게 좀 잇겠다 했는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019867.1.23",
          "form": "아무것도아닌데 그냥 늘상 있는 일인데 좋은것ㅎ 꼭 군대에서 누워서 티비보는걱처럼ㅎ",
          "original_form": "아무것도아닌데 그냥 늘상 있는 일인데 좋은것ㅎ 꼭 군대에서 누워서 티비보는걱처럼ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019867.1.24",
          "form": "아 근데 4주도 받긴 받는거자나요",
          "original_form": "아 근데 4주도 받긴 받는거자나요",
          "speaker_id": "1",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019867.1.25",
          "form": "에이ㅎㅎ 장난이죠 그건ㅎ",
          "original_form": "에이ㅎㅎ 장난이죠 그건ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019867.1.26",
          "form": "다른 남자들에 비하면",
          "original_form": "다른 남자들에 비하면",
          "speaker_id": "2",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019867.1.27",
          "form": "에에 근데 군대보다 항해사가 더힘들거 같은데요",
          "original_form": "에에 근데 군대보다 항해사가 더힘들거 같은데요",
          "speaker_id": "1",
          "time": "20210827 22:39"
        },
        {
          "id": "MDRW2100019867.1.28",
          "form": "저희는 회사차량이 델다주고 몸만 상하지않게 돌아와라... 꼭 어디가지말고 배타라 이랬는데요뭐ㅎㅎㅎ 그때 장교들도 몸 안다치시고 잘 맞췄으면 좋겟습니다하고ㅎㅎ",
          "original_form": "저희는 회사차량이 델다주고 몸만 상하지않게 돌아와라... 꼭 어디가지말고 배타라 이랬는데요뭐ㅎㅎㅎ 그때 장교들도 몸 안다치시고 잘 맞췄으면 좋겟습니다하고ㅎㅎ",
