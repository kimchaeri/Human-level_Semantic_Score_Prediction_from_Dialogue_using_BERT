      "utterance": [
        {
          "id": "MDRW2100004313.1.1",
          "form": "안녕하세요! 같이 대화해도 괜찮을까요?",
          "original_form": "안녕하세요! 같이 대화해도 괜찮을까요?",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004313.1.2",
          "form": "네 ~ 운동하세요?",
          "original_form": "네 ~ 운동하세요?",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004313.1.3",
          "form": "운동 하려고 노력하는데 꾸준하진 않아요 ㅋㅋㅋ 운동 하세요?",
          "original_form": "운동 하려고 노력하는데 꾸준하진 않아요 ㅋㅋㅋ 운동 하세요?",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004313.1.4",
          "form": "저도 노력만요 ..",
          "original_form": "저도 노력만요 ..",
          "speaker_id": "2",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004313.1.5",
          "form": "생각만 ..ㅋㅋ",
          "original_form": "생각만 ..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004313.1.6",
          "form": "ㅋㅋㅋ 다 똑같은가봐요... 운동 하면 어떤 거 하세요? 저는 집에서 하거나 혼자 하는 거 위주로 해요",
          "original_form": "ㅋㅋㅋ 다 똑같은가봐요... 운동 하면 어떤 거 하세요? 저는 집에서 하거나 혼자 하는 거 위주로 해요",
          "speaker_id": "1",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004313.1.7",
          "form": "저도 혼자 홈트하거나",
          "original_form": "저도 혼자 홈트하거나",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004313.1.8",
          "form": "혼자 걸으러가는거 좋아해요",
          "original_form": "혼자 걸으러가는거 좋아해요",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004313.1.9",
          "form": "같이는 집중이 안되서..",
          "original_form": "같이는 집중이 안되서..",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004313.1.10",
          "form": "저도 걷기 좋아해요! 근데 저는 대화하면서 걷는 거 좋아해요",
          "original_form": "저도 걷기 좋아해요! 근데 저는 대화하면서 걷는 거 좋아해요",
          "speaker_id": "1",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004313.1.11",
          "form": "저는 세네시간도 걸어본 것 같아요 대화하느라... ㅋㅋㅋㅋ",
          "original_form": "저는 세네시간도 걸어본 것 같아요 대화하느라... ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004313.1.12",
          "form": "맞아요 저도 걷는게 좋아용",
          "original_form": "맞아요 저도 걷는게 좋아용",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004313.1.13",
          "form": "숨차는 운동은 힘들잖아여 ㅠㅠ",
          "original_form": "숨차는 운동은 힘들잖아여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004313.1.14",
          "form": "맞아요 땀나고ㅜ 저는 여름~가을밤에 대화하면서 걷는 거 제일 좋아해요!",
          "original_form": "맞아요 땀나고ㅜ 저는 여름~가을밤에 대화하면서 걷는 거 제일 좋아해요!",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004313.1.15",
          "form": "근데 이사하고나니까 동네친구가 없어서 못해가지고ㅜ 너무 아쉬워요",
          "original_form": "근데 이사하고나니까 동네친구가 없어서 못해가지고ㅜ 너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004313.1.16",
          "form": "그냥 숨쉬기 운동만 하는중 ..",
          "original_form": "그냥 숨쉬기 운동만 하는중 ..",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004313.1.17",
          "form": "아아 저도 타지와서 혼자에 익숙해진거있죠 ㅋㅋ",
          "original_form": "아아 저도 타지와서 혼자에 익숙해진거있죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004313.1.18",
          "form": "그쵸ㅠㅠ 동네친구 구하는 게 정말 어려운 것 같아요",
          "original_form": "그쵸ㅠㅠ 동네친구 구하는 게 정말 어려운 것 같아요",
          "speaker_id": "1",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004313.1.19",
          "form": "동네친구랑 같이 운동하고싶네영 ㅠㅜ",
          "original_form": "동네친구랑 같이 운동하고싶네영 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004313.1.20",
