      "utterance": [
        {
          "id": "MDRW2100010634.1.1",
          "form": "유튜브로 뭐 자주 보세요?",
          "original_form": "유튜브로 뭐 자주 보세요?",
          "speaker_id": "2",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010634.1.2",
          "form": "헉 저 방금 그 준우 아시나요",
          "original_form": "헉 저 방금 그 준우 아시나요",
          "speaker_id": "1",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010634.1.3",
          "form": "카톡대신 손절",
          "original_form": "카톡대신 손절",
          "speaker_id": "1",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010634.1.4",
          "form": "이런거 하시는 분",
          "original_form": "이런거 하시는 분",
          "speaker_id": "1",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010634.1.5",
          "form": "그분 거 보고 열받고 있었어요",
          "original_form": "그분 거 보고 열받고 있었어요",
          "speaker_id": "1",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010634.1.6",
          "form": "오 뭔가요.. 궁금하네요",
          "original_form": "오 뭔가요.. 궁금하네요",
          "speaker_id": "2",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010634.1.7",
          "form": "그게 약간 막 친구 돈없다고 무시하는데 이제 본인이 열받아서 잘 못말할 것 같을 때 신청하는건가봐요",
          "original_form": "그게 약간 막 친구 돈없다고 무시하는데 이제 본인이 열받아서 잘 못말할 것 같을 때 신청하는건가봐요",
          "speaker_id": "1",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100010634.1.8",
          "form": "그래서 준우라는 유튜버가 대신 카톡에 접속해서",
          "original_form": "그래서 준우라는 유튜버가 대신 카톡에 접속해서",
          "speaker_id": "1",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010634.1.9",
          "form": "거짓말하는 거 있으면 밝혀내고 대신 손절해주는 콘텐츠거든요",
          "original_form": "거짓말하는 거 있으면 밝혀내고 대신 손절해주는 콘텐츠거든요",
          "speaker_id": "1",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010634.1.10",
          "form": "알고리즘에 걸려서 다봣어요",
          "original_form": "알고리즘에 걸려서 다봣어요",
          "speaker_id": "1",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010634.1.11",
          "form": "재프리님은 최근에 유툽에서 어떤거 보셨나요?!",
          "original_form": "재프리님은 최근에 유툽에서 어떤거 보셨나요?!",
          "speaker_id": "1",
          "time": "20210823 20:21"
        },
        {
          "id": "MDRW2100010634.1.12",
          "form": "아하 신기한 컨텐츠가 많네요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아하 신기한 컨텐츠가 많네요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:21"
        },
        {
          "id": "MDRW2100010634.1.13",
          "form": "저 심야괴담회 봤는데 혹시 아세여?",
          "original_form": "저 심야괴담회 봤는데 혹시 아세여?",
          "speaker_id": "2",
          "time": "20210823 20:21"
        },
        {
          "id": "MDRW2100010634.1.14",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210823 20:23"
        },
        {
          "id": "MDRW2100010634.1.15",
          "form": "허어어어얼",
          "original_form": "허어어어얼",
          "speaker_id": "1",
          "time": "20210823 20:23"
        },
        {
          "id": "MDRW2100010634.1.16",
          "form": "저 그프로 완전 좋아해요",
          "original_form": "저 그프로 완전 좋아해요",
          "speaker_id": "1",
          "time": "20210823 20:23"
        },
        {
          "id": "MDRW2100010634.1.17",
          "form": "그것이 알고싶다 같은거만 보다가",
          "original_form": "그것이 알고싶다 같은거만 보다가",
          "speaker_id": "1",
          "time": "20210823 20:23"
        },
        {
          "id": "MDRW2100010634.1.18",
          "form": "귀신보니까 너무 좋은거 있죠",
          "original_form": "귀신보니까 너무 좋은거 있죠",
          "speaker_id": "1",
          "time": "20210823 20:23"
        },
        {
          "id": "MDRW2100010634.1.19",
          "form": "근데 무섭지 않아요?",
          "original_form": "근데 무섭지 않아요?",
          "speaker_id": "2",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100010634.1.20",
          "form": "무서운 거 잘보세요??!",
          "original_form": "무서운 거 잘보세요??!",
          "speaker_id": "2",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100010634.1.21",
          "form": "저 좋아해요 무서운거",
          "original_form": "저 좋아해요 무서운거",
          "speaker_id": "1",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100010634.1.22",
          "form": "근데 거의 눈감고 보긴 해요(?)",
          "original_form": "근데 거의 눈감고 보긴 해요(?)",
          "speaker_id": "1",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100010634.1.23",
          "form": "곤지암 보셨나요 영화",
          "original_form": "곤지암 보셨나요 영화",
          "speaker_id": "1",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010634.1.24",
          "form": "그거 어쩌다 2번 영화관에서 봤는데 첨보는 사람이랑 친해졌어요",
          "original_form": "그거 어쩌다 2번 영화관에서 봤는데 첨보는 사람이랑 친해졌어요",
          "speaker_id": "1",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010634.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 어쩌다가요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 어쩌다가요?",
          "speaker_id": "2",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010634.1.26",
          "form": "진짜...진짜 무섭더라고요 영화관..",
          "original_form": "진짜...진짜 무섭더라고요 영화관..",
          "speaker_id": "1",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010634.1.27",
          "form": "전 좋아하는데 못 봐요 너무 무서워요ㅋㅋㅋㅋㅋㅋㅌ",
          "original_form": "전 좋아하는데 못 봐요 너무 무서워요ㅋㅋㅋㅋㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010634.1.28",
          "form": "아니 처음에는 서로 눈치보느라 리액션을 못했는데",
          "original_form": "아니 처음에는 서로 눈치보느라 리액션을 못했는데",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010634.1.29",
          "form": "한 사람이 물꼬트니까 다들 소리지르면서 난리났어요",
          "original_form": "한 사람이 물꼬트니까 다들 소리지르면서 난리났어요",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010634.1.30",
          "form": "근데 거기 저랑 친구랑 그 쪽 무리 2명",
          "original_form": "근데 거기 저랑 친구랑 그 쪽 무리 2명",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010634.1.31",
          "form": "이렇게 4명 밖에 없었는데",
          "original_form": "이렇게 4명 밖에 없었는데",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010634.1.32",
          "form": "영화 끝나고 나설 때는 이제 전우애가 생기더라고요",
          "original_form": "영화 끝나고 나설 때는 이제 전우애가 생기더라고요",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010634.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ 우와 신기하네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 우와 신기하네요",
          "speaker_id": "2",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010634.1.34",
          "form": "같이 무서운 거 보니까 통했나봐요",
          "original_form": "같이 무서운 거 보니까 통했나봐요",
          "speaker_id": "2",
          "time": "20210823 21:39"
        }
      ]
    }
  ]
