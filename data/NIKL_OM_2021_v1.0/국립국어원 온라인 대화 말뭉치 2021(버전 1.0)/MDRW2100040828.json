        },
        {
          "id": "MDRW2100040828.1.2",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210905 09:09"
        },
        {
          "id": "MDRW2100040828.1.3",
          "form": "예전에 키웠었어요",
          "original_form": "예전에 키웠었어요",
          "speaker_id": "2",
          "time": "20210905 09:17"
        },
        {
          "id": "MDRW2100040828.1.4",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "2",
          "time": "20210905 09:17"
        },
        {
          "id": "MDRW2100040828.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100040828.1.6",
          "form": "저는 예전에 반려동물 청거북이 키웠어요",
          "original_form": "저는 예전에 반려동물 청거북이 키웠어요",
          "speaker_id": "1",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100040828.1.7",
          "form": "거북이요?",
          "original_form": "거북이요?",
          "speaker_id": "2",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100040828.1.8",
          "form": "흔치 않은 동물이네요",
          "original_form": "흔치 않은 동물이네요",
          "speaker_id": "2",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100040828.1.9",
          "form": "거북이 키우기 쉬웠나요?",
          "original_form": "거북이 키우기 쉬웠나요?",
          "speaker_id": "2",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100040828.1.10",
          "form": "네 어항에서 그니까요",
          "original_form": "네 어항에서 그니까요",
          "speaker_id": "1",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100040828.1.11",
          "form": "강아지 키우실 때 목욕은 자녀들이 시켰나요?",
          "original_form": "강아지 키우실 때 목욕은 자녀들이 시켰나요?",
          "speaker_id": "1",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100040828.1.12",
          "form": "그럴리가요..ㅎㅎ",
          "original_form": "그럴리가요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100040828.1.13",
          "form": "데리고 올때 분명 자기가  한다고 허락해달라고 하더니만",
          "original_form": "데리고 올때 분명 자기가  한다고 허락해달라고 하더니만",
          "speaker_id": "2",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100040828.1.14",
          "form": "다 무임승차 했네요",
          "original_form": "다 무임승차 했네요",
          "speaker_id": "1",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100040828.1.15",
          "form": "결국 엄마몫이 됐죠",
          "original_form": "결국 엄마몫이 됐죠",
          "speaker_id": "2",
          "time": "20210905 09:24"
        },
        {
          "id": "MDRW2100040828.1.16",
          "form": "그럴 줄 알고 반대했던건데",
          "original_form": "그럴 줄 알고 반대했던건데",
          "speaker_id": "2",
          "time": "20210905 09:24"
        },
        {
          "id": "MDRW2100040828.1.17",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 09:24"
        },
        {
          "id": "MDRW2100040828.1.18",
          "form": "자식 이기는 부모 없죠..ㅎㅎ",
          "original_form": "자식 이기는 부모 없죠..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:24"
        },
        {
          "id": "MDRW2100040828.1.19",
          "form": "저희 엄마는 애초에 이거 다 내 일이 될 거라며 반대해요",
          "original_form": "저희 엄마는 애초에 이거 다 내 일이 될 거라며 반대해요",
          "speaker_id": "1",
          "time": "20210905 09:24"
        },
        {
          "id": "MDRW2100040828.1.20",
          "form": "그쵸 근데 그게 사실이기도 하고요",
          "original_form": "그쵸 근데 그게 사실이기도 하고요",
          "speaker_id": "2",
          "time": "20210905 09:30"
        },
        {
          "id": "MDRW2100040828.1.21",
