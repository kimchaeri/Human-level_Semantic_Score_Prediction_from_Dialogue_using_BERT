      "utterance": [
        {
          "id": "MDRW2100000316.1.1",
          "form": "키우시는 반려동물 있으세요?",
          "original_form": "키우시는 반려동물 있으세요?",
          "speaker_id": "2",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000316.1.2",
          "form": "저는 이전에 웰시코기 키웠었어요",
          "original_form": "저는 이전에 웰시코기 키웠었어요",
          "speaker_id": "1",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000316.1.3",
          "form": "name2님은 키우시는 반려동물 있으세요?",
          "original_form": "&name2&님은 키우시는 반려동물 있으세요?",
          "speaker_id": "1",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000316.1.4",
          "form": "아뇨ㅋㅋ저는 진짜 옛날에 물고기만 키워봤어요",
          "original_form": "아뇨ㅋㅋ저는 진짜 옛날에 물고기만 키워봤어요",
          "speaker_id": "2",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000316.1.5",
          "form": "앗 물고기도 귀엽겠어요",
          "original_form": "앗 물고기도 귀엽겠어요",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000316.1.6",
          "form": "웰시코기는 참 귀여운데 털을 정말 많이 뿜어요 ㅋㅋ",
          "original_form": "웰시코기는 참 귀여운데 털을 정말 많이 뿜어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000316.1.7",
          "form": "ㅋㅋㅋㅋ웰시코기는 귀여운 엉덩이가 포인트아닌가요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ웰시코기는 귀여운 엉덩이가 포인트아닌가요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000316.1.8",
          "form": "털 많이 빠지는거 관리하기 힘들죠ㅜㅜ",
          "original_form": "털 많이 빠지는거 관리하기 힘들죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000316.1.9",
          "form": "네 정말",
          "original_form": "네 정말",
          "speaker_id": "1",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000316.1.10",
          "form": "검은옷을 입으면 드러나는 존재감..",
          "original_form": "검은옷을 입으면 드러나는 존재감..",
          "speaker_id": "1",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000316.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ끈끈이? 가 필요하죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ끈끈이? 가 필요하죠",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000316.1.12",
          "form": "끈끈이 청소기 건조기 필수요 ㅋㅋ",
          "original_form": "끈끈이 청소기 건조기 필수요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000316.1.13",
          "form": "그래도 그런걸 다 감안할정도로 이뻐요",
          "original_form": "그래도 그런걸 다 감안할정도로 이뻐요",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000316.1.14",
          "form": "ㅋㅋㅋ맞아요 이쁘죠ㅜ 저는 키울 여건이 안되어서 가끔 고양이카페에 가요",
          "original_form": "ㅋㅋㅋ맞아요 이쁘죠ㅜ 저는 키울 여건이 안되어서 가끔 고양이카페에 가요",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000316.1.15",
          "form": "고양이 좋아하시나요?",
          "original_form": "고양이 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000316.1.16",
          "form": "저도 고양이도 좋아해요",
          "original_form": "저도 고양이도 좋아해요",
          "speaker_id": "1",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000316.1.17",
          "form": "요즘더 고양이 카페가 있나봐요! 예전에 유행했잖아요",
          "original_form": "요즘더 고양이 카페가 있나봐요! 예전에 유행했잖아요",
          "speaker_id": "1",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000316.1.18",
          "form": "네! 아직 있어요ㅋㅋ요즘은 못가고있지만요ㅜ",
          "original_form": "네! 아직 있어요ㅋㅋ요즘은 못가고있지만요ㅜ",
          "speaker_id": "2",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000316.1.19",
          "form": "요즘은..그렇겠네요ㅠㅠ",
          "original_form": "요즘은..그렇겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000316.1.20",
          "form": "저는 예전에 갔을때 본체만체하더니 닭가슴살 간식 사니까 몰려들던 슬픈 기억이 있어요",
          "original_form": "저는 예전에 갔을때 본체만체하더니 닭가슴살 간식 사니까 몰려들던 슬픈 기억이 있어요",
          "speaker_id": "1",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000316.1.21",
          "form": "자본주의 고양이들..",
          "original_form": "자본주의 고양이들..",
          "speaker_id": "1",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000316.1.22",
          "form": "ㅋㅋㅋㅋㅋ자본주의 고양이들ㅋㅋㅋㅋ너무 웃겨요",
          "original_form": "ㅋㅋㅋㅋㅋ자본주의 고양이들ㅋㅋㅋㅋ너무 웃겨요",
          "speaker_id": "2",
          "time": "20210819 17:45"
        }
      ]
    }
