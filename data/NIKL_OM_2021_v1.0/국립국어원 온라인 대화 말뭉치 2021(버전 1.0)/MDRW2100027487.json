      "utterance": [
        {
          "id": "MDRW2100027487.1.1",
          "form": "도서관가면 무슨책읽어??ㅎㅎ",
          "original_form": "도서관가면 무슨책읽어??ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027487.1.2",
          "form": "나는 도서관에 잘 안가ㅎㅎ",
          "original_form": "나는 도서관에 잘 안가ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027487.1.3",
          "form": "너는 도서관에 얼마나 자주 가는디??",
          "original_form": "너는 도서관에 얼마나 자주 가는디??",
          "speaker_id": "1",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027487.1.4",
          "form": "나는 도서관은 안가구 서점가 ㅎㅎ",
          "original_form": "나는 도서관은 안가구 서점가 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027487.1.5",
          "form": "서점에서 베스트셀러를 보는데 괜찮은 책들이 많다룩",
          "original_form": "서점에서 베스트셀러를 보는데 괜찮은 책들이 많다룩",
          "speaker_id": "2",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027487.1.6",
          "form": "많더라두",
          "original_form": "많더라두",
          "speaker_id": "2",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027487.1.7",
          "form": "많더라구 ㅎㅎ",
          "original_form": "많더라구 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027487.1.8",
          "form": "일리있네..",
          "original_form": "일리있네..",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027487.1.9",
          "form": "근데 나는 서점보다 인터냇 서점이 좋더라 ㅎㅎ",
          "original_form": "근데 나는 서점보다 인터냇 서점이 좋더라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027487.1.10",
          "form": "그럴수있지 ㅎㅎ",
          "original_form": "그럴수있지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027487.1.11",
          "form": "그리고 인터넷 책이 더 싸잖아 ㅎㅎ",
          "original_form": "그리고 인터넷 책이 더 싸잖아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027487.1.12",
          "form": "마자마자",
          "original_form": "마자마자",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027487.1.13",
          "form": "이북사면 보기도 편하구",
          "original_form": "이북사면 보기도 편하구",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027487.1.14",
          "form": "맞어 ㅎㅎ",
          "original_form": "맞어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027487.1.15",
          "form": "우리집에도 괴델 에셔 바흐라는 책 잇는데",
          "original_form": "우리집에도 괴델 에셔 바흐라는 책 잇는데",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027487.1.16",
          "form": "두꺼워서 펼치지도 못하잖아..",
          "original_form": "두꺼워서 펼치지도 못하잖아..",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027487.1.17",
          "form": "그랭?",
          "original_form": "그랭?",
          "speaker_id": "2",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100027487.1.18",
          "form": "마자 ㅎㅎ",
          "original_form": "마자 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100027487.1.19",
          "form": "그런데 잡지는 실물이 더 나은거같아 ㅎㅎ",
          "original_form": "그런데 잡지는 실물이 더 나은거같아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100027487.1.20",
          "form": "잡지 본적 잇어?",
          "original_form": "잡지 본적 잇어?",
          "speaker_id": "1",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100027487.1.21",
          "form": "과학동산인가",
          "original_form": "과학동산인가",
          "speaker_id": "2",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100027487.1.22",
          "form": "과학나라?",
          "original_form": "과학나라?",
          "speaker_id": "2",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100027487.1.23",
          "form": "예전에 학교 도서관 문앞에 가끔 잇엇는데...",
          "original_form": "예전에 학교 도서관 문앞에 가끔 잇엇는데...",
          "speaker_id": "1",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100027487.1.24",
          "form": "재밌겠다 ㅎㅎ",
          "original_form": "재밌겠다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027487.1.25",
          "form": "먼지가 쌓여잇기도 했지",
          "original_form": "먼지가 쌓여잇기도 했지",
          "speaker_id": "1",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027487.1.26",
          "form": "드럽긴 하네",
          "original_form": "드럽긴 하네",
          "speaker_id": "2",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027487.1.27",
          "form": "역시 인터넷책이 좋은거같애 ㅎㅎ",
          "original_form": "역시 인터넷책이 좋은거같애 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027487.1.28",
          "form": "웃긴다",
          "original_form": "웃긴다",
          "speaker_id": "1",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027487.1.29",
          "form": "나도 그렇게 생각해 ㅎㅎ 보관도 계속 할 수 잇고",
          "original_form": "나도 그렇게 생각해 ㅎㅎ 보관도 계속 할 수 잇고",
          "speaker_id": "1",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027487.1.30",
          "form": "맞아 ㅎㅎ",
          "original_form": "맞아 ㅎㅎ",
