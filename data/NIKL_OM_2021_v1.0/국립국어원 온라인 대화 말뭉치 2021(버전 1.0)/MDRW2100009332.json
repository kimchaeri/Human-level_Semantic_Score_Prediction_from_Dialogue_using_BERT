        {
          "id": "MDRW2100009332.1.1",
          "form": "무슨음식 좋아하세요?",
          "original_form": "무슨음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009332.1.2",
          "form": "저 떡볶이랑 치킨이요",
          "original_form": "저 떡볶이랑 치킨이요",
          "speaker_id": "1",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009332.1.3",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009332.1.4",
          "form": "와 요즘 떡볶이랑치킨 세트로 팔잖아요 굿",
          "original_form": "와 요즘 떡볶이랑치킨 세트로 팔잖아요 굿",
          "speaker_id": "2",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009332.1.5",
          "form": "맞아요 말하니까 먹고싶네요",
          "original_form": "맞아요 말하니까 먹고싶네요",
          "speaker_id": "1",
          "time": "20210822 22:48"
        },
        {
          "id": "MDRW2100009332.1.6",
          "form": "저도 떡볶이 엄청좋아해요",
          "original_form": "저도 떡볶이 엄청좋아해요",
          "speaker_id": "2",
          "time": "20210822 22:48"
        },
        {
          "id": "MDRW2100009332.1.7",
          "form": "매콤한거 좋아하거든요",
          "original_form": "매콤한거 좋아하거든요",
          "speaker_id": "2",
          "time": "20210822 22:49"
        },
        {
          "id": "MDRW2100009332.1.8",
          "form": "그러시구나 전 매운거 잘 못먹는데 그냥 먹어요",
          "original_form": "그러시구나 전 매운거 잘 못먹는데 그냥 먹어요",
          "speaker_id": "1",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100009332.1.9",
          "form": "요즘 점심식사로 어떤거 주로 드세요??",
          "original_form": "요즘 점심식사로 어떤거 주로 드세요??",
          "speaker_id": "1",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100009332.1.10",
          "form": "주로 한식이요 ㅎ",
          "original_form": "주로 한식이요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100009332.1.11",
          "form": "밥 좋아하거든요",
          "original_form": "밥 좋아하거든요",
          "speaker_id": "2",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100009332.1.12",
          "form": "님은 ?",
          "original_form": "님은 ?",
          "speaker_id": "2",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100009332.1.13",
          "form": "저도 한식이요!",
          "original_form": "저도 한식이요!",
          "speaker_id": "1",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100009332.1.14",
          "form": "sns보고 레시피 따라해본적 있으세요?",
          "original_form": "sns보고 레시피 따라해본적 있으세요?",
          "speaker_id": "1",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100009332.1.15",
          "form": "네넹 음식하는거 좋아해요",
          "original_form": "네넹 음식하는거 좋아해요",
          "speaker_id": "2",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100009332.1.16",
          "form": "하는거 좋아하세요?",
          "original_form": "하는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009332.1.17",
          "form": "앗 네 저도 만들어 먹는거 좋아해요",
          "original_form": "앗 네 저도 만들어 먹는거 좋아해요",
          "speaker_id": "1",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009332.1.18",
          "form": "어떤거 만들어 보셨어요?",
          "original_form": "어떤거 만들어 보셨어요?",
