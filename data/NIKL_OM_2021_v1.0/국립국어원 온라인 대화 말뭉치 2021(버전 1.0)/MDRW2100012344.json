      "utterance": [
        {
          "id": "MDRW2100012344.1.1",
          "form": "일하는거 재미있나??",
          "original_form": "일하는거 재미있나??",
          "speaker_id": "1",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012344.1.2",
          "form": "일하는건 재밌지",
          "original_form": "일하는건 재밌지",
          "speaker_id": "2",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100012344.1.3",
          "form": "니는 회사 다니고싶나?",
          "original_form": "니는 회사 다니고싶나?",
          "speaker_id": "2",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100012344.1.4",
          "form": "어 일하고싶네",
          "original_form": "어 일하고싶네",
          "speaker_id": "1",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100012344.1.5",
          "form": "일하면 힘들겠지만",
          "original_form": "일하면 힘들겠지만",
          "speaker_id": "1",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100012344.1.6",
          "form": "직장사람 힘든건없나?",
          "original_form": "직장사람 힘든건없나?",
          "speaker_id": "1",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012344.1.7",
          "form": "여기는 아직은...",
          "original_form": "여기는 아직은...",
          "speaker_id": "2",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012344.1.8",
          "form": "옛날 회사가고싶나?",
          "original_form": "옛날 회사가고싶나?",
          "speaker_id": "2",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012344.1.9",
          "form": "난 아직 예전회사사람들 꿈꾼다",
          "original_form": "난 아직 예전회사사람들 꿈꾼다",
          "speaker_id": "1",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012344.1.10",
          "form": "안가고싶다",
          "original_form": "안가고싶다",
          "speaker_id": "1",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012344.1.11",
          "form": "일많고... 악몽이다",
          "original_form": "일많고... 악몽이다",
          "speaker_id": "1",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012344.1.12",
          "form": "그럼 어디 취직하고싶은데?",
          "original_form": "그럼 어디 취직하고싶은데?",
          "speaker_id": "2",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012344.1.13",
          "form": "취직은 모르겠다",
          "original_form": "취직은 모르겠다",
          "speaker_id": "1",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012344.1.14",
          "form": "뭘해야할지 뭘잘할지..",
          "original_form": "뭘해야할지 뭘잘할지..",
          "speaker_id": "1",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012344.1.15",
          "form": "가게 오픈을 해야하나싶기도하고",
          "original_form": "가게 오픈을 해야하나싶기도하고",
          "speaker_id": "1",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012344.1.16",
          "form": "무슨 가게하고싶은데?",
          "original_form": "무슨 가게하고싶은데?",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012344.1.17",
          "form": "모르겠다\n난 잘모르겠다",
          "original_form": "모르겠다\n난 잘모르겠다",
          "speaker_id": "1",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100012344.1.18",
          "form": "뭔가는 하고싶은데..잘할줄아는게 없고",
          "original_form": "뭔가는 하고싶은데..잘할줄아는게 없고",
          "speaker_id": "1",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100012344.1.19",
          "form": "커피숍이 어울리긴하다",
          "original_form": "커피숍이 어울리긴하다",
          "speaker_id": "2",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100012344.1.20",
          "form": "모르겠다\n빵집도괜찮을거같긴하고",
          "original_form": "모르겠다\n빵집도괜찮을거같긴하고",
          "speaker_id": "1",
          "time": "20210824 21:38"
        },
        {
          "id": "MDRW2100012344.1.21",
          "form": "그럼 베이킹을 해야지 파바가 제일 나을듯",
          "original_form": "그럼 베이킹을 해야지 파바가 제일 나을듯",
          "speaker_id": "2",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100012344.1.22",
          "form": "모르겠다 나의미래를... 우에하고살아야할지",
          "original_form": "모르겠다 나의미래를... 우에하고살아야할지",
          "speaker_id": "1",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100012344.1.23",
          "form": "직업있어서 부럽다",
          "original_form": "직업있어서 부럽다",
          "speaker_id": "1",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100012344.1.24",
          "form": "그냥 살면 되지 뭐",
          "original_form": "그냥 살면 되지 뭐",
          "speaker_id": "2",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100012344.1.25",
          "form": "고민하지마라",
          "original_form": "고민하지마라",
          "speaker_id": "2",
          "time": "20210824 21:41"
        },
        {
          "id": "MDRW2100012344.1.26",
          "form": "삶이 재미가없잖아",
          "original_form": "삶이 재미가없잖아",
          "speaker_id": "1",
          "time": "20210824 21:42"
        },
        {
          "id": "MDRW2100012344.1.27",
          "form": "일을하면 성취욕이 생기는데",
          "original_form": "일을하면 성취욕이 생기는데",
          "speaker_id": "1",
          "time": "20210824 21:42"
        },
        {
          "id": "MDRW2100012344.1.28",
          "form": "일해도 그럭저럭이다",
          "original_form": "일해도 그럭저럭이다",
          "speaker_id": "2",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100012344.1.29",
          "form": "삶은 뭘까..라는 생각이자주드네",
          "original_form": "삶은 뭘까..라는 생각이자주드네",
          "speaker_id": "1",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100012344.1.30",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100012344.1.31",
          "form": "공부를 해라~",
          "original_form": "공부를 해라~",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012344.1.32",
          "form": "구직활동할수 있는 공부",
          "original_form": "구직활동할수 있는 공부",
          "speaker_id": "2",
          "time": "20210824 21:44"
        }
      ]
    }
  ]
