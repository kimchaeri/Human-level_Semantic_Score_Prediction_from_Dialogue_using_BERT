        },
        {
          "id": "MDRW2100036641.1.2",
          "form": "야채 좋아하세요?",
          "original_form": "야채 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 03:40"
        },
        {
          "id": "MDRW2100036641.1.3",
          "form": "무난",
          "original_form": "무난",
          "speaker_id": "2",
          "time": "20210831 03:40"
        },
        {
          "id": "MDRW2100036641.1.4",
          "form": "어떤야채 최애에여?",
          "original_form": "어떤야채 최애에여?",
          "speaker_id": "1",
          "time": "20210831 03:41"
        },
        {
          "id": "MDRW2100036641.1.5",
          "form": "저는.....\r\n..........\r\n\r\n.......구황작물 되나여?",
          "original_form": "저는.....\r\n..........\r\n\r\n.......구황작물 되나여?",
          "speaker_id": "2",
          "time": "20210831 03:41"
        },
        {
          "id": "MDRW2100036641.1.6",
          "form": "구황작물?",
          "original_form": "구황작물?",
          "speaker_id": "1",
          "time": "20210831 03:41"
        },
        {
          "id": "MDRW2100036641.1.7",
          "form": "감자 고구마여",
          "original_form": "감자 고구마여",
          "speaker_id": "2",
          "time": "20210831 03:42"
        },
        {
          "id": "MDRW2100036641.1.8",
          "form": "조, 피, 메밀은요?",
          "original_form": "조, 피, 메밀은요?",
          "speaker_id": "1",
          "time": "20210831 03:43"
        },
        {
          "id": "MDRW2100036641.1.9",
          "form": "저는 파프리카, 양파, 감자 좋아해여",
          "original_form": "저는 파프리카, 양파, 감자 좋아해여",
          "speaker_id": "2",
          "time": "20210831 03:43"
        },
        {
          "id": "MDRW2100036641.1.10",
          "form": "아니 뭔소리져?",
          "original_form": "아니 뭔소리져?",
          "speaker_id": "2",
          "time": "20210831 03:43"
        },
        {
          "id": "MDRW2100036641.1.11",
          "form": "저도 물어봐줘요",
          "original_form": "저도 물어봐줘요",
          "speaker_id": "1",
          "time": "20210831 03:44"
        },
        {
          "id": "MDRW2100036641.1.12",
          "form": "어떤야채가 최애에여?",
          "original_form": "어떤야채가 최애에여?",
          "speaker_id": "2",
          "time": "20210831 03:44"
        },
        {
          "id": "MDRW2100036641.1.13",
          "form": "마늘 생양파 파채",
          "original_form": "마늘 생양파 파채",
          "speaker_id": "1",
          "time": "20210831 03:44"
        },
        {
          "id": "MDRW2100036641.1.14",
          "form": "아.. 혹시",
          "original_form": "아.. 혹시",
          "speaker_id": "2",
          "time": "20210831 03:44"
        },
        {
