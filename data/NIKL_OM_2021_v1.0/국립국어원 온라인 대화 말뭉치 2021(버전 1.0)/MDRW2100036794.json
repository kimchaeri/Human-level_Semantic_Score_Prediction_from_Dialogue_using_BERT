      "utterance": [
        {
          "id": "MDRW2100036794.1.1",
          "form": "점심 드셨어요?!",
          "original_form": "점심 드셨어요?!",
          "speaker_id": "1",
          "time": "20210831 12:16"
        },
        {
          "id": "MDRW2100036794.1.2",
          "form": "아뇨 name1님은요? 점심드셨어요?",
          "original_form": "아뇨 &name1&님은요? 점심드셨어요?",
          "speaker_id": "2",
          "time": "20210831 12:18"
        },
        {
          "id": "MDRW2100036794.1.3",
          "form": "지금 먹고있어요!",
          "original_form": "지금 먹고있어요!",
          "speaker_id": "1",
          "time": "20210831 12:18"
        },
        {
          "id": "MDRW2100036794.1.4",
          "form": "언제드세요??",
          "original_form": "언제드세요??",
          "speaker_id": "1",
          "time": "20210831 12:18"
        },
        {
          "id": "MDRW2100036794.1.5",
          "form": "글쎄요 아침을 늦게먹었어요",
          "original_form": "글쎄요 아침을 늦게먹었어요",
          "speaker_id": "2",
          "time": "20210831 12:18"
        },
        {
          "id": "MDRW2100036794.1.6",
          "form": "앗ㅠ 그러시구나",
          "original_form": "앗ㅠ 그러시구나",
          "speaker_id": "1",
          "time": "20210831 12:24"
        },
        {
          "id": "MDRW2100036794.1.7",
          "form": "저는 전을 먹어용!",
          "original_form": "저는 전을 먹어용!",
          "speaker_id": "1",
          "time": "20210831 12:24"
        },
        {
          "id": "MDRW2100036794.1.8",
          "form": "전 좋아하세요?",
          "original_form": "전 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100036794.1.9",
          "form": "네 전 없어서 못먹죠 ㅋㅋ",
          "original_form": "네 전 없어서 못먹죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100036794.1.10",
          "form": "ㅋㅋ역시!",
          "original_form": "ㅋㅋ역시!",
          "speaker_id": "1",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100036794.1.11",
          "form": "진짜 전은 손이 많이가도 먹어도 먹어도 먹고싶어요ㅋㅋ",
          "original_form": "진짜 전은 손이 많이가도 먹어도 먹어도 먹고싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100036794.1.12",
          "form": "고소하고 맛있고 그래요 ㅎㅎㅎ",
          "original_form": "고소하고 맛있고 그래요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 12:27"
        },
        {
          "id": "MDRW2100036794.1.13",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:31"
        },
        {
          "id": "MDRW2100036794.1.14",
          "form": "배부름 젓가락 좀 내려놓고 그래야하는데말예요",
          "original_form": "배부름 젓가락 좀 내려놓고 그래야하는데말예요",
          "speaker_id": "1",
          "time": "20210831 12:31"
        },
        {
          "id": "MDRW2100036794.1.15",
          "form": "어릴때부터 맛있더라구요 제사지낼때 만들잖아요",
          "original_form": "어릴때부터 맛있더라구요 제사지낼때 만들잖아요",
          "speaker_id": "2",
          "time": "20210831 12:32"
        },
        {
          "id": "MDRW2100036794.1.16",
          "form": "오 저는 제사해본적이 없어서 몰랐는데 전도 올리는군요!",
          "original_form": "오 저는 제사해본적이 없어서 몰랐는데 전도 올리는군요!",
          "speaker_id": "1",
          "time": "20210831 12:37"
        },
        {
          "id": "MDRW2100036794.1.17",
          "form": "네 전이 들어가요 ㅎㅎ",
          "original_form": "네 전이 들어가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 12:38"
        },
        {
          "id": "MDRW2100036794.1.18",
          "form": "오ㅋㅋ 몰랐어요",
          "original_form": "오ㅋㅋ 몰랐어요",
