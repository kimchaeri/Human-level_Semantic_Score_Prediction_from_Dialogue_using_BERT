        {
          "id": "MDRW2100029532.1.1",
          "form": "오 국물닭발 맛집알아요",
          "original_form": "오 국물닭발 맛집알아요",
          "speaker_id": "2",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100029532.1.2",
          "form": "저녁에 양배추 쪄서 싸서 먹으려합니다",
          "original_form": "저녁에 양배추 쪄서 싸서 먹으려합니다",
          "speaker_id": "1",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100029532.1.3",
          "form": "인천에 웅이네 닭발",
          "original_form": "인천에 웅이네 닭발",
          "speaker_id": "2",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100029532.1.4",
          "form": "맛있겠너요",
          "original_form": "맛있겠너요",
          "speaker_id": "2",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100029532.1.5",
          "form": "국물닭발이 더 맛있나요",
          "original_form": "국물닭발이 더 맛있나요",
          "speaker_id": "1",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100029532.1.6",
          "form": "인천까진 멀어요",
          "original_form": "인천까진 멀어요",
          "speaker_id": "1",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100029532.1.7",
          "form": "국물닭발 맛있게 먹는법",
          "original_form": "국물닭발 맛있게 먹는법",
          "speaker_id": "2",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100029532.1.8",
          "form": "알려드릴까요",
          "original_form": "알려드릴까요",
          "speaker_id": "2",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100029532.1.9",
          "form": "뭐지요",
          "original_form": "뭐지요",
          "speaker_id": "1",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100029532.1.10",
          "form": "궁금해요",
          "original_form": "궁금해요",
          "speaker_id": "1",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100029532.1.11",
          "form": "마지막에 깻잎을 넣으세요",
          "original_form": "마지막에 깻잎을 넣으세요",
          "speaker_id": "2",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100029532.1.12",
          "form": "존맛대잔치 됩니다",
          "original_form": "존맛대잔치 됩니다",
          "speaker_id": "2",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100029532.1.13",
          "form": "향이 좋겠군요",
          "original_form": "향이 좋겠군요",
          "speaker_id": "1",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100029532.1.14",
          "form": "무조건 맛있어져요",
          "original_form": "무조건 맛있어져요",
          "speaker_id": "2",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100029532.1.15",
          "form": "산것은 숯불닭발인데요ㅜ",
          "original_form": "산것은 숯불닭발인데요ㅜ",
          "speaker_id": "1",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100029532.1.16",
          "form": "싸서드시면되지요",
          "original_form": "싸서드시면되지요",
