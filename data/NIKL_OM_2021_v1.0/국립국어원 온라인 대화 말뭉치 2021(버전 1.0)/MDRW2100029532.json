          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100029532.1.17",
          "form": "숯불에 넣어도 되나요",
          "original_form": "숯불에 넣어도 되나요",
          "speaker_id": "1",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100029532.1.18",
          "form": "쌈무랑 드세요",
          "original_form": "쌈무랑 드세요",
