      "utterance": [
        {
          "id": "MDRW2100040117.1.1",
          "form": "안녕하세요게임하는거있으세용",
          "original_form": "안녕하세요게임하는거있으세용",
          "speaker_id": "1",
          "time": "20210904 10:41"
        },
        {
          "id": "MDRW2100040117.1.2",
          "form": "안녕하세요~ 하는게임은.없네여",
          "original_form": "안녕하세요~ 하는게임은.없네여",
          "speaker_id": "2",
          "time": "20210904 11:01"
        },
        {
          "id": "MDRW2100040117.1.3",
          "form": "게임좋아하세요?",
          "original_form": "게임좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 11:01"
        },
        {
          "id": "MDRW2100040117.1.4",
          "form": "저도 게임은 잘 안해요",
          "original_form": "저도 게임은 잘 안해요",
          "speaker_id": "1",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100040117.1.5",
          "form": "마리오 이런 고전게임 좋아해요 심즈랑",
          "original_form": "마리오 이런 고전게임 좋아해요 심즈랑",
          "speaker_id": "1",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100040117.1.6",
          "form": "저두요ㅠㅎㅎ 전테트리스나",
          "original_form": "저두요ㅠㅎㅎ 전테트리스나",
          "speaker_id": "2",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100040117.1.7",
          "form": "퍼즐게임같은거",
          "original_form": "퍼즐게임같은거",
          "speaker_id": "2",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100040117.1.8",
          "form": "아 저도요 ㅋㅋ",
          "original_form": "아 저도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100040117.1.9",
          "form": "롤같은건 어려유ㅓ서 못하겠어요",
          "original_form": "롤같은건 어려유ㅓ서 못하겠어요",
          "speaker_id": "1",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100040117.1.10",
          "form": "한번도 안해봣네요 ㅠㅠ",
          "original_form": "한번도 안해봣네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100040117.1.11",
          "form": "저두요 스타쿠래프트가 마지막이에요",
          "original_form": "저두요 스타쿠래프트가 마지막이에요",
          "speaker_id": "1",
          "time": "20210904 11:09"
        },
        {
          "id": "MDRW2100040117.1.12",
          "form": "전 그것도 안해봣네욬ㅋㅋ게임이랑 거리가머네요",
          "original_form": "전 그것도 안해봣네욬ㅋㅋ게임이랑 거리가머네요",
          "speaker_id": "2",
          "time": "20210904 11:10"
        },
        {
          "id": "MDRW2100040117.1.13",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:14"
        },
        {
          "id": "MDRW2100040117.1.14",
          "form": "최근 제페토앱 해봤어요 메타버수",
          "original_form": "최근 제페토앱 해봤어요 메타버수",
          "speaker_id": "1",
          "time": "20210904 11:14"
        },
        {
          "id": "MDRW2100040117.1.15",
          "form": "전 모르는거네요ㅠㅠ ㅋㅋ어려운게임세계에여",
          "original_form": "전 모르는거네요ㅠㅠ ㅋㅋ어려운게임세계에여",
          "speaker_id": "2",
          "time": "20210904 11:15"
        },
        {
          "id": "MDRW2100040117.1.16",
          "form": "네이버앱인데 아바타 꾸미는거에요",
          "original_form": "네이버앱인데 아바타 꾸미는거에요",
          "speaker_id": "1",
          "time": "20210904 11:22"
        },
        {
          "id": "MDRW2100040117.1.17",
          "form": "다양한 게임이 많아요 ㅎ",
          "original_form": "다양한 게임이 많아요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 14:46"
        },
        {
          "id": "MDRW2100040117.1.18",
          "form": "맞아요 게임 재밌지만 저는 좀 시간이 아까워요",
          "original_form": "맞아요 게임 재밌지만 저는 좀 시간이 아까워요",
          "speaker_id": "1",
          "time": "20210904 15:01"
        }
