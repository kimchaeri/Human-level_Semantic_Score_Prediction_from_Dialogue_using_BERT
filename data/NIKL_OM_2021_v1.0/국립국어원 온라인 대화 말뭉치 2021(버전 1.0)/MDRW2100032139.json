        {
          "id": "MDRW2100032139.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100032139.1.9",
          "form": "사실 교수님이랍니다",
          "original_form": "사실 교수님이랍니다",
          "speaker_id": "1",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100032139.1.10",
          "form": "name2학생",
          "original_form": "&name2&학생",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100032139.1.11",
          "form": "개강하니 어떤가요?",
          "original_form": "개강하니 어떤가요?",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100032139.1.12",
