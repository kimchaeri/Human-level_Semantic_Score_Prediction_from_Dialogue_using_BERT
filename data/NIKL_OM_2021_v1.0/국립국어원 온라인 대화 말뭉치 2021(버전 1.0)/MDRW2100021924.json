        },
        {
          "id": "MDRW2100021924.1.5",
          "form": "자주 가세요?",
          "original_form": "자주 가세요?",
          "speaker_id": "2",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100021924.1.6",
          "form": "원래는 매주 갔어요",
