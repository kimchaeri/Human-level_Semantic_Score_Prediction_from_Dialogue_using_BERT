      "utterance": [
        {
          "id": "MDRW2100021968.1.1",
          "form": "헏 ㅠ....",
          "original_form": "헏 ㅠ....",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021968.1.2",
          "form": "모르겠어요..........",
          "original_form": "모르겠어요..........",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021968.1.3",
          "form": "이런!",
          "original_form": "이런!",
          "speaker_id": "1",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021968.1.4",
          "form": "아니면 그 ... 비빔면 해먹는 양념장에",
          "original_form": "아니면 그 ... 비빔면 해먹는 양념장에",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021968.1.5",
          "form": "칼국수면이랑 해서 비벼먹어도.. 맛있어요",
          "original_form": "칼국수면이랑 해서 비벼먹어도.. 맛있어요",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021968.1.6",
          "form": "면발이...쫜득하겠어요",
          "original_form": "면발이...쫜득하겠어요",
          "speaker_id": "1",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021968.1.7",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021968.1.8",
          "form": "맛있겠다....",
          "original_form": "맛있겠다....",
          "speaker_id": "1",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021968.1.9",
          "form": "저는 이상하게 물에 빠진 면은 금방 질리더라구요",
          "original_form": "저는 이상하게 물에 빠진 면은 금방 질리더라구요",
          "speaker_id": "1",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021968.1.10",
          "form": "계속 똑같은 맛만 나는 것 같고...",
          "original_form": "계속 똑같은 맛만 나는 것 같고...",
          "speaker_id": "1",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021968.1.11",
          "form": "제 주변에는",
          "original_form": "제 주변에는",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021968.1.12",
          "form": "물에 빠진 고기를 싫어하는 친구들이 많아요",
          "original_form": "물에 빠진 고기를 싫어하는 친구들이 많아요",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021968.1.13",
          "form": "고기가 물에 빠질 일이 자주 있나요...?",
          "original_form": "고기가 물에 빠질 일이 자주 있나요...?",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021968.1.14",
          "form": "수육?",
          "original_form": "수육?",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021968.1.15",
          "form": "곰탕",
          "original_form": "곰탕",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021968.1.16",
          "form": "요런것들?",
          "original_form": "요런것들?",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021968.1.17",
          "form": "샤브셔브",
          "original_form": "샤브셔브",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021968.1.18",
          "form": "헐... 전 둘 다 좋아요",
          "original_form": "헐... 전 둘 다 좋아요",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021968.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021968.1.20",
          "form": "저도요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100021968.1.21",
          "form": "훠궈 마라 이런것도 좋아하세요?",
          "original_form": "훠궈 마라 이런것도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100021968.1.22",
          "form": "아뇨ㅠㅠ 그쪽은 입맛에 안맞더라구요ㅠㅠ",
          "original_form": "아뇨ㅠㅠ 그쪽은 입맛에 안맞더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100021968.1.23",
          "form": "아 정말 아쉽다",
          "original_form": "아 정말 아쉽다",
          "speaker_id": "1",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021968.1.24",
          "form": "전 진짜 훠궈 너무 좋아해요...",
          "original_form": "전 진짜 훠궈 너무 좋아해요...",
          "speaker_id": "1",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021968.1.25",
          "form": "훠궈는 안먹어봤는데 마라가 안맞으면 훠궈도 안맞을거라고하더라구여ㅠ",
          "original_form": "훠궈는 안먹어봤는데 마라가 안맞으면 훠궈도 안맞을거라고하더라구여ㅠ",
          "speaker_id": "2",
          "time": "20210829 00:25"
        },
        {
          "id": "MDRW2100021968.1.26",
          "form": "마라에 있는 땅콩소스 때문에 싫은 거면 훠궈는 잘 드실 수도 있는데",
          "original_form": "마라에 있는 땅콩소스 때문에 싫은 거면 훠궈는 잘 드실 수도 있는데",
          "speaker_id": "1",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021968.1.27",
          "form": "맵고 얼한 향 자체가 싫은 거면 좀 힘드시겠어요",
          "original_form": "맵고 얼한 향 자체가 싫은 거면 좀 힘드시겠어요",
          "speaker_id": "1",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021968.1.28",
          "form": "네네 후자쪽인거같아요ㅠㅠ",
          "original_form": "네네 후자쪽인거같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 00:26"
        }
      ]
    }
