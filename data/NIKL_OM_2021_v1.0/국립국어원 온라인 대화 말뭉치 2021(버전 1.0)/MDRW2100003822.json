      "utterance": [
        {
          "id": "MDRW2100003822.1.1",
          "form": "임창정은 피부가 많이 안좋네",
          "original_form": "임창정은 피부가 많이 안좋네",
          "speaker_id": "2",
          "time": "20210821 12:31"
        },
        {
          "id": "MDRW2100003822.1.2",
          "form": "지금은  완전 좋아짐",
          "original_form": "지금은  완전 좋아짐",
          "speaker_id": "1",
          "time": "20210821 12:31"
        },
        {
          "id": "MDRW2100003822.1.3",
          "form": "피부과의 힘인가보다 ㅋㅋ",
          "original_form": "피부과의 힘인가보다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 12:33"
        },
        {
          "id": "MDRW2100003822.1.4",
          "form": "돈 많이 버니 피부에 투자 많이 하겠지",
          "original_form": "돈 많이 버니 피부에 투자 많이 하겠지",
          "speaker_id": "1",
          "time": "20210821 12:33"
        },
        {
          "id": "MDRW2100003822.1.5",
          "form": "팬클럽 이름이 빠빠라기인가보다 언급하네",
          "original_form": "팬클럽 이름이 빠빠라기인가보다 언급하네",
          "speaker_id": "2",
          "time": "20210821 12:33"
        },
        {
          "id": "MDRW2100003822.1.6",
          "form": "ㅋㅋㅋ 들어봤던거 같다",
          "original_form": "ㅋㅋㅋ 들어봤던거 같다",
          "speaker_id": "1",
          "time": "20210821 12:34"
        },
        {
          "id": "MDRW2100003822.1.7",
          "form": "이미 나에게로 이노래 후렴은 알겠다 ㅋㅋ",
          "original_form": "이미 나에게로 이노래 후렴은 알겠다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 12:34"
        },
        {
          "id": "MDRW2100003822.1.8",
          "form": "노래는 많지..  남자들이 좋아하는 노래",
          "original_form": "노래는 많지..  남자들이 좋아하는 노래",
          "speaker_id": "1",
          "time": "20210821 12:34"
        },
        {
          "id": "MDRW2100003822.1.9",
          "form": "임창정이 중간 공백이 많았었네",
          "original_form": "임창정이 중간 공백이 많았었네",
          "speaker_id": "2",
          "time": "20210821 12:34"
        },
        {
          "id": "MDRW2100003822.1.10",
          "form": "결혼도 두번 하고",
          "original_form": "결혼도 두번 하고",
          "speaker_id": "1",
          "time": "20210821 12:35"
        },
        {
          "id": "MDRW2100003822.1.11",
          "form": "인생도 파란만장했다",
          "original_form": "인생도 파란만장했다",
          "speaker_id": "2",
          "time": "20210821 12:41"
        },
        {
          "id": "MDRW2100003822.1.12",
          "form": "이제 끝나고 차달남 할려나 보다",
          "original_form": "이제 끝나고 차달남 할려나 보다",
          "speaker_id": "2",
          "time": "20210821 12:42"
        },
        {
          "id": "MDRW2100003822.1.13",
          "form": "그래도 다 미인이랑 결혼하고 능력자다",
          "original_form": "그래도 다 미인이랑 결혼하고 능력자다",
          "speaker_id": "1",
          "time": "20210821 12:43"
        },
        {
          "id": "MDRW2100003822.1.14",
          "form": "그렇다",
