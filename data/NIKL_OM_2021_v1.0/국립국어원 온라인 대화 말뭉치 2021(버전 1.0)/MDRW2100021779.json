      "utterance": [
        {
          "id": "MDRW2100021779.1.1",
          "form": "마늘 좋아라세요?",
          "original_form": "마늘 좋아라세요?",
          "speaker_id": "1",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021779.1.2",
          "form": "좋아하져~",
          "original_form": "좋아하져~",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021779.1.3",
          "form": "한국사람 거의 좋아하잖아요 ㅋㅋ",
          "original_form": "한국사람 거의 좋아하잖아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021779.1.4",
          "form": "신기하죠... 다들 왤케 마늘 좋아하는지.. ㅋㅋ",
          "original_form": "신기하죠... 다들 왤케 마늘 좋아하는지.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100021779.1.5",
          "form": "맛있잖아요 ㅋㅋ",
          "original_form": "맛있잖아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100021779.1.6",
          "form": "음식에 파마늘 넣는거랑 안 넣는거랑 확다르더라구요",
          "original_form": "음식에 파마늘 넣는거랑 안 넣는거랑 확다르더라구요",
          "speaker_id": "2",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021779.1.7",
          "form": "마늘 싫어하세요??",
          "original_form": "마늘 싫어하세요??",
          "speaker_id": "2",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021779.1.8",
          "form": "전 마늘 달고 살아요... 마늘 먹음 피가 잘돌아서..",
          "original_form": "전 마늘 달고 살아요... 마늘 먹음 피가 잘돌아서..",
          "speaker_id": "1",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021779.1.9",
          "form": "1킬로 짜리 냉동해서.. \r\n라면끊일때도 5-6개씩 넣어요.",
          "original_form": "1킬로 짜리 냉동해서.. \r\n라면끊일때도 5-6개씩 넣어요.",
          "speaker_id": "1",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021779.1.10",
          "form": "와 헐 그 정도에요?",
          "original_form": "와 헐 그 정도에요?",
          "speaker_id": "2",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021779.1.11",
          "form": "ㅋㅋㅋㅋㅋ전 그정돈 아니에여",
          "original_form": "ㅋㅋㅋㅋㅋ전 그정돈 아니에여",
          "speaker_id": "2",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021779.1.12",
          "form": "마늘이 다좋은데.. 다음날 냄새가..",
          "original_form": "마늘이 다좋은데.. 다음날 냄새가..",
          "speaker_id": "1",
          "time": "20210828 22:28"
        },
        {
          "id": "MDRW2100021779.1.13",
          "form": "메드포갈릭 이라는 식당? 아세여?",
          "original_form": "메드포갈릭 이라는 식당? 아세여?",
          "speaker_id": "1",
          "time": "20210828 22:28"
        },
        {
          "id": "MDRW2100021779.1.14",
          "form": "알죠알죠 ㅋㅋㅋ",
          "original_form": "알죠알죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:29"
        },
        {
          "id": "MDRW2100021779.1.15",
          "form": "저 거기 엄청 좋아하는데.. \r\n다음날 막 땀에서도 마늘냄새나죠..",
          "original_form": "저 거기 엄청 좋아하는데.. \r\n다음날 막 땀에서도 마늘냄새나죠..",
          "speaker_id": "1",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100021779.1.16",
          "form": "아 근데 그렇게 냄새가 나나요? 모르겠어서... ㅋㅋㅋㅋㅋ",
          "original_form": "아 근데 그렇게 냄새가 나나요? 모르겠어서... ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021779.1.17",
          "form": "저 생마늘도 잘 먹는데 좀 지나면? 괜찮던데요 ㅋㅋ",
          "original_form": "저 생마늘도 잘 먹는데 좀 지나면? 괜찮던데요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021779.1.18",
          "form": "아마.. 주위사람은 알듯.. ㅋㅋ",
          "original_form": "아마.. 주위사람은 알듯.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021779.1.19",
          "form": "나만 모르는거 일 수도 잇어요..",
          "original_form": "나만 모르는거 일 수도 잇어요..",
          "speaker_id": "1",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021779.1.20",
          "form": "ㅋㅋㅋㅋ 물어봐도 마늘먹은거 모른다고 하던데요",
          "original_form": "ㅋㅋㅋㅋ 물어봐도 마늘먹은거 모른다고 하던데요",
          "speaker_id": "2",
          "time": "20210828 22:44"
        }
      ]
