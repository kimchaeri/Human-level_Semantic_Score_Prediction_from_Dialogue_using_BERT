      "utterance": [
        {
          "id": "MDRW2100037361.1.1",
          "form": "응 아직은 받은 정품을 써",
          "original_form": "응 아직은 받은 정품을 써",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.2",
          "form": "근데 끊어지기도 잘 끊어져서 조금 화나",
          "original_form": "근데 끊어지기도 잘 끊어져서 조금 화나",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.3",
          "form": "근데 또 사려니까 맞아 지출이 너무 크다",
          "original_form": "근데 또 사려니까 맞아 지출이 너무 크다",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.4",
          "form": "아이패드 선도 괜찮아?",
          "original_form": "아이패드 선도 괜찮아?",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.5",
          "form": "너 충전기 보호캡은 써봤니?",
          "original_form": "너 충전기 보호캡은 써봤니?",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.6",
          "form": "애플은 선이 너무 약해",
          "original_form": "애플은 선이 너무 약해",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.7",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.8",
          "form": "걔도 곧 죽을 것 같아",
          "original_form": "걔도 곧 죽을 것 같아",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.9",
          "form": "근데 별로 보호는 안해줘",
          "original_form": "근데 별로 보호는 안해줘",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.10",
          "form": "아아. 정품 선을 찾고있어?",
          "original_form": "아아. 정품 선을 찾고있어?",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.11",
          "form": "나는 심지어 아이패드 충전선은",
          "original_form": "나는 심지어 아이패드 충전선은",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.12",
          "form": "나는 삼성건 정품이 아니더라도 좋더라",
          "original_form": "나는 삼성건 정품이 아니더라도 좋더라",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.13",
          "form": "정품으로 두번째껀데",
          "original_form": "정품으로 두번째껀데",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.14",
          "form": "돈아까워죽겠어",
          "original_form": "돈아까워죽겠어",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.15",
          "form": "밧줄같은 선을 사용해",
          "original_form": "밧줄같은 선을 사용해",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.16",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.17",
          "form": "선을 험하게 다루는구나",
          "original_form": "선을 험하게 다루는구나",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.18",
          "form": "그래? 그럼 그냥 밧줄같은거로 사야겠다",
          "original_form": "그래? 그럼 그냥 밧줄같은거로 사야겠다",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.19",
          "form": "좀 더 아껴줘",
          "original_form": "좀 더 아껴줘",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037361.1.20",
          "form": "꿀팁 고마워 그래",
          "original_form": "꿀팁 고마워 그래",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100037361.1.21",
          "form": "내가 대체 이 얘기를 몇번이나 하는지 모르겠다.",
          "original_form": "내가 대체 이 얘기를 몇번이나 하는지 모르겠다.",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037361.1.22",
          "form": "내가 약 11일 전에 주문한 북트롤리말이야.",
          "original_form": "내가 약 11일 전에 주문한 북트롤리말이야.",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037361.1.23",
          "form": "중국에서 나무를 베어서 만들고있는걸까? 너무 안온다",
          "original_form": "중국에서 나무를 베어서 만들고있는걸까? 너무 안온다",
          "speaker_id": "1",
          "time": "20210831 22:57"
        }
