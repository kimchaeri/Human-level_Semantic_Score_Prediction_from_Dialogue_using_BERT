        {
          "id": "MDRW2100007315.1.1",
          "form": "동물 좋아하시나요?",
          "original_form": "동물 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007315.1.2",
          "form": "네네네네",
          "original_form": "네네네네",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007315.1.3",
          "form": "name1님 어떤 동물 좋아하시나요",
          "original_form": "&name1&님 어떤 동물 좋아하시나요",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007315.1.4",
          "form": "전 고양이 강아지 포유류는 다 좋아하는 거 같아요",
          "original_form": "전 고양이 강아지 포유류는 다 좋아하는 거 같아요",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007315.1.5",
          "form": "혹시 반려동물 기르시나요",
          "original_form": "혹시 반려동물 기르시나요",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007315.1.6",
          "form": "헉 예전에",
          "original_form": "헉 예전에",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007315.1.7",
          "form": "닭 길렀다가",
          "original_form": "닭 길렀다가",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007315.1.8",
          "form": "예전에",
          "original_form": "예전에",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007315.1.9",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007315.1.10",
          "form": "보내줬어요",
          "original_form": "보내줬어요",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007315.1.11",
          "form": "생각도 못한답변",
          "original_form": "생각도 못한답변",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007315.1.12",
          "form": "그 암탉 3마리 있는 곳으로",
          "original_form": "그 암탉 3마리 있는 곳으로",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007315.1.13",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007315.1.14",
          "form": "의자왕이 되라며",
          "original_form": "의자왕이 되라며",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007315.1.15",
          "form": "어떻게 기르게 됐나요?",
          "original_form": "어떻게 기르게 됐나요?",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007315.1.16",
          "form": "복날에 삼계탕 먹고 나오는데 길거리에 병아리와 닭 사이의 아이가 돌아다니더라고요",
          "original_form": "복날에 삼계탕 먹고 나오는데 길거리에 병아리와 닭 사이의 아이가 돌아다니더라고요",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007315.1.17",
          "form": "잡아서 삼계탕집 거냐고 물어봤는데",
          "original_form": "잡아서 삼계탕집 거냐고 물어봤는데",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007315.1.18",
          "form": "거기는 닭을 안키운다해서",
          "original_form": "거기는 닭을 안키운다해서",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007315.1.19",
          "form": "집에 데려왔어요",
          "original_form": "집에 데려왔어요",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007315.1.20",
          "form": "근데 애가 수컷인거있죠",
          "original_form": "근데 애가 수컷인거있죠",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007315.1.21",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007315.1.22",
          "form": "흥미로운데요",
          "original_form": "흥미로운데요",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007315.1.23",
          "form": "ㅋㅋㅋㅋㅋ앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007315.1.24",
          "form": "수컷이면",
          "original_form": "수컷이면",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007315.1.25",
          "form": "그 머리에 그거뭐죠",
          "original_form": "그 머리에 그거뭐죠",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007315.1.26",
          "form": "어우 새벽마다 울더라고요",
          "original_form": "어우 새벽마다 울더라고요",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007315.1.27",
          "form": "닭벼슬",
          "original_form": "닭벼슬",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007315.1.28",
          "form": "맞다 닭벼슬",
          "original_form": "맞다 닭벼슬",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007315.1.29",
          "form": "그거 멋지게 자라던가요",
          "original_form": "그거 멋지게 자라던가요",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007315.1.30",
          "form": "글고 튼튼하게 키우려고",
          "original_form": "글고 튼튼하게 키우려고",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007315.1.31",
          "form": "매미나 잡자리 잡아다가",
          "original_form": "매미나 잡자리 잡아다가",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007315.1.32",
          "form": "와우 지극정성",
          "original_form": "와우 지극정성",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007315.1.33",
          "form": "먹이고 그랬습니다",
          "original_form": "먹이고 그랬습니다",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007315.1.34",
          "form": "와우 엄청튼튼하게 자랐을 거 같아요",
          "original_form": "와우 엄청튼튼하게 자랐을 거 같아요",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007315.1.35",
          "form": "진짜 너무 재밌고 신기하다",
          "original_form": "진짜 너무 재밌고 신기하다",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007315.1.36",
          "form": "흰색 닭이었는데 아주 이뻣답니다",
          "original_form": "흰색 닭이었는데 아주 이뻣답니다",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007315.1.37",
          "form": "암탉 3마리 있는 곳에 보내고",
          "original_form": "암탉 3마리 있는 곳에 보내고",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007315.1.38",
          "form": "멀지만 몇번 찾아갔는데",
          "original_form": "멀지만 몇번 찾아갔는데",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007315.1.39",
          "form": "진짜 이뻤겠다",
          "original_form": "진짜 이뻤겠다",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007315.1.40",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007315.1.41",
          "form": "한 5번째 되니까",
          "original_form": "한 5번째 되니까",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007315.1.42",
          "form": "우리를 못알아보더라고요",
          "original_form": "우리를 못알아보더라고요",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007315.1.43",
          "form": "아 서운해ㅠ",
          "original_form": "아 서운해ㅠ",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007315.1.44",
          "form": "닭대가리라 하잖아요",
          "original_form": "닭대가리라 하잖아요",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007315.1.45",
