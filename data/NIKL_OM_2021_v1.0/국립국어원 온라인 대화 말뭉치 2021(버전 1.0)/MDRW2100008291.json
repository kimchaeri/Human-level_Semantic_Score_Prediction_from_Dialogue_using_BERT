        {
          "id": "MDRW2100008291.1.1",
          "form": "자빱아세요?",
          "original_form": "자빱아세요?",
          "speaker_id": "1",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008291.1.2",
          "form": "네!!!!",
          "original_form": "네!!!!",
          "speaker_id": "2",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008291.1.3",
          "form": "어제도 세븐데이즈..머시기 봤는걸요ㅎㅎ",
          "original_form": "어제도 세븐데이즈..머시기 봤는걸요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008291.1.4",
          "form": "우정리도 보시나요",
          "original_form": "우정리도 보시나요",
          "speaker_id": "1",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008291.1.5",
          "form": "그럼요 저 다 챙겨봐요!",
          "original_form": "그럼요 저 다 챙겨봐요!",
          "speaker_id": "2",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100008291.1.6",
          "form": "제일 좋아하는 컨텐츠가 뭔가요",
          "original_form": "제일 좋아하는 컨텐츠가 뭔가요",
          "speaker_id": "1",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008291.1.7",
          "form": "잔인한 질문을 하시네요....",
          "original_form": "잔인한 질문을 하시네요....",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008291.1.8",
          "form": "그래도 마음의 고향 우정리...",
          "original_form": "그래도 마음의 고향 우정리...",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008291.1.9",
          "form": "저는 매운맛 서리사막...",
          "original_form": "저는 매운맛 서리사막...",
          "speaker_id": "1",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008291.1.10",
          "form": "아..서리사막..인정합니다....",
          "original_form": "아..서리사막..인정합니다....",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008291.1.11",
          "form": "진짜 저 아직도 yjk교도소에 있잖아요..",
          "original_form": "진짜 저 아직도 yjk교도소에 있잖아요..",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008291.1.12",
          "form": "우정리 1이 좋나요 2가 좋나요",
          "original_form": "우정리 1이 좋나요 2가 좋나요",
          "speaker_id": "1",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008291.1.13",
          "form": "아직은 1이요...그  뜨수운 마을 축제들 잊지 못해요..",
          "original_form": "아직은 1이요...그  뜨수운 마을 축제들 잊지 못해요..",
          "speaker_id": "2",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100008291.1.14",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100008291.1.15",
          "form": "저도 아직은 1입니다",
          "original_form": "저도 아직은 1입니다",
          "speaker_id": "1",
          "time": "20210822 18:56"
        },
        {
          "id": "MDRW2100008291.1.16",
          "form": "자빱말고 다른 게임컨텐츠 보시는거 있나요",
          "original_form": "자빱말고 다른 게임컨텐츠 보시는거 있나요",
          "speaker_id": "1",
          "time": "20210822 18:56"
        },
        {
          "id": "MDRW2100008291.1.17",
          "form": "리미님..오함마님..하말넘많님들도 보고..",
          "original_form": "리미님..오함마님..하말넘많님들도 보고..",
          "speaker_id": "2",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008291.1.18",
          "form": "약간 게임을 본다기보다는 그 게임을 플레이하는 사람을 보는 것 같아요",
          "original_form": "약간 게임을 본다기보다는 그 게임을 플레이하는 사람을 보는 것 같아요",
          "speaker_id": "2",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008291.1.19",
          "form": "저도 게임방송 자체는 별로 안좋아해요",
