      "utterance": [
        {
          "id": "MDRW2100046498.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211027 00:07"
        },
        {
          "id": "MDRW2100046498.1.2",
          "form": "안녕하세요! 요즘 날씨가 많이 선선해졌네요~",
          "original_form": "안녕하세요! 요즘 날씨가 많이 선선해졌네요~",
          "speaker_id": "2",
          "time": "20211027 00:32"
        },
        {
          "id": "MDRW2100046498.1.3",
          "form": "조금 추워요 ㅋㅋ",
          "original_form": "조금 추워요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211027 00:38"
        },
        {
          "id": "MDRW2100046498.1.4",
          "form": "식사는 하셨나요?",
          "original_form": "식사는 하셨나요?",
          "speaker_id": "1",
          "time": "20211027 00:38"
        },
        {
          "id": "MDRW2100046498.1.5",
          "form": "넵! 저는 돈까스를 먹었어요! 맛있는거 드셨나요?",
          "original_form": "넵! 저는 돈까스를 먹었어요! 맛있는거 드셨나요?",
          "speaker_id": "2",
          "time": "20211027 00:40"
        },
        {
          "id": "MDRW2100046498.1.6",
          "form": "저는 짬뽕요",
          "original_form": "저는 짬뽕요",
          "speaker_id": "1",
          "time": "20211027 00:41"
        },
        {
          "id": "MDRW2100046498.1.7",
          "form": "오 무슨 짬뽕이요",
          "original_form": "오 무슨 짬뽕이요",
          "speaker_id": "2",
          "time": "20211027 00:41"
        },
        {
          "id": "MDRW2100046498.1.8",
          "form": "내일은 돈까스 먹어야겠어요",
          "original_form": "내일은 돈까스 먹어야겠어요",
          "speaker_id": "1",
          "time": "20211027 00:41"
        },
        {
          "id": "MDRW2100046498.1.9",
          "form": "그냥 일반 짬뽕요",
          "original_form": "그냥 일반 짬뽕요",
          "speaker_id": "1",
          "time": "20211027 00:41"
        },
        {
          "id": "MDRW2100046498.1.10",
          "form": "결혼은 하고 싶은데",
          "original_form": "결혼은 하고 싶은데",
          "speaker_id": "1",
          "time": "20211027 00:42"
        },
        {
          "id": "MDRW2100046498.1.11",
          "form": "가진 게 없으면",
          "original_form": "가진 게 없으면",
          "speaker_id": "1",
          "time": "20211027 00:42"
        },
        {
          "id": "MDRW2100046498.1.12",
          "form": "어떡해야 하나요?",
          "original_form": "어떡해야 하나요?",
          "speaker_id": "1",
          "time": "20211027 00:42"
        },
        {
          "id": "MDRW2100046498.1.13",
          "form": "그냥 보내줘야 할까요?",
          "original_form": "그냥 보내줘야 할까요?",
          "speaker_id": "1",
          "time": "20211027 00:43"
        },
        {
          "id": "MDRW2100046498.1.14",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20211027 00:44"
        },
        {
          "id": "MDRW2100046498.1.15",
          "form": "말 할 친구가 없어요",
          "original_form": "말 할 친구가 없어요",
          "speaker_id": "1",
          "time": "20211027 00:45"
        },
        {
          "id": "MDRW2100046498.1.16",
          "form": "오.. 연애를 오래 하셨나요?",
          "original_form": "오.. 연애를 오래 하셨나요?",
          "speaker_id": "2",
          "time": "20211027 00:48"
        },
        {
          "id": "MDRW2100046498.1.17",
          "form": "4년 만났어요",
          "original_form": "4년 만났어요",
          "speaker_id": "1",
          "time": "20211027 00:48"
        },
        {
          "id": "MDRW2100046498.1.18",
          "form": "너무 좋은데 현실이 녹록치 않아요",
          "original_form": "너무 좋은데 현실이 녹록치 않아요",
          "speaker_id": "1",
          "time": "20211027 00:49"
        },
        {
          "id": "MDRW2100046498.1.19",
          "form": "맞아요.. 요즘 결혼하기 힘들어요",
          "original_form": "맞아요.. 요즘 결혼하기 힘들어요",
          "speaker_id": "2",
          "time": "20211027 00:49"
        },
        {
          "id": "MDRW2100046498.1.20",
          "form": "상대분이랑 결혼관은 잘 맞으시나요?",
          "original_form": "상대분이랑 결혼관은 잘 맞으시나요?",
          "speaker_id": "2",
          "time": "20211027 00:49"
        },
        {
          "id": "MDRW2100046498.1.21",
          "form": "네 여친이랑은 잘 맞아요",
          "original_form": "네 여친이랑은 잘 맞아요",
          "speaker_id": "1",
          "time": "20211027 00:50"
        },
        {
          "id": "MDRW2100046498.1.22",
          "form": "어떤게 가장 걸리시는 건가요..",
          "original_form": "어떤게 가장 걸리시는 건가요..",
          "speaker_id": "2",
          "time": "20211027 00:51"
        },
        {
          "id": "MDRW2100046498.1.23",
          "form": "집이죠 뭐 ㅠ",
          "original_form": "집이죠 뭐 ㅠ",
          "speaker_id": "1",
          "time": "20211027 00:51"
        },
        {
          "id": "MDRW2100046498.1.24",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "1",
          "time": "20211027 00:51"
        },
        {
          "id": "MDRW2100046498.1.25",
          "form": "결혼 하셨나요?",
          "original_form": "결혼 하셨나요?",
          "speaker_id": "1",
          "time": "20211027 00:52"
        },
        {
          "id": "MDRW2100046498.1.26",
          "form": "아뇨 전 하지 않았지만",
          "original_form": "아뇨 전 하지 않았지만",
          "speaker_id": "2",
          "time": "20211027 00:52"
        },
        {
          "id": "MDRW2100046498.1.27",
          "form": "주변에서 여러 케이스를 봤어요",
          "original_form": "주변에서 여러 케이스를 봤어요",
          "speaker_id": "2",
          "time": "20211027 00:52"
        },
        {
          "id": "MDRW2100046498.1.28",
          "form": "아.. 네",
          "original_form": "아.. 네",
          "speaker_id": "1",
          "time": "20211027 00:52"
        },
        {
          "id": "MDRW2100046498.1.29",
          "form": "상대분이랑 의견을 모아보셨나요?",
          "original_form": "상대분이랑 의견을 모아보셨나요?",
          "speaker_id": "2",
          "time": "20211027 00:53"
        },
        {
          "id": "MDRW2100046498.1.30",
          "form": "어떻게 될 지는 모르겠지만 머리가 아프네요",
          "original_form": "어떻게 될 지는 모르겠지만 머리가 아프네요",
          "speaker_id": "1",
          "time": "20211027 00:53"
        },
        {
          "id": "MDRW2100046498.1.31",
          "form": "네 서로 좋아해서",
          "original_form": "네 서로 좋아해서",
          "speaker_id": "1",
          "time": "20211027 00:53"
        },
        {
          "id": "MDRW2100046498.1.32",
          "form": "결혼하는 쪽으로 가닥은 잡고 있는데",
          "original_form": "결혼하는 쪽으로 가닥은 잡고 있는데",
          "speaker_id": "1",
          "time": "20211027 00:53"
        },
        {
          "id": "MDRW2100046498.1.33",
          "form": "휴....",
          "original_form": "휴....",
          "speaker_id": "1",
          "time": "20211027 00:54"
        },
        {
          "id": "MDRW2100046498.1.34",
          "form": "구체적인 계획이랑 단기, 장기 플랜을 세세히 짜야",
          "original_form": "구체적인 계획이랑 단기, 장기 플랜을 세세히 짜야",
          "speaker_id": "2",
          "time": "20211027 00:54"
        },
        {
          "id": "MDRW2100046498.1.35",
          "form": "무리없이 결혼생활 하더라구요",
          "original_form": "무리없이 결혼생활 하더라구요",
          "speaker_id": "2",
          "time": "20211027 00:54"
        },
        {
          "id": "MDRW2100046498.1.36",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211027 00:54"
        },
        {
          "id": "MDRW2100046498.1.37",
          "form": "머리아프고 번거로워도 여러번 자주 이야기 꺼내는게 좋을 것 같아요",
          "original_form": "머리아프고 번거로워도 여러번 자주 이야기 꺼내는게 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20211027 00:54"
        },
        {
          "id": "MDRW2100046498.1.38",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20211027 00:55"
        },
        {
          "id": "MDRW2100046498.1.39",
          "form": "여친도 스트레스고 저도 스트레스고",
          "original_form": "여친도 스트레스고 저도 스트레스고",
          "speaker_id": "1",
          "time": "20211027 00:55"
        },
        {
          "id": "MDRW2100046498.1.40",
          "form": "힘든 나날이네요",
          "original_form": "힘든 나날이네요",
          "speaker_id": "1",
          "time": "20211027 00:55"
        },
        {
          "id": "MDRW2100046498.1.41",
          "form": "지금부터 차근차근 준비해서",
          "original_form": "지금부터 차근차근 준비해서",
          "speaker_id": "2",
          "time": "20211027 00:55"
        },
        {
          "id": "MDRW2100046498.1.42",
          "form": "봄에 결혼 성공하시면 좋겠어요",
          "original_form": "봄에 결혼 성공하시면 좋겠어요",
          "speaker_id": "2",
          "time": "20211027 00:55"
        },
        {
          "id": "MDRW2100046498.1.43",
          "form": "네 고마워요",
          "original_form": "네 고마워요",
          "speaker_id": "1",
          "time": "20211027 00:55"
        },
        {
          "id": "MDRW2100046498.1.44",
          "form": "화이팅 하십쇼!",
          "original_form": "화이팅 하십쇼!",
          "speaker_id": "2",
          "time": "20211027 00:55"
        }
      ]
    }
  ]
