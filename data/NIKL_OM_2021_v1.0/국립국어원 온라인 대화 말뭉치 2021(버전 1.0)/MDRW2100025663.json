      "utterance": [
        {
          "id": "MDRW2100025663.1.1",
          "form": "ㅋㅋㅋㄱㅋ 뀨도 자기 민방위라",
          "original_form": "ㅋㅋㅋㄱㅋ 뀨도 자기 민방위라",
          "speaker_id": "1",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100025663.1.2",
          "form": "얀센맞았다고 안전하다고 드립치던데",
          "original_form": "얀센맞았다고 안전하다고 드립치던데",
          "speaker_id": "1",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100025663.1.3",
          "form": "헤드윅다돌면",
          "original_form": "헤드윅다돌면",
          "speaker_id": "1",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100025663.1.4",
          "form": "누가무슨백신맞았는지도 다알듯ㅋㅋㅋㅋㅋ",
          "original_form": "누가무슨백신맞았는지도 다알듯ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100025663.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그런 TMI 너무 좋아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그런 TMI 너무 좋아",
          "speaker_id": "2",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100025663.1.6",
          "form": "그 맛에 회전문 돌지",
          "original_form": "그 맛에 회전문 돌지",
          "speaker_id": "2",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025663.1.7",
          "form": "마자 ㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "마자 ㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025663.1.8",
          "form": "뀨 헤롱이드립도 침",
          "original_form": "뀨 헤롱이드립도 침",
          "speaker_id": "1",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025663.1.9",
          "form": "자기 부끄럽지만 자기가 약쟁이여서",
          "original_form": "자기 부끄럽지만 자기가 약쟁이여서",
          "speaker_id": "1",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025663.1.10",
          "form": "감빵에도 두번이나갔다올뻔했지만",
          "original_form": "감빵에도 두번이나갔다올뻔했지만",
          "speaker_id": "1",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025663.1.11",
          "form": "슬기롭게빠져나왔다고ㅋㅋㄱㅋㅋㅋ",
          "original_form": "슬기롭게빠져나왔다고ㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:40"
        },
        {
          "id": "MDRW2100025663.1.12",
          "form": "ㅋㅋㅋㅋㅋ 거기서 완전다들빵터짐",
          "original_form": "ㅋㅋㅋㅋㅋ 거기서 완전다들빵터짐",
          "speaker_id": "1",
          "time": "20210901 15:40"
        },
        {
          "id": "MDRW2100025663.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ뀨는 그런 자기나 상대방 경력 관련으로 준비된 애드립을 잘치더라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ뀨는 그런 자기나 상대방 경력 관련으로 준비된 애드립을 잘치더라",
          "speaker_id": "2",
          "time": "20210901 15:40"
        },
        {
          "id": "MDRW2100025663.1.14",
          "form": "그칰ㄱㅋㅋㅋㅋ",
          "original_form": "그칰ㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100025663.1.15",
          "form": "순발력으로 잘함",
          "original_form": "순발력으로 잘함",
          "speaker_id": "1",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100025663.1.16",
          "form": "ㅋㄱㅋㅋㅋㅋㅋ 조는",
          "original_form": "ㅋㄱㅋㅋㅋㅋㅋ 조는",
          "speaker_id": "1",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100025663.1.17",
          "form": "뭐가나쁜말이야 이놈아",
          "original_form": "뭐가나쁜말이야 이놈아",
          "speaker_id": "1",
          "time": "20210901 15:42"
