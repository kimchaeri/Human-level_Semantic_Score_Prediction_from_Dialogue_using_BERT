        {
          "id": "MDRW2100025663.1.18",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100025663.1.19",
          "form": "누가 저번에 망원경가져왔는데",
          "original_form": "누가 저번에 망원경가져왔는데",
          "speaker_id": "1",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100025663.1.20",
          "form": "2열에서 나시카들음ㅋㅋㅋㅋㄱㄱ",
          "original_form": "2열에서 나시카들음ㅋㅋㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100025663.1.21",
          "form": "그랬더니 계속 군사작전때 쓰는 망원경같다곸ㅋㄱㅋㅋㅋ",
          "original_form": "그랬더니 계속 군사작전때 쓰는 망원경같다곸ㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:43"
        },
        {
          "id": "MDRW2100025663.1.22",
          "form": "그거끝까지 놀려먹음ㅋㄱㅋㄱ",
          "original_form": "그거끝까지 놀려먹음ㅋㄱㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 15:43"
        },
        {
          "id": "MDRW2100025663.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ2열에서 나시카는 에바 아닌가여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ2열에서 나시카는 에바 아닌가여",
          "speaker_id": "2",
          "time": "20210901 15:43"
        },
        {
          "id": "MDRW2100025663.1.24",
          "form": "모공을 보겠다는 의지인가",
          "original_form": "모공을 보겠다는 의지인가",
          "speaker_id": "2",
          "time": "20210901 15:43"
        },
        {
          "id": "MDRW2100025663.1.25",
          "form": "저도 그렇게생각하는데......",
          "original_form": "저도 그렇게생각하는데......",
          "speaker_id": "1",
          "time": "20210901 15:43"
        },
        {
          "id": "MDRW2100025663.1.26",
          "form": "ㅋㅋㅋㅋㅋ들지말라고 계속그러더라고",
          "original_form": "ㅋㅋㅋㅋㅋ들지말라고 계속그러더라고",
          "speaker_id": "1",
          "time": "20210901 15:43"
        },
        {
          "id": "MDRW2100025663.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ다들망원경보고 빵터짐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ다들망원경보고 빵터짐",
          "speaker_id": "1",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100025663.1.28",
          "form": "팔이 더 아플듯ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "팔이 더 아플듯ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100025663.1.29",
          "form": "저는 니쿠라샀어요",
          "original_form": "저는 니쿠라샀어요",
          "speaker_id": "1",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100025663.1.30",
          "form": "오글은 가벼워야해...",
          "original_form": "오글은 가벼워야해...",
          "speaker_id": "1",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100025663.1.31",
          "form": "가벼워야 계속 들기 편하지.....",
          "original_form": "가벼워야 계속 들기 편하지.....",
