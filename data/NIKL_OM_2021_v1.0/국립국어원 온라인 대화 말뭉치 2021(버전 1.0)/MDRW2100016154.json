        },
        {
          "id": "MDRW2100016154.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210826 10:28"
        },
        {
          "id": "MDRW2100016154.1.3",
          "form": "요리 좋아하세요?",
          "original_form": "요리 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 10:28"
        },
        {
          "id": "MDRW2100016154.1.4",
          "form": "저 요리하는 거 말고 먹는 건좋아해요",
          "original_form": "저 요리하는 거 말고 먹는 건좋아해요",
          "speaker_id": "1",
          "time": "20210826 10:30"
        },
        {
          "id": "MDRW2100016154.1.5",
          "form": "name2은 좋아하나요",
          "original_form": "&name2&은 좋아하나요",
          "speaker_id": "1",
          "time": "20210826 10:30"
        },
        {
          "id": "MDRW2100016154.1.6",
          "form": "즐기지는 않는데 가끔해요",
          "original_form": "즐기지는 않는데 가끔해요",
          "speaker_id": "2",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100016154.1.7",
          "form": "집에 식자재 체리해야할때",
          "original_form": "집에 식자재 체리해야할때",
          "speaker_id": "2",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100016154.1.8",
          "form": "아 그럴 때 필요하죠",
          "original_form": "아 그럴 때 필요하죠",
          "speaker_id": "1",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100016154.1.9",
          "form": "보통 어떤식으로 처리하시나요",
          "original_form": "보통 어떤식으로 처리하시나요",
          "speaker_id": "1",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100016154.1.10",
          "form": "얼마전에는 집에 감자가 있어서",
          "original_form": "얼마전에는 집에 감자가 있어서",
          "speaker_id": "2",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100016154.1.11",
          "form": "감자수프만들었어요",
          "original_form": "감자수프만들었어요",
          "speaker_id": "2",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016154.1.12",
          "form": "두부 유통기한 지났길래 두부강정 만들어 먹었고요",
          "original_form": "두부 유통기한 지났길래 두부강정 만들어 먹었고요",
          "speaker_id": "2",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016154.1.13",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016154.1.14",
          "form": "너무 맛있었겠다",
          "original_form": "너무 맛있었겠다",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016154.1.15",
          "form": "저는 볶음밥좋아해요",
          "original_form": "저는 볶음밥좋아해요",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016154.1.16",
          "form": "볶음밥좋아하시나요",
          "original_form": "볶음밥좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100016154.1.17",
          "form": "그럼오",
          "original_form": "그럼오",
          "speaker_id": "2",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100016154.1.18",
          "form": "굴소스 사다가 집에서 해먹은 적도 많아요",
          "original_form": "굴소스 사다가 집에서 해먹은 적도 많아요",
          "speaker_id": "2",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100016154.1.19",
          "form": "어떤볶음밥을 좋아하시나요",
          "original_form": "어떤볶음밥을 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100016154.1.20",
          "form": "굴소스 그것은 마약같은존재",
          "original_form": "굴소스 그것은 마약같은존재",
          "speaker_id": "1",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100016154.1.21",
          "form": "가리지 않아요",
          "original_form": "가리지 않아요",
          "speaker_id": "2",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100016154.1.22",
          "form": "저두요ㅎㅎ",
          "original_form": "저두요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100016154.1.23",
          "form": "그런데 굴소스는",
          "original_form": "그런데 굴소스는",
          "speaker_id": "2",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100016154.1.24",
          "form": "모든 맛을 굴소스화시켜서ㅎㅎ",
          "original_form": "모든 맛을 굴소스화시켜서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100016154.1.25",
          "form": "그건 그래요ㅎ",
          "original_form": "그건 그래요ㅎ",
          "speaker_id": "1",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100016154.1.26",
          "form": "맛이 너무 강해서ㅋㅋ",
          "original_form": "맛이 너무 강해서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:47"
        }
      ]
