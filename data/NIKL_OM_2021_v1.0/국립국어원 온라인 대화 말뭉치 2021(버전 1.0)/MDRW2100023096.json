      "utterance": [
        {
          "id": "MDRW2100023096.1.1",
          "form": "남자가 해도 좋은 운동 같아서 ㅋㅋㅋ",
          "original_form": "남자가 해도 좋은 운동 같아서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023096.1.2",
          "form": "오 당첨되어서 하는거였군요",
          "original_form": "오 당첨되어서 하는거였군요",
          "speaker_id": "2",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023096.1.3",
          "form": "필라테스 진짜 부끄러울거 하나도 없는데",
          "original_form": "필라테스 진짜 부끄러울거 하나도 없는데",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023096.1.4",
          "form": "되게 유연이 중요해서 그런가봐요",
          "original_form": "되게 유연이 중요해서 그런가봐요",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023096.1.5",
          "form": "약간 레깅스 입고 하고 그래서 그런가",
          "original_form": "약간 레깅스 입고 하고 그래서 그런가",
          "speaker_id": "2",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023096.1.6",
          "form": "ㅠㅠ 집에서할 땐 레깅스 안입고 그냥 하는데",
          "original_form": "ㅠㅠ 집에서할 땐 레깅스 안입고 그냥 하는데",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023096.1.7",
          "form": "그거 꼭 입어야해요?",
          "original_form": "그거 꼭 입어야해요?",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023096.1.8",
          "form": "아니요ㅋㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023096.1.9",
          "form": "근데 헐렁한거 입고할땐 강사님이 자세 잡아주기 어려워서",
          "original_form": "근데 헐렁한거 입고할땐 강사님이 자세 잡아주기 어려워서",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023096.1.10",
          "form": "그럼 필라테스 학원 다니는 남성분들도 다 레깅스 입었어요?",
          "original_form": "그럼 필라테스 학원 다니는 남성분들도 다 레깅스 입었어요?",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023096.1.11",
          "form": "지금 쓰는 근육 잘 보이려면 입는 것이 좋죠",
          "original_form": "지금 쓰는 근육 잘 보이려면 입는 것이 좋죠",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023096.1.12",
          "form": "내가 제대로 된 부분을 운동하고 있는가 확인용",
          "original_form": "내가 제대로 된 부분을 운동하고 있는가 확인용",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023096.1.13",
          "form": "헿 저 나이키 레깅스도 탐나더라구요",
          "original_form": "헿 저 나이키 레깅스도 탐나더라구요",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023096.1.14",
          "form": "집에 전신거울도 갖다놓고 싶어요 ㅋㅋㅋ",
          "original_form": "집에 전신거울도 갖다놓고 싶어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023096.1.15",
          "form": "운동할수록 운동복이 탐나여 ㅋㅋㅋ",
          "original_form": "운동할수록 운동복이 탐나여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023096.1.16",
          "form": "오 전신거울 있으면 좋을듯",
          "original_form": "오 전신거울 있으면 좋을듯",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023096.1.17",
          "form": "그쵸? 전신거울 그냥 벽면 통째로 해놓고 싶어요",
          "original_form": "그쵸? 전신거울 그냥 벽면 통째로 해놓고 싶어요",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023096.1.18",
          "form": "자그마한거 말구 ㅠㅠ",
          "original_form": "자그마한거 말구 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023096.1.19",
          "form": "마자요 연습실에 잇는거",
          "original_form": "마자요 연습실에 잇는거",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023096.1.20",
          "form": "필라테스 할 때 뼈소리 우두둑 나는거 정상이에요?",
          "original_form": "필라테스 할 때 뼈소리 우두둑 나는거 정상이에요?",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023096.1.21",
          "form": "뼈소리 저는 안나던데",
          "original_form": "뼈소리 저는 안나던데",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023096.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023096.1.23",
          "form": "...전 뭐만 움직이면 우두둑 소리 나요 ㅜㅜ",
          "original_form": "...전 뭐만 움직이면 우두둑 소리 나요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023096.1.24",
          "form": "근육을 쓰는거라 뼈는 안 쓰지 않나요",
          "original_form": "근육을 쓰는거라 뼈는 안 쓰지 않나요",
          "speaker_id": "2",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100023096.1.25",
          "form": "그럼 집에는 필라테스 기구 없어요?",
          "original_form": "그럼 집에는 필라테스 기구 없어요?",
          "speaker_id": "1",
          "time": "20210829 21:43"
        }
      ]
