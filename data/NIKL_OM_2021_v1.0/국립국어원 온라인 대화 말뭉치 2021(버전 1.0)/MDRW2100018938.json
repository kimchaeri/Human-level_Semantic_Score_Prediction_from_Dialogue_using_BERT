      "utterance": [
        {
          "id": "MDRW2100018938.1.1",
          "form": "딸이 노리더니",
          "original_form": "딸이 노리더니",
          "speaker_id": "2",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018938.1.2",
          "form": "요샌 딸도 다른거 사서",
          "original_form": "요샌 딸도 다른거 사서",
          "speaker_id": "2",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018938.1.3",
          "form": "관심없어해요 ㅋㅋ",
          "original_form": "관심없어해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018938.1.4",
          "form": "ㅋㅋㅋ 따님은 뭐 좋아하세요?",
          "original_form": "ㅋㅋㅋ 따님은 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018938.1.5",
          "form": "만화책 쇼핑에 빠졌어요 딸이",
          "original_form": "만화책 쇼핑에 빠졌어요 딸이",
          "speaker_id": "2",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018938.1.6",
          "form": "만화책좋죠",
          "original_form": "만화책좋죠",
          "speaker_id": "1",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018938.1.7",
          "form": "귀멸의 칼날",
          "original_form": "귀멸의 칼날",
          "speaker_id": "1",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018938.1.8",
          "form": "또 배구만화도 재밌던데",
          "original_form": "또 배구만화도 재밌던데",
          "speaker_id": "1",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018938.1.9",
          "form": "그건 제가 ㅋㅋ",
          "original_form": "그건 제가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018938.1.10",
          "form": "딸은 바니타스수기인가",
          "original_form": "딸은 바니타스수기인가",
          "speaker_id": "2",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018938.1.11",
          "form": "헉 만화도 좋아하세요?",
          "original_form": "헉 만화도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018938.1.12",
          "form": "그거랑 한국 웹툰이요",
          "original_form": "그거랑 한국 웹툰이요",
          "speaker_id": "2",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018938.1.13",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018938.1.14",
          "form": "애니 좋아해서",
          "original_form": "애니 좋아해서",
          "speaker_id": "2",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018938.1.15",
          "form": "정말 행복하게 사시는듯",
          "original_form": "정말 행복하게 사시는듯",
          "speaker_id": "1",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018938.1.16",
          "form": "굿즈 딸한테 사줘요 ㅋㅋ",
          "original_form": "굿즈 딸한테 사줘요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018938.1.17",
          "form": "저대신 ㅋㅋ",
          "original_form": "저대신 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018938.1.18",
          "form": "가챠 쇼핑 엄청 했네요 ㅋㅋ",
          "original_form": "가챠 쇼핑 엄청 했네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018938.1.19",
          "form": "와 진짜 좋은 엄마다",
          "original_form": "와 진짜 좋은 엄마다",
          "speaker_id": "1",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018938.1.20",
          "form": "가챠 너무 즐겁죠",
          "original_form": "가챠 너무 즐겁죠",
          "speaker_id": "1",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018938.1.21",
          "form": "네 뽑는 순간 기대감 ㅋㅋ",
          "original_form": "네 뽑는 순간 기대감 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018938.1.22",
          "form": "짱리죠",
          "original_form": "짱리죠",
          "speaker_id": "2",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018938.1.23",
          "form": "이죠",
          "original_form": "이죠",
          "speaker_id": "2",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018938.1.24",
          "form": "제가 더 좋아해요",
          "original_form": "제가 더 좋아해요",
          "speaker_id": "2",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018938.1.25",
          "form": "가챠가챠소리ㅋㅋ",
          "original_form": "가챠가챠소리ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018938.1.26",
          "form": "지금 사시는 곳은 가챠샵이 없겠네요ㅠ",
          "original_form": "지금 사시는 곳은 가챠샵이 없겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018938.1.27",
          "form": "네 ㅜㅜ",
          "original_form": "네 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018938.1.28",
          "form": "아 삶의 낙이 없네요",
          "original_form": "아 삶의 낙이 없네요",
          "speaker_id": "2",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018938.1.29",
          "form": "낼 서울가면 용산에 있으려나",
          "original_form": "낼 서울가면 용산에 있으려나",
          "speaker_id": "2",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018938.1.30",
          "form": "ㅎㅎ 하나 차리세요",
          "original_form": "ㅎㅎ 하나 차리세요",
          "speaker_id": "1",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018938.1.31",
          "form": "있죠 용산!!!",
          "original_form": "있죠 용산!!!",
          "speaker_id": "1",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018938.1.32",
          "form": "망해요 ㅡㅡ",
          "original_form": "망해요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018938.1.33",
          "form": "오 그래요?",
          "original_form": "오 그래요?",
          "speaker_id": "2",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018938.1.34",
          "form": "용산 설레시겠어요",
          "original_form": "용산 설레시겠어요",
          "speaker_id": "1",
          "time": "20210827 13:51"
        }
      ]
