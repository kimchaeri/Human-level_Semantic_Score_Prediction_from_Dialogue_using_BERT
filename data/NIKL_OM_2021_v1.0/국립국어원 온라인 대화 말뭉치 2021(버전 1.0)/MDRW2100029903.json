      "utterance": [
        {
          "id": "MDRW2100029903.1.1",
          "form": "펜촉하고 실제 글씨 써지는 부분하고 잘 맞아 떨어지나요?",
          "original_form": "펜촉하고 실제 글씨 써지는 부분하고 잘 맞아 떨어지나요?",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100029903.1.2",
          "form": "네~!!",
          "original_form": "네~!!",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100029903.1.3",
          "form": "전 좀 차이가 있는것같아요ㅠ",
          "original_form": "전 좀 차이가 있는것같아요ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100029903.1.4",
          "form": "아주 굿이에요",
          "original_form": "아주 굿이에요",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100029903.1.5",
          "form": "이번에 나온게 필기감이 좋다하더라구요",
          "original_form": "이번에 나온게 필기감이 좋다하더라구요",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100029903.1.6",
          "form": "!!!!",
          "original_form": "!!!!",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100029903.1.7",
          "form": "서비스 센터 한번 가보시면 안되나요??ㅠㅠ",
          "original_form": "서비스 센터 한번 가보시면 안되나요??ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100029903.1.8",
          "form": "ㅜㅜㅜㅠㅠㅠㅠ그래볼까 싶어요",
          "original_form": "ㅜㅜㅜㅠㅠㅠㅠ그래볼까 싶어요",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100029903.1.9",
          "form": "s7도 좋다고 하던데",
          "original_form": "s7도 좋다고 하던데",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100029903.1.10",
          "form": "하...ㅠ",
          "original_form": "하...ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100029903.1.11",
          "form": "그거 어니예요??",
          "original_form": "그거 어니예요??",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100029903.1.12",
          "form": "어 맞는 것 같아요!!",
          "original_form": "어 맞는 것 같아요!!",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100029903.1.13",
          "form": "허루ㅠㅠㅜㅜ",
          "original_form": "허루ㅠㅠㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100029903.1.14",
          "form": "두 달 전에 샀는데 서비스센터 가봐야겠어요 ㅠㅠ",
          "original_form": "두 달 전에 샀는데 서비스센터 가봐야겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100029903.1.15",
          "form": "왜그러지",
          "original_form": "왜그러지",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100029903.1.16",
          "form": "보호필름 붙여보셨어요??",
          "original_form": "보호필름 붙여보셨어요??",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100029903.1.17",
          "form": "그래서 그런가?",
          "original_form": "그래서 그런가?",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100029903.1.18",
          "form": "필름때매?",
          "original_form": "필름때매?",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100029903.1.19",
          "form": "네 붙였어요!!",
          "original_form": "네 붙였어요!!",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100029903.1.20",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100029903.1.21",
          "form": "그런걸까요ㅠ? 홍당무님은 안붙이셨어요??",
          "original_form": "그런걸까요ㅠ? 홍당무님은 안붙이셨어요??",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100029903.1.22",
          "form": "붙이긴했는데....",
          "original_form": "붙이긴했는데....",
