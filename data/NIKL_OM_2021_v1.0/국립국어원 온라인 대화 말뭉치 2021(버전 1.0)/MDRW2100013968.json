      "utterance": [
        {
          "id": "MDRW2100013968.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013968.1.2",
          "form": "네 좋아하죠!",
          "original_form": "네 좋아하죠!",
          "speaker_id": "2",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013968.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013968.1.4",
          "form": "영화 신작 재밌을거같으몬",
          "original_form": "영화 신작 재밌을거같으몬",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013968.1.5",
          "form": "영화관가서 꼭 보곤해요 ㅎㅎ",
          "original_form": "영화관가서 꼭 보곤해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013968.1.6",
          "form": "거리두기가 문제죠",
          "original_form": "거리두기가 문제죠",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013968.1.7",
          "form": "거리두기가 문제긴한데",
          "original_form": "거리두기가 문제긴한데",
          "speaker_id": "2",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013968.1.8",
          "form": "요즘 오히려 영화관 가는 사람들이 거의 없어서",
          "original_form": "요즘 오히려 영화관 가는 사람들이 거의 없어서",
          "speaker_id": "2",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013968.1.9",
          "form": "오히려 편하긴해요",
          "original_form": "오히려 편하긴해요",
          "speaker_id": "2",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013968.1.10",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013968.1.11",
          "form": "사람이 안 모이니",
          "original_form": "사람이 안 모이니",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013968.1.12",
          "form": "네 사람이 거의 안모이다보니까",
          "original_form": "네 사람이 거의 안모이다보니까",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013968.1.13",
          "form": "영화관에 사람이 별로 없어서 오히려 편한거같아요",
          "original_form": "영화관에 사람이 별로 없어서 오히려 편한거같아요",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013968.1.14",
          "form": "다만 취식금지라 팝콘을 못먹는다는게..",
          "original_form": "다만 취식금지라 팝콘을 못먹는다는게..",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013968.1.15",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100013968.1.16",
          "form": "그 영화관가면",
          "original_form": "그 영화관가면",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100013968.1.17",
          "form": "팝콘먹으면서 보는게",
          "original_form": "팝콘먹으면서 보는게",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100013968.1.18",
          "form": "묘미인데",
          "original_form": "묘미인데",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100013968.1.19",
          "form": "못하니 참 안타깝죠 ㅠㅠ",
          "original_form": "못하니 참 안타깝죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100013968.1.20",
          "form": "그게 묘미인데",
          "original_form": "그게 묘미인데",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100013968.1.21",
          "form": "맞아요! 그나마 음료는 가능하다고 하던데",
          "original_form": "맞아요! 그나마 음료는 가능하다고 하던데",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100013968.1.22",
          "form": "음료론 묘미 느끼기 애매하죠",
          "original_form": "음료론 묘미 느끼기 애매하죠",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100013968.1.23",
          "form": "씹는맛이",
          "original_form": "씹는맛이",
          "speaker_id": "1",
