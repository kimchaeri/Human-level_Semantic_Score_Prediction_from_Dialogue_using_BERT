      "utterance": [
        {
          "id": "MDRW2100002427.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210820 12:06"
        },
        {
          "id": "MDRW2100002427.1.2",
          "form": "반가워요!",
          "original_form": "반가워요!",
          "speaker_id": "2",
          "time": "20210820 12:11"
        },
        {
          "id": "MDRW2100002427.1.3",
          "form": "넵 좋아하는게임있으신가요?",
          "original_form": "넵 좋아하는게임있으신가요?",
          "speaker_id": "1",
          "time": "20210820 12:12"
        },
        {
          "id": "MDRW2100002427.1.4",
          "form": "게임 하는것보다 구경하는걸 좋아해요",
          "original_form": "게임 하는것보다 구경하는걸 좋아해요",
          "speaker_id": "2",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002427.1.5",
          "form": "아 게임구경을 좋아하시는구나ㅎㅎ",
          "original_form": "아 게임구경을 좋아하시는구나ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002427.1.6",
          "form": "저도 게임은 그닥좋아하지않은편이라서ㅜ",
          "original_form": "저도 게임은 그닥좋아하지않은편이라서ㅜ",
          "speaker_id": "1",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002427.1.7",
          "form": "하는건 스위치?닌텐도스위치 재밌게 하긴 했어",
          "original_form": "하는건 스위치?닌텐도스위치 재밌게 하긴 했어",
          "speaker_id": "2",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002427.1.8",
          "form": "저는 카트라이더 가끔해요ㅋㅋ",
          "original_form": "저는 카트라이더 가끔해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002427.1.9",
          "form": "카트 아시나요?",
          "original_form": "카트 아시나요?",
          "speaker_id": "1",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002427.1.10",
          "form": "카트라이더요!?",
          "original_form": "카트라이더요!?",
          "speaker_id": "2",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002427.1.11",
          "form": "저는 카트라이더 너무 어렵더라구요",
          "original_form": "저는 카트라이더 너무 어렵더라구요",
          "speaker_id": "2",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002427.1.12",
          "form": "대신 크아는 많이 했어요ㅋㅋ",
          "original_form": "대신 크아는 많이 했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002427.1.13",
          "form": "아 크아저도했어요ㅋㅋㅋ",
          "original_form": "아 크아저도했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002427.1.14",
          "form": "캠프맵 기억나시나요",
          "original_form": "캠프맵 기억나시나요",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002427.1.15",
          "form": "아~알죠알죠",
          "original_form": "아~알죠알죠",
          "speaker_id": "2",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002427.1.16",
          "form": "ㅋㅋㅋㅋ레고도 귀여워서 좋아했어요",
          "original_form": "ㅋㅋㅋㅋ레고도 귀여워서 좋아했어요",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002427.1.17",
          "form": "ㅋㅋㅋ진짜추억의 게임이네요",
          "original_form": "ㅋㅋㅋ진짜추억의 게임이네요",
          "speaker_id": "1",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002427.1.18",
          "form": "배찌캐릭터자주했었어요저는ㅋㅋ",
          "original_form": "배찌캐릭터자주했었어요저는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002427.1.19",
          "form": "ㅋㅋㅋ저는 그 단발머리캐릭터 이름 뭐죠",
          "original_form": "ㅋㅋㅋ저는 그 단발머리캐릭터 이름 뭐죠",
          "speaker_id": "2",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002427.1.20",
          "form": "기억이 안나는데 그거 많이 했어요",
          "original_form": "기억이 안나는데 그거 많이 했어요",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002427.1.21",
          "form": "마리드요ㅋㅋㅋ",
          "original_form": "마리드요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002427.1.22",
          "form": "삼각김밥머리",
          "original_form": "삼각김밥머리",
