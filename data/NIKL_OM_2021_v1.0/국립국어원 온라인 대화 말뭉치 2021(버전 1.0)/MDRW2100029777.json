      "utterance": [
        {
          "id": "MDRW2100029777.1.1",
          "form": "오늘은 점심 안 먹고",
          "original_form": "오늘은 점심 안 먹고",
          "speaker_id": "2",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100029777.1.2",
          "form": "일찍 퇴근하는 날이라서",
          "original_form": "일찍 퇴근하는 날이라서",
          "speaker_id": "2",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100029777.1.3",
          "form": "안타깝게도",
          "original_form": "안타깝게도",
          "speaker_id": "2",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100029777.1.4",
          "form": "안 먹었네요",
          "original_form": "안 먹었네요",
          "speaker_id": "2",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100029777.1.5",
          "form": "그러 집에 가서 먹는건가요?",
          "original_form": "그러 집에 가서 먹는건가요?",
          "speaker_id": "1",
          "time": "20210903 13:25"
        },
        {
          "id": "MDRW2100029777.1.6",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "2",
          "time": "20210903 13:26"
        },
        {
          "id": "MDRW2100029777.1.7",
          "form": "아 그러고보미",
          "original_form": "아 그러고보미",
          "speaker_id": "2",
          "time": "20210903 13:26"
        },
        {
          "id": "MDRW2100029777.1.8",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "2",
          "time": "20210903 13:26"
        },
        {
          "id": "MDRW2100029777.1.9",
          "form": "결국 메뉴를 골라야 하는군요",
          "original_form": "결국 메뉴를 골라야 하는군요",
          "speaker_id": "2",
          "time": "20210903 13:26"
        },
        {
          "id": "MDRW2100029777.1.10",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 13:26"
        },
        {
          "id": "MDRW2100029777.1.11",
          "form": "전 방금 먹었어요",
          "original_form": "전 방금 먹었어요",
          "speaker_id": "1",
          "time": "20210903 13:26"
        },
        {
          "id": "MDRW2100029777.1.12",
          "form": "돼지고기 구워서 집에서 먹었답니다",
          "original_form": "돼지고기 구워서 집에서 먹었답니다",
          "speaker_id": "1",
          "time": "20210903 13:26"
        },
        {
          "id": "MDRW2100029777.1.13",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "2",
          "time": "20210903 13:27"
        },
        {
          "id": "MDRW2100029777.1.14",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210903 13:27"
        },
        {
          "id": "MDRW2100029777.1.15",
          "form": "저도 오늘은 삼겹살이나...",
          "original_form": "저도 오늘은 삼겹살이나...",
          "speaker_id": "2",
          "time": "20210903 13:28"
        },
        {
          "id": "MDRW2100029777.1.16",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "1",
          "time": "20210903 13:28"
        },
        {
          "id": "MDRW2100029777.1.17",
          "form": "이제 퇴근하면서",
          "original_form": "이제 퇴근하면서",
          "speaker_id": "2",
          "time": "20210903 16:21"
        },
        {
          "id": "MDRW2100029777.1.18",
          "form": "사갈 예정입니다",
          "original_form": "사갈 예정입니다",
          "speaker_id": "2",
          "time": "20210903 16:21"
        },
        {
          "id": "MDRW2100029777.1.19",
          "form": "점심 샴겹살 맛있게 먹었나요?",
          "original_form": "점심 샴겹살 맛있게 먹었나요?",
          "speaker_id": "1",
          "time": "20210903 19:35"
        },
        {
          "id": "MDRW2100029777.1.20",
          "form": "안타깝게도",
          "original_form": "안타깝게도",
          "speaker_id": "2",
          "time": "20210903 20:02"
        },
        {
          "id": "MDRW2100029777.1.21",
          "form": "조금 늦게 퇴근해서",
          "original_form": "조금 늦게 퇴근해서",
          "speaker_id": "2",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100029777.1.22",
          "form": "저녁 삼겹살이 됐네요",
          "original_form": "저녁 삼겹살이 됐네요",
          "speaker_id": "2",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100029777.1.23",
          "form": "그렇군요.. 이미 드셨겠죠? 점심도 못먹고",
          "original_form": "그렇군요.. 이미 드셨겠죠? 점심도 못먹고",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100029777.1.24",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100029777.1.25",
          "form": "하지만 먹었습니다",
          "original_form": "하지만 먹었습니다",
          "speaker_id": "2",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100029777.1.26",
          "form": "맛있어서 화풀림",
          "original_form": "맛있어서 화풀림",
          "speaker_id": "2",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100029777.1.27",
          "form": "맛있으면 된거죠..ㅎㅎ",
          "original_form": "맛있으면 된거죠..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100029777.1.28",
          "form": "맛없었으면",
          "original_form": "맛없었으면",
          "speaker_id": "2",
          "time": "20210903 20:49"
        },
        {
          "id": "MDRW2100029777.1.29",
          "form": "실망할 뻔 했죠...",
          "original_form": "실망할 뻔 했죠...",
          "speaker_id": "2",
          "time": "20210903 20:49"
        },
        {
          "id": "MDRW2100029777.1.30",
          "form": "큰일날뻔 했네요.. 다행입니다",
          "original_form": "큰일날뻔 했네요.. 다행입니다",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100029777.1.31",
          "form": "야식도 먹는 타입인가요?",
          "original_form": "야식도 먹는 타입인가요?",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100029777.1.32",
          "form": "11시 이전에 자는 타입인지라",
          "original_form": "11시 이전에 자는 타입인지라",
          "speaker_id": "2",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100029777.1.33",
          "form": "다행히도",
          "original_form": "다행히도",
          "speaker_id": "2",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100029777.1.34",
          "form": "야식은...",
          "original_form": "야식은...",
          "speaker_id": "2",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100029777.1.35",
          "form": "오.. 그렇군요.. 저희집도 야식은 안먹습니다",
          "original_form": "오.. 그렇군요.. 저희집도 야식은 안먹습니다",
          "speaker_id": "1",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100029777.1.36",
          "form": "야식 안먹는대신에 아침을 꼭 먹죠",
          "original_form": "야식 안먹는대신에 아침을 꼭 먹죠",
          "speaker_id": "1",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100029777.1.37",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100029777.1.38",
          "form": "저도 아침은 반드시",
          "original_form": "저도 아침은 반드시",
          "speaker_id": "2",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100029777.1.39",
          "form": "챙겨 먹습니다",
          "original_form": "챙겨 먹습니다",
          "speaker_id": "2",
          "time": "20210903 21:06"
        }
      ]
    }
