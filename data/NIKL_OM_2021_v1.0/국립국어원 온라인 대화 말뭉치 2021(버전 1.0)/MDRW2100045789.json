      "utterance": [
        {
          "id": "MDRW2100045789.1.1",
          "form": "이제는 추운 날씨예요",
          "original_form": "이제는 추운 날씨예요",
          "speaker_id": "1",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100045789.1.2",
          "form": "이제 추워졌죠?",
          "original_form": "이제 추워졌죠?",
          "speaker_id": "1",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100045789.1.3",
          "form": "어제 비가 하루종일 오더니 날이 추워졌네요",
          "original_form": "어제 비가 하루종일 오더니 날이 추워졌네요",
          "speaker_id": "2",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100045789.1.4",
          "form": "여기두요",
          "original_form": "여기두요",
          "speaker_id": "1",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100045789.1.5",
          "form": "지금은 좀 흐리기만 하네요",
          "original_form": "지금은 좀 흐리기만 하네요",
          "speaker_id": "1",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100045789.1.6",
          "form": "그래도 우산은 챙겨가야겠죠?",
          "original_form": "그래도 우산은 챙겨가야겠죠?",
          "speaker_id": "1",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100045789.1.7",
          "form": "여기도 비는 그쳤지만 날이 많이 흐리네요",
          "original_form": "여기도 비는 그쳤지만 날이 많이 흐리네요",
          "speaker_id": "2",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100045789.1.8",
          "form": "이러다 금방 겨울올듯해요",
          "original_form": "이러다 금방 겨울올듯해요",
          "speaker_id": "1",
          "time": "20210908 07:12"
        },
        {
          "id": "MDRW2100045789.1.9",
          "form": "옷정리 하셨나요?",
          "original_form": "옷정리 하셨나요?",
          "speaker_id": "1",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100045789.1.10",
          "form": "아직 정리 못했어요",
          "original_form": "아직 정리 못했어요",
          "speaker_id": "2",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045789.1.11",
          "form": "겨울옷 꺼내야 할것 같네요",
          "original_form": "겨울옷 꺼내야 할것 같네요",
          "speaker_id": "2",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045789.1.12",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100045789.1.13",
          "form": "이러다 리빙박스에서 꺼내입다가 겨울올지",
          "original_form": "이러다 리빙박스에서 꺼내입다가 겨울올지",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045789.1.14",
          "form": "겨울옷 준비할 때가 다가 오네요",
          "original_form": "겨울옷 준비할 때가 다가 오네요",
          "speaker_id": "2",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045789.1.15",
          "form": "우산챙겨야지하다가",
          "original_form": "우산챙겨야지하다가",
          "speaker_id": "1",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045789.1.16",
          "form": "급히나온다고 잊어버리고",
          "original_form": "급히나온다고 잊어버리고",
          "speaker_id": "1",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045789.1.17",
          "form": "그냥나왔네요",
          "original_form": "그냥나왔네요",
          "speaker_id": "1",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045789.1.18",
          "form": "비가 올수도 있는데 ......",
          "original_form": "비가 올수도 있는데 ......",
          "speaker_id": "2",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100045789.1.19",
          "form": "오후엔 갠다고 했으니 괜찮을거예요",
          "original_form": "오후엔 갠다고 했으니 괜찮을거예요",
          "speaker_id": "2",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100045789.1.20",
          "form": "그러길바래봐요",
          "original_form": "그러길바래봐요",
          "speaker_id": "1",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100045789.1.21",
          "form": "아님사무실에 언니한테 여분 빌려야쥬",
          "original_form": "아님사무실에 언니한테 여분 빌려야쥬",
          "speaker_id": "1",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100045789.1.22",
          "form": "날이 좋아지길 바라며 오늘도 힘내세요",
          "original_form": "날이 좋아지길 바라며 오늘도 힘내세요",
          "speaker_id": "2",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100045789.1.23",
          "form": "좋아져야죠 ㅎㅎ",
          "original_form": "좋아져야죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:26"
        }
