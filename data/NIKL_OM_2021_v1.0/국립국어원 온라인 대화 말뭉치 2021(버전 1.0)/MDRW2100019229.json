      "utterance": [
        {
          "id": "MDRW2100019229.1.1",
          "form": "메타버스는 vr도 포함이겠죠?",
          "original_form": "메타버스는 vr도 포함이겠죠?",
          "speaker_id": "1",
          "time": "20210827 16:29"
        },
        {
          "id": "MDRW2100019229.1.2",
          "form": "그렇겠죠?",
          "original_form": "그렇겠죠?",
          "speaker_id": "2",
          "time": "20210827 16:29"
        },
        {
          "id": "MDRW2100019229.1.3",
          "form": "vr로 만나는분들도 꽤있다고 들었어요",
          "original_form": "vr로 만나는분들도 꽤있다고 들었어요",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019229.1.4",
          "form": "VR기기 집에 있으신가용?",
          "original_form": "VR기기 집에 있으신가용?",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019229.1.5",
          "form": "요즘 그거 사는 사람들 많더라구요",
          "original_form": "요즘 그거 사는 사람들 많더라구요",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019229.1.6",
          "form": "없어요... 저도 사고싶어서 노리고있어요",
          "original_form": "없어요... 저도 사고싶어서 노리고있어요",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019229.1.7",
          "form": "있으신가요?",
          "original_form": "있으신가요?",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019229.1.8",
          "form": "저두 없어서 눈물만 흘리는 중입니다",
          "original_form": "저두 없어서 눈물만 흘리는 중입니다",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019229.1.9",
          "form": "그거하면 요즘 시국에 온라인으로 친구 만나서 놀 수도 있고 좋은 것같아요",
          "original_form": "그거하면 요즘 시국에 온라인으로 친구 만나서 놀 수도 있고 좋은 것같아요",
          "speaker_id": "1",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019229.1.10",
          "form": "3d멀미가능성만 빼면 요즘시국에특히 유용한거 같아요",
          "original_form": "3d멀미가능성만 빼면 요즘시국에특히 유용한거 같아요",
          "speaker_id": "2",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019229.1.11",
          "form": "vt체험은 해봤는데 퀄리티가 높더라구요",
          "original_form": "vt체험은 해봤는데 퀄리티가 높더라구요",
          "speaker_id": "2",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019229.1.12",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "1",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019229.1.13",
          "form": "집에 없으니 VR 게임하러 가야하지만ㅋㅋㅋㅋ",
          "original_form": "집에 없으니 VR 게임하러 가야하지만ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019229.1.14",
          "form": "특히 재밌게 한거 있으세요?",
          "original_form": "특히 재밌게 한거 있으세요?",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019229.1.15",
          "form": "저는 다인용 총게임이요!",
          "original_form": "저는 다인용 총게임이요!",
          "speaker_id": "1",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019229.1.16",
          "form": "어떠셨나요?",
          "original_form": "어떠셨나요?",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019229.1.17",
          "form": "친구랑 했는데 제가 이겨서 너무 재밌었습니다ㅎ",
          "original_form": "친구랑 했는데 제가 이겨서 너무 재밌었습니다ㅎ",
          "speaker_id": "1",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019229.1.18",
          "form": "역시 이기면 더재밌죠ㅋㅋㅋㅋㅋ",
          "original_form": "역시 이기면 더재밌죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019229.1.19",
          "form": "리듬게임도 해보셨어요?",
          "original_form": "리듬게임도 해보셨어요?",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019229.1.20",
          "form": "비트세이버라구",
          "original_form": "비트세이버라구",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019229.1.21",
          "form": "아아 네! 그거 VR 게임 체험장에 꼭 있는 게임 중 하나더라구요",
          "original_form": "아아 네! 그거 VR 게임 체험장에 꼭 있는 게임 중 하나더라구요",
          "speaker_id": "1",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019229.1.22",
          "form": "근데 제가 박치인지 잘 못했어요ㅋㅋㅋㅋ",
          "original_form": "근데 제가 박치인지 잘 못했어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019229.1.23",
          "form": "name2님은 해보셨나요?",
          "original_form": "&name2&님은 해보셨나요?",
