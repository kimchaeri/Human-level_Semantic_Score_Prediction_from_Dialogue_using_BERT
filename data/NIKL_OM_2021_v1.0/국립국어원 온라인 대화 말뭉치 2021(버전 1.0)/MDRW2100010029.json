      "utterance": [
        {
          "id": "MDRW2100010029.1.1",
          "form": "공연이나 전시회 보러가는 거 좋아하시나요?",
          "original_form": "공연이나 전시회 보러가는 거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100010029.1.2",
          "form": "네. 근데 기회가 없어서 많이 접하지는 못했어요",
          "original_form": "네. 근데 기회가 없어서 많이 접하지는 못했어요",
          "speaker_id": "2",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100010029.1.3",
          "form": "고요님은 어떠신가요",
          "original_form": "고요님은 어떠신가요",
          "speaker_id": "2",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100010029.1.4",
          "form": "저는 공연이나 전시회는 일년에 한번씩은 가는 거 같아요",
          "original_form": "저는 공연이나 전시회는 일년에 한번씩은 가는 거 같아요",
          "speaker_id": "1",
          "time": "20210823 13:41"
        },
        {
          "id": "MDRW2100010029.1.5",
          "form": "어떤 공연이 기억에 남나요",
          "original_form": "어떤 공연이 기억에 남나요",
          "speaker_id": "2",
          "time": "20210823 13:43"
        },
        {
          "id": "MDRW2100010029.1.6",
          "form": "러브 포엠 때 아이유 공연 간게 기억에 남네여!",
          "original_form": "러브 포엠 때 아이유 공연 간게 기억에 남네여!",
          "speaker_id": "1",
          "time": "20210823 13:55"
        },
        {
          "id": "MDRW2100010029.1.7",
          "form": "아이유씨 콘서트 가셨구나",
          "original_form": "아이유씨 콘서트 가셨구나",
          "speaker_id": "2",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100010029.1.8",
          "form": "티케팅 빡셌을텐데",
          "original_form": "티케팅 빡셌을텐데",
          "speaker_id": "2",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100010029.1.9",
          "form": "어땠나요",
          "original_form": "어땠나요",
          "speaker_id": "2",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100010029.1.10",
          "form": "엄청 빡셌죠",
          "original_form": "엄청 빡셌죠",
          "speaker_id": "1",
          "time": "20210823 13:57"
        },
        {
          "id": "MDRW2100010029.1.11",
          "form": "진짜 너무 좋았어요",
          "original_form": "진짜 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210823 13:57"
        },
        {
          "id": "MDRW2100010029.1.12",
          "form": "가장 기억에 나는곡은 뭐였나요",
          "original_form": "가장 기억에 나는곡은 뭐였나요",
          "speaker_id": "2",
          "time": "20210823 13:57"
        },
        {
          "id": "MDRW2100010029.1.13",
          "form": "정말 오롯이 행복해지는 느낌이라고 해야하나",
          "original_form": "정말 오롯이 행복해지는 느낌이라고 해야하나",
          "speaker_id": "1",
          "time": "20210823 13:57"
        },
        {
          "id": "MDRW2100010029.1.14",
          "form": "역시 내손을 잡아죠!!!!",
          "original_form": "역시 내손을 잡아죠!!!!",
          "speaker_id": "1",
          "time": "20210823 13:58"
        },
        {
          "id": "MDRW2100010029.1.15",
          "form": "그 노래 좋아요",
          "original_form": "그 노래 좋아요",
          "speaker_id": "2",
          "time": "20210823 13:58"
        },
        {
          "id": "MDRW2100010029.1.16",
          "form": "직접들었다니 너무 좋았겠다",
          "original_form": "직접들었다니 너무 좋았겠다",
          "speaker_id": "2",
          "time": "20210823 13:58"
        },
        {
          "id": "MDRW2100010029.1.17",
          "form": "맞아요 유튜브에 그거 떴을 때 엄청 기뻤어요ㅋㅋㅋ",
          "original_form": "맞아요 유튜브에 그거 떴을 때 엄청 기뻤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:59"
        },
        {
          "id": "MDRW2100010029.1.18",
          "form": "막 설레고 그 때가 또 생각나고",
          "original_form": "막 설레고 그 때가 또 생각나고",
          "speaker_id": "2",
          "time": "20210823 13:59"
        },
        {
          "id": "MDRW2100010029.1.19",
          "form": "그러셨겠어요",
          "original_form": "그러셨겠어요",
          "speaker_id": "2",
          "time": "20210823 13:59"
        },
        {
          "id": "MDRW2100010029.1.20",
          "form": "그거 보면서 또 아련해지고... 또가고 싶고... 그랬죠ㅋㅋㅋ",
          "original_form": "그거 보면서 또 아련해지고... 또가고 싶고... 그랬죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:00"
        }
      ]
