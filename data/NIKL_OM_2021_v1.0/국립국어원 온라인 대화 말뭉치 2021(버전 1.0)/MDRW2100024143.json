      "utterance": [
        {
          "id": "MDRW2100024143.1.1",
          "form": "좋아하시는 음료는 있으신가요?",
          "original_form": "좋아하시는 음료는 있으신가요?",
          "speaker_id": "2",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024143.1.2",
          "form": "당연히 술은 회랑 빠질 수 없는 친구구요ㅋㅋ",
          "original_form": "당연히 술은 회랑 빠질 수 없는 친구구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024143.1.3",
          "form": "음료ㅎㅎ 사실 전 술맛도 커피맛도 몰라요",
          "original_form": "음료ㅎㅎ 사실 전 술맛도 커피맛도 몰라요",
          "speaker_id": "1",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100024143.1.4",
          "form": "오직 안주발ㅎㅎ",
          "original_form": "오직 안주발ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100024143.1.5",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024143.1.6",
          "form": "본연의 맛을 즐기시군요!",
          "original_form": "본연의 맛을 즐기시군요!",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024143.1.7",
          "form": "그렇죠 사실 메인은 안주지 술이 아니잖아요ㅎ",
          "original_form": "그렇죠 사실 메인은 안주지 술이 아니잖아요ㅎ",
          "speaker_id": "1",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100024143.1.8",
          "form": "안주 먹기 위해 술 마셔요",
          "original_form": "안주 먹기 위해 술 마셔요",
          "speaker_id": "1",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100024143.1.9",
          "form": "님은 좋아하는 음료가 있나요",
          "original_form": "님은 좋아하는 음료가 있나요",
          "speaker_id": "1",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100024143.1.10",
          "form": "음료는 ㅋㅋㅋ",
          "original_form": "음료는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100024143.1.11",
          "form": "솔의눈이요ㅋㅋ",
          "original_form": "솔의눈이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100024143.1.12",
          "form": "여름에 얼음 타마시면 맛있어요!ㅋ",
          "original_form": "여름에 얼음 타마시면 맛있어요!ㅋ",
          "speaker_id": "2",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024143.1.13",
          "form": "아니ㅋㅋㅋ",
          "original_form": "아니ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024143.1.14",
          "form": "혹시 아침햇살 민초 데자와 중에 취향인게 있나요?",
          "original_form": "혹시 아침햇살 민초 데자와 중에 취향인게 있나요?",
          "speaker_id": "1",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024143.1.15",
          "form": "일단 다 먹는데 민초보다는 피스타치오가 더 좋고 데자와는 사랑합니다ㅋㅋㅋ",
          "original_form": "일단 다 먹는데 민초보다는 피스타치오가 더 좋고 데자와는 사랑합니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100024143.1.16",
          "form": "민초도 먹긴해요ㅋ",
          "original_form": "민초도 먹긴해요ㅋ",
          "speaker_id": "2",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100024143.1.17",
          "form": "파인애플 피자도 드시겠죵?",
          "original_form": "파인애플 피자도 드시겠죵?",
          "speaker_id": "1",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100024143.1.18",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "2",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100024143.1.19",
          "form": "그건 용납못하겠어요ㅋㄱㅋ",
          "original_form": "그건 용납못하겠어요ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100024143.1.20",
          "form": "헉;  실망이에요",
          "original_form": "헉;  실망이에요",
          "speaker_id": "1",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100024143.1.21",
          "form": "전 드디어 동지를 만난줄ㅜㅇㅜ",
          "original_form": "전 드디어 동지를 만난줄ㅜㅇㅜ",
          "speaker_id": "1",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100024143.1.22",
          "form": "헉!",
          "original_form": "헉!",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024143.1.23",
          "form": "파인애플은 그냥 먹어도 모자라는데..",
          "original_form": "파인애플은 그냥 먹어도 모자라는데..",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024143.1.24",
          "form": "근데 맛있나요?",
          "original_form": "근데 맛있나요?",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024143.1.25",
          "form": "사실 안먹어봤어요ㅋㅋ파인애플 피자는ㅋㅋ",
          "original_form": "사실 안먹어봤어요ㅋㅋ파인애플 피자는ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024143.1.26",
          "form": "페퍼로니 듬뿍에...나름 어울려요ㅎㅎ",
          "original_form": "페퍼로니 듬뿍에...나름 어울려요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024143.1.27",
          "form": "하지만 괴식 카테고리긴 하죠ㅎ",
          "original_form": "하지만 괴식 카테고리긴 하죠ㅎ",
          "speaker_id": "1",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100024143.1.28",
          "form": "한번 드셔보세요!",
          "original_form": "한번 드셔보세요!",
          "speaker_id": "1",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100024143.1.29",
          "form": "name2님이라면 하실 수 있음!!ㅎㅎ",
          "original_form": "&name2&님이라면 하실 수 있음!!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100024143.1.30",
          "form": "맛있을 수도 있겠네요",
          "original_form": "맛있을 수도 있겠네요",
          "speaker_id": "2",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100024143.1.31",
          "form": "괜찮나요?ㅋㅋ",
          "original_form": "괜찮나요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100024143.1.32",
          "form": "어;; 저는 종종 먹어요. 자주는 아니고ㅎ",
          "original_form": "어;; 저는 종종 먹어요. 자주는 아니고ㅎ",
          "speaker_id": "1",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024143.1.33",
          "form": "생각보다 안 이상해요!",
          "original_form": "생각보다 안 이상해요!",
          "speaker_id": "1",
          "time": "20210831 22:01"
        }
      ]
