      "utterance": [
        {
          "id": "MDRW2100038315.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100038315.1.2",
          "form": "안냐세여",
          "original_form": "안냐세여",
          "speaker_id": "2",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100038315.1.3",
          "form": "최근 잘샀다고 생각하시는 꿀템있으세요 ㅎㅎ",
          "original_form": "최근 잘샀다고 생각하시는 꿀템있으세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100038315.1.4",
          "form": "무선청소기요",
          "original_form": "무선청소기요",
          "speaker_id": "2",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100038315.1.5",
          "form": "오 어디꺼로 사셨어요?",
          "original_form": "오 어디꺼로 사셨어요?",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100038315.1.6",
          "form": "제주 이사오면서 방이 더 깨끗해졌어요",
          "original_form": "제주 이사오면서 방이 더 깨끗해졌어요",
          "speaker_id": "2",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100038315.1.7",
          "form": "차이슨이요",
          "original_form": "차이슨이요",
          "speaker_id": "2",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100038315.1.8",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100038315.1.9",
          "form": "최근 어떤 쇼핑을?",
          "original_form": "최근 어떤 쇼핑을?",
          "speaker_id": "2",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100038315.1.10",
          "form": "저는 아!",
          "original_form": "저는 아!",
          "speaker_id": "1",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100038315.1.11",
          "form": "할인소식",
          "original_form": "할인소식",
          "speaker_id": "1",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100038315.1.12",
          "form": "그 혹시 가족결합되있으세요?",
          "original_form": "그 혹시 가족결합되있으세요?",
          "speaker_id": "1",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100038315.1.13",
          "form": "저희는 핸드폰 티비 인터넷 전부가족결합되어있고 sk로 쭉 쓰고있거든요",
          "original_form": "저희는 핸드폰 티비 인터넷 전부가족결합되어있고 sk로 쭉 쓰고있거든요",
          "speaker_id": "1",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100038315.1.14",
          "form": "오 저도 아직 인터넷 없는데",
          "original_form": "오 저도 아직 인터넷 없는데",
          "speaker_id": "2",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100038315.1.15",
          "form": "공유해주실 수 있나요",
          "original_form": "공유해주실 수 있나요",
          "speaker_id": "2",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100038315.1.16",
          "form": "일단 sk로 핸드폰 티비 인터넷 전부 가족결합되있고",
          "original_form": "일단 sk로 핸드폰 티비 인터넷 전부 가족결합되있고",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038315.1.17",
          "form": "오래쓴사람이면",
          "original_form": "오래쓴사람이면",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038315.1.18",
          "form": "계약 갱신할때마다 상품권줘요ㅜ",
          "original_form": "계약 갱신할때마다 상품권줘요ㅜ",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038315.1.19",
          "form": "왜 우리인터넷 처음 가입하면사은품주듯이ㅜㅜ",
          "original_form": "왜 우리인터넷 처음 가입하면사은품주듯이ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038315.1.20",
          "form": "아 좋네요 가족은 다 육지에 있어서 힘든데",
          "original_form": "아 좋네요 가족은 다 육지에 있어서 힘든데",
          "speaker_id": "2",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100038315.1.21",
          "form": "옛날부터 그런 할인이 있었는데 전 광고인줄만",
          "original_form": "옛날부터 그런 할인이 있었는데 전 광고인줄만",
          "speaker_id": "2",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100038315.1.22",
          "form": "저도 동생이랑 아빠랑 다 타지에있어요",
          "original_form": "저도 동생이랑 아빠랑 다 타지에있어요",
          "speaker_id": "1",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100038315.1.23",
          "form": "말그대로 통신사가같고",
          "original_form": "말그대로 통신사가같고",
          "speaker_id": "1",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100038315.1.24",
          "form": "가족결합이되어있으면되는거같아요",
          "original_form": "가족결합이되어있으면되는거같아요",
          "speaker_id": "1",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100038315.1.25",
          "form": "아 저는 현재는 알뜰폰 통신사써여",
          "original_form": "아 저는 현재는 알뜰폰 통신사써여",
          "speaker_id": "2",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100038315.1.26",
          "form": "암튼 통신비가 비싸자나요",
          "original_form": "암튼 통신비가 비싸자나요",
          "speaker_id": "2",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100038315.1.27",
          "form": "오 그럼 한달에 얼마나오세요?",
          "original_form": "오 그럼 한달에 얼마나오세요?",
          "speaker_id": "1",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100038315.1.28",
          "form": "저 그냥 문자통화데이터 무제한 3만3천원이요",
          "original_form": "저 그냥 문자통화데이터 무제한 3만3천원이요",
