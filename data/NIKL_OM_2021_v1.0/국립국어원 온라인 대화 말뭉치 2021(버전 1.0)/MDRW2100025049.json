      "utterance": [
        {
          "id": "MDRW2100025049.1.1",
          "form": "오우 학교!",
          "original_form": "오우 학교!",
          "speaker_id": "2",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025049.1.2",
          "form": "편입을 할까도 고민했어요",
          "original_form": "편입을 할까도 고민했어요",
          "speaker_id": "2",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025049.1.3",
          "form": "그러다가 직장때려칠 현실이 아니라 포기했지만요",
          "original_form": "그러다가 직장때려칠 현실이 아니라 포기했지만요",
          "speaker_id": "2",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025049.1.4",
          "form": "가고싶은 과나 학교가 있으셨나요?",
          "original_form": "가고싶은 과나 학교가 있으셨나요?",
          "speaker_id": "1",
          "time": "20210901 09:40"
        },
        {
          "id": "MDRW2100025049.1.5",
          "form": "가고싶다기 보다는 먹고살려고 안전성 따져서 가려고 했는데 쉽진 않네요",
          "original_form": "가고싶다기 보다는 먹고살려고 안전성 따져서 가려고 했는데 쉽진 않네요",
          "speaker_id": "2",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100025049.1.6",
          "form": "그렇군요\r\n학교다니실 때 수강신청 잘하셨나요?",
          "original_form": "그렇군요\r\n학교다니실 때 수강신청 잘하셨나요?",
          "speaker_id": "1",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100025049.1.7",
          "form": "저는 어렵지않게 했던거 같아요!",
          "original_form": "저는 어렵지않게 했던거 같아요!",
          "speaker_id": "2",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025049.1.8",
          "form": "대부분 전공과목이고",
          "original_form": "대부분 전공과목이고",
          "speaker_id": "2",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025049.1.9",
          "form": "특수한 과이셨나봐요",
          "original_form": "특수한 과이셨나봐요",
          "speaker_id": "1",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025049.1.10",
          "form": "네네 맞아요ㅎㅎ 특수과라서 그닥 어렵지않았어요",
          "original_form": "네네 맞아요ㅎㅎ 특수과라서 그닥 어렵지않았어요",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025049.1.11",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025049.1.12",
          "form": "그럼 학교다니실 때 동아리하셨나요?",
          "original_form": "그럼 학교다니실 때 동아리하셨나요?",
          "speaker_id": "1",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025049.1.13",
          "form": "아뇨 ㅜ",
          "original_form": "아뇨 ㅜ",
          "speaker_id": "2",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025049.1.14",
          "form": "대학은 재미없게 다녔어요",
          "original_form": "대학은 재미없게 다녔어요",
          "speaker_id": "2",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025049.1.15",
          "form": "워낙 집순이라 학교 집 학교 집 이었다죠",
          "original_form": "워낙 집순이라 학교 집 학교 집 이었다죠",
          "speaker_id": "2",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025049.1.16",
          "form": "그렇군요 저도 집순인데ㅠㅠ\r\n혹시 보건과셨나요?",
          "original_form": "그렇군요 저도 집순인데ㅠㅠ\r\n혹시 보건과셨나요?",
          "speaker_id": "1",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025049.1.17",
          "form": "보건쪽이에요 ㅎ",
          "original_form": "보건쪽이에요 ㅎ",
          "speaker_id": "2",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025049.1.18",
          "form": "그럼 지금 학과관련 일하시는건가요?",
          "original_form": "그럼 지금 학과관련 일하시는건가요?",
          "speaker_id": "1",
          "time": "20210901 11:20"
        },
        {
          "id": "MDRW2100025049.1.19",
          "form": "네 ㅎ 그 전공 일 하고있어요",
          "original_form": "네 ㅎ 그 전공 일 하고있어요",
          "speaker_id": "2",
          "time": "20210901 11:21"
        },
        {
          "id": "MDRW2100025049.1.20",
          "form": "다른거 하고싶은데",
          "original_form": "다른거 하고싶은데",
          "speaker_id": "2",
          "time": "20210901 11:21"
        },
        {
          "id": "MDRW2100025049.1.21",
          "form": "못하네요",
          "original_form": "못하네요",
          "speaker_id": "2",
          "time": "20210901 11:21"
        },
        {
          "id": "MDRW2100025049.1.22",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "2",
          "time": "20210901 11:21"
        }
      ]
