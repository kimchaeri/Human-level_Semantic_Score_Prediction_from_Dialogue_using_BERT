        {
          "id": "MDRW2100032900.1.1",
          "form": "여보 우리 신혼여행간거 생각나니??",
          "original_form": "여보 우리 신혼여행간거 생각나니??",
          "speaker_id": "1",
          "time": "20210906 04:28"
        },
        {
          "id": "MDRW2100032900.1.2",
          "form": "어 좋았지...",
          "original_form": "어 좋았지...",
          "speaker_id": "2",
          "time": "20210906 04:28"
        },
        {
          "id": "MDRW2100032900.1.3",
          "form": "몰디브도 몰디브지만....",
          "original_form": "몰디브도 몰디브지만....",
          "speaker_id": "1",
          "time": "20210906 04:28"
        },
        {
          "id": "MDRW2100032900.1.4",
          "form": "그때의 여유가 너무 그립네..",
          "original_form": "그때의 여유가 너무 그립네..",
          "speaker_id": "1",
          "time": "20210906 04:29"
        },
        {
          "id": "MDRW2100032900.1.5",
          "form": "지금은 여유가 너무 없지",
          "original_form": "지금은 여유가 너무 없지",
          "speaker_id": "2",
          "time": "20210906 04:29"
        },
        {
          "id": "MDRW2100032900.1.6",
          "form": "없지... 이새벽에만 여유있게 폰하고 있잖아...",
          "original_form": "없지... 이새벽에만 여유있게 폰하고 있잖아...",
          "speaker_id": "1",
          "time": "20210906 04:29"
        },
        {
          "id": "MDRW2100032900.1.7",
          "form": "휴..... 애들도 자꾸 깨고..",
          "original_form": "휴..... 애들도 자꾸 깨고..",
          "speaker_id": "2",
          "time": "20210906 04:29"
        },
        {
          "id": "MDRW2100032900.1.8",
          "form": "어.....",
          "original_form": "어.....",
          "speaker_id": "1",
          "time": "20210906 04:29"
        },
        {
          "id": "MDRW2100032900.1.9",
          "form": "다시 갈 수 있을까???",
          "original_form": "다시 갈 수 있을까???",
          "speaker_id": "1",
          "time": "20210906 04:29"
        },
        {
          "id": "MDRW2100032900.1.10",
          "form": "어디 몰디브???",
          "original_form": "어디 몰디브???",
          "speaker_id": "2",
          "time": "20210906 04:29"
        },
        {
          "id": "MDRW2100032900.1.11",
          "form": "어어",
          "original_form": "어어",
          "speaker_id": "1",
          "time": "20210906 04:29"
        },
        {
          "id": "MDRW2100032900.1.12",
          "form": "돈만 있으면 가지.ㅋㅋㅋㅋ",
          "original_form": "돈만 있으면 가지.ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 04:30"
        },
        {
          "id": "MDRW2100032900.1.13",
          "form": "ㅋㅋㅋㅋ맞는말이네..",
          "original_form": "ㅋㅋㅋㅋ맞는말이네..",
          "speaker_id": "1",
          "time": "20210906 04:30"
        },
        {
          "id": "MDRW2100032900.1.14",
          "form": "근데 애들 다데리고 가기에는 너무 비싸지 않냐??",
          "original_form": "근데 애들 다데리고 가기에는 너무 비싸지 않냐??",
          "speaker_id": "1",
          "time": "20210906 04:30"
        },
        {
          "id": "MDRW2100032900.1.15",
          "form": "그치... 한 2000은 우습게 깨질듯??",
          "original_form": "그치... 한 2000은 우습게 깨질듯??",
          "speaker_id": "2",
          "time": "20210906 04:30"
        },
        {
          "id": "MDRW2100032900.1.16",
          "form": "2000?? 비행기 값만 1000나오겠다..ㅋㅋㅋ",
          "original_form": "2000?? 비행기 값만 1000나오겠다..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 04:30"
        },
        {
          "id": "MDRW2100032900.1.17",
          "form": "그렇게 비쌌었나???",
          "original_form": "그렇게 비쌌었나???",
          "speaker_id": "2",
          "time": "20210906 04:31"
        },
        {
          "id": "MDRW2100032900.1.18",
          "form": "어....",
          "original_form": "어....",
          "speaker_id": "1",
          "time": "20210906 04:31"
        },
        {
          "id": "MDRW2100032900.1.19",
          "form": "현실이네..ㅋㅋㅋ 이젠 우린 해외여행은 못갈지도..",
          "original_form": "현실이네..ㅋㅋㅋ 이젠 우린 해외여행은 못갈지도..",
