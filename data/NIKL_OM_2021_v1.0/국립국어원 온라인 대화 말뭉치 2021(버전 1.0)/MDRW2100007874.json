      "utterance": [
        {
          "id": "MDRW2100007874.1.1",
          "form": "재택근무 해보신 소감이 어떠세요",
          "original_form": "재택근무 해보신 소감이 어떠세요",
          "speaker_id": "1",
          "time": "20210822 11:36"
        },
        {
          "id": "MDRW2100007874.1.2",
          "form": "재택근무 할때 좀 많이자요",
          "original_form": "재택근무 할때 좀 많이자요",
          "speaker_id": "2",
          "time": "20210822 13:30"
        },
        {
          "id": "MDRW2100007874.1.3",
          "form": "재택근무를 하시라고요",
          "original_form": "재택근무를 하시라고요",
          "speaker_id": "1",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100007874.1.4",
          "form": "재택까지만 하시면 어떡합니까",
          "original_form": "재택까지만 하시면 어떡합니까",
          "speaker_id": "1",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100007874.1.5",
          "form": "근무는 별로 하고싶지  않은걸요",
          "original_form": "근무는 별로 하고싶지  않은걸요",
          "speaker_id": "2",
          "time": "20210822 13:46"
        },
        {
          "id": "MDRW2100007874.1.6",
          "form": "하고싶지않아도 해야해",
          "original_form": "하고싶지않아도 해야해",
          "speaker_id": "1",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100007874.1.7",
          "form": "우린 어른이야",
          "original_form": "우린 어른이야",
          "speaker_id": "1",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100007874.1.8",
          "form": "어른 이라서  저의 할당량은 하고 자지만",
          "original_form": "어른 이라서  저의 할당량은 하고 자지만",
          "speaker_id": "2",
          "time": "20210822 18:48"
        },
        {
          "id": "MDRW2100007874.1.9",
          "form": "양심에 찔리는건 어쩔수가 없네",
          "original_form": "양심에 찔리는건 어쩔수가 없네",
          "speaker_id": "2",
          "time": "20210822 18:48"
        },
        {
          "id": "MDRW2100007874.1.10",
          "form": "그리고 직업 아니고 실습인데 열심히 해야해.?",
          "original_form": "그리고 직업 아니고 실습인데 열심히 해야해.?",
          "speaker_id": "2",
          "time": "20210822 18:49"
        },
        {
          "id": "MDRW2100007874.1.11",
          "form": "흠 그건 맞지",
          "original_form": "흠 그건 맞지",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100007874.1.12",
          "form": "난 내 본래 직업인 학생의 의무",
          "original_form": "난 내 본래 직업인 학생의 의무",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100007874.1.13",
          "form": "공부안하니까",
          "original_form": "공부안하니까",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100007874.1.14",
          "form": "흠......  그것도 ㄴㅏ 찔리네?",
          "original_form": "흠......  그것도 ㄴㅏ 찔리네?",
          "speaker_id": "2",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100007874.1.15",
          "form": "넌 일도안하고 공부도 안하고",
          "original_form": "넌 일도안하고 공부도 안하고",
          "speaker_id": "1",
          "time": "20210823 12:31"
        },
        {
          "id": "MDRW2100007874.1.16",
          "form": "나도 마찬가지지만",
          "original_form": "나도 마찬가지지만",
          "speaker_id": "1",
          "time": "20210823 12:31"
        },
        {
          "id": "MDRW2100007874.1.17",
          "form": "너가 더하지!",
          "original_form": "너가 더하지!",
          "speaker_id": "2",
          "time": "20210823 13:25"
        },
        {
          "id": "MDRW2100007874.1.18",
          "form": "난 그래도 인턴을 하고 있다구!",
          "original_form": "난 그래도 인턴을 하고 있다구!",
          "speaker_id": "2",
          "time": "20210823 13:25"
        },
        {
          "id": "MDRW2100007874.1.19",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210823 18:02"
        },
        {
          "id": "MDRW2100007874.1.20",
          "form": "재택근무의 장점은 잔다구나",
          "original_form": "재택근무의 장점은 잔다구나",
          "speaker_id": "1",
          "time": "20210823 18:03"
        },
        {
          "id": "MDRW2100007874.1.21",
          "form": "지금 끝난다 도  있어",
          "original_form": "지금 끝난다 도  있어",
          "speaker_id": "2",
          "time": "20210823 18:16"
        },
        {
          "id": "MDRW2100007874.1.22",
          "form": "화목이랬나???",
          "original_form": "화목이랬나???",
          "speaker_id": "1",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100007874.1.23",
          "form": "재택근무하는날",
          "original_form": "재택근무하는날",
          "speaker_id": "1",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100007874.1.24",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100007874.1.25",
          "form": "내일도 재택이야",
          "original_form": "내일도 재택이야",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100007874.1.26",
          "form": "난 내일 노는데",
          "original_form": "난 내일 노는데",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100007874.1.27",
          "form": "재택백수",
          "original_form": "재택백수",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100007874.1.28",
          "form": "재택백수",
          "original_form": "재택백수",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100007874.1.29",
          "form": "부러운데?",
          "original_form": "부러운데?",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100007874.1.30",
          "form": "나는 집안 인턴인디",
          "original_form": "나는 집안 인턴인디",
          "speaker_id": "2",
          "time": "20210825 22:53"
        }
      ]
