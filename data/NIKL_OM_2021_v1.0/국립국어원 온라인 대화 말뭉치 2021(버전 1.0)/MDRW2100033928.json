          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100033928.1.3",
          "form": "배달음식 자주 먹어?",
          "original_form": "배달음식 자주 먹어?",
          "speaker_id": "2",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100033928.1.4",
          "form": "일주일에 한 번 정도",
          "original_form": "일주일에 한 번 정도",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100033928.1.5",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100033928.1.6",
          "form": "나는 일주일에 2~3번 먹는 거 같아",
          "original_form": "나는 일주일에 2~3번 먹는 거 같아",
          "speaker_id": "2",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100033928.1.7",
          "form": "뭐 시켜 먹어?",
          "original_form": "뭐 시켜 먹어?",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100033928.1.8",
          "form": "며칠 전에 짜글이 시켜 먹었어",
          "original_form": "며칠 전에 짜글이 시켜 먹었어",
          "speaker_id": "2",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100033928.1.9",
          "form": "너는 뭐 자주 시켜먹어?",
          "original_form": "너는 뭐 자주 시켜먹어?",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100033928.1.10",
          "form": "나는 마라탕",
          "original_form": "나는 마라탕",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100033928.1.11",
          "form": "마라탕 좋아해?",
          "original_form": "마라탕 좋아해?",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100033928.1.12",
          "form": "엄청 좋아해",
          "original_form": "엄청 좋아해",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100033928.1.13",
          "form": "고구마떡이랑 옥수수면 좋아해",
          "original_form": "고구마떡이랑 옥수수면 좋아해",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100033928.1.14",
          "form": "맛있지!",
          "original_form": "맛있지!",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100033928.1.15",
          "form": "나는 건두부 좋아해",
          "original_form": "나는 건두부 좋아해",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100033928.1.16",
          "form": "나도 요즘 건두부 맛있더라",
          "original_form": "나도 요즘 건두부 맛있더라",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100033928.1.17",
          "form": "고기는 뭐넣어 먹어?",
          "original_form": "고기는 뭐넣어 먹어?",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100033928.1.18",
          "form": "나는 소고기",
          "original_form": "나는 소고기",
          "speaker_id": "1",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100033928.1.19",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "1",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100033928.1.20",
          "form": "나도 양고기는 양꼬치만 먹어",
          "original_form": "나도 양고기는 양꼬치만 먹어",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100033928.1.21",
          "form": "양꼬치 맛있지!",
          "original_form": "양꼬치 맛있지!",
          "speaker_id": "1",
          "time": "20210825 16:04"
        }
      ]
