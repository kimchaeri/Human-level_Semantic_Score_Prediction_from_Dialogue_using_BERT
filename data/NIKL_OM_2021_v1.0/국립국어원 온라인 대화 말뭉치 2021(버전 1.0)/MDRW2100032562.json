      "utterance": [
        {
          "id": "MDRW2100032562.1.1",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "2",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100032562.1.2",
          "form": "이제 귀가해서 먹으려고요",
          "original_form": "이제 귀가해서 먹으려고요",
          "speaker_id": "1",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100032562.1.3",
          "form": "에너지바로 대충 떼우고 있었네요ㅜ",
          "original_form": "에너지바로 대충 떼우고 있었네요ㅜ",
          "speaker_id": "1",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100032562.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100032562.1.5",
          "form": "저는 볶음밥 먹었어요~",
          "original_form": "저는 볶음밥 먹었어요~",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100032562.1.6",
          "form": "오늘 한 끼도 제대로 못 드셨나요?ㅜㅜ",
          "original_form": "오늘 한 끼도 제대로 못 드셨나요?ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100032562.1.7",
          "form": "점심은 연안식당서 꼬막밥 먹었어요!",
          "original_form": "점심은 연안식당서 꼬막밥 먹었어요!",
          "speaker_id": "1",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100032562.1.8",
          "form": "요리 잘하신다니 직접 하셨어요?",
          "original_form": "요리 잘하신다니 직접 하셨어요?",
          "speaker_id": "1",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100032562.1.9",
          "form": "아하 아니요 오늘은 알바라서 거기서 먹었어요~!",
          "original_form": "아하 아니요 오늘은 알바라서 거기서 먹었어요~!",
          "speaker_id": "2",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100032562.1.10",
          "form": "집 가시면 뭐 드시나요?",
          "original_form": "집 가시면 뭐 드시나요?",
          "speaker_id": "2",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100032562.1.11",
          "form": "배달앱 검색 중입니다",
          "original_form": "배달앱 검색 중입니다",
          "speaker_id": "1",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100032562.1.12",
          "form": "치킨 피자 말고 뭐 없을까요ㅜ",
          "original_form": "치킨 피자 말고 뭐 없을까요ㅜ",
          "speaker_id": "1",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100032562.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ뭐드셨나요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ뭐드셨나요!",
          "speaker_id": "2",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100032562.1.14",
          "form": "저는 배달은 주로 마라샹궈 시켜먹어요 집에서 하기 버거운 거?",
          "original_form": "저는 배달은 주로 마라샹궈 시켜먹어요 집에서 하기 버거운 거?",
          "speaker_id": "2",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100032562.1.15",
          "form": "살치살 굽고 있습니다ㅎ",
          "original_form": "살치살 굽고 있습니다ㅎ",
          "speaker_id": "1",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100032562.1.16",
          "form": "주문하게 도착해 있네요ㅎㅎ",
          "original_form": "주문하게 도착해 있네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100032562.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맛있겠네요~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맛있겠네요~",
          "speaker_id": "2",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100032562.1.18",
          "form": "평소에 어떤 음식 좋아하세요?",
          "original_form": "평소에 어떤 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032562.1.19",
          "form": "매운거 좋아하고",
          "original_form": "매운거 좋아하고",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032562.1.20",
          "form": "해산물  좋아해요",
          "original_form": "해산물  좋아해요",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032562.1.21",
          "form": "마라샹궈 사랑합니다ㅎㅎ",
          "original_form": "마라샹궈 사랑합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032562.1.22",
          "form": "앗 마라샹궈도 좋아하시는군요!",
          "original_form": "앗 마라샹궈도 좋아하시는군요!",
          "speaker_id": "2",
          "time": "20210906 00:33"
        },
        {
          "id": "MDRW2100032562.1.23",
          "form": "완전 매운 거 잘 드시나봐요!!",
          "original_form": "완전 매운 거 잘 드시나봐요!!",
          "speaker_id": "2",
          "time": "20210906 00:33"
        },
        {
          "id": "MDRW2100032562.1.24",
          "form": "혹시 좋아하시는 마라샹궈 맛집 있으신가요?",
          "original_form": "혹시 좋아하시는 마라샹궈 맛집 있으신가요?",
          "speaker_id": "2",
          "time": "20210906 00:34"
        },
        {
          "id": "MDRW2100032562.1.25",
          "form": "가게마다 나름 다 특색이 있어서 안 가리고 좋아해요",
          "original_form": "가게마다 나름 다 특색이 있어서 안 가리고 좋아해요",
          "speaker_id": "1",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032562.1.26",
          "form": "라향각과 라화쿵부 두곳이 특히 취향입니다ㅎ",
          "original_form": "라향각과 라화쿵부 두곳이 특히 취향입니다ㅎ",
          "speaker_id": "1",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032562.1.27",
          "form": "님은 어디서 주로 드시나요?",
          "original_form": "님은 어디서 주로 드시나요?",
          "speaker_id": "1",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032562.1.28",
          "form": "저는 요즘은 많이 못 가지만 애정마라샹궈랑 중경마라탕! 이 두군데가 제일 좋았어요!",
          "original_form": "저는 요즘은 많이 못 가지만 애정마라샹궈랑 중경마라탕! 이 두군데가 제일 좋았어요!",
          "speaker_id": "2",
          "time": "20210906 01:05"
        },
        {
          "id": "MDRW2100032562.1.29",
          "form": "요즘은 주로 손오공마라탕 체인점을 갑니다~!",
          "original_form": "요즘은 주로 손오공마라탕 체인점을 갑니다~!",
          "speaker_id": "2",
          "time": "20210906 01:05"
        },
        {
          "id": "MDRW2100032562.1.30",
          "form": "오오 중경마라탕 빼고 안가본 곳들이네요",
          "original_form": "오오 중경마라탕 빼고 안가본 곳들이네요",
          "speaker_id": "1",
          "time": "20210906 01:08"
        },
        {
          "id": "MDRW2100032562.1.31",
          "form": "중경도 맛있었죠",
          "original_form": "중경도 맛있었죠",
          "speaker_id": "1",
          "time": "20210906 01:09"
        },
        {
          "id": "MDRW2100032562.1.32",
          "form": "다른 두군데도 검색해봐야겠어요.!.",
          "original_form": "다른 두군데도 검색해봐야겠어요.!.",
          "speaker_id": "1",
          "time": "20210906 01:09"
        }
      ]
    }
