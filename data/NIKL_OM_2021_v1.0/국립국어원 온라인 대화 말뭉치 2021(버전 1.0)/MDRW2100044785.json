        },
        {
          "id": "MDRW2100044785.1.2",
          "form": "안녕하세요 ㅋㅋ",
          "original_form": "안녕하세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044785.1.3",
          "form": "노래좋아하시나요?",
          "original_form": "노래좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044785.1.4",
          "form": "그렇게 자주듣는건 아니에요",
          "original_form": "그렇게 자주듣는건 아니에요",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044785.1.5",
          "form": "아항 그럼 좋아하는 프로그램있으신가요",
          "original_form": "아항 그럼 좋아하는 프로그램있으신가요",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044785.1.6",
          "form": "유퀴즈 챙겨봐요ㅋㅋ",
          "original_form": "유퀴즈 챙겨봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044785.1.7",
          "form": "오 ㅋㅋ좋아하는 편 있나요?",
          "original_form": "오 ㅋㅋ좋아하는 편 있나요?",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044785.1.8",
          "form": "옛날에 돌아다니면서 찍었던게 재미잇엇져",
          "original_form": "옛날에 돌아다니면서 찍었던게 재미잇엇져",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044785.1.9",
          "form": "애기들 나오면 순수해서 보기좋아여",
          "original_form": "애기들 나오면 순수해서 보기좋아여",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044785.1.10",
          "form": "ㅋㅋㅋ아 저도 그 애기들나온거 모아둔 클립 본적있어요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ아 저도 그 애기들나온거 모아둔 클립 본적있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044785.1.11",
          "form": "너무 귀엽더라구요",
          "original_form": "너무 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044785.1.12",
          "form": "맛아요 요즘도 재밋지만 코로나 전이 재밋엇어여",
          "original_form": "맛아요 요즘도 재밋지만 코로나 전이 재밋엇어여",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044785.1.13",
          "form": "맞아요ㅠ 직접 돌아다니는게 매력인데ㅠㅠ",
          "original_form": "맞아요ㅠ 직접 돌아다니는게 매력인데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044785.1.14",
          "form": "할머니들 나오는것도 재밋엇어여ㅋ",
          "original_form": "할머니들 나오는것도 재밋엇어여ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044785.1.15",
          "form": "오 ㅋㅋ 그 클립도 한번 찾아볼게요 ㅋㅋㅋ",
          "original_form": "오 ㅋㅋ 그 클립도 한번 찾아볼게요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
