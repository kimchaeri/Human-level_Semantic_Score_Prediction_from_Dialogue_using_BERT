      "utterance": [
        {
          "id": "MDRW2100041166.1.1",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:17"
        },
        {
          "id": "MDRW2100041166.1.2",
          "form": "지금도 잘하고 있나요?",
          "original_form": "지금도 잘하고 있나요?",
          "speaker_id": "2",
          "time": "20210905 19:18"
        },
        {
          "id": "MDRW2100041166.1.3",
          "form": "넘 잘해요ㅋㅋ",
          "original_form": "넘 잘해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:19"
        },
        {
          "id": "MDRW2100041166.1.4",
          "form": "일박이일 넘 재밌어요ㅜㅜ",
          "original_form": "일박이일 넘 재밌어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 19:23"
        },
        {
          "id": "MDRW2100041166.1.5",
          "form": "잠도 실내취침확정?",
          "original_form": "잠도 실내취침확정?",
          "speaker_id": "2",
          "time": "20210905 19:24"
        },
        {
          "id": "MDRW2100041166.1.6",
          "form": "이제 대결중!",
          "original_form": "이제 대결중!",
          "speaker_id": "1",
          "time": "20210905 19:27"
        },
        {
          "id": "MDRW2100041166.1.7",
          "form": "이겼어?",
          "original_form": "이겼어?",
          "speaker_id": "2",
          "time": "20210905 19:36"
        },
        {
          "id": "MDRW2100041166.1.8",
          "form": "졌어ㅋㅋ",
          "original_form": "졌어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:37"
        },
        {
          "id": "MDRW2100041166.1.9",
          "form": "저런",
          "original_form": "저런",
          "speaker_id": "2",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100041166.1.10",
          "form": "그럼 실외야?",
          "original_form": "그럼 실외야?",
          "speaker_id": "2",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100041166.1.11",
          "form": "늘 그렇대",
          "original_form": "늘 그렇대",
          "speaker_id": "1",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100041166.1.12",
          "form": "늘 먹는건 이기고 자는건 지고",
          "original_form": "늘 먹는건 이기고 자는건 지고",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100041166.1.13",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100041166.1.14",
          "form": "그것도 신기하넼ㅋㅋㅋㅋㅋ",
          "original_form": "그것도 신기하넼ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100041166.1.15",
          "form": "ㅋㅋ아 너무 웃겨",
          "original_form": "ㅋㅋ아 너무 웃겨",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100041166.1.16",
          "form": "연정훈은 또 실외? 밥도 못먹고?",
          "original_form": "연정훈은 또 실외? 밥도 못먹고?",
          "speaker_id": "2",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100041166.1.17",
          "form": "밥은 다 나눠먹었어ㅋㅋ",
          "original_form": "밥은 다 나눠먹었어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100041166.1.18",
          "form": "코골이 두명이랑 에어컨방에서 자기와 덥고 조용한 야외취침",
          "original_form": "코골이 두명이랑 에어컨방에서 자기와 덥고 조용한 야외취침",
          "speaker_id": "1",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100041166.1.19",
          "form": "아 그렇구만ㅋㅋㅋㅋ",
          "original_form": "아 그렇구만ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:08"
        },
        {
          "id": "MDRW2100041166.1.20",
          "form": "재밌었으면 되었당ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "재밌었으면 되었당ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:08"
        }
      ]
