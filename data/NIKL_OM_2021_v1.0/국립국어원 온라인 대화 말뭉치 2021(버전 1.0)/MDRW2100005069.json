      "utterance": [
        {
          "id": "MDRW2100005069.1.1",
          "form": "어떤 동물 키우세요",
          "original_form": "어떤 동물 키우세요",
          "speaker_id": "2",
          "time": "20210821 20:18"
        },
        {
          "id": "MDRW2100005069.1.2",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:18"
        },
        {
          "id": "MDRW2100005069.1.3",
          "form": "저 고양이 키워요",
          "original_form": "저 고양이 키워요",
          "speaker_id": "1",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005069.1.4",
          "form": "반려동물 키우시나요",
          "original_form": "반려동물 키우시나요",
          "speaker_id": "1",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005069.1.5",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "2",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005069.1.6",
          "form": "전 도마뱀이요",
          "original_form": "전 도마뱀이요",
          "speaker_id": "2",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005069.1.7",
          "form": "비어디 드래곤 2마리요 ㅎㅎ",
          "original_form": "비어디 드래곤 2마리요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005069.1.8",
          "form": "집이 4자 두개라 엄청커요 ㅎㅎ",
          "original_form": "집이 4자 두개라 엄청커요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005069.1.9",
          "form": "고앵이는 밥잘 먹나요?",
          "original_form": "고앵이는 밥잘 먹나요?",
          "speaker_id": "2",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005069.1.10",
          "form": "고양이",
          "original_form": "고양이",
          "speaker_id": "2",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005069.1.11",
          "form": "저희집 도마뱀은 밥을 안먹어서 걱정이 많아요",
          "original_form": "저희집 도마뱀은 밥을 안먹어서 걱정이 많아요",
          "speaker_id": "2",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005069.1.12",
          "form": "앗 도마뱀은 그거 알아욛",
          "original_form": "앗 도마뱀은 그거 알아욛",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005069.1.13",
          "form": "도마뱀맞나모르겟는데",
          "original_form": "도마뱀맞나모르겟는데",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005069.1.14",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005069.1.15",
          "form": "레오파드 게코인가요??",
          "original_form": "레오파드 게코인가요??",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005069.1.16",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005069.1.17",
          "form": "그거 본적있어요 귀엽더라구요!",
          "original_form": "그거 본적있어요 귀엽더라구요!",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005069.1.18",
          "form": "저희 고양이도 입이 짧아서 잘 안먹어서 걱정이에요ㅠ",
          "original_form": "저희 고양이도 입이 짧아서 잘 안먹어서 걱정이에요ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005069.1.19",
          "form": "리오파드 게코는 귀엽고",
          "original_form": "리오파드 게코는 귀엽고",
          "speaker_id": "2",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005069.1.20",
          "form": "저희애는 용같아요 ㅋㅋ",
          "original_form": "저희애는 용같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005069.1.21",
          "form": "아 안먹는군요 ㅜㅜ",
          "original_form": "아 안먹는군요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005069.1.22",
          "form": "저 억지로 먹여요",
          "original_form": "저 억지로 먹여요",
          "speaker_id": "2",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005069.1.23",
          "form": "멋있을거같아요 ㅋㅋ",
          "original_form": "멋있을거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005069.1.24",
          "form": "억지로라도 먹으면 다행인데",
          "original_form": "억지로라도 먹으면 다행인데",
          "speaker_id": "1",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005069.1.25",
          "form": "네 아주 거만해요",
          "original_form": "네 아주 거만해요",
          "speaker_id": "2",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005069.1.26",
          "form": "줘도 안먹더라구요ㅠ",
          "original_form": "줘도 안먹더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005069.1.27",
          "form": "무시해요 ㅋㅋ",
          "original_form": "무시해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005069.1.28",
          "form": "거만ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "거만ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005069.1.29",
          "form": "눈을 감으며 무시해요",
          "original_form": "눈을 감으며 무시해요",
          "speaker_id": "2",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005069.1.30",
          "form": "먹이는 뭘 주로먹나요?",
          "original_form": "먹이는 뭘 주로먹나요?",
          "speaker_id": "1",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005069.1.31",
          "form": "호박 당근 밀웜",
          "original_form": "호박 당근 밀웜",
          "speaker_id": "2",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005069.1.32",
          "form": "사료요",
          "original_form": "사료요",
          "speaker_id": "2",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005069.1.33",
          "form": "고양이는 뭐 먹어요?",
          "original_form": "고양이는 뭐 먹어요?",
          "speaker_id": "2",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005069.1.34",
          "form": "아 호박 당근같은것두 먹는군요",
          "original_form": "아 호박 당근같은것두 먹는군요",
          "speaker_id": "1",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005069.1.35",
          "form": "저희딸 길에 고양이 먹이 주고다녀요",
          "original_form": "저희딸 길에 고양이 먹이 주고다녀요",
          "speaker_id": "2",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005069.1.36",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005069.1.37",
          "form": "캔사료나 건식사료 이런거 먹어요!",
          "original_form": "캔사료나 건식사료 이런거 먹어요!",
          "speaker_id": "1",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005069.1.38",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005069.1.39",
          "form": "따님이 착하시네요 ㅠㅠ",
          "original_form": "따님이 착하시네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005069.1.40",
          "form": "저희딸도 사서 주더다고요",
          "original_form": "저희딸도 사서 주더다고요",
          "speaker_id": "2",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005069.1.41",
          "form": "네 name3이라고 이름도 지었더라고요",
          "original_form": "네 &name3&이라고 이름도 지었더라고요",
          "speaker_id": "2",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005069.1.42",
          "form": "제가 털있으면 못길러서 ㅜㅜ못키우니 그래요",
          "original_form": "제가 털있으면 못길러서 ㅜㅜ못키우니 그래요",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005069.1.43",
          "form": "아 알러지가 있으신가봐요 ㅜㅜ",
          "original_form": "아 알러지가 있으신가봐요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005069.1.44",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005069.1.45",
          "form": "의사쌤이 기르지 마라셔서요",
          "original_form": "의사쌤이 기르지 마라셔서요",
          "speaker_id": "2",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005069.1.46",
          "form": "그래도 name3이는 따님덕에 조금이라두 따뜻하겠어용 ㅎㅎ",
          "original_form": "그래도 &name3&이는 따님덕에 조금이라두 따뜻하겠어용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005069.1.47",
          "form": "알러지있으면 위험하죠",
          "original_form": "알러지있으면 위험하죠",
          "speaker_id": "1",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005069.1.48",
          "form": "기관지위험이라서 이해해요 ㅠㅠ",
          "original_form": "기관지위험이라서 이해해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005069.1.49",
          "form": "네 기관지 기침이 심해요",
          "original_form": "네 기관지 기침이 심해요",
          "speaker_id": "2",
          "time": "20210821 20:27"
        }
      ]
