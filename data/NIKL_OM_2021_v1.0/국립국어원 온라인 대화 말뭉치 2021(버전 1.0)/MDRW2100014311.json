      "utterance": [
        {
          "id": "MDRW2100014311.1.1",
          "form": "하아.. 우리 싸이월드 부활한대",
          "original_form": "하아.. 우리 싸이월드 부활한대",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014311.1.2",
          "form": "내 중2병 감성들 넘치는 창고가 봉인해제된다니 ㅠㅠ",
          "original_form": "내 중2병 감성들 넘치는 창고가 봉인해제된다니 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014311.1.3",
          "form": "너 싸이월드 했었니?",
          "original_form": "너 싸이월드 했었니?",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014311.1.4",
          "form": "어휴 name2 그걸 안 하던 사람이 어딨어요",
          "original_form": "어휴 &name2& 그걸 안 하던 사람이 어딨어요",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014311.1.5",
          "form": "우리 시절엔 다~~ 했죠!!",
          "original_form": "우리 시절엔 다~~ 했죠!!",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014311.1.6",
          "form": "나는 부활한다는 소식 보자마자 탈퇴했어",
          "original_form": "나는 부활한다는 소식 보자마자 탈퇴했어",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014311.1.7",
          "form": "흑역사가 너무 많아서",
          "original_form": "흑역사가 너무 많아서",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014311.1.8",
          "form": "어머 걱정하지마세요 언니",
          "original_form": "어머 걱정하지마세요 언니",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014311.1.9",
          "form": "언니가 그려줬던 그림들 저 아직도 가지고 있어요",
          "original_form": "언니가 그려줬던 그림들 저 아직도 가지고 있어요",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014311.1.10",
          "form": "어찌나 예쁘던지~ 아직도 종종 보고있어요",
          "original_form": "어찌나 예쁘던지~ 아직도 종종 보고있어요",
          "speaker_id": "1",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014311.1.11",
          "form": "그거 저작권 위반이야 ㅠ 지워줘",
          "original_form": "그거 저작권 위반이야 ㅠ 지워줘",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014311.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014311.1.13",
          "form": "너 그럼 도토리 충전도 해봤어?",
          "original_form": "너 그럼 도토리 충전도 해봤어?",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014311.1.14",
          "form": "도토리묵으로 제 몸에 도토리충전은 해봤는데",
          "original_form": "도토리묵으로 제 몸에 도토리충전은 해봤는데",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014311.1.15",
          "form": "그땐 도토리 살 그런 여력이 안 되어서 ㅠㅠ 못 샀죠 ㅠㅠ",
          "original_form": "그땐 도토리 살 그런 여력이 안 되어서 ㅠㅠ 못 샀죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014311.1.16",
          "form": "나도 내돈주고는 충전 안해보구",
          "original_form": "나도 내돈주고는 충전 안해보구",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014311.1.17",
          "form": "그땐 엄마도 싸이월드를 했거든~~",
          "original_form": "그땐 엄마도 싸이월드를 했거든~~",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014311.1.18",
          "form": "엄마가 충전 해줬었어",
          "original_form": "엄마가 충전 해줬었어",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014311.1.19",
          "form": "노래도 사고 말이야~",
          "original_form": "노래도 사고 말이야~",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014311.1.20",
          "form": "와.. 싸이월드 감성 노래들 생각나네",
          "original_form": "와.. 싸이월드 감성 노래들 생각나네",
          "speaker_id": "1",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014311.1.21",
          "form": "근데 이제는 싸이월드 부활해도 사람들이 잘 이용을 하려나 모르겠네 ㅠㅠ",
          "original_form": "근데 이제는 싸이월드 부활해도 사람들이 잘 이용을 하려나 모르겠네 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014311.1.22",
          "form": "맞아 유튜브에 싸이월드만 쳐도 그때 유행하던 노래 리스트로 브금 나오더라",
          "original_form": "맞아 유튜브에 싸이월드만 쳐도 그때 유행하던 노래 리스트로 브금 나오더라",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014311.1.23",
          "form": "나도 솔직히 요새 감성에는 싸이월드가 안어울린다고 생각해",
          "original_form": "나도 솔직히 요새 감성에는 싸이월드가 안어울린다고 생각해",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014311.1.24",
          "form": "역시 추억은 추억으로 남아야해~",
          "original_form": "역시 추억은 추억으로 남아야해~",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014311.1.25",
          "form": "요즘 보면 오그리토그리...",
          "original_form": "요즘 보면 오그리토그리...",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014311.1.26",
          "form": "완전 오그라들지뭐야",
          "original_form": "완전 오그라들지뭐야",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014311.1.27",
          "form": "맞아 굳이 싸이월드를 부활시키는 이유가 뭔지 모르겠어",
          "original_form": "맞아 굳이 싸이월드를 부활시키는 이유가 뭔지 모르겠어",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014311.1.28",
          "form": "그때 유행해서 지금도 유행할거라고 생각하는 걸까?",
          "original_form": "그때 유행해서 지금도 유행할거라고 생각하는 걸까?",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014311.1.29",
          "form": "다 떳떳하게 살았다고 생각하나봐",
          "original_form": "다 떳떳하게 살았다고 생각하나봐",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014311.1.30",
          "form": "다들 부활했다고 좋아하다가",
          "original_form": "다들 부활했다고 좋아하다가",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014311.1.31",
          "form": "내 주위는 일단 다들 소식듣고 탈퇴했어",
          "original_form": "내 주위는 일단 다들 소식듣고 탈퇴했어",
          "speaker_id": "2",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014311.1.32",
          "form": "휴 ㅠㅠ 나도 탈퇴해야할까봐 부활해서 잠깐 좋았다가",
          "original_form": "휴 ㅠㅠ 나도 탈퇴해야할까봐 부활해서 잠깐 좋았다가",
          "speaker_id": "1",
