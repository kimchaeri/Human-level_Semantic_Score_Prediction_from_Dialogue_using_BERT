        {
          "id": "MDRW2100002060.1.1",
          "form": "동물 키워요?",
          "original_form": "동물 키워요?",
          "speaker_id": "2",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100002060.1.2",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "1",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100002060.1.3",
          "form": "키우고싶은 동물은 ?",
          "original_form": "키우고싶은 동물은 ?",
          "speaker_id": "2",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100002060.1.4",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210819 23:19"
        },
        {
          "id": "MDRW2100002060.1.5",
          "form": "저도요 고양이는요?",
          "original_form": "저도요 고양이는요?",
          "speaker_id": "2",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100002060.1.6",
          "form": "털이 너무 날려서 별로요",
          "original_form": "털이 너무 날려서 별로요",
          "speaker_id": "1",
          "time": "20210819 23:21"
        },
        {
          "id": "MDRW2100002060.1.7",
          "form": "토끼랑 거북이는 ?",
          "original_form": "토끼랑 거북이는 ?",
          "speaker_id": "2",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100002060.1.8",
          "form": "관리가 어려울듯",
          "original_form": "관리가 어려울듯",
          "speaker_id": "1",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002060.1.9",
          "form": "뱀이나 이구아나는?",
          "original_form": "뱀이나 이구아나는?",
          "speaker_id": "2",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002060.1.10",
          "form": "그것도요",
          "original_form": "그것도요",
          "speaker_id": "1",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002060.1.11",
          "form": "무서운건 아니고용? ㅎ.ㅎ",
          "original_form": "무서운건 아니고용? ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002060.1.12",
          "form": "아  그럴수도 ㅋㅋ",
          "original_form": "아  그럴수도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002060.1.13",
          "form": "ㅋㅋㅋㅋ 요즘은 심심해서 뭐라도 키우고싶네요",
