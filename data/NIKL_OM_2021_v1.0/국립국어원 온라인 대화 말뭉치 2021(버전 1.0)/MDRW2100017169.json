        },
        {
          "id": "MDRW2100017169.1.3",
          "form": "이번 여름 휴가 다녀오셨나요?",
          "original_form": "이번 여름 휴가 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017169.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100017169.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100017169.1.6",
          "form": "저는 이번 일요일에 갈 예정이에요",
          "original_form": "저는 이번 일요일에 갈 예정이에요",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100017169.1.7",
          "form": "가장 최근 여행지는 어디에요?",
          "original_form": "가장 최근 여행지는 어디에요?",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100017169.1.8",
          "form": "어디가나요?!!!!",
          "original_form": "어디가나요?!!!!",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100017169.1.9",
          "form": "부산에 가요",
          "original_form": "부산에 가요",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100017169.1.10",
          "form": "호 텔에만 있으려고요",
          "original_form": "호 텔에만 있으려고요",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100017169.1.11",
          "form": "와은",
          "original_form": "와은",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100017169.1.12",
          "form": "어디호텔인가요",
          "original_form": "어디호텔인가요",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100017169.1.13",
          "form": "그랜도조선이요",
          "original_form": "그랜도조선이요",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100017169.1.14",
          "form": "회사 휴양시설 당첨됐어요",
          "original_form": "회사 휴양시설 당첨됐어요",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100017169.1.15",
          "form": "좋네요ㅋㅋㅋㅋ",
          "original_form": "좋네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100017169.1.16",
          "form": "부산가깝나요",
          "original_form": "부산가깝나요",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100017169.1.17",
          "form": "울산살아요",
          "original_form": "울산살아요",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100017169.1.18",
          "form": "가까워요",
          "original_form": "가까워요",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100017169.1.19",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100017169.1.20",
          "form": "좋네요ㅋㅋ ㅋ 가깝고",
          "original_form": "좋네요ㅋㅋ ㅋ 가깝고",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100017169.1.21",
          "form": "네 해운대 맛집이나 돌아다녀볼까해요",
          "original_form": "네 해운대 맛집이나 돌아다녀볼까해요",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100017169.1.22",
          "form": "가장 최근 여행지는 어디에요?",
          "original_form": "가장 최근 여행지는 어디에요?",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100017169.1.23",
          "form": "혼자가나요?",
          "original_form": "혼자가나요?",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100017169.1.24",
          "form": "전 여수ㅋㅋㅋ",
          "original_form": "전 여수ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100017169.1.25",
          "form": "혼자다녀오셨어요?",
          "original_form": "혼자다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210826 23:35"
        },
        {
          "id": "MDRW2100017169.1.26",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100017169.1.27",
          "form": "님은 누구랑가나요?",
          "original_form": "님은 누구랑가나요?",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100017169.1.28",
          "form": "저는 와이프랑 둘이 가요",
          "original_form": "저는 와이프랑 둘이 가요",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100017169.1.29",
          "form": "와이프 컨디션이 안좋아서 호텔에만 있을 계획",
