      },
      "utterance": [
        {
          "id": "MDRW2100005053.1.1",
          "form": "너 무슨 겜 한다 그랬지",
          "original_form": "너 무슨 겜 한다 그랬지",
          "speaker_id": "1",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005053.1.2",
          "form": "나는 롤 배그 다 하긴하지",
          "original_form": "나는 롤 배그 다 하긴하지",
          "speaker_id": "2",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005053.1.3",
          "form": "난 롤이나 배그를 안해봤고...",
          "original_form": "난 롤이나 배그를 안해봤고...",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005053.1.4",
          "form": "거의 모든게임을 해본 것 같은데",
          "original_form": "거의 모든게임을 해본 것 같은데",
          "speaker_id": "2",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005053.1.5",
          "form": "넌 쿠킹덤을 안해보고...",
          "original_form": "넌 쿠킹덤을 안해보고...",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005053.1.6",
          "form": "핸드폰게임 너무 돈을 쓰게 해",
          "original_form": "핸드폰게임 너무 돈을 쓰게 해",
          "speaker_id": "2",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100005053.1.7",
          "form": "난 쿠킹덤 여태 하면서 1원도 안썼다",
          "original_form": "난 쿠킹덤 여태 하면서 1원도 안썼다",
          "speaker_id": "1",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100005053.1.8",
          "form": "흐하하하",
          "original_form": "흐하하하",
          "speaker_id": "1",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100005053.1.9",
          "form": "돈 안쓰는거랑 쓰는거랑 차이 크지 않아?",
          "original_form": "돈 안쓰는거랑 쓰는거랑 차이 크지 않아?",
          "speaker_id": "2",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100005053.1.10",
          "form": "근데 생각보다 쿠킹덤은 안써도 해볼만 함",
          "original_form": "근데 생각보다 쿠킹덤은 안써도 해볼만 함",
          "speaker_id": "1",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100005053.1.11",
          "form": "자주 접속해서 템 얻는게 중요",
          "original_form": "자주 접속해서 템 얻는게 중요",
          "speaker_id": "1",
          "time": "20210821 20:18"
        },
        {
          "id": "MDRW2100005053.1.12",
          "form": "롤 배그는 캐릭터꾸미는 것만 돈이필요해",
          "original_form": "롤 배그는 캐릭터꾸미는 것만 돈이필요해",
          "speaker_id": "2",
          "time": "20210821 20:18"
        },
        {
          "id": "MDRW2100005053.1.13",
          "form": "하지만 나는 썼지",
          "original_form": "하지만 나는 썼지",
          "speaker_id": "2",
          "time": "20210821 20:18"
        },
        {
          "id": "MDRW2100005053.1.14",
          "form": "뭐야 안꾸며도 게임만 잘하면 되자나",
          "original_form": "뭐야 안꾸며도 게임만 잘하면 되자나",
          "speaker_id": "1",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005053.1.15",
          "form": "하지만 캐릭터나 무기가 이쁘면 기분이 좋다고...",
          "original_form": "하지만 캐릭터나 무기가 이쁘면 기분이 좋다고...",
          "speaker_id": "2",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005053.1.16",
          "form": "ㅋㅋㅋㅋㅋ아니 그런거네 남자들도 돈을 써?",
          "original_form": "ㅋㅋㅋㅋㅋ아니 그런거네 남자들도 돈을 써?",
          "speaker_id": "1",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005053.1.17",
          "form": "같이 게임하는 사람들 남녀모두 돈을 쓰지",
          "original_form": "같이 게임하는 사람들 남녀모두 돈을 쓰지",
          "speaker_id": "2",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005053.1.18",
          "form": "뭐 다 쓰는건 아닌데",
          "original_form": "뭐 다 쓰는건 아닌데",
          "speaker_id": "2",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005053.1.19",
          "form": "게임에 돈 쓰는건 너무 아까워",
          "original_form": "게임에 돈 쓰는건 너무 아까워",
          "speaker_id": "1",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005053.1.20",
          "form": "그런 겜은 안할랭",
          "original_form": "그런 겜은 안할랭",
          "speaker_id": "1",
          "time": "20210821 20:22"
        }
      ]
