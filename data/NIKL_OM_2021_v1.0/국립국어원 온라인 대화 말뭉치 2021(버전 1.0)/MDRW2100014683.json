      "utterance": [
        {
          "id": "MDRW2100014683.1.1",
          "form": "안녕하세요ㅎㅎ",
          "original_form": "안녕하세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014683.1.2",
          "form": "결혼하셨ㅓ요?",
          "original_form": "결혼하셨ㅓ요?",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014683.1.3",
          "form": "아뇨 아직 미혼입니다 ㅎ",
          "original_form": "아뇨 아직 미혼입니다 ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014683.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014683.1.5",
          "form": "아하ㅎㅎ 전 결혼했어요ㅎㅎ",
          "original_form": "아하ㅎㅎ 전 결혼했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014683.1.6",
          "form": "아 결혼하셨구나",
          "original_form": "아 결혼하셨구나",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014683.1.7",
          "form": "얼마나 되셨어요?",
          "original_form": "얼마나 되셨어요?",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014683.1.8",
          "form": "ㅎㅎ 이제 결혼3년차에요ㄹㅎ",
          "original_form": "ㅎㅎ 이제 결혼3년차에요ㄹㅎ",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014683.1.9",
          "form": "아이가 한명있구요ㄹㄹ",
          "original_form": "아이가 한명있구요ㄹㄹ",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014683.1.10",
          "form": "결혼하고싶으세요?ㅎ",
          "original_form": "결혼하고싶으세요?ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014683.1.11",
          "form": "저도 결혼할 나이고 여친도 있어서 하려고 하는데",
          "original_form": "저도 결혼할 나이고 여친도 있어서 하려고 하는데",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014683.1.12",
          "form": "어떤가요?",
          "original_form": "어떤가요?",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014683.1.13",
          "form": "결혼해보니 좋은가요?ㅋㅋ",
          "original_form": "결혼해보니 좋은가요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014683.1.14",
          "form": "음....",
          "original_form": "음....",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014683.1.15",
          "form": "음....",
          "original_form": "음....",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014683.1.16",
          "form": "주위에 물어보면 똑같은 방응이실꺼에요ㅎ",
          "original_form": "주위에 물어보면 똑같은 방응이실꺼에요ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014683.1.17",
          "form": "가장 행복할때 하셔요ㄹㄹㄹ",
          "original_form": "가장 행복할때 하셔요ㄹㄹㄹ",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014683.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014683.1.19",
          "form": "대답회피....ㅋㅋㅋ",
          "original_form": "대답회피....ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014683.1.20",
          "form": "ㅎㅎ 해보시면 아실꺼에요ㅎ",
          "original_form": "ㅎㅎ 해보시면 아실꺼에요ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014683.1.21",
          "form": "근데 단하나 행복한건ㄹ",
          "original_form": "근데 단하나 행복한건ㄹ",
          "speaker_id": "1",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014683.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014683.1.23",
          "form": "아이가 생기는거요ㅎ 그건 무엇보다 바꿀수없는 인생의 행복이에요 ㅎ",
          "original_form": "아이가 생기는거요ㅎ 그건 무엇보다 바꿀수없는 인생의 행복이에요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014683.1.24",
          "form": "아이가 아빠를 찾을때 만큼은ㅎㅎ",
          "original_form": "아이가 아빠를 찾을때 만큼은ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014683.1.25",
          "form": "ㅋㅋㅋㅋㅋ 가장 큰 행복?",
          "original_form": "ㅋㅋㅋㅋㅋ 가장 큰 행복?",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014683.1.26",
          "form": "육아에서 벗어나 오로지 나를 위한 시간이주어진다면 더 큰 행복?",
          "original_form": "육아에서 벗어나 오로지 나를 위한 시간이주어진다면 더 큰 행복?",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014683.1.27",
          "form": "글긴해요ㅎㅎㅎ",
          "original_form": "글긴해요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014683.1.28",
          "form": "근데 아이랑 노는게 더 행복해요ㅎㄹ",
          "original_form": "근데 아이랑 노는게 더 행복해요ㅎㄹ",
          "speaker_id": "1",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014683.1.29",
          "form": "아이가 태어나는순간 아이만 바라보게되요ㅎㅎ",
          "original_form": "아이가 태어나는순간 아이만 바라보게되요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014683.1.30",
          "form": "친구들도 다들 그러더라구요",
          "original_form": "친구들도 다들 그러더라구요",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014683.1.31",
          "form": "온통 아이사진으로 도배",
          "original_form": "온통 아이사진으로 도배",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014683.1.32",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014683.1.33",
          "form": "맞아요ㅎㅎ 아이가 인생에 쵝오ㅎ",
          "original_form": "맞아요ㅎㅎ 아이가 인생에 쵝오ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014683.1.34",
          "form": "아이 가지고 싶으세요?",
          "original_form": "아이 가지고 싶으세요?",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014683.1.35",
          "form": "저는 한명만 원해요",
          "original_form": "저는 한명만 원해요",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014683.1.36",
          "form": "두명은 자신없고",
          "original_form": "두명은 자신없고",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014683.1.37",
          "form": "저도 가정을 꾸리고 행복하게 살고싶은 맘이 있거든요 ㅋㅋ 비혼주의자는 아니라서",
          "original_form": "저도 가정을 꾸리고 행복하게 살고싶은 맘이 있거든요 ㅋㅋ 비혼주의자는 아니라서",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014683.1.38",
          "form": "빨리그랬으면좋겟네요ㅎ",
          "original_form": "빨리그랬으면좋겟네요ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014683.1.39",
          "form": "고생하시지만 나중에 행복하실꺼에요ㅎ",
          "original_form": "고생하시지만 나중에 행복하실꺼에요ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014683.1.40",
          "form": "애기는 몇살인가요?",
          "original_form": "애기는 몇살인가요?",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014683.1.41",
          "form": "애기 이제 4살이요ㅎㅎ",
          "original_form": "애기 이제 4살이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014683.1.42",
          "form": "아이가 말을하면 행복은 두배에요ㄹㄹ",
          "original_form": "아이가 말을하면 행복은 두배에요ㄹㄹ",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014683.1.43",
          "form": "얼마나 귀여운지ㅎㅎ",
          "original_form": "얼마나 귀여운지ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014683.1.44",
          "form": "전 아직 아이가 없어서 그 기분을 잘 모르지만",
          "original_form": "전 아직 아이가 없어서 그 기분을 잘 모르지만",
          "speaker_id": "2",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014683.1.45",
          "form": "아빠가 되면 무슨기분일까 생각해본적은 있어요",
          "original_form": "아빠가 되면 무슨기분일까 생각해본적은 있어요",
          "speaker_id": "2",
          "time": "20210825 20:46"
        }
      ]
