      "utterance": [
        {
          "id": "MDRW2100046552.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211029 11:57"
        },
        {
          "id": "MDRW2100046552.1.2",
          "form": "안녕하세요~ 요즘 즐겨하시는 운동 있으신가요?",
          "original_form": "안녕하세요~ 요즘 즐겨하시는 운동 있으신가요?",
          "speaker_id": "2",
          "time": "20211030 00:54"
        },
        {
          "id": "MDRW2100046552.1.3",
          "form": "아니요 없습니다",
          "original_form": "아니요 없습니다",
          "speaker_id": "1",
          "time": "20211029 13:01"
        },
        {
          "id": "MDRW2100046552.1.4",
          "form": "요즘 의욕이 안생기네요",
          "original_form": "요즘 의욕이 안생기네요",
          "speaker_id": "1",
          "time": "20211029 13:01"
        },
        {
          "id": "MDRW2100046552.1.5",
          "form": "ㅜㅜ그럼 취미활동도 없으신가요..",
          "original_form": "ㅜㅜ그럼 취미활동도 없으신가요..",
          "speaker_id": "2",
          "time": "20211029 13:01"
        },
        {
          "id": "MDRW2100046552.1.6",
          "form": "날도 추워져서",
          "original_form": "날도 추워져서",
          "speaker_id": "1",
          "time": "20211029 13:01"
        },
        {
          "id": "MDRW2100046552.1.7",
          "form": "곰이 됐습니다",
          "original_form": "곰이 됐습니다",
          "speaker_id": "1",
          "time": "20211029 13:02"
        },
        {
          "id": "MDRW2100046552.1.8",
          "form": "누워서 유튜브 보고 있어요",
          "original_form": "누워서 유튜브 보고 있어요",
          "speaker_id": "1",
          "time": "20211029 13:02"
        },
        {
          "id": "MDRW2100046552.1.9",
          "form": "퇴근 후 최고의 낙입니다",
          "original_form": "퇴근 후 최고의 낙입니다",
          "speaker_id": "1",
          "time": "20211029 13:02"
        },
        {
          "id": "MDRW2100046552.1.10",
          "form": "저도 날이 추워져서 그런지 운동을 쉬고 있긴해요..!!",
          "original_form": "저도 날이 추워져서 그런지 운동을 쉬고 있긴해요..!!",
          "speaker_id": "2",
          "time": "20211029 13:02"
        },
        {
          "id": "MDRW2100046552.1.11",
          "form": "유튜브는 주로 어떤거 보시나요?",
          "original_form": "유튜브는 주로 어떤거 보시나요?",
          "speaker_id": "2",
          "time": "20211029 13:02"
        },
        {
          "id": "MDRW2100046552.1.12",
          "form": "노래도 들었다가 낚시 방송도 봤다가",
          "original_form": "노래도 들었다가 낚시 방송도 봤다가",
          "speaker_id": "1",
          "time": "20211029 13:03"
        },
        {
          "id": "MDRW2100046552.1.13",
          "form": "흥미로운거 있으면 봅니다",
          "original_form": "흥미로운거 있으면 봅니다",
          "speaker_id": "1",
          "time": "20211029 13:03"
        },
        {
          "id": "MDRW2100046552.1.14",
          "form": "특별히 찾아서 보는것은 없습니다",
          "original_form": "특별히 찾아서 보는것은 없습니다",
          "speaker_id": "1",
          "time": "20211029 13:03"
        },
        {
          "id": "MDRW2100046552.1.15",
          "form": "아 그럼 알고리즘에 뜨는거 위주로 보시는군요!!",
          "original_form": "아 그럼 알고리즘에 뜨는거 위주로 보시는군요!!",
          "speaker_id": "2",
          "time": "20211029 13:04"
        },
        {
          "id": "MDRW2100046552.1.16",
          "form": "낚시 자주 가시나요?",
          "original_form": "낚시 자주 가시나요?",
          "speaker_id": "2",
          "time": "20211029 13:04"
        },
        {
          "id": "MDRW2100046552.1.17",
          "form": "아니요 낚시 추워요",
          "original_form": "아니요 낚시 추워요",
          "speaker_id": "1",
          "time": "20211029 13:04"
        },
        {
          "id": "MDRW2100046552.1.18",
          "form": "소질도 없습니다",
          "original_form": "소질도 없습니다",
          "speaker_id": "1",
          "time": "20211029 13:04"
        },
        {
          "id": "MDRW2100046552.1.19",
          "form": "지렁이도 냄내나서...",
          "original_form": "지렁이도 냄내나서...",
          "speaker_id": "1",
          "time": "20211029 13:05"
        },
        {
          "id": "MDRW2100046552.1.20",
          "form": "낚시방송보신다고 해서 낚시 좋아하시는 줄 알았어요 ㅋㅋ집에서 누워서 보는게 최고긴 하죠!!",
          "original_form": "낚시방송보신다고 해서 낚시 좋아하시는 줄 알았어요 ㅋㅋ집에서 누워서 보는게 최고긴 하죠!!",
          "speaker_id": "2",
          "time": "20211029 13:05"
        },
        {
          "id": "MDRW2100046552.1.21",
          "form": "집이 최고입니다",
          "original_form": "집이 최고입니다",
          "speaker_id": "1",
          "time": "20211029 13:05"
        },
        {
          "id": "MDRW2100046552.1.22",
          "form": "오늘 불금인데 오늘도 집에서 쉴 계획이신가요?",
          "original_form": "오늘 불금인데 오늘도 집에서 쉴 계획이신가요?",
          "speaker_id": "2",
          "time": "20211029 13:06"
        },
        {
          "id": "MDRW2100046552.1.23",
          "form": "네 오늘도 퇴근 후 바로 집으로 갑니다",
          "original_form": "네 오늘도 퇴근 후 바로 집으로 갑니다",
          "speaker_id": "1",
          "time": "20211029 13:06"
        },
        {
          "id": "MDRW2100046552.1.24",
          "form": "주말에도 집입니다",
          "original_form": "주말에도 집입니다",
          "speaker_id": "1",
          "time": "20211029 13:06"
        },
        {
          "id": "MDRW2100046552.1.25",
          "form": "그래서 마누라가 싫어하네요ㅜ",
          "original_form": "그래서 마누라가 싫어하네요ㅜ",
          "speaker_id": "1",
          "time": "20211029 13:07"
        },
        {
          "id": "MDRW2100046552.1.26",
          "form": "ㅋㅋㅋ날씨가 조금이라도 괜찮을때 가족분들이랑 주말에는 드라이브하면서 기분전환하는게 좋을 것 같아요!!!",
          "original_form": "ㅋㅋㅋ날씨가 조금이라도 괜찮을때 가족분들이랑 주말에는 드라이브하면서 기분전환하는게 좋을 것 같아요!!!",
          "speaker_id": "2",
          "time": "20211029 13:08"
        },
        {
          "id": "MDRW2100046552.1.27",
          "form": "오늘 퇴근 때 까지 화이팅하시고 푹 쉬세요 :)",
