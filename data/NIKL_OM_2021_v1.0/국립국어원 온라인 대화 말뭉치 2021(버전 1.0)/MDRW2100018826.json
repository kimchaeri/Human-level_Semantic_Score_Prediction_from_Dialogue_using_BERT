        {
          "id": "MDRW2100018826.1.1",
          "form": "운동 하시나요.?",
          "original_form": "운동 하시나요.?",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018826.1.2",
          "form": "네 하고 있어요!",
          "original_form": "네 하고 있어요!",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018826.1.3",
          "form": "운동 하시나요?",
          "original_form": "운동 하시나요?",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018826.1.4",
          "form": "아뇨 지금은 하지 않아요ㅠ",
          "original_form": "아뇨 지금은 하지 않아요ㅠ",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018826.1.5",
          "form": "무슨 운동 하시나요?",
          "original_form": "무슨 운동 하시나요?",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018826.1.6",
          "form": "걷기랑 스쿼트 하고 있어요",
          "original_form": "걷기랑 스쿼트 하고 있어요",
          "speaker_id": "2",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018826.1.7",
          "form": "비가와서 쉽진 않네요ㅠ",
          "original_form": "비가와서 쉽진 않네요ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018826.1.8",
          "form": "주변에 산책로나 운동할 공간이 있나봐요!",
          "original_form": "주변에 산책로나 운동할 공간이 있나봐요!",
          "speaker_id": "1",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018826.1.9",
          "form": "네! 큰 공원이 있는데 산 가는길로 쭉 걸을 수 있는곳이 있어요!",
          "original_form": "네! 큰 공원이 있는데 산 가는길로 쭉 걸을 수 있는곳이 있어요!",
          "speaker_id": "2",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100018826.1.10",
          "form": "그럼 운동하러 가기 좋을 것 같아요",
          "original_form": "그럼 운동하러 가기 좋을 것 같아요",
          "speaker_id": "1",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100018826.1.11",
          "form": "저는 차타고 10분은 가야해서 ㅠ",
          "original_form": "저는 차타고 10분은 가야해서 ㅠ",
          "speaker_id": "1",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100018826.1.12",
          "form": "헉ㅠ 꽤 머네요",
          "original_form": "헉ㅠ 꽤 머네요",
          "speaker_id": "2",
          "time": "20210827 13:19"
        },
        {
          "id": "MDRW2100018826.1.13",
          "form": "운동하는곳은 가까운곳에 있는게 좋네요",
          "original_form": "운동하는곳은 가까운곳에 있는게 좋네요",
          "speaker_id": "2",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018826.1.14",
          "form": "맞아요 그래야 더 자주 갈 수도 있고",
          "original_form": "맞아요 그래야 더 자주 갈 수도 있고",
          "speaker_id": "1",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100018826.1.15",
          "form": "걷기는 얼마나하시나요?",
          "original_form": "걷기는 얼마나하시나요?",
          "speaker_id": "1",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100018826.1.16",
          "form": "가볍게 한시간정도 하고 있어요",
          "original_form": "가볍게 한시간정도 하고 있어요",
          "speaker_id": "2",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018826.1.17",
          "form": "한시간이 가볍다니!!",
          "original_form": "한시간이 가볍다니!!",
          "speaker_id": "1",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100018826.1.18",
          "form": "대단하세요",
          "original_form": "대단하세요",
          "speaker_id": "1",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100018826.1.19",
          "form": "약간의 허세를 가미했습니다ㅋㅋ",
          "original_form": "약간의 허세를 가미했습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018826.1.20",
          "form": "그래도 스스로 잘지키는중이라 뿌듯해요",
          "original_form": "그래도 스스로 잘지키는중이라 뿌듯해요",
          "speaker_id": "2",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018826.1.21",
          "form": "저는 런데이 해봣는데 4주차까지가 최대더라구요ㅠ",
