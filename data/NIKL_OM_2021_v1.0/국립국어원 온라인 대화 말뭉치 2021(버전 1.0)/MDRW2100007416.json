      "utterance": [
        {
          "id": "MDRW2100007416.1.1",
          "form": "오늘 맛있는거 드셨나요?",
          "original_form": "오늘 맛있는거 드셨나요?",
          "speaker_id": "1",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007416.1.2",
          "form": "오늘 삼겹살 먹었어요!",
          "original_form": "오늘 삼겹살 먹었어요!",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007416.1.3",
          "form": "비오는날 삼겹살",
          "original_form": "비오는날 삼겹살",
          "speaker_id": "1",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007416.1.4",
          "form": "너무 좋은 메뉴 선택입니다",
          "original_form": "너무 좋은 메뉴 선택입니다",
          "speaker_id": "1",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007416.1.5",
          "form": "좋죠좋죠",
          "original_form": "좋죠좋죠",
          "speaker_id": "2",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007416.1.6",
          "form": "맛있는 거 드셨어요",
          "original_form": "맛있는 거 드셨어요",
          "speaker_id": "2",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007416.1.7",
          "form": "저는 갈비에 김치찌개 먹었습니당",
          "original_form": "저는 갈비에 김치찌개 먹었습니당",
          "speaker_id": "1",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007416.1.8",
          "form": "역시 고기를 먹어줘야죠",
          "original_form": "역시 고기를 먹어줘야죠",
          "speaker_id": "1",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007416.1.9",
          "form": "왓시",
          "original_form": "왓시",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007416.1.10",
          "form": "김치찌개 제 소울푸드예요",
          "original_form": "김치찌개 제 소울푸드예요",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007416.1.11",
          "form": "ㅋㅋㅋ 김치찌개 뭐 좋아하세요?",
          "original_form": "ㅋㅋㅋ 김치찌개 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007416.1.12",
          "form": "고기에 김치찌개라니 완벽하다",
          "original_form": "고기에 김치찌개라니 완벽하다",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007416.1.13",
          "form": "재료요?",
          "original_form": "재료요?",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007416.1.14",
          "form": "참치 돼지 스팸?",
          "original_form": "참치 돼지 스팸?",
          "speaker_id": "1",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007416.1.15",
          "form": "네네 ㅋㅋ",
          "original_form": "네네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007416.1.16",
          "form": "전 참치김치찌개는 별로 안 좋아해요",
          "original_form": "전 참치김치찌개는 별로 안 좋아해요",
          "speaker_id": "1",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007416.1.17",
          "form": "전 돼지랑 참치",
          "original_form": "전 돼지랑 참치",
          "speaker_id": "2",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007416.1.18",
          "form": "저는 햄류 들어간 게 좀 별로예요ㅜㅜ",
          "original_form": "저는 햄류 들어간 게 좀 별로예요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007416.1.19",
          "form": "헉 아쉬워요",
          "original_form": "헉 아쉬워요",
          "speaker_id": "1",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007416.1.20",
          "form": "전 스팸이 제일 좋은데ㅜ",
          "original_form": "전 스팸이 제일 좋은데ㅜ",
          "speaker_id": "1",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007416.1.21",
          "form": "돼지고기도 좋아하지만요!",
          "original_form": "돼지고기도 좋아하지만요!",
          "speaker_id": "1",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007416.1.22",
          "form": "그럼 부대찌개도 좋아하시겠네요",
          "original_form": "그럼 부대찌개도 좋아하시겠네요",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007416.1.23",
          "form": "부대찌개 완전 좋아하죠 ㅋㅋㅋ",
          "original_form": "부대찌개 완전 좋아하죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007416.1.24",
          "form": "부대찌개에 그 통조림 콩 들어간 것도 너무 맛있어요",
          "original_form": "부대찌개에 그 통조림 콩 들어간 것도 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007416.1.25",
          "form": "오 맞아요 그 콩 맛있죠ㅋㅋㅋ",
          "original_form": "오 맞아요 그 콩 맛있죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007416.1.26",
          "form": "네 ㅋㅋㅋ 입안에서 스무스하게 으깨지는 맛 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ 입안에서 스무스하게 으깨지는 맛 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:06"
        }
      ]
    }
