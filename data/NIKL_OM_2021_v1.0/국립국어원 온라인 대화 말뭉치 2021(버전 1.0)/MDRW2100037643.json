      "utterance": [
        {
          "id": "MDRW2100037643.1.1",
          "form": "맛집 많이 아세요?",
          "original_form": "맛집 많이 아세요?",
          "speaker_id": "1",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100037643.1.2",
          "form": "아니요 그냥 주워듣는 정도로만 ㅋㅋㅋ",
          "original_form": "아니요 그냥 주워듣는 정도로만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100037643.1.3",
          "form": "많이 가시나요",
          "original_form": "많이 가시나요",
          "speaker_id": "2",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100037643.1.4",
          "form": "저도 즉흥적으로 가요",
          "original_form": "저도 즉흥적으로 가요",
          "speaker_id": "1",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100037643.1.5",
          "form": "ㅎㅎㅎ 저도 후기 좀 찾아보고 괜찮으면 가요 ㅋㅋ",
          "original_form": "ㅎㅎㅎ 저도 후기 좀 찾아보고 괜찮으면 가요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100037643.1.6",
          "form": "반은 성공",
          "original_form": "반은 성공",
          "speaker_id": "1",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100037643.1.7",
          "form": "반은 실패ㅋ",
          "original_form": "반은 실패ㅋ",
          "speaker_id": "1",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100037643.1.8",
          "form": "맞아요 ㅋㅋㅋ 아저씨들이 많은 집이 맛집이기도 하더라고요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ 아저씨들이 많은 집이 맛집이기도 하더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100037643.1.9",
          "form": "아저씨들ㅋㅋㅋ",
          "original_form": "아저씨들ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:20"
        },
        {
          "id": "MDRW2100037643.1.10",
          "form": "기사식당도요",
          "original_form": "기사식당도요",
          "speaker_id": "1",
          "time": "20210901 14:20"
        },
        {
          "id": "MDRW2100037643.1.11",
          "form": "기사식당 좋죠 ㅋㅋㅋ 밥도 많이 먹어도 되고 ㅋㅋㅋ",
          "original_form": "기사식당 좋죠 ㅋㅋㅋ 밥도 많이 먹어도 되고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:25"
        },
        {
          "id": "MDRW2100037643.1.12",
          "form": "한식부페 이런거 좋아하는데 은근 찾기 어려워요..",
          "original_form": "한식부페 이런거 좋아하는데 은근 찾기 어려워요..",
          "speaker_id": "2",
          "time": "20210901 14:25"
        },
        {
          "id": "MDRW2100037643.1.13",
          "form": "한식이 별로 없더라구여",
          "original_form": "한식이 별로 없더라구여",
          "speaker_id": "1",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100037643.1.14",
          "form": "그쵸.. 요새 맛있는 한식집 찾기가 은근 어려워요 백반집도 거의 없는거 같고",
          "original_form": "그쵸.. 요새 맛있는 한식집 찾기가 은근 어려워요 백반집도 거의 없는거 같고",
          "speaker_id": "2",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100037643.1.15",
          "form": "맞어요",
          "original_form": "맞어요",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100037643.1.16",
          "form": "슬퍼요",
          "original_form": "슬퍼요",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100037643.1.17",
          "form": "먹어도 먹어도 끝이없어요",
          "original_form": "먹어도 먹어도 끝이없어요",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100037643.1.18",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100037643.1.19",
          "form": "뭘 제일 좋아하세요 음식?",
          "original_form": "뭘 제일 좋아하세요 음식?",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100037643.1.20",
          "form": "전밀가루여",
          "original_form": "전밀가루여",
          "speaker_id": "1",
