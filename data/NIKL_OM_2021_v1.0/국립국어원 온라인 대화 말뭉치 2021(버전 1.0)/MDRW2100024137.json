        {
          "id": "MDRW2100024137.1.1",
          "form": "요리는 잘하시나요?",
          "original_form": "요리는 잘하시나요?",
          "speaker_id": "1",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024137.1.2",
          "form": "요리 잘합니다",
          "original_form": "요리 잘합니다",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100024137.1.3",
          "form": "볶음밥 잘해요",
          "original_form": "볶음밥 잘해요",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024137.1.4",
          "form": "아 그리고 삼겹살 볶음?",
          "original_form": "아 그리고 삼겹살 볶음?",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024137.1.5",
          "form": "어떤 음식 좋아하세요",
          "original_form": "어떤 음식 좋아하세요",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024137.1.6",
          "form": "똑똑똑 집 앞이에오",
          "original_form": "똑똑똑 집 앞이에오",
          "speaker_id": "1",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024137.1.7",
          "form": "문 열어주세요",
          "original_form": "문 열어주세요",
          "speaker_id": "1",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024137.1.8",
          "form": "네?당근?",
          "original_form": "네?당근?",
          "speaker_id": "2",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024137.1.9",
          "form": "ㅎㅎ 삼겹살 볶음 준비됐쥬?",
          "original_form": "ㅎㅎ 삼겹살 볶음 준비됐쥬?",
          "speaker_id": "1",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100024137.1.10",
          "form": "삼겹살만들고 오세요 ㅋㅋ",
          "original_form": "삼겹살만들고 오세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024137.1.11",
          "form": "ㅜㅇㅜ 왜차별하세용",
          "original_form": "ㅜㅇㅜ 왜차별하세용",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024137.1.12",
          "form": "저도 입인데",
          "original_form": "저도 입인데",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024137.1.13",
          "form": "아 그럼 제가 지금 정육점 문연지 확인좀할게요",
          "original_form": "아 그럼 제가 지금 정육점 문연지 확인좀할게요",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100024137.1.14",
          "form": "아녜요ㅎ 저 손에 삼겹살 들었어용:D",
          "original_form": "아녜요ㅎ 저 손에 삼겹살 들었어용:D",
          "speaker_id": "1",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100024137.1.15",
          "form": "혹시 애완동물 기를 생각 없으신가요",
          "original_form": "혹시 애완동물 기를 생각 없으신가요",
          "speaker_id": "1",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100024137.1.16",
          "form": "네 아들 한명있는데 힘드네요",
          "original_form": "네 아들 한명있는데 힘드네요",
          "speaker_id": "2",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100024137.1.17",
          "form": "말잘 듣고 음식만 잘 주면 되는 인간 하나인데ㅜ",
          "original_form": "말잘 듣고 음식만 잘 주면 되는 인간 하나인데ㅜ",
          "speaker_id": "1",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100024137.1.18",
          "form": "공간도 한평만 있음 돼요",
          "original_form": "공간도 한평만 있음 돼요",
          "speaker_id": "1",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100024137.1.19",
          "form": "아 남자는 아들로 족합니다 ㅋㅋ",
          "original_form": "아 남자는 아들로 족합니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100024137.1.20",
          "form": "ㅜㅇㅜ 성차별하면 신고 들어갑니당",
          "original_form": "ㅜㅇㅜ 성차별하면 신고 들어갑니당",
          "speaker_id": "1",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024137.1.21",
          "form": "아 지송",
          "original_form": "아 지송",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
