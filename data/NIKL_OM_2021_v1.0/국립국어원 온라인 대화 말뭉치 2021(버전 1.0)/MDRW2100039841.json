          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100039841.1.11",
          "form": "싫어하는 영화는요",
          "original_form": "싫어하는 영화는요",
          "speaker_id": "1",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100039841.1.12",
          "form": "절 아세요 ?",
          "original_form": "절 아세요 ?",
          "speaker_id": "2",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100039841.1.13",
          "form": "딱히 기억이 나는게 없는데 ,,",
          "original_form": "딱히 기억이 나는게 없는데 ,,",
          "speaker_id": "2",
          "time": "20210907 14:42"
        },
        {
          "id": "MDRW2100039841.1.14",
          "form": "저희 대화 좀 해서 알죠",
          "original_form": "저희 대화 좀 해서 알죠",
          "speaker_id": "1",
          "time": "20210907 14:42"
        },
        {
          "id": "MDRW2100039841.1.15",
          "form": "마블영화 안좋아해요",
          "original_form": "마블영화 안좋아해요",
          "speaker_id": "2",
          "time": "20210907 14:42"
        },
        {
          "id": "MDRW2100039841.1.16",
          "form": "세계관 어려워서요",
          "original_form": "세계관 어려워서요",
          "speaker_id": "2",
          "time": "20210907 14:42"
        },
        {
          "id": "MDRW2100039841.1.17",
          "form": "그렇군요 내적친분이 좀 쌓이셨나봐요",
          "original_form": "그렇군요 내적친분이 좀 쌓이셨나봐요",
          "speaker_id": "2",
          "time": "20210907 14:42"
        },
        {
          "id": "MDRW2100039841.1.18",
          "form": "님은 좋아하는 영화가 뭔가요?",
          "original_form": "님은 좋아하는 영화가 뭔가요?",
          "speaker_id": "2",
          "time": "20210907 14:42"
