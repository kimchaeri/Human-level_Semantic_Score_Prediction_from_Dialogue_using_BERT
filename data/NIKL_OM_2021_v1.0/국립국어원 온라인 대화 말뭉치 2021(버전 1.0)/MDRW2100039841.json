        {
          "id": "MDRW2100039841.1.19",
          "form": "전",
          "original_form": "전",
          "speaker_id": "1",
          "time": "20210907 14:42"
        },
        {
          "id": "MDRW2100039841.1.20",
          "form": "벌sae 야구소녀 윤희에게",
          "original_form": "벌sae 야구소녀 윤희에게",
          "speaker_id": "1",
          "time": "20210907 14:42"
