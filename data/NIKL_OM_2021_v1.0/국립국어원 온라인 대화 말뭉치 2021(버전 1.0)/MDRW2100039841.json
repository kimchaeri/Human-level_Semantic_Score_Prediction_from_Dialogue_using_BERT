        {
          "id": "MDRW2100039841.1.21",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210907 14:43"
        },
        {
          "id": "MDRW2100039841.1.22",
          "form": "오 하나만 꼽자면요?",
          "original_form": "오 하나만 꼽자면요?",
          "speaker_id": "2",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100039841.1.23",
          "form": "하나도 모르죠?",
          "original_form": "하나도 모르죠?",
          "speaker_id": "1",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100039841.1.24",
          "form": "음 못 골라요",
          "original_form": "음 못 골라요",
          "speaker_id": "1",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100039841.1.25",
          "form": "죽기 전에 딱 하나 볼 수 있다면",
          "original_form": "죽기 전에 딱 하나 볼 수 있다면",
          "speaker_id": "1",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100039841.1.26",
          "form": "벌sae 아님 야구소녀",
          "original_form": "벌sae 아님 야구소녀",
          "speaker_id": "1",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100039841.1.27",
          "form": "이주영 사랑해",
          "original_form": "이주영 사랑해",
          "speaker_id": "1",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100039841.1.28",
          "form": "하나만 골라주세요",
          "original_form": "하나만 골라주세요",
          "speaker_id": "2",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100039841.1.29",
          "form": "왜요",
