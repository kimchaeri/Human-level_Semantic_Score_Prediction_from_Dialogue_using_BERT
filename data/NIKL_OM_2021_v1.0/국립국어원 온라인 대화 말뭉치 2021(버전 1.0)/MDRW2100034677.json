          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034677.1.4",
          "form": "그걸먹고싶다는거야",
          "original_form": "그걸먹고싶다는거야",
          "speaker_id": "1",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034677.1.5",
          "form": "피자, 치킨 둘다 먹고싶다 이거지",
          "original_form": "피자, 치킨 둘다 먹고싶다 이거지",
          "speaker_id": "1",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100034677.1.6",
          "form": "구거 맛있다^^",
          "original_form": "구거 맛있다^^",
          "speaker_id": "2",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100034677.1.7",
          "form": "여긴 없잖아??",
          "original_form": "여긴 없잖아??",
