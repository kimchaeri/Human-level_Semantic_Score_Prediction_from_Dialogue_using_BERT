      "utterance": [
        {
          "id": "MDRW2100033152.1.1",
          "form": "헐~ 여행가고싶다",
          "original_form": "헐~ 여행가고싶다",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100033152.1.2",
          "form": "미투",
          "original_form": "미투",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100033152.1.3",
          "form": "우리 일본간게 엊그제같은데,",
          "original_form": "우리 일본간게 엊그제같은데,",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100033152.1.4",
          "form": "같이 첸백시오빠들보면서",
          "original_form": "같이 첸백시오빠들보면서",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100033152.1.5",
          "form": "맞아!!!!",
          "original_form": "맞아!!!!",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100033152.1.6",
          "form": "감동을한몸으로.",
          "original_form": "감동을한몸으로.",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100033152.1.7",
          "form": "일본 진짜~ 넘넘넘 가고 싶어",
          "original_form": "일본 진짜~ 넘넘넘 가고 싶어",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100033152.1.8",
          "form": "어디든 떠나고 싶어",
          "original_form": "어디든 떠나고 싶어",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100033152.1.9",
          "form": "나도.",
          "original_form": "나도.",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100033152.1.10",
          "form": "나진짜코로나만끝나면",
          "original_form": "나진짜코로나만끝나면",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100033152.1.11",
          "form": "우리나라 말고 해외로",
          "original_form": "우리나라 말고 해외로",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100033152.1.12",
          "form": "멋지게 여권들고떠나준다",
          "original_form": "멋지게 여권들고떠나준다",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100033152.1.13",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100033152.1.14",
          "form": "도피할래",
          "original_form": "도피할래",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100033152.1.15",
          "form": "난제주도도 좋은듯",
          "original_form": "난제주도도 좋은듯",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100033152.1.16",
          "form": "제주도 좋지",
          "original_form": "제주도 좋지",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100033152.1.17",
          "form": "타지느낌이있어",
          "original_form": "타지느낌이있어",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100033152.1.18",
          "form": "맞아마자",
          "original_form": "맞아마자",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100033152.1.19",
          "form": "야자수가 있어서 그런지",
          "original_form": "야자수가 있어서 그런지",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100033152.1.20",
          "form": "아맞아",
          "original_form": "아맞아",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100033152.1.21",
          "form": "유럽도 가고 싶고",
          "original_form": "유럽도 가고 싶고",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100033152.1.22",
          "form": "거기 열대?임 거의??",
          "original_form": "거기 열대?임 거의??",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100033152.1.23",
          "form": "유럽좋지..",
          "original_form": "유럽좋지..",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100033152.1.24",
          "form": "돈만잇으면 뭐든지",
          "original_form": "돈만잇으면 뭐든지",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100033152.1.25",
          "form": "나 요즘 세계테마기행",
          "original_form": "나 요즘 세계테마기행",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100033152.1.26",
          "form": "욘나 봄",
          "original_form": "욘나 봄",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100033152.1.27",
          "form": "매저녁마다",
          "original_form": "매저녁마다",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100033152.1.28",
          "form": "테마기행??",
          "original_form": "테마기행??",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100033152.1.29",
          "form": "세계테마기행 애청자임",
          "original_form": "세계테마기행 애청자임",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100033152.1.30",
          "form": "나는 유튜브로 여행브이로그봐",
          "original_form": "나는 유튜브로 여행브이로그봐",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100033152.1.31",
          "form": "이비에스에서 하는 거",
          "original_form": "이비에스에서 하는 거",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100033152.1.32",
          "form": "아 그 므ㅓ냐 무슨 보틀? 여행 유튜버",
          "original_form": "아 그 므ㅓ냐 무슨 보틀? 여행 유튜버",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100033152.1.33",
          "form": "교육방송이네",
          "original_form": "교육방송이네",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100033152.1.34",
          "form": "아 빠니보틀",
          "original_form": "아 빠니보틀",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100033152.1.35",
          "form": "그사람 인도가는거 개열심히봄 나",
          "original_form": "그사람 인도가는거 개열심히봄 나",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100033152.1.36",
          "form": "그 사람은 지금 여행을 다니는 거야?아니면",
          "original_form": "그 사람은 지금 여행을 다니는 거야?아니면",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100033152.1.37",
          "form": "옛날 용상이야",
          "original_form": "옛날 용상이야",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100033152.1.38",
          "form": "영상",
          "original_form": "영상",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100033152.1.39",
          "form": "나 코로나전에봐서",
          "original_form": "나 코로나전에봐서",
          "speaker_id": "2",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100033152.1.40",
          "form": "요즘은 잘모르겠다??",
          "original_form": "요즘은 잘모르겠다??",
          "speaker_id": "2",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100033152.1.41",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100033152.1.42",
          "form": "막 나 며칠 전에 보니까 미국 가고",
          "original_form": "막 나 며칠 전에 보니까 미국 가고",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100033152.1.43",
          "form": "그런 거 같아서",
          "original_form": "그런 거 같아서",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100033152.1.44",
          "form": "진짜갔을지도",
          "original_form": "진짜갔을지도",
          "speaker_id": "2",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100033152.1.45",
          "form": "호주...가고 싶다...",
          "original_form": "호주...가고 싶다...",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100033152.1.46",
          "form": "나도,",
          "original_form": "나도,",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100033152.1.47",
          "form": "어디든.",
          "original_form": "어디든.",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100033152.1.48",
          "form": "대자연에 밀리고 싶다...",
          "original_form": "대자연에 밀리고 싶다...",
          "speaker_id": "1",
          "time": "20210906 17:46"
        }
      ]
    }
