        {
          "id": "MDRW2100027211.1.1",
          "form": "네 중고거래해요",
          "original_form": "네 중고거래해요",
          "speaker_id": "2",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027211.1.2",
          "form": "하세요?",
          "original_form": "하세요?",
          "speaker_id": "2",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027211.1.3",
          "form": "네 당근이요",
          "original_form": "네 당근이요",
          "speaker_id": "1",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027211.1.4",
          "form": "저두요ㅋ",
          "original_form": "저두요ㅋ",
          "speaker_id": "2",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027211.1.5",
          "form": "당근이 제일편해요",
          "original_form": "당근이 제일편해요",
          "speaker_id": "2",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027211.1.6",
          "form": "동네라서",
          "original_form": "동네라서",
          "speaker_id": "1",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027211.1.7",
          "form": "네 가깝고요ㅋ",
          "original_form": "네 가깝고요ㅋ",
          "speaker_id": "2",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027211.1.8",
          "form": "빠르조",
          "original_form": "빠르조",
          "speaker_id": "1",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027211.1.9",
          "form": "네 돈도저렴하고요ㅋㅋ",
          "original_form": "네 돈도저렴하고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027211.1.10",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027211.1.11",
          "form": "네ㅋㅋ 당근에 팔거있는지 또 봐야겠네요ㅋ",
          "original_form": "네ㅋㅋ 당근에 팔거있는지 또 봐야겠네요ㅋ",
          "speaker_id": "2",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027211.1.12",
          "form": "찾는것이 일이네요",
          "original_form": "찾는것이 일이네요",
          "speaker_id": "1",
          "time": "20210902 11:09"
        },
        {
          "id": "MDRW2100027211.1.13",
          "form": "네 정리하다보면 나와요ㅋ",
          "original_form": "네 정리하다보면 나와요ㅋ",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100027211.1.14",
          "form": "이것저것 팔리더라고요.ㅋ",
          "original_form": "이것저것 팔리더라고요.ㅋ",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100027211.1.15",
          "form": "ㅋㅋ 당근에서 뭐파신거있으세요?",
          "original_form": "ㅋㅋ 당근에서 뭐파신거있으세요?",
          "speaker_id": "2",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100027211.1.16",
          "form": "공기청정기요",
          "original_form": "공기청정기요",
          "speaker_id": "1",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027211.1.17",
          "form": "공기청정기 파셨군요ㅋㅋ오 수입좀 있었겠어요ㅋ",
          "original_form": "공기청정기 파셨군요ㅋㅋ오 수입좀 있었겠어요ㅋ",
