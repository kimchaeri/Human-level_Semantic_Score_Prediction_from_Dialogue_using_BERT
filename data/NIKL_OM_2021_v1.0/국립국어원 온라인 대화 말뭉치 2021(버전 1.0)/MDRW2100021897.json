      "utterance": [
        {
          "id": "MDRW2100021897.1.1",
          "form": "국내여행 위주로 다니셨군여",
          "original_form": "국내여행 위주로 다니셨군여",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021897.1.2",
          "form": "네네! 해외는 많으면 일년에 두번??ㅎㅎ",
          "original_form": "네네! 해외는 많으면 일년에 두번??ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021897.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021897.1.4",
          "form": "전 국내는 많이 안가고 해외여행에 맛들려서 해외다니다가 코로나터져서ㅜㅜ",
          "original_form": "전 국내는 많이 안가고 해외여행에 맛들려서 해외다니다가 코로나터져서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021897.1.5",
          "form": "ㅜㅜㅜㅜㅜㅜ맞아요 코로나때문에...",
          "original_form": "ㅜㅜㅜㅜㅜㅜ맞아요 코로나때문에...",
          "speaker_id": "2",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100021897.1.6",
          "form": "코로나 전에 가보신 해외여행지는",
          "original_form": "코로나 전에 가보신 해외여행지는",
          "speaker_id": "2",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100021897.1.7",
          "form": "어디어디 가보셨어요?ㅎㅎ",
          "original_form": "어디어디 가보셨어요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100021897.1.8",
          "form": "일본 홍콩 영국 프랑스 미국  이정도....",
          "original_form": "일본 홍콩 영국 프랑스 미국  이정도....",
          "speaker_id": "1",
          "time": "20210829 01:29"
        },
        {
          "id": "MDRW2100021897.1.9",
          "form": "헉 비싼 여행지 위주로 많이가셧네요!!!",
          "original_form": "헉 비싼 여행지 위주로 많이가셧네요!!!",
          "speaker_id": "2",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100021897.1.10",
          "form": "프랑스는 어떠셬ㅅ어요 ㅎㅎㅎ 젤 궁금해요",
          "original_form": "프랑스는 어떠셬ㅅ어요 ㅎㅎㅎ 젤 궁금해요",
          "speaker_id": "2",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100021897.1.11",
          "form": "프랑스 진짜 좋았어요",
          "original_form": "프랑스 진짜 좋았어요",
          "speaker_id": "1",
          "time": "20210829 10:27"
        },
        {
          "id": "MDRW2100021897.1.12",
          "form": "또 가고싶을만큼",
          "original_form": "또 가고싶을만큼",
          "speaker_id": "1",
          "time": "20210829 10:27"
        },
        {
          "id": "MDRW2100021897.1.13",
          "form": "영국은 그냥저냥",
          "original_form": "영국은 그냥저냥",
          "speaker_id": "1",
          "time": "20210829 10:27"
        },
        {
          "id": "MDRW2100021897.1.14",
          "form": "프랑스에 볼게 많아가지고",
          "original_form": "프랑스에 볼게 많아가지고",
          "speaker_id": "1",
          "time": "20210829 10:27"
        },
        {
          "id": "MDRW2100021897.1.15",
          "form": "여기저기 다니다보면 시간 순삭",
          "original_form": "여기저기 다니다보면 시간 순삭",
          "speaker_id": "1",
          "time": "20210829 10:28"
        },
        {
          "id": "MDRW2100021897.1.16",
          "form": "아아ㅎㅎㅎ그쵸그쵸",
          "original_form": "아아ㅎㅎㅎ그쵸그쵸",
          "speaker_id": "2",
          "time": "20210829 15:36"
        },
        {
          "id": "MDRW2100021897.1.17",
          "form": "프랑스는 그냥 생각해도 볼게 엄청 많을거같아요ㅠㅠㅠ",
          "original_form": "프랑스는 그냥 생각해도 볼게 엄청 많을거같아요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 15:36"
        },
        {
          "id": "MDRW2100021897.1.18",
          "form": "파리올림픽 얼마나 성대하게 열릴지!!",
          "original_form": "파리올림픽 얼마나 성대하게 열릴지!!",
          "speaker_id": "2",
          "time": "20210829 15:36"
        },
        {
          "id": "MDRW2100021897.1.19",
          "form": "아 다음이 파리군요?",
          "original_form": "아 다음이 파리군요?",
          "speaker_id": "1",
          "time": "20210829 15:38"
        },
        {
          "id": "MDRW2100021897.1.20",
          "form": "네!!ㅋㅋㅋ",
          "original_form": "네!!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:38"
        },
        {
          "id": "MDRW2100021897.1.21",
          "form": "에펠탑 앞에 경기장 만들고",
          "original_form": "에펠탑 앞에 경기장 만들고",
          "speaker_id": "2",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100021897.1.22",
          "form": "뭐 그런식으로",
          "original_form": "뭐 그런식으로",
          "speaker_id": "2",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100021897.1.23",
          "form": "파리 유적지 활용할건가보더라구요!",
          "original_form": "파리 유적지 활용할건가보더라구요!",
          "speaker_id": "2",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100021897.1.24",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100021897.1.25",
          "form": "그땐 코로나 괜찮으려나요",
          "original_form": "그땐 코로나 괜찮으려나요",
          "speaker_id": "1",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100021897.1.26",
          "form": "그렇지않을까요??ㅠㅠㅠ모르겠네요",
          "original_form": "그렇지않을까요??ㅠㅠㅠ모르겠네요",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100021897.1.27",
          "form": "백신 80프로 이상 맞으면 괜찮아질건가보더라구요",
          "original_form": "백신 80프로 이상 맞으면 괜찮아질건가보더라구요",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100021897.1.28",
          "form": "마스크는 계속 써야하니ㅜㅜ",
          "original_form": "마스크는 계속 써야하니ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 19:49"
        },
        {
          "id": "MDRW2100021897.1.29",
          "form": "앗 그러려나요ㅠㅠ",
          "original_form": "앗 그러려나요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100021897.1.30",
          "form": "이놈의 마스크 대체 언제까지 써야할지ㅠㅠㅠㅠ",
          "original_form": "이놈의 마스크 대체 언제까지 써야할지ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100021897.1.31",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100021897.1.32",
          "form": "마스크 사는것도 귀찮고",
          "original_form": "마스크 사는것도 귀찮고",
          "speaker_id": "1",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100021897.1.33",
          "form": "맞아요 마스크 사는것도 귀찮아요 ㅋㅋㅋ",
          "original_form": "맞아요 마스크 사는것도 귀찮아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100021897.1.34",
          "form": "요즘은 마스크 선택지도 넘 많아서 결정하는데도 힘들구",
          "original_form": "요즘은 마스크 선택지도 넘 많아서 결정하는데도 힘들구",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100021897.1.35",
