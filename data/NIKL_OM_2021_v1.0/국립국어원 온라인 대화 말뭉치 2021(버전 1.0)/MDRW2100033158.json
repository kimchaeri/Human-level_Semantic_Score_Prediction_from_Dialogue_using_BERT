        },
        {
          "id": "MDRW2100033158.1.18",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100033158.1.19",
          "form": "추석 일주일간은 가족모임에 한해 8명까지 모일 수 있대",
          "original_form": "추석 일주일간은 가족모임에 한해 8명까지 모일 수 있대",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100033158.1.20",
          "form": "근데 뭐.... 안내려갈 것 같은데",
          "original_form": "근데 뭐.... 안내려갈 것 같은데",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100033158.1.21",
          "form": "아마 나도",
          "original_form": "아마 나도",
          "speaker_id": "1",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100033158.1.22",
          "form": "온라인세배 가보자고",
          "original_form": "온라인세배 가보자고",
          "speaker_id": "1",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100033158.1.23",
          "form": "ㅋㅋ 못내려가서 용돈 없죠?",
          "original_form": "ㅋㅋ 못내려가서 용돈 없죠?",
