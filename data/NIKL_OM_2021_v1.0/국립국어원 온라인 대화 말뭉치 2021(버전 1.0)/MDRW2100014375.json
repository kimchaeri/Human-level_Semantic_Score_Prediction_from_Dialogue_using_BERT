      "utterance": [
        {
          "id": "MDRW2100014375.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014375.1.2",
          "form": "영화관 가는건 좋은데 영화를 많이 안봐요..",
          "original_form": "영화관 가는건 좋은데 영화를 많이 안봐요..",
          "speaker_id": "2",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014375.1.3",
          "form": "요즘 특히 코로나때문에 더 안가게 되는것 같아요",
          "original_form": "요즘 특히 코로나때문에 더 안가게 되는것 같아요",
          "speaker_id": "2",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014375.1.4",
          "form": "ㅠㅡㅜ",
          "original_form": "ㅠㅡㅜ",
          "speaker_id": "2",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014375.1.5",
          "form": "영화좋아하세여?",
          "original_form": "영화좋아하세여?",
          "speaker_id": "2",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014375.1.6",
          "form": "저도 영화관에서 보는 영화를 좋아해요 ㅠㅠ",
          "original_form": "저도 영화관에서 보는 영화를 좋아해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014375.1.7",
          "form": "최근에 뭐 보신거 있으신가요??",
          "original_form": "최근에 뭐 보신거 있으신가요??",
          "speaker_id": "2",
          "time": "20210825 18:52"
        },
        {
          "id": "MDRW2100014375.1.8",
          "form": "흠 집에서 보스베이비요 ㅋㅋ",
          "original_form": "흠 집에서 보스베이비요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100014375.1.9",
          "form": "보스베이비 이름만 알아요 ㅋㅋㅋ 애니메이션인가유?",
          "original_form": "보스베이비 이름만 알아요 ㅋㅋㅋ 애니메이션인가유?",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014375.1.10",
          "form": "네네 ㅋㅋ어른들도 좋아라하는 애니영화죠 !",
          "original_form": "네네 ㅋㅋ어른들도 좋아라하는 애니영화죠 !",
          "speaker_id": "1",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100014375.1.11",
          "form": "그렇구나ㅋㅋㅋ 저는 집에서 베일리어게인 이라는 영화봤거든요!",
          "original_form": "그렇구나ㅋㅋㅋ 저는 집에서 베일리어게인 이라는 영화봤거든요!",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014375.1.12",
          "form": "이영화 혹시 아세요?",
          "original_form": "이영화 혹시 아세요?",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014375.1.13",
          "form": "아뇽 그게뭐죠?",
          "original_form": "아뇽 그게뭐죠?",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014375.1.14",
          "form": "영환데 베일리라는 강아지가 나와요~",
          "original_form": "영환데 베일리라는 강아지가 나와요~",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014375.1.15",
          "form": "재밌어요?",
          "original_form": "재밌어요?",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014375.1.16",
          "form": "강아지가 한 인생을 살고 죽고 다시 새로운 강아지로 태어나고 반복하는데 첫번째 주인을 계속 기억하고 새로 태어났어도 첫 주인을 찾아가는 영화예요",
          "original_form": "강아지가 한 인생을 살고 죽고 다시 새로운 강아지로 태어나고 반복하는데 첫번째 주인을 계속 기억하고 새로 태어났어도 첫 주인을 찾아가는 영화예요",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014375.1.17",
          "form": "강아지가 너무 귀여워요 ㅋㅋㅋㅋ",
          "original_form": "강아지가 너무 귀여워요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014375.1.18",
          "form": "매번 다른 강아지가 나오는데 저희엄마도 재밌게 보셨어요",
          "original_form": "매번 다른 강아지가 나오는데 저희엄마도 재밌게 보셨어요",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014375.1.19",
          "form": "오 듣기만해도 재밌을거같은데요?",
          "original_form": "오 듣기만해도 재밌을거같은데요?",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014375.1.20",
          "form": "넹넹 추천이요 ㅋㅋ",
          "original_form": "넹넹 추천이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014375.1.21",
          "form": "이거 1편있고 2편이 있는데 2편에는 헨리나와요 ㅋㅋ",
          "original_form": "이거 1편있고 2편이 있는데 2편에는 헨리나와요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014375.1.22",
          "form": "영화보다가 깜짝놀랐어요",
          "original_form": "영화보다가 깜짝놀랐어요",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014375.1.23",
