      "utterance": [
        {
          "id": "MDRW2100005225.1.1",
          "form": "뱅기타고 여행가고싶구먼요~",
          "original_form": "뱅기타고 여행가고싶구먼요~",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005225.1.2",
          "form": "기다릴께요 ㅋㅋㅋㅋㅋ",
          "original_form": "기다릴께요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005225.1.3",
          "form": "나도 ㅠ",
          "original_form": "나도 ㅠ",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005225.1.4",
          "form": "뱅기 언제 타겠노",
          "original_form": "뱅기 언제 타겠노",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005225.1.5",
          "form": "언닌 그래도 함 댕기왔잖아여ㅠ",
          "original_form": "언닌 그래도 함 댕기왔잖아여ㅠ",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005225.1.6",
          "form": "슬프구만요ㅠ",
          "original_form": "슬프구만요ㅠ",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005225.1.7",
          "form": "맘편하게 함 댕기오곱네여~",
          "original_form": "맘편하게 함 댕기오곱네여~",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005225.1.8",
          "form": "기념품은 설악산서 사야져 ㅋㅋ",
          "original_form": "기념품은 설악산서 사야져 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005225.1.9",
          "form": "ㅋㅋㅋㅋㅋ 그래 뭐 그냥 그랬다",
          "original_form": "ㅋㅋㅋㅋㅋ 그래 뭐 그냥 그랬다",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005225.1.10",
          "form": "ㅋㅋㅋ마스크 벗을때",
          "original_form": "ㅋㅋㅋ마스크 벗을때",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005225.1.11",
          "form": "갈수있겠지",
          "original_form": "갈수있겠지",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
