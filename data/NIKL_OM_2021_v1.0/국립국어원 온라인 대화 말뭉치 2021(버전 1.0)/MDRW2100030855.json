      "utterance": [
        {
          "id": "MDRW2100030855.1.1",
          "form": "ㅁ날씨어때유",
          "original_form": "ㅁ날씨어때유",
          "speaker_id": "1",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030855.1.2",
          "form": "서울 맑아요! 사진 올릴 수 있으면 하늘을 찍어서 보여드리고 싶을 정도",
          "original_form": "서울 맑아요! 사진 올릴 수 있으면 하늘을 찍어서 보여드리고 싶을 정도",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030855.1.3",
          "form": "저도서울 ㅋㅋ",
          "original_form": "저도서울 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030855.1.4",
          "form": "집이세요?",
          "original_form": "집이세요?",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030855.1.5",
          "form": "네네 집에 있어요. 주말 맞아서 집 청소도 하는 중이여요",
          "original_form": "네네 집에 있어요. 주말 맞아서 집 청소도 하는 중이여요",
          "speaker_id": "2",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030855.1.6",
          "form": "청소하기조은날씨",
          "original_form": "청소하기조은날씨",
          "speaker_id": "1",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100030855.1.7",
          "form": "혼자요?",
          "original_form": "혼자요?",
          "speaker_id": "1",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100030855.1.8",
          "form": "네네 혼자요~ 창문 열어두고 있어요 ㅋㅋㅋ 날씨가 좋아서... 바람도 선선하게 부네요",
          "original_form": "네네 혼자요~ 창문 열어두고 있어요 ㅋㅋㅋ 날씨가 좋아서... 바람도 선선하게 부네요",
          "speaker_id": "2",
          "time": "20210904 13:40"
        },
        {
          "id": "MDRW2100030855.1.9",
          "form": "오 옷은 머입으셧어요",
          "original_form": "오 옷은 머입으셧어요",
          "speaker_id": "1",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100030855.1.10",
          "form": "지금 입고 있는 옷이요? 그냥 편하게 입고 있어욤~",
          "original_form": "지금 입고 있는 옷이요? 그냥 편하게 입고 있어욤~",
          "speaker_id": "2",
          "time": "20210904 13:48"
        },
        {
          "id": "MDRW2100030855.1.11",
          "form": "반팔 ??",
          "original_form": "반팔 ??",
          "speaker_id": "1",
          "time": "20210904 13:52"
        },
        {
          "id": "MDRW2100030855.1.12",
          "form": "네네 반팔!",
          "original_form": "네네 반팔!",
          "speaker_id": "2",
          "time": "20210904 13:52"
        },
        {
          "id": "MDRW2100030855.1.13",
          "form": "name1님은요~? 집이셔요?",
          "original_form": "&name1&님은요~? 집이셔요?",
          "speaker_id": "2",
          "time": "20210904 13:52"
        },
        {
          "id": "MDRW2100030855.1.14",
          "form": "나왓엉유",
          "original_form": "나왓엉유",
          "speaker_id": "1",
          "time": "20210904 13:53"
        },
        {
          "id": "MDRW2100030855.1.15",
          "form": "나가보세용 날 조은대",
          "original_form": "나가보세용 날 조은대",
          "speaker_id": "1",
          "time": "20210904 13:53"
        },
        {
          "id": "MDRW2100030855.1.16",
          "form": "나갈 예정이 없긴 한데 한번 그래볼게욤!",
          "original_form": "나갈 예정이 없긴 한데 한번 그래볼게욤!",
          "speaker_id": "2",
          "time": "20210904 13:53"
        },
        {
          "id": "MDRW2100030855.1.17",
          "form": "해가 쨍해요",
          "original_form": "해가 쨍해요",
          "speaker_id": "1",
          "time": "20210904 13:55"
        }
      ]
