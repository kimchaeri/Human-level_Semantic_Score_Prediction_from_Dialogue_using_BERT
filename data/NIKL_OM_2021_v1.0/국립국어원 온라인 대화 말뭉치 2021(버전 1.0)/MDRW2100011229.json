        },
        {
          "id": "MDRW2100011229.1.14",
          "form": "생명존중캠페인이었어요ㅋㅋ",
          "original_form": "생명존중캠페인이었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:48"
        },
        {
          "id": "MDRW2100011229.1.15",
          "form": "국내투어 한다길래 재밌어보여서...",
          "original_form": "국내투어 한다길래 재밌어보여서...",
