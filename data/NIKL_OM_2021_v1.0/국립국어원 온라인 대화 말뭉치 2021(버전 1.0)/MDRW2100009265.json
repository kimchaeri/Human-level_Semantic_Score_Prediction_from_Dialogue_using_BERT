      "utterance": [
        {
          "id": "MDRW2100009265.1.1",
          "form": "오늘 점심식사메뉴는 뭔가요?",
          "original_form": "오늘 점심식사메뉴는 뭔가요?",
          "speaker_id": "2",
          "time": "20210823 11:44"
        },
        {
          "id": "MDRW2100009265.1.2",
          "form": "저는찜닭먹었어요",
          "original_form": "저는찜닭먹었어요",
          "speaker_id": "1",
          "time": "20210823 21:02"
        },
        {
          "id": "MDRW2100009265.1.3",
          "form": "간장찜닭인가요 고추장찜닭인가요?",
          "original_form": "간장찜닭인가요 고추장찜닭인가요?",
          "speaker_id": "2",
          "time": "20210824 00:28"
        },
        {
          "id": "MDRW2100009265.1.4",
          "form": "간장과고추장을섞은게있어요",
          "original_form": "간장과고추장을섞은게있어요",
          "speaker_id": "1",
          "time": "20210824 13:19"
        },
        {
          "id": "MDRW2100009265.1.5",
          "form": "그게 맛잇더라구요",
          "original_form": "그게 맛잇더라구요",
          "speaker_id": "1",
          "time": "20210824 13:19"
        },
        {
          "id": "MDRW2100009265.1.6",
          "form": "황금찜닭맞나요?",
          "original_form": "황금찜닭맞나요?",
          "speaker_id": "2",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100009265.1.7",
          "form": "네 맞아요 매콤하고짭쪼롬하고",
          "original_form": "네 맞아요 매콤하고짭쪼롬하고",
          "speaker_id": "1",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100009265.1.8",
          "form": "완벽하죠",
          "original_form": "완벽하죠",
          "speaker_id": "1",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100009265.1.9",
          "form": "안동찜닭도 참 맛있겠어요",
          "original_form": "안동찜닭도 참 맛있겠어요",
          "speaker_id": "2",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100009265.1.10",
          "form": "안동가서드셔보셨나요",
          "original_form": "안동가서드셔보셨나요",
          "speaker_id": "1",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100009265.1.11",
          "form": "저는먹어봤는데 그냥 비슷비슷",
          "original_form": "저는먹어봤는데 그냥 비슷비슷",
          "speaker_id": "1",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100009265.1.12",
          "form": "어릴적이라 기억은 잘 안나네요ㅜ",
          "original_form": "어릴적이라 기억은 잘 안나네요ㅜ",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100009265.1.13",
          "form": "요즘로제찜닭도있던데",
          "original_form": "요즘로제찜닭도있던데",
          "speaker_id": "1",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100009265.1.14",
          "form": "그건시도안해봤네요",
          "original_form": "그건시도안해봤네요",
          "speaker_id": "1",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100009265.1.15",
          "form": "신기한게 많이나오네요",
          "original_form": "신기한게 많이나오네요",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100009265.1.16",
          "form": "찜닭값이 많이 올랐죠?",
          "original_form": "찜닭값이 많이 올랐죠?",
          "speaker_id": "2",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100009265.1.17",
          "form": "네 집에서해먹는게이득이예요",
          "original_form": "네 집에서해먹는게이득이예요",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100009265.1.18",
          "form": "닭얼마안하는데",
          "original_form": "닭얼마안하는데",
          "speaker_id": "1",
          "time": "20210824 13:31"
        },
        {
          "id": "MDRW2100009265.1.19",
          "form": "양념맛때문에",
          "original_form": "양념맛때문에",
          "speaker_id": "1",
          "time": "20210824 13:31"
        },
        {
          "id": "MDRW2100009265.1.20",
          "form": "찜닭양념법만 잘 알면 그게 훨씬 좋겠네요",
          "original_form": "찜닭양념법만 잘 알면 그게 훨씬 좋겠네요",
          "speaker_id": "2",
          "time": "20210824 13:31"
        }
      ]
