      "utterance": [
        {
          "id": "MDRW2100020961.1.1",
          "form": "점심 뭐드셨어요?",
          "original_form": "점심 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100020961.1.2",
          "form": "앗ㅜ 오늘 갑자기 출근하느라 늦게 봤네요",
          "original_form": "앗ㅜ 오늘 갑자기 출근하느라 늦게 봤네요",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100020961.1.3",
          "form": "점심은 간단히 찌개 먹고 저녁엔 어죽 먹었어요",
          "original_form": "점심은 간단히 찌개 먹고 저녁엔 어죽 먹었어요",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100020961.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100020961.1.5",
          "form": "저는 토달볶이요!",
          "original_form": "저는 토달볶이요!",
          "speaker_id": "2",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100020961.1.6",
          "form": "직접 조리해드셨군요ㄷㄷ",
          "original_form": "직접 조리해드셨군요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100020961.1.7",
          "form": "어렵지 않나요?",
          "original_form": "어렵지 않나요?",
          "speaker_id": "1",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100020961.1.8",
          "form": "아녜요 어머니가 해주셨어요ㅋㅋㅋ별로 어렵지 않대요",
          "original_form": "아녜요 어머니가 해주셨어요ㅋㅋㅋ별로 어렵지 않대요",
          "speaker_id": "2",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100020961.1.9",
          "form": "오오 어머니가 요리를 잘하시나봐요",
          "original_form": "오오 어머니가 요리를 잘하시나봐요",
          "speaker_id": "1",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100020961.1.10",
          "form": "냉부서 보고 궁금했는데",
          "original_form": "냉부서 보고 궁금했는데",
          "speaker_id": "1",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100020961.1.11",
          "form": "네네 어머니가 요리를 좋아하세요 냉부서가 뭔가요?",
          "original_form": "네네 어머니가 요리를 좋아하세요 냉부서가 뭔가요?",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100020961.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100020961.1.13",
          "form": "냉장고를부탁해군요ㅋㅋㄱㅋㅋ",
          "original_form": "냉장고를부탁해군요ㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100020961.1.14",
          "form": "아 냉장고를부탁해요ㅎ",
          "original_form": "아 냉장고를부탁해요ㅎ",
          "speaker_id": "1",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100020961.1.15",
          "form": "네 최애 프로였죠",
          "original_form": "네 최애 프로였죠",
          "speaker_id": "1",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100020961.1.16",
          "form": "거기 나오는 요리 따라해보시기도 했나요?",
          "original_form": "거기 나오는 요리 따라해보시기도 했나요?",
          "speaker_id": "2",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100020961.1.17",
          "form": "어려워보이던데...ㅎㅎ",
          "original_form": "어려워보이던데...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100020961.1.18",
          "form": "ㅎㅎ 마음은 굴뚝인데",
          "original_form": "ㅎㅎ 마음은 굴뚝인데",
          "speaker_id": "1",
          "time": "20210830 15:02"
        },
        {
          "id": "MDRW2100020961.1.19",
          "form": "워낙 곰손이라 보기만 했죠ㅜ",
          "original_form": "워낙 곰손이라 보기만 했죠ㅜ",
          "speaker_id": "1",
          "time": "20210830 15:02"
        },
        {
          "id": "MDRW2100020961.1.20",
          "form": "김풍거는 따라하고 싶었어요ㅎ",
          "original_form": "김풍거는 따라하고 싶었어요ㅎ",
          "speaker_id": "1",
          "time": "20210830 15:03"
        },
        {
          "id": "MDRW2100020961.1.21",
          "form": "어떤 요리였는데요? 김풍이 주로 중국음식을 했던가.. 기언이 잘 안나네요",
          "original_form": "어떤 요리였는데요? 김풍이 주로 중국음식을 했던가.. 기언이 잘 안나네요",
          "speaker_id": "2",
          "time": "20210830 15:05"
        },
        {
          "id": "MDRW2100020961.1.22",
          "form": "다이김ㅎㅎ",
          "original_form": "다이김ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 15:05"
        },
        {
          "id": "MDRW2100020961.1.23",
          "form": "김넣고 휘휘 저었는데 이건 진짜 간단해보이더라고요",
          "original_form": "김넣고 휘휘 저었는데 이건 진짜 간단해보이더라고요",
          "speaker_id": "1",
          "time": "20210830 15:05"
        },
        {
          "id": "MDRW2100020961.1.24",
          "form": "방금 검색하고왔어요 매콤한 중국 김국이군요",
          "original_form": "방금 검색하고왔어요 매콤한 중국 김국이군요",
          "speaker_id": "2",
          "time": "20210830 15:05"
        },
        {
          "id": "MDRW2100020961.1.25",
          "form": "심지어 극찬받음ㅎㅎ",
          "original_form": "심지어 극찬받음ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100020961.1.26",
          "form": "네네 김풍 요리가 그런게 많더라고요",
          "original_form": "네네 김풍 요리가 그런게 많더라고요",
          "speaker_id": "1",
          "time": "20210830 15:06"
        }
      ]
