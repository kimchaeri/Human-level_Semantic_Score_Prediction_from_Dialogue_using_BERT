      "utterance": [
        {
          "id": "MDRW2100009928.1.1",
          "form": "중고 개학했던데",
          "original_form": "중고 개학했던데",
          "speaker_id": "2",
          "time": "20210823 11:55"
        },
        {
          "id": "MDRW2100009928.1.2",
          "form": "목욜 개학이라니...40일 빨리 가는구먼",
          "original_form": "목욜 개학이라니...40일 빨리 가는구먼",
          "speaker_id": "2",
          "time": "20210823 11:55"
        },
        {
          "id": "MDRW2100009928.1.3",
          "form": "숙제우짜노ㅡㅡ",
          "original_form": "숙제우짜노ㅡㅡ",
          "speaker_id": "1",
          "time": "20210823 12:36"
        },
        {
          "id": "MDRW2100009928.1.4",
          "form": "모르겠다 저거숙젠데ㅋㅋ",
          "original_form": "모르겠다 저거숙젠데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 12:36"
        },
        {
          "id": "MDRW2100009928.1.5",
          "form": "내숙제도 바쁨",
          "original_form": "내숙제도 바쁨",
          "speaker_id": "1",
          "time": "20210823 12:36"
        },
        {
          "id": "MDRW2100009928.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋ 언니숙제뭔데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋ 언니숙제뭔데",
          "speaker_id": "2",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100009928.1.7",
          "form": "우리도 슬슬 일기가 주1회가 되어가고 있음",
          "original_form": "우리도 슬슬 일기가 주1회가 되어가고 있음",
          "speaker_id": "2",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100009928.1.8",
          "form": "곧 개학인데",
          "original_form": "곧 개학인데",
          "speaker_id": "2",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100009928.1.9",
          "form": "권장도서 한권도 못읽음",
          "original_form": "권장도서 한권도 못읽음",
          "speaker_id": "2",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100009928.1.10",
          "form": "숙제모른다ㅋㅋ",
          "original_form": "숙제모른다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:47"
        },
        {
          "id": "MDRW2100009928.1.11",
          "form": "권장도서는 뭐고ㅋㅋㅋㅋ",
          "original_form": "권장도서는 뭐고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100009928.1.12",
          "form": "스트레스네ㅋㅋ",
          "original_form": "스트레스네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100009928.1.13",
          "form": "ㅋㅋㅋㅋ 언니 학교생활에 대해 아는게 뭔데 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 언니 학교생활에 대해 아는게 뭔데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:53"
        },
        {
          "id": "MDRW2100009928.1.14",
          "form": "나의학교생활ㅋㅋ",
          "original_form": "나의학교생활ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:53"
        },
        {
          "id": "MDRW2100009928.1.15",
          "form": "아이들의학교는 스스로 하는걸로ㅋ",
          "original_form": "아이들의학교는 스스로 하는걸로ㅋ",
          "speaker_id": "1",
          "time": "20210823 14:53"
        },
        {
          "id": "MDRW2100009928.1.16",
          "form": "수강신청하고",
          "original_form": "수강신청하고",
          "speaker_id": "1",
          "time": "20210823 14:53"
        },
        {
          "id": "MDRW2100009928.1.17",
          "form": "수업들어야됨",
          "original_form": "수업들어야됨",
          "speaker_id": "1",
          "time": "20210823 14:53"
        },
        {
          "id": "MDRW2100009928.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ  사회복지??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ  사회복지??",
          "speaker_id": "2",
          "time": "20210823 14:59"
        },
        {
          "id": "MDRW2100009928.1.19",
          "form": "그래ㅋ 사회복지",
          "original_form": "그래ㅋ 사회복지",
          "speaker_id": "1",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100009928.1.20",
          "form": "수업들어야지",
          "original_form": "수업들어야지",
          "speaker_id": "1",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100009928.1.21",
          "form": "고생이 많네",
          "original_form": "고생이 많네",
          "speaker_id": "2",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100009928.1.22",
          "form": "우리 원은 학원가서도 게임시간 늘려달라고 전화왔네...",
          "original_form": "우리 원은 학원가서도 게임시간 늘려달라고 전화왔네...",
          "speaker_id": "2",
          "time": "20210823 15:37"
        },
        {
          "id": "MDRW2100009928.1.23",
          "form": "휴...",
          "original_form": "휴...",
          "speaker_id": "2",
          "time": "20210823 15:37"
        },
        {
          "id": "MDRW2100009928.1.24",
          "form": "늘려줘라ㅋㅋ",
          "original_form": "늘려줘라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:41"
        },
        {
          "id": "MDRW2100009928.1.25",
          "form": "고생이많네 학원가는 아이들",
          "original_form": "고생이많네 학원가는 아이들",
          "speaker_id": "1",
          "time": "20210823 15:41"
        },
        {
          "id": "MDRW2100009928.1.26",
          "form": "무제한으로 해줄수도 없고",
          "original_form": "무제한으로 해줄수도 없고",
          "speaker_id": "2",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100009928.1.27",
          "form": "난감하군요",
          "original_form": "난감하군요",
          "speaker_id": "2",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100009928.1.28",
          "form": "그래 마미 화이팅ㅋㅋ",
          "original_form": "그래 마미 화이팅ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:55"
        },
        {
          "id": "MDRW2100009928.1.29",
          "form": "스트레스 받지말고ㅋㅋ",
          "original_form": "스트레스 받지말고ㅋㅋ",
          "speaker_id": "1",
