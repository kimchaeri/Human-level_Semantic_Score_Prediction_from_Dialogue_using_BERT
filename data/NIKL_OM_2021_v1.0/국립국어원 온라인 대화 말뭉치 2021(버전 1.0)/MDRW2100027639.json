      "utterance": [
        {
          "id": "MDRW2100027639.1.1",
          "form": "요즘 많지",
          "original_form": "요즘 많지",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100027639.1.2",
          "form": "오메가3 종비 유산균 마그네슘 프로폴리스",
          "original_form": "오메가3 종비 유산균 마그네슘 프로폴리스",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027639.1.3",
          "form": "근데 내킬때만 먹어",
          "original_form": "근데 내킬때만 먹어",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027639.1.4",
          "form": "마그네슘은 어디에조아?",
          "original_form": "마그네슘은 어디에조아?",
          "speaker_id": "1",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027639.1.5",
          "form": "잘모르겠어 ㅋㅋㅋ",
          "original_form": "잘모르겠어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027639.1.6",
          "form": "잠 잘 못자면 자기 전에 먹으래서 샀는데",
          "original_form": "잠 잘 못자면 자기 전에 먹으래서 샀는데",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027639.1.7",
          "form": "이완작용 같은거...",
          "original_form": "이완작용 같은거...",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027639.1.8",
          "form": "근육이완작용?",
          "original_form": "근육이완작용?",
          "speaker_id": "1",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027639.1.9",
          "form": "아 잠 못잘때",
          "original_form": "아 잠 못잘때",
          "speaker_id": "1",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027639.1.10",
          "form": "신경 아닐가?",
          "original_form": "신경 아닐가?",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027639.1.11",
          "form": "그렇구먼",
          "original_form": "그렇구먼",
          "speaker_id": "1",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027639.1.12",
          "form": "아님 ㅋㅋㅋ사실 잘 몰라",
          "original_form": "아님 ㅋㅋㅋ사실 잘 몰라",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027639.1.13",
          "form": "어디에서 주로 사?",
          "original_form": "어디에서 주로 사?",
          "speaker_id": "1",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027639.1.14",
          "form": "그냥 먹는겨",
          "original_form": "그냥 먹는겨",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027639.1.15",
          "form": "쿠팡직구나 아이허브지",
          "original_form": "쿠팡직구나 아이허브지",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027639.1.16",
          "form": "영양제 새로 사게?",
          "original_form": "영양제 새로 사게?",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027639.1.17",
          "form": "그렇구나 아니 나는 계속 같응고 먹는즁 ㅋㅋㅋㅋ",
          "original_form": "그렇구나 아니 나는 계속 같응고 먹는즁 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027639.1.18",
          "form": "ㅋㅋㅋㅋ효과있나",
          "original_form": "ㅋㅋㅋㅋ효과있나",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027639.1.19",
          "form": "ㅋㅋㅋㅋㅋ영양제는 안먹으면 티많이나",
          "original_form": "ㅋㅋㅋㅋㅋ영양제는 안먹으면 티많이나",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027639.1.20",
          "form": "유산균은 효과좋다",
          "original_form": "유산균은 효과좋다",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027639.1.21",
          "form": "ㅋㅋㅋ...내가 꾸준히 안먹어서 그런가",
          "original_form": "ㅋㅋㅋ...내가 꾸준히 안먹어서 그런가",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027639.1.22",
          "form": "영양제는 매일매일묵짜",
          "original_form": "영양제는 매일매일묵짜",
          "speaker_id": "1",
          "time": "20210902 14:06"
        }
      ]
