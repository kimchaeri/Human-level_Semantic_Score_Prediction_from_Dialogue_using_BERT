      "utterance": [
        {
          "id": "MDRW2100035660.1.1",
          "form": "엑소 세계관 뒤로 다시 말씀해주세요!",
          "original_form": "엑소 세계관 뒤로 다시 말씀해주세요!",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035660.1.2",
          "form": "아 걔 세계관이 비행이였는데 !",
          "original_form": "아 걔 세계관이 비행이였는데 !",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100035660.1.3",
          "form": "또 젤 첫번째로 나르고 그래서 ㅋㅋㅋ",
          "original_form": "또 젤 첫번째로 나르고 그래서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100035660.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 세계관에 충실했네요 어쨌든..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 세계관에 충실했네요 어쨌든..",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100035660.1.5",
          "form": "아 그 세계관 첨에 진짜... 충격이었는데",
          "original_form": "아 그 세계관 첨에 진짜... 충격이었는데",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100035660.1.6",
          "form": "아우 저두요",
          "original_form": "아우 저두요",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100035660.1.7",
          "form": "전 뭐 런닝맨인줄",
          "original_form": "전 뭐 런닝맨인줄",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100035660.1.8",
          "form": "알았어요",
          "original_form": "알았어요",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100035660.1.9",
          "form": "시간을 거스르는자~ 느낌 ...",
          "original_form": "시간을 거스르는자~ 느낌 ...",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035660.1.10",
          "form": "아악~~~~~~",
          "original_form": "아악~~~~~~",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035660.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ근데 이제 약간 적응됐어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ근데 이제 약간 적응됐어요",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035660.1.12",
          "form": "적응된 내가 싫다...",
          "original_form": "적응된 내가 싫다...",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035660.1.13",
          "form": "아 인정이요 ...",
          "original_form": "아 인정이요 ...",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035660.1.14",
          "form": "괜히 세계관 소개 안하면 섭섭했는데 ...",
          "original_form": "괜히 세계관 소개 안하면 섭섭했는데 ...",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035660.1.15",
          "form": "이수만의 큰그림..",
          "original_form": "이수만의 큰그림..",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100035660.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ수만... 그 에스파도",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ수만... 그 에스파도",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100035660.1.17",
          "form": "첨에 진짜... 진짜... 모르는 뇌 삽니다",
          "original_form": "첨에 진짜... 진짜... 모르는 뇌 삽니다",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100035660.1.18",
          "form": "하고싶었는데 이젠...",
          "original_form": "하고싶었는데 이젠...",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100035660.1.19",
          "form": "아니 .. 할배 진짜 감 쩔지않나요?",
          "original_form": "아니 .. 할배 진짜 감 쩔지않나요?",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100035660.1.20",
          "form": "인정이요",
          "original_form": "인정이요",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035660.1.21",
          "form": "전 에스파가 이리 빨리 뜰줄 몰랐어요",
          "original_form": "전 에스파가 이리 빨리 뜰줄 몰랐어요",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035660.1.22",
          "form": "제가 할배 뒤를 따라가고 있잖아요",
          "original_form": "제가 할배 뒤를 따라가고 있잖아요",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035660.1.23",
          "form": "심지어 따라가기 버거워요",
          "original_form": "심지어 따라가기 버거워요",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035660.1.24",
          "form": "아 ㅋㅋㅋ 인정이요 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋ 인정이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035660.1.25",
          "form": "할배 천천히가~~%!!",
          "original_form": "할배 천천히가~~%!!",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035660.1.26",
          "form": "전 아직 광야도 적응 안됐는데 !!!",
          "original_form": "전 아직 광야도 적응 안됐는데 !!!",
