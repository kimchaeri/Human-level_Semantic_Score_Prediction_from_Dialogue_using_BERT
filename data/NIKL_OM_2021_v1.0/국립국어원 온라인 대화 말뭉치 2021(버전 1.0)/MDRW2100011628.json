      "utterance": [
        {
          "id": "MDRW2100011628.1.1",
          "form": "취업중이신가요?",
          "original_form": "취업중이신가요?",
          "speaker_id": "1",
          "time": "20210824 14:39"
        },
        {
          "id": "MDRW2100011628.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210824 14:39"
        },
        {
          "id": "MDRW2100011628.1.3",
          "form": "유튜브하고 책내려고 준비중이에요",
          "original_form": "유튜브하고 책내려고 준비중이에요",
          "speaker_id": "2",
          "time": "20210824 14:39"
        },
        {
          "id": "MDRW2100011628.1.4",
          "form": "멋있으시네요..",
          "original_form": "멋있으시네요..",
          "speaker_id": "1",
          "time": "20210824 14:40"
        },
        {
          "id": "MDRW2100011628.1.5",
          "form": "어떤일하세요?",
          "original_form": "어떤일하세요?",
          "speaker_id": "1",
          "time": "20210824 14:40"
        },
        {
          "id": "MDRW2100011628.1.6",
          "form": "유튜브요 ㅋㅋ",
          "original_form": "유튜브요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:40"
        },
        {
          "id": "MDRW2100011628.1.7",
          "form": "근데 요즘은 잘 안하고 요즘엔 이것저것 배우는걸 주로해요 유튜브 영상에 반영하려구요",
          "original_form": "근데 요즘은 잘 안하고 요즘엔 이것저것 배우는걸 주로해요 유튜브 영상에 반영하려구요",
          "speaker_id": "2",
          "time": "20210824 14:40"
        },
        {
          "id": "MDRW2100011628.1.8",
          "form": "아 본업이 유튜버 와대박",
          "original_form": "아 본업이 유튜버 와대박",
          "speaker_id": "1",
          "time": "20210824 14:40"
        },
        {
          "id": "MDRW2100011628.1.9",
          "form": "하꼬방 유튜법니다 ㅋㅋ",
          "original_form": "하꼬방 유튜법니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:41"
        },
        {
          "id": "MDRW2100011628.1.10",
          "form": "저는 백수입니다ㅎㅎ",
          "original_form": "저는 백수입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:41"
        },
        {
          "id": "MDRW2100011628.1.11",
          "form": "준비하시는 쪽 여쭤봐도되요?",
          "original_form": "준비하시는 쪽 여쭤봐도되요?",
          "speaker_id": "2",
          "time": "20210824 14:41"
        },
        {
          "id": "MDRW2100011628.1.12",
          "form": "직장 다니다가 그냥 쉬고있어요 번아웃 와서",
          "original_form": "직장 다니다가 그냥 쉬고있어요 번아웃 와서",
          "speaker_id": "1",
          "time": "20210824 14:42"
        },
        {
          "id": "MDRW2100011628.1.13",
          "form": "아 그러시군요... 쉴땐 쉬어야하는거같아요 정말요",
          "original_form": "아 그러시군요... 쉴땐 쉬어야하는거같아요 정말요",
          "speaker_id": "2",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011628.1.14",
          "form": "맞아요 사람이 일만 하고 살순없어요",
          "original_form": "맞아요 사람이 일만 하고 살순없어요",
          "speaker_id": "1",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011628.1.15",
          "form": "아르바이트 해보신적있으세요?",
          "original_form": "아르바이트 해보신적있으세요?",
          "speaker_id": "1",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011628.1.16",
          "form": "네 이것저것 해봤어요",
          "original_form": "네 이것저것 해봤어요",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011628.1.17",
          "form": "백화점 와인알바",
          "original_form": "백화점 와인알바",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011628.1.18",
          "form": "전단지 돌리기",
          "original_form": "전단지 돌리기",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011628.1.19",
          "form": "편의점 공장",
          "original_form": "편의점 공장",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011628.1.20",
          "form": "해봤어요",
          "original_form": "해봤어요",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011628.1.21",
          "form": "다양한걸 해보셨네요!",
          "original_form": "다양한걸 해보셨네요!",
          "speaker_id": "1",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011628.1.22",
          "form": "아 과외도요",
          "original_form": "아 과외도요",
          "speaker_id": "2",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011628.1.23",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:45"
        }
      ]
