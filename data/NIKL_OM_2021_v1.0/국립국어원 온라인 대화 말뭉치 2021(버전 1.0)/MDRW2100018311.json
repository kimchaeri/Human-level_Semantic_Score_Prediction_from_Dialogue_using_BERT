      "utterance": [
        {
          "id": "MDRW2100018311.1.1",
          "form": "오늘은 택배 뭐 왓어",
          "original_form": "오늘은 택배 뭐 왓어",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100018311.1.2",
          "form": "매일오는데",
          "original_form": "매일오는데",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100018311.1.3",
          "form": "매일주문하긴하지 오늘 만두는?",
          "original_form": "매일주문하긴하지 오늘 만두는?",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100018311.1.4",
          "form": "냉동실네 넣엇어",
          "original_form": "냉동실네 넣엇어",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100018311.1.5",
          "form": "잘햇네",
          "original_form": "잘햇네",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100018311.1.6",
          "form": "내일 만두국 먹을까",
          "original_form": "내일 만두국 먹을까",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100018311.1.7",
          "form": "니가 할꺼니",
          "original_form": "니가 할꺼니",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100018311.1.8",
          "form": "아니 ㅋㅋㅋ 난아냐",
          "original_form": "아니 ㅋㅋㅋ 난아냐",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100018311.1.9",
          "form": "낼식혜도 올거야",
          "original_form": "낼식혜도 올거야",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100018311.1.10",
          "form": "그래 받아둘게",
          "original_form": "그래 받아둘게",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100018311.1.11",
          "form": "응 그것도 시원하게 냉장고에 넣어줘",
          "original_form": "응 그것도 시원하게 냉장고에 넣어줘",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100018311.1.12",
          "form": "어 넣어둘게",
          "original_form": "어 넣어둘게",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100018311.1.13",
          "form": "응 그거 말고도 여러개 올꺼야",
          "original_form": "응 그거 말고도 여러개 올꺼야",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100018311.1.14",
          "form": "더온다고?",
          "original_form": "더온다고?",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100018311.1.15",
          "form": "응 곱창전골이란 쥐포랑 뭐드라 여러개 시켯는데",
          "original_form": "응 곱창전골이란 쥐포랑 뭐드라 여러개 시켯는데",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100018311.1.16",
          "form": "적당히 주문해",
          "original_form": "적당히 주문해",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100018311.1.17",
          "form": "응 다 영상 찍을꺼니까",
          "original_form": "응 다 영상 찍을꺼니까",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100018311.1.18",
          "form": "영상?",
          "original_form": "영상?",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100018311.1.19",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100018311.1.20",
          "form": "거긴 손해인듯한데",
          "original_form": "거긴 손해인듯한데",
