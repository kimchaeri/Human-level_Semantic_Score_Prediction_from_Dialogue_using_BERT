      "utterance": [
        {
          "id": "MDRW2100019741.1.1",
          "form": "운동은 하시면서 공부하시는건가요?",
          "original_form": "운동은 하시면서 공부하시는건가요?",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019741.1.2",
          "form": "네네 그러고 있어요 ㅎㅎ",
          "original_form": "네네 그러고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019741.1.3",
          "form": "운동은 자주하세여?",
          "original_form": "운동은 자주하세여?",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019741.1.4",
          "form": "1만보걷기 자주했는데",
          "original_form": "1만보걷기 자주했는데",
          "speaker_id": "2",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019741.1.5",
          "form": "무지 피곤하네요",
          "original_form": "무지 피곤하네요",
          "speaker_id": "2",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019741.1.6",
          "form": "만보면 7키로 정도될텐데.. 운동 좋아하시나보네여",
          "original_form": "만보면 7키로 정도될텐데.. 운동 좋아하시나보네여",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019741.1.7",
          "form": "그렇지는 않고ㅜ 다이어트 때매 운동해요",
          "original_form": "그렇지는 않고ㅜ 다이어트 때매 운동해요",
          "speaker_id": "2",
          "time": "20210828 09:10"
        },
        {
          "id": "MDRW2100019741.1.8",
          "form": "다이어트 얼마나하셧나요?",
          "original_form": "다이어트 얼마나하셧나요?",
          "speaker_id": "1",
          "time": "20210828 12:56"
        },
        {
          "id": "MDRW2100019741.1.9",
          "form": "평생?ㅋㅋ",
          "original_form": "평생?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:25"
        },
        {
          "id": "MDRW2100019741.1.10",
          "form": "아.. ㅋㅋㅋ 전 10년차입니다..",
          "original_form": "아.. ㅋㅋㅋ 전 10년차입니다..",
          "speaker_id": "1",
          "time": "20210828 13:25"
        },
        {
          "id": "MDRW2100019741.1.11",
          "form": "지금 목표는 급접해가시나유?",
          "original_form": "지금 목표는 급접해가시나유?",
          "speaker_id": "1",
          "time": "20210828 13:26"
        },
        {
          "id": "MDRW2100019741.1.12",
          "form": "아뇨 점점 더 찌고 있어요ㅜㅜ",
          "original_form": "아뇨 점점 더 찌고 있어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100019741.1.13",
          "form": "아.. 다이어트가 건강을 위해 체중을 늘리다.. 라는 의미죠? ㅋㅋㅋ",
          "original_form": "아.. 다이어트가 건강을 위해 체중을 늘리다.. 라는 의미죠? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100019741.1.14",
          "form": "걷고 배고프니까 더 잘먹어요 ㅋㅋㅋ",
          "original_form": "걷고 배고프니까 더 잘먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100019741.1.15",
          "form": "걷는거 말고 다른 운동은 안하세여?",
          "original_form": "걷는거 말고 다른 운동은 안하세여?",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100019741.1.16",
          "form": "하기가실어요ㅜ",
          "original_form": "하기가실어요ㅜ",
          "speaker_id": "2",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100019741.1.17",
          "form": "고통없으면 다이어트가 아니에요.. ㅋㅋㅋ",
