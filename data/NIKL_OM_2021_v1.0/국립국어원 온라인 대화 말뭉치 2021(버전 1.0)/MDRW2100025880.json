      "utterance": [
        {
          "id": "MDRW2100025880.1.1",
          "form": "동물 키우시나용",
          "original_form": "동물 키우시나용",
          "speaker_id": "1",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100025880.1.2",
          "form": "아뇨ㅎㅎ 저는 강아지키으고싶어요!",
          "original_form": "아뇨ㅎㅎ 저는 강아지키으고싶어요!",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100025880.1.3",
          "form": "name1님ㅇㄴ요",
          "original_form": "&name1&님ㅇㄴ요",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100025880.1.4",
          "form": "저도 강아지용... ㅠ",
          "original_form": "저도 강아지용... ㅠ",
          "speaker_id": "1",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100025880.1.5",
          "form": "언제쯤...ㅠㅠㅠ",
          "original_form": "언제쯤...ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100025880.1.6",
          "form": "ㅠㅠ그러게요 좋아하는 종은 있으세요?",
          "original_form": "ㅠㅠ그러게요 좋아하는 종은 있으세요?",
          "speaker_id": "2",
          "time": "20210901 19:22"
        },
        {
          "id": "MDRW2100025880.1.7",
          "form": "음 골든리트리버?.ㅠㅠ 하지만 무리일거같구요..언젠가 그냥 소형견...말티즈나...",
          "original_form": "음 골든리트리버?.ㅠㅠ 하지만 무리일거같구요..언젠가 그냥 소형견...말티즈나...",
          "speaker_id": "1",
          "time": "20210901 19:24"
        },
        {
          "id": "MDRW2100025880.1.8",
          "form": "중소형견...키워보고시ㅠ네용",
          "original_form": "중소형견...키워보고시ㅠ네용",
          "speaker_id": "1",
          "time": "20210901 19:24"
        },
        {
          "id": "MDRW2100025880.1.9",
          "form": "아하 저도알거같아요ㅋㅋ",
          "original_form": "아하 저도알거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100025880.1.10",
          "form": "큰것도 좋고 작은것도좋고..",
          "original_form": "큰것도 좋고 작은것도좋고..",
          "speaker_id": "2",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100025880.1.11",
          "form": "섀퍼드같은개도좋아요",
          "original_form": "섀퍼드같은개도좋아요",
          "speaker_id": "2",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100025880.1.12",
          "form": "흐엉진쯔 듬직하니ㅜㅜ",
          "original_form": "흐엉진쯔 듬직하니ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100025880.1.13",
          "form": "ㅋㅋㅋ알러지는 없으세요?",
          "original_form": "ㅋㅋㅋ알러지는 없으세요?",
          "speaker_id": "2",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100025880.1.14",
          "form": "넹넹 그런건 없어요",
          "original_form": "넹넹 그런건 없어요",
          "speaker_id": "1",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100025880.1.15",
          "form": "저는 고양이알러지는 있더라구요ㅠㅠ",
          "original_form": "저는 고양이알러지는 있더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100025880.1.16",
          "form": "그레서 좀 걱정이에요",
          "original_form": "그레서 좀 걱정이에요",
          "speaker_id": "2",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100025880.1.17",
          "form": "아하 고양이는 털이 워낙빠져서ㅜㅜ",
          "original_form": "아하 고양이는 털이 워낙빠져서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100025880.1.18",
          "form": "고양이는 안좋아하세요?",
          "original_form": "고양이는 안좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100025880.1.19",
          "form": "조아해용 ㅋㅋㅋㅋ 다조아해요",
          "original_form": "조아해용 ㅋㅋㅋㅋ 다조아해요",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100025880.1.20",
          "form": "고양이도키우고싶어요",
          "original_form": "고양이도키우고싶어요",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100025880.1.21",
          "form": "같이키우는거로망",
          "original_form": "같이키우는거로망",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100025880.1.22",
          "form": "ㅋㅋㅋㅋ저두요",
          "original_form": "ㅋㅋㅋㅋ저두요",
          "speaker_id": "2",
          "time": "20210901 19:43"
        }
      ]
