        {
          "id": "MDRW2100020641.1.1",
          "form": "오늘 날씨 쥑이네요~!!",
          "original_form": "오늘 날씨 쥑이네요~!!",
          "speaker_id": "2",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020641.1.2",
          "form": "캠핑할맛 나겠구만요~",
          "original_form": "캠핑할맛 나겠구만요~",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020641.1.3",
          "form": "옹 오늘 날씨 좋구먼",
          "original_form": "옹 오늘 날씨 좋구먼",
          "speaker_id": "1",
          "time": "20210828 12:24"
        },
        {
          "id": "MDRW2100020641.1.4",
          "form": "매미 장난아니게 울어대네",
          "original_form": "매미 장난아니게 울어대네",
          "speaker_id": "1",
          "time": "20210828 12:24"
        },
        {
          "id": "MDRW2100020641.1.5",
          "form": "귀가 아프다 ㅋㅋㅋㅋㅋ",
          "original_form": "귀가 아프다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:24"
        },
        {
          "id": "MDRW2100020641.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아여 ㅋㅋㅋㅋ매미바로옆에서 울면 귀따가움 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아여 ㅋㅋㅋㅋ매미바로옆에서 울면 귀따가움 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:25"
        },
        {
          "id": "MDRW2100020641.1.7",
          "form": "어 ㅋㅋㅋㄱ 어제는 비와서 사람 거의 없더니",
          "original_form": "어 ㅋㅋㅋㄱ 어제는 비와서 사람 거의 없더니",
          "speaker_id": "1",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020641.1.8",
          "form": "오늘은 날씨 좋아서 사람들 좀 오네",
          "original_form": "오늘은 날씨 좋아서 사람들 좀 오네",
          "speaker_id": "1",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020641.1.9",
          "form": "햇빛 쨍쨍이구먼",
          "original_form": "햇빛 쨍쨍이구먼",
          "speaker_id": "1",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020641.1.10",
          "form": "아버님 욕나옴 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아버님 욕나옴 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:45"
        },
        {
          "id": "MDRW2100020641.1.11",
          "form": "어제 비 엄청 오는데",
          "original_form": "어제 비 엄청 오는데",
          "speaker_id": "1",
          "time": "20210828 12:45"
        },
        {
          "id": "MDRW2100020641.1.12",
          "form": "텐트못쳐가지고",
          "original_form": "텐트못쳐가지고",
          "speaker_id": "1",
          "time": "20210828 12:45"
        },
        {
          "id": "MDRW2100020641.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ손에 익을라면 또 열심히 댕겨야 겠구먼요~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ손에 익을라면 또 열심히 댕겨야 겠구먼요~",
          "speaker_id": "2",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100020641.1.14",
          "form": "문구사 가서 첫찌 준비물 사야는데..못나가겠네여 ㅋㅋㅋㅋ",
          "original_form": "문구사 가서 첫찌 준비물 사야는데..못나가겠네여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100020641.1.15",
          "form": "ㅋㅋㅋㅋㅋ 덥다이",
          "original_form": "ㅋㅋㅋㅋㅋ 덥다이",
          "speaker_id": "1",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100020641.1.16",
          "form": "애들 물놀이 하기에는 좋은날씬데",
          "original_form": "애들 물놀이 하기에는 좋은날씬데",
          "speaker_id": "1",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100020641.1.17",
          "form": "비오고 가을 올거 같았는데 ㅋㅋㅋㅋㅋ",
          "original_form": "비오고 가을 올거 같았는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100020641.1.18",
          "form": "다시 여름이네여 ㅋㅋㅋ",
          "original_form": "다시 여름이네여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100020641.1.19",
          "form": "글타 다시 덥다잉",
          "original_form": "글타 다시 덥다잉",
          "speaker_id": "1",
          "time": "20210828 16:13"
        },
        {
          "id": "MDRW2100020641.1.20",
          "form": "같이 놀아도 우리투만 시커매지는 느낌...",
          "original_form": "같이 놀아도 우리투만 시커매지는 느낌...",
