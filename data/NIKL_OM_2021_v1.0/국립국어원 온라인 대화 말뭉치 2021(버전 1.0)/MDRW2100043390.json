      "utterance": [
        {
          "id": "MDRW2100043390.1.1",
          "form": "이상형이 어떻게돼?",
          "original_form": "이상형이 어떻게돼?",
          "speaker_id": "2",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100043390.1.2",
          "form": "외모?",
          "original_form": "외모?",
          "speaker_id": "1",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100043390.1.3",
          "form": "외모랑 성격 둘 다",
          "original_form": "외모랑 성격 둘 다",
          "speaker_id": "2",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100043390.1.4",
          "form": "외모는.. 일단 키가 좀 작으면 좋겠어",
          "original_form": "외모는.. 일단 키가 좀 작으면 좋겠어",
          "speaker_id": "1",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100043390.1.5",
          "form": "좀 보호해주고싶은 스타일이 취향이구나",
          "original_form": "좀 보호해주고싶은 스타일이 취향이구나",
          "speaker_id": "2",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100043390.1.6",
          "form": "어 그리고 마른게 좋아",
          "original_form": "어 그리고 마른게 좋아",
          "speaker_id": "1",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100043390.1.7",
          "form": "귀엽겠네",
          "original_form": "귀엽겠네",
          "speaker_id": "2",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100043390.1.8",
          "form": "얼굴은 걍 내가 봤을때 이쁘면돼",
          "original_form": "얼굴은 걍 내가 봤을때 이쁘면돼",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100043390.1.9",
          "form": "너무 애매하잔아ㅋㅋ",
          "original_form": "너무 애매하잔아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100043390.1.10",
          "form": "여우상?",
          "original_form": "여우상?",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100043390.1.11",
          "form": "아맞다 너 여우상 좋아하지",
          "original_form": "아맞다 너 여우상 좋아하지",
          "speaker_id": "2",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100043390.1.12",
          "form": "그리고 성격은 좀 애교스러웠음 좋겠어",
          "original_form": "그리고 성격은 좀 애교스러웠음 좋겠어",
          "speaker_id": "1",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100043390.1.13",
          "form": "아 확실히 애교있는애가 좋지",
          "original_form": "아 확실히 애교있는애가 좋지",
          "speaker_id": "2",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100043390.1.14",
          "form": "그리고 눈치가 있어야돼",
          "original_form": "그리고 눈치가 있어야돼",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043390.1.15",
          "form": "아 인정 곰이랑은 못살어",
          "original_form": "아 인정 곰이랑은 못살어",
          "speaker_id": "2",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043390.1.16",
          "form": "깔끔떠는건 노우...",
          "original_form": "깔끔떠는건 노우...",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043390.1.17",
          "form": "아니 왜?ㅋㅋ",
          "original_form": "아니 왜?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043390.1.18",
          "form": "내가 지저분하니깐ㅋ",
          "original_form": "내가 지저분하니깐ㅋ",
          "speaker_id": "1",
          "time": "20210907 18:54"
        }
