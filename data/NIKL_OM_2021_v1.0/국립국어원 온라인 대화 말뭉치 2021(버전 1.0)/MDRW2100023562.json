      "utterance": [
        {
          "id": "MDRW2100023562.1.1",
          "form": "너꺼?",
          "original_form": "너꺼?",
          "speaker_id": "2",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023562.1.2",
          "form": "쓰려고해ㅛ는데 배민1 막혀서 못썼어",
          "original_form": "쓰려고해ㅛ는데 배민1 막혀서 못썼어",
          "speaker_id": "2",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023562.1.3",
          "form": "너 아직 첫구매쿠폰있더라?",
          "original_form": "너 아직 첫구매쿠폰있더라?",
          "speaker_id": "2",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023562.1.4",
          "form": "응 내꺼 삼천원 나온거",
          "original_form": "응 내꺼 삼천원 나온거",
          "speaker_id": "1",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023562.1.5",
          "form": "왜 막혀?",
          "original_form": "왜 막혀?",
          "speaker_id": "1",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023562.1.6",
          "form": "전화해서 물어봤는데",
          "original_form": "전화해서 물어봤는데",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023562.1.7",
          "form": "아 나 첫구매 쿠폰도 있어?",
          "original_form": "아 나 첫구매 쿠폰도 있어?",
          "speaker_id": "1",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023562.1.8",
          "form": "배민1이 쿠팡이츠같은거라서",
          "original_form": "배민1이 쿠팡이츠같은거라서",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023562.1.9",
          "form": "응 그것도 안썼어아직",
          "original_form": "응 그것도 안썼어아직",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023562.1.10",
          "form": "이제 이벤트 끝난거야?\n오늘은 이벤트 안뜨네",
          "original_form": "이제 이벤트 끝난거야?\n오늘은 이벤트 안뜨네",
          "speaker_id": "1",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023562.1.11",
          "form": "배달기사가 그 시간대에 부족해서 막힌거같다 하더라구",
          "original_form": "배달기사가 그 시간대에 부족해서 막힌거같다 하더라구",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023562.1.12",
          "form": "응 끝났어ㅋㅋ",
          "original_form": "응 끝났어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023562.1.13",
          "form": "아 막힐수도 있구나",
          "original_form": "아 막힐수도 있구나",
          "speaker_id": "1",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023562.1.14",
          "form": "그래서 못시켜먹었구나?",
          "original_form": "그래서 못시켜먹었구나?",
          "speaker_id": "1",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023562.1.15",
          "form": "아냐 그냥 배민1말고 일반으로 시켜먹음^^ㅋㅋ",
          "original_form": "아냐 그냥 배민1말고 일반으로 시켜먹음^^ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023562.1.16",
          "form": "아 시켜먹긴 먹었구낰ㅋㅋ",
          "original_form": "아 시켜먹긴 먹었구낰ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023562.1.17",
          "form": "뭐 먹었어?",
          "original_form": "뭐 먹었어?",
          "speaker_id": "1",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023562.1.18",
          "form": "숙성회!ㅋㅋㅋ",
          "original_form": "숙성회!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023562.1.19",
          "form": "아 숙성회 드디어  먹었구나?",
          "original_form": "아 숙성회 드디어  먹었구나?",
