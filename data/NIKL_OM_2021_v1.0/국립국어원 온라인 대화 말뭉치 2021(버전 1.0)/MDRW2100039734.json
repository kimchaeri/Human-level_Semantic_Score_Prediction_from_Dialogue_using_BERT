      "utterance": [
        {
          "id": "MDRW2100039734.1.1",
          "form": "동물 키우세요?",
          "original_form": "동물 키우세요?",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100039734.1.2",
          "form": "동물 키우시나요?",
          "original_form": "동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039734.1.3",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100039734.1.4",
          "form": "예전엔 새 키우긴했어요",
          "original_form": "예전엔 새 키우긴했어요",
          "speaker_id": "2",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100039734.1.5",
          "form": "name1님은 키우시나요?",
          "original_form": "&name1&님은 키우시나요?",
          "speaker_id": "2",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100039734.1.6",
          "form": "저는 항상 생각만요 ㅎ",
          "original_form": "저는 항상 생각만요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100039734.1.7",
          "form": "새는 키우기 편한가요?",
          "original_form": "새는 키우기 편한가요?",
          "speaker_id": "1",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100039734.1.8",
          "form": "손이 많이 가진 않는데",
          "original_form": "손이 많이 가진 않는데",
          "speaker_id": "2",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100039734.1.9",
          "form": "교류할 일도 없고",
          "original_form": "교류할 일도 없고",
          "speaker_id": "2",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100039734.1.10",
          "form": "아 ᆢ 그게 단점이네요",
          "original_form": "아 ᆢ 그게 단점이네요",
          "speaker_id": "1",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100039734.1.11",
          "form": "그냥 평생 서먹한 사이였어요ㅋㅋㅋ",
          "original_form": "그냥 평생 서먹한 사이였어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100039734.1.12",
          "form": "사이좋으면 핸들링?도 하더라구요",
          "original_form": "사이좋으면 핸들링?도 하더라구요",
          "speaker_id": "2",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100039734.1.13",
          "form": "무슨 종이었나요?",
          "original_form": "무슨 종이었나요?",
          "speaker_id": "1",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100039734.1.14",
          "form": "새장에서 새 꺼내서 손에 얹고",
          "original_form": "새장에서 새 꺼내서 손에 얹고",
          "speaker_id": "2",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100039734.1.15",
          "form": "문조였어요!",
          "original_form": "문조였어요!",
          "speaker_id": "2",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100039734.1.16",
          "form": "신기해라",
          "original_form": "신기해라",
          "speaker_id": "1",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100039734.1.17",
          "form": "문조 귀여울거같아요",
          "original_form": "문조 귀여울거같아요",
          "speaker_id": "1",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100039734.1.18",
          "form": "참새만한 새인데",
          "original_form": "참새만한 새인데",
          "speaker_id": "2",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100039734.1.19",
          "form": "볼이 통통해서 귀여워요",
          "original_form": "볼이 통통해서 귀여워요",
          "speaker_id": "2",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100039734.1.20",
          "form": "귀엽겠어요ㅜㅜㅜ",
          "original_form": "귀엽겠어요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100039734.1.21",
          "form": "몇년키우셨어요?",
          "original_form": "몇년키우셨어요?",
          "speaker_id": "1",
          "time": "20210903 21:07"
        },
        {
          "id": "MDRW2100039734.1.22",
          "form": "정확히 기억은 안나는데 3-4년 정도 키웠던 것 같아요!",
          "original_form": "정확히 기억은 안나는데 3-4년 정도 키웠던 것 같아요!",
          "speaker_id": "2",
          "time": "20210903 21:07"
        },
        {
          "id": "MDRW2100039734.1.23",
          "form": "오 오래키우셨네요",
          "original_form": "오 오래키우셨네요",
          "speaker_id": "1",
          "time": "20210903 21:07"
        }
