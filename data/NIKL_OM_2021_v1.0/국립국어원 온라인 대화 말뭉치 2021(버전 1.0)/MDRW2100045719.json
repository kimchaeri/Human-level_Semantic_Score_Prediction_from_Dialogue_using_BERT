      "utterance": [
        {
          "id": "MDRW2100045719.1.1",
          "form": "맞아요ㅠㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:01"
        },
        {
          "id": "MDRW2100045719.1.2",
          "form": "진짜 밑도끝도없이 말할 수 있어요",
          "original_form": "진짜 밑도끝도없이 말할 수 있어요",
          "speaker_id": "2",
          "time": "20210908 06:01"
        },
        {
          "id": "MDRW2100045719.1.3",
          "form": "맞아요. 저 지금ㅋㅋ 튀김하니까 급 케이에프씨가 먹고 싶어져버렸지 뭐에욬ㅋㅋㅋ",
          "original_form": "맞아요. 저 지금ㅋㅋ 튀김하니까 급 케이에프씨가 먹고 싶어져버렸지 뭐에욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:02"
        },
        {
          "id": "MDRW2100045719.1.4",
          "form": "엄청 건너뛰기 같은 느낌이지만욬ㅋㅋ",
          "original_form": "엄청 건너뛰기 같은 느낌이지만욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:03"
        },
        {
          "id": "MDRW2100045719.1.5",
          "form": "와앜ㅋㅋㅋ넘 맛있겠다ㅠㅠㅠ",
          "original_form": "와앜ㅋㅋㅋ넘 맛있겠다ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:04"
        },
        {
          "id": "MDRW2100045719.1.6",
          "form": "저도 먹고싶어졌어요! 책임지세요ㅠㅠㅠ",
          "original_form": "저도 먹고싶어졌어요! 책임지세요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:04"
        },
        {
          "id": "MDRW2100045719.1.7",
          "form": "간식으로 오떠신가요?!",
          "original_form": "간식으로 오떠신가요?!",
          "speaker_id": "1",
          "time": "20210908 06:04"
        },
        {
          "id": "MDRW2100045719.1.8",
          "form": "달려갑니다 날 밝으면ㅠㅠㅠ",
          "original_form": "달려갑니다 날 밝으면ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:05"
        },
        {
          "id": "MDRW2100045719.1.9",
          "form": "이제 몇시간 안남았어요!!",
          "original_form": "이제 몇시간 안남았어요!!",
          "speaker_id": "1",
          "time": "20210908 06:06"
        },
        {
          "id": "MDRW2100045719.1.10",
          "form": "가게들 더 일찍 열었으몀 좋았을텐데 왜 최소 10시인지ㅠㅠㅠ",
          "original_form": "가게들 더 일찍 열었으몀 좋았을텐데 왜 최소 10시인지ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:06"
        },
        {
          "id": "MDRW2100045719.1.11",
          "form": "그니까요ㅠㅠㅠㅠㅠ",
          "original_form": "그니까요ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:07"
        },
        {
          "id": "MDRW2100045719.1.12",
          "form": "이렇게 대기타는 내가 있는데!",
          "original_form": "이렇게 대기타는 내가 있는데!",
          "speaker_id": "2",
          "time": "20210908 06:08"
        },
        {
          "id": "MDRW2100045719.1.13",
          "form": "24시간 가게들도 요즘은 다 닫잖아요ㅠㅠㅠ0",
          "original_form": "24시간 가게들도 요즘은 다 닫잖아요ㅠㅠㅠ0",
          "speaker_id": "1",
          "time": "20210908 06:08"
        },
        {
          "id": "MDRW2100045719.1.14",
          "form": "진짜 너무 슬퍼요ㅠㅠㅠ",
          "original_form": "진짜 너무 슬퍼요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:08"
        },
        {
          "id": "MDRW2100045719.1.15",
          "form": "흑흑ㅠㅠ 코로나때문에 더 그런 거 같아요",
          "original_form": "흑흑ㅠㅠ 코로나때문에 더 그런 거 같아요",
          "speaker_id": "2",
          "time": "20210908 06:09"
        },
        {
          "id": "MDRW2100045719.1.16",
          "form": "넘 슬프다...",
          "original_form": "넘 슬프다...",
          "speaker_id": "2",
          "time": "20210908 06:09"
        },
        {
          "id": "MDRW2100045719.1.17",
          "form": "맞아요ㅠㅠㅠㅠ 아직 못가본 가게들이 문을 닫고....ㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠㅠ 아직 못가본 가게들이 문을 닫고....ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:09"
        },
        {
          "id": "MDRW2100045719.1.18",
          "form": "흑흑ㅠㅠㅠ 날 두고 떠나는 수많은 맛집들ㅠㅠㅜ",
          "original_form": "흑흑ㅠㅠㅠ 날 두고 떠나는 수많은 맛집들ㅠㅠㅜ",
          "speaker_id": "2",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100045719.1.19",
          "form": "코로나 빨리 종식되면 좋겠어요ㅠㅠ",
          "original_form": "코로나 빨리 종식되면 좋겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100045719.1.20",
          "form": "맞아요ㅠㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100045719.1.21",
          "form": "코로나가 끝나야 이동해가면서 맛집엘 가는대ㅠㅠㅠㅠ",
          "original_form": "코로나가 끝나야 이동해가면서 맛집엘 가는대ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100045719.1.22",
          "form": "영영맛볼 수 없는 곳들을 샹각허면...ㅂㄷㅂㄷ...",
          "original_form": "영영맛볼 수 없는 곳들을 샹각허면...ㅂㄷㅂㄷ...",
          "speaker_id": "1",
          "time": "20210908 06:12"
        },
        {
          "id": "MDRW2100045719.1.23",
          "form": "아아ㅠㅠㅠ",
          "original_form": "아아ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:13"
        },
        {
          "id": "MDRW2100045719.1.24",
          "form": "아 헐 갑자기 생각났는데",
          "original_form": "아 헐 갑자기 생각났는데",
          "speaker_id": "2",
          "time": "20210908 06:13"
        },
        {
          "id": "MDRW2100045719.1.25",
          "form": "동빙고 팥빙수 꼭 드세요",
          "original_form": "동빙고 팥빙수 꼭 드세요",
          "speaker_id": "2",
          "time": "20210908 06:13"
        },
        {
          "id": "MDRW2100045719.1.26",
          "form": "제 인생빙수예요",
          "original_form": "제 인생빙수예요",
          "speaker_id": "2",
          "time": "20210908 06:13"
        },
        {
          "id": "MDRW2100045719.1.27",
          "form": "바로 메모해두겠습니다.",
          "original_form": "바로 메모해두겠습니다.",
          "speaker_id": "1",
          "time": "20210908 06:14"
        },
        {
          "id": "MDRW2100045719.1.28",
          "form": "네 꼭이요ㅠㅠ",
          "original_form": "네 꼭이요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:14"
        },
        {
          "id": "MDRW2100045719.1.29",
          "form": "미리 전화해서 포장예약하면",
          "original_form": "미리 전화해서 포장예약하면",
          "speaker_id": "2",
          "time": "20210908 06:14"
        },
        {
          "id": "MDRW2100045719.1.30",
          "form": "얼려두시거든요",
          "original_form": "얼려두시거든요",
          "speaker_id": "2",
          "time": "20210908 06:14"
        },
        {
          "id": "MDRW2100045719.1.31",
          "form": "거기 뚜벅이는 너무 불편하고",
          "original_form": "거기 뚜벅이는 너무 불편하고",
          "speaker_id": "2",
          "time": "20210908 06:15"
        },
        {
          "id": "MDRW2100045719.1.32",
          "form": "자차있으시면 포장해서 드세요!",
          "original_form": "자차있으시면 포장해서 드세요!",
          "speaker_id": "2",
          "time": "20210908 06:15"
        },
        {
          "id": "MDRW2100045719.1.33",
          "form": "넵넵! 꼭 그럴게요!!",
          "original_form": "넵넵! 꼭 그럴게요!!",
          "speaker_id": "1",
          "time": "20210908 06:16"
        },
        {
          "id": "MDRW2100045719.1.34",
          "form": "빙수하니까 설빙도 가고싶어지네요ㅠㅠ",
          "original_form": "빙수하니까 설빙도 가고싶어지네요ㅠㅠ",
