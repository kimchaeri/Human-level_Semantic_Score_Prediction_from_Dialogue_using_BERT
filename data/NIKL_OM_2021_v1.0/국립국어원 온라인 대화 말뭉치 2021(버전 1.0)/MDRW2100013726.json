        {
          "id": "MDRW2100013726.1.14",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013726.1.15",
          "form": "이사가고나서바꾸죠",
          "original_form": "이사가고나서바꾸죠",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013726.1.16",
          "form": "드럼으로바꾸고싶다",
          "original_form": "드럼으로바꾸고싶다",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013726.1.17",
          "form": "그래통돌이말고",
