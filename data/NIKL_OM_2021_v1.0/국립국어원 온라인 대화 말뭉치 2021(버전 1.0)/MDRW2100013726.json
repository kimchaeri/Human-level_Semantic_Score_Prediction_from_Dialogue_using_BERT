        {
          "id": "MDRW2100013726.1.1",
          "form": "엄마 청소 열심히 할게요",
          "original_form": "엄마 청소 열심히 할게요",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013726.1.2",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013726.1.3",
          "form": "니는니방만어떻게해라",
          "original_form": "니는니방만어떻게해라",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013726.1.4",
          "form": "엄마가먼저안하면되는데",
          "original_form": "엄마가먼저안하면되는데",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013726.1.5",
          "form": "먼저하는게 어렵다",
          "original_form": "먼저하는게 어렵다",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013726.1.6",
          "form": "네ㅠ그래도 빨래는 도와주자나여",
          "original_form": "네ㅠ그래도 빨래는 도와주자나여",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013726.1.7",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013726.1.8",
          "form": "그래야지",
          "original_form": "그래야지",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013726.1.9",
          "form": "맞져그래야져ㅋㅋㅋ",
          "original_form": "맞져그래야져ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013726.1.10",
          "form": "근데우리집세탁기너무높아",
          "original_form": "근데우리집세탁기너무높아",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013726.1.11",
          "form": "ㅋㅋㅋ그래맞다",
          "original_form": "ㅋㅋㅋ그래맞다",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013726.1.12",
          "form": "바꿔야하는데",
          "original_form": "바꿔야하는데",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013726.1.13",
          "form": "굳이",
          "original_form": "굳이",
          "speaker_id": "2",
          "time": "20210825 15:56"
