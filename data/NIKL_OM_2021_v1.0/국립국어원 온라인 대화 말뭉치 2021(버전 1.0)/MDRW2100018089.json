      "utterance": [
        {
          "id": "MDRW2100018089.1.1",
          "form": "먹장 자주보시나여?",
          "original_form": "먹장 자주보시나여?",
          "speaker_id": "1",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018089.1.2",
          "form": "먹장이뭔가요 ㅋㅋ",
          "original_form": "먹장이뭔가요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018089.1.3",
          "form": "아.. 아이디 보다가 헷갈림... 먹방이요",
          "original_form": "아.. 아이디 보다가 헷갈림... 먹방이요",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018089.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 먹방은 잘 안봐요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 먹방은 잘 안봐요!",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018089.1.5",
          "form": "제가먹는걸 더 좋아해요...",
          "original_form": "제가먹는걸 더 좋아해요...",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018089.1.6",
          "form": "아.. ㅋㅋㅋㅋ그럼  유튜브 주로 뭐 보세요?",
          "original_form": "아.. ㅋㅋㅋㅋ그럼  유튜브 주로 뭐 보세요?",
          "speaker_id": "1",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018089.1.7",
          "form": "음 ! 패션 ??? 노래 이런거랑 각종 취미생활같은거 찾아봐요",
          "original_form": "음 ! 패션 ??? 노래 이런거랑 각종 취미생활같은거 찾아봐요",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018089.1.8",
          "form": "브이로그도",
          "original_form": "브이로그도",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018089.1.9",
          "form": "패션이면? 룩북 같은거여?",
          "original_form": "패션이면? 룩북 같은거여?",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018089.1.10",
          "form": "막 자라 패션하울 이런거요 ㅋㅋ",
          "original_form": "막 자라 패션하울 이런거요 ㅋㅋ",
