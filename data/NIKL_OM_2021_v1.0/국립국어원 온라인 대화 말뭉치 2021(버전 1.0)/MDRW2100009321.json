      "utterance": [
        {
          "id": "MDRW2100009321.1.1",
          "form": "스포츠좋아하시나요?",
          "original_form": "스포츠좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100009321.1.2",
          "form": "보는거 좋아해요!",
          "original_form": "보는거 좋아해요!",
          "speaker_id": "2",
          "time": "20210822 22:59"
        },
        {
          "id": "MDRW2100009321.1.3",
          "form": "직접가서 보기도 하세요?",
          "original_form": "직접가서 보기도 하세요?",
          "speaker_id": "1",
          "time": "20210822 22:59"
        },
        {
          "id": "MDRW2100009321.1.4",
          "form": "그랬는데 요새 코로나가. .",
          "original_form": "그랬는데 요새 코로나가. .",
          "speaker_id": "2",
          "time": "20210822 23:00"
        },
        {
          "id": "MDRW2100009321.1.5",
          "form": "요즘 야구는 무관중인가요?",
          "original_form": "요즘 야구는 무관중인가요?",
          "speaker_id": "1",
          "time": "20210822 23:00"
        },
        {
          "id": "MDRW2100009321.1.6",
          "form": "관중은 있을텐데 아무래도...",
          "original_form": "관중은 있을텐데 아무래도...",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009321.1.7",
          "form": "아 관람은 가능하군요",
          "original_form": "아 관람은 가능하군요",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009321.1.8",
          "form": "요즘 야구티켓 가격이 얼마나해요?",
          "original_form": "요즘 야구티켓 가격이 얼마나해요?",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009321.1.9",
          "form": "네 가고싶긴해요",
          "original_form": "네 가고싶긴해요",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009321.1.10",
          "form": "꽤 비싸죠",
          "original_form": "꽤 비싸죠",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009321.1.11",
          "form": "외야도 만원할껄요",
          "original_form": "외야도 만원할껄요",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009321.1.12",
          "form": "경기보러 가본적이 없어서 ㅎ",
          "original_form": "경기보러 가본적이 없어서 ㅎ",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009321.1.13",
          "form": "만원이면 싼편아닌가요ㅎ ㅁ",
          "original_form": "만원이면 싼편아닌가요ㅎ ㅁ",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009321.1.14",
          "form": "영화관이 요즘 13000원",
          "original_form": "영화관이 요즘 13000원",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009321.1.15",
          "form": "근데 쩌~~~멀리잖아요 ㅎㅎ",
          "original_form": "근데 쩌~~~멀리잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009321.1.16",
          "form": "ㅋㅋ 글킨하죠",
          "original_form": "ㅋㅋ 글킨하죠",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009321.1.17",
          "form": "현장감만 느낄수있는 ㅋ",
          "original_form": "현장감만 느낄수있는 ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009321.1.18",
          "form": "그건 그래요!",
          "original_form": "그건 그래요!",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009321.1.19",
          "form": "야구장 축구장 딱 한번씩 직관해봤네요",
          "original_form": "야구장 축구장 딱 한번씩 직관해봤네요",
          "speaker_id": "1",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009321.1.20",
          "form": "어릴때 농구장은 많이 가봤는데",
          "original_form": "어릴때 농구장은 많이 가봤는데",
