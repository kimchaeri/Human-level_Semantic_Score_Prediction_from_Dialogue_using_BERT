      "utterance": [
        {
          "id": "MDRW2100004406.1.1",
          "form": "좋아하는 게임이 있나요?",
          "original_form": "좋아하는 게임이 있나요?",
          "speaker_id": "1",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004406.1.2",
          "form": "음 예전에는 핸드폰으로 쿠키런?!",
          "original_form": "음 예전에는 핸드폰으로 쿠키런?!",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004406.1.3",
          "form": "했던것같아요~ 요즘은 잘 안해서 모르겠어요ㅠ",
          "original_form": "했던것같아요~ 요즘은 잘 안해서 모르겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004406.1.4",
          "form": "저도 쿠키런 했었는데 요즘엔 안 하고 있어요",
          "original_form": "저도 쿠키런 했었는데 요즘엔 안 하고 있어요",
          "speaker_id": "1",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004406.1.5",
          "form": "그냥 퍼즐게임도 재밌더라구요",
          "original_form": "그냥 퍼즐게임도 재밌더라구요",
          "speaker_id": "1",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004406.1.6",
          "form": "오오 이름이 뭔가용~?",
          "original_form": "오오 이름이 뭔가용~?",
          "speaker_id": "2",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004406.1.7",
          "form": "오버액션 토끼 캐릭터가 나오는 퍼즐게임이요!",
          "original_form": "오버액션 토끼 캐릭터가 나오는 퍼즐게임이요!",
          "speaker_id": "1",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004406.1.8",
          "form": "해리포터 퍼즐도 재밌어요",
          "original_form": "해리포터 퍼즐도 재밌어요",
          "speaker_id": "1",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004406.1.9",
          "form": "퍼즐게임 좋아하나요?",
          "original_form": "퍼즐게임 좋아하나요?",
          "speaker_id": "1",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004406.1.10",
          "form": "어 저도 오버액션해본것같아요~",
          "original_form": "어 저도 오버액션해본것같아요~",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004406.1.11",
          "form": "그냥 가리지않고 한번씩은 해보는것같아요!!",
          "original_form": "그냥 가리지않고 한번씩은 해보는것같아요!!",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004406.1.12",
          "form": "제일 재밌게 했던 게임이 뭔가요?",
          "original_form": "제일 재밌게 했던 게임이 뭔가요?",
          "speaker_id": "1",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004406.1.13",
          "form": "아 저는 카이로소프트아세여?",
          "original_form": "아 저는 카이로소프트아세여?",
          "speaker_id": "2",
          "time": "20210821 16:23"
        },
