      "utterance": [
        {
          "id": "MDRW2100015406.1.1",
          "form": "오늘은 화장실 청소를 했습니다...",
          "original_form": "오늘은 화장실 청소를 했습니다...",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015406.1.2",
          "form": "너무 힘들었어~~",
          "original_form": "너무 힘들었어~~",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015406.1.3",
          "form": "하고 바로 누움 ㅠㅠ",
          "original_form": "하고 바로 누움 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015406.1.4",
          "form": "그렇게 더러웠어?",
          "original_form": "그렇게 더러웠어?",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015406.1.5",
          "form": "더러웠냐니ㅠㅠ 그렇진 않은데 혼자 다 청소하려니 힘드네",
          "original_form": "더러웠냐니ㅠㅠ 그렇진 않은데 혼자 다 청소하려니 힘드네",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015406.1.6",
          "form": "하하. 그동안 엄마가 얼마나 고생했는지 알겠지.",
          "original_form": "하하. 그동안 엄마가 얼마나 고생했는지 알겠지.",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015406.1.7",
          "form": "원래 집안일이 힘든 거야.",
          "original_form": "원래 집안일이 힘든 거야.",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015406.1.8",
          "form": "그래 엄마가 고생 많았네",
          "original_form": "그래 엄마가 고생 많았네",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015406.1.9",
          "form": "이 힘든 걸 어떻게 혼자 다 했대.",
          "original_form": "이 힘든 걸 어떻게 혼자 다 했대.",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015406.1.10",
          "form": "엄마가 최고다~~",
          "original_form": "엄마가 최고다~~",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015406.1.11",
          "form": "알면 좀 잘해봐.",
          "original_form": "알면 좀 잘해봐.",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015406.1.12",
          "form": "본가에 좀 자주 들리고.",
          "original_form": "본가에 좀 자주 들리고.",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015406.1.13",
          "form": "일주일에 한번씩은 꼬박꼬박 가는데 뭘",
          "original_form": "일주일에 한번씩은 꼬박꼬박 가는데 뭘",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015406.1.14",
          "form": "딸이라도 너무 자주보면 지겨울걸?ㅋㅋㅋ",
          "original_form": "딸이라도 너무 자주보면 지겨울걸?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015406.1.15",
          "form": "전화 매일 하잖아",
          "original_form": "전화 매일 하잖아",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015406.1.16",
          "form": "아빠도 그렇고 할머니도 그러시고 다 너 보고 싶어하셔.",
          "original_form": "아빠도 그렇고 할머니도 그러시고 다 너 보고 싶어하셔.",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015406.1.17",
          "form": "주말마다 봐도 부족하지.",
          "original_form": "주말마다 봐도 부족하지.",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015406.1.18",
          "form": "고슴도치도 자기 자식은 예쁘다던데 내가 그 고슴도치 자식이엇구만...",
          "original_form": "고슴도치도 자기 자식은 예쁘다던데 내가 그 고슴도치 자식이엇구만...",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015406.1.19",
          "form": "웃기당",
          "original_form": "웃기당",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015406.1.20",
          "form": "잠깐 들러서 밥만 먹고 티비 보다 가도 괜찮으니까 심심하면 자주 들러.",
          "original_form": "잠깐 들러서 밥만 먹고 티비 보다 가도 괜찮으니까 심심하면 자주 들러.",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015406.1.21",
          "form": "알겠습니다~ 사랑받는 딸이라 행복하네요~~",
          "original_form": "알겠습니다~ 사랑받는 딸이라 행복하네요~~",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015406.1.22",
          "form": "아 내일 아빠한테 잠깐 내 집에 와줄수 있는지 물어봐줘",
          "original_form": "아 내일 아빠한테 잠깐 내 집에 와줄수 있는지 물어봐줘",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015406.1.23",
          "form": "부탁할 거 있어",
          "original_form": "부탁할 거 있어",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015406.1.24",
          "form": "뭐 부탁하게? 아빠 내일 오전에 시간 될 거야.",
          "original_form": "뭐 부탁하게? 아빠 내일 오전에 시간 될 거야.",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015406.1.25",
          "form": "오전 괜찮아. 나 그때 집에 있을 거야!",
          "original_form": "오전 괜찮아. 나 그때 집에 있을 거야!",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015406.1.26",
          "form": "책상이 덜컹거랴서 한번 봐달라고 하게.",
          "original_form": "책상이 덜컹거랴서 한번 봐달라고 하게.",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015406.1.27",
          "form": "오늘도 빨래했다ㅠㅠ",
          "original_form": "오늘도 빨래했다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100015406.1.28",
          "form": "집안일은 해도해도 끝이 없네..",
          "original_form": "집안일은 해도해도 끝이 없네..",
          "speaker_id": "2",
          "time": "20210826 23:41"
        }
      ]
