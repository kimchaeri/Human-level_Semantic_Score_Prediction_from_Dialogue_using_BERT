      "utterance": [
        {
          "id": "MDRW2100031929.1.1",
          "form": "ㅎㅎ 저도 잘몰랐어요ㅎ 근데 군대랑 비슷하게 학교가 군기가있어서 똑같애요ㅎㅎ",
          "original_form": "ㅎㅎ 저도 잘몰랐어요ㅎ 근데 군대랑 비슷하게 학교가 군기가있어서 똑같애요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100031929.1.2",
          "form": "그냥 군대안가고 돈벌었다 정도?",
          "original_form": "그냥 군대안가고 돈벌었다 정도?",
          "speaker_id": "1",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100031929.1.3",
          "form": "아 그럼 군대를 학교로 다닌거네요",
          "original_form": "아 그럼 군대를 학교로 다닌거네요",
          "speaker_id": "2",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100031929.1.4",
          "form": "기숙사죠?",
          "original_form": "기숙사죠?",
          "speaker_id": "2",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100031929.1.5",
          "form": "암요ㅎㅎㅎ",
          "original_form": "암요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100031929.1.6",
          "form": "전원기숙사생활에ㅎ 매일 아침구보에ㅎ 옷은 칼쥬름해서 다녀야해요ㅎ",
          "original_form": "전원기숙사생활에ㅎ 매일 아침구보에ㅎ 옷은 칼쥬름해서 다녀야해요ㅎ",
          "speaker_id": "1",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100031929.1.7",
          "form": "인사도해야하구요 ㅡㅎㅎㅎ",
          "original_form": "인사도해야하구요 ㅡㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100031929.1.8",
          "form": "아.. ㅋㅋ 그럼 그게 군대잖아요ㅋㅋ",
          "original_form": "아.. ㅋㅋ 그럼 그게 군대잖아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100031929.1.9",
          "form": "ㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100031929.1.10",
          "form": "맞습니당ㅎ 군대죠뭐ㅎㅎ",
          "original_form": "맞습니당ㅎ 군대죠뭐ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100031929.1.11",
          "form": "이제 안부럽습니다ㅋㅋ",
          "original_form": "이제 안부럽습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100031929.1.12",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100031929.1.13",
          "form": "4년내내 머리짧게짤랐아요ㅎ",
          "original_form": "4년내내 머리짧게짤랐아요ㅎ",
          "speaker_id": "1",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100031929.1.14",
          "form": "ㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100031929.1.15",
          "form": "여자도 머리 규정이있음요ㅎ",
          "original_form": "여자도 머리 규정이있음요ㅎ",
          "speaker_id": "1",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100031929.1.16",
          "form": "전 동생 면회 열번인가 갔어요",
          "original_form": "전 동생 면회 열번인가 갔어요",
          "speaker_id": "2",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100031929.1.17",
          "form": "단발에 1학년때는 화장하면안되고",
          "original_form": "단발에 1학년때는 화장하면안되고",
          "speaker_id": "1",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100031929.1.18",
          "form": "2학년때는 틴트정도?ㅎㅎ",
          "original_form": "2학년때는 틴트정도?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100031929.1.19",
          "form": "오옹ㅎㅎ",
          "original_form": "오옹ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100031929.1.20",
          "form": "면회열번이면 엄청많이가셨네요",
          "original_form": "면회열번이면 엄청많이가셨네요",
          "speaker_id": "1",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100031929.1.21",
          "form": "ㅋㅋ 전방이고 그때 총기난사사건있고 직후라",
          "original_form": "ㅋㅋ 전방이고 그때 총기난사사건있고 직후라",
          "speaker_id": "2",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100031929.1.22",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100031929.1.23",
          "form": "애고 하필그시기에",
          "original_form": "애고 하필그시기에",
          "speaker_id": "1",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100031929.1.24",
          "form": "걱정많이되셨겟네여",
          "original_form": "걱정많이되셨겟네여",
          "speaker_id": "1",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100031929.1.25",
          "form": "짠해서 자주 가줬는데 고마워하지않더라고요",
          "original_form": "짠해서 자주 가줬는데 고마워하지않더라고요",
          "speaker_id": "2",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100031929.1.26",
          "form": "당연하게 생각하고ㅋㅋ",
          "original_form": "당연하게 생각하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100031929.1.27",
          "form": "ㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:08"
        },
        {
          "id": "MDRW2100031929.1.28",
          "form": "가족들은 그렇다면서요?",
          "original_form": "가족들은 그렇다면서요?",
          "speaker_id": "1",
          "time": "20210905 09:08"
        },
        {
          "id": "MDRW2100031929.1.29",
          "form": "여자친구오면 엄청 좋아하고?",
          "original_form": "여자친구오면 엄청 좋아하고?",
