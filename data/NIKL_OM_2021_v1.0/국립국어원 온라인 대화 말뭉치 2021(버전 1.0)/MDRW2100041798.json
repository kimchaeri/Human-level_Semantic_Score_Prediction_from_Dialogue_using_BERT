      "utterance": [
        {
          "id": "MDRW2100041798.1.1",
          "form": "그래 가지고요",
          "original_form": "그래 가지고요",
          "speaker_id": "1",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041798.1.2",
          "form": "근데 name3는 게임이  지워져서 점수가 없잖아",
          "original_form": "근데 &name3&는 게임이  지워져서 점수가 없잖아",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041798.1.3",
          "form": "아 그게 또 여기서 이어지는구만",
          "original_form": "아 그게 또 여기서 이어지는구만",
          "speaker_id": "1",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041798.1.4",
          "form": "그러니까 name3는 게임이 하기 싫은거야",
          "original_form": "그러니까 &name3&는 게임이 하기 싫은거야",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041798.1.5",
          "form": "그렇겠죠",
          "original_form": "그렇겠죠",
          "speaker_id": "1",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041798.1.6",
          "form": "근데 name3가 계속 울면서  엄마한테 전화하니까",
          "original_form": "근데 &name3&가 계속 울면서  엄마한테 전화하니까",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041798.1.7",
          "form": "name4이 짜증나서",
          "original_form": "&name4&이 짜증나서",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041798.1.8",
          "form": "그 친구한테 야 그냥 우리끼리 놀자",
          "original_form": "그 친구한테 야 그냥 우리끼리 놀자",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041798.1.9",
          "form": "이렇게 말한거를",
          "original_form": "이렇게 말한거를",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041798.1.10",
          "form": "아이고ㅠ",
          "original_form": "아이고ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041798.1.11",
          "form": "name3가 또 자기 엄마한테",
          "original_form": "&name3&가 또 자기 엄마한테",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041798.1.12",
          "form": "엄마 name4이가 나 빼고 둘이 놀자 고만해라고 말을 했어",
          "original_form": "엄마 &name4&이가 나 빼고 둘이 놀자 고만해라고 말을 했어",
          "speaker_id": "2",
          "time": "20210906 15:55"
        },
        {
          "id": "MDRW2100041798.1.13",
          "form": "그래서 이제 name3 엄마도 화가 난 거지",
          "original_form": "그래서 이제 &name3& 엄마도 화가 난 거지",
          "speaker_id": "2",
          "time": "20210906 15:55"
        },
        {
          "id": "MDRW2100041798.1.14",
          "form": "응 돌이키기 힘들겠네요 언니 돌이키기 힘들겠네",
          "original_form": "응 돌이키기 힘들겠네요 언니 돌이키기 힘들겠네",
          "speaker_id": "1",
          "time": "20210906 15:55"
        },
        {
          "id": "MDRW2100041798.1.15",
          "form": "그래서 name4이한테 전화해서",
          "original_form": "그래서 &name4&이한테 전화해서",
          "speaker_id": "2",
          "time": "20210906 15:55"
        },
        {
          "id": "MDRW2100041798.1.16",
          "form": "왜 그렇게 말했냐 하고 물어봤다 네",
          "original_form": "왜 그렇게 말했냐 하고 물어봤다 네",
          "speaker_id": "2",
          "time": "20210906 15:55"
        },
        {
          "id": "MDRW2100041798.1.17",
          "form": "그런데 그렇게 말하는 사이에 name3는 울면서 자전거를 끌고",
          "original_form": "그런데 그렇게 말하는 사이에 &name3&는 울면서 자전거를 끌고",
          "speaker_id": "2",
          "time": "20210906 15:55"
        },
        {
          "id": "MDRW2100041798.1.18",
          "form": "자기 집으로 갔는데 또 아빠가 있었던 거지",
          "original_form": "자기 집으로 갔는데 또 아빠가 있었던 거지",
          "speaker_id": "2",
          "time": "20210906 15:55"
        },
        {
          "id": "MDRW2100041798.1.19",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210906 15:55"
        },
        {
          "id": "MDRW2100041798.1.20",
          "form": "그래서 name3 아빠가 또 우리 신랑한테 전화가 왔어",
          "original_form": "그래서 &name3& 아빠가 또 우리 신랑한테 전화가 왔어",
          "speaker_id": "2",
          "time": "20210906 15:55"
        },
        {
          "id": "MDRW2100041798.1.21",
          "form": "근데 이렇게 서로 놀자 안 놀자 뭐 자전거를 타자 이거 게임을 하자 이러다가 서로 싸운 적이 많아",
          "original_form": "근데 이렇게 서로 놀자 안 놀자 뭐 자전거를 타자 이거 게임을 하자 이러다가 서로 싸운 적이 많아",
          "speaker_id": "2",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100041798.1.22",
          "form": "두 번째 거는 전화를 안 했으면 더 좋았을 텐데",
          "original_form": "두 번째 거는 전화를 안 했으면 더 좋았을 텐데",
          "speaker_id": "2",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100041798.1.23",
          "form": "근데 원래 또 둘이",
          "original_form": "근데 원래 또 둘이",
          "speaker_id": "1",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100041798.1.24",
          "form": "계속 두 번째 거까지 전화가 오니까",
          "original_form": "계속 두 번째 거까지 전화가 오니까",
          "speaker_id": "2",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100041798.1.25",
          "form": "우리도 이제 쫌 화가 나는 거야",
          "original_form": "우리도 이제 쫌 화가 나는 거야",
          "speaker_id": "2",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100041798.1.26",
          "form": "name3나 우리 name4이나 하늘 짓은 똑같거든",
          "original_form": "&name3&나 우리 &name4&이나 하늘 짓은 똑같거든",
          "speaker_id": "2",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100041798.1.27",
          "form": "name3도 name4이 전화 하면 짜증내면서 전화 받은 적이 한두 번이 아니야",
          "original_form": "&name3&도 &name4&이 전화 하면 짜증내면서 전화 받은 적이 한두 번이 아니야",
          "speaker_id": "2",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100041798.1.28",
          "form": "그래서 우리도 name4 보고 name3한테 전화하지 마라고 몇 번 얘기한 적도 있거든",
          "original_form": "그래서 우리도 &name4& 보고 &name3&한테 전화하지 마라고 몇 번 얘기한 적도 있거든",
          "speaker_id": "2",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100041798.1.29",
          "form": "뭔지 알꺼같네요~",
          "original_form": "뭔지 알꺼같네요~",
          "speaker_id": "1",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100041798.1.30",
          "form": "하루이틀 그렇게 지낸것도 아닌데~",
          "original_form": "하루이틀 그렇게 지낸것도 아닌데~",
          "speaker_id": "1",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100041798.1.31",
          "form": "이게 애들 노는거에 어른이.개입되믄..",
          "original_form": "이게 애들 노는거에 어른이.개입되믄..",
