      "utterance": [
        {
          "id": "MDRW2100042759.1.1",
          "form": "오늘은또습해용",
          "original_form": "오늘은또습해용",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042759.1.2",
          "form": "그러네요 가을 셔츠 여름 치마인데 딱 맞는듯",
          "original_form": "그러네요 가을 셔츠 여름 치마인데 딱 맞는듯",
          "speaker_id": "1",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042759.1.3",
          "form": "살짝 더워요 ㅋㅋㅋ",
          "original_form": "살짝 더워요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042759.1.4",
          "form": "으어 덥습니다",
          "original_form": "으어 덥습니다",
          "speaker_id": "2",
          "time": "20210907 10:24"
        },
        {
          "id": "MDRW2100042759.1.5",
          "form": "땀납니닥",
          "original_form": "땀납니닥",
          "speaker_id": "2",
          "time": "20210907 10:24"
        },
        {
          "id": "MDRW2100042759.1.6",
          "form": "저도요 에이포 가져온다고 돌아다녔더니 더워요",
          "original_form": "저도요 에이포 가져온다고 돌아다녔더니 더워요",
          "speaker_id": "1",
          "time": "20210907 10:40"
        },
        {
          "id": "MDRW2100042759.1.7",
          "form": "가을셔츠여름치마이쁘겟어용",
          "original_form": "가을셔츠여름치마이쁘겟어용",
          "speaker_id": "2",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042759.1.8",
          "form": "",
          "original_form": "{emoji:♡}",
          "speaker_id": "2",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042759.1.9",
          "form": "네네 오늘도 예브게 입고 왔어요",
          "original_form": "네네 오늘도 예브게 입고 왔어요",
          "speaker_id": "1",
          "time": "20210907 11:24"
        },
        {
          "id": "MDRW2100042759.1.10",
          "form": "땀은 흘리지만 ㅠㅠ",
          "original_form": "땀은 흘리지만 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042759.1.11",
          "form": "ㅎㅎ그래두이쁜옷차림이네용흐흐",
          "original_form": "ㅎㅎ그래두이쁜옷차림이네용흐흐",
          "speaker_id": "2",
          "time": "20210907 11:32"
        },
        {
          "id": "MDRW2100042759.1.12",
          "form": "네네 먄날 이쁨거 입고 싶어요 한번 사는 인생",
          "original_form": "네네 먄날 이쁨거 입고 싶어요 한번 사는 인생",
          "speaker_id": "1",
          "time": "20210907 12:05"
        },
        {
          "id": "MDRW2100042759.1.13",
          "form": "맞아요그래야하눈데",
          "original_form": "맞아요그래야하눈데",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042759.1.14",
          "form": "오늘 낮엔 찐다 덥네 날씨",
          "original_form": "오늘 낮엔 찐다 덥네 날씨",
          "speaker_id": "1",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042759.1.15",
          "form": "지금은해떳는감",
          "original_form": "지금은해떳는감",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042759.1.16",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042759.1.17",
          "form": "아니 해는 안뜨고 찐다 더워",
          "original_form": "아니 해는 안뜨고 찐다 더워",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042759.1.18",
          "form": "아 그게찐더위다",
          "original_form": "아 그게찐더위다",
          "speaker_id": "2",
          "time": "20210907 12:20"
