      "utterance": [
        {
          "id": "MDRW2100035977.1.1",
          "form": "동해는 놀기에 좋더라구용",
          "original_form": "동해는 놀기에 좋더라구용",
          "speaker_id": "1",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100035977.1.2",
          "form": "바다물색이 어쩜그리 예쁜지!!(사진으로만 봤습니다)",
          "original_form": "바다물색이 어쩜그리 예쁜지!!(사진으로만 봤습니다)",
          "speaker_id": "2",
          "time": "20210830 13:05"
        },
        {
          "id": "MDRW2100035977.1.3",
          "form": "맛있는거 !!!",
          "original_form": "맛있는거 !!!",
          "speaker_id": "2",
          "time": "20210830 13:05"
        },
        {
          "id": "MDRW2100035977.1.4",
          "form": "뭐 드셨었나요?!?!",
          "original_form": "뭐 드셨었나요?!?!",
          "speaker_id": "2",
          "time": "20210830 13:05"
        },
        {
          "id": "MDRW2100035977.1.5",
          "form": "속초는 만석닭강정이랑",
          "original_form": "속초는 만석닭강정이랑",
          "speaker_id": "1",
          "time": "20210830 13:06"
        },
        {
          "id": "MDRW2100035977.1.6",
          "form": "물회",
          "original_form": "물회",
          "speaker_id": "1",
          "time": "20210830 13:06"
        },
        {
          "id": "MDRW2100035977.1.7",
          "form": "이거 두개를 가장 맛있게 먹었던 기억이 나용",
          "original_form": "이거 두개를 가장 맛있게 먹었던 기억이 나용",
          "speaker_id": "1",
          "time": "20210830 13:06"
        },
        {
          "id": "MDRW2100035977.1.8",
          "form": "으앙 ㅜㅜ 튀긴 닭과 회는 인정이죠 ㅠㅠ ㅋㅋㅋㅋㅋ",
          "original_form": "으앙 ㅜㅜ 튀긴 닭과 회는 인정이죠 ㅠㅠ ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:09"
        },
        {
          "id": "MDRW2100035977.1.9",
          "form": "담에는 무조건 속초 강릉쪽으로 !!!",
          "original_form": "담에는 무조건 속초 강릉쪽으로 !!!",
          "speaker_id": "2",
          "time": "20210830 13:09"
        },
        {
          "id": "MDRW2100035977.1.10",
          "form": "요즘에 코로나때문에 제주도 비행기 값에 싼가봐여",
          "original_form": "요즘에 코로나때문에 제주도 비행기 값에 싼가봐여",
          "speaker_id": "2",
          "time": "20210830 13:09"
        },
        {
          "id": "MDRW2100035977.1.11",
          "form": "회사직원분이 ㅋㅋ 저번주에 왕복5만원으로 갔다왔대요",
          "original_form": "회사직원분이 ㅋㅋ 저번주에 왕복5만원으로 갔다왔대요",
          "speaker_id": "2",
          "time": "20210830 13:09"
        },
        {
          "id": "MDRW2100035977.1.12",
          "form": "우와 진짜 싸네요",
          "original_form": "우와 진짜 싸네요",
          "speaker_id": "1",
          "time": "20210830 13:11"
        },
        {
          "id": "MDRW2100035977.1.13",
          "form": "ktx 편도급을",
          "original_form": "ktx 편도급을",
          "speaker_id": "1",
          "time": "20210830 13:11"
        },
        {
          "id": "MDRW2100035977.1.14",
          "form": "비행기로!!",
          "original_form": "비행기로!!",
          "speaker_id": "1",
          "time": "20210830 13:11"
        },
        {
          "id": "MDRW2100035977.1.15",
          "form": "제주도 3월에 다녀오셨으면",
          "original_form": "제주도 3월에 다녀오셨으면",
          "speaker_id": "1",
          "time": "20210830 13:11"
        },
        {
          "id": "MDRW2100035977.1.16",
          "form": "유채꽃밭 가보셨어용?",
          "original_form": "유채꽃밭 가보셨어용?",
          "speaker_id": "1",
          "time": "20210830 13:11"
        },
        {
          "id": "MDRW2100035977.1.17",
          "form": "네네!!! 그때 아주 만개는 아니었는데 이뻤어여!!",
          "original_form": "네네!!! 그때 아주 만개는 아니었는데 이뻤어여!!",
          "speaker_id": "2",
          "time": "20210830 13:12"
        },
        {
          "id": "MDRW2100035977.1.18",
          "form": "3월초라",
          "original_form": "3월초라",
          "speaker_id": "2",
          "time": "20210830 13:12"
        },
        {
          "id": "MDRW2100035977.1.19",
          "form": "동백도 봤어요!!",
          "original_form": "동백도 봤어요!!",
          "speaker_id": "2",
          "time": "20210830 13:12"
        },
        {
          "id": "MDRW2100035977.1.20",
          "form": "저도 그맘때쯤 갔는데",
          "original_form": "저도 그맘때쯤 갔는데",
          "speaker_id": "1",
          "time": "20210830 13:12"
        },
        {
          "id": "MDRW2100035977.1.21",
          "form": "카멜리야? 거기 가서 동백꽃도보고",
          "original_form": "카멜리야? 거기 가서 동백꽃도보고",
          "speaker_id": "1",
          "time": "20210830 13:12"
        },
        {
          "id": "MDRW2100035977.1.22",
          "form": "유채도 봤어용!",
          "original_form": "유채도 봤어용!",
          "speaker_id": "1",
          "time": "20210830 13:12"
        },
        {
          "id": "MDRW2100035977.1.23",
          "form": "오호!! 동백 그렇게 가득핀거 첨봤는데 이쁘더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "오호!! 동백 그렇게 가득핀거 첨봤는데 이쁘더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100035977.1.24",
          "form": "꽃 좋아하시나여?!",
          "original_form": "꽃 좋아하시나여?!",
          "speaker_id": "2",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100035977.1.25",
          "form": "넹넹 좋아해요ㅎㅎ",
          "original_form": "넹넹 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100035977.1.26",
          "form": "제주도가서 꽃구경 실컷하다왔네용ㅎㅎ",
          "original_form": "제주도가서 꽃구경 실컷하다왔네용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100035977.1.27",
          "form": "저 변산반도 갔다했자나요 ..!! 저긴 샤스타데이지 꽃밭있어요 !!",
          "original_form": "저 변산반도 갔다했자나요 ..!! 저긴 샤스타데이지 꽃밭있어요 !!",
          "speaker_id": "2",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100035977.1.28",
          "form": "바다 바로옆에 있어서",
          "original_form": "바다 바로옆에 있어서",
          "speaker_id": "2",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100035977.1.29",
          "form": "바다랑 꽃이랑 한번에 보이는 뷰가 아주 이뻤어요",
          "original_form": "바다랑 꽃이랑 한번에 보이는 뷰가 아주 이뻤어요",
          "speaker_id": "2",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100035977.1.30",
          "form": "추천추천!!!",
          "original_form": "추천추천!!!",
          "speaker_id": "2",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100035977.1.31",
          "form": "샤스타데이지용??? 꽃 이름이 넘 예쁘네요",
          "original_form": "샤스타데이지용??? 꽃 이름이 넘 예쁘네요",
          "speaker_id": "1",
          "time": "20210830 13:16"
        },
        {
          "id": "MDRW2100035977.1.32",
          "form": "유튜브로 한번 찾아봐야겠어요",
          "original_form": "유튜브로 한번 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210830 13:16"
        },
        {
          "id": "MDRW2100035977.1.33",
          "form": "노을에 꽃에 완전 감성감성하네요ㅋㅋ",
          "original_form": "노을에 꽃에 완전 감성감성하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:16"
        },
        {
          "id": "MDRW2100035977.1.34",
          "form": "내년엔 겹벚꽃보러 경주 놀러가고싶어요..!!",
          "original_form": "내년엔 겹벚꽃보러 경주 놀러가고싶어요..!!",
          "speaker_id": "2",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100035977.1.35",
          "form": "수학여행 이후로 간적없는 경주 ㅋㅋㅋㅋ",
          "original_form": "수학여행 이후로 간적없는 경주 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100035977.1.36",
          "form": "경주 좋져!!",
          "original_form": "경주 좋져!!",
          "speaker_id": "1",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100035977.1.37",
          "form": "거기 그 충무김밥 먹고싶네요ㅋㅋㅋ",
          "original_form": "거기 그 충무김밥 먹고싶네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100035977.1.38",
          "form": "경주 충무김밥 유명한가요?!?! 기억해두고있겠습니다~!!",
          "original_form": "경주 충무김밥 유명한가요?!?! 기억해두고있겠습니다~!!",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035977.1.39",
          "form": "흠 이게 원래 경주꺼?인지 유명한지는 모르겠는데",
          "original_form": "흠 이게 원래 경주꺼?인지 유명한지는 모르겠는데",
          "speaker_id": "1",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100035977.1.40",
          "form": "전 경주갔을때 넘 맛있게 먹었어요!ㅎㅎ",
          "original_form": "전 경주갔을때 넘 맛있게 먹었어요!ㅎㅎ",
          "speaker_id": "1",
