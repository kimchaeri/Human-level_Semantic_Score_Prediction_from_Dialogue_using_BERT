        },
        {
          "id": "MDRW2100025658.1.2",
          "form": "그니까 헤드윅이",
          "original_form": "그니까 헤드윅이",
          "speaker_id": "1",
          "time": "20210901 15:30"
        },
        {
          "id": "MDRW2100025658.1.3",
          "form": "미국건너올때",
          "original_form": "미국건너올때",
          "speaker_id": "1",
          "time": "20210901 15:30"
        },
        {
          "id": "MDRW2100025658.1.4",
          "form": "수술을 받아야했던것처럼",
          "original_form": "수술을 받아야했던것처럼",
          "speaker_id": "1",
          "time": "20210901 15:30"
        },
        {
          "id": "MDRW2100025658.1.5",
          "form": "자유에는 희생이 따르는걸 이츠학한테 똑같이 푸는게",
          "original_form": "자유에는 희생이 따르는걸 이츠학한테 똑같이 푸는게",
          "speaker_id": "1",
          "time": "20210901 15:31"
        },
        {
          "id": "MDRW2100025658.1.6",
          "form": "일반적인 느낌이면",
          "original_form": "일반적인 느낌이면",
          "speaker_id": "1",
          "time": "20210901 15:31"
        },
        {
          "id": "MDRW2100025658.1.7",
          "form": "조는 자기같은 딴따라같은인생",
          "original_form": "조는 자기같은 딴따라같은인생",
          "speaker_id": "1",
          "time": "20210901 15:31"
        },
        {
          "id": "MDRW2100025658.1.8",
          "form": "자기만 하면됐다",
          "original_form": "자기만 하면됐다",
          "speaker_id": "1",
          "time": "20210901 15:31"
        },
        {
          "id": "MDRW2100025658.1.9",
          "form": "아끼는 너까지 하게할수없어",
          "original_form": "아끼는 너까지 하게할수없어",
          "speaker_id": "1",
          "time": "20210901 15:31"
        },
        {
          "id": "MDRW2100025658.1.10",
          "form": "이런느낌",
          "original_form": "이런느낌",
          "speaker_id": "1",
          "time": "20210901 15:31"
        },
        {
          "id": "MDRW2100025658.1.11",
          "form": "아 완전 내가 생각도 못한 해석",
          "original_form": "아 완전 내가 생각도 못한 해석",
          "speaker_id": "2",
          "time": "20210901 15:32"
        },
        {
          "id": "MDRW2100025658.1.12",
          "form": "ㄴㅇㄱ",
          "original_form": "ㄴㅇㄱ",
          "speaker_id": "2",
          "time": "20210901 15:32"
        },
        {
          "id": "MDRW2100025658.1.13",
          "form": "ㅋㄱㄱㅋㅋㄱㅋㅋㄱㄱ 그래서 사실이츠학캐릭터가",
          "original_form": "ㅋㄱㄱㅋㅋㄱㅋㅋㄱㄱ 그래서 사실이츠학캐릭터가",
          "speaker_id": "1",
          "time": "20210901 15:32"
        },
        {
          "id": "MDRW2100025658.1.14",
          "form": "죽는느낌도있어",
          "original_form": "죽는느낌도있어",
          "speaker_id": "1",
          "time": "20210901 15:32"
        },
        {
          "id": "MDRW2100025658.1.15",
          "form": "착할수있구나....그렇게 풀다니 신기하다",
          "original_form": "착할수있구나....그렇게 풀다니 신기하다",
          "speaker_id": "2",
          "time": "20210901 15:32"
        },
        {
          "id": "MDRW2100025658.1.16",
          "form": "이츠학이랑 ㅎ9헤드윅이랑 애증 혐관",
          "original_form": "이츠학이랑 ㅎ9헤드윅이랑 애증 혐관",
          "speaker_id": "1",
          "time": "20210901 15:32"
        },
        {
          "id": "MDRW2100025658.1.17",
          "form": "이래야하는데",
          "original_form": "이래야하는데",
          "speaker_id": "1",
          "time": "20210901 15:32"
        },
        {
          "id": "MDRW2100025658.1.18",
          "form": "아예 좀 새로운 극 느낌이네",
