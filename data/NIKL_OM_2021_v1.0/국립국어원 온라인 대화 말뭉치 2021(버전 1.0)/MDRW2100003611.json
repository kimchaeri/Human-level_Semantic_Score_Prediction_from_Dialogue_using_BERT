      "utterance": [
        {
          "id": "MDRW2100003611.1.1",
          "form": "도쿄 올림픽보셨나요?",
          "original_form": "도쿄 올림픽보셨나요?",
          "speaker_id": "1",
          "time": "20210821 08:24"
        },
        {
          "id": "MDRW2100003611.1.2",
          "form": "몇종목만 어쩌다 봤어요.",
          "original_form": "몇종목만 어쩌다 봤어요.",
          "speaker_id": "2",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100003611.1.3",
          "form": "에구~!여길 한동안 안 들여다보고 이제 봤더니 많이 늦었네요.ㅠㅠ",
          "original_form": "에구~!여길 한동안 안 들여다보고 이제 봤더니 많이 늦었네요.ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100003611.1.4",
          "form": "ㅎㅎ 괜찮아요",
          "original_form": "ㅎㅎ 괜찮아요",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100003611.1.5",
          "form": "양궁 보셨어요?",
          "original_form": "양궁 보셨어요?",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100003611.1.6",
          "form": "아니요~그건 뒤늦게 뉴스로 봤어요.",
          "original_form": "아니요~그건 뒤늦게 뉴스로 봤어요.",
          "speaker_id": "2",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100003611.1.7",
          "form": "배구는 열심히 봤는디..",
          "original_form": "배구는 열심히 봤는디..",
          "speaker_id": "2",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100003611.1.8",
          "form": "아 저는 배구는 보다가 껐어요",
          "original_form": "아 저는 배구는 보다가 껐어요",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100003611.1.9",
          "form": "너무 답답했어요",
          "original_form": "너무 답답했어요",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100003611.1.10",
          "form": "그랬어요?^^",
          "original_form": "그랬어요?^^",
          "speaker_id": "2",
          "time": "20210903 07:50"
        },
        {
          "id": "MDRW2100003611.1.11",
          "form": "체력적으로 브라진 선수들이 남자선수같아보일정도더라구요",
          "original_form": "체력적으로 브라진 선수들이 남자선수같아보일정도더라구요",
          "speaker_id": "1",
          "time": "20210903 08:42"
        },
        {
          "id": "MDRW2100003611.1.12",
          "form": "심판도 편파심파같고 ㅜㅠ",
          "original_form": "심판도 편파심파같고 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210903 08:42"
        },
        {
          "id": "MDRW2100003611.1.13",
          "form": "외국선수들이 좀 그렇게보이긴 해요.",
          "original_form": "외국선수들이 좀 그렇게보이긴 해요.",
          "speaker_id": "2",
          "time": "20210903 08:44"
        },
        {
          "id": "MDRW2100003611.1.14",
          "form": "글쵸 너무 마음아파서 ㅜㅠ",
          "original_form": "글쵸 너무 마음아파서 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210903 09:10"
        },
        {
          "id": "MDRW2100003611.1.15",
          "form": "김연경이 좀 안쓰러워지더라구요",
          "original_form": "김연경이 좀 안쓰러워지더라구요",
          "speaker_id": "1",
          "time": "20210903 09:11"
        },
        {
          "id": "MDRW2100003611.1.16",
          "form": "갓 연경쓰",
          "original_form": "갓 연경쓰",
          "speaker_id": "1",
          "time": "20210903 09:11"
        },
        {
          "id": "MDRW2100003611.1.17",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:11"
        },
        {
          "id": "MDRW2100003611.1.18",
          "form": "어디든 리더가 중요하지만 스포츠경기에서는 특히 더 그런듯해요",
          "original_form": "어디든 리더가 중요하지만 스포츠경기에서는 특히 더 그런듯해요",
          "speaker_id": "2",
          "time": "20210903 09:13"
        },
        {
          "id": "MDRW2100003611.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210903 09:14"
        },
        {
          "id": "MDRW2100003611.1.20",
          "form": "양궁은 진짜 조마조마하면서봤어요 ㅎ",
          "original_form": "양궁은 진짜 조마조마하면서봤어요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 09:15"
        },
        {
          "id": "MDRW2100003611.1.21",
          "form": "심박수도 나오던데 이게 다른나라선수는 130이런데",
          "original_form": "심박수도 나오던데 이게 다른나라선수는 130이런데",
          "speaker_id": "1",
          "time": "20210903 09:15"
        },
        {
          "id": "MDRW2100003611.1.22",
          "form": "우리나러만 60 80 이렇더라구요",
          "original_form": "우리나러만 60 80 이렇더라구요",
          "speaker_id": "1",
          "time": "20210903 09:16"
        },
        {
          "id": "MDRW2100003611.1.23",
          "form": "와 진짜 강심장들이였어요",
          "original_form": "와 진짜 강심장들이였어요",
          "speaker_id": "1",
          "time": "20210903 09:16"
        },
        {
          "id": "MDRW2100003611.1.24",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 09:16"
        },
        {
          "id": "MDRW2100003611.1.25",
          "form": "양궁은  역시 우리나라선수가 짱이죠~",
          "original_form": "양궁은  역시 우리나라선수가 짱이죠~",
          "speaker_id": "2",
          "time": "20210903 09:16"
        },
        {
          "id": "MDRW2100003611.1.26",
          "form": "글쵸",
          "original_form": "글쵸",
          "speaker_id": "1",
          "time": "20210903 09:19"
        },
        {
          "id": "MDRW2100003611.1.27",
          "form": "안산 선수 어린데도 너무 침착하게",
          "original_form": "안산 선수 어린데도 너무 침착하게",
          "speaker_id": "1",
          "time": "20210903 09:19"
        },
        {
          "id": "MDRW2100003611.1.28",
          "form": "머찌더라구요",
          "original_form": "머찌더라구요",
          "speaker_id": "1",
          "time": "20210903 09:19"
        },
        {
          "id": "MDRW2100003611.1.29",
          "form": "나도 멋지게  살고 싶네요.ㅎㅎ",
          "original_form": "나도 멋지게  살고 싶네요.ㅎㅎ",
