          "original_form": "치즈김밥 좋아해요~",
          "speaker_id": "2",
          "time": "20210825 03:23"
        },
        {
          "id": "MDRW2100033804.1.12",
          "form": "쌀떡vs밀떡",
          "original_form": "쌀떡vs밀떡",
          "speaker_id": "2",
          "time": "20210825 03:23"
        },
        {
          "id": "MDRW2100033804.1.13",
          "form": "음.. 밀떡?",
          "original_form": "음.. 밀떡?",
          "speaker_id": "1",
          "time": "20210825 03:23"
        },
        {
          "id": "MDRW2100033804.1.14",
          "form": "name2님은요?",
