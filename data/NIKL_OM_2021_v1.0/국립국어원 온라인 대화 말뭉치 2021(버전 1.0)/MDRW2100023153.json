      "utterance": [
        {
          "id": "MDRW2100023153.1.1",
          "form": "너 여행 가고싶은곳 있어?",
          "original_form": "너 여행 가고싶은곳 있어?",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023153.1.2",
          "form": "응 북유럽 공기 좋은 곳으로 가고싶어",
          "original_form": "응 북유럽 공기 좋은 곳으로 가고싶어",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023153.1.3",
          "form": "넌 어디 가고 싶어?",
          "original_form": "넌 어디 가고 싶어?",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023153.1.4",
          "form": "난 친구가 캐나다로 유학가서",
          "original_form": "난 친구가 캐나다로 유학가서",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023153.1.5",
          "form": "나도 캐나다가려고",
          "original_form": "나도 캐나다가려고",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023153.1.6",
          "form": "캐나다도 괜찮다더라",
          "original_form": "캐나다도 괜찮다더라",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023153.1.7",
          "form": "나도 캐나다 가보고싶어",
          "original_form": "나도 캐나다 가보고싶어",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023153.1.8",
          "form": "캐나다로 한달정도 다녀오려는데",
          "original_form": "캐나다로 한달정도 다녀오려는데",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023153.1.9",
          "form": "비행기표만 200이앚ㆍ",
          "original_form": "비행기표만 200이앚ㆍ",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023153.1.10",
          "form": "너도 유학가려구?",
          "original_form": "너도 유학가려구?",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023153.1.11",
          "form": "아니아니 한달만 여행겸",
          "original_form": "아니아니 한달만 여행겸",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023153.1.12",
          "form": "친구보러 가려고",
          "original_form": "친구보러 가려고",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023153.1.13",
          "form": "친구가 감동먹겠다",
          "original_form": "친구가 감동먹겠다",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023153.1.14",
          "form": "만나러 비행기 타고 오면 엄청 좋아할거야",
          "original_form": "만나러 비행기 타고 오면 엄청 좋아할거야",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023153.1.15",
          "form": "비행기 값이 꽤 비싸네",
          "original_form": "비행기 값이 꽤 비싸네",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023153.1.16",
          "form": "시기 따라 내릴 일은 없을까?",
          "original_form": "시기 따라 내릴 일은 없을까?",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023153.1.17",
          "form": "그래서 제일 싼 비행기는",
          "original_form": "그래서 제일 싼 비행기는",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023153.1.18",
          "form": "3번정도 경유하더라",
          "original_form": "3번정도 경유하더라",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023153.1.19",
          "form": "여러 번 나눠서 가는구나",
          "original_form": "여러 번 나눠서 가는구나",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023153.1.20",
          "form": "제일 싼 건 얼마쯤 하니?",
          "original_form": "제일 싼 건 얼마쯤 하니?",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023153.1.21",
          "form": "120정도 했던것같아",
          "original_form": "120정도 했던것같아",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023153.1.22",
          "form": "와 80만 원이나 저렴하구나",
          "original_form": "와 80만 원이나 저렴하구나",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023153.1.23",
          "form": "확실히 경유가 저렴하네",
          "original_form": "확실히 경유가 저렴하네",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023153.1.24",
          "form": "겨울에 갈 거니?",
          "original_form": "겨울에 갈 거니?",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023153.1.25",
          "form": "아니 가을에가려고",
          "original_form": "아니 가을에가려고",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023153.1.26",
          "form": "캐나다 겨울 춥대...",
          "original_form": "캐나다 겨울 춥대...",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023153.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ그렇구나",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그렇구나",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023153.1.28",
          "form": "좋은 시간 보내고 오길 바라",
          "original_form": "좋은 시간 보내고 오길 바라",
          "speaker_id": "2",
          "time": "20210829 22:22"
        }
      ]
    }
  ]
}
