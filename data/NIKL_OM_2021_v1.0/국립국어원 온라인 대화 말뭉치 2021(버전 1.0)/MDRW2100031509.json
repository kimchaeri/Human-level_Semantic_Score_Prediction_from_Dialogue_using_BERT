        {
          "id": "MDRW2100031509.1.1",
          "form": "유튜브 자주 보세요",
          "original_form": "유튜브 자주 보세요",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100031509.1.2",
          "form": "네 밥 말아 갑니다",
          "original_form": "네 밥 말아 갑니다",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100031509.1.3",
          "form": "네 자주 봅니다",
          "original_form": "네 자주 봅니다",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100031509.1.4",
          "form": "주로 어떤 거 보세요",
          "original_form": "주로 어떤 거 보세요",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100031509.1.5",
          "form": "임영웅 트로트 자주 봅니다",
          "original_form": "임영웅 트로트 자주 봅니다",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100031509.1.6",
          "form": "아 맞어 임영웅 노래 참 잘 부르죠",
          "original_form": "아 맞어 임영웅 노래 참 잘 부르죠",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100031509.1.7",
          "form": "네 덕분에 위안을 얻고 있습니다",
          "original_form": "네 덕분에 위안을 얻고 있습니다",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100031509.1.8",
          "form": "다른 건 또 어떤 거 보시나요",
          "original_form": "다른 건 또 어떤 거 보시나요",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100031509.1.9",
          "form": "정치 뉴스로 보고 다양한",
          "original_form": "정치 뉴스로 보고 다양한",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100031509.1.10",
          "form": "뉴스를 자주 검색하여 됐습니다",
          "original_form": "뉴스를 자주 검색하여 됐습니다",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100031509.1.11",
          "form": "대단하시네요",
          "original_form": "대단하시네요",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100031509.1.12",
          "form": "아니 뭘요",
          "original_form": "아니 뭘요",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100031509.1.13",
          "form": "임영욱 노래 좋은 거 있던데",
          "original_form": "임영욱 노래 좋은 거 있던데",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100031509.1.14",
          "form": "어떤 거야",
          "original_form": "어떤 거야",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100031509.1.15",
          "form": "무슨 노부부 얘기하는데",
          "original_form": "무슨 노부부 얘기하는데",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100031509.1.16",
          "form": "요새 저녁마다 유튜브는 꼭 보는 거 같아요",
