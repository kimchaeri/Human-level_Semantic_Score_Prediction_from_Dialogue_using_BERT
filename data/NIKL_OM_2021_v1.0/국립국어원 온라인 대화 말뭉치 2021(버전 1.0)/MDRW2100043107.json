        },
        {
          "id": "MDRW2100043107.1.2",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210907 14:24"
        },
        {
          "id": "MDRW2100043107.1.3",
          "form": "최근에가보신곳이어딘가요",
          "original_form": "최근에가보신곳이어딘가요",
          "speaker_id": "1",
          "time": "20210907 14:25"
        },
        {
          "id": "MDRW2100043107.1.4",
          "form": "최근 여름휴가 독도랑 울릉도여!!",
          "original_form": "최근 여름휴가 독도랑 울릉도여!!",
          "speaker_id": "2",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100043107.1.5",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100043107.1.6",
          "form": "독도성공하셨나요",
          "original_form": "독도성공하셨나요",
          "speaker_id": "1",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100043107.1.7",
          "form": "가기어렵던데",
          "original_form": "가기어렵던데",
          "speaker_id": "1",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100043107.1.8",
          "form": "입도성공햇어여!!!!",
          "original_form": "입도성공햇어여!!!!",
          "speaker_id": "2",
          "time": "20210907 14:27"
        },
        {
          "id": "MDRW2100043107.1.9",
          "form": "거기비석같은데서사진찍으셨어요?",
          "original_form": "거기비석같은데서사진찍으셨어요?",
          "speaker_id": "1",
          "time": "20210907 14:28"
        },
        {
          "id": "MDRW2100043107.1.10",
          "form": "우찌아세요?",
          "original_form": "우찌아세요?",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043107.1.11",
          "form": "고게모죶?",
          "original_form": "고게모죶?",
          "speaker_id": "1",
          "time": "20210907 14:33"
        },
        {
          "id": "MDRW2100043107.1.12",
          "form": "비석에서 사진찍는거",
          "original_form": "비석에서 사진찍는거",
          "speaker_id": "2",
          "time": "20210907 14:33"
        },
        {
          "id": "MDRW2100043107.1.13",
          "form": "어떻게 아셧어요?",
          "original_form": "어떻게 아셧어요?",
          "speaker_id": "2",
          "time": "20210907 14:33"
