      "utterance": [
        {
          "id": "MDRW2100034601.1.1",
          "form": "저녁식사는 무엇으로 하시나요?",
          "original_form": "저녁식사는 무엇으로 하시나요?",
          "speaker_id": "1",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100034601.1.2",
          "form": "아직 안 먹었는데 고민 중이에요",
          "original_form": "아직 안 먹었는데 고민 중이에요",
          "speaker_id": "2",
          "time": "20210828 18:27"
        },
        {
          "id": "MDRW2100034601.1.3",
          "form": "저는 오늘 베이킹해서",
          "original_form": "저는 오늘 베이킹해서",
          "speaker_id": "1",
          "time": "20210828 18:28"
        },
        {
          "id": "MDRW2100034601.1.4",
          "form": "파운드케이크 만든 것 먹으려고요",
          "original_form": "파운드케이크 만든 것 먹으려고요",
          "speaker_id": "1",
          "time": "20210828 18:28"
        },
        {
          "id": "MDRW2100034601.1.5",
          "form": "파운드케이크 만들기 어려운가요?",
          "original_form": "파운드케이크 만들기 어려운가요?",
          "speaker_id": "2",
          "time": "20210828 18:36"
        },
        {
          "id": "MDRW2100034601.1.6",
          "form": "많이 어렵진 않았어요",
          "original_form": "많이 어렵진 않았어요",
          "speaker_id": "1",
          "time": "20210828 18:36"
        },
        {
          "id": "MDRW2100034601.1.7",
          "form": "파운드 케이크가 설탕 계란 밀가루가 1파운드씩 들어가서 그렇게 이름을 붙였다더라고요",
          "original_form": "파운드 케이크가 설탕 계란 밀가루가 1파운드씩 들어가서 그렇게 이름을 붙였다더라고요",
          "speaker_id": "1",
          "time": "20210828 18:36"
        },
        {
          "id": "MDRW2100034601.1.8",
          "form": "세가지를 동량으로 넣음 되는 것 같아요",
          "original_form": "세가지를 동량으로 넣음 되는 것 같아요",
          "speaker_id": "1",
          "time": "20210828 18:37"
        },
        {
          "id": "MDRW2100034601.1.9",
          "form": "그런 뜻이었군요. 맛있을 것 같아요",
          "original_form": "그런 뜻이었군요. 맛있을 것 같아요",
          "speaker_id": "2",
          "time": "20210828 18:39"
        },
        {
          "id": "MDRW2100034601.1.10",
          "form": "네 레몬이랑 초코파운드 케이크 만들었는데",
          "original_form": "네 레몬이랑 초코파운드 케이크 만들었는데",
          "speaker_id": "1",
          "time": "20210828 18:39"
        },
        {
          "id": "MDRW2100034601.1.11",
          "form": "생각보다 맛있네요ㅎㅎ",
          "original_form": "생각보다 맛있네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100034601.1.12",
          "form": "저도 나간 김에 빵 좀 사올 걸 그랬네요\n레몬이랑 초코라니 맛있는 조합!ㅋㅋ",
          "original_form": "저도 나간 김에 빵 좀 사올 걸 그랬네요\n레몬이랑 초코라니 맛있는 조합!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100034601.1.13",
          "form": "네네 맛있는 조합!!",
          "original_form": "네네 맛있는 조합!!",
          "speaker_id": "1",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100034601.1.14",
          "form": "저녁에 맛있는 것 드셔요",
          "original_form": "저녁에 맛있는 것 드셔요",
          "speaker_id": "1",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100034601.1.15",
          "form": "있는 반찬들이랑 해서 어서 차려 먹어야겠어요",
          "original_form": "있는 반찬들이랑 해서 어서 차려 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100034601.1.16",
          "form": "네네 집밥 맛있게 드셔요ㅎㅎ",
          "original_form": "네네 집밥 맛있게 드셔요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 18:42"
        },
        {
          "id": "MDRW2100034601.1.17",
          "form": "아이스크림 사왔는데 밥 먹고 그것도 먹고요ㅋㅋ",
          "original_form": "아이스크림 사왔는데 밥 먹고 그것도 먹고요ㅋㅋ",
