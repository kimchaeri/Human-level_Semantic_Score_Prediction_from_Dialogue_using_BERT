      "utterance": [
        {
          "id": "MDRW2100027782.1.1",
          "form": "염려해 주셔서 정말 감사합니다",
          "original_form": "염려해 주셔서 정말 감사합니다",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027782.1.2",
          "form": "저도 그래서 아직 약 안 먹고 기다리고 있어요",
          "original_form": "저도 그래서 아직 약 안 먹고 기다리고 있어요",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027782.1.3",
          "form": "건강이 참 중요한 건데",
          "original_form": "건강이 참 중요한 건데",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027782.1.4",
          "form": "ㅜㅜㅜ",
          "original_form": "ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027782.1.5",
          "form": "name1 님은 백신 접종 언제 끝내셨어요?",
          "original_form": "&name1& 님은 백신 접종 언제 끝내셨어요?",
          "speaker_id": "2",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027782.1.6",
          "form": "저는 지난주 목요일에 맞았어요",
          "original_form": "저는 지난주 목요일에 맞았어요",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027782.1.7",
          "form": "화이자 접종하셨나요?",
          "original_form": "화이자 접종하셨나요?",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027782.1.8",
          "form": "네 화이자요",
          "original_form": "네 화이자요",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027782.1.9",
          "form": "저도 화이자인데ㅜ 사람마다 정말 다른가봐요",
          "original_form": "저도 화이자인데ㅜ 사람마다 정말 다른가봐요",
          "speaker_id": "1",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027782.1.10",
          "form": "그런 듯 해요 천차만별",
          "original_form": "그런 듯 해요 천차만별",
          "speaker_id": "2",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027782.1.11",
          "form": "님은 크게 아프지 않으셨다 해서 참 다행입니다",
          "original_form": "님은 크게 아프지 않으셨다 해서 참 다행입니다",
          "speaker_id": "2",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027782.1.12",
          "form": "약 먹고나면 괜찮아 지기를 빌게요 ㅠ",
          "original_form": "약 먹고나면 괜찮아 지기를 빌게요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027782.1.13",
          "form": "주변보니까 팔근육통은 꽤 오래가는 사람들도 있더라구요",
          "original_form": "주변보니까 팔근육통은 꽤 오래가는 사람들도 있더라구요",
          "speaker_id": "1",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027782.1.14",
          "form": "어 맞아요 팔 잘 못 쓰고 아파 하더라고요",
          "original_form": "어 맞아요 팔 잘 못 쓰고 아파 하더라고요",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027782.1.15",
          "form": "2주 지나고도 잘때 그쪽으로 못눕는 동료도 있더라구요 ㅠ",
          "original_form": "2주 지나고도 잘때 그쪽으로 못눕는 동료도 있더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 15:24"
        },
        {
          "id": "MDRW2100027782.1.16",
          "form": "제 주변분들도요",
          "original_form": "제 주변분들도요",
          "speaker_id": "2",
          "time": "20210902 15:24"
        },
        {
          "id": "MDRW2100027782.1.17",
          "form": "뭐 팁이라도 드리고 싶은데 제 주변도 아프기만 하고 덜아픈 방법은 못들어서ㅜ 도움을 못드리네요",
          "original_form": "뭐 팁이라도 드리고 싶은데 제 주변도 아프기만 하고 덜아픈 방법은 못들어서ㅜ 도움을 못드리네요",
          "speaker_id": "1",
          "time": "20210902 15:26"
        },
        {
          "id": "MDRW2100027782.1.18",
          "form": "말씀만으로도 감사합니다",
          "original_form": "말씀만으로도 감사합니다",
          "speaker_id": "2",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100027782.1.19",
          "form": "얼음과 타이레놀로 어떻게든 버텨 보려구요",
          "original_form": "얼음과 타이레놀로 어떻게든 버텨 보려구요",
          "speaker_id": "2",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100027782.1.20",
          "form": "평소에 운동도 하고 건강도 좀 챙겼어야 하는데",
          "original_form": "평소에 운동도 하고 건강도 좀 챙겼어야 하는데",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027782.1.21",
          "form": "늦은 후회",
          "original_form": "늦은 후회",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027782.1.22",
          "form": "평소에도 자주 아프신 편이셨나요? 감기같은거 잘걸리시는? ㅜㅜ",
          "original_form": "평소에도 자주 아프신 편이셨나요? 감기같은거 잘걸리시는? ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027782.1.23",
          "form": "감기는 별로 안 걸리는데",
          "original_form": "감기는 별로 안 걸리는데",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027782.1.24",
          "form": "알러지도 좀 있고",
          "original_form": "알러지도 좀 있고",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027782.1.25",
          "form": "대상포진도 한번 겪고",
          "original_form": "대상포진도 한번 겪고",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027782.1.26",
          "form": "건강체질은 아닌 듯 해요",
          "original_form": "건강체질은 아닌 듯 해요",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027782.1.27",
          "form": "아이고 ㅜㅜㅜㅜ 대상포진도 진짜 아프다던데ㅠㅠㅠㅠ 고생하셨네요",
          "original_form": "아이고 ㅜㅜㅜㅜ 대상포진도 진짜 아프다던데ㅠㅠㅠㅠ 고생하셨네요",
          "speaker_id": "1",
          "time": "20210902 15:43"
        }
      ]
