      "utterance": [
        {
          "id": "MDRW2100024305.1.1",
          "form": "했는데 인천대나오셨대요ㅋㅋ",
          "original_form": "했는데 인천대나오셨대요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:29"
        },
        {
          "id": "MDRW2100024305.1.2",
          "form": "하삼동커피알아요?",
          "original_form": "하삼동커피알아요?",
          "speaker_id": "2",
          "time": "20210831 00:29"
        },
        {
          "id": "MDRW2100024305.1.3",
          "form": "아 그래요",
          "original_form": "아 그래요",
          "speaker_id": "2",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100024305.1.4",
          "form": "그거 오픈했다고해서",
          "original_form": "그거 오픈했다고해서",
          "speaker_id": "2",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100024305.1.5",
          "form": "아메리카노먹으러갈꺼에요",
          "original_form": "아메리카노먹으러갈꺼에요",
          "speaker_id": "2",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100024305.1.6",
          "form": "오ㅋㅋ아아",
          "original_form": "오ㅋㅋ아아",
          "speaker_id": "1",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100024305.1.7",
          "form": "하삼동커피들어봤음요",
          "original_form": "하삼동커피들어봤음요",
          "speaker_id": "1",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100024305.1.8",
          "form": "더벤티 이디야 하삼동",
          "original_form": "더벤티 이디야 하삼동",
          "speaker_id": "2",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100024305.1.9",
          "form": "세번째카페네요",
          "original_form": "세번째카페네요",
          "speaker_id": "2",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024305.1.10",
          "form": "검신 세번째 카페",
          "original_form": "검신 세번째 카페",
          "speaker_id": "1",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024305.1.11",
          "form": "빨리가서 포스팅해야되서",
          "original_form": "빨리가서 포스팅해야되서",
          "speaker_id": "2",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024305.1.12",
          "form": "스벅이생겨야할텐데",
          "original_form": "스벅이생겨야할텐데",
          "speaker_id": "1",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024305.1.13",
          "form": "빨리카페 가서 마시고싶어요",
          "original_form": "빨리카페 가서 마시고싶어요",
          "speaker_id": "2",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024305.1.14",
          "form": "1등으로 맛집리뷰해야죠ㅋㅋ",
          "original_form": "1등으로 맛집리뷰해야죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024305.1.15",
          "form": "스벅생기면 더 좋을듯ㅋㅋ",
          "original_form": "스벅생기면 더 좋을듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024305.1.16",
          "form": "만 원 쓰 고",
          "original_form": "만 원 쓰 고",
          "speaker_id": "2",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024305.1.17",
          "form": "십 원 벌 기",
          "original_form": "십 원 벌 기",
          "speaker_id": "2",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024305.1.18",
          "form": "인생은 커피처럼 쓰다",
          "original_form": "인생은 커피처럼 쓰다",
          "speaker_id": "2",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100024305.1.19",
          "form": "우리ㅋㅋ식사는 뭐먹죠?",
          "original_form": "우리ㅋㅋ식사는 뭐먹죠?",
          "speaker_id": "1",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100024305.1.20",
          "form": "식이를 곁드린 맛있는거ㅋㅋㅋ",
          "original_form": "식이를 곁드린 맛있는거ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100024305.1.21",
          "form": "머먹을까요",
          "original_form": "머먹을까요",
          "speaker_id": "2",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100024305.1.22",
          "form": "배달음식은 노노ㅋㅋㅋ식이에 도움이 안되니까여ㄲㅋㅋ",
          "original_form": "배달음식은 노노ㅋㅋㅋ식이에 도움이 안되니까여ㄲㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100024305.1.23",
          "form": "단백질 가득한식사를 해보아요",
          "original_form": "단백질 가득한식사를 해보아요",
          "speaker_id": "2",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100024305.1.24",
          "form": "샤브샤브 어때요?ㅋ",
          "original_form": "샤브샤브 어때요?ㅋ",
          "speaker_id": "1",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100024305.1.25",
          "form": "블로그보니 맛있어보여서요",
          "original_form": "블로그보니 맛있어보여서요",
          "speaker_id": "1",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100024305.1.26",
          "form": "같은구성으로",
          "original_form": "같은구성으로",
          "speaker_id": "2",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100024305.1.27",
          "form": "샤브샤브",
          "original_form": "샤브샤브",
          "speaker_id": "2",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100024305.1.28",
          "form": "샤브샤브 저 진짜 좋아해요ㅋㅋㅋ",
          "original_form": "샤브샤브 저 진짜 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100024305.1.29",
          "form": "등촌샤브샤브",
          "original_form": "등촌샤브샤브",
          "speaker_id": "2",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100024305.1.30",
          "form": "마라탕",
          "original_form": "마라탕",
          "speaker_id": "2",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100024305.1.31",
          "form": "오ㅋㅋ대박",
          "original_form": "오ㅋㅋ대박",
          "speaker_id": "1",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100024305.1.32",
          "form": "세가지로만들수있어요",
          "original_form": "세가지로만들수있어요",
          "speaker_id": "2",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100024305.1.33",
          "form": "마라탕은 살쪄서 안됨요ㅋㅋㅋ",
          "original_form": "마라탕은 살쪄서 안됨요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100024305.1.34",
          "form": "요새 최애음식 ㅋㅋㅋ",
          "original_form": "요새 최애음식 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100024305.1.35",
          "form": "그건 기름져서 다이어터는 노노ㅋㅋㅋ",
          "original_form": "그건 기름져서 다이어터는 노노ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100024305.1.36",
          "form": "소스가 기름지긴해요",
          "original_form": "소스가 기름지긴해요",
          "speaker_id": "2",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100024305.1.37",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100024305.1.38",
          "form": "저탄고지라",
          "original_form": "저탄고지라",
          "speaker_id": "2",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100024305.1.39",
          "form": "샤브샤브먹고 칼국수가 진리인데ㅋㅋㅋ",
          "original_form": "샤브샤브먹고 칼국수가 진리인데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100024305.1.40",
          "form": "괜차나요",
          "original_form": "괜차나요",
          "speaker_id": "2",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100024305.1.41",
          "form": "탄수화물안먹으먼",
          "original_form": "탄수화물안먹으먼",
          "speaker_id": "2",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100024305.1.42",
          "form": "괜찮드라구요",
          "original_form": "괜찮드라구요",
          "speaker_id": "2",
          "time": "20210831 00:35"
        },
        {
          "id": "MDRW2100024305.1.43",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "1",
          "time": "20210831 00:35"
        },
        {
          "id": "MDRW2100024305.1.44",
          "form": "탄수화물이 문제구나",
          "original_form": "탄수화물이 문제구나",
          "speaker_id": "1",
          "time": "20210831 00:35"
        }
      ]
    }
