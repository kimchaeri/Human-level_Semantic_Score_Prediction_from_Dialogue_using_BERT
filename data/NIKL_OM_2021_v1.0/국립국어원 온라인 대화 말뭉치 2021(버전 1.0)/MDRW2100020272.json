      "utterance": [
        {
          "id": "MDRW2100020272.1.1",
          "form": "방학이 있다는 게 부럽다",
          "original_form": "방학이 있다는 게 부럽다",
          "speaker_id": "2",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020272.1.2",
          "form": "너는 졸업을 이미 했으니까",
          "original_form": "너는 졸업을 이미 했으니까",
          "speaker_id": "1",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020272.1.3",
          "form": "너도 이번이 마지막 학기 인걸로 알고 있어",
          "original_form": "너도 이번이 마지막 학기 인걸로 알고 있어",
          "speaker_id": "2",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020272.1.4",
          "form": "맞아 그래서 소중한 방학이야",
          "original_form": "맞아 그래서 소중한 방학이야",
          "speaker_id": "1",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020272.1.5",
          "form": "학교가 가고 싶지는 않았니",
          "original_form": "학교가 가고 싶지는 않았니",
          "speaker_id": "2",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020272.1.6",
          "form": "이제 와 생각해보면 그립기도 해",
          "original_form": "이제 와 생각해보면 그립기도 해",
          "speaker_id": "1",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020272.1.7",
          "form": "학교에 있는 친구들이 그립지",
          "original_form": "학교에 있는 친구들이 그립지",
          "speaker_id": "2",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020272.1.8",
          "form": "맞아 그리고 학교에 있는 도서관이 그리워",
          "original_form": "맞아 그리고 학교에 있는 도서관이 그리워",
          "speaker_id": "1",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020272.1.9",
          "form": "너희 학교 도서관은 정말 시설이 좋아",
          "original_form": "너희 학교 도서관은 정말 시설이 좋아",
          "speaker_id": "2",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020272.1.10",
          "form": "맞아 그리고 관리가 정말 잘 돼",
          "original_form": "맞아 그리고 관리가 정말 잘 돼",
          "speaker_id": "1",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020272.1.11",
          "form": "그 점은 정말 부럽다",
          "original_form": "그 점은 정말 부럽다",
          "speaker_id": "2",
          "time": "20210828 00:54"
        },
        {
          "id": "MDRW2100020272.1.12",
          "form": "학교에 있는 식당도 싸고 맛있어",
          "original_form": "학교에 있는 식당도 싸고 맛있어",
          "speaker_id": "1",
          "time": "20210828 00:54"
        },
        {
          "id": "MDRW2100020272.1.13",
          "form": "너네 학교는 학교 음식점이 정말 유명 하더라",
          "original_form": "너네 학교는 학교 음식점이 정말 유명 하더라",
          "speaker_id": "2",
          "time": "20210828 00:54"
        },
        {
          "id": "MDRW2100020272.1.14",
          "form": "그때 내가 한번 사준 적 있지",
          "original_form": "그때 내가 한번 사준 적 있지",
          "speaker_id": "1",
