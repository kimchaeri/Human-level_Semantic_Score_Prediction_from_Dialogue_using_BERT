      "utterance": [
        {
          "id": "MDRW2100035903.1.1",
          "form": "오 제가 다 떨리네요 !",
          "original_form": "오 제가 다 떨리네요 !",
          "speaker_id": "1",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035903.1.2",
          "form": "집에 측정되는 체중계가 있으신가요?",
          "original_form": "집에 측정되는 체중계가 있으신가요?",
          "speaker_id": "1",
          "time": "20210830 11:05"
        },
        {
          "id": "MDRW2100035903.1.3",
          "form": "네 체지방지수 나오는 거 있어요",
          "original_form": "네 체지방지수 나오는 거 있어요",
          "speaker_id": "2",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100035903.1.4",
          "form": "오오 괜찮은가요???",
          "original_form": "오오 괜찮은가요???",
          "speaker_id": "1",
          "time": "20210830 11:32"
        },
        {
          "id": "MDRW2100035903.1.5",
          "form": "정확하진 않을 수도 있는데 간단히 보기엔 괜찮은 것 같아요",
          "original_form": "정확하진 않을 수도 있는데 간단히 보기엔 괜찮은 것 같아요",
          "speaker_id": "2",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100035903.1.6",
          "form": "하긴 같은 기계로 계속 재는게",
          "original_form": "하긴 같은 기계로 계속 재는게",
          "speaker_id": "1",
          "time": "20210830 11:34"
        },
        {
          "id": "MDRW2100035903.1.7",
          "form": "더 좋다더라구요!",
          "original_form": "더 좋다더라구요!",
          "speaker_id": "1",
          "time": "20210830 11:34"
        },
        {
          "id": "MDRW2100035903.1.8",
          "form": "집에 인바디를 놔둘 순 없으니 ㅋㅋ ㅜㅜ",
          "original_form": "집에 인바디를 놔둘 순 없으니 ㅋㅋ ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 11:34"
        },
        {
          "id": "MDRW2100035903.1.9",
          "form": "ㅋㅋㅋ측정해봤는데 먼지만큼 골격근량 늘어난 것 말곤 없네요",
          "original_form": "ㅋㅋㅋ측정해봤는데 먼지만큼 골격근량 늘어난 것 말곤 없네요",
          "speaker_id": "2",
          "time": "20210830 15:13"
        },
        {
          "id": "MDRW2100035903.1.10",
          "form": "오 그래도 늘어났네요 !!",
          "original_form": "오 그래도 늘어났네요 !!",
          "speaker_id": "1",
          "time": "20210830 15:32"
        },
        {
          "id": "MDRW2100035903.1.11",
          "form": "축하드려요 !!",
          "original_form": "축하드려요 !!",
          "speaker_id": "1",
          "time": "20210830 15:32"
        },
        {
          "id": "MDRW2100035903.1.12",
          "form": "원래 체지방 줄이고 근육량 늘리는게 젤 어렵잖아요 ㅜㅜ",
          "original_form": "원래 체지방 줄이고 근육량 늘리는게 젤 어렵잖아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 15:32"
        },
        {
          "id": "MDRW2100035903.1.13",
          "form": "ㅋㅋㅋ감사해요",
          "original_form": "ㅋㅋㅋ감사해요",
          "speaker_id": "2",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100035903.1.14",
          "form": "그러게요 그거 어떻게 하는거죠....",
          "original_form": "그러게요 그거 어떻게 하는거죠....",
          "speaker_id": "2",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100035903.1.15",
          "form": "그걸 한다면.. 바로 트레이너입니다",
          "original_form": "그걸 한다면.. 바로 트레이너입니다",
          "speaker_id": "1",
          "time": "20210830 18:19"
        },
        {
          "id": "MDRW2100035903.1.16",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:19"
        },
        {
          "id": "MDRW2100035903.1.17",
          "form": "ㅋㅋㅋ맞네요",
          "original_form": "ㅋㅋㅋ맞네요",
          "speaker_id": "2",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100035903.1.18",
          "form": "전 근육량 유지만 했으면 좋겠어요",
          "original_form": "전 근육량 유지만 했으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100035903.1.19",
          "form": "맞아요 ㅋㅋ 사실 잃지만 않으면 되는데",
          "original_form": "맞아요 ㅋㅋ 사실 잃지만 않으면 되는데",
          "speaker_id": "1",
          "time": "20210830 19:31"
        },
        {
          "id": "MDRW2100035903.1.20",
          "form": "운동 안하면 후루룩 .. 빠지죠",
          "original_form": "운동 안하면 후루룩 .. 빠지죠",
          "speaker_id": "1",
          "time": "20210830 19:31"
        },
        {
          "id": "MDRW2100035903.1.21",
          "form": "그러니까요 운동 조금만 안 하면 근육이 녹아요ㅋㅋ",
          "original_form": "그러니까요 운동 조금만 안 하면 근육이 녹아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100035903.1.22",
          "form": "저 코로나땜에 저번에 한달쉬었는데",
          "original_form": "저 코로나땜에 저번에 한달쉬었는데",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100035903.1.23",
          "form": "근육 2키로 녹았었어욬ㅋㅋ",
          "original_form": "근육 2키로 녹았었어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100035903.1.24",
          "form": "헐 2키로는 너무한거 아닌가요",
          "original_form": "헐 2키로는 너무한거 아닌가요",
          "speaker_id": "2",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100035903.1.25",
          "form": "진짜 자비없네요",
          "original_form": "진짜 자비없네요",
          "speaker_id": "2",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100035903.1.26",
          "form": "맞죠 ㅋㅋㅋ 근육 키우는덴 몇개월 걸렸는데 ....",
          "original_form": "맞죠 ㅋㅋㅋ 근육 키우는덴 몇개월 걸렸는데 ....",
