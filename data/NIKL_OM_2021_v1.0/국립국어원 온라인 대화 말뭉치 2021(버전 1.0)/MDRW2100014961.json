      "utterance": [
        {
          "id": "MDRW2100014961.1.1",
          "form": "활동 중이신 동호회가 있나요??",
          "original_form": "활동 중이신 동호회가 있나요??",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014961.1.2",
          "form": "아뇨ㅜㅜ하시는거 있으세요?",
          "original_form": "아뇨ㅜㅜ하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014961.1.3",
          "form": "저는 최근에 배드민턴 동호회에 가입했는데",
          "original_form": "저는 최근에 배드민턴 동호회에 가입했는데",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014961.1.4",
          "form": "코로나때문에 참석을 못하고있어요ㅠ",
          "original_form": "코로나때문에 참석을 못하고있어요ㅠ",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014961.1.5",
          "form": "오 배드민턴 동호회 저도 들고 싶은데 잘해야 가능하겠죠?ㅎㅎ",
          "original_form": "오 배드민턴 동호회 저도 들고 싶은데 잘해야 가능하겠죠?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014961.1.6",
          "form": "동호회마다 가입조건이 다른 것 같아요!",
          "original_form": "동호회마다 가입조건이 다른 것 같아요!",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014961.1.7",
          "form": "저도 잘 못해서 초심자도 가능한 곳우로 가입햇어요!",
          "original_form": "저도 잘 못해서 초심자도 가능한 곳우로 가입햇어요!",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014961.1.8",
          "form": "오 저도 초보 가능한 곳으로 알아봐야겠네요ㅎㅎ",
          "original_form": "오 저도 초보 가능한 곳으로 알아봐야겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014961.1.9",
          "form": "같이 모여서 운동하면 재밌을거같아요",
          "original_form": "같이 모여서 운동하면 재밌을거같아요",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014961.1.10",
          "form": "그니까요 망할 코로나 ㅠ",
          "original_form": "그니까요 망할 코로나 ㅠ",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014961.1.11",
          "form": "또 관심 있으신 동호회 있으신가요?",
          "original_form": "또 관심 있으신 동호회 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014961.1.12",
          "form": "그니까요  저 런닝크루같은것도 해보고싶어요ㅎㅎ",
          "original_form": "그니까요  저 런닝크루같은것도 해보고싶어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014961.1.13",
          "form": "오! 런닝크루도 재밌을 것 같아요!!",
          "original_form": "오! 런닝크루도 재밌을 것 같아요!!",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014961.1.14",
          "form": "가끔 혼자 런데이 하는데 심심하더라구요ㅠ",
          "original_form": "가끔 혼자 런데이 하는데 심심하더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014961.1.15",
          "form": "맞아요ㅋㅋ왜 가입 하는지 알겠어요 의지도 더 커지구 ㅎㅎ",
          "original_form": "맞아요ㅋㅋ왜 가입 하는지 알겠어요 의지도 더 커지구 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014961.1.16",
          "form": "맞어요 혼자 하면 잘 안나간다구요ㅠ",
          "original_form": "맞어요 혼자 하면 잘 안나간다구요ㅠ",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014961.1.17",
          "form": "진짜요ㅋㅋ약간 팀플처럼ㅋㅋㅋ",
          "original_form": "진짜요ㅋㅋ약간 팀플처럼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014961.1.18",
          "form": "서로 으쌰으쌰하면서 ㅋㅋ",
          "original_form": "서로 으쌰으쌰하면서 ㅋㅋ",
          "speaker_id": "1",
