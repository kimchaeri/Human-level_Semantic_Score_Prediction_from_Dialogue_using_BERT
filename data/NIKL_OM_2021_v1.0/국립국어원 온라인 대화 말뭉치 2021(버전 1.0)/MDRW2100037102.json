      "utterance": [
        {
          "id": "MDRW2100037102.1.1",
          "form": "사고픈게 많은 요즘이에요",
          "original_form": "사고픈게 많은 요즘이에요",
          "speaker_id": "1",
          "time": "20210831 19:40"
        },
        {
          "id": "MDRW2100037102.1.2",
          "form": "오 어떤 거 사고 싶으신가요?",
          "original_form": "오 어떤 거 사고 싶으신가요?",
          "speaker_id": "2",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100037102.1.3",
          "form": "요즘은 요리재료요!",
          "original_form": "요즘은 요리재료요!",
          "speaker_id": "1",
          "time": "20210831 20:22"
        },
        {
          "id": "MDRW2100037102.1.4",
          "form": "예를 들어 어떤 게 있을까요?",
          "original_form": "예를 들어 어떤 게 있을까요?",
          "speaker_id": "2",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100037102.1.5",
          "form": "스파게티면이나 소스들이요!!",
          "original_form": "스파게티면이나 소스들이요!!",
          "speaker_id": "1",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100037102.1.6",
          "form": "소스에 빠져버렸어요",
          "original_form": "소스에 빠져버렸어요",
          "speaker_id": "1",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100037102.1.7",
          "form": "냉동새우나 오징어도요!",
          "original_form": "냉동새우나 오징어도요!",
          "speaker_id": "1",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100037102.1.8",
          "form": "스파게티 좋아하시나봐요!",
          "original_form": "스파게티 좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210831 20:38"
        },
        {
          "id": "MDRW2100037102.1.9",
          "form": "넹!",
          "original_form": "넹!",
          "speaker_id": "1",
          "time": "20210831 20:41"
        },
        {
          "id": "MDRW2100037102.1.10",
          "form": "사먹다가 집에서 만들어먹고싶어져서요!!",
          "original_form": "사먹다가 집에서 만들어먹고싶어져서요!!",
          "speaker_id": "1",
          "time": "20210831 20:41"
        },
        {
          "id": "MDRW2100037102.1.11",
          "form": "어떤 스파게티 제일 좋아하시나요?",
          "original_form": "어떤 스파게티 제일 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100037102.1.12",
          "form": "치즈오븐스파게티요.!",
          "original_form": "치즈오븐스파게티요.!",
          "speaker_id": "1",
          "time": "20210831 20:43"
        },
        {
          "id": "MDRW2100037102.1.13",
          "form": "name2님은 사고싶은거 있으세요?",
          "original_form": "&name2&님은 사고싶은거 있으세요?",
          "speaker_id": "1",
          "time": "20210831 20:44"
        },
        {
          "id": "MDRW2100037102.1.14",
          "form": "보조배터리요!",
          "original_form": "보조배터리요!",
          "speaker_id": "2",
          "time": "20210831 20:46"
        },
        {
          "id": "MDRW2100037102.1.15",
          "form": "없이 다녔는데 외출할 때 있으면 좋을 거 같아서 하나 살까 고민중이에요",
          "original_form": "없이 다녔는데 외출할 때 있으면 좋을 거 같아서 하나 살까 고민중이에요",
          "speaker_id": "2",
          "time": "20210831 20:46"
        },
        {
          "id": "MDRW2100037102.1.16",
          "form": "아 확실히 필요하긴하죠!",
          "original_form": "아 확실히 필요하긴하죠!",
          "speaker_id": "1",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100037102.1.17",
          "form": "용량큰건 무거워서 좀 그렇지만 있음 진짜 좋긴하죠",
          "original_form": "용량큰건 무거워서 좀 그렇지만 있음 진짜 좋긴하죠",
          "speaker_id": "1",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100037102.1.18",
          "form": "확실히 너무 무거운 건 불편할 거 같아요",
          "original_form": "확실히 너무 무거운 건 불편할 거 같아요",
          "speaker_id": "2",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100037102.1.19",
          "form": "안 쓴지 오래 됐는데 학교 다니면서 가끔 필요할 때가 있더라고요",
          "original_form": "안 쓴지 오래 됐는데 학교 다니면서 가끔 필요할 때가 있더라고요",
          "speaker_id": "2",
          "time": "20210831 21:01"
        },
        {
          "id": "MDRW2100037102.1.20",
          "form": "맞아오 맞아요",
          "original_form": "맞아오 맞아요",
          "speaker_id": "1",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100037102.1.21",
          "form": "충전기는 망가지기도 쉽고 전기 찾는것두 일이구요",
          "original_form": "충전기는 망가지기도 쉽고 전기 찾는것두 일이구요",
