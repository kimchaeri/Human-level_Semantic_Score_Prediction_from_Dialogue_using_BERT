      "utterance": [
        {
          "id": "MDRW2100018638.1.1",
          "form": "직업이라..",
          "original_form": "직업이라..",
          "speaker_id": "1",
          "time": "20210827 09:43"
        },
        {
          "id": "MDRW2100018638.1.2",
          "form": "학생이신가요 ㅎㅎ",
          "original_form": "학생이신가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 09:53"
        },
        {
          "id": "MDRW2100018638.1.3",
          "form": "학생은 아니에요",
          "original_form": "학생은 아니에요",
          "speaker_id": "1",
          "time": "20210827 09:53"
        },
        {
          "id": "MDRW2100018638.1.4",
          "form": "직장인이신가쇼",
          "original_form": "직장인이신가쇼",
          "speaker_id": "1",
          "time": "20210827 09:53"
        },
        {
          "id": "MDRW2100018638.1.5",
          "form": "네ㅎㅎ 그럼 취준이신가보네요",
          "original_form": "네ㅎㅎ 그럼 취준이신가보네요",
          "speaker_id": "2",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018638.1.6",
          "form": "아니요 직장인입니다",
          "original_form": "아니요 직장인입니다",
          "speaker_id": "1",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018638.1.7",
          "form": "무슨 업무를 하시나요",
          "original_form": "무슨 업무를 하시나요",
          "speaker_id": "1",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018638.1.8",
          "form": "저는 지하철에서 일해요",
          "original_form": "저는 지하철에서 일해요",
          "speaker_id": "2",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018638.1.9",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018638.1.10",
          "form": "저는 사무직이요",
          "original_form": "저는 사무직이요",
          "speaker_id": "1",
          "time": "20210827 09:57"
        },
        {
          "id": "MDRW2100018638.1.11",
          "form": "지하철은 주로 어떤일을 배정받나요",
          "original_form": "지하철은 주로 어떤일을 배정받나요",
          "speaker_id": "1",
          "time": "20210827 09:57"
        },
        {
          "id": "MDRW2100018638.1.12",
          "form": "저도 사무 직렬인데",
          "original_form": "저도 사무 직렬인데",
          "speaker_id": "2",
          "time": "20210827 10:00"
        },
        {
          "id": "MDRW2100018638.1.13",
          "form": "역사관리 해요 ㅎㅎ",
          "original_form": "역사관리 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 10:00"
        },
        {
          "id": "MDRW2100018638.1.14",
          "form": "역사관리면 민원 이런쪽은 아니신가",
          "original_form": "역사관리면 민원 이런쪽은 아니신가",
          "speaker_id": "1",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018638.1.15",
          "form": "그거도 역사관리죠 ㅎ ㅎ",
          "original_form": "그거도 역사관리죠 ㅎ ㅎ",
          "speaker_id": "2",
          "time": "20210827 10:06"
        },
        {
          "id": "MDRW2100018638.1.16",
          "form": "힘든점은 없나요",
          "original_form": "힘든점은 없나요",
          "speaker_id": "1",
          "time": "20210827 10:06"
        },
        {
          "id": "MDRW2100018638.1.17",
          "form": "음 정상인은 안힘든데",
          "original_form": "음 정상인은 안힘든데",
          "speaker_id": "2",
          "time": "20210827 10:07"
        },
        {
          "id": "MDRW2100018638.1.18",
          "form": "가끔 진상들이 ㅎㅎ",
          "original_form": "가끔 진상들이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 10:07"
        },
        {
          "id": "MDRW2100018638.1.19",
          "form": "어떤진상 있던가요",
          "original_form": "어떤진상 있던가요",
          "speaker_id": "1",
          "time": "20210827 10:07"
        },
        {
          "id": "MDRW2100018638.1.20",
          "form": "취객이 제일 심하죠 ㅎㅎ",
          "original_form": "취객이 제일 심하죠 ㅎㅎ",
