        {
          "id": "MDRW2100009416.1.1",
          "form": "운동 하시나요?",
          "original_form": "운동 하시나요?",
          "speaker_id": "2",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009416.1.2",
          "form": "운동 요즘은 안해요!!",
          "original_form": "운동 요즘은 안해요!!",
          "speaker_id": "1",
          "time": "20210823 00:47"
        },
        {
          "id": "MDRW2100009416.1.3",
          "form": "젊으신가봐요~",
          "original_form": "젊으신가봐요~",
          "speaker_id": "2",
          "time": "20210823 08:22"
        },
        {
          "id": "MDRW2100009416.1.4",
          "form": "넹 학생이에요 ㅋㅋㅋ",
          "original_form": "넹 학생이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100009416.1.5",
          "form": "아 부럽네요. 청춘.. 건강관리보다는 미용에 관심이 많을..?",
          "original_form": "아 부럽네요. 청춘.. 건강관리보다는 미용에 관심이 많을..?",
          "speaker_id": "2",
          "time": "20210823 16:39"
        },
        {
          "id": "MDRW2100009416.1.6",
          "form": "넹넹 아무래도 그렇죵",
          "original_form": "넹넹 아무래도 그렇죵",
          "speaker_id": "1",
          "time": "20210823 17:10"
        },
        {
          "id": "MDRW2100009416.1.7",
          "form": "어디에 관심있으세요 제일?",
          "original_form": "어디에 관심있으세요 제일?",
          "speaker_id": "2",
          "time": "20210823 17:13"
        },
        {
          "id": "MDRW2100009416.1.8",
          "form": "음 다이어트요 ㅋㅋ",
          "original_form": "음 다이어트요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100009416.1.9",
          "form": "먹는 거 줄이는걸로요?",
          "original_form": "먹는 거 줄이는걸로요?",
          "speaker_id": "2",
          "time": "20210823 19:52"
        },
        {
          "id": "MDRW2100009416.1.10",
          "form": "넹 ㅋㅋㅋ",
          "original_form": "넹 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:53"
        },
        {
          "id": "MDRW2100009416.1.11",
          "form": "굶진않고?",
          "original_form": "굶진않고?",
          "speaker_id": "2",
          "time": "20210823 19:53"
        },
        {
          "id": "MDRW2100009416.1.12",
          "form": "넹 굶기엔 너무 힘들어요",
          "original_form": "넹 굶기엔 너무 힘들어요",
          "speaker_id": "1",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100009416.1.13",
          "form": "무슨다이어트해요그럼?",
          "original_form": "무슨다이어트해요그럼?",
          "speaker_id": "2",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100009416.1.14",
          "form": "그냥 평소에 밥양에 반정도만 먹고",
          "original_form": "그냥 평소에 밥양에 반정도만 먹고",
          "speaker_id": "1",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100009416.1.15",
          "form": "스트레칭 좀하구요",
          "original_form": "스트레칭 좀하구요",
          "speaker_id": "1",
          "time": "20210823 20:00"
        },
        {
          "id": "MDRW2100009416.1.16",
          "form": "전 요가해요",
