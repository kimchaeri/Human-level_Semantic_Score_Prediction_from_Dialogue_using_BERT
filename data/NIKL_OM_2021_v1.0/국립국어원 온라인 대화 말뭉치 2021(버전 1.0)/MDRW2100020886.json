      "utterance": [
        {
          "id": "MDRW2100020886.1.1",
          "form": "저는 저번주에 맞았어요!!",
          "original_form": "저는 저번주에 맞았어요!!",
          "speaker_id": "1",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100020886.1.2",
          "form": "근육통괜찮으세요?",
          "original_form": "근육통괜찮으세요?",
          "speaker_id": "1",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100020886.1.3",
          "form": "네네 만지면 아프긴 한데",
          "original_form": "네네 만지면 아프긴 한데",
          "speaker_id": "2",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100020886.1.4",
          "form": "그렇게 심하진 않아요!!!",
          "original_form": "그렇게 심하진 않아요!!!",
          "speaker_id": "2",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100020886.1.5",
          "form": "근데 좀 피곤하긴 하네요,,",
          "original_form": "근데 좀 피곤하긴 하네요,,",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020886.1.6",
          "form": "name1님은 백신 맞으셨나요?",
          "original_form": "&name1&님은 백신 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020886.1.7",
          "form": "맞고오면 엄청 노곤하고 졸리더라고요",
          "original_form": "맞고오면 엄청 노곤하고 졸리더라고요",
          "speaker_id": "1",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020886.1.8",
          "form": "저도 화이자맞았어요 저번주에!",
          "original_form": "저도 화이자맞았어요 저번주에!",
          "speaker_id": "1",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020886.1.9",
          "form": "오오 그러셨구나!!!",
          "original_form": "오오 그러셨구나!!!",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020886.1.10",
          "form": "저보다 1주 선배시네요 ><",
          "original_form": "저보다 1주 선배시네요 ><",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020886.1.11",
          "form": "부작용 이야기 듣고 진짜 무서웠는데",
          "original_form": "부작용 이야기 듣고 진짜 무서웠는데",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020886.1.12",
          "form": "아직까진 별 일 없어서 다행이에요ㅠ",
          "original_form": "아직까진 별 일 없어서 다행이에요ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020886.1.13",
          "form": "ㅋㅋㅋ저는 3일 내내 타이레놀먹었어요",
          "original_form": "ㅋㅋㅋ저는 3일 내내 타이레놀먹었어요",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020886.1.14",
          "form": "근육통때문에,,",
          "original_form": "근육통때문에,,",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020886.1.15",
          "form": "헉 그러셨군요",
          "original_form": "헉 그러셨군요",
          "speaker_id": "2",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100020886.1.16",
          "form": "주사 맞은 팔이 그렇게 아프셨던건가요?!?!",
          "original_form": "주사 맞은 팔이 그렇게 아프셨던건가요?!?!",
          "speaker_id": "2",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100020886.1.17",
          "form": "네 저는 팔이 직각으로 안올라가더라고요!!",
          "original_form": "네 저는 팔이 직각으로 안올라가더라고요!!",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100020886.1.18",
          "form": "넥스트레벨 불가",
          "original_form": "넥스트레벨 불가",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100020886.1.19",
          "form": "아 웃겨요",
          "original_form": "아 웃겨요",
          "speaker_id": "2",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100020886.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100020886.1.21",
          "form": "암 온 더 넥 스트 레블",
          "original_form": "암 온 더 넥 스트 레블",
          "speaker_id": "2",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100020886.1.22",
          "form": "제 친구들은",
          "original_form": "제 친구들은",
          "speaker_id": "2",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100020886.1.23",
          "form": "화이자 1차 맞고나서 반응이 아주 제각각이었어요",
          "original_form": "화이자 1차 맞고나서 반응이 아주 제각각이었어요",
          "speaker_id": "2",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100020886.1.24",
          "form": "어떤 애들은 아무렇지 않았다 하고",
          "original_form": "어떤 애들은 아무렇지 않았다 하고",
          "speaker_id": "2",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100020886.1.25",
          "form": "누구는 열나고 아프고 난리도 아니었대요!",
          "original_form": "누구는 열나고 아프고 난리도 아니었대요!",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020886.1.26",
          "form": "어 맞아요 저는 친구가 저보다 하루먼저맞았는데",
          "original_form": "어 맞아요 저는 친구가 저보다 하루먼저맞았는데",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020886.1.27",
          "form": "밤에 아프고 토하고 난리났어서 좀 무서웠어요",
          "original_form": "밤에 아프고 토하고 난리났어서 좀 무서웠어요",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100020886.1.28",
          "form": "잔여백신잡은 친구들아니고는 친구들중에서 제가 일찍맞은편이라",
          "original_form": "잔여백신잡은 친구들아니고는 친구들중에서 제가 일찍맞은편이라",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100020886.1.29",
          "form": "친구들이 저를 관찰하는 롤,,",
          "original_form": "친구들이 저를 관찰하는 롤,,",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100020886.1.30",
          "form": "헉 친구분이 엄청 고생하셨겠어요",
          "original_form": "헉 친구분이 엄청 고생하셨겠어요",
          "speaker_id": "2",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100020886.1.31",
          "form": "1차 맞았는데도 그렇게 반응 보인거예요???",
          "original_form": "1차 맞았는데도 그렇게 반응 보인거예요???",
          "speaker_id": "2",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100020886.1.32",
          "form": "저 그 얘기 들었는데 그...",
          "original_form": "저 그 얘기 들었는데 그...",
          "speaker_id": "2",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100020886.1.33",
          "form": "1차 맞았는데 엄청 아픈 사람들이 코로나 걸렸다가 나은걸수도 있다고?!",
          "original_form": "1차 맞았는데 엄청 아픈 사람들이 코로나 걸렸다가 나은걸수도 있다고?!",
          "speaker_id": "2",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100020886.1.34",
          "form": "네 걔 1차맞고 그래서 지금 2차완전 긴장중",
