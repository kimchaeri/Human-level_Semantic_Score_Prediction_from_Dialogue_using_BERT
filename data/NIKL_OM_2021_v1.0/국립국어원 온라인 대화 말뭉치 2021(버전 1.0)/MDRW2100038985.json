      "utterance": [
        {
          "id": "MDRW2100038985.1.1",
          "form": "다람쥐라니ㅎㅎ 엄청 귀엽겠어요",
          "original_form": "다람쥐라니ㅎㅎ 엄청 귀엽겠어요",
          "speaker_id": "2",
          "time": "20210903 08:16"
        },
        {
          "id": "MDRW2100038985.1.2",
          "form": "네ㅋㅋㅋ 귀여워요!\r\n다른 물건들도 많아서 고민하다가 그립톡을 선택했어요ㅎㅎ",
          "original_form": "네ㅋㅋㅋ 귀여워요!\r\n다른 물건들도 많아서 고민하다가 그립톡을 선택했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 14:18"
        },
        {
          "id": "MDRW2100038985.1.3",
          "form": "그립톡 쓰면 편한가요",
          "original_form": "그립톡 쓰면 편한가요",
          "speaker_id": "2",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100038985.1.4",
          "form": "이번에 처음 써보는중인데, 어색한 느낌도 있지만 괜찮은거같아요",
          "original_form": "이번에 처음 써보는중인데, 어색한 느낌도 있지만 괜찮은거같아요",
          "speaker_id": "1",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100038985.1.5",
          "form": "괜찮으시다고 하니 저도 그립톡 한번 봐야겠네요",
          "original_form": "괜찮으시다고 하니 저도 그립톡 한번 봐야겠네요",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100038985.1.6",
          "form": "name2님도 아직 그립톡 써보지 않으셨나보네요",
          "original_form": "&name2&님도 아직 그립톡 써보지 않으셨나보네요",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100038985.1.7",
          "form": "네 일반 케이스만 끼우고 다녀요",
          "original_form": "네 일반 케이스만 끼우고 다녀요",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100038985.1.8",
          "form": "저도 지금까지 그냥 케이스에 끼우고 다니기만 했어요",
          "original_form": "저도 지금까지 그냥 케이스에 끼우고 다니기만 했어요",
          "speaker_id": "1",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100038985.1.9",
          "form": "동물들 캐릭터나 인형들 어느 동물 좋아하세요?",
          "original_form": "동물들 캐릭터나 인형들 어느 동물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100038985.1.10",
          "form": "알파카 좋아해요",
          "original_form": "알파카 좋아해요",
          "speaker_id": "2",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100038985.1.11",
          "form": "알파카 귀엽죠!\r\n저는 토끼나 다람쥐를 좋아해요",
          "original_form": "알파카 귀엽죠!\r\n저는 토끼나 다람쥐를 좋아해요",
          "speaker_id": "1",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100038985.1.12",
          "form": "토끼랑 다람쥐도 좋죠",
          "original_form": "토끼랑 다람쥐도 좋죠",
          "speaker_id": "2",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100038985.1.13",
          "form": "무엇보다 굿즈가 많잖아요",
          "original_form": "무엇보다 굿즈가 많잖아요",
          "speaker_id": "2",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100038985.1.14",
          "form": "맞아요\r\n그래서 고를 때 선택하기 힘들어요",
          "original_form": "맞아요\r\n그래서 고를 때 선택하기 힘들어요",
          "speaker_id": "1",
          "time": "20210903 15:51"
        },
        {
          "id": "MDRW2100038985.1.15",
