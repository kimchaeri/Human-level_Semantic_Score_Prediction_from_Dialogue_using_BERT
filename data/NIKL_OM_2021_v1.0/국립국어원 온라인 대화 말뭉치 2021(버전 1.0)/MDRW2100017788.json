        },
        {
          "id": "MDRW2100017788.1.2",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017788.1.3",
          "form": "선선해요",
          "original_form": "선선해요",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017788.1.4",
          "form": "여기도 선선하네요",
          "original_form": "여기도 선선하네요",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017788.1.5",
          "form": "추석 지나면 춥겠어요",
          "original_form": "추석 지나면 춥겠어요",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017788.1.6",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017788.1.7",
          "form": "패딩을 빼야겠네요",
          "original_form": "패딩을 빼야겠네요",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017788.1.8",
          "form": "넘 이르지않나요ㅋㅋ",
          "original_form": "넘 이르지않나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017788.1.9",
          "form": "그런가요ㅋㅋ",
          "original_form": "그런가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017788.1.10",
          "form": "코트 드라이 맡겨야겠어요",
          "original_form": "코트 드라이 맡겨야겠어요",
          "speaker_id": "2",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017788.1.11",
          "form": "코트 이번엔 며칠이나입으련지",
          "original_form": "코트 이번엔 며칠이나입으련지",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017788.1.12",
          "form": "한 이주?",
          "original_form": "한 이주?",
          "speaker_id": "2",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017788.1.13",
          "form": "진짜 열심히 입고다녀야겠어요",
          "original_form": "진짜 열심히 입고다녀야겠어요",
          "speaker_id": "1",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017788.1.14",
          "form": "코로나때문에 옷살일이없네요ㅋㅋ",
          "original_form": "코로나때문에 옷살일이없네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017788.1.15",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017788.1.16",
