      "utterance": [
        {
          "id": "MDRW2100014835.1.1",
          "form": "아저씨 머리 자를때 아직이네요",
          "original_form": "아저씨 머리 자를때 아직이네요",
          "speaker_id": "1",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014835.1.2",
          "form": "짧게 깎으라니까",
          "original_form": "짧게 깎으라니까",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014835.1.3",
          "form": "엄마 머리 자를 때 갈 거예요?",
          "original_form": "엄마 머리 자를 때 갈 거예요?",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014835.1.4",
          "form": "코로나 때문에 무서워서 못 간다면서",
          "original_form": "코로나 때문에 무서워서 못 간다면서",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014835.1.5",
          "form": "어서 말을 하거라",
          "original_form": "어서 말을 하거라",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014835.1.6",
          "form": "엄마 동네에서 곧 자를 거 같아요",
          "original_form": "엄마 동네에서 곧 자를 거 같아요",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014835.1.7",
          "form": "머리 잘 자라는데 알어",
          "original_form": "머리 잘 자라는데 알어",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014835.1.8",
          "form": "아빠 오타",
          "original_form": "아빠 오타",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014835.1.9",
          "form": "그런데는 몰라요",
          "original_form": "그런데는 몰라요",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014835.1.10",
          "form": "머리 잘 자르는 곳 알아",
          "original_form": "머리 잘 자르는 곳 알아",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014835.1.11",
          "form": "다 오타네요",
          "original_form": "다 오타네요",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014835.1.12",
          "form": "잘 자르는데는 모르는데",
          "original_form": "잘 자르는데는 모르는데",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014835.1.13",
          "form": "아무데나 가서 자르면 되지",
          "original_form": "아무데나 가서 자르면 되지",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014835.1.14",
          "form": "넹 그냥 가까운데서 빨리 자르려고요",
          "original_form": "넹 그냥 가까운데서 빨리 자르려고요",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014835.1.15",
          "form": "마음에 들게 계속 요구를 잘해봐",
          "original_form": "마음에 들게 계속 요구를 잘해봐",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014835.1.16",
          "form": "그래야겠어요 머리 잘 자르는데 근처에 있음 좋겠네요",
          "original_form": "그래야겠어요 머리 잘 자르는데 근처에 있음 좋겠네요",
          "speaker_id": "1",
          "time": "20210825 20:57"
        }
