      "utterance": [
        {
          "id": "MDRW2100015711.1.1",
          "form": "좋아하는 동물이나 키우고 있는 반려동물 있으신가요?",
          "original_form": "좋아하는 동물이나 키우고 있는 반려동물 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100015711.1.2",
          "form": "강아지 좋아하는데 키워본 적은 없어요",
          "original_form": "강아지 좋아하는데 키워본 적은 없어요",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015711.1.3",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015711.1.4",
          "form": "저두저두!",
          "original_form": "저두저두!",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015711.1.5",
          "form": "강아지 좋아하는데 키워본 적없어요",
          "original_form": "강아지 좋아하는데 키워본 적없어요",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015711.1.6",
          "form": "고양이도 좋아하시나요?",
          "original_form": "고양이도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015711.1.7",
          "form": "앗 고양이는 좀 무서워해요...",
          "original_form": "앗 고양이는 좀 무서워해요...",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015711.1.8",
          "form": "어렸을 때 엄마가 겁을 줘서...",
          "original_form": "어렸을 때 엄마가 겁을 줘서...",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015711.1.9",
          "form": "앗 고양이가 째려본다 이렇게영?",
          "original_form": "앗 고양이가 째려본다 이렇게영?",
          "speaker_id": "1",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015711.1.10",
          "form": "저희 엄마두 고양이 무서워하세요",
          "original_form": "저희 엄마두 고양이 무서워하세요",
          "speaker_id": "1",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015711.1.11",
          "form": "눈이 무섭다네요",
          "original_form": "눈이 무섭다네요",
          "speaker_id": "1",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015711.1.12",
          "form": "길고양이 쳐다보면 달려든다고 겁주셨어요..ㅋㅋ",
          "original_form": "길고양이 쳐다보면 달려든다고 겁주셨어요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015711.1.13",
          "form": "name1님은 고양이 좋아하세요??",
          "original_form": "&name1&님은 고양이 좋아하세요??",
          "speaker_id": "2",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015711.1.14",
          "form": "네넹 전 동물 거의 다 좋아해요ㅎㅎ",
          "original_form": "네넹 전 동물 거의 다 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015711.1.15",
          "form": "전 만약에 동물을 키운다면",
          "original_form": "전 만약에 동물을 키운다면",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015711.1.16",
          "form": "콩이라고 이름 짓고 싶었는데",
          "original_form": "콩이라고 이름 짓고 싶었는데",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015711.1.17",
          "form": "제 성이 홍씨라서",
          "original_form": "제 성이 홍씨라서",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015711.1.18",
          "form": "풀네임이........",
          "original_form": "풀네임이........",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015711.1.19",
          "form": "앜ㅋㅋㅋ 이름이 좀 특이해지네요...",
          "original_form": "앜ㅋㅋㅋ 이름이 좀 특이해지네요...",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015711.1.20",
          "form": "전 강아지 키우면 꼬마라고 짓고 싶어요ㅎㅎ",
          "original_form": "전 강아지 키우면 꼬마라고 짓고 싶어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015711.1.21",
          "form": "아잇 기여울 것같아요",
          "original_form": "아잇 기여울 것같아요",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015711.1.22",
          "form": "엇 생각해보니 제 친구 고양이 이름이 꼬마인데",
          "original_form": "엇 생각해보니 제 친구 고양이 이름이 꼬마인데",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015711.1.23",
          "form": "가족들은 그냥 다 꼬맹이라 부르더라구요ㅋㅋㅋ",
          "original_form": "가족들은 그냥 다 꼬맹이라 부르더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015711.1.24",
          "form": "앜ㅋㅋㅋ 저절로 별칭이 생겼네요ㅋㅋ",
          "original_form": "앜ㅋㅋㅋ 저절로 별칭이 생겼네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015711.1.25",
          "form": "혹시 다른 이름도 생각해보신 적 있으신가요??",
          "original_form": "혹시 다른 이름도 생각해보신 적 있으신가요??",
          "speaker_id": "2",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015711.1.26",
          "form": "음...왠지 애기라 부를 것같아요",
          "original_form": "음...왠지 애기라 부를 것같아요",
          "speaker_id": "1",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015711.1.27",
          "form": "오 근데 저도 이름 붙여줘도 애기라 부를 것 같아요ㅋㅋ",
          "original_form": "오 근데 저도 이름 붙여줘도 애기라 부를 것 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015711.1.28",
          "form": "그쵸ㅋㅋㅋ귀여워가지구ㅠ",
