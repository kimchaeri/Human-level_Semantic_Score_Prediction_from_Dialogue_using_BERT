      "utterance": [
        {
          "id": "MDRW2100041138.1.1",
          "form": "대학생때만요ㅎ",
          "original_form": "대학생때만요ㅎ",
          "speaker_id": "2",
          "time": "20210905 17:48"
        },
        {
          "id": "MDRW2100041138.1.2",
          "form": "name1님은 많이 하셨어요?",
          "original_form": "&name1&님은 많이 하셨어요?",
          "speaker_id": "2",
          "time": "20210905 17:48"
        },
        {
          "id": "MDRW2100041138.1.3",
          "form": "저는 지금 하고 있어요!",
          "original_form": "저는 지금 하고 있어요!",
          "speaker_id": "1",
          "time": "20210905 17:49"
        },
        {
          "id": "MDRW2100041138.1.4",
          "form": "어떤 알바 하셨어요!?",
          "original_form": "어떤 알바 하셨어요!?",
          "speaker_id": "1",
          "time": "20210905 17:49"
        },
        {
          "id": "MDRW2100041138.1.5",
          "form": "무슨 알방사세요?",
          "original_form": "무슨 알방사세요?",
          "speaker_id": "2",
          "time": "20210905 17:49"
        },
        {
          "id": "MDRW2100041138.1.6",
          "form": "전 음식점했어요",
          "original_form": "전 음식점했어요",
          "speaker_id": "2",
          "time": "20210905 17:49"
        },
        {
          "id": "MDRW2100041138.1.7",
          "form": "저는 과외알바 하고 있어요",
          "original_form": "저는 과외알바 하고 있어요",
          "speaker_id": "1",
          "time": "20210905 17:49"
        },
        {
          "id": "MDRW2100041138.1.8",
          "form": "알바하실 때 스트레스는 없으셨나요?",
          "original_form": "알바하실 때 스트레스는 없으셨나요?",
          "speaker_id": "1",
          "time": "20210905 17:49"
        },
        {
          "id": "MDRW2100041138.1.9",
          "form": "오 엘리트시구나",
          "original_form": "오 엘리트시구나",
          "speaker_id": "2",
          "time": "20210905 17:49"
        },
        {
          "id": "MDRW2100041138.1.10",
          "form": "저는 알바할때는 괜찮았어요 ㅎㅎ",
          "original_form": "저는 알바할때는 괜찮았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 17:49"
        },
        {
          "id": "MDRW2100041138.1.11",
          "form": "name1님은 스트레스 받으세요?ㅜ",
          "original_form": "&name1&님은 스트레스 받으세요?ㅜ",
          "speaker_id": "2",
          "time": "20210905 17:50"
        },
        {
          "id": "MDRW2100041138.1.12",
          "form": "저도 딱히 스트레스는 없는 것 같아요",
          "original_form": "저도 딱히 스트레스는 없는 것 같아요",
          "speaker_id": "1",
          "time": "20210905 17:50"
        },
        {
          "id": "MDRW2100041138.1.13",
          "form": "알바는 어디서 구하셨나요?",
          "original_form": "알바는 어디서 구하셨나요?",
          "speaker_id": "1",
          "time": "20210905 17:50"
        },
        {
          "id": "MDRW2100041138.1.14",
          "form": "알바천국이요",
          "original_form": "알바천국이요",
          "speaker_id": "2",
          "time": "20210905 17:51"
        },
        {
          "id": "MDRW2100041138.1.15",
          "form": "과외는 가르칠만한가요?",
          "original_form": "과외는 가르칠만한가요?",
          "speaker_id": "2",
          "time": "20210905 17:51"
        },
        {
          "id": "MDRW2100041138.1.16",
          "form": "네 전공 가르치는 거라 괜찮은 것 같아요",
          "original_form": "네 전공 가르치는 거라 괜찮은 것 같아요",
          "speaker_id": "1",
          "time": "20210905 17:52"
        },
        {
          "id": "MDRW2100041138.1.17",
          "form": "카페 알바도 해보셨나요?",
          "original_form": "카페 알바도 해보셨나요?",
          "speaker_id": "1",
          "time": "20210905 17:52"
        },
        {
          "id": "MDRW2100041138.1.18",
          "form": "아뇨 무슨 전공이예요?",
          "original_form": "아뇨 무슨 전공이예요?",
          "speaker_id": "2",
          "time": "20210905 17:52"
        },
        {
          "id": "MDRW2100041138.1.19",
          "form": "언어 전공하고 있습니다",
          "original_form": "언어 전공하고 있습니다",
          "speaker_id": "1",
          "time": "20210905 17:53"
        },
        {
          "id": "MDRW2100041138.1.20",
          "form": "해보고 싶으신 알바 있으신가요",
          "original_form": "해보고 싶으신 알바 있으신가요",
          "speaker_id": "1",
          "time": "20210905 17:53"
        },
        {
          "id": "MDRW2100041138.1.21",
          "form": "저는 이미 직장인이라 알바보다는 더 좋은곳으로 이직하고싶네요",
          "original_form": "저는 이미 직장인이라 알바보다는 더 좋은곳으로 이직하고싶네요",
          "speaker_id": "2",
          "time": "20210905 17:53"
        },
        {
          "id": "MDRW2100041138.1.22",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210905 17:54"
        },
        {
          "id": "MDRW2100041138.1.23",
          "form": "어떤 분야에서 일하시나요?",
          "original_form": "어떤 분야에서 일하시나요?",
