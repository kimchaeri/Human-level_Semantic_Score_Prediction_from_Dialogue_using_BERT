      "utterance": [
        {
          "id": "MDRW2100008090.1.1",
          "form": "오늘 날씨가 진짜 선선해요~",
          "original_form": "오늘 날씨가 진짜 선선해요~",
          "speaker_id": "2",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100008090.1.2",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100008090.1.3",
          "form": "지금은 비 안 오죠?",
          "original_form": "지금은 비 안 오죠?",
          "speaker_id": "1",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100008090.1.4",
          "form": "네 오늘 원래 비온다고 했는데 비가 안오네요~",
          "original_form": "네 오늘 원래 비온다고 했는데 비가 안오네요~",
          "speaker_id": "2",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100008090.1.5",
          "form": "다음주부터는 비가 내내 온대요..",
          "original_form": "다음주부터는 비가 내내 온대요..",
          "speaker_id": "2",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100008090.1.6",
          "form": "장마랑 태풍이 겹쳤대요",
          "original_form": "장마랑 태풍이 겹쳤대요",
          "speaker_id": "1",
          "time": "20210822 13:18"
        },
        {
          "id": "MDRW2100008090.1.7",
          "form": "비 좋아하시나요?",
          "original_form": "비 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 13:18"
        },
        {
          "id": "MDRW2100008090.1.8",
          "form": "실내에서 비오는걸 보는건 좋아해요",
          "original_form": "실내에서 비오는걸 보는건 좋아해요",
          "speaker_id": "2",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100008090.1.9",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100008090.1.10",
          "form": "저도 비 맞는 건 안 좋아하는데 빗소리를 좋아해서",
          "original_form": "저도 비 맞는 건 안 좋아하는데 빗소리를 좋아해서",
          "speaker_id": "1",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008090.1.11",
          "form": "실내에 있으면 창문 조금 열어놓고 비 구경해요",
          "original_form": "실내에 있으면 창문 조금 열어놓고 비 구경해요",
          "speaker_id": "1",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008090.1.12",
          "form": "맞아요 빗소리가 참 좋죠",
          "original_form": "맞아요 빗소리가 참 좋죠",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008090.1.13",
          "form": "9월되면은 이제 긴팔입고 다녀도 되겠어요",
          "original_form": "9월되면은 이제 긴팔입고 다녀도 되겠어요",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008090.1.14",
          "form": "이번 장마 끝나면 정말 가을이네요",
          "original_form": "이번 장마 끝나면 정말 가을이네요",
          "speaker_id": "1",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008090.1.15",
          "form": "옷 정리도 슬슬 해야겠어요",
          "original_form": "옷 정리도 슬슬 해야겠어요",
          "speaker_id": "1",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008090.1.16",
          "form": "정리 하셨나요?",
          "original_form": "정리 하셨나요?",
          "speaker_id": "1",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008090.1.17",
          "form": "다음주쯤에 해야겠어요~",
          "original_form": "다음주쯤에 해야겠어요~",
          "speaker_id": "2",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008090.1.18",
          "form": "이번에는 가을이 좀 길었으면 좋겠네요",
          "original_form": "이번에는 가을이 좀 길었으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008090.1.19",
          "form": "그러게요 작년에는 특히 짧아서 아쉬웠어요",
          "original_form": "그러게요 작년에는 특히 짧아서 아쉬웠어요",
          "speaker_id": "1",
          "time": "20210822 13:30"
        },
        {
          "id": "MDRW2100008090.1.20",
          "form": "가을 좋아하세요?",
          "original_form": "가을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 13:30"
        },
        {
          "id": "MDRW2100008090.1.21",
          "form": "네 저는 가을이 제일 좋더라구요!",
          "original_form": "네 저는 가을이 제일 좋더라구요!",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008090.1.22",
          "form": "name1님은 어떤 계절 제일 좋아하세요?",
          "original_form": "&name1&님은 어떤 계절 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008090.1.23",
          "form": "저도 가을 제일 좋아해요",
          "original_form": "저도 가을 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008090.1.24",
          "form": "바람이 시원해서 너무 좋아요",
          "original_form": "바람이 시원해서 너무 좋아요",
          "speaker_id": "1",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008090.1.25",
          "form": "그쵸 가을날씨 너무 좋아요~",
          "original_form": "그쵸 가을날씨 너무 좋아요~",
          "speaker_id": "2",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008090.1.26",
          "form": "봄도 좋아했는데 황사랑 콧샘추위가 있어서 가을이 최고인 것 같아요",
          "original_form": "봄도 좋아했는데 황사랑 콧샘추위가 있어서 가을이 최고인 것 같아요",
          "speaker_id": "2",
          "time": "20210822 13:41"
        }
      ]
