        {
          "id": "MDRW2100018725.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018725.1.2",
          "form": "여행 정말 좋아해요!!",
          "original_form": "여행 정말 좋아해요!!",
          "speaker_id": "2",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018725.1.3",
          "form": "코로나 끝나면 해외여행 가고 싶어요",
          "original_form": "코로나 끝나면 해외여행 가고 싶어요",
          "speaker_id": "2",
          "time": "20210827 11:28"
        },
        {
          "id": "MDRW2100018725.1.4",
          "form": "어디가보셨어요?",
          "original_form": "어디가보셨어요?",
          "speaker_id": "1",
          "time": "20210827 11:28"
        },
        {
          "id": "MDRW2100018725.1.5",
          "form": "일본 도쿄에 한번 가봤어요",
          "original_form": "일본 도쿄에 한번 가봤어요",
          "speaker_id": "2",
          "time": "20210827 11:28"
        },
        {
          "id": "MDRW2100018725.1.6",
          "form": "오 저도 가봤어요",
          "original_form": "오 저도 가봤어요",
          "speaker_id": "1",
          "time": "20210827 11:29"
        },
        {
          "id": "MDRW2100018725.1.7",
          "form": "맛있는게 너무 많더라구요",
          "original_form": "맛있는게 너무 많더라구요",
          "speaker_id": "1",
          "time": "20210827 11:29"
        },
        {
          "id": "MDRW2100018725.1.8",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "2",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018725.1.9",
          "form": "와 일본 가보셨군요 반가워요 ㅎㅎㅎ",
          "original_form": "와 일본 가보셨군요 반가워요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018725.1.10",
          "form": "저도 도쿄만 가봤어요ㅎㅎ",
          "original_form": "저도 도쿄만 가봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:32"
        },
        {
          "id": "MDRW2100018725.1.11",
          "form": "다음엔 오사카도 가보고싶어요",
          "original_form": "다음엔 오사카도 가보고싶어요",
          "speaker_id": "1",
          "time": "20210827 11:32"
        },
        {
          "id": "MDRW2100018725.1.12",
          "form": "오사카 좋네요~",
          "original_form": "오사카 좋네요~",
          "speaker_id": "2",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018725.1.13",
          "form": "일본 유명관광지는 다 가보고 싶어요",
          "original_form": "일본 유명관광지는 다 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210827 11:36"
        },
        {
          "id": "MDRW2100018725.1.14",
          "form": "맞아요ㅎㅎ국내는 어디가보셨어요?",
          "original_form": "맞아요ㅎㅎ국내는 어디가보셨어요?",
          "speaker_id": "1",
          "time": "20210827 11:36"
        },
        {
          "id": "MDRW2100018725.1.15",
          "form": "춘천이랑 군산 정도요!",
          "original_form": "춘천이랑 군산 정도요!",
          "speaker_id": "2",
          "time": "20210827 11:39"
        },
        {
          "id": "MDRW2100018725.1.16",
          "form": "춘천 닭갈비 드셨나요?ㅎㅎ",
          "original_form": "춘천 닭갈비 드셨나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100018725.1.17",
          "form": "꼭 먹어야요!! 닭갈비 엄청 좋아하거든요",
          "original_form": "꼭 먹어야요!! 닭갈비 엄청 좋아하거든요",
