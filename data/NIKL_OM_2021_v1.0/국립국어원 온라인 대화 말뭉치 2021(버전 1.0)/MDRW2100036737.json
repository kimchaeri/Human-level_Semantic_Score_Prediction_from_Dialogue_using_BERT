      "utterance": [
        {
          "id": "MDRW2100036737.1.1",
          "form": "오!?",
          "original_form": "오!?",
          "speaker_id": "2",
          "time": "20210831 10:54"
        },
        {
          "id": "MDRW2100036737.1.2",
          "form": "대박대박 있어요!! 집근처에 있는곳이에요!!",
          "original_form": "대박대박 있어요!! 집근처에 있는곳이에요!!",
          "speaker_id": "2",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100036737.1.3",
          "form": "토핑소시지맛집입니다 !",
          "original_form": "토핑소시지맛집입니다 !",
          "speaker_id": "1",
          "time": "20210831 10:56"
        },
        {
          "id": "MDRW2100036737.1.4",
          "form": "로제 맛있어요 !!",
          "original_form": "로제 맛있어요 !!",
          "speaker_id": "1",
          "time": "20210831 10:56"
        },
        {
          "id": "MDRW2100036737.1.5",
          "form": "우왕!  대박이네요 잠깐 훑어봤는데 닭강정도 맛날거같아용",
          "original_form": "우왕!  대박이네요 잠깐 훑어봤는데 닭강정도 맛날거같아용",
          "speaker_id": "2",
          "time": "20210831 11:06"
        },
        {
          "id": "MDRW2100036737.1.6",
          "form": "오 닭강정도 있나요??!",
          "original_form": "오 닭강정도 있나요??!",
          "speaker_id": "1",
          "time": "20210831 11:06"
        },
        {
          "id": "MDRW2100036737.1.7",
          "form": "그건 안먹어봤어요 ㅋㅋ",
          "original_form": "그건 안먹어봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:06"
        },
        {
          "id": "MDRW2100036737.1.8",
          "form": "에 소시지?! 핫도그 아니죠??",
          "original_form": "에 소시지?! 핫도그 아니죠??",
          "speaker_id": "2",
          "time": "20210831 11:06"
        },
        {
          "id": "MDRW2100036737.1.9",
          "form": "지역ㅇ마다 다른걸까요.. 내 소시지..",
          "original_form": "지역ㅇ마다 다른걸까요.. 내 소시지..",
          "speaker_id": "2",
          "time": "20210831 11:07"
        },
        {
          "id": "MDRW2100036737.1.10",
          "form": "그그 사이드 아니구",
          "original_form": "그그 사이드 아니구",
          "speaker_id": "1",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100036737.1.11",
          "form": "토핑 추가에 있어요!!",
          "original_form": "토핑 추가에 있어요!!",
          "speaker_id": "1",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100036737.1.12",
          "form": "떡볶이 토핑이여서 !!",
          "original_form": "떡볶이 토핑이여서 !!",
          "speaker_id": "1",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100036737.1.13",
          "form": "아!!",
          "original_form": "아!!",
          "speaker_id": "2",
          "time": "20210831 11:18"
        },
        {
          "id": "MDRW2100036737.1.14",
          "form": "태리소시지!!? 맞나용?!",
          "original_form": "태리소시지!!? 맞나용?!",
          "speaker_id": "2",
          "time": "20210831 11:19"
        },
        {
          "id": "MDRW2100036737.1.15",
          "form": "네네 ! 맞아요!",
          "original_form": "네네 ! 맞아요!",
          "speaker_id": "1",
          "time": "20210831 11:20"
        },
        {
          "id": "MDRW2100036737.1.16",
          "form": "크으으 찾아써요ㅠㅜ",
          "original_form": "크으으 찾아써요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210831 11:28"
        },
        {
          "id": "MDRW2100036737.1.17",
          "form": "이거 진짜 아 점심만 안준비했어도 각인데 말이죠ㅠ",
          "original_form": "이거 진짜 아 점심만 안준비했어도 각인데 말이죠ㅠ",
          "speaker_id": "2",
          "time": "20210831 11:28"
        },
        {
          "id": "MDRW2100036737.1.18",
          "form": "어떤 소시지에요???",
          "original_form": "어떤 소시지에요???",
          "speaker_id": "2",
          "time": "20210831 11:29"
        },
        {
          "id": "MDRW2100036737.1.19",
          "form": "그 뽀드득 소리나는 소시지인데 ㅜㅜ",
          "original_form": "그 뽀드득 소리나는 소시지인데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 11:52"
        },
        {
          "id": "MDRW2100036737.1.20",
          "form": "고기 함량 높은 비엔나 소시지요 !!!!",
          "original_form": "고기 함량 높은 비엔나 소시지요 !!!!",
          "speaker_id": "1",
          "time": "20210831 11:52"
        },
        {
          "id": "MDRW2100036737.1.21",
          "form": "그런 맛입니다 !!",
          "original_form": "그런 맛입니다 !!",
          "speaker_id": "1",
          "time": "20210831 11:52"
        },
        {
          "id": "MDRW2100036737.1.22",
          "form": "오우 대박대박",
          "original_form": "오우 대박대박",
          "speaker_id": "2",
          "time": "20210831 11:53"
        },
        {
          "id": "MDRW2100036737.1.23",
          "form": "최고네요 진짜",
          "original_form": "최고네요 진짜",
          "speaker_id": "2",
          "time": "20210831 11:53"
        },
        {
          "id": "MDRW2100036737.1.24",
          "form": "떡볶이 소시지 넣는사람 평생 적게일하고 많이버셔야해요",
          "original_form": "떡볶이 소시지 넣는사람 평생 적게일하고 많이버셔야해요",
          "speaker_id": "2",
          "time": "20210831 11:54"
        },
        {
          "id": "MDRW2100036737.1.25",
          "form": "맞아요 분명히 먹잘알의 생각이라구엿 ㅜㅜ",
          "original_form": "맞아요 분명히 먹잘알의 생각이라구엿 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 12:03"
        },
        {
          "id": "MDRW2100036737.1.26",
          "form": "그러고보니 오늘 점심은 뭘로 드시나요?",
          "original_form": "그러고보니 오늘 점심은 뭘로 드시나요?",
          "speaker_id": "1",
          "time": "20210831 12:04"
        },
        {
          "id": "MDRW2100036737.1.27",
          "form": "진짜요!",
          "original_form": "진짜요!",
          "speaker_id": "2",
          "time": "20210831 12:07"
        },
        {
          "id": "MDRW2100036737.1.28",
          "form": "저 야채듬뿍 넣은 김치전에 오징어 넣어서 먹고있어옹!",
          "original_form": "저 야채듬뿍 넣은 김치전에 오징어 넣어서 먹고있어옹!",
          "speaker_id": "2",
          "time": "20210831 12:07"
        },
        {
          "id": "MDRW2100036737.1.29",
          "form": "와 한입만 주세여.. 전이라니...",
          "original_form": "와 한입만 주세여.. 전이라니...",
          "speaker_id": "1",
          "time": "20210831 12:49"
        }
      ]
