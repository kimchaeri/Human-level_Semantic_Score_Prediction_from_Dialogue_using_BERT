      "utterance": [
        {
          "id": "MDRW2100024025.1.1",
          "form": "여행은 자주 다니시나요?",
          "original_form": "여행은 자주 다니시나요?",
          "speaker_id": "1",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024025.1.2",
          "form": "자주라면 자주 다녔는데 요즘은 시국이 시국인지라...",
          "original_form": "자주라면 자주 다녔는데 요즘은 시국이 시국인지라...",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100024025.1.3",
          "form": "못간지 꽤 됐어요ㅠ 최근에 가려고 했다가 취소되기도 했구요",
          "original_form": "못간지 꽤 됐어요ㅠ 최근에 가려고 했다가 취소되기도 했구요",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100024025.1.4",
          "form": "그쵸ㅜㅜ",
          "original_form": "그쵸ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100024025.1.5",
          "form": "코로나 잠잠해지면 가고 싶은 여행지는 있으세요?",
          "original_form": "코로나 잠잠해지면 가고 싶은 여행지는 있으세요?",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100024025.1.6",
          "form": "전 국내도 좋지만 해외요!",
          "original_form": "전 국내도 좋지만 해외요!",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024025.1.7",
          "form": "특히 작년 6월에 원래 하와이 가려고 하다가 취소해서요ㅠㅠ",
          "original_form": "특히 작년 6월에 원래 하와이 가려고 하다가 취소해서요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024025.1.8",
          "form": "하와이ㅜㅜ",
          "original_form": "하와이ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024025.1.9",
          "form": "넘 미련이 남네요ㅠㅠ",
          "original_form": "넘 미련이 남네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024025.1.10",
          "form": "곧 다시 가실 수 있으실거에요!",
          "original_form": "곧 다시 가실 수 있으실거에요!",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024025.1.11",
          "form": "하와이 어디를 제일가보고 싶으세요?",
          "original_form": "하와이 어디를 제일가보고 싶으세요?",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024025.1.12",
          "form": "하와이하면 와이키키...!!!",
          "original_form": "하와이하면 와이키키...!!!",
          "speaker_id": "2",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100024025.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ거기가 제일 유명해서 가보고 싶었어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ거기가 제일 유명해서 가보고 싶었어요",
          "speaker_id": "2",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100024025.1.14",
          "form": "저도요! 하와이는 뭔가 신혼여행지란 느낌이었는데ㅋ",
          "original_form": "저도요! 하와이는 뭔가 신혼여행지란 느낌이었는데ㅋ",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100024025.1.15",
          "form": "이젠 정말 휴양지네요ㅎㅎ",
          "original_form": "이젠 정말 휴양지네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100024025.1.16",
          "form": "ㅋㅋㅋㅋ tmi지만",
          "original_form": "ㅋㅋㅋㅋ tmi지만",
          "speaker_id": "2",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100024025.1.17",
          "form": "작년에 일 때문에 너~무 힘들어서 퇴사하고 쉬고 싶어서 하와이 고른거였거든요",
          "original_form": "작년에 일 때문에 너~무 힘들어서 퇴사하고 쉬고 싶어서 하와이 고른거였거든요",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024025.1.18",
          "form": "하와이하면 딱 여유롭고 해변~ 휴양~ 이런 느낌이 있는 거 같아요 ㅋㅋㅋㅋ",
          "original_form": "하와이하면 딱 여유롭고 해변~ 휴양~ 이런 느낌이 있는 거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024025.1.19",
          "form": "약간 환상이 있어요 ㅋㅋ",
          "original_form": "약간 환상이 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024025.1.20",
          "form": "ㅜㅜ 일이 힘드셨구나",
          "original_form": "ㅜㅜ 일이 힘드셨구나",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100024025.1.21",
          "form": "토닥토닥 지금은 괜찮으세요?",
          "original_form": "토닥토닥 지금은 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100024025.1.22",
          "form": "하와이꼭 가실 수 있으실거에요!",
          "original_form": "하와이꼭 가실 수 있으실거에요!",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100024025.1.23",
          "form": "네 ㅋㅋㅋㅋㅋ 퇴사하구.. 지금은 이직해서 잘 다니구 있어요",
          "original_form": "네 ㅋㅋㅋㅋㅋ 퇴사하구.. 지금은 이직해서 잘 다니구 있어요",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100024025.1.24",
          "form": "ㅋㅋ코로나 끝나면 꼭 갈거애용!!!",
          "original_form": "ㅋㅋ코로나 끝나면 꼭 갈거애용!!!",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100024025.1.25",
          "form": "name1 님은 여행 좋아하세요??",
          "original_form": "&name1& 님은 여행 좋아하세요??",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100024025.1.26",
          "form": "네 전 해외여행도 좋아하지만 국내여행이 더 좋아요ㅎㅎ",
          "original_form": "네 전 해외여행도 좋아하지만 국내여행이 더 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024025.1.27",
          "form": "어디가 제일 좋으셨어요?",
          "original_form": "어디가 제일 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024025.1.28",
          "form": "국내는 전 종교는 없지만 하동에 삼성궁이라는 곳이요",
          "original_form": "국내는 전 종교는 없지만 하동에 삼성궁이라는 곳이요",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100024025.1.29",
          "form": "단군모시는 곳인데 우리나라아닌줄 알았어요ㅎㅎ",
          "original_form": "단군모시는 곳인데 우리나라아닌줄 알았어요ㅎㅎ",
