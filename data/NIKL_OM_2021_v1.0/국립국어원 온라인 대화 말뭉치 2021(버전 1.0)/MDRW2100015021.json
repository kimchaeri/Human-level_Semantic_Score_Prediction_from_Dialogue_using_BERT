      "utterance": [
        {
          "id": "MDRW2100015021.1.1",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015021.1.2",
          "form": "전 콘서트 뮤지컬 좋아해ㅛ",
          "original_form": "전 콘서트 뮤지컬 좋아해ㅛ",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015021.1.3",
          "form": "저는 연극을 좋아해요",
          "original_form": "저는 연극을 좋아해요",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015021.1.4",
          "form": "연극 어떤 거 보셨어요?",
          "original_form": "연극 어떤 거 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015021.1.5",
          "form": "이제는 코로나로 못 간지 오래되서 가물가물하네요",
          "original_form": "이제는 코로나로 못 간지 오래되서 가물가물하네요",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015021.1.6",
          "form": "마지막이 언제쯤이세요?",
          "original_form": "마지막이 언제쯤이세요?",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015021.1.7",
          "form": "뮤지컬은 어떤걸 좋아하시나요?",
          "original_form": "뮤지컬은 어떤걸 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015021.1.8",
          "form": "대극장이 볼거리는 많은데 금액이 부담스러워요",
          "original_form": "대극장이 볼거리는 많은데 금액이 부담스러워요",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015021.1.9",
          "form": "20년도 1월이네요.. ㅜㅜ",
          "original_form": "20년도 1월이네요.. ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015021.1.10",
          "form": "그렇죠 .. 아무래도 금액을 무시 못하죠",
          "original_form": "그렇죠 .. 아무래도 금액을 무시 못하죠",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015021.1.11",
          "form": "아득하네요 전 마지막 공연이 19년 말이네요",
          "original_form": "아득하네요 전 마지막 공연이 19년 말이네요",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015021.1.12",
          "form": "콘서트나 뮤지컬이 좋으시면 노래를 좋아하시나봐요?",
          "original_form": "콘서트나 뮤지컬이 좋으시면 노래를 좋아하시나봐요?",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015021.1.13",
          "form": "공연장가면 웅웅 발로 진동오는게 집에서 보는것과 큰 차이인거같아요",
          "original_form": "공연장가면 웅웅 발로 진동오는게 집에서 보는것과 큰 차이인거같아요",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015021.1.14",
          "form": "진짜 아득하네요",
          "original_form": "진짜 아득하네요",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015021.1.15",
          "form": "네네 저도 공연장의 그 울림은 좋아요",
          "original_form": "네네 저도 공연장의 그 울림은 좋아요",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015021.1.16",
          "form": "네 요즘은 온라인공연으로 대신합니다",
          "original_form": "네 요즘은 온라인공연으로 대신합니다",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015021.1.17",
          "form": "연극무대는 배우님의 발성을 느낄수있어요 ㅎㅎ",
          "original_form": "연극무대는 배우님의 발성을 느낄수있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015021.1.18",
          "form": "조성진공연 영상 자주 찾아보고있어요",
          "original_form": "조성진공연 영상 자주 찾아보고있어요",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015021.1.19",
          "form": "피아니스트 조성진이요?",
          "original_form": "피아니스트 조성진이요?",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015021.1.20",
          "form": "아 진짜요? 가까이서 보면 신기할거같아요",
          "original_form": "아 진짜요? 가까이서 보면 신기할거같아요",
          "speaker_id": "1",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015021.1.21",
          "form": "네 정명훈 지휘로",
          "original_form": "네 정명훈 지휘로",
          "speaker_id": "1",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015021.1.22",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015021.1.23",
          "form": "그럼 좋아하시는 곡은 있으신가요?",
          "original_form": "그럼 좋아하시는 곡은 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015021.1.24",
          "form": "네 베토벤의 황제 요즘 자주 봐요",
          "original_form": "네 베토벤의 황제 요즘 자주 봐요",
          "speaker_id": "1",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015021.1.25",
          "form": "추천합니다",
          "original_form": "추천합니다",
          "speaker_id": "1",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015021.1.26",
          "form": "베토벤이면 웅장할거 같은데..",
          "original_form": "베토벤이면 웅장할거 같은데..",
          "speaker_id": "2",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015021.1.27",
          "form": "네 기회가 되면 들어볼께요",
          "original_form": "네 기회가 되면 들어볼께요",
          "speaker_id": "2",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015021.1.28",
          "form": "추천감사합니다~",
          "original_form": "추천감사합니다~",
          "speaker_id": "2",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015021.1.29",
          "form": "그런데 피아노라 굉장히 섬세해요",
          "original_form": "그런데 피아노라 굉장히 섬세해요",
          "speaker_id": "1",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015021.1.30",
          "form": "네 꼭 들어보세요",
          "original_form": "네 꼭 들어보세요",
          "speaker_id": "1",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015021.1.31",
          "form": "영화는 좋아하시나요?",
          "original_form": "영화는 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015021.1.32",
          "form": "영화는 재밌다고 하는걸 보러가는 편이예요..",
          "original_form": "영화는 재밌다고 하는걸 보러가는 편이예요..",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015021.1.33",
          "form": "좋아하는 배우님이 나오는것도 보러가지만요 ㅎㅎ",
          "original_form": "좋아하는 배우님이 나오는것도 보러가지만요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015021.1.34",
          "form": "어떤 영화를 좋아하시나요?",
          "original_form": "어떤 영화를 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015021.1.35",
          "form": "전 소소한 로코 좋아해요",
          "original_form": "전 소소한 로코 좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015021.1.36",
          "form": "죽고 죽이는건 좋아하지않아요ㅜ",
          "original_form": "죽고 죽이는건 좋아하지않아요ㅜ",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015021.1.37",
          "form": "저도 개인적으로 죽고 죽이는 스릴러는 별로예요",
          "original_form": "저도 개인적으로 죽고 죽이는 스릴러는 별로예요",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015021.1.38",
          "form": "저는 애절한 로맨스도 좋아해요",
          "original_form": "저는 애절한 로맨스도 좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015021.1.39",
          "form": "가장 추천하는 영화가 있나요?",
          "original_form": "가장 추천하는 영화가 있나요?",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015021.1.40",
          "form": "미드나잇파리나 어바웃타임이 지금은 기억나네요 ㅎㅎ",
          "original_form": "미드나잇파리나 어바웃타임이 지금은 기억나네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015021.1.41",
          "form": "저도 어바웃타임 좋아해요",
          "original_form": "저도 어바웃타임 좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015021.1.42",
          "form": "오에스티가 최고죠",
          "original_form": "오에스티가 최고죠",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015021.1.43",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015021.1.44",
          "form": "최고의 명장면 뭐라 생각하세요?",
          "original_form": "최고의 명장면 뭐라 생각하세요?",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015021.1.45",
          "form": "오에스티도 좋고 영상미도 예뻐서요",
          "original_form": "오에스티도 좋고 영상미도 예뻐서요",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015021.1.46",
          "form": "결혼식 장면 좋아해요",
          "original_form": "결혼식 장면 좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015021.1.47",
          "form": "둘만의 청혼장면도 좋아요.. 시간을 돌려서 맞췄지만요",
          "original_form": "둘만의 청혼장면도 좋아요.. 시간을 돌려서 맞췄지만요",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015021.1.48",
          "form": "결혼식장면은 영상미도 그 따뜻한 분위기도 좋아요",
          "original_form": "결혼식장면은 영상미도 그 따뜻한 분위기도 좋아요",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015021.1.49",
          "form": "그 부분이 쫌 웃기죠.",
          "original_form": "그 부분이 쫌 웃기죠.",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015021.1.50",
          "form": "비가 오는데도 다들 행복해보여서 좋아요",
          "original_form": "비가 오는데도 다들 행복해보여서 좋아요",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015021.1.51",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
