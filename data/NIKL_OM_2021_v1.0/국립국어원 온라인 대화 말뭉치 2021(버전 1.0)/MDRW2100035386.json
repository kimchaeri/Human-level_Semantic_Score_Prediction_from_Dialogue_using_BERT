      "utterance": [
        {
          "id": "MDRW2100035386.1.1",
          "form": "전신거울 있으세욪?",
          "original_form": "전신거울 있으세욪?",
          "speaker_id": "1",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100035386.1.2",
          "form": "전신거울은 없어요",
          "original_form": "전신거울은 없어요",
          "speaker_id": "2",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100035386.1.3",
          "form": "신발장에 있는 거울을 전신거울 삼아",
          "original_form": "신발장에 있는 거울을 전신거울 삼아",
          "speaker_id": "2",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100035386.1.4",
          "form": "오오 저랑 같군요!",
          "original_form": "오오 저랑 같군요!",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100035386.1.5",
          "form": "가끔 전신 거울 사고싶은데 놓은곳이 없으니 못사겠어요ㅜ",
          "original_form": "가끔 전신 거울 사고싶은데 놓은곳이 없으니 못사겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100035386.1.6",
          "form": "네 전신거울 놓을 곳 마련하는 것도 일이죠",
          "original_form": "네 전신거울 놓을 곳 마련하는 것도 일이죠",
          "speaker_id": "2",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100035386.1.7",
          "form": "맞아요 있는거 치울 곳도 없고 버릴만한것도 없구요",
          "original_form": "맞아요 있는거 치울 곳도 없고 버릴만한것도 없구요",
          "speaker_id": "1",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100035386.1.8",
          "form": "맞아요 새 물건 사려면 있는 물건 중에 버려야 하는데",
          "original_form": "맞아요 새 물건 사려면 있는 물건 중에 버려야 하는데",
          "speaker_id": "2",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100035386.1.9",
          "form": "버리지 않고 사들이니 점점 물건이 쌓여요",
          "original_form": "버리지 않고 사들이니 점점 물건이 쌓여요",
          "speaker_id": "2",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100035386.1.10",
          "form": "맞아요 그러니 부피큰건 최대한 피하려하게 되더라구요",
          "original_form": "맞아요 그러니 부피큰건 최대한 피하려하게 되더라구요",
          "speaker_id": "1",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100035386.1.11",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100035386.1.12",
          "form": "근데 작은 부피의 물건도 쌓이면 한가득이라",
          "original_form": "근데 작은 부피의 물건도 쌓이면 한가득이라",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100035386.1.13",
          "form": "맞아요! 주기적으로 버려야하는데 바로는 못버리겠더라구요",
          "original_form": "맞아요! 주기적으로 버려야하는데 바로는 못버리겠더라구요",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100035386.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100035386.1.15",
          "form": "매번 더 이상 물건 사지 말아야지 하면서",
          "original_form": "매번 더 이상 물건 사지 말아야지 하면서",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100035386.1.16",
          "form": "전에 사둔 건 버려야지 하면서도",
          "original_form": "전에 사둔 건 버려야지 하면서도",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100035386.1.17",
          "form": "실행에 옮기지 못하고...",
          "original_form": "실행에 옮기지 못하고...",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100035386.1.18",
          "form": "진짜요.. 사기전에 한번 둘러보면 좋은데 보질 않네요",
          "original_form": "진짜요.. 사기전에 한번 둘러보면 좋은데 보질 않네요",
          "speaker_id": "1",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100035386.1.19",
          "form": "구매할땐 사야할 이유만 생각나니까요ㅎㅎ",
          "original_form": "구매할땐 사야할 이유만 생각나니까요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100035386.1.20",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
