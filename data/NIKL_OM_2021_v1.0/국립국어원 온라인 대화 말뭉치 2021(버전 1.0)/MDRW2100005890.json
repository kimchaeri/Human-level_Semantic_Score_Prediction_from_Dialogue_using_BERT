      "utterance": [
        {
          "id": "MDRW2100005890.1.1",
          "form": "뉴스 봤어?",
          "original_form": "뉴스 봤어?",
          "speaker_id": "2",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100005890.1.2",
          "form": "네 드디어 북방왕과 남방왕이 뚜렷해지는 건가요?",
          "original_form": "네 드디어 북방왕과 남방왕이 뚜렷해지는 건가요?",
          "speaker_id": "1",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100005890.1.3",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100005890.1.4",
          "form": "한국이 선진국이 되긴 했나봐요",
          "original_form": "한국이 선진국이 되긴 했나봐요",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100005890.1.5",
          "form": "지정학적으로 중요한 위치기도 하지",
          "original_form": "지정학적으로 중요한 위치기도 하지",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100005890.1.6",
          "form": "중국과 미국의 대리전이 펼쳐진 곳이기도 하니까요",
          "original_form": "중국과 미국의 대리전이 펼쳐진 곳이기도 하니까요",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100005890.1.7",
          "form": "아직도고",
          "original_form": "아직도고",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100005890.1.8",
          "form": "맞아요 아직도 휴전인 상태죠",
          "original_form": "맞아요 아직도 휴전인 상태죠",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100005890.1.9",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100005890.1.10",
          "form": "앞으로 어떻게 역사가 진행될까요?",
          "original_form": "앞으로 어떻게 역사가 진행될까요?",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005890.1.11",
          "form": "우리가 알순없지만",
          "original_form": "우리가 알순없지만",
          "speaker_id": "2",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005890.1.12",
          "form": "한국은 중국 말고 미국과 함께할텐데요",
          "original_form": "한국은 중국 말고 미국과 함께할텐데요",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005890.1.13",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005890.1.14",
          "form": "한국이 북방왕계열이 되진 않을테니까요",
          "original_form": "한국이 북방왕계열이 되진 않을테니까요",
