      "utterance": [
        {
          "id": "MDRW2100034342.1.1",
          "form": "수강신청....어땠어요?",
          "original_form": "수강신청....어땠어요?",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100034342.1.2",
          "form": "저는 졸업한지 꽤 되어서 모르겠어요",
          "original_form": "저는 졸업한지 꽤 되어서 모르겠어요",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100034342.1.3",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100034342.1.4",
          "form": "저도 졸업한지 2년 지났어요ㅋㅋ",
          "original_form": "저도 졸업한지 2년 지났어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100034342.1.5",
          "form": "학교생활 어땠었나요?",
          "original_form": "학교생활 어땠었나요?",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100034342.1.6",
          "form": "만족스럽게 지나갔나요?",
          "original_form": "만족스럽게 지나갔나요?",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100034342.1.7",
          "form": "그러저럭 다닌거 같아요 전공을 못살렸다보니 만족은 덜하구요",
          "original_form": "그러저럭 다닌거 같아요 전공을 못살렸다보니 만족은 덜하구요",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100034342.1.8",
          "form": "만족스러우셨나요?",
          "original_form": "만족스러우셨나요?",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100034342.1.9",
          "form": "저도 전공 못살려서 돈아깝다는 생각 하고있어요",
          "original_form": "저도 전공 못살려서 돈아깝다는 생각 하고있어요",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100034342.1.10",
          "form": "뭐 그래도 졸업장은 따 두는게 맞긴 한것같기도 하고..",
          "original_form": "뭐 그래도 졸업장은 따 두는게 맞긴 한것같기도 하고..",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100034342.1.11",
          "form": "네 저도 공감해요",
          "original_form": "네 저도 공감해요",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100034342.1.12",
          "form": "동아리활동 하셨나요?",
          "original_form": "동아리활동 하셨나요?",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100034342.1.13",
          "form": "동아리는 안했어요ㅎㅎ",
          "original_form": "동아리는 안했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100034342.1.14",
          "form": "학교 통학만해도 너무 바쁘더라구요",
          "original_form": "학교 통학만해도 너무 바쁘더라구요",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100034342.1.15",
          "form": "그렇긴하죠...",
          "original_form": "그렇긴하죠...",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100034342.1.16",
          "form": "통학시간이 길었나요?",
          "original_form": "통학시간이 길었나요?",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100034342.1.17",
          "form": "저는 멀었어요ㅠㅠ",
          "original_form": "저는 멀었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100034342.1.18",
          "form": "전철로 2시간반정도?",
          "original_form": "전철로 2시간반정도?",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100034342.1.19",
          "form": "기차타면 한시간반",
          "original_form": "기차타면 한시간반",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100034342.1.20",
          "form": "헉... 진짜 고생하셨겠다",
          "original_form": "헉... 진짜 고생하셨겠다",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100034342.1.21",
          "form": "그래서 결국엔 자취했어요ㅋㅋ",
          "original_form": "그래서 결국엔 자취했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100034342.1.22",
          "form": "통학 얼마나 걸리셨어요?",
          "original_form": "통학 얼마나 걸리셨어요?",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100034342.1.23",
          "form": "저는 30분걸렸어요",
          "original_form": "저는 30분걸렸어요",
          "speaker_id": "1",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100034342.1.24",
          "form": "가깝네요!",
          "original_form": "가깝네요!",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100034342.1.25",
          "form": "네 가까웠어요.",
          "original_form": "네 가까웠어요.",
          "speaker_id": "1",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100034342.1.26",
          "form": "학교든 일이든 가까워야하더라구요...",
          "original_form": "학교든 일이든 가까워야하더라구요...",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100034342.1.27",
          "form": "너무고생하셨다...",
          "original_form": "너무고생하셨다...",
          "speaker_id": "1",
          "time": "20210827 23:55"
        }
      ]
    }
  ]
