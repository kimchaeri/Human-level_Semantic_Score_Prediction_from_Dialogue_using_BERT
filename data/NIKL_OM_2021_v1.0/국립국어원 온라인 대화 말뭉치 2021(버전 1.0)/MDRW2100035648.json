        },
        {
          "id": "MDRW2100035648.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100035648.1.3",
          "form": "친구가 이 논란 지금 터져서 다행이라고..",
          "original_form": "친구가 이 논란 지금 터져서 다행이라고..",
          "speaker_id": "1",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100035648.1.4",
          "form": "그때 터졌으면 자기네들 1승도 못했다고...",
          "original_form": "그때 터졌으면 자기네들 1승도 못했다고...",
          "speaker_id": "1",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100035648.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100035648.1.6",
          "form": "아 1승 ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "original_form": "아 1승 ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035648.1.7",
          "form": "엑 소 팬 들 은",
          "original_form": "엑 소 팬 들 은",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035648.1.8",
          "form": " 하나 지나가면",
          "original_form": "{censored} 하나 지나가면",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100035648.1.9",
          "form": "ㅋㅋㅋㅋㅋ그 띄어쓰기는 뭐예옄ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ그 띄어쓰기는 뭐예옄ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100035648.1.10",
          "form": "또 오고 . .",
          "original_form": "또 오고 . .",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100035648.1.11",
          "form": "아니 왜죠?ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니 왜죠?ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100035648.1.12",
          "form": "그니까욬ㅋㅋㄱㄱ 엑소",
          "original_form": "그니까욬ㅋㅋㄱㄱ 엑소",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100035648.1.13",
          "form": "이거는 괜찮은데???",
          "original_form": "이거는 괜찮은데???",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100035648.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ하여튼",
          "original_form": "ㅋㅋㅋㅋㅋㅋ하여튼",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100035648.1.15",
          "form": "쨋든 그 그룹에 바람둥이도",
          "original_form": "쨋든 그 그룹에 바람둥이도",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100035648.1.16",
          "form": "바람잘날 없어요...",
          "original_form": "바람잘날 없어요...",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100035648.1.17",
          "form": "안나갸그..",
          "original_form": "안나갸그..",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100035648.1.18",
          "form": "아나 맞아요맞아요",
          "original_form": "아나 맞아요맞아요",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100035648.1.19",
          "form": "애아빠도 안나가고 ..",
          "original_form": "애아빠도 안나가고 ..",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100035648.1.20",
          "form": "탈퇴한 중국인 걔는",
          "original_form": "탈퇴한 중국인 걔는",
