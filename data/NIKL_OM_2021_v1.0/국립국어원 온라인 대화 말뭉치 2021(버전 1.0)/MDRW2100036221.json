      "utterance": [
        {
          "id": "MDRW2100036221.1.1",
          "form": "안녕하세요 저녁은 드셨나요~?",
          "original_form": "안녕하세요 저녁은 드셨나요~?",
          "speaker_id": "1",
          "time": "20210830 19:16"
        },
        {
          "id": "MDRW2100036221.1.2",
          "form": "피자로 먹었어요",
          "original_form": "피자로 먹었어요",
          "speaker_id": "2",
          "time": "20210830 19:16"
        },
        {
          "id": "MDRW2100036221.1.3",
          "form": "드셨나요?",
          "original_form": "드셨나요?",
          "speaker_id": "2",
          "time": "20210830 19:16"
        },
        {
          "id": "MDRW2100036221.1.4",
          "form": "네네 저도 먹었어용ㅎㅎ",
          "original_form": "네네 저도 먹었어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100036221.1.5",
          "form": "어디 피자 드셨어요?",
          "original_form": "어디 피자 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100036221.1.6",
          "form": "영구스피자라고 맛있어요",
          "original_form": "영구스피자라고 맛있어요",
          "speaker_id": "2",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100036221.1.7",
          "form": "오 거기서 어떤 피자가 맛있어용?",
          "original_form": "오 거기서 어떤 피자가 맛있어용?",
          "speaker_id": "1",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100036221.1.8",
          "form": "한번 시켜먹어보게용",
          "original_form": "한번 시켜먹어보게용",
          "speaker_id": "1",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100036221.1.9",
          "form": "주변이있나요?",
          "original_form": "주변이있나요?",
          "speaker_id": "2",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100036221.1.10",
          "form": "고구마나 치즈프라이가 베스트더라고요",
          "original_form": "고구마나 치즈프라이가 베스트더라고요",
          "speaker_id": "2",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100036221.1.11",
          "form": "그건 한번 찾아봐야할것같은데",
          "original_form": "그건 한번 찾아봐야할것같은데",
          "speaker_id": "1",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100036221.1.12",
          "form": "잘없어서 ㅎ",
          "original_form": "잘없어서 ㅎ",
          "speaker_id": "2",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100036221.1.13",
          "form": "맛있다고하면 저는 찾아가서 포장해오기도해요ㅋㅋ",
          "original_form": "맛있다고하면 저는 찾아가서 포장해오기도해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100036221.1.14",
          "form": "오 진짜 맛있긴해서 ㅎ",
          "original_form": "오 진짜 맛있긴해서 ㅎ",
          "speaker_id": "2",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100036221.1.15",
          "form": "먹는거에 진심이라ㅋㅋㅋㅋ",
          "original_form": "먹는거에 진심이라ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:39"
        },
        {
          "id": "MDRW2100036221.1.16",
          "form": "식어도 맛있더라고요",
          "original_form": "식어도 맛있더라고요",
          "speaker_id": "2",
          "time": "20210830 19:39"
        },
        {
          "id": "MDRW2100036221.1.17",
          "form": "추천해주신거 메모해뒀다가 나중에 사먹어봐야겠어요",
          "original_form": "추천해주신거 메모해뒀다가 나중에 사먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210830 19:39"
        },
        {
          "id": "MDRW2100036221.1.18",
          "form": "넹넹 맛있었음 좋겠네요",
          "original_form": "넹넹 맛있었음 좋겠네요",
          "speaker_id": "2",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100036221.1.19",
          "form": "먹고 맛있으면 name2님 생각하기...!ㅋㅋㅋ",
