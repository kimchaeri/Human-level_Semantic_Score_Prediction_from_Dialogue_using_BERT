        {
          "id": "MDRW2100016013.1.1",
          "form": "룸메이트 같이 사는거 생각보다 괜찮네요",
          "original_form": "룸메이트 같이 사는거 생각보다 괜찮네요",
          "speaker_id": "2",
          "time": "20210828 20:12"
        },
        {
          "id": "MDRW2100016013.1.2",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210828 20:13"
        },
        {
          "id": "MDRW2100016013.1.3",
          "form": "name3name4도 다행이 좋아하네욤",
          "original_form": "&name3&&name4&도 다행이 좋아하네욤",
          "speaker_id": "2",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100016013.1.4",
          "form": "집안일은 분담하나요?",
          "original_form": "집안일은 분담하나요?",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100016013.1.5",
          "form": "특별히 하는건 없어요",
          "original_form": "특별히 하는건 없어요",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100016013.1.6",
          "form": "네 글쿤요",
          "original_form": "네 글쿤요",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100016013.1.7",
          "form": "아이는 있으신가요",
          "original_form": "아이는 있으신가요",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100016013.1.8",
          "form": "귀염둥이들 있습니당",
          "original_form": "귀염둥이들 있습니당",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100016013.1.9",
          "form": "아 여러 명인가보네요",
          "original_form": "아 여러 명인가보네요",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100016013.1.10",
          "form": "네 귀여워요",
          "original_form": "네 귀여워요",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100016013.1.11",
          "form": "네 몇명인가요",
          "original_form": "네 몇명인가요",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100016013.1.12",
          "form": "아이 2명이여",
          "original_form": "아이 2명이여",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100016013.1.13",
          "form": "오호 다둥이네요",
          "original_form": "오호 다둥이네요",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100016013.1.14",
          "form": "지금은 자고있어요",
          "original_form": "지금은 자고있어요",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100016013.1.15",
