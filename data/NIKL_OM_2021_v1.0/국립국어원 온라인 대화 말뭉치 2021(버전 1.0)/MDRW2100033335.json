        },
        {
          "id": "MDRW2100033335.1.2",
          "form": "뭐드셨어요",
          "original_form": "뭐드셨어요",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100033335.1.3",
          "form": "전 오늘",
          "original_form": "전 오늘",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100033335.1.4",
          "form": "짜글이랑 알탕 먹었어요",
          "original_form": "짜글이랑 알탕 먹었어요",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100033335.1.5",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100033335.1.6",
          "form": "저는 급식먹었어요",
          "original_form": "저는 급식먹었어요",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100033335.1.7",
          "form": "오징어가 맛있더라구요",
          "original_form": "오징어가 맛있더라구요",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100033335.1.8",
          "form": "버터구이 오징어였나요",
          "original_form": "버터구이 오징어였나요",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100033335.1.9",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100033335.1.10",
          "form": "오징어볶음이었어요",
          "original_form": "오징어볶음이었어요",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100033335.1.11",
          "form": "저 오징어볶음 잘 만들어요",
          "original_form": "저 오징어볶음 잘 만들어요",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100033335.1.12",
          "form": "오징어 많이 넣어주셨나요 영양사분이",
          "original_form": "오징어 많이 넣어주셨나요 영양사분이",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100033335.1.13",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100033335.1.14",
          "form": "엄청 많아요",
          "original_form": "엄청 많아요",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100033335.1.15",
          "form": "맛있었어요",
          "original_form": "맛있었어요",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100033335.1.16",
          "form": "근데 매웠어요ㅠㅜ",
          "original_form": "근데 매웠어요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100033335.1.17",
          "form": "ㅜㅜ 야채가 얼마 없었나요",
          "original_form": "ㅜㅜ 야채가 얼마 없었나요",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100033335.1.18",
          "form": "후식으로 음료 같은 건 안 줬나요?",
          "original_form": "후식으로 음료 같은 건 안 줬나요?",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100033335.1.19",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100033335.1.20",
          "form": "어떻게알았어요",
          "original_form": "어떻게알았어요",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100033335.1.21",
          "form": "요구르트준거",
          "original_form": "요구르트준거",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100033335.1.22",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100033335.1.23",
          "form": "매운 거 나오니까 줄 거 같았어요ㅋㅋ",
          "original_form": "매운 거 나오니까 줄 거 같았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100033335.1.24",
          "form": "요구르트 그 조그마한 건가요?",
          "original_form": "요구르트 그 조그마한 건가요?",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100033335.1.25",
          "form": "얼려먹는 요구르튼데",
          "original_form": "얼려먹는 요구르튼데",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100033335.1.26",
          "form": "이상해여",
          "original_form": "이상해여",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100033335.1.27",
          "form": "먹기불편했음ㅠㅠ",
          "original_form": "먹기불편했음ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100033335.1.28",
