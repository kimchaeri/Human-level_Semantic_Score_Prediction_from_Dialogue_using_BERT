      "utterance": [
        {
          "id": "MDRW2100015573.1.1",
          "form": "미용이나 건강 둘 중 어떤 걸 더 관심있어 하시나요",
          "original_form": "미용이나 건강 둘 중 어떤 걸 더 관심있어 하시나요",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015573.1.2",
          "form": "저는 건강쪽이요",
          "original_form": "저는 건강쪽이요",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015573.1.3",
          "form": "퇴근하면 gx하는데",
          "original_form": "퇴근하면 gx하는데",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015573.1.4",
          "form": "이제 3년정도 됐어요",
          "original_form": "이제 3년정도 됐어요",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015573.1.5",
          "form": "와 정말 꾸준히 하셨네요",
          "original_form": "와 정말 꾸준히 하셨네요",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015573.1.6",
          "form": "어떤 종류 하세요?",
          "original_form": "어떤 종류 하세요?",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015573.1.7",
          "form": "아 근데 옥상달빛 보러가신다니 너무 부러워요..",
          "original_form": "아 근데 옥상달빛 보러가신다니 너무 부러워요..",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015573.1.8",
          "form": "그거 이벤트에 응모해서 당첨됐어요",
          "original_form": "그거 이벤트에 응모해서 당첨됐어요",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015573.1.9",
          "form": "운이 좋았어요",
          "original_form": "운이 좋았어요",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015573.1.10",
          "form": "와 원래도 좋아하세요 옥달?",
          "original_form": "와 원래도 좋아하세요 옥달?",
          "speaker_id": "2",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015573.1.11",
          "form": "gx는 바벨 덤벨 이용한 운동 위주",
          "original_form": "gx는 바벨 덤벨 이용한 운동 위주",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015573.1.12",
          "form": "아..미용에 관한 얘기릉 해야하네요ㅋㅋ",
          "original_form": "아..미용에 관한 얘기릉 해야하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015573.1.13",
          "form": "오 덤벨",
          "original_form": "오 덤벨",
          "speaker_id": "2",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015573.1.14",
          "form": "저는 집에서 덤벨을 들고있어요",
          "original_form": "저는 집에서 덤벨을 들고있어요",
          "speaker_id": "2",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015573.1.15",
          "form": "근데 무게가 한개밖에 없어서 하면 할수록 여러무게를 사고싶어져요ㅋㅋ",
          "original_form": "근데 무게가 한개밖에 없어서 하면 할수록 여러무게를 사고싶어져요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015573.1.16",
          "form": "그것도 비용 많이 들지 않아요?",
          "original_form": "그것도 비용 많이 들지 않아요?",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015573.1.17",
          "form": "그래서 고민하고있어요ㅋㅋ",
          "original_form": "그래서 고민하고있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015573.1.18",
          "form": "바벨은 몇키로 드세요?",
          "original_form": "바벨은 몇키로 드세요?",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015573.1.19",
          "form": "운동따라 다른데 4~10kg 정도 들어요",
          "original_form": "운동따라 다른데 4~10kg 정도 들어요",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015573.1.20",
          "form": "다른 운동 해보신 적 있어요?",
          "original_form": "다른 운동 해보신 적 있어요?",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015573.1.21",
          "form": "다른 운동은 해본적이 없어요",
          "original_form": "다른 운동은 해본적이 없어요",
          "speaker_id": "2",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100015573.1.22",
          "form": "그래도 기회되면 웨이트 기구 사용방법은 익혀보고싶어요",
          "original_form": "그래도 기회되면 웨이트 기구 사용방법은 익혀보고싶어요",
          "speaker_id": "2",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100015573.1.23",
          "form": "천천히 시도해보시면 되죠",
          "original_form": "천천히 시도해보시면 되죠",
          "speaker_id": "1",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100015573.1.24",
          "form": "어렵지 않을거에요",
          "original_form": "어렵지 않을거에요",
          "speaker_id": "1",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100015573.1.25",
          "form": "그러길 바래요ㅋㅋ",
          "original_form": "그러길 바래요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100015573.1.26",
          "form": "유산소만 하다가 무산소로 근육 느는걸 보니",
          "original_form": "유산소만 하다가 무산소로 근육 느는걸 보니",
          "speaker_id": "2",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100015573.1.27",
          "form": "욕심이 생기더라고요",
          "original_form": "욕심이 생기더라고요",
