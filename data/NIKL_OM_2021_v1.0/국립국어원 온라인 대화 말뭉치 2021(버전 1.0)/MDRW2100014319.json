      "utterance": [
        {
          "id": "MDRW2100014319.1.1",
          "form": "제주도",
          "original_form": "제주도",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014319.1.2",
          "form": "아 제주도 정말 좋지",
          "original_form": "아 제주도 정말 좋지",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014319.1.3",
          "form": "가보고 싶어",
          "original_form": "가보고 싶어",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014319.1.4",
          "form": "코로나 끝나면 꼭 가자 엄청 좋아",
          "original_form": "코로나 끝나면 꼭 가자 엄청 좋아",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014319.1.5",
          "form": "언제 가보냐",
          "original_form": "언제 가보냐",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014319.1.6",
          "form": "그러게 말이야 우리 계획은 예전부터 세웠었는데",
          "original_form": "그러게 말이야 우리 계획은 예전부터 세웠었는데",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014319.1.7",
          "form": "그니까 계획만 세우고",
          "original_form": "그니까 계획만 세우고",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014319.1.8",
          "form": "계획으로는 벌써 한 세 번 갔어",
          "original_form": "계획으로는 벌써 한 세 번 갔어",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014319.1.9",
          "form": "인정 ㅋㅋ",
          "original_form": "인정 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014319.1.10",
          "form": "맛있는 것도 진짜 많은데",
          "original_form": "맛있는 것도 진짜 많은데",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014319.1.11",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014319.1.12",
          "form": "회 같은 해산물 종류",
          "original_form": "회 같은 해산물 종류",
          "speaker_id": "1",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014319.1.13",
          "form": "해산물 맛있지",
          "original_form": "해산물 맛있지",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014319.1.14",
          "form": "고등어회 진짜 맛있었어",
          "original_form": "고등어회 진짜 맛있었어",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014319.1.15",
          "form": "비릴 거 같은데",
          "original_form": "비릴 거 같은데",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014319.1.16",
          "form": "고등어회 비릴 줄 알았는데 진짜 하나도 안 비려",
          "original_form": "고등어회 비릴 줄 알았는데 진짜 하나도 안 비려",
