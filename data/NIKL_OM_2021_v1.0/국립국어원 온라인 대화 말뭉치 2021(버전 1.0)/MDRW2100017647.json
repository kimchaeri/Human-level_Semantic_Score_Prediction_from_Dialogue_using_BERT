      "utterance": [
        {
          "id": "MDRW2100017647.1.1",
          "form": "마늘빵 아몬드도 맛잇어요 그것도 짭쪼름",
          "original_form": "마늘빵 아몬드도 맛잇어요 그것도 짭쪼름",
          "speaker_id": "1",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017647.1.2",
          "form": "맞아요!! 진짜 취향저격이더라구요",
          "original_form": "맞아요!! 진짜 취향저격이더라구요",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017647.1.3",
          "form": "아몬드 시리즈 좋아해서 다 정복해보려구요",
          "original_form": "아몬드 시리즈 좋아해서 다 정복해보려구요",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017647.1.4",
          "form": "한번 먹기시작하면 멈출수가 없어요 ㅋㅋㅋㅋㅋ",
          "original_form": "한번 먹기시작하면 멈출수가 없어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017647.1.5",
          "form": "그니까요 햄스터마냥 계속 까먹고 있어요",
          "original_form": "그니까요 햄스터마냥 계속 까먹고 있어요",
          "speaker_id": "2",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017647.1.6",
          "form": "와사비맛 드셔보셧어요?",
          "original_form": "와사비맛 드셔보셧어요?",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017647.1.7",
          "form": "아니요 그걸 아직 못 시켰어요",
          "original_form": "아니요 그걸 아직 못 시켰어요",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017647.1.8",
          "form": "와사비 과자 좋아해서 기대하고 있어요",
          "original_form": "와사비 과자 좋아해서 기대하고 있어요",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017647.1.9",
          "form": "맛잇어요!",
          "original_form": "맛잇어요!",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017647.1.10",
          "form": "눈물 흘리며 먹는 맛 ㅠㅠ 장바구니에 넣어야겠네요",
          "original_form": "눈물 흘리며 먹는 맛 ㅠㅠ 장바구니에 넣어야겠네요",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017647.1.11",
          "form": "몇개먹을때는 맛잇네 하는데",
          "original_form": "몇개먹을때는 맛잇네 하는데",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017647.1.12",
          "form": "먹다보면 슬슬 혀가 ㅋㅋㅋㅋㅋ",
          "original_form": "먹다보면 슬슬 혀가 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017647.1.13",
          "form": "ㅋㅋㅋㅋ하루에 몇개씩만 조절하며 먹어야죠",
          "original_form": "ㅋㅋㅋㅋ하루에 몇개씩만 조절하며 먹어야죠",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017647.1.14",
          "form": "다른맛은 뭐 드셔보셧어요?",
          "original_form": "다른맛은 뭐 드셔보셧어요?",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017647.1.15",
          "form": "그 복숭아랑 별빛팡팡이랑 쿠키오레오?",
          "original_form": "그 복숭아랑 별빛팡팡이랑 쿠키오레오?",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017647.1.16",
          "form": "뭔가 엽기적인 맛 위주로 먹어봤어요",
          "original_form": "뭔가 엽기적인 맛 위주로 먹어봤어요",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017647.1.17",
          "form": "우와 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "우와 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017647.1.18",
          "form": "어땟어요?",
          "original_form": "어땟어요?",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017647.1.19",
          "form": "어 그 도전정신은 아름다웠으나",
          "original_form": "어 그 도전정신은 아름다웠으나",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017647.1.20",
          "form": "탄생하면 안되는 아몬드였다",
          "original_form": "탄생하면 안되는 아몬드였다",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017647.1.21",
          "form": "이런 느낌이었네요",
          "original_form": "이런 느낌이었네요",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017647.1.22",
          "form": "아...역시...ㅋㅋㅋㅋ",
          "original_form": "아...역시...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017647.1.23",
          "form": "혹시나 햇지만 역시나...ㅋㅋㅋㅋ",
          "original_form": "혹시나 햇지만 역시나...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017647.1.24",
          "form": "다이소에 엄청 작은 버전으로 팔길래 호기심으로 한번 사봤죠",
          "original_form": "다이소에 엄청 작은 버전으로 팔길래 호기심으로 한번 사봤죠",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017647.1.25",
          "form": "뭐가 제일 최악이엇나요 ㅋㅋ",
          "original_form": "뭐가 제일 최악이엇나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017647.1.26",
          "form": "음 전 복숭아?",
          "original_form": "음 전 복숭아?",
          "speaker_id": "2",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017647.1.27",
          "form": "참고할게요 ㅋㅋㅋㅋ",
          "original_form": "참고할게요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017647.1.28",
          "form": "복숭아맛은 역시 복숭아 본체를 먹어야한다는 교훈",
          "original_form": "복숭아맛은 역시 복숭아 본체를 먹어야한다는 교훈",
          "speaker_id": "2",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017647.1.29",
          "form": "복숭아는 절대 사먹지않는걸로 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "복숭아는 절대 사먹지않는걸로 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017647.1.30",
          "form": "과자는 뭘 좋아하세요?",
          "original_form": "과자는 뭘 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 20:44"
        }
      ]
    }
