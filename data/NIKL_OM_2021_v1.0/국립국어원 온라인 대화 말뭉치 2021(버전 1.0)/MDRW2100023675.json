      "utterance": [
        {
          "id": "MDRW2100023675.1.1",
          "form": "우리 대만여행 갔을때 딤섬 먹었던 데 이름이 뭐지?",
          "original_form": "우리 대만여행 갔을때 딤섬 먹었던 데 이름이 뭐지?",
          "speaker_id": "1",
          "time": "20210830 14:37"
        },
        {
          "id": "MDRW2100023675.1.2",
          "form": "팀호완?",
          "original_form": "팀호완?",
          "speaker_id": "2",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100023675.1.3",
          "form": "맞다 팀호완이었지",
          "original_form": "맞다 팀호완이었지",
          "speaker_id": "1",
          "time": "20210830 14:49"
        },
        {
          "id": "MDRW2100023675.1.4",
          "form": "딘타이펑도 대만인가?",
          "original_form": "딘타이펑도 대만인가?",
          "speaker_id": "1",
          "time": "20210830 14:49"
        },
        {
          "id": "MDRW2100023675.1.5",
          "form": "그럴걸?",
          "original_form": "그럴걸?",
          "speaker_id": "2",
          "time": "20210830 14:49"
        },
        {
          "id": "MDRW2100023675.1.6",
          "form": "딘타이펑이랑 팀호완이랑 헷갈려",
          "original_form": "딘타이펑이랑 팀호완이랑 헷갈려",
          "speaker_id": "1",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100023675.1.7",
          "form": "우리 팀호완만 가고 딘타이펑은 안가지않았어?",
          "original_form": "우리 팀호완만 가고 딘타이펑은 안가지않았어?",
          "speaker_id": "2",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100023675.1.8",
          "form": "그니까 우리 팀호완만 갔는데",
          "original_form": "그니까 우리 팀호완만 갔는데",
          "speaker_id": "1",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023675.1.9",
          "form": "근데 둘 다 이제 한국에 있다야",
          "original_form": "근데 둘 다 이제 한국에 있다야",
          "speaker_id": "2",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023675.1.10",
          "form": "이름이 비슷한 느낌이라 그런가?ㅋㅋㅋ",
          "original_form": "이름이 비슷한 느낌이라 그런가?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023675.1.11",
          "form": "나도 헷갈려ㅋㅋ",
          "original_form": "나도 헷갈려ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023675.1.12",
          "form": "메뉴도 같아서ㅋㅋ",
          "original_form": "메뉴도 같아서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023675.1.13",
          "form": "그니까 둘 다 있더라고",
          "original_form": "그니까 둘 다 있더라고",
          "speaker_id": "1",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023675.1.14",
          "form": "아 메뉴도 비슷한가?",
          "original_form": "아 메뉴도 비슷한가?",
          "speaker_id": "1",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023675.1.15",
          "form": "근데 난 아리차이 딤섬도 나쁘지않던데",
          "original_form": "근데 난 아리차이 딤섬도 나쁘지않던데",
          "speaker_id": "2",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023675.1.16",
          "form": "둘다 딤섬집아녀?",
          "original_form": "둘다 딤섬집아녀?",
          "speaker_id": "2",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023675.1.17",
          "form": "마자 아리차이도 진짜 맛있어",
          "original_form": "마자 아리차이도 진짜 맛있어",
          "speaker_id": "1",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023675.1.18",
          "form": "굳이 팀호완 안찾아가도 될거같아",
          "original_form": "굳이 팀호완 안찾아가도 될거같아",
          "speaker_id": "1",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023675.1.19",
          "form": "아리차이도 함 먹어줘야하는뎅",
          "original_form": "아리차이도 함 먹어줘야하는뎅",
          "speaker_id": "2",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023675.1.20",
          "form": "마자ㅜ먹을게 참 많군ㅋㅋㅋㅋ",
          "original_form": "마자ㅜ먹을게 참 많군ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100023675.1.21",
          "form": "맞아ㅋㅋ",
          "original_form": "맞아ㅋㅋ",
          "speaker_id": "2",
