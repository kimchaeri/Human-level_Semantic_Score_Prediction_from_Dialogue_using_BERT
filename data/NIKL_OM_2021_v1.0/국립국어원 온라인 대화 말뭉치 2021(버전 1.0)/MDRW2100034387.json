        },
        {
          "id": "MDRW2100034387.1.2",
          "form": "나 요즘 옛날 드라마 봐",
          "original_form": "나 요즘 옛날 드라마 봐",
          "speaker_id": "1",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100034387.1.3",
          "form": "어떤 드라마 보는 중인데",
          "original_form": "어떤 드라마 보는 중인데",
          "speaker_id": "2",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100034387.1.4",
          "form": "응답하라를 보고 있어",
          "original_form": "응답하라를 보고 있어",
          "speaker_id": "1",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100034387.1.5",
          "form": "세계 중 어떤 편을 보고 있니",
          "original_form": "세계 중 어떤 편을 보고 있니",
          "speaker_id": "2",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100034387.1.6",
          "form": "구사 버전이라는 재밌어",
          "original_form": "구사 버전이라는 재밌어",
          "speaker_id": "1",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100034387.1.7",
          "form": "응 사가 재미있긴 했지",
          "original_form": "응 사가 재미있긴 했지",
          "speaker_id": "2",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100034387.1.8",
          "form": "나는 칠봉이가 좋아",
          "original_form": "나는 칠봉이가 좋아",
          "speaker_id": "1",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100034387.1.9",
          "form": "나도 칠봉이가 되길 원했어",
          "original_form": "나도 칠봉이가 되길 원했어",
          "speaker_id": "2",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100034387.1.10",
          "form": "여주인공 취향이 특이한가봐",
          "original_form": "여주인공 취향이 특이한가봐",
          "speaker_id": "1",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100034387.1.11",
          "form": "응팔도 재밌게 봤니",
          "original_form": "응팔도 재밌게 봤니",
          "speaker_id": "2",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100034387.1.12",
          "form": "보검이 얼굴이 재밌어",
          "original_form": "보검이 얼굴이 재밌어",
          "speaker_id": "1",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100034387.1.13",
          "form": "남자 주인공이 보검이라서 좋더라",
          "original_form": "남자 주인공이 보검이라서 좋더라",
          "speaker_id": "2",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100034387.1.14",
          "form": "맞아 이번엔 마음에 들어",
          "original_form": "맞아 이번엔 마음에 들어",
          "speaker_id": "1",
