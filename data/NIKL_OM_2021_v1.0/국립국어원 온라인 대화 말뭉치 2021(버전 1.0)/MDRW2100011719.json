      "utterance": [
        {
          "id": "MDRW2100011719.1.1",
          "form": "우리 또 코로나끝나면",
          "original_form": "우리 또 코로나끝나면",
          "speaker_id": "1",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011719.1.2",
          "form": "여행가서",
          "original_form": "여행가서",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011719.1.3",
          "form": "여행가야지",
          "original_form": "여행가야지",
          "speaker_id": "1",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011719.1.4",
          "form": "돈쓰고 싶다",
          "original_form": "돈쓰고 싶다",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011719.1.5",
          "form": "일단 끝나기 전에 경주 한번 가자",
          "original_form": "일단 끝나기 전에 경주 한번 가자",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011719.1.6",
          "form": "나 목요일에 마지막 수업",
          "original_form": "나 목요일에 마지막 수업",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011719.1.7",
          "form": "오 돈쓰고싶대 대박",
          "original_form": "오 돈쓰고싶대 대박",
          "speaker_id": "1",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011719.1.8",
          "form": "언니멋지다",
          "original_form": "언니멋지다",
          "speaker_id": "1",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011719.1.9",
          "form": "무슨수업?",
          "original_form": "무슨수업?",
          "speaker_id": "1",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011719.1.10",
          "form": "12시에 끝나니까",
          "original_form": "12시에 끝나니까",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011719.1.11",
          "form": "학교 수업",
          "original_form": "학교 수업",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011719.1.12",
          "form": "돈은 없어도 쓰고 싶음,,",
          "original_form": "돈은 없어도 쓰고 싶음,,",
          "speaker_id": "2",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011719.1.13",
          "form": "목요일에 가서",
          "original_form": "목요일에 가서",
          "speaker_id": "2",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011719.1.14",
          "form": "한밤 자고 금요일 밤에 오는거지",
          "original_form": "한밤 자고 금요일 밤에 오는거지",
          "speaker_id": "2",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011719.1.15",
          "form": "어때",
          "original_form": "어때",
          "speaker_id": "2",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011719.1.16",
          "form": "좋지ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "좋지ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011719.1.17",
          "form": "월화목 대면 수업 있음",
          "original_form": "월화목 대면 수업 있음",
          "speaker_id": "2",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011719.1.18",
          "form": "와우....최고네",
          "original_form": "와우....최고네",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011719.1.19",
          "form": "즐겁겠다",
          "original_form": "즐겁겠다",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011719.1.20",
          "form": "진짜 ㄱ ㅐ 짜증이다",
          "original_form": "진짜 ㄱ ㅐ 짜증이다",
          "speaker_id": "2",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011719.1.21",
          "form": "전라도 쪽도 가보고싶어",
          "original_form": "전라도 쪽도 가보고싶어",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011719.1.22",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011719.1.23",
          "form": "순천 이쪽이 엄청이쁘다던데",
          "original_form": "순천 이쪽이 엄청이쁘다던데",
          "speaker_id": "1",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011719.1.24",
          "form": "전라도 어디??",
          "original_form": "전라도 어디??",
          "speaker_id": "2",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011719.1.25",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011719.1.26",
          "form": "나 순천만 진짜 좋아해",
          "original_form": "나 순천만 진짜 좋아해",
          "speaker_id": "2",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011719.1.27",
          "form": "사실 전라도쪽 잘 몰라",
          "original_form": "사실 전라도쪽 잘 몰라",
          "speaker_id": "1",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011719.1.28",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011719.1.29",
          "form": "가봄??",
          "original_form": "가봄??",
          "speaker_id": "1",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011719.1.30",
          "form": "응 2번인가 가봤어",
          "original_form": "응 2번인가 가봤어",
          "speaker_id": "2",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011719.1.31",
          "form": "초가집들 이뻐",
          "original_form": "초가집들 이뻐",
          "speaker_id": "2",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011719.1.32",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210824 15:24"
        }
      ]
    }
  ]
