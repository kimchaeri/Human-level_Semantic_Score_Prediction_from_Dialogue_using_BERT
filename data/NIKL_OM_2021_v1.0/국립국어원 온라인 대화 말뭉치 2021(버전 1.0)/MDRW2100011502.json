      "utterance": [
        {
          "id": "MDRW2100011502.1.1",
          "form": "아빠랑그렇게연끊고",
          "original_form": "아빠랑그렇게연끊고",
          "speaker_id": "1",
          "time": "20210824 13:22"
        },
        {
          "id": "MDRW2100011502.1.2",
          "form": "엄마한테조금만기다렸다가",
          "original_form": "엄마한테조금만기다렸다가",
          "speaker_id": "1",
          "time": "20210824 13:22"
        },
        {
          "id": "MDRW2100011502.1.3",
          "form": "나졸업하면같이나가작ㅎ",
          "original_form": "나졸업하면같이나가작ㅎ",
          "speaker_id": "1",
          "time": "20210824 13:22"
        },
        {
          "id": "MDRW2100011502.1.4",
          "form": "컬리지학비이런거내가다내준다고",
          "original_form": "컬리지학비이런거내가다내준다고",
          "speaker_id": "1",
          "time": "20210824 13:22"
        },
        {
          "id": "MDRW2100011502.1.5",
          "form": "했었는데",
          "original_form": "했었는데",
          "speaker_id": "1",
          "time": "20210824 13:22"
        },
        {
          "id": "MDRW2100011502.1.6",
          "form": "죽어도싫대",
          "original_form": "죽어도싫대",
          "speaker_id": "1",
          "time": "20210824 13:23"
        },
        {
          "id": "MDRW2100011502.1.7",
          "form": "아이고 세상에",
          "original_form": "아이고 세상에",
          "speaker_id": "2",
          "time": "20210824 13:23"
        },
        {
          "id": "MDRW2100011502.1.8",
          "form": "자기도아빠화나는데",
          "original_form": "자기도아빠화나는데",
          "speaker_id": "1",
          "time": "20210824 13:23"
        },
        {
          "id": "MDRW2100011502.1.9",
          "form": "무조건나보고사과하래",
          "original_form": "무조건나보고사과하래",
          "speaker_id": "1",
          "time": "20210824 13:23"
        },
        {
          "id": "MDRW2100011502.1.10",
          "form": "그래서",
          "original_form": "그래서",
          "speaker_id": "1",
          "time": "20210824 13:23"
        },
        {
          "id": "MDRW2100011502.1.11",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210824 13:23"
        },
        {
          "id": "MDRW2100011502.1.12",
          "form": "그냥둘다신경끄고",
          "original_form": "그냥둘다신경끄고",
          "speaker_id": "1",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011502.1.13",
          "form": "나혼자나가려고",
          "original_form": "나혼자나가려고",
          "speaker_id": "1",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011502.1.14",
          "form": "아이고 그럼 어쩔 수 없지",
          "original_form": "아이고 그럼 어쩔 수 없지",
          "speaker_id": "2",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011502.1.15",
          "form": "최선의 선택이네",
          "original_form": "최선의 선택이네",
          "speaker_id": "2",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011502.1.16",
          "form": "너는어머님한테잘설득해보ㅓ",
          "original_form": "너는어머님한테잘설득해보ㅓ",
          "speaker_id": "1",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011502.1.17",
          "form": "아니 근데 우리 어머니께서는 여기 계시는 걸 좋아하시는 것 같아",
          "original_form": "아니 근데 우리 어머니께서는 여기 계시는 걸 좋아하시는 것 같아",
          "speaker_id": "2",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100011502.1.18",
          "form": "여행 개념은 괜찮은데 이민까지는 아니신 것 같음",
          "original_form": "여행 개념은 괜찮은데 이민까지는 아니신 것 같음",
          "speaker_id": "2",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100011502.1.19",
          "form": "아니면 너 자리잡고 나서 설득해보면",
          "original_form": "아니면 너 자리잡고 나서 설득해보면",
          "speaker_id": "1",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011502.1.20",
          "form": "더 괜찮을지도 모름",
          "original_form": "더 괜찮을지도 모름",
          "speaker_id": "1",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011502.1.21",
          "form": "확신이없어서망설이시는걸수도",
          "original_form": "확신이없어서망설이시는걸수도",
          "speaker_id": "1",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011502.1.22",
          "form": "있음",
          "original_form": "있음",
          "speaker_id": "1",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011502.1.23",
          "form": "아 그렇겠네 막 대 인기 작가 된 다음에",
          "original_form": "아 그렇겠네 막 대 인기 작가 된 다음에",
          "speaker_id": "2",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011502.1.24",
          "form": "미국에 크다란 펜샨 하나 사서",
          "original_form": "미국에 크다란 펜샨 하나 사서",
          "speaker_id": "2",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011502.1.25",
          "form": "ㅇㅇㅇ맞음. 적어도 시민권정도 따고나서",
          "original_form": "ㅇㅇㅇ맞음. 적어도 시민권정도 따고나서",
          "speaker_id": "1",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011502.1.26",
          "form": "부르는게더괜찮을수도",
          "original_form": "부르는게더괜찮을수도",
          "speaker_id": "1",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011502.1.27",
          "form": "아 그럴 수도 있지",
          "original_form": "아 그럴 수도 있지",
          "speaker_id": "2",
          "time": "20210824 13:27"
        }
      ]
    }
  ]
