        {
          "id": "MDRW2100013264.1.1",
          "form": "버블티 먹구싶다",
          "original_form": "버블티 먹구싶다",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013264.1.2",
          "form": "으헝",
          "original_form": "으헝",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013264.1.3",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "1",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013264.1.4",
          "form": "아마스빈",
          "original_form": "아마스빈",
          "speaker_id": "1",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013264.1.5",
          "form": "수빈이 먹고싶다",
          "original_form": "수빈이 먹고싶다",
          "speaker_id": "1",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013264.1.6",
          "form": "오레오쉐니크",
          "original_form": "오레오쉐니크",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013264.1.7",
          "form": "캬 진짜맛있는데",
          "original_form": "캬 진짜맛있는데",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013264.1.8",
          "form": "나두...",
          "original_form": "나두...",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013264.1.9",
          "form": "어레오짱짱",
          "original_form": "어레오짱짱",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013264.1.10",
          "form": "진짜 오레오가젤좋아",
          "original_form": "진짜 오레오가젤좋아",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013264.1.11",
          "form": "나는 녹차는싫어",
          "original_form": "나는 녹차는싫어",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013264.1.12",
          "form": "오레오가 좋고",
          "original_form": "오레오가 좋고",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013264.1.13",
          "form": "그담엔 레자몽",
          "original_form": "그담엔 레자몽",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013264.1.14",
          "form": "내친구가 레자몽 알려줬지",
          "original_form": "내친구가 레자몽 알려줬지",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013264.1.15",
          "form": "아주맛나",
          "original_form": "아주맛나",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013264.1.16",
          "form": "레자몽에다가",
          "original_form": "레자몽에다가",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013264.1.17",
          "form": "알로에 펄 넣으면",
          "original_form": "알로에 펄 넣으면",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013264.1.18",
          "form": "극락이다",
          "original_form": "극락이다",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013264.1.19",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013264.1.20",
          "form": "무조건 알로에야",
          "original_form": "무조건 알로에야",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013264.1.21",
          "form": "타피오카는",
          "original_form": "타피오카는",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013264.1.22",
          "form": "오레오",
          "original_form": "오레오",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013264.1.23",
          "form": "코코넛은",
          "original_form": "코코넛은",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013264.1.24",
          "form": "딸기요거트에 넣어야해",
          "original_form": "딸기요거트에 넣어야해",
          "speaker_id": "2",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013264.1.25",
          "form": "또 나머지펄",
          "original_form": "또 나머지펄",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013264.1.26",
          "form": "하나 머있더라",
          "original_form": "하나 머있더라",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013264.1.27",
          "form": "기억이 안나네",
          "original_form": "기억이 안나네",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013264.1.28",
          "form": "마저..",
          "original_form": "마저..",
          "speaker_id": "2",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013264.1.29",
          "form": "4갠가 5개엿던드스",
          "original_form": "4갠가 5개엿던드스",
          "speaker_id": "2",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013264.1.30",
          "form": "듯?",
          "original_form": "듯?",
          "speaker_id": "2",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013264.1.31",
          "form": "검색해봐야겟다",
          "original_form": "검색해봐야겟다",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013264.1.32",
          "form": "멀까 과연",
          "original_form": "멀까 과연",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013264.1.33",
          "form": "두구두구",
          "original_form": "두구두구",
          "speaker_id": "2",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013264.1.34",
          "form": "마지막 펄은?",
          "original_form": "마지막 펄은?",
          "speaker_id": "2",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013264.1.35",
          "form": "좀이따 공개됩니다",
