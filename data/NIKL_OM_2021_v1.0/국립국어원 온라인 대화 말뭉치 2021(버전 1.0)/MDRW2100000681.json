        },
        {
          "id": "MDRW2100000681.1.2",
          "form": "못먹습니다 사실",
          "original_form": "못먹습니다 사실",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000681.1.3",
          "form": "어쩌다 닉네임이..",
          "original_form": "어쩌다 닉네임이..",
          "speaker_id": "1",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000681.1.4",
          "form": "없어서 못먹어요",
          "original_form": "없어서 못먹어요",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000681.1.5",
          "form": "재치있으시네요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "재치있으시네요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000681.1.6",
          "form": "오늘 점심 뭐드셨나요",
          "original_form": "오늘 점심 뭐드셨나요",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000681.1.7",
          "form": "닉값으로 name2면 좋은데",
          "original_form": "닉값으로 &name2&면 좋은데",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000681.1.8",
          "form": "햇반 닭곰탕 먹었어요",
          "original_form": "햇반 닭곰탕 먹었어요",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000681.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그거 마싯나요??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그거 마싯나요??",
          "speaker_id": "1",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000681.1.10",
          "form": "한끼 때우는거죠 뭐",
          "original_form": "한끼 때우는거죠 뭐",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000681.1.11",
          "form": "20개를 사버려서",
          "original_form": "20개를 사버려서",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000681.1.12",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000681.1.13",
          "form": "안질리나요",
          "original_form": "안질리나요",
          "speaker_id": "1",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000681.1.14",
          "form": "닉네임 보세요",
          "original_form": "닉네임 보세요",
          "speaker_id": "2",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000681.1.15",
          "form": "간절한 외침입니다",
          "original_form": "간절한 외침입니다",
          "speaker_id": "2",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000681.1.16",
          "form": "제발 name2",
          "original_form": "제발 &name2&",
          "speaker_id": "2",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000681.1.17",
          "form": "아아..ㅠㅠㅠㅠ",
          "original_form": "아아..ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000681.1.18",
          "form": "저녁은name2드세요",
          "original_form": "저녁은&name2&드세요",
          "speaker_id": "1",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000681.1.19",
          "form": "아니면 내일 점심..",
          "original_form": "아니면 내일 점심..",
          "speaker_id": "1",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000681.1.20",
          "form": "저 끔찍한 닭곰탕들을 먹기 전까진 안돼요",
          "original_form": "저 끔찍한 닭곰탕들을 먹기 전까진 안돼요",
          "speaker_id": "2",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000681.1.21",
          "form": "여자친구가 엄청 꼽줍니다",
          "original_form": "여자친구가 엄청 꼽줍니다",
          "speaker_id": "2",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000681.1.22",
          "form": "몇개남으셧나요",
          "original_form": "몇개남으셧나요",
          "speaker_id": "1",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000681.1.23",
          "form": "12개요...",
          "original_form": "12개요...",
          "speaker_id": "2",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000681.1.24",
          "form": "아..여자친구랑 사이좋게",
          "original_form": "아..여자친구랑 사이좋게",
          "speaker_id": "1",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000681.1.25",
          "form": "6개씩 나눠드세요",
          "original_form": "6개씩 나눠드세요",
          "speaker_id": "1",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000681.1.26",
          "form": "여자친구는 하나 먹더니 나머지 저한테 집어던졌어요",
          "original_form": "여자친구는 하나 먹더니 나머지 저한테 집어던졌어요",
          "speaker_id": "2",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000681.1.27",
          "form": "오해하지마세요 그런 취향은 아닙니다",
          "original_form": "오해하지마세요 그런 취향은 아닙니다",
          "speaker_id": "2",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000681.1.28",
          "form": "음~~",
          "original_form": "음~~",
          "speaker_id": "1",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000681.1.29",
          "form": "혼자 12개 다 드셔야겟네요",
          "original_form": "혼자 12개 다 드셔야겟네요",
          "speaker_id": "1",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000681.1.30",
          "form": "혹시 놀리시는건가요",
          "original_form": "혹시 놀리시는건가요",
          "speaker_id": "2",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000681.1.31",
          "form": "닭곰탕 맛 좀 보실래요?",
          "original_form": "닭곰탕 맛 좀 보실래요?",
          "speaker_id": "2",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000681.1.32",
          "form": "아니에용",
          "original_form": "아니에용",
          "speaker_id": "1",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000681.1.33",
          "form": "여자친구분이 던진거보면",
          "original_form": "여자친구분이 던진거보면",
          "speaker_id": "1",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000681.1.34",
          "form": "맛없나봐요",
