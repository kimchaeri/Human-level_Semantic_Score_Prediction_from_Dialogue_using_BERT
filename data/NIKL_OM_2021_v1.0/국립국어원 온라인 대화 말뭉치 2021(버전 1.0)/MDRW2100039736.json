      "utterance": [
        {
          "id": "MDRW2100039736.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039736.1.2",
          "form": "백신맞으셨나요?",
          "original_form": "백신맞으셨나요?",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039736.1.3",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039736.1.4",
          "form": "네 저 월요일에 맞았어요",
          "original_form": "네 저 월요일에 맞았어요",
          "speaker_id": "2",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039736.1.5",
          "form": "오 부작용 있으셨어요?",
          "original_form": "오 부작용 있으셨어요?",
          "speaker_id": "1",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100039736.1.6",
          "form": "저는 근육통 있었어요 ㅎㅎ",
          "original_form": "저는 근육통 있었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100039736.1.7",
          "form": "이틀정도 아프더니 괜찮아졌어요",
          "original_form": "이틀정도 아프더니 괜찮아졌어요",
          "speaker_id": "2",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100039736.1.8",
          "form": "백신 아직이세요?",
          "original_form": "백신 아직이세요?",
          "speaker_id": "2",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100039736.1.9",
          "form": "저는 8월에 1차맞았어요",
          "original_form": "저는 8월에 1차맞았어요",
          "speaker_id": "1",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100039736.1.10",
          "form": "어머 그러셨구나",
          "original_form": "어머 그러셨구나",
          "speaker_id": "2",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100039736.1.11",
          "form": "화이자 맞으셨어요?",
          "original_form": "화이자 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100039736.1.12",
          "form": "네화이자!",
          "original_form": "네화이자!",
          "speaker_id": "1",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100039736.1.13",
          "form": "저두욤",
          "original_form": "저두욤",
          "speaker_id": "2",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100039736.1.14",
          "form": "부작용 있으셨어요?",
          "original_form": "부작용 있으셨어요?",
          "speaker_id": "2",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100039736.1.15",
          "form": "근육통이 며칠있었어요ㅋㅋ",
          "original_form": "근육통이 며칠있었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100039736.1.16",
          "form": "ㅋㅋㅋ다 비슷한가 봐요",
          "original_form": "ㅋㅋㅋ다 비슷한가 봐요",
          "speaker_id": "2",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100039736.1.17",
          "form": "저는 타이레놀 먹었는데도 효과가 없더라구요",
          "original_form": "저는 타이레놀 먹었는데도 효과가 없더라구요",
          "speaker_id": "2",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100039736.1.18",
          "form": "진짜 무서웠는데",
          "original_form": "진짜 무서웠는데",
          "speaker_id": "2",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100039736.1.19",
          "form": "저는타이레놀도 잘안들어서ㅠ 난리였어요",
          "original_form": "저는타이레놀도 잘안들어서ㅠ 난리였어요",
          "speaker_id": "1",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100039736.1.20",
          "form": "저도!",
          "original_form": "저도!",
          "speaker_id": "1",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100039736.1.21",
          "form": "ㅠㅠ 진짜 걱정됬네요 ㅠㅠ",
          "original_form": "ㅠㅠ 진짜 걱정됬네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 21:07"
        },
        {
          "id": "MDRW2100039736.1.22",
          "form": "뉴스 나오는 거 아닌가 이러면서 잠들었어요 ㅋㅋㅋ",
          "original_form": "뉴스 나오는 거 아닌가 이러면서 잠들었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:07"
        },
        {
          "id": "MDRW2100039736.1.23",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100039736.1.24",
          "form": "2차도걱정되네요",
          "original_form": "2차도걱정되네요",
