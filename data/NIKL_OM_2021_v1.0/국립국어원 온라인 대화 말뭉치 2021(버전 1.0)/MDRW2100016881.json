      "utterance": [
        {
          "id": "MDRW2100016881.1.1",
          "form": "한그릇에 얼마에요?",
          "original_form": "한그릇에 얼마에요?",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016881.1.2",
          "form": "한그릇에 얼마에요?",
          "original_form": "한그릇에 얼마에요?",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016881.1.3",
          "form": "16000원이여ㅋㅋ",
          "original_form": "16000원이여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:04"
        },
        {
          "id": "MDRW2100016881.1.4",
          "form": "ㄷㄷ",
          "original_form": "ㄷㄷ",
          "speaker_id": "2",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016881.1.5",
          "form": "고급진곳인가",
          "original_form": "고급진곳인가",
          "speaker_id": "2",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016881.1.6",
          "form": "미슐랭받았다고 그런 걸지도요...",
          "original_form": "미슐랭받았다고 그런 걸지도요...",
          "speaker_id": "1",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016881.1.7",
          "form": "근데 맛있긴 제일 맛있었어요",
          "original_form": "근데 맛있긴 제일 맛있었어요",
          "speaker_id": "1",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016881.1.8",
          "form": "청담쪽에 있는곳인가요",
          "original_form": "청담쪽에 있는곳인가요",
          "speaker_id": "2",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016881.1.9",
          "form": "지금도 그 맛이 그대로인지는 모르겠지만",
          "original_form": "지금도 그 맛이 그대로인지는 모르겠지만",
          "speaker_id": "1",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016881.1.10",
          "form": "아뇨 연남",
          "original_form": "아뇨 연남",
          "speaker_id": "1",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016881.1.11",
          "form": "국밥 좋아한다더니",
          "original_form": "국밥 좋아한다더니",
          "speaker_id": "2",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016881.1.12",
          "form": "고급진 입맛이구만요",
          "original_form": "고급진 입맛이구만요",
          "speaker_id": "2",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016881.1.13",
          "form": "배신자",
          "original_form": "배신자",
          "speaker_id": "2",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016881.1.14",
          "form": "팟타이는 얼마예요 거기",
          "original_form": "팟타이는 얼마예요 거기",
          "speaker_id": "2",
          "time": "20210826 16:08"
        },
        {
          "id": "MDRW2100016881.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ전 의외로 미식가랍니다?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ전 의외로 미식가랍니다?",
          "speaker_id": "1",
          "time": "20210826 16:08"
        },
        {
          "id": "MDRW2100016881.1.16",
          "form": "팟타이도 비쌀텐데",
          "original_form": "팟타이도 비쌀텐데",
          "speaker_id": "1",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016881.1.17",
          "form": "전 똠얌꿍보다는 팟타이 좋아해요",
          "original_form": "전 똠얌꿍보다는 팟타이 좋아해요",
          "speaker_id": "2",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016881.1.18",
          "form": "13000원입니다 고객님",
          "original_form": "13000원입니다 고객님",
          "speaker_id": "1",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016881.1.19",
          "form": "아예 피시소스 맛좀봐라 하는 음식이 나은듯",
          "original_form": "아예 피시소스 맛좀봐라 하는 음식이 나은듯",
          "speaker_id": "2",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016881.1.20",
          "form": "허허",
          "original_form": "허허",
          "speaker_id": "2",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016881.1.21",
          "form": "13000원이면 저희 동네에선 세그릇 잡숫겠네요",
          "original_form": "13000원이면 저희 동네에선 세그릇 잡숫겠네요",
          "speaker_id": "2",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016881.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ피시소스 맛 좀 봐라 음식이 뭔가요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ피시소스 맛 좀 봐라 음식이 뭔가요",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016881.1.23",
          "form": "아예 그냥 그맛으로 먹는 음식 있잖아요",
          "original_form": "아예 그냥 그맛으로 먹는 음식 있잖아요",
          "speaker_id": "2",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016881.1.24",
          "form": "ㅋㅋㅋㅋㅋ비싸요.  비싸...",
          "original_form": "ㅋㅋㅋㅋㅋ비싸요.  비싸...",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016881.1.25",
          "form": "아아ㅋ",
          "original_form": "아아ㅋ",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016881.1.26",
          "form": "지금 가격오른걸거에요",
          "original_form": "지금 가격오른걸거에요",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016881.1.27",
          "form": "대학교 2학년 때 태국친구 한 명 있어서",
          "original_form": "대학교 2학년 때 태국친구 한 명 있어서",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016881.1.28",
          "form": "태국친구가 인정한 그맛인가요?",
          "original_form": "태국친구가 인정한 그맛인가요?",
          "speaker_id": "2",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100016881.1.29",
          "form": "언젠가 가봐야겠다",
          "original_form": "언젠가 가봐야겠다",
          "speaker_id": "2",
          "time": "20210826 16:12"
        }
      ]
    }
