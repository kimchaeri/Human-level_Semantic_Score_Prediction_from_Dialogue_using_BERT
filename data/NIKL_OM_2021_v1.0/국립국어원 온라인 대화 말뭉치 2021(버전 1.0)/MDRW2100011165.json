      "utterance": [
        {
          "id": "MDRW2100011165.1.1",
          "form": "해외는 어디가 좋으셨어요?",
          "original_form": "해외는 어디가 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210824 02:07"
        },
        {
          "id": "MDRW2100011165.1.2",
          "form": "추천여행지있나요?",
          "original_form": "추천여행지있나요?",
          "speaker_id": "2",
          "time": "20210824 02:07"
        },
        {
          "id": "MDRW2100011165.1.3",
          "form": "대만 너무 좋았어요!",
          "original_form": "대만 너무 좋았어요!",
          "speaker_id": "1",
          "time": "20210824 02:09"
        },
        {
          "id": "MDRW2100011165.1.4",
          "form": "대만 저도~  날씨가 넘더워서 글치ㅎ",
          "original_form": "대만 저도~  날씨가 넘더워서 글치ㅎ",
          "speaker_id": "2",
          "time": "20210824 02:09"
        },
        {
          "id": "MDRW2100011165.1.5",
          "form": "근데 전 향식료에 약해서 음식은 안맞는것도 있더라구요",
          "original_form": "근데 전 향식료에 약해서 음식은 안맞는것도 있더라구요",
          "speaker_id": "2",
          "time": "20210824 02:10"
        },
        {
          "id": "MDRW2100011165.1.6",
          "form": "악 세븐일레븐 향신료냄새가 아직도 기억에 남네요",
          "original_form": "악 세븐일레븐 향신료냄새가 아직도 기억에 남네요",
          "speaker_id": "1",
          "time": "20210824 02:11"
        },
        {
          "id": "MDRW2100011165.1.7",
          "form": "전 소세지요ㅋ",
          "original_form": "전 소세지요ㅋ",
          "speaker_id": "2",
          "time": "20210824 02:12"
        },
        {
          "id": "MDRW2100011165.1.8",
          "form": "지파이도 그향이나던데",
          "original_form": "지파이도 그향이나던데",
          "speaker_id": "2",
          "time": "20210824 02:12"
        },
        {
          "id": "MDRW2100011165.1.9",
          "form": "와 소시지 한입먹고 버렸어요ㅋㅋㅋ",
          "original_form": "와 소시지 한입먹고 버렸어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 02:13"
        },
        {
          "id": "MDRW2100011165.1.10",
          "form": "친구랑 넘 맛있어보여서 소세지두개샀는데 한입먹고  휴지통으로",
          "original_form": "친구랑 넘 맛있어보여서 소세지두개샀는데 한입먹고  휴지통으로",
          "speaker_id": "2",
          "time": "20210824 02:13"
        },
        {
          "id": "MDRW2100011165.1.11",
          "form": "우육면은 진짜 맛있었는데ㅠㅠ",
          "original_form": "우육면은 진짜 맛있었는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 02:13"
        },
        {
          "id": "MDRW2100011165.1.12",
          "form": "제친구인줄ㅎㅎ",
          "original_form": "제친구인줄ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 02:14"
        },
        {
          "id": "MDRW2100011165.1.13",
          "form": "저흰그래서 최대한 향 없는데 찾아갔어요",
          "original_form": "저흰그래서 최대한 향 없는데 찾아갔어요",
          "speaker_id": "2",
          "time": "20210824 02:14"
        },
        {
          "id": "MDRW2100011165.1.14",
          "form": "블로그 검색해서",
          "original_form": "블로그 검색해서",
          "speaker_id": "2",
          "time": "20210824 02:14"
        },
        {
          "id": "MDRW2100011165.1.15",
          "form": "몇년도에 다녀오셨어요??",
          "original_form": "몇년도에 다녀오셨어요??",
          "speaker_id": "1",
          "time": "20210824 02:15"
        },
        {
          "id": "MDRW2100011165.1.16",
          "form": "저 4년전쯤",
          "original_form": "저 4년전쯤",
          "speaker_id": "2",
          "time": "20210824 02:16"
        },
        {
          "id": "MDRW2100011165.1.17",
          "form": "선배님이시군요 저는 3년전쯤ㅎㅎ",
          "original_form": "선배님이시군요 저는 3년전쯤ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 02:17"
        },
        {
          "id": "MDRW2100011165.1.18",
          "form": "ㅎㅎ 음식빼곤 그래도 다좋더라구요",
          "original_form": "ㅎㅎ 음식빼곤 그래도 다좋더라구요",
          "speaker_id": "2",
          "time": "20210824 02:19"
        }
      ]
