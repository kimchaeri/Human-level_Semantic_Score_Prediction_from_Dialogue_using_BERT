      "utterance": [
        {
          "id": "MDRW2100036039.1.1",
          "form": "우동 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "우동 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100036039.1.2",
          "form": "우리도 미역국 있는데 ㅋㅋㅋㅋ",
          "original_form": "우리도 미역국 있는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100036039.1.3",
          "form": "어 우동 좋다 ㅎㅎㅎ",
          "original_form": "어 우동 좋다 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100036039.1.4",
          "form": "언제먹었는지 기억도 안나네 ㅋㅋㅋ",
          "original_form": "언제먹었는지 기억도 안나네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100036039.1.5",
          "form": "점심 뭐 먹었노??",
          "original_form": "점심 뭐 먹었노??",
          "speaker_id": "1",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100036039.1.6",
          "form": "맨날 면달라해여;;",
          "original_form": "맨날 면달라해여;;",
          "speaker_id": "2",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100036039.1.7",
          "form": "누가??",
          "original_form": "누가??",
          "speaker_id": "1",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100036039.1.8",
          "form": "지는 열무김치랑 계란이랑 비빔밥 ㅋㅋㅋㅋ",
          "original_form": "지는 열무김치랑 계란이랑 비빔밥 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100036039.1.9",
          "form": "첫찌요ㅠ",
          "original_form": "첫찌요ㅠ",
          "speaker_id": "2",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100036039.1.10",
          "form": "다이어트 한다면서 많이 먹었네 니",
          "original_form": "다이어트 한다면서 많이 먹었네 니",
          "speaker_id": "1",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100036039.1.11",
          "form": "둘찌는 내 김치만 먹고..",
          "original_form": "둘찌는 내 김치만 먹고..",
          "speaker_id": "2",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100036039.1.12",
          "form": "오 김치를 잘먹나보네",
          "original_form": "오 김치를 잘먹나보네",
          "speaker_id": "1",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100036039.1.13",
          "form": "ㅋㅋㅋㅋㅋ아..언니한테얘기했져??ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아..언니한테얘기했져??ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100036039.1.14",
          "form": "나도 요새는\n면이 땡기네 ㅋㅋㅋㅋㄱ",
          "original_form": "나도 요새는\n면이 땡기네 ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100036039.1.15",
          "form": "어 ㅋㅋㅋㄱ 니 다이어트한다했잖아",
          "original_form": "어 ㅋㅋㅋㄱ 니 다이어트한다했잖아",
          "speaker_id": "1",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100036039.1.16",
          "form": "오늘부터 다이어트 한다면서",
          "original_form": "오늘부터 다이어트 한다면서",
          "speaker_id": "1",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100036039.1.17",
          "form": "시작부터 거짓말이고",
          "original_form": "시작부터 거짓말이고",
          "speaker_id": "1",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100036039.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ언니 아닌줄...다 뽀록 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ언니 아닌줄...다 뽀록 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100036039.1.19",
          "form": "다 기억하고 있다",
          "original_form": "다 기억하고 있다",
          "speaker_id": "1",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100036039.1.20",
          "form": "이 부른 배를 끌어안고 자야겠음 ㅋㅋㅋㅋㅋ",
          "original_form": "이 부른 배를 끌어안고 자야겠음 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100036039.1.21",
          "form": "웃기지마라",
          "original_form": "웃기지마라",
          "speaker_id": "1",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100036039.1.22",
          "form": "저녁 또 먹을거잖아 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저녁 또 먹을거잖아 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100036039.1.23",
          "form": "난 수제비 먹었다 점심에",
          "original_form": "난 수제비 먹었다 점심에",
          "speaker_id": "1",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100036039.1.24",
          "form": "요새 왜이래 면이 땡기노",
          "original_form": "요새 왜이래 면이 땡기노",
          "speaker_id": "1",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100036039.1.25",
          "form": "저녁은 최대한 노력해보께요 ㅋㅋㅋㅋ",
          "original_form": "저녁은 최대한 노력해보께요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100036039.1.26",
          "form": "난도 수제비ㅠㅠ",
          "original_form": "난도 수제비ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100036039.1.27",
          "form": "글네여~난 요즘 밀가루 먹으니 소화가 안되여;;",
          "original_form": "글네여~난 요즘 밀가루 먹으니 소화가 안되여;;",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100036039.1.28",
          "form": "안이랬는데ㅠ",
          "original_form": "안이랬는데ㅠ",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100036039.1.29",
          "form": "내시경 한번 해봐",
          "original_form": "내시경 한번 해봐",
          "speaker_id": "1",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100036039.1.30",
          "form": "소화가 왜 안되노",
          "original_form": "소화가 왜 안되노",
          "speaker_id": "1",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100036039.1.31",
          "form": "너무 빨리 먹는거 아니가",
          "original_form": "너무 빨리 먹는거 아니가",
          "speaker_id": "1",
