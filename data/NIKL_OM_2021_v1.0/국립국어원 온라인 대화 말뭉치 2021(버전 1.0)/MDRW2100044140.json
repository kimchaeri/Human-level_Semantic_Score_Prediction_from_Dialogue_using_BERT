        },
        {
          "id": "MDRW2100044140.1.2",
          "form": "트레이더스에서 샀어요",
          "original_form": "트레이더스에서 샀어요",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044140.1.3",
