      "utterance": [
        {
          "id": "MDRW2100013672.1.1",
          "form": "먹을꺼 얘기해요 ㅎㅎ!",
          "original_form": "먹을꺼 얘기해요 ㅎㅎ!",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013672.1.2",
          "form": "점심 뭐드셨나용",
          "original_form": "점심 뭐드셨나용",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013672.1.3",
          "form": "방금먹었어요.. 짜파게티.. \r\n뭐 드셧나요?",
          "original_form": "방금먹었어요.. 짜파게티.. \r\n뭐 드셧나요?",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013672.1.4",
          "form": "헉 맛있겠다 저는 이제 일어나서 아직안먹었어요..ㅎㅎ",
          "original_form": "헉 맛있겠다 저는 이제 일어나서 아직안먹었어요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013672.1.5",
          "form": "비빔면 먹고싶어요",
          "original_form": "비빔면 먹고싶어요",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013672.1.6",
          "form": "라면 어떤 종류 제일 좋아하시나용",
          "original_form": "라면 어떤 종류 제일 좋아하시나용",
          "speaker_id": "1",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013672.1.7",
          "form": "헉? 지금이요?",
          "original_form": "헉? 지금이요?",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013672.1.8",
          "form": "두꺼운 면 좋죠... 우동 너구리 같은거요",
          "original_form": "두꺼운 면 좋죠... 우동 너구리 같은거요",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013672.1.9",
          "form": "앟 넵 ㅎㅎ 아직 대학생 방학이라..^^",
          "original_form": "앟 넵 ㅎㅎ 아직 대학생 방학이라..^^",
          "speaker_id": "1",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013672.1.10",
          "form": "아 배고프네용",
          "original_form": "아 배고프네용",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013672.1.11",
          "form": "어제 구워둔 고기랑 샐러드먹어어겠어요!!",
          "original_form": "어제 구워둔 고기랑 샐러드먹어어겠어요!!",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013672.1.12",
          "form": "엄청 부유하시네요..",
          "original_form": "엄청 부유하시네요..",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013672.1.13",
          "form": "고기가 남아요? ㅋㅋㅋ",
          "original_form": "고기가 남아요? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013672.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 웃겨용 ㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 웃겨용 ㅜ",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013672.1.15",
          "form": "귀찮아서 한번에 많이 구웟어욤 ㅎㅎ",
          "original_form": "귀찮아서 한번에 많이 구웟어욤 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013672.1.16",
          "form": "어느부위좋아하세요?",
          "original_form": "어느부위좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013672.1.17",
          "form": "헤.. 갈수롭 놀랍네요? 골라서 먹을 수 있어요? ㅋㅋㅋ",
          "original_form": "헤.. 갈수롭 놀랍네요? 골라서 먹을 수 있어요? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013672.1.18",
          "form": "그냥 살 수 있는거 눈에 보이는거 먹는거 라닌가요? ㅋㅋㅋ",
          "original_form": "그냥 살 수 있는거 눈에 보이는거 먹는거 라닌가요? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013672.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013672.1.20",
          "form": "저는 목살 좋아해용",
          "original_form": "저는 목살 좋아해용",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013672.1.21",
          "form": "돼지는.. 그거 갈매기살인가?",
          "original_form": "돼지는.. 그거 갈매기살인가?",
          "speaker_id": "2",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013672.1.22",
          "form": "ㅁ맛잇던게",
          "original_form": "ㅁ맛잇던게",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013672.1.23",
          "form": "오옹 소를 더 좋아하세요?",
          "original_form": "오옹 소를 더 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013672.1.24",
          "form": "네.. 설마 소 보다 돼지파인신가요?",
          "original_form": "네.. 설마 소 보다 돼지파인신가요?",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013672.1.25",
          "form": "ㅋㅋㅋㅋㅋ 그냥 구워먹는건 삼겹살...!",
          "original_form": "ㅋㅋㅋㅋㅋ 그냥 구워먹는건 삼겹살...!",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013672.1.26",
          "form": "하지만 스테이크가 제일 좋아요 ..ㅎㅎㅎ",
          "original_form": "하지만 스테이크가 제일 좋아요 ..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013672.1.27",
          "form": "황제 다이어트 해보고싶네요..",
          "original_form": "황제 다이어트 해보고싶네요..",
