      "utterance": [
        {
          "id": "MDRW2100019121.1.1",
          "form": "빈센조요",
          "original_form": "빈센조요",
          "speaker_id": "2",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100019121.1.2",
          "form": "아 저도 빈센조 재밌게봤어요",
          "original_form": "아 저도 빈센조 재밌게봤어요",
          "speaker_id": "1",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019121.1.3",
          "form": "검블유도 재밌더라구요",
          "original_form": "검블유도 재밌더라구요",
          "speaker_id": "2",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019121.1.4",
          "form": "장기용 배우가 장난없었죠",
          "original_form": "장기용 배우가 장난없었죠",
          "speaker_id": "1",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019121.1.5",
          "form": "나의아저씨 땐 별로였는데",
          "original_form": "나의아저씨 땐 별로였는데",
          "speaker_id": "2",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019121.1.6",
          "form": "나의아저씨는 징짱이 다했죠",
          "original_form": "나의아저씨는 징짱이 다했죠",
          "speaker_id": "1",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019121.1.7",
          "form": "징짱 드라마 다 너무 좋아요",
          "original_form": "징짱 드라마 다 너무 좋아요",
          "speaker_id": "2",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019121.1.8",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "1",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019121.1.9",
          "form": "어쩜 연기를 그렇게 잘하죠",
          "original_form": "어쩜 연기를 그렇게 잘하죠",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019121.1.10",
          "form": "감성이 풍부해서 그런가봐요",
          "original_form": "감성이 풍부해서 그런가봐요",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019121.1.11",
          "form": "천상 배우 이지은",
          "original_form": "천상 배우 이지은",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019121.1.12",
          "form": "ㅋㄲㅋㅋ 진짜좋아하시나봐요",
          "original_form": "ㅋㄲㅋㅋ 진짜좋아하시나봐요",
