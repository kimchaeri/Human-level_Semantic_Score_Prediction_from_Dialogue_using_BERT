      "utterance": [
        {
          "id": "MDRW2100010752.1.1",
          "form": "겨울 패딩도 사고는싶은뎅",
          "original_form": "겨울 패딩도 사고는싶은뎅",
          "speaker_id": "1",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100010752.1.2",
          "form": "넘 가격대가 쎄서 ㅠ흡",
          "original_form": "넘 가격대가 쎄서 ㅠ흡",
          "speaker_id": "1",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100010752.1.3",
          "form": "맞아요ㅠ 패딩 뭐이리 비싼지 절레절레",
          "original_form": "맞아요ㅠ 패딩 뭐이리 비싼지 절레절레",
          "speaker_id": "2",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100010752.1.4",
          "form": "이월상품 이런걸 노려봐야하나요",
          "original_form": "이월상품 이런걸 노려봐야하나요",
          "speaker_id": "2",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100010752.1.5",
          "form": "맞어요 지금 벌써 겨울옷 파는거알아요?",
          "original_form": "맞어요 지금 벌써 겨울옷 파는거알아요?",
          "speaker_id": "1",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100010752.1.6",
          "form": "저렴하게 해서 팔더라구용 온라인에!",
          "original_form": "저렴하게 해서 팔더라구용 온라인에!",
          "speaker_id": "1",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100010752.1.7",
          "form": "헐 벌써 겨울옷이 팔아요?!?!?!? 세상에 정말 빠르네요.. 이참에 지금 겨울옷을 좀 사놓을까요",
          "original_form": "헐 벌써 겨울옷이 팔아요?!?!?!? 세상에 정말 빠르네요.. 이참에 지금 겨울옷을 좀 사놓을까요",
          "speaker_id": "2",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100010752.1.8",
          "form": "니트도 사고싶고 니트조끼도 사고싶고",
          "original_form": "니트도 사고싶고 니트조끼도 사고싶고",
          "speaker_id": "2",
          "time": "20210823 21:31"
        },
        {
          "id": "MDRW2100010752.1.9",
          "form": "ㅋㅋㅋ저도 이번달 월급타면",
          "original_form": "ㅋㅋㅋ저도 이번달 월급타면",
          "speaker_id": "1",
          "time": "20210823 21:31"
        },
        {
          "id": "MDRW2100010752.1.10",
          "form": "좀 사려구용",
          "original_form": "좀 사려구용",
          "speaker_id": "1",
          "time": "20210823 21:31"
        },
        {
          "id": "MDRW2100010752.1.11",
          "form": "입을 옷이 없네요 ..",
          "original_form": "입을 옷이 없네요 ..",
          "speaker_id": "1",
          "time": "20210823 21:31"
        },
        {
          "id": "MDRW2100010752.1.12",
          "form": "국룰 ..",
          "original_form": "국룰 ..",
          "speaker_id": "1",
          "time": "20210823 21:31"
        },
        {
          "id": "MDRW2100010752.1.13",
          "form": "ㅠㅠㅋㅋㅋㅋㅋㅋ맞아욬ㅋㅋㅋㅋ 옷은 많지만 내가 입을 옷은 없다 그러니 쇼핑을 해야해",
          "original_form": "ㅠㅠㅋㅋㅋㅋㅋㅋ맞아욬ㅋㅋㅋㅋ 옷은 많지만 내가 입을 옷은 없다 그러니 쇼핑을 해야해",
          "speaker_id": "2",
          "time": "20210823 21:31"
        },
        {
          "id": "MDRW2100010752.1.14",
          "form": "그렇게 텅장..이되고...",
          "original_form": "그렇게 텅장..이되고...",
          "speaker_id": "2",
          "time": "20210823 21:31"
        },
        {
          "id": "MDRW2100010752.1.15",
          "form": "ㅋㅋㅋㅋㅋ 진짜로ㅠ 옷장보면 안입는옷들만 수두룩..",
          "original_form": "ㅋㅋㅋㅋㅋ 진짜로ㅠ 옷장보면 안입는옷들만 수두룩..",
          "speaker_id": "1",
          "time": "20210823 21:32"
        },
        {
          "id": "MDRW2100010752.1.16",
          "form": "뭐에 홀려서 원피스사고 그랬는지 몰라요 허허",
          "original_form": "뭐에 홀려서 원피스사고 그랬는지 몰라요 허허",
          "speaker_id": "2",
          "time": "20210823 21:33"
        },
        {
          "id": "MDRW2100010752.1.17",
          "form": "name1은 옷사면 주로 어떤걸로 사시나요?",
          "original_form": "&name1&은 옷사면 주로 어떤걸로 사시나요?",
          "speaker_id": "2",
          "time": "20210823 21:33"
        },
        {
          "id": "MDRW2100010752.1.18",
          "form": "저는 대부분 원피스나 블라우스나 슬랙스 청바지를 주로 사요!",
          "original_form": "저는 대부분 원피스나 블라우스나 슬랙스 청바지를 주로 사요!",
          "speaker_id": "2",
          "time": "20210823 21:33"
        },
        {
          "id": "MDRW2100010752.1.19",
          "form": "저도 원피스 많이사구용",
          "original_form": "저도 원피스 많이사구용",
          "speaker_id": "1",
          "time": "20210823 21:34"
        },
        {
          "id": "MDRW2100010752.1.20",
          "form": "티종류를 많이 사는거같아용",
          "original_form": "티종류를 많이 사는거같아용",
          "speaker_id": "1",
          "time": "20210823 21:34"
        },
        {
          "id": "MDRW2100010752.1.21",
          "form": "최근에도 원피스 샀네요 흰색원피스! 여름에 한번입고 옷장에 이제 들어간..",
          "original_form": "최근에도 원피스 샀네요 흰색원피스! 여름에 한번입고 옷장에 이제 들어간..",
          "speaker_id": "2",
          "time": "20210823 21:34"
        },
        {
          "id": "MDRW2100010752.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅠㅠ 저도 최근에 원피스산거있는데 ....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅠㅠ 저도 최근에 원피스산거있는데 ....",
          "speaker_id": "1",
          "time": "20210823 21:35"
        },
        {
          "id": "MDRW2100010752.1.23",
          "form": "못입고 들어갈판이에여",
          "original_form": "못입고 들어갈판이에여",
          "speaker_id": "1",
          "time": "20210823 21:35"
        },
        {
          "id": "MDRW2100010752.1.24",
          "form": "오! 티셔츠! 기본티셔츠 좀 있어야하는데..\r\nㅋㅋㅋㅋㅋㅋㅋ세상엨ㅋㅋㅋ 이번 주말에 놀러갈때 구매한 원피스 입어야되겠는갈요??!",
          "original_form": "오! 티셔츠! 기본티셔츠 좀 있어야하는데..\r\nㅋㅋㅋㅋㅋㅋㅋ세상엨ㅋㅋㅋ 이번 주말에 놀러갈때 구매한 원피스 입어야되겠는갈요??!",
          "speaker_id": "2",
          "time": "20210823 21:35"
        },
        {
          "id": "MDRW2100010752.1.25",
          "form": "그쵸 그래서 날이 좋아야해용 ...",
          "original_form": "그쵸 그래서 날이 좋아야해용 ...",
          "speaker_id": "1",
          "time": "20210823 21:36"
        },
        {
          "id": "MDRW2100010752.1.26",
          "form": "맨투맨 티셔츠 좋아하세요?",
          "original_form": "맨투맨 티셔츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 21:36"
        }
      ]
