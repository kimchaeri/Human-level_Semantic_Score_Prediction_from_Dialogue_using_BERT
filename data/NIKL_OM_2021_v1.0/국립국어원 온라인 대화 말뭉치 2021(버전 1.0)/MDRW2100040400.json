      "utterance": [
        {
          "id": "MDRW2100040400.1.1",
          "form": "함께하는 반려동물이 있으신가요?",
          "original_form": "함께하는 반려동물이 있으신가요?",
          "speaker_id": "1",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100040400.1.2",
          "form": "아뇨 님은용",
          "original_form": "아뇨 님은용",
          "speaker_id": "2",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100040400.1.3",
          "form": "아니요 저도 따로 없어요",
          "original_form": "아니요 저도 따로 없어요",
          "speaker_id": "1",
          "time": "20210904 18:28"
        },
        {
          "id": "MDRW2100040400.1.4",
          "form": "강아지 키우고파요",
          "original_form": "강아지 키우고파요",
          "speaker_id": "2",
          "time": "20210904 18:28"
        },
        {
          "id": "MDRW2100040400.1.5",
          "form": "키우고 싶은 동물이 있으신가요?",
          "original_form": "키우고 싶은 동물이 있으신가요?",
          "speaker_id": "1",
          "time": "20210904 18:28"
        },
        {
          "id": "MDRW2100040400.1.6",
          "form": "ㅋㅋㅋ 강아지 귀엽죠",
          "original_form": "ㅋㅋㅋ 강아지 귀엽죠",
          "speaker_id": "1",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100040400.1.7",
          "form": "고양이도요",
          "original_form": "고양이도요",
          "speaker_id": "2",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100040400.1.8",
          "form": "키우신다면 대형견? 소형견?",
          "original_form": "키우신다면 대형견? 소형견?",
          "speaker_id": "1",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100040400.1.9",
          "form": "대형견 로망이지만",
          "original_form": "대형견 로망이지만",
          "speaker_id": "2",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100040400.1.10",
          "form": "현실적으로 소형견",
          "original_form": "현실적으로 소형견",
          "speaker_id": "2",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100040400.1.11",
          "form": "고양이도 좋아요",
          "original_form": "고양이도 좋아요",
          "speaker_id": "1",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100040400.1.12",
          "form": "직접 키우지는 못해서 랜선으로 보는 거에 대리 만족중이에요",
          "original_form": "직접 키우지는 못해서 랜선으로 보는 거에 대리 만족중이에요",
          "speaker_id": "1",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100040400.1.13",
          "form": "마자요 근데 산책하고싶어요",
          "original_form": "마자요 근데 산책하고싶어요",
          "speaker_id": "2",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100040400.1.14",
          "form": "알러지가 있어서 키우지 못하시나요?",
          "original_form": "알러지가 있어서 키우지 못하시나요?",
          "speaker_id": "1",
          "time": "20210904 18:31"
        },
        {
          "id": "MDRW2100040400.1.15",
          "form": "ㅜㅜㅜ 제가 책임져야하는데 ㅠㅠ혼자사니까요ㅠㅠ",
          "original_form": "ㅜㅜㅜ 제가 책임져야하는데 ㅠㅠ혼자사니까요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100040400.1.16",
          "form": "아 ㅠㅠ 혼자 케어하기도 힘들거고",
          "original_form": "아 ㅠㅠ 혼자 케어하기도 힘들거고",
          "speaker_id": "1",
          "time": "20210904 18:33"
        },
        {
          "id": "MDRW2100040400.1.17",
          "form": "외출한 동안 또 동물이 혼자 있는 것도 신경 쓰이니까 그렇겠군요 ㅠ",
          "original_form": "외출한 동안 또 동물이 혼자 있는 것도 신경 쓰이니까 그렇겠군요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 18:33"
        },
        {
          "id": "MDRW2100040400.1.18",
          "form": "넹 ㅠㅠ흑흑흑 너무키우고싶다...",
          "original_form": "넹 ㅠㅠ흑흑흑 너무키우고싶다...",
