      "utterance": [
        {
          "id": "MDRW2100034571.1.1",
          "form": "오늘 날씨 참 좋네요",
          "original_form": "오늘 날씨 참 좋네요",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034571.1.2",
          "form": "네 오랜만에 맑은 주말인거같아요!",
          "original_form": "네 오랜만에 맑은 주말인거같아요!",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034571.1.3",
          "form": "외출하셨나요?,",
          "original_form": "외출하셨나요?,",
          "speaker_id": "1",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100034571.1.4",
          "form": "사실 이 좋은날 출근중이에요 ㅋㅋㅋ",
          "original_form": "사실 이 좋은날 출근중이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100034571.1.5",
          "form": "으악 근데 이시간에 출근하시나요,",
          "original_form": "으악 근데 이시간에 출근하시나요,",
          "speaker_id": "1",
          "time": "20210828 15:31"
        },
        {
          "id": "MDRW2100034571.1.6",
          "form": "네 ㅋㅋㅋ 평일에도 오후부터 밤까지 일해요 ㅋㅋ 직장이 코앞이라 날씨좋은날은 걸어가면서 막 화가나요 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ 평일에도 오후부터 밤까지 일해요 ㅋㅋ 직장이 코앞이라 날씨좋은날은 걸어가면서 막 화가나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100034571.1.7",
          "form": "아 오후에 일하시는군요 오늘 날씨 좋네요 쨍한데 선선하고...",
          "original_form": "아 오후에 일하시는군요 오늘 날씨 좋네요 쨍한데 선선하고...",
          "speaker_id": "1",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100034571.1.8",
          "form": "맞아요 바람이 막 뜨겁지 않더라구요! 계절이라는게 참 신기해요 ㅋㅋ",
          "original_form": "맞아요 바람이 막 뜨겁지 않더라구요! 계절이라는게 참 신기해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100034571.1.9",
          "form": "그렇죠 며칠 지나면 9월 이라니  ",
          "original_form": "그렇죠 며칠 지나면 9월 이라니  ",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034571.1.10",
          "form": "심지어 추석도 곧이에요..세월이 무상해요ㅋㅋㅋ",
          "original_form": "심지어 추석도 곧이에요..세월이 무상해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034571.1.11",
          "form": "맞아요 올 추석은 덥겠어요 일러서",
          "original_form": "맞아요 올 추석은 덥겠어요 일러서",
          "speaker_id": "1",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100034571.1.12",
          "form": "맞아요 몇년전에도 이른 추석이었는데 추석이라서 좀 예쁜 옷입었었는데 ㅋㅋ 너무 더운거에요!! ㅋㅋㅋ  더웠던기억밖에없어요 그때 ㅋㅋ",
          "original_form": "맞아요 몇년전에도 이른 추석이었는데 추석이라서 좀 예쁜 옷입었었는데 ㅋㅋ 너무 더운거에요!! ㅋㅋㅋ  더웠던기억밖에없어요 그때 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100034571.1.13",
          "form": "오 맞아요 추석빔이라기엔 여름옷을 사기도 애매한 이른 추석이 있었죠",
          "original_form": "오 맞아요 추석빔이라기엔 여름옷을 사기도 애매한 이른 추석이 있었죠",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100034571.1.14",
          "form": "이번 추석때는 날씨가 좋았으면 좋겠어요 ㅋㅋ",
          "original_form": "이번 추석때는 날씨가 좋았으면 좋겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:46"
        }
      ]
