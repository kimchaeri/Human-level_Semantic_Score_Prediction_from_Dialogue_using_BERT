      "utterance": [
        {
          "id": "MDRW2100023697.1.1",
          "form": "거기서 오징어튀김먹고",
          "original_form": "거기서 오징어튀김먹고",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023697.1.2",
          "form": "우리 대만에서 배도 따로 탈 뻔했지?ㅋㅋ",
          "original_form": "우리 대만에서 배도 따로 탈 뻔했지?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023697.1.3",
          "form": "맞아 줄섰을때?",
          "original_form": "맞아 줄섰을때?",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023697.1.4",
          "form": "오징어튀김 짱 맛있었는데",
          "original_form": "오징어튀김 짱 맛있었는데",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023697.1.5",
          "form": "나랑 누가 늦게 와가지구",
          "original_form": "나랑 누가 늦게 와가지구",
          "speaker_id": "1",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023697.1.6",
          "form": "마자 맛있었지ㅠㅠ",
          "original_form": "마자 맛있었지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023697.1.7",
          "form": "뭐때뭄이었지? 그거 대왕카스테라사느라 그랬었나?",
          "original_form": "뭐때뭄이었지? 그거 대왕카스테라사느라 그랬었나?",
          "speaker_id": "2",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100023697.1.8",
          "form": "그거 뭐야 버블티 쏟아서 정리하고 오느라 그랬던거같은데",
          "original_form": "그거 뭐야 버블티 쏟아서 정리하고 오느라 그랬던거같은데",
          "speaker_id": "1",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100023697.1.9",
          "form": "맞아 버블티도 쏟았지ㅋㅋㅋ",
          "original_form": "맞아 버블티도 쏟았지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100023697.1.10",
          "form": "대왕카스테라는 다른데서 샀을껄?",
          "original_form": "대왕카스테라는 다른데서 샀을껄?",
          "speaker_id": "1",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100023697.1.11",
          "form": "버블티랑 카스테라가 같은데아닌가?",
          "original_form": "버블티랑 카스테라가 같은데아닌가?",
          "speaker_id": "2",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100023697.1.12",
          "form": "같은덴가?",
          "original_form": "같은덴가?",
          "speaker_id": "1",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100023697.1.13",
          "form": "쏟을 떄 너 있었어?",
          "original_form": "쏟을 떄 너 있었어?",
          "speaker_id": "1",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100023697.1.14",
          "form": "난 카스테라아니고 버블티였던듯?",
          "original_form": "난 카스테라아니고 버블티였던듯?",
          "speaker_id": "2",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100023697.1.15",
          "form": "카스테라 살때 나 안가서 모르겠네",
          "original_form": "카스테라 살때 나 안가서 모르겠네",
          "speaker_id": "1",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100023697.1.16",
