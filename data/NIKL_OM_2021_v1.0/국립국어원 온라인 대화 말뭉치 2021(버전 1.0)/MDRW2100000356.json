      "utterance": [
        {
          "id": "MDRW2100000356.1.1",
          "form": "영화 보는거 좋아하세요?",
          "original_form": "영화 보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 15:31"
        },
        {
          "id": "MDRW2100000356.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210819 15:36"
        },
        {
          "id": "MDRW2100000356.1.3",
          "form": "무슨영화좋아하세요",
          "original_form": "무슨영화좋아하세요",
          "speaker_id": "2",
          "time": "20210819 15:36"
        },
        {
          "id": "MDRW2100000356.1.4",
          "form": "음... 전 한국영화 거의 다 좋아해요ㅋㅋㅋㅋ",
          "original_form": "음... 전 한국영화 거의 다 좋아해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 15:37"
        },
        {
          "id": "MDRW2100000356.1.5",
          "form": "싱크홀 보셨어요?",
          "original_form": "싱크홀 보셨어요?",
          "speaker_id": "1",
          "time": "20210819 15:38"
        },
        {
          "id": "MDRW2100000356.1.6",
          "form": "저도ㅎㅎ자막안봐도되서",
          "original_form": "저도ㅎㅎ자막안봐도되서",
          "speaker_id": "2",
          "time": "20210819 15:38"
        },
        {
          "id": "MDRW2100000356.1.7",
          "form": "저예고편만봤어요",
          "original_form": "저예고편만봤어요",
          "speaker_id": "2",
          "time": "20210819 15:38"
        },
        {
          "id": "MDRW2100000356.1.8",
          "form": "저도 아직 안봤는데",
          "original_form": "저도 아직 안봤는데",
          "speaker_id": "1",
          "time": "20210819 15:39"
        },
        {
          "id": "MDRW2100000356.1.9",
          "form": "광수님 나와서 재밌을거같아요ㅎㅎ",
          "original_form": "광수님 나와서 재밌을거같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 15:39"
        },
        {
          "id": "MDRW2100000356.1.10",
          "form": "광수재미써요",
          "original_form": "광수재미써요",
          "speaker_id": "2",
          "time": "20210819 15:44"
        },
        {
          "id": "MDRW2100000356.1.11",
          "form": "맞아요ㅋㅋㅋㅋ 런닝맨에서 빠져서 너무 아쉬운ㅠㅠ",
          "original_form": "맞아요ㅋㅋㅋㅋ 런닝맨에서 빠져서 너무 아쉬운ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 15:45"
        },
        {
          "id": "MDRW2100000356.1.12",
          "form": "유재석이노력하던데",
          "original_form": "유재석이노력하던데",
          "speaker_id": "2",
          "time": "20210819 15:45"
        },
        {
          "id": "MDRW2100000356.1.13",
          "form": "빠진거메울려고",
          "original_form": "빠진거메울려고",
          "speaker_id": "2",
          "time": "20210819 15:45"
        },
        {
          "id": "MDRW2100000356.1.14",
          "form": "ㅋㅋㅋㅋㅋ혹시 연극같은건 좋아하세요?",
          "original_form": "ㅋㅋㅋㅋㅋ혹시 연극같은건 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 15:47"
        },
        {
          "id": "MDRW2100000356.1.15",
          "form": "연극좋아합니다",
          "original_form": "연극좋아합니다",
          "speaker_id": "2",
          "time": "20210819 15:48"
        },
        {
          "id": "MDRW2100000356.1.16",
          "form": "근데많이못봐서아쉬워용",
          "original_form": "근데많이못봐서아쉬워용",
          "speaker_id": "2",
          "time": "20210819 15:48"
        },
        {
          "id": "MDRW2100000356.1.17",
          "form": "코로나ㅠㅜㅜㅜ",
          "original_form": "코로나ㅠㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 15:48"
        },
        {
          "id": "MDRW2100000356.1.18",
          "form": "맞아요 코로나ㅠㅠㅠ",
          "original_form": "맞아요 코로나ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000356.1.19",
          "form": "저 진짜 보고싶은 뮤지컬이 있어서 예매했는데",
          "original_form": "저 진짜 보고싶은 뮤지컬이 있어서 예매했는데",
          "speaker_id": "1",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000356.1.20",
          "form": "코로나때문에 취소했어요..ㅠㅠ",
          "original_form": "코로나때문에 취소했어요..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000356.1.21",
          "form": "뮤지컬어떤거요",
          "original_form": "뮤지컬어떤거요",
          "speaker_id": "2",
          "time": "20210819 15:55"
        },
        {
          "id": "MDRW2100000356.1.22",
          "form": "뮤지컬은본적이없어서ㅠ",
          "original_form": "뮤지컬은본적이없어서ㅠ",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000356.1.23",
          "form": "베어더뮤지컬이요! 좋아하는 배우분이 하셔서",
          "original_form": "베어더뮤지컬이요! 좋아하는 배우분이 하셔서",
