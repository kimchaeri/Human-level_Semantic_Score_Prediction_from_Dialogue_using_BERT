      "utterance": [
        {
          "id": "MDRW2100034067.1.1",
          "form": "비가 오네요ㅠ",
          "original_form": "비가 오네요ㅠ",
          "speaker_id": "1",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100034067.1.2",
          "form": "맞아요 여기도 비가 내리고 있어요",
          "original_form": "맞아요 여기도 비가 내리고 있어요",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100034067.1.3",
          "form": "빨리 그치면 좋겠어요ㅠ",
          "original_form": "빨리 그치면 좋겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100034067.1.4",
          "form": "그러니까요 좋아하는 계절 있으신가요?",
          "original_form": "그러니까요 좋아하는 계절 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100034067.1.5",
          "form": "저는 가을 좋아해요",
          "original_form": "저는 가을 좋아해요",
          "speaker_id": "1",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100034067.1.6",
          "form": "그럼 지금 날씨 엄청 좋아하시겠네요",
          "original_form": "그럼 지금 날씨 엄청 좋아하시겠네요",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100034067.1.7",
          "form": "비는 안 왔으면 해요...ㅎ",
          "original_form": "비는 안 왔으면 해요...ㅎ",
          "speaker_id": "1",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100034067.1.8",
          "form": "앗!!그렇죠 가을이 바람도 불고 선선해서 좋은 것 같아요",
          "original_form": "앗!!그렇죠 가을이 바람도 불고 선선해서 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210827 17:24"
        },
        {
          "id": "MDRW2100034067.1.9",
          "form": "비만 그치면 딱 좋겠어요ㅎㅎ",
          "original_form": "비만 그치면 딱 좋겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:51"
        },
        {
          "id": "MDRW2100034067.1.10",
          "form": "맞아요 이제 슬슬 긴팔을 입어야할 때가 온 것 같아요",
          "original_form": "맞아요 이제 슬슬 긴팔을 입어야할 때가 온 것 같아요",
          "speaker_id": "2",
          "time": "20210827 17:59"
        },
        {
          "id": "MDRW2100034067.1.11",
          "form": "가을 옷을 꺼내야겠어요ㅋㅋ",
          "original_form": "가을 옷을 꺼내야겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:08"
        },
        {
          "id": "MDRW2100034067.1.12",
          "form": "그러니까요 아직도 그쪽은 비가 오나요? 여긴 거의 그쳤는데",
          "original_form": "그러니까요 아직도 그쪽은 비가 오나요? 여긴 거의 그쳤는데",
          "speaker_id": "2",
          "time": "20210827 18:09"
        },
        {
          "id": "MDRW2100034067.1.13",
          "form": "그쳤다가 이제 또 와요ㅠㅠ",
          "original_form": "그쳤다가 이제 또 와요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100034067.1.14",
          "form": "아이고 얼른 그쳤으면 좋겠네요",
          "original_form": "아이고 얼른 그쳤으면 좋겠네요",
