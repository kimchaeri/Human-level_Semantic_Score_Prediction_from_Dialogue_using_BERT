      "utterance": [
        {
          "id": "MDRW2100018997.1.1",
          "form": "일은 잘 하고 계신가여",
          "original_form": "일은 잘 하고 계신가여",
          "speaker_id": "1",
          "time": "20210827 14:32"
        },
        {
          "id": "MDRW2100018997.1.2",
          "form": "지겹진 않나요",
          "original_form": "지겹진 않나요",
          "speaker_id": "1",
          "time": "20210827 14:32"
        },
        {
          "id": "MDRW2100018997.1.3",
          "form": "네 잘하고 있....",
          "original_form": "네 잘하고 있....",
          "speaker_id": "2",
          "time": "20210827 14:34"
        },
        {
          "id": "MDRW2100018997.1.4",
          "form": "디자인이 막혀서 골치아프네요",
          "original_form": "디자인이 막혀서 골치아프네요",
          "speaker_id": "2",
          "time": "20210827 14:34"
        },
        {
          "id": "MDRW2100018997.1.5",
          "form": "허허 화이팅하세요",
          "original_form": "허허 화이팅하세요",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100018997.1.6",
          "form": "어떤 디자인을 하시나요",
          "original_form": "어떤 디자인을 하시나요",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100018997.1.7",
          "form": "지금은 앱디자인 하고 있어요",
          "original_form": "지금은 앱디자인 하고 있어요",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100018997.1.8",
          "form": "오 어려울거 같아요",
          "original_form": "오 어려울거 같아요",
          "speaker_id": "1",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100018997.1.9",
          "form": "커피 드시면서 하세쇼",
          "original_form": "커피 드시면서 하세쇼",
          "speaker_id": "1",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100018997.1.10",
          "form": "네 커피 한 잔 했어요ㅋㅋ",
          "original_form": "네 커피 한 잔 했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100018997.1.11",
          "form": "그래서 오후에는 정신이 맑네요",
          "original_form": "그래서 오후에는 정신이 맑네요",
          "speaker_id": "2",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100018997.1.12",
          "form": "일 하기 싫은데 그래도 금요일이에요ㅎㅎ",
          "original_form": "일 하기 싫은데 그래도 금요일이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 16:10"
        },
        {
          "id": "MDRW2100018997.1.13",
          "form": "앗 그렇네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗 그렇네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100018997.1.14",
          "form": "불금이면 일할 맛 나죠",
          "original_form": "불금이면 일할 맛 나죠",
          "speaker_id": "1",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100018997.1.15",
          "form": "이제 한시간 하고 40분 남았네요",
          "original_form": "이제 한시간 하고 40분 남았네요",
          "speaker_id": "2",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100018997.1.16",
          "form": "얼른 퇴근하고 싶어요ㅜㅜ",
          "original_form": "얼른 퇴근하고 싶어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100018997.1.17",
          "form": "사무실에 코코도르 디퓨저가 생겼는데",
          "original_form": "사무실에 코코도르 디퓨저가 생겼는데",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100018997.1.18",
          "form": "코코도르 발향이 약하다더니",
          "original_form": "코코도르 발향이 약하다더니",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100018997.1.19",
          "form": "생각보다 향 많이 나네요",
          "original_form": "생각보다 향 많이 나네요",
          "speaker_id": "2",
          "time": "20210827 16:21"
        },
        {
          "id": "MDRW2100018997.1.20",
          "form": "사무실이 남초라ㅎㅎ 좀 퀘퀘했는데",
          "original_form": "사무실이 남초라ㅎㅎ 좀 퀘퀘했는데",
          "speaker_id": "2",
          "time": "20210827 16:21"
        },
        {
          "id": "MDRW2100018997.1.21",
          "form": "좋네여",
          "original_form": "좋네여",
          "speaker_id": "2",
          "time": "20210827 16:21"
        },
        {
          "id": "MDRW2100018997.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100018997.1.23",
          "form": "코코도르 은근 향 좋은거 같아요",
          "original_form": "코코도르 은근 향 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100018997.1.24",
          "form": "어떤 향인가요?",
          "original_form": "어떤 향인가요?",
          "speaker_id": "1",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100018997.1.25",
          "form": "에이프릴 프레쉬에요",
          "original_form": "에이프릴 프레쉬에요",
          "speaker_id": "2",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100018997.1.26",
          "form": "근데 디퓨저는 액체 향 다 날아가면 교체하나요?",
          "original_form": "근데 디퓨저는 액체 향 다 날아가면 교체하나요?",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100018997.1.27",
          "form": "오 이름부터 프레쉬하네요",
          "original_form": "오 이름부터 프레쉬하네요",
          "speaker_id": "1",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100018997.1.28",
          "form": "넹 액체가 점점 줄어들어요",
          "original_form": "넹 액체가 점점 줄어들어요",
          "speaker_id": "1",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100018997.1.29",
          "form": "액체 다 사라질때까지 써요?",
          "original_form": "액체 다 사라질때까지 써요?",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100018997.1.30",
          "form": "다 쓰면 사무비품 주문할때마다 사면 좋을 것 같아서요ㅋㅋ",
          "original_form": "다 쓰면 사무비품 주문할때마다 사면 좋을 것 같아서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100018997.1.31",
          "form": "넵 그 막대 뒤집어끼우면 다시 발향이 돼요",
          "original_form": "넵 그 막대 뒤집어끼우면 다시 발향이 돼요",
          "speaker_id": "1",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100018997.1.32",
          "form": "ㅋㅋㅋㅋㅋ실내공기가 텁텁할 때마다 주문합시다",
          "original_form": "ㅋㅋㅋㅋㅋ실내공기가 텁텁할 때마다 주문합시다",
          "speaker_id": "1",
          "time": "20210827 16:35"
        }
      ]
