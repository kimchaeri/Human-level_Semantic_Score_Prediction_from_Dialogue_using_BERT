      "utterance": [
        {
          "id": "MDRW2100037915.1.1",
          "form": "지금 옥션 대세일한대요",
          "original_form": "지금 옥션 대세일한대요",
          "speaker_id": "2",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100037915.1.2",
          "form": "무슨 스탬프붙은거 무조건할인 이렇게",
          "original_form": "무슨 스탬프붙은거 무조건할인 이렇게",
          "speaker_id": "2",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100037915.1.3",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037915.1.4",
          "form": "낼부터 올리브영도 음청 세일한대",
          "original_form": "낼부터 올리브영도 음청 세일한대",
          "speaker_id": "1",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037915.1.5",
          "form": "70퍼",
          "original_form": "70퍼",
          "speaker_id": "1",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037915.1.6",
          "form": "ㅋㅋ재고떨이냐 70퍼는 대박이다",
          "original_form": "ㅋㅋ재고떨이냐 70퍼는 대박이다",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037915.1.7",
          "form": "화장품 살거가 없어 아쉬웁네",
          "original_form": "화장품 살거가 없어 아쉬웁네",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100037915.1.8",
          "form": "나둥 쓸게많아 ㅋㅋ",
          "original_form": "나둥 쓸게많아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100037915.1.9",
          "form": "ㅇㅇ그런건 모자라지 않음 ㅋㅋ화장을 안해서 그런가",
          "original_form": "ㅇㅇ그런건 모자라지 않음 ㅋㅋ화장을 안해서 그런가",
          "speaker_id": "2",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100037915.1.10",
          "form": "ㅇ 이번 환절기부터는 부지런히바르자!!",
          "original_form": "ㅇ 이번 환절기부터는 부지런히바르자!!",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100037915.1.11",
          "form": "그러고보니 썬크림도 안바르고 부지런히태웠네여 ㅋㅋ",
          "original_form": "그러고보니 썬크림도 안바르고 부지런히태웠네여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100037915.1.12",
          "form": "난 올여름은 열심히 바름. ㅜ",
          "original_form": "난 올여름은 열심히 바름. ㅜ",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037915.1.13",
          "form": "아 나 당근당근 함",
          "original_form": "아 나 당근당근 함",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100037915.1.14",
          "form": "머를?",
          "original_form": "머를?",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100037915.1.15",
          "form": "스벅 아아랑",
          "original_form": "스벅 아아랑",
          "speaker_id": "2",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100037915.1.16",
          "form": "홈플러스 2만원상품권",
          "original_form": "홈플러스 2만원상품권",
          "speaker_id": "2",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100037915.1.17",
          "form": "오오!",
          "original_form": "오오!",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100037915.1.18",
          "form": "역시 기다리면 나타나는군",
          "original_form": "역시 기다리면 나타나는군",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100037915.1.19",
          "form": "아직 헌혈은 남아있지만 그건 내년2월까지니카",
          "original_form": "아직 헌혈은 남아있지만 그건 내년2월까지니카",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100037915.1.20",
          "form": "좀더지켜보게요",
          "original_form": "좀더지켜보게요",
