      "utterance": [
        {
          "id": "MDRW2100020660.1.1",
          "form": "혹시 대탈출 보세용?",
          "original_form": "혹시 대탈출 보세용?",
          "speaker_id": "1",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020660.1.2",
          "form": "앗 저 챙겨보지는 않고 티비틀다 나오면 가끔 봐요 !!",
          "original_form": "앗 저 챙겨보지는 않고 티비틀다 나오면 가끔 봐요 !!",
          "speaker_id": "2",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020660.1.3",
          "form": "제일최근건가 ? 좀비편은 봤어요",
          "original_form": "제일최근건가 ? 좀비편은 봤어요",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020660.1.4",
          "form": "탈출실패한거요 !",
          "original_form": "탈출실패한거요 !",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020660.1.5",
          "form": "오호ㅋㅋㅋㅋ",
          "original_form": "오호ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020660.1.6",
          "form": "그 저번주랑 이번주가",
          "original_form": "그 저번주랑 이번주가",
          "speaker_id": "1",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020660.1.7",
          "form": "한편짜리인데",
          "original_form": "한편짜리인데",
          "speaker_id": "1",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020660.1.8",
          "form": "아 진짜요 ????",
          "original_form": "아 진짜요 ????",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020660.1.9",
          "form": "저번주가 역대급이었어용",
          "original_form": "저번주가 역대급이었어용",
          "speaker_id": "1",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020660.1.10",
          "form": "네네 그리고",
          "original_form": "네네 그리고",
          "speaker_id": "1",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020660.1.11",
          "form": "대탈출이 세계관처럼 이어지는 스토리가 몇개있는데",
          "original_form": "대탈출이 세계관처럼 이어지는 스토리가 몇개있는데",
          "speaker_id": "1",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020660.1.12",
          "form": "앗 맞아요 !!",
          "original_form": "앗 맞아요 !!",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020660.1.13",
          "form": "그 천해명",
          "original_form": "그 천해명",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020660.1.14",
          "form": "천해명 스토리 추천드려용",
          "original_form": "천해명 스토리 추천드려용",
          "speaker_id": "1",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020660.1.15",
          "form": "ㅇ오오옼ㅋㅋㅋ",
          "original_form": "ㅇ오오옼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020660.1.16",
          "form": "오 역시 !!!",
          "original_form": "오 역시 !!!",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020660.1.17",
          "form": "저 사실 조마테오 그거 보다가",
          "original_form": "저 사실 조마테오 그거 보다가",
          "speaker_id": "2",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020660.1.18",
          "form": "이거 몇번씩 돌려봤어요ㅋㅋㅋㅋ",
          "original_form": "이거 몇번씩 돌려봤어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020660.1.19",
          "form": "그 악령 그 침대에 묶어놓고 의식 한거 있잖아요",
          "original_form": "그 악령 그 침대에 묶어놓고 의식 한거 있잖아요",
          "speaker_id": "2",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020660.1.20",
          "form": "그거보고 너무 무서워서",
          "original_form": "그거보고 너무 무서워서",
          "speaker_id": "2",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020660.1.21",
          "form": "아 엘베에서 동영상인가 소리나올때 진짜 오줌 지릴뻔",
          "original_form": "아 엘베에서 동영상인가 소리나올때 진짜 오줌 지릴뻔",
          "speaker_id": "1",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020660.1.22",
          "form": "밤에 잠을 못잤어요 ㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌ",
          "original_form": "밤에 잠을 못잤어요 ㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌ",
          "speaker_id": "2",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020660.1.23",
          "form": "아 ㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020660.1.24",
          "form": "엘베 와 진짜 무서우셨을 것 같아요 ㅋㅋㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "엘베 와 진짜 무서우셨을 것 같아요 ㅋㅋㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020660.1.25",
          "form": "근데 진짜 피디들 너무 천재예요 어떻가 그런 스토리를 생각해는걸까요",
          "original_form": "근데 진짜 피디들 너무 천재예요 어떻가 그런 스토리를 생각해는걸까요",
          "speaker_id": "2",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020660.1.26",
          "form": "여고추리반도 같은 감독이라던데",
          "original_form": "여고추리반도 같은 감독이라던데",
          "speaker_id": "2",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020660.1.27",
          "form": "맞아요 정종연 피디 진짜 천재에요",
          "original_form": "맞아요 정종연 피디 진짜 천재에요",
          "speaker_id": "1",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020660.1.28",
          "form": "여고추리반 저도 봤어여!",
          "original_form": "여고추리반 저도 봤어여!",
          "speaker_id": "1",
          "time": "20210828 11:01"
        }
      ]
    }
