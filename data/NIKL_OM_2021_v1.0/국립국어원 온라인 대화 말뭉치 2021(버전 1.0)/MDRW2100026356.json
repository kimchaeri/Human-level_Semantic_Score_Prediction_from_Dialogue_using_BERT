      "utterance": [
        {
          "id": "MDRW2100026356.1.1",
          "form": "이제  하루를 마감해야해서 언제든 또뵈어여^^",
          "original_form": "이제  하루를 마감해야해서 언제든 또뵈어여^^",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026356.1.2",
          "form": "네 또뵈어요~",
          "original_form": "네 또뵈어요~",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026356.1.3",
          "form": "전 아직500개 멀어서 한분씩 가실때마다 슬프네여 ㅋㅋ",
          "original_form": "전 아직500개 멀어서 한분씩 가실때마다 슬프네여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026356.1.4",
          "form": "굳밤용!",
          "original_form": "굳밤용!",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026356.1.5",
          "form": "악 낼 대화많이해여",
          "original_form": "악 낼 대화많이해여",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026356.1.6",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026356.1.7",
          "form": "저도 멀엇어요",
          "original_form": "저도 멀엇어요",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026356.1.8",
          "form": "저 얼마전에 긴급헌혈단 가입했는데",
          "original_form": "저 얼마전에 긴급헌혈단 가입했는데",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100026356.1.9",
          "form": "좀 주변에 널리 알리고싶어요",
          "original_form": "좀 주변에 널리 알리고싶어요",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100026356.1.10",
          "form": "혹시 제가 이미 이야기했던가요?",
          "original_form": "혹시 제가 이미 이야기했던가요?",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100026356.1.11",
          "form": "온라인커뮤니티인가요?",
          "original_form": "온라인커뮤니티인가요?",
          "speaker_id": "1",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100026356.1.12",
          "form": "정모도 하나요?",
          "original_form": "정모도 하나요?",
          "speaker_id": "1",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100026356.1.13",
          "form": "커뮤니티 사람들이 함께하는거에요?",
          "original_form": "커뮤니티 사람들이 함께하는거에요?",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100026356.1.14",
          "form": "아뇨 그냥 네이버카페에요",
          "original_form": "아뇨 그냥 네이버카페에요",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100026356.1.15",
          "form": "지정헌혈이라고 들어보셨나요",
          "original_form": "지정헌혈이라고 들어보셨나요",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100026356.1.16",
          "form": "네이버카페군요",
          "original_form": "네이버카페군요",
          "speaker_id": "1",
          "time": "20210902 16:50"
        },
        {
          "id": "MDRW2100026356.1.17",
          "form": "저는 그단어가 낯설어요",
          "original_form": "저는 그단어가 낯설어요",
          "speaker_id": "1",
          "time": "20210902 16:50"
        },
        {
          "id": "MDRW2100026356.1.18",
          "form": "온라인으로만 활동하는 커뮤니티인가요",
          "original_form": "온라인으로만 활동하는 커뮤니티인가요",
          "speaker_id": "1",
          "time": "20210902 16:50"
        },
        {
          "id": "MDRW2100026356.1.19",
          "form": "활동이랄건 딱히없고요",
          "original_form": "활동이랄건 딱히없고요",
          "speaker_id": "2",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100026356.1.20",
          "form": "지정헌혈은 내가 헌혈을해서 누구에게 주겠다라고 말 그대로 지정할수있는거에욤",
          "original_form": "지정헌혈은 내가 헌혈을해서 누구에게 주겠다라고 말 그대로 지정할수있는거에욤",
          "speaker_id": "2",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100026356.1.21",
          "form": "커뮤니티에서 정보 얻고 헌헐의집에서 헌혈후 증서를 드리는거에요?",
          "original_form": "커뮤니티에서 정보 얻고 헌헐의집에서 헌혈후 증서를 드리는거에요?",
          "speaker_id": "1",
          "time": "20210902 17:29"
        },
        {
          "id": "MDRW2100026356.1.22",
          "form": "네이버카페 이름이 뭐에요?",
          "original_form": "네이버카페 이름이 뭐에요?",
          "speaker_id": "1",
          "time": "20210902 17:29"
        },
        {
          "id": "MDRW2100026356.1.23",
          "form": "아뇨 헌혈하러 가기전에 연락하면",
          "original_form": "아뇨 헌혈하러 가기전에 연락하면",
          "speaker_id": "2",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100026356.1.24",
          "form": "오늘가장 헌혈이 급한 환자 정보를 줘요 그럼 헌혈하실때 그 환자한테 헌혈한다고",
          "original_form": "오늘가장 헌혈이 급한 환자 정보를 줘요 그럼 헌혈하실때 그 환자한테 헌혈한다고",
          "speaker_id": "2",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100026356.1.25",
          "form": "서류 작성하시는거에요 이해가시나용",
          "original_form": "서류 작성하시는거에요 이해가시나용",
          "speaker_id": "2",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100026356.1.26",
          "form": "오 그런게 있군요",
          "original_form": "오 그런게 있군요",
          "speaker_id": "1",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100026356.1.27",
          "form": "취지가 너무 좋네요",
          "original_form": "취지가 너무 좋네요",
          "speaker_id": "1",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100026356.1.28",
          "form": "네이버 카페 저도 가입할게요",
          "original_form": "네이버 카페 저도 가입할게요",
          "speaker_id": "1",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100026356.1.29",
          "form": "선한 취지 좋아요",
          "original_form": "선한 취지 좋아요",
          "speaker_id": "1",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100026356.1.30",
          "form": "커뮤로 만나 더 커지고요",
          "original_form": "커뮤로 만나 더 커지고요",
          "speaker_id": "1",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100026356.1.31",
          "form": "정보감사해요",
          "original_form": "정보감사해요",
          "speaker_id": "1",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100026356.1.32",
          "form": "혹시 남자분들께 많이 추천해주세요",
          "original_form": "혹시 남자분들께 많이 추천해주세요",
          "speaker_id": "2",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100026356.1.33",
          "form": "여자들은 혈소판 헌혈하기 힘들거든요ㅜ",
          "original_form": "여자들은 혈소판 헌혈하기 힘들거든요ㅜ",
          "speaker_id": "2",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100026356.1.34",
          "form": "네. 저도 헌혈 튕겨서..",
          "original_form": "네. 저도 헌혈 튕겨서..",
          "speaker_id": "1",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100026356.1.35",
          "form": "특히 출산후엔 혈소판 헌혈못하고요ㅜ",
          "original_form": "특히 출산후엔 혈소판 헌혈못하고요ㅜ",
          "speaker_id": "2",
          "time": "20210902 19:45"
        }
      ]
