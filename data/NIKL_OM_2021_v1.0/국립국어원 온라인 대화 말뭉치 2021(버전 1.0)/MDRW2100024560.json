      "utterance": [
        {
          "id": "MDRW2100024560.1.1",
          "form": "연극이나 뮤지컬 좋아하세요?",
          "original_form": "연극이나 뮤지컬 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100024560.1.2",
          "form": "예전에 좋아하는 배우 보러 몇 번 갔었어요!",
          "original_form": "예전에 좋아하는 배우 보러 몇 번 갔었어요!",
          "speaker_id": "1",
          "time": "20210831 14:51"
        },
        {
          "id": "MDRW2100024560.1.3",
          "form": "혹시 공연 좋아하시나요?",
          "original_form": "혹시 공연 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 14:51"
        },
        {
          "id": "MDRW2100024560.1.4",
          "form": "좋아하는 편이에요ㅎㅎ",
          "original_form": "좋아하는 편이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 15:10"
        },
        {
          "id": "MDRW2100024560.1.5",
          "form": "어떤 배우 좋아하세요?",
          "original_form": "어떤 배우 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 15:10"
        },
        {
          "id": "MDRW2100024560.1.6",
          "form": "서경수라는 뮤지컬배우 좋아하는데 혹시 아세요?",
          "original_form": "서경수라는 뮤지컬배우 좋아하는데 혹시 아세요?",
          "speaker_id": "1",
          "time": "20210831 15:10"
        },
        {
          "id": "MDRW2100024560.1.7",
          "form": "잘 몰라요ㅜㅜ제가 뮤지컬배우들을 잘 알 지 못해요...ㅋㅋㅋ",
          "original_form": "잘 몰라요ㅜㅜ제가 뮤지컬배우들을 잘 알 지 못해요...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100024560.1.8",
          "form": "아 그럴수있죠ㅎㅎ 혹시 어떤 종류 공연 좋아하세요?",
          "original_form": "아 그럴수있죠ㅎㅎ 혹시 어떤 종류 공연 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100024560.1.9",
          "form": "시간 날 때 가끔 연극보러가요ㅎ 소극장연극이요! 뮤지컬은 영웅 뮤지컬 봤어요",
          "original_form": "시간 날 때 가끔 연극보러가요ㅎ 소극장연극이요! 뮤지컬은 영웅 뮤지컬 봤어요",
          "speaker_id": "2",
          "time": "20210831 15:15"
        },
        {
          "id": "MDRW2100024560.1.10",
          "form": "요즘에는 코로나때문에 전혀 못가고 있지만요ㅜ",
          "original_form": "요즘에는 코로나때문에 전혀 못가고 있지만요ㅜ",
          "speaker_id": "2",
          "time": "20210831 15:15"
        },
        {
          "id": "MDRW2100024560.1.11",
          "form": "아 맞아요 ㅠㅠ 코로나때문에 저도 못 본 지 2년이 넘어가네요",
          "original_form": "아 맞아요 ㅠㅠ 코로나때문에 저도 못 본 지 2년이 넘어가네요",
          "speaker_id": "1",
          "time": "20210831 15:15"
        },
        {
          "id": "MDRW2100024560.1.12",
          "form": "영화관은 가시나요?",
          "original_form": "영화관은 가시나요?",
          "speaker_id": "1",
          "time": "20210831 15:16"
        },
        {
          "id": "MDRW2100024560.1.13",
          "form": "영화관도 못가고있어요ㅜㅜ",
          "original_form": "영화관도 못가고있어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 15:17"
        },
        {
          "id": "MDRW2100024560.1.14",
          "form": "집에서 구매해서 봅니다ㅜㅜ",
          "original_form": "집에서 구매해서 봅니다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 15:17"
        },
        {
          "id": "MDRW2100024560.1.15",
          "form": "저두요 가서 관람하기가 쉽지 않죠 ㅠㅠ",
          "original_form": "저두요 가서 관람하기가 쉽지 않죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 15:20"
        },
        {
          "id": "MDRW2100024560.1.16",
          "form": "전시회는 그래도 거리두기가 되어서 가기 어렵지 않더라구요!",
          "original_form": "전시회는 그래도 거리두기가 되어서 가기 어렵지 않더라구요!",
          "speaker_id": "1",
          "time": "20210831 15:21"
        },
        {
          "id": "MDRW2100024560.1.17",
          "form": "괜찮은 전시회 있나요?",
          "original_form": "괜찮은 전시회 있나요?",
          "speaker_id": "2",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100024560.1.18",
          "form": "아 저는 국립현대미술관 가끔 가요ㅎㅎㅎ 미술 잘은 모르지만 근처라",
          "original_form": "아 저는 국립현대미술관 가끔 가요ㅎㅎㅎ 미술 잘은 모르지만 근처라",
          "speaker_id": "1",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100024560.1.19",
          "form": "이번에 이건희 소장품? 그것도 하더라구요",
          "original_form": "이번에 이건희 소장품? 그것도 하더라구요",
          "speaker_id": "1",
          "time": "20210831 15:31"
        },
        {
          "id": "MDRW2100024560.1.20",
          "form": "아아 이건희 소장품 전시회 보셨어요?",
          "original_form": "아아 이건희 소장품 전시회 보셨어요?",
          "speaker_id": "2",
          "time": "20210831 15:34"
        },
        {
          "id": "MDRW2100024560.1.21",
          "form": "그건 예매하기가 어려워서 못 갔습니다 ㅠㅠ 나중에 가보려구요",
          "original_form": "그건 예매하기가 어려워서 못 갔습니다 ㅠㅠ 나중에 가보려구요",
          "speaker_id": "1",
          "time": "20210831 15:35"
        }
      ]
    }
