      "utterance": [
        {
          "id": "MDRW2100009887.1.1",
          "form": "오일하고 이제 듬뿍 듬뿍 ㅋㅋㅋㅋ",
          "original_form": "오일하고 이제 듬뿍 듬뿍 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:17"
        },
        {
          "id": "MDRW2100009887.1.2",
          "form": "양말안신고 슬리퍼 신고 다녔더니 발도 건조해여 ㅋㅋㅋㅋ",
          "original_form": "양말안신고 슬리퍼 신고 다녔더니 발도 건조해여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:18"
        },
        {
          "id": "MDRW2100009887.1.3",
          "form": "좀 서늘해지면 운동도 좀 하고~",
          "original_form": "좀 서늘해지면 운동도 좀 하고~",
          "speaker_id": "2",
          "time": "20210823 11:18"
        },
        {
          "id": "MDRW2100009887.1.4",
          "form": "그래 우리원도 여름엔 발 건조하니",
          "original_form": "그래 우리원도 여름엔 발 건조하니",
          "speaker_id": "1",
          "time": "20210823 11:56"
        },
        {
          "id": "MDRW2100009887.1.5",
          "form": "항상 껍질벗겨지더라",
          "original_form": "항상 껍질벗겨지더라",
          "speaker_id": "1",
          "time": "20210823 11:56"
        },
        {
          "id": "MDRW2100009887.1.6",
          "form": "팩도 좀 하고 해야되는데 왜이래 안되노 ㅎㅎ",
          "original_form": "팩도 좀 하고 해야되는데 왜이래 안되노 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 11:56"
        },
        {
          "id": "MDRW2100009887.1.7",
          "form": "그기 진짜 안되요ㅠ",
          "original_form": "그기 진짜 안되요ㅠ",
          "speaker_id": "2",
          "time": "20210823 12:04"
        },
        {
          "id": "MDRW2100009887.1.8",
          "form": "나 예전엔 1일1팩했는데 ㅋㅋㅋㅋ지금은 뭐..",
          "original_form": "나 예전엔 1일1팩했는데 ㅋㅋㅋㅋ지금은 뭐..",
          "speaker_id": "2",
          "time": "20210823 12:05"
        },
        {
          "id": "MDRW2100009887.1.9",
          "form": "다 썩어서 버려요 ㅋㅋㅋㅋ",
          "original_form": "다 썩어서 버려요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:05"
        },
        {
          "id": "MDRW2100009887.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ 그래 1팩은 어렵다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그래 1팩은 어렵다",
          "speaker_id": "1",
          "time": "20210823 12:21"
        },
        {
          "id": "MDRW2100009887.1.11",
          "form": "뉴스킨에 팩 하나 살라고 했는데",
          "original_form": "뉴스킨에 팩 하나 살라고 했는데",
          "speaker_id": "1",
          "time": "20210823 12:21"
        },
        {
          "id": "MDRW2100009887.1.12",
          "form": "비싸서 또 고민하고 있음",
          "original_form": "비싸서 또 고민하고 있음",
          "speaker_id": "1",
          "time": "20210823 12:22"
        },
        {
          "id": "MDRW2100009887.1.13",
          "form": "ㅋㅋㅋㅋㅋ뉴스킨 뭐요??",
          "original_form": "ㅋㅋㅋㅋㅋ뉴스킨 뭐요??",
          "speaker_id": "2",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100009887.1.14",
          "form": "뉴스킨 페이스리프트팩",
          "original_form": "뉴스킨 페이스리프트팩",
          "speaker_id": "1",
          "time": "20210823 12:24"
        },
        {
          "id": "MDRW2100009887.1.15",
          "form": "괴물팩",
          "original_form": "괴물팩",
          "speaker_id": "1",
          "time": "20210823 12:24"
        },
        {
          "id": "MDRW2100009887.1.16",
          "form": "그걸줄 알았음 ㅋㅋㅋㅋㅋ",
          "original_form": "그걸줄 알았음 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:24"
        },
        {
          "id": "MDRW2100009887.1.17",
          "form": "언니 뉴스킨 써요?? 난 옛날에 같이일했단 언니가 뉴스킨해서ㅠ",
          "original_form": "언니 뉴스킨 써요?? 난 옛날에 같이일했단 언니가 뉴스킨해서ㅠ",
          "speaker_id": "2",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009887.1.18",
          "form": "보조제하고 윽시 시켜먹고 발랐는데..",
          "original_form": "보조제하고 윽시 시켜먹고 발랐는데..",
          "speaker_id": "2",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009887.1.19",
          "form": "아 ㅋㅋㅋㅋㅋㅋㄱ 아니 팩만 그거쓴다",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㄱ 아니 팩만 그거쓴다",
          "speaker_id": "1",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009887.1.20",
          "form": "근데 사놓고 잘 안바르니 ㅎㅎ",
          "original_form": "근데 사놓고 잘 안바르니 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009887.1.21",
          "form": "가격대비 별로던데ㅠ 지금도 돈아까움..ㅍㅍ",
          "original_form": "가격대비 별로던데ㅠ 지금도 돈아까움..ㅍㅍ",
          "speaker_id": "2",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009887.1.22",
          "form": "그람 말아요 ㅋㅋㅋㅋㅋ",
          "original_form": "그람 말아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009887.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ 인터넷이 더 싸길래",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 인터넷이 더 싸길래",
          "speaker_id": "1",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009887.1.24",
          "form": "짝퉁일까싶어 고민중",
          "original_form": "짝퉁일까싶어 고민중",
          "speaker_id": "1",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009887.1.25",
          "form": "그래 나도 한때 뉴스킨 180 그거쓰고 했는데",
          "original_form": "그래 나도 한때 뉴스킨 180 그거쓰고 했는데",
          "speaker_id": "1",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009887.1.26",
          "form": "너무 비쌈",
          "original_form": "너무 비쌈",
          "speaker_id": "1",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009887.1.27",
          "form": "ㅋㅋㅋㅋ맞아여~그거 짝퉁워낙 많아가~",
          "original_form": "ㅋㅋㅋㅋ맞아여~그거 짝퉁워낙 많아가~",
          "speaker_id": "2",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009887.1.28",
          "form": "엉 정품사는게 낫겠제??",
          "original_form": "엉 정품사는게 낫겠제??",
          "speaker_id": "1",
          "time": "20210823 12:27"
        },
        {
          "id": "MDRW2100009887.1.29",
          "form": "미스트랑 하니 만원차이나던데",
          "original_form": "미스트랑 하니 만원차이나던데",
          "speaker_id": "1",
          "time": "20210823 12:27"
        },
        {
          "id": "MDRW2100009887.1.30",
          "form": "살꺼면 정품으로 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "살꺼면 정품으로 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:27"
        },
        {
          "id": "MDRW2100009887.1.31",
          "form": "옹 알았으 정품사야긋다",
          "original_form": "옹 알았으 정품사야긋다",
          "speaker_id": "1",
          "time": "20210823 12:27"
        },
        {
          "id": "MDRW2100009887.1.32",
          "form": "수분크림도 사야되고",
          "original_form": "수분크림도 사야되고",
          "speaker_id": "1",
          "time": "20210823 12:27"
        },
        {
          "id": "MDRW2100009887.1.33",
          "form": "짝퉁진짜 똑같던데~중국놈들 ㅋㅋㅋㅋ",
          "original_form": "짝퉁진짜 똑같던데~중국놈들 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:27"
        },
        {
          "id": "MDRW2100009887.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 12:27"
        },
        {
          "id": "MDRW2100009887.1.35",
          "form": "이제 많이 발라야 겨울에 안땡김 ㅋㅋㅋㅋ",
          "original_form": "이제 많이 발라야 겨울에 안땡김 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:27"
        },
        {
          "id": "MDRW2100009887.1.36",
          "form": "언니 오일은 안써여??",
          "original_form": "언니 오일은 안써여??",
          "speaker_id": "2",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009887.1.37",
          "form": "난 오일도 쓰지",
          "original_form": "난 오일도 쓰지",
          "speaker_id": "1",
          "time": "20210823 13:31"
        },
        {
          "id": "MDRW2100009887.1.38",
          "form": "내 악건성이라서 여름에도 크림까지 다바르고 썬크림 바름",
