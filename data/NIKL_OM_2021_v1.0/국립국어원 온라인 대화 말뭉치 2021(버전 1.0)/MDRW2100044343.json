        {
          "id": "MDRW2100044343.1.1",
          "form": "스키즈에 비하먄 다 큰편이군ㅋㅋㅋ",
          "original_form": "스키즈에 비하먄 다 큰편이군ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044343.1.2",
          "form": "그니까 스키즈 완전.. 투바투가 생각보다 크다던데",
          "original_form": "그니까 스키즈 완전.. 투바투가 생각보다 크다던데",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044343.1.3",
          "form": "ㅋㅋㅋㅋ 아그래? 하긴 수빈도 크고 나머진 잘 모르지만..",
          "original_form": "ㅋㅋㅋㅋ 아그래? 하긴 수빈도 크고 나머진 잘 모르지만..",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044343.1.4",
          "form": "요즘엔 키큰사람이 없는듯..",
          "original_form": "요즘엔 키큰사람이 없는듯..",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044343.1.5",
          "form": "맞아 예전엔 180 거의 넘고 그랬던거같은데",
          "original_form": "맞아 예전엔 180 거의 넘고 그랬던거같은데",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044343.1.6",
          "form": "진짜 안넘어도 170중후반",
          "original_form": "진짜 안넘어도 170중후반",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044343.1.7",
          "form": "그니까 그정도도 작다고 그랬던거같은데",
          "original_form": "그니까 그정도도 작다고 그랬던거같은데",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044343.1.8",
          "form": "이젠 여돌들이랑 별 차이도 안날듯ㅋㅋㅋ",
          "original_form": "이젠 여돌들이랑 별 차이도 안날듯ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044343.1.9",
          "form": "ㅋㅋㅋ 그니까 여돌들이 큰애들 더 많을거같아",
          "original_form": "ㅋㅋㅋ 그니까 여돌들이 큰애들 더 많을거같아",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044343.1.10",
          "form": "맞아 진짜 노답..",
          "original_form": "맞아 진짜 노답..",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044343.1.11",
          "form": "힐 신으면 비슷하겠다",
          "original_form": "힐 신으면 비슷하겠다",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044343.1.12",
          "form": "하이힐은 걔네가 신어야 겠어 \n그니깤ㅋㅋ",
          "original_form": "하이힐은 걔네가 신어야 겠어 \n그니깤ㅋㅋ",
