      "utterance": [
        {
          "id": "MDRW2100022379.1.1",
          "form": "메타버스 익숙한가요?",
          "original_form": "메타버스 익숙한가요?",
          "speaker_id": "2",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022379.1.2",
          "form": "별루 잘 모르겠어요ㅎ",
          "original_form": "별루 잘 모르겠어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100022379.1.3",
          "form": "공부를 해야할듯요ㅠ",
          "original_form": "공부를 해야할듯요ㅠ",
          "speaker_id": "1",
          "time": "20210829 15:37"
        },
        {
          "id": "MDRW2100022379.1.4",
          "form": "세상이 급변하네요",
          "original_form": "세상이 급변하네요",
          "speaker_id": "1",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022379.1.5",
          "form": "따라가기힘든",
          "original_form": "따라가기힘든",
          "speaker_id": "1",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022379.1.6",
          "form": "네네 네이버 제페토 아세요?",
          "original_form": "네네 네이버 제페토 아세요?",
          "speaker_id": "2",
          "time": "20210829 16:02"
        },
        {
          "id": "MDRW2100022379.1.7",
          "form": "그것도 말만들어봤어요ㅠ",
          "original_form": "그것도 말만들어봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100022379.1.8",
          "form": "검색해보니 꽤 인기가 많은 서비스같아요..ㅎㅎ",
          "original_form": "검색해보니 꽤 인기가 많은 서비스같아요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100022379.1.9",
          "form": "아그래요?가상현실이라",
          "original_form": "아그래요?가상현실이라",
          "speaker_id": "1",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100022379.1.10",
          "form": "네 저희 어렸을때도 가상 가수 아담? 있었던거 기억나세요?",
          "original_form": "네 저희 어렸을때도 가상 가수 아담? 있었던거 기억나세요?",
          "speaker_id": "2",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100022379.1.11",
          "form": "아 맞네요ㅋㅋ",
          "original_form": "아 맞네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022379.1.12",
          "form": "생각나요",
          "original_form": "생각나요",
          "speaker_id": "1",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022379.1.13",
          "form": "오 기억하시는군요 저는 가상세계 가수 아담의 노래도 기억이 날 것 같아요..필요없는 것만 기억..ㅋㅋ",
          "original_form": "오 기억하시는군요 저는 가상세계 가수 아담의 노래도 기억이 날 것 같아요..필요없는 것만 기억..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022379.1.14",
          "form": "전 얼굴이 기억나요",
          "original_form": "전 얼굴이 기억나요",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
