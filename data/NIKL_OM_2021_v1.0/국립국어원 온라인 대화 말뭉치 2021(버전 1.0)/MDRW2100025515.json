        {
          "id": "MDRW2100025515.1.1",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "1",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100025515.1.2",
          "form": "전 없어요",
          "original_form": "전 없어요",
          "speaker_id": "1",
          "time": "20210901 12:32"
        },
        {
          "id": "MDRW2100025515.1.3",
          "form": "저도 없고",
          "original_form": "저도 없고",
          "speaker_id": "2",
          "time": "20210901 12:55"
        },
        {
          "id": "MDRW2100025515.1.4",
          "form": "부모님은불교요",
          "original_form": "부모님은불교요",
          "speaker_id": "2",
          "time": "20210901 12:55"
        },
        {
          "id": "MDRW2100025515.1.5",
          "form": "절은 자주 가시나요?",
          "original_form": "절은 자주 가시나요?",
          "speaker_id": "1",
          "time": "20210901 12:56"
        },
        {
          "id": "MDRW2100025515.1.6",
          "form": "한번 가봤어요",
          "original_form": "한번 가봤어요",
          "speaker_id": "2",
          "time": "20210901 12:58"
        },
        {
          "id": "MDRW2100025515.1.7",
          "form": "교회는요?",
          "original_form": "교회는요?",
          "speaker_id": "1",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025515.1.8",
          "form": "친구따라 얘전애",
          "original_form": "친구따라 얘전애",
          "speaker_id": "2",
          "time": "20210901 13:05"
        },
        {
          "id": "MDRW2100025515.1.9",
          "form": "종교에 관심은 없으신거죠,",
          "original_form": "종교에 관심은 없으신거죠,",
          "speaker_id": "1",
          "time": "20210901 14:14"
        },
        {
          "id": "MDRW2100025515.1.10",
          "form": "아직은요",
          "original_form": "아직은요",
          "speaker_id": "2",
          "time": "20210901 14:35"
        },
        {
          "id": "MDRW2100025515.1.11",
          "form": "그래도 절은 위치가 참 좋은듯",
          "original_form": "그래도 절은 위치가 참 좋은듯",
          "speaker_id": "1",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100025515.1.12",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "2",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025515.1.13",
          "form": "주변에 종교인이 있나요",
          "original_form": "주변에 종교인이 있나요",
          "speaker_id": "1",
          "time": "20210901 17:21"
