        },
        {
          "id": "MDRW2100004683.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004683.1.3",
          "form": "올림픽이후에 스포츠 계속보시나요?",
          "original_form": "올림픽이후에 스포츠 계속보시나요?",
          "speaker_id": "1",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004683.1.4",
          "form": "아니요ㅎㅎ",
          "original_form": "아니요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004683.1.5",
          "form": "패럴림픽이 개막하긴 했더라고요",
          "original_form": "패럴림픽이 개막하긴 했더라고요",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004683.1.6",
          "form": "아그러네요 스포츠에 그래도 많이들 관심가지는거같아요",
          "original_form": "아그러네요 스포츠에 그래도 많이들 관심가지는거같아요",
          "speaker_id": "1",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004683.1.7",
          "form": "맞아요 올림픽쯤되면 반짝",
          "original_form": "맞아요 올림픽쯤되면 반짝",
          "speaker_id": "2",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004683.1.8",
          "form": "그래도 여자배구에 흥미가 생겨서 좋아요",
          "original_form": "그래도 여자배구에 흥미가 생겨서 좋아요",
          "speaker_id": "2",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004683.1.9",
          "form": "맞아요 메달못따는거 아는데도 정말 응원해주고싶더라구요",
          "original_form": "맞아요 메달못따는거 아는데도 정말 응원해주고싶더라구요",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004683.1.10",
          "form": "경기시간에 일부러 챙겨보기도 했고요 ㅎㅎ",
          "original_form": "경기시간에 일부러 챙겨보기도 했고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004683.1.11",
          "form": "시차도 없으니까 진짜 다챙겨봤어요ㅋㅋ",
          "original_form": "시차도 없으니까 진짜 다챙겨봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004683.1.12",
          "form": "네 시차 없는 올림픽 진짜 좋았어요 ㅎㅎ",
          "original_form": "네 시차 없는 올림픽 진짜 좋았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 18:08"
        },
        {
          "id": "MDRW2100004683.1.13",
          "form": "야구는 꽤 실망했고요",
          "original_form": "야구는 꽤 실망했고요",
          "speaker_id": "2",
          "time": "20210821 18:08"
        },
        {
          "id": "MDRW2100004683.1.14",
          "form": "야구는 이상했어요 졌는데 경기를 계속 하고 ㅋㅋㅋ 선수들도 포기한것처럼 보이고 ㅜㅜ",
          "original_form": "야구는 이상했어요 졌는데 경기를 계속 하고 ㅋㅋㅋ 선수들도 포기한것처럼 보이고 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 18:09"
        },
        {
          "id": "MDRW2100004683.1.15",
          "form": "네 감독 문제였던거 같기도하고 영 별로였어요",
          "original_form": "네 감독 문제였던거 같기도하고 영 별로였어요",
          "speaker_id": "2",
          "time": "20210821 18:09"
        },
        {
          "id": "MDRW2100004683.1.16",
          "form": "그렇군요ㅜㅜ 대학생때 야구좋아해서 직관하구 그랬는데 아쉬어요",
          "original_form": "그렇군요ㅜㅜ 대학생때 야구좋아해서 직관하구 그랬는데 아쉬어요",
          "speaker_id": "1",
          "time": "20210821 18:12"
        },
        {
          "id": "MDRW2100004683.1.17",
          "form": "저도 두산전은 꼭 갔었는데 점점 안가게되더라고요",
          "original_form": "저도 두산전은 꼭 갔었는데 점점 안가게되더라고요",
          "speaker_id": "2",
          "time": "20210821 18:12"
        }
      ]
