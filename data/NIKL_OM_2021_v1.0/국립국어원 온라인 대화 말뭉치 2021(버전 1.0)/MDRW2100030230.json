        },
        {
          "id": "MDRW2100030230.1.2",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030230.1.3",
          "form": "똥집  먹고싶네요",
          "original_form": "똥집  먹고싶네요",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030230.1.4",
          "form": "똥집 볶음이랑 튀김",
          "original_form": "똥집 볶음이랑 튀김",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030230.1.5",
          "form": "튀겨도 맛있죠",
          "original_form": "튀겨도 맛있죠",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030230.1.6",
          "form": "뭘더좋아하시나요",
          "original_form": "뭘더좋아하시나요",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030230.1.7",
          "form": "볶아도 맛있죠",
          "original_form": "볶아도 맛있죠",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030230.1.8",
          "form": "우열을 가릴수없네요",
          "original_form": "우열을 가릴수없네요",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030230.1.9",
          "form": "둘중 고르라면 튀김이요",
          "original_form": "둘중 고르라면 튀김이요",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030230.1.10",
          "form": "막상막하",
          "original_form": "막상막하",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030230.1.11",
          "form": "튀긴건  진리",
          "original_form": "튀긴건  진리",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030230.1.12",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030230.1.13",
          "form": "근데 마늘에 볶아도 좋아요",
