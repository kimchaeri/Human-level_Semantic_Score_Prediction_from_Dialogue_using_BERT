      "utterance": [
        {
          "id": "MDRW2100029599.1.1",
          "form": "우선 1년 계약 채우고 퇴직금이랑 실업급여 받으면서",
          "original_form": "우선 1년 계약 채우고 퇴직금이랑 실업급여 받으면서",
          "speaker_id": "2",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100029599.1.2",
          "form": "다음스텝으로 넘어가려구요 ㅋㅋㅋㅋ 고민중",
          "original_form": "다음스텝으로 넘어가려구요 ㅋㅋㅋㅋ 고민중",
          "speaker_id": "2",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100029599.1.3",
          "form": "저 아이엘츠 공부해보려고 하는데 혹시 해보신 적 있으세요?",
          "original_form": "저 아이엘츠 공부해보려고 하는데 혹시 해보신 적 있으세요?",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029599.1.4",
          "form": "오 대박...!!!",
          "original_form": "오 대박...!!!",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029599.1.5",
          "form": "아뇨아뇨",
          "original_form": "아뇨아뇨",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029599.1.6",
          "form": "아이엘츠 완전 실용영어 아닌가요?!",
          "original_form": "아이엘츠 완전 실용영어 아닌가요?!",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029599.1.7",
          "form": "엄청 어려워보이던데,,,",
          "original_form": "엄청 어려워보이던데,,,",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029599.1.8",
          "form": "저는 토플도 하다가 관뒀었어요 8ㅅ8",
          "original_form": "저는 토플도 하다가 관뒀었어요 8ㅅ8",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029599.1.9",
          "form": "토플이 더 어렵다고 들었는데!",
          "original_form": "토플이 더 어렵다고 들었는데!",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029599.1.10",
          "form": "name2 진로 계획 아주 좋아보여요 !!!!!! 1년 채우고 퇴직금받기!!!",
          "original_form": "&name2& 진로 계획 아주 좋아보여요 !!!!!! 1년 채우고 퇴직금받기!!!",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029599.1.11",
          "form": "아그런가요?!",
          "original_form": "아그런가요?!",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029599.1.12",
          "form": "그 어려운걸 하시다니 ㅋㅋㅋㅋ시도하신것도 대단한거죠",
          "original_form": "그 어려운걸 하시다니 ㅋㅋㅋㅋ시도하신것도 대단한거죠",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029599.1.13",
          "form": "토플도 어느정도 문제은행이라",
          "original_form": "토플도 어느정도 문제은행이라",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029599.1.14",
          "form": "네넼ㅋㅋㅋ",
          "original_form": "네넼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029599.1.15",
          "form": "외우면 된다고 하더라구요?!",
          "original_form": "외우면 된다고 하더라구요?!",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029599.1.16",
          "form": "물론 저는 하다가 말았습니다....",
          "original_form": "물론 저는 하다가 말았습니다....",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029599.1.17",
          "form": "아이엘츠도 사실 달달 외우면..된다도라고요? 공식이..있다더라고요?ㅋㅋㅋㅋ",
          "original_form": "아이엘츠도 사실 달달 외우면..된다도라고요? 공식이..있다더라고요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029599.1.18",
          "form": "모든 시험이란..",
          "original_form": "모든 시험이란..",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029599.1.19",
          "form": "그게 의미가 있는건지 참..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그게 의미가 있는건지 참..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029599.1.20",
          "form": "아진짜요?!?!",
          "original_form": "아진짜요?!?!",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029599.1.21",
          "form": "와 전혀 몰랐어요",
          "original_form": "와 전혀 몰랐어요",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029599.1.22",
          "form": "그래서 뭔가",
          "original_form": "그래서 뭔가",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029599.1.23",
          "form": "워홀 가시는 분들이",
          "original_form": "워홀 가시는 분들이",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029599.1.24",
          "form": "많이 준비하는 시험이라고",
          "original_form": "많이 준비하는 시험이라고",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029599.1.25",
          "form": "아 그래요???",
          "original_form": "아 그래요???",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029599.1.26",
          "form": "알고있었는데 아닌가요?! 아이엘츠,,,",
          "original_form": "알고있었는데 아닌가요?! 아이엘츠,,,",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029599.1.27",
          "form": "저근데 제가 모르는 분야에 대해서는",
          "original_form": "저근데 제가 모르는 분야에 대해서는",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
