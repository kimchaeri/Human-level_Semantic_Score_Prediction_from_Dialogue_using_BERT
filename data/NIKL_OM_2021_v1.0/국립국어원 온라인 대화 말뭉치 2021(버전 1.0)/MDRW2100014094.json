        {
          "id": "MDRW2100014094.1.1",
          "form": "아 한쪽수술후..또 언제수술해?",
          "original_form": "아 한쪽수술후..또 언제수술해?",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014094.1.2",
          "form": "6개월~1년",
          "original_form": "6개월~1년",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014094.1.3",
          "form": "어디서 수술할려고??",
          "original_form": "어디서 수술할려고??",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014094.1.4",
          "form": "분당서울대",
          "original_form": "분당서울대",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014094.1.5",
          "form": "엄마 상처가 크데",
          "original_form": "엄마 상처가 크데",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014094.1.6",
          "form": "수술어렵다고 하더나??",
          "original_form": "수술어렵다고 하더나??",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014094.1.7",
          "form": "그런말은 안하고 상처가 크다고",
          "original_form": "그런말은 안하고 상처가 크다고",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014094.1.8",
          "form": "내시경으로",
          "original_form": "내시경으로",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014094.1.9",
          "form": "수술비는 많이나온다고 하더나",
          "original_form": "수술비는 많이나온다고 하더나",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014094.1.10",
          "form": "수술비는 안 물어봤다",
          "original_form": "수술비는 안 물어봤다",
          "speaker_id": "1",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100014094.1.11",
          "form": "입원은 얼마나 해야한데??",
          "original_form": "입원은 얼마나 해야한데??",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014094.1.12",
          "form": "3박 4일",
          "original_form": "3박 4일",
          "speaker_id": "1",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014094.1.13",
          "form": "엄마 팔깁스는 한달반인가이고",
          "original_form": "엄마 팔깁스는 한달반인가이고",
          "speaker_id": "1",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014094.1.14",
          "form": "금방퇴원하네",
          "original_form": "금방퇴원하네",
          "speaker_id": "2",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014094.1.15",
          "form": "언니 내년에 휴직하나",
          "original_form": "언니 내년에 휴직하나",
          "speaker_id": "2",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014094.1.16",
          "form": "그래야지 휴직이나 시간제",
          "original_form": "그래야지 휴직이나 시간제",
          "speaker_id": "1",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014094.1.17",
          "form": "그래야 하겠네",
          "original_form": "그래야 하겠네",
          "speaker_id": "2",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014094.1.18",
          "form": "겁나네 수술한다고 생각하니깐",
          "original_form": "겁나네 수술한다고 생각하니깐",
          "speaker_id": "2",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014094.1.19",
          "form": "그래서 지금하는게 맞는듯",
          "original_form": "그래서 지금하는게 맞는듯",
          "speaker_id": "1",
          "time": "20210825 18:52"
