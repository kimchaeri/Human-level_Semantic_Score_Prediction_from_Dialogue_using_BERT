      "utterance": [
        {
          "id": "MDRW2100023588.1.1",
          "form": "오늘 아침메뉴는 뭐였나요?",
          "original_form": "오늘 아침메뉴는 뭐였나요?",
          "speaker_id": "2",
          "time": "20210830 12:01"
        },
        {
          "id": "MDRW2100023588.1.2",
          "form": "눈뜨자마자 출근해요",
          "original_form": "눈뜨자마자 출근해요",
          "speaker_id": "1",
          "time": "20210830 12:06"
        },
        {
          "id": "MDRW2100023588.1.3",
          "form": "ㅋㅋ아침원래거의안먹어요",
          "original_form": "ㅋㅋ아침원래거의안먹어요",
          "speaker_id": "1",
          "time": "20210830 12:06"
        },
        {
          "id": "MDRW2100023588.1.4",
          "form": "아침 안드시면 배 고플텐데",
          "original_form": "아침 안드시면 배 고플텐데",
          "speaker_id": "2",
          "time": "20210830 12:07"
        },
        {
          "id": "MDRW2100023588.1.5",
          "form": "저는 미숫가루 한잔 먹고나와요",
          "original_form": "저는 미숫가루 한잔 먹고나와요",
          "speaker_id": "2",
          "time": "20210830 12:07"
        },
        {
          "id": "MDRW2100023588.1.6",
          "form": "출근시간 짧나요?",
          "original_form": "출근시간 짧나요?",
          "speaker_id": "2",
          "time": "20210830 12:07"
        },
        {
          "id": "MDRW2100023588.1.7",
          "form": "걸어서 십분이에요",
          "original_form": "걸어서 십분이에요",
          "speaker_id": "1",
          "time": "20210830 12:10"
        },
        {
          "id": "MDRW2100023588.1.8",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210830 12:10"
        },
        {
          "id": "MDRW2100023588.1.9",
          "form": "와우 좋습니다 ㅋㅋ",
          "original_form": "와우 좋습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023588.1.10",
          "form": "점심은 뭐시드나요?",
          "original_form": "점심은 뭐시드나요?",
          "speaker_id": "2",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100023588.1.11",
          "form": "아 전 이제 먹어서 시간이 이렇게되었네요",
          "original_form": "아 전 이제 먹어서 시간이 이렇게되었네요",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023588.1.12",
          "form": "뭐 드셧나요?",
          "original_form": "뭐 드셧나요?",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023588.1.13",
          "form": "저는 미역국 먹었습니다 재택근무라 집에서 집밥먹네요",
          "original_form": "저는 미역국 먹었습니다 재택근무라 집에서 집밥먹네요",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023588.1.14",
          "form": "재택근무 최고네요ㅜㅜㅜㅜㅜㅜ",
          "original_form": "재택근무 최고네요ㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 14:54"
        },
        {
          "id": "MDRW2100023588.1.15",
          "form": "점심엔 그냥 도시락배달 다같이 먹엇어욤",
          "original_form": "점심엔 그냥 도시락배달 다같이 먹엇어욤",
          "speaker_id": "1",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100023588.1.16",
          "form": "음 도시락 업체가 호황이더라구요 코로나 때문에 ㅠ",
          "original_form": "음 도시락 업체가 호황이더라구요 코로나 때문에 ㅠ",
          "speaker_id": "2",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100023588.1.17",
          "form": "반찬 뭐뭐 있엇서요?",
          "original_form": "반찬 뭐뭐 있엇서요?",
          "speaker_id": "2",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023588.1.18",
          "form": "그냥 백반인데",
          "original_form": "그냥 백반인데",
          "speaker_id": "1",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023588.1.19",
          "form": "맛은 그닥이에요 ㅎㅎ",
          "original_form": "맛은 그닥이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023588.1.20",
          "form": "빨리 집에가서 맛잇는거 먹고싶어요",
          "original_form": "빨리 집에가서 맛잇는거 먹고싶어요",
          "speaker_id": "1",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023588.1.21",
          "form": "집에 맛있는거 있서요?",
          "original_form": "집에 맛있는거 있서요?",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023588.1.22",
          "form": "라면이요",
          "original_form": "라면이요",
          "speaker_id": "1",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023588.1.23",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023588.1.24",
          "form": "라면 저도 좋아합니다 ㅎㅎ",
          "original_form": "라면 저도 좋아합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023588.1.25",
          "form": "라면 좋아하는사람치고 나쁜사람 없음!",
          "original_form": "라면 좋아하는사람치고 나쁜사람 없음!",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023588.1.26",
          "form": "면중독자는 면이라면 다좋아요 ㅋㅋㅋ",
          "original_form": "면중독자는 면이라면 다좋아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100023588.1.27",
          "form": "저도 면좋아합니다 ㅎㅎㅎ",
          "original_form": "저도 면좋아합니다 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023588.1.28",
          "form": "짬뽕인가요 짜장인가요?",
          "original_form": "짬뽕인가요 짜장인가요?",
          "speaker_id": "2",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023588.1.29",
          "form": "아 어렵지만 지금이순간은",
          "original_form": "아 어렵지만 지금이순간은",
          "speaker_id": "1",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023588.1.30",
          "form": "짜장으로할게요",
          "original_form": "짜장으로할게요",
          "speaker_id": "1",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023588.1.31",
          "form": "희대의 문제입니다",
          "original_form": "희대의 문제입니다",
          "speaker_id": "2",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023588.1.32",
          "form": "난제죠",
          "original_form": "난제죠",
          "speaker_id": "2",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023588.1.33",
          "form": "매일 고민합니다",
          "original_form": "매일 고민합니다",
          "speaker_id": "2",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023588.1.34",
          "form": "그래도 저는 51퍼 짜장입니다",
          "original_form": "그래도 저는 51퍼 짜장입니다",
          "speaker_id": "2",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023588.1.35",
          "form": "백종원 홍콩반점 고추짜장 맛있더라구요",
          "original_form": "백종원 홍콩반점 고추짜장 맛있더라구요",
          "speaker_id": "2",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023588.1.36",
          "form": "고추는 반틈 뺍니다",
          "original_form": "고추는 반틈 뺍니다",
          "speaker_id": "2",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023588.1.37",
          "form": "크 거기 맛잇져ㅜㅜ",
          "original_form": "크 거기 맛잇져ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 15:03"
        },
        {
          "id": "MDRW2100023588.1.38",
          "form": "탕수육도 맛나더라구요",
          "original_form": "탕수육도 맛나더라구요",
          "speaker_id": "1",
          "time": "20210830 15:03"
        }
      ]
