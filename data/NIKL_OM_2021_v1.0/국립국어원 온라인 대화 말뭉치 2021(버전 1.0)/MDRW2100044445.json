        {
          "id": "MDRW2100044445.1.1",
          "form": "언제 왜",
          "original_form": "언제 왜",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044445.1.2",
          "form": "그냥 먹고 싶어서요",
          "original_form": "그냥 먹고 싶어서요",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044445.1.3",
          "form": "내일 점심시간 2 시간이니까",
          "original_form": "내일 점심시간 2 시간이니까",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044445.1.4",
          "form": "아 내일 수요일이구나",
          "original_form": "아 내일 수요일이구나",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044445.1.5",
          "form": "점심시간에 먹게",
          "original_form": "점심시간에 먹게",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044445.1.6",
          "form": "응 튀김은 점심에 먹어야 소화 잘 되지",
          "original_form": "응 튀김은 점심에 먹어야 소화 잘 되지",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044445.1.7",
          "form": "치킨 뭐 시킬 건데",
          "original_form": "치킨 뭐 시킬 건데",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044445.1.8",
          "form": "글쎄 어떤 게 제일 맛있지",
          "original_form": "글쎄 어떤 게 제일 맛있지",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044445.1.9",
          "form": "수원은 그냥 수원 통닭이 맛있지 않아",
          "original_form": "수원은 그냥 수원 통닭이 맛있지 않아",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044445.1.10",
          "form": "용성 성적이 제일 맛있는데",
          "original_form": "용성 성적이 제일 맛있는데",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044445.1.11",
          "form": "거기는 배달이 안 돼요",
          "original_form": "거기는 배달이 안 돼요",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044445.1.12",
          "form": "아 가질러 가야 돼",
          "original_form": "아 가질러 가야 돼",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044445.1.13",
          "form": "아마 그럴걸",
          "original_form": "아마 그럴걸",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044445.1.14",
          "form": "배달되는 곳에서 시켜",
          "original_form": "배달되는 곳에서 시켜",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044445.1.15",
          "form": "그럼 교촌치킨",
          "original_form": "그럼 교촌치킨",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044445.1.16",
          "form": "거기 맛있어",
          "original_form": "거기 맛있어",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
