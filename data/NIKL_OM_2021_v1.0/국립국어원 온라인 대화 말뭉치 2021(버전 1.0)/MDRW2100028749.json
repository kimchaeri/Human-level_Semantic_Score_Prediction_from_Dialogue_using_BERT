        },
        {
          "id": "MDRW2100028749.1.2",
          "form": "저녁 뭐드셨어요",
          "original_form": "저녁 뭐드셨어요",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028749.1.3",
          "form": "아니? 어느새 name1이?ㅋㅋㅋ",
          "original_form": "아니? 어느새 &name1&이?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028749.1.4",
          "form": "곧 사실거잖아요",
          "original_form": "곧 사실거잖아요",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028749.1.5",
          "form": "전 간만에 저녁시간에 집밥 먹었습니다ㅜ",
          "original_form": "전 간만에 저녁시간에 집밥 먹었습니다ㅜ",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028749.1.6",
          "form": "이 감격",
          "original_form": "이 감격",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028749.1.7",
          "form": "바라는대로 이루어진다고 합니다",
          "original_form": "바라는대로 이루어진다고 합니다",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028749.1.8",
          "form": "오 원래는",
          "original_form": "오 원래는",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028749.1.9",
          "form": "늦게 오시나요",
          "original_form": "늦게 오시나요",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028749.1.10",
          "form": "그럼 name2님은 제게 name1 자리를 물려주시고 물러나나요ㅋㅋ",
          "original_form": "그럼 &name2&님은 제게 &name1& 자리를 물려주시고 물러나나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028749.1.11",
          "form": "네 원래 좀 늦는 직장이라ㅜ",
          "original_form": "네 원래 좀 늦는 직장이라ㅜ",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028749.1.12",
          "form": "끝나고 술자리와 저녁이 섞이죠ㅎ",
          "original_form": "끝나고 술자리와 저녁이 섞이죠ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028749.1.13",
          "form": "라이언 역장 오만원에 팝니다",
          "original_form": "라이언 역장 오만원에 팝니다",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028749.1.14",
          "form": "이 시국에 회식을 하나요ㅎㅎㅎ",
          "original_form": "이 시국에 회식을 하나요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028749.1.15",
          "form": "아ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028749.1.16",
          "form": "회식말고",
          "original_form": "회식말고",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028749.1.17",
          "form": "짝수로 가서  둘둘 앉죠",
          "original_form": "짝수로 가서  둘둘 앉죠",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028749.1.18",
          "form": "이게 뭐하는 건지ㅜ",
          "original_form": "이게 뭐하는 건지ㅜ",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028749.1.19",
          "form": "회식은 원래 잘 안해요ㅎㅎ",
          "original_form": "회식은 원래 잘 안해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028749.1.20",
          "form": "그게 회식이 아닌가요",
          "original_form": "그게 회식이 아닌가요",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028749.1.21",
          "form": "아니ㅋㅋ",
          "original_form": "아니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028749.1.22",
          "form": "부서 전체로 안 가고",
          "original_form": "부서 전체로 안 가고",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028749.1.23",
          "form": "일 늦는 사람만 가는 거라서요ㅜ",
          "original_form": "일 늦는 사람만 가는 거라서요ㅜ",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028749.1.24",
          "form": "당직이나",
          "original_form": "당직이나",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028749.1.25",
          "form": "당직도 아닌데 일에 치인 사람이나",
          "original_form": "당직도 아닌데 일에 치인 사람이나",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028749.1.26",
          "form": "암튼 소규모입니다ㅎ",
          "original_form": "암튼 소규모입니다ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028749.1.27",
          "form": "어지간함 4명내에서 해결돼요ㅎ",
          "original_form": "어지간함 4명내에서 해결돼요ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:31"
        },
        {
          "id": "MDRW2100028749.1.28",
          "form": "name1으로 프사변경",
          "original_form": "&name1&으로 프사변경",
