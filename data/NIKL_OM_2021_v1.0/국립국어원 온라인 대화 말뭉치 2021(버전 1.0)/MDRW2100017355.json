      "utterance": [
        {
          "id": "MDRW2100017355.1.1",
          "form": "문특 올라온거 봤어?",
          "original_form": "문특 올라온거 봤어?",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017355.1.2",
          "form": "누구나온거?\n나 요새는 안봐가지구",
          "original_form": "누구나온거?\n나 요새는 안봐가지구",
          "speaker_id": "2",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017355.1.3",
          "form": "오늘! 부끄뚱ㅋㅋㅋ",
          "original_form": "오늘! 부끄뚱ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017355.1.4",
          "form": "언능 봐",
          "original_form": "언능 봐",
          "speaker_id": "1",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017355.1.5",
          "form": "나도 지금 보는중",
          "original_form": "나도 지금 보는중",
          "speaker_id": "1",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017355.1.6",
          "form": "헐 부끄뚱 나온다고?ㅋㅋㅋ",
          "original_form": "헐 부끄뚱 나온다고?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017355.1.7",
          "form": "짱 재밌겠네",
          "original_form": "짱 재밌겠네",
          "speaker_id": "2",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017355.1.8",
          "form": "응ㅋㅋ 먹방한다ㅋㅋㅋ",
          "original_form": "응ㅋㅋ 먹방한다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017355.1.9",
          "form": "라비도 나오네ㅋㅋ\n맥주마시는거 대박이다ㅋㅋ",
          "original_form": "라비도 나오네ㅋㅋ\n맥주마시는거 대박이다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:58"
        },
        {
          "id": "MDRW2100017355.1.10",
          "form": "난 찜닭까지 봤다 왤케 빨리봐?ㅋㅋ",
          "original_form": "난 찜닭까지 봤다 왤케 빨리봐?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017355.1.11",
          "form": "맥주는 영상 앞에 잠깐 나오더라고ㅋㅋㅋ",
          "original_form": "맥주는 영상 앞에 잠깐 나오더라고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017355.1.12",
          "form": "와 이제 다 봤는데 진짜 완전 웃기다ㅋㅋㅋ",
          "original_form": "와 이제 다 봤는데 진짜 완전 웃기다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017355.1.13",
          "form": "난 밥먹고 이제 들어간당~ 가서 봐야지ㅋㅋ",
          "original_form": "난 밥먹고 이제 들어간당~ 가서 봐야지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017355.1.14",
          "form": "그래 이따봐봐ㅋㅋ\n역시 문세윤이야ㅋㅋ",
          "original_form": "그래 이따봐봐ㅋㅋ\n역시 문세윤이야ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017355.1.15",
          "form": "오키ㅋㅋ",
          "original_form": "오키ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017355.1.16",
          "form": "진짜 잘먹어ㅋㅋ",
          "original_form": "진짜 잘먹어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100017355.1.17",
          "form": "맥주 안따갑나 목구멍열고 먹으면ㅋㅋ",
          "original_form": "맥주 안따갑나 목구멍열고 먹으면ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100017355.1.18",
          "form": "그니까ㅋㅋ쉽게 잘 마시더라ㅋㅋ",
          "original_form": "그니까ㅋㅋ쉽게 잘 마시더라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:36"
        },
        {
          "id": "MDRW2100017355.1.19",
          "form": "쌈 먹을때 진짜 멋있어ㅋㅋㄱㅋ",
          "original_form": "쌈 먹을때 진짜 멋있어ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210827 11:36"
        },
        {
          "id": "MDRW2100017355.1.20",
          "form": "어제 저녁먹어서 오늘 저녁 안먹는다는거 봤지?ㅋㅋㅋ",
          "original_form": "어제 저녁먹어서 오늘 저녁 안먹는다는거 봤지?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:38"
        },
        {
          "id": "MDRW2100017355.1.21",
          "form": "어이없는표정ㅋㅋ",
          "original_form": "어이없는표정ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:38"
        },
        {
          "id": "MDRW2100017355.1.22",
          "form": "아니그니까  진짜 말도안돼ㅋㅋ",
          "original_form": "아니그니까  진짜 말도안돼ㅋㅋ",
