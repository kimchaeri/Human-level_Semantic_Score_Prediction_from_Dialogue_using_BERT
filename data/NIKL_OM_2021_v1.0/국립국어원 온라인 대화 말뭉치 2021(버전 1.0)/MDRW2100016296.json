      "utterance": [
        {
          "id": "MDRW2100016296.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100016296.1.2",
          "form": "좋아하시는 음악이나 드라마 영화 있으세요?",
          "original_form": "좋아하시는 음악이나 드라마 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100016296.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100016296.1.4",
          "form": "음악은 탑100들어요",
          "original_form": "음악은 탑100들어요",
          "speaker_id": "2",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100016296.1.5",
          "form": "영화 추천해주세요!",
          "original_form": "영화 추천해주세요!",
          "speaker_id": "2",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100016296.1.6",
          "form": "아항ㅎㅎ",
          "original_form": "아항ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016296.1.7",
          "form": "저 최근에 마샬이라고",
          "original_form": "저 최근에 마샬이라고",
          "speaker_id": "1",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016296.1.8",
          "form": "블랙펜서로 유명한 채드윅보스만 배우가 주연한 영화봤는데",
          "original_form": "블랙펜서로 유명한 채드윅보스만 배우가 주연한 영화봤는데",
          "speaker_id": "1",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016296.1.9",
          "form": "감명깊었어요!",
          "original_form": "감명깊었어요!",
          "speaker_id": "1",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016296.1.10",
          "form": "아 마셜이에요!ㅎㅎ",
          "original_form": "아 마셜이에요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016296.1.11",
          "form": "아하 어디서 볼수있어요?",
          "original_form": "아하 어디서 볼수있어요?",
          "speaker_id": "2",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016296.1.12",
          "form": "스트리밍 사이트? 아니면 극장인가요?",
          "original_form": "스트리밍 사이트? 아니면 극장인가요?",
          "speaker_id": "2",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016296.1.13",
          "form": "저는 왓챠에서 봤는데",
          "original_form": "저는 왓챠에서 봤는데",
          "speaker_id": "1",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016296.1.14",
          "form": "다른 OTT 사이트에도 있는지는 모르겠어용ㅠ",
          "original_form": "다른 OTT 사이트에도 있는지는 모르겠어용ㅠ",
          "speaker_id": "1",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016296.1.15",
          "form": "아.. 왓챠없는데 ㅜㅜ",
          "original_form": "아.. 왓챠없는데 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016296.1.16",
          "form": "좋아하는 영화 있으세요?",
          "original_form": "좋아하는 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016296.1.17",
          "form": "음 저는 액션 주로봐요",
          "original_form": "음 저는 액션 주로봐요",
          "speaker_id": "2",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100016296.1.18",
          "form": "최근에 넷플릭스에서 뤼팽 드라마 보고있어요!",
          "original_form": "최근에 넷플릭스에서 뤼팽 드라마 보고있어요!",
          "speaker_id": "2",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100016296.1.19",
          "form": "액션이라기보다는... 범죄? 쪽인거같네요 ㅎㅎ",
          "original_form": "액션이라기보다는... 범죄? 쪽인거같네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100016296.1.20",
          "form": "뤼팽 처음 들어보는데 꼭 한번 봐볼게요!ㅎㅎ",
          "original_form": "뤼팽 처음 들어보는데 꼭 한번 봐볼게요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100016296.1.21",
          "form": "최근에 영화관 가신적 있으세요?",
          "original_form": "최근에 영화관 가신적 있으세요?",
          "speaker_id": "2",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100016296.1.22",
          "form": "아 저 최근에 개봉한 영화",
          "original_form": "아 저 최근에 개봉한 영화",
          "speaker_id": "1",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100016296.1.23",
          "form": "그 세글잔데 뭐죠 이광수나오는",
          "original_form": "그 세글잔데 뭐죠 이광수나오는",
          "speaker_id": "1",
          "time": "20210826 11:49"
        },
        {
          "id": "MDRW2100016296.1.24",
          "form": "아그 집무너지는거 맞죠?",
          "original_form": "아그 집무너지는거 맞죠?",
          "speaker_id": "2",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100016296.1.25",
          "form": "네네 맞아요 이거 예고편 봤는데 너무 재밌어 보이더라구요ㅋㅋ",
          "original_form": "네네 맞아요 이거 예고편 봤는데 너무 재밌어 보이더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100016296.1.26",
          "form": "저도 함 보러 가야겠네요 ㅎㅎ",
          "original_form": "저도 함 보러 가야겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016296.1.27",
          "form": "유튜브는 뭐 주로 보세요?",
          "original_form": "유튜브는 뭐 주로 보세요?",
          "speaker_id": "2",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016296.1.28",
          "form": "주로 동물 동영상 보는데 name2님은 뭐보세용?",
          "original_form": "주로 동물 동영상 보는데 &name2&님은 뭐보세용?",
          "speaker_id": "1",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100016296.1.29",
          "form": "저는 먹방 많이봐요ㅋㅋㅋ",
          "original_form": "저는 먹방 많이봐요ㅋㅋㅋ",
          "speaker_id": "2",
