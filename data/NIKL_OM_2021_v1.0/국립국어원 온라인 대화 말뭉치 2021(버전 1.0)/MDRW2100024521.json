      "utterance": [
        {
          "id": "MDRW2100024521.1.1",
          "form": "저도 그런거 느껴보고 싶네요",
          "original_form": "저도 그런거 느껴보고 싶네요",
          "speaker_id": "2",
          "time": "20210831 13:03"
        },
        {
          "id": "MDRW2100024521.1.2",
          "form": "아리송한거 말고 확신적인 그런 끌림 ㅋㅋ",
          "original_form": "아리송한거 말고 확신적인 그런 끌림 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:03"
        },
        {
          "id": "MDRW2100024521.1.3",
          "form": "그런게 인연이겠죠",
          "original_form": "그런게 인연이겠죠",
          "speaker_id": "1",
          "time": "20210831 13:06"
        },
        {
          "id": "MDRW2100024521.1.4",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "2",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100024521.1.5",
          "form": "저희 부모님도 제게 그런게 나타나길 학수고대 중이신듯 하네요 ㅋㅋ",
          "original_form": "저희 부모님도 제게 그런게 나타나길 학수고대 중이신듯 하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100024521.1.6",
          "form": "자식들의 연애를 바라는 부모님이 꼭 계시죠ㅋㅋ",
          "original_form": "자식들의 연애를 바라는 부모님이 꼭 계시죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100024521.1.7",
          "form": "언제쯤 기대를 버리시련지..",
          "original_form": "언제쯤 기대를 버리시련지..",
          "speaker_id": "2",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100024521.1.8",
          "form": "연애 참견은 싫죠ㅋㅋ",
          "original_form": "연애 참견은 싫죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:14"
        },
        {
          "id": "MDRW2100024521.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 13:15"
        },
        {
          "id": "MDRW2100024521.1.10",
          "form": "전 결혼도 안할 것 같은데ㅋㅋ 부모님은 내심 제가 언젠가 할거라 생각하고 계신듯하네요",
          "original_form": "전 결혼도 안할 것 같은데ㅋㅋ 부모님은 내심 제가 언젠가 할거라 생각하고 계신듯하네요",
          "speaker_id": "2",
          "time": "20210831 13:16"
        },
        {
          "id": "MDRW2100024521.1.11",
          "form": "세대에 따라 결혼에 대한 생각차이가 있기 마련인데 말이죠",
          "original_form": "세대에 따라 결혼에 대한 생각차이가 있기 마련인데 말이죠",
          "speaker_id": "1",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100024521.1.12",
          "form": "그러게 말입니다",
          "original_form": "그러게 말입니다",
          "speaker_id": "2",
          "time": "20210831 13:21"
        },
        {
          "id": "MDRW2100024521.1.13",
          "form": "제 여동생은 저랑 반대예요",
          "original_form": "제 여동생은 저랑 반대예요",
          "speaker_id": "2",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100024521.1.14",
          "form": "걘 금방 사랑에 빠지고 금방 식어요ㅋㅋ",
          "original_form": "걘 금방 사랑에 빠지고 금방 식어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100024521.1.15",
          "form": "그래서 결혼 못할 것 같다더군요 자기도 ㅎ",
          "original_form": "그래서 결혼 못할 것 같다더군요 자기도 ㅎ",
          "speaker_id": "2",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100024521.1.16",
          "form": "완전 상극이네요 그렇게 다를 수 있군요",
          "original_form": "완전 상극이네요 그렇게 다를 수 있군요",
          "speaker_id": "1",
          "time": "20210831 13:26"
        },
        {
          "id": "MDRW2100024521.1.17",
          "form": "그쵸ㅋㅋ 서로 이해 못해요",
          "original_form": "그쵸ㅋㅋ 서로 이해 못해요",
          "speaker_id": "2",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100024521.1.18",
          "form": "공통점은 쨌든 깊은 연애감정을 못 느껴봤다는거",
          "original_form": "공통점은 쨌든 깊은 연애감정을 못 느껴봤다는거",
          "speaker_id": "2",
          "time": "20210831 13:34"
        },
        {
          "id": "MDRW2100024521.1.19",
          "form": "살다보면 인연이 있겠죠ㅋㅋ",
          "original_form": "살다보면 인연이 있겠죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100024521.1.20",
          "form": "언젠가 두분 다 바뀌실 거예요",
          "original_form": "언젠가 두분 다 바뀌실 거예요",
          "speaker_id": "1",
          "time": "20210831 14:10"
        },
        {
          "id": "MDRW2100024521.1.21",
          "form": "그러면 매우 신기하겠네요 ㅋㅋ",
          "original_form": "그러면 매우 신기하겠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:08"
        }
      ]
