      "utterance": [
        {
          "id": "MDRW2100011282.1.1",
          "form": "어떤거드시나요?",
          "original_form": "어떤거드시나요?",
          "speaker_id": "1",
          "time": "20210824 10:59"
        },
        {
          "id": "MDRW2100011282.1.2",
          "form": "저는 종비 먹어요ㅋㅋㅋ",
          "original_form": "저는 종비 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:59"
        },
        {
          "id": "MDRW2100011282.1.3",
          "form": "저는 지금 유산균이랑철분만요ㅎ",
          "original_form": "저는 지금 유산균이랑철분만요ㅎ",
          "speaker_id": "2",
          "time": "20210824 11:00"
        },
        {
          "id": "MDRW2100011282.1.4",
          "form": "유산균 어디꺼드세요?",
          "original_form": "유산균 어디꺼드세요?",
          "speaker_id": "1",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100011282.1.5",
          "form": "저는 아이허브서 직구요",
          "original_form": "저는 아이허브서 직구요",
          "speaker_id": "2",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100011282.1.6",
          "form": "좋네요ㅋㅋㅋ 저는 가루유산균먹다가",
          "original_form": "좋네요ㅋㅋㅋ 저는 가루유산균먹다가",
          "speaker_id": "1",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100011282.1.7",
          "form": "요즘은 안먹고있네요",
          "original_form": "요즘은 안먹고있네요",
          "speaker_id": "1",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100011282.1.8",
          "form": "가루가 먹기는 편하죠",
          "original_form": "가루가 먹기는 편하죠",
          "speaker_id": "2",
          "time": "20210824 11:25"
        },
        {
          "id": "MDRW2100011282.1.9",
          "form": "부모님은 락토핏드세여ㅋ",
          "original_form": "부모님은 락토핏드세여ㅋ",
          "speaker_id": "2",
          "time": "20210824 11:25"
        },
        {
          "id": "MDRW2100011282.1.10",
          "form": "저도 락토핏먹었었어요ㅋㅋㅋ",
          "original_form": "저도 락토핏먹었었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100011282.1.11",
          "form": "맛이 아주달더라고요 영양제아닌것처럼",
          "original_form": "맛이 아주달더라고요 영양제아닌것처럼",
          "speaker_id": "1",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100011282.1.12",
          "form": "맞아요 그래서 거부감없죠",
          "original_form": "맞아요 그래서 거부감없죠",
          "speaker_id": "2",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100011282.1.13",
          "form": "점심은뭐드세요?",
          "original_form": "점심은뭐드세요?",
          "speaker_id": "2",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100011282.1.14",
          "form": "전 짜파게티요ㅋㅋㅋ",
          "original_form": "전 짜파게티요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:39"
        },
        {
          "id": "MDRW2100011282.1.15",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210824 11:39"
        },
        {
          "id": "MDRW2100011282.1.16",
          "form": "전 아직 못정햇어요ㅎ",
          "original_form": "전 아직 못정햇어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 11:44"
        },
        {
          "id": "MDRW2100011282.1.17",
          "form": "아침에 코코볼ㅎㅎ",
          "original_form": "아침에 코코볼ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:45"
        },
        {
          "id": "MDRW2100011282.1.18",
          "form": "코코볼 좋네요ㅋㅋㅋ",
          "original_form": "코코볼 좋네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:52"
        },
        {
          "id": "MDRW2100011282.1.19",
          "form": "갑자기 간짜장도 먹고싶네요",
          "original_form": "갑자기 간짜장도 먹고싶네요",
          "speaker_id": "1",
          "time": "20210824 11:52"
        },
        {
          "id": "MDRW2100011282.1.20",
          "form": "전 김밥이요",
          "original_form": "전 김밥이요",
          "speaker_id": "2",
          "time": "20210824 11:56"
        }
      ]
    }
