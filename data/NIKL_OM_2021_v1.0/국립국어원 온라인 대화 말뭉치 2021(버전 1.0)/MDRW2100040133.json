        },
        {
          "id": "MDRW2100040133.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100040133.1.3",
          "form": "결혼했지요",
          "original_form": "결혼했지요",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100040133.1.4",
          "form": "와 결혼하셨군요",
          "original_form": "와 결혼하셨군요",
          "speaker_id": "2",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100040133.1.5",
          "form": "결혼 생활 어때요",
          "original_form": "결혼 생활 어때요",
          "speaker_id": "2",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100040133.1.6",
          "form": "미혼이세요?",
          "original_form": "미혼이세요?",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100040133.1.7",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100040133.1.8",
          "form": "남편을잘만나야할듯",
          "original_form": "남편을잘만나야할듯",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100040133.1.9",
          "form": "연애중인데 왕 장거리에요",
          "original_form": "연애중인데 왕 장거리에요",
          "speaker_id": "2",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100040133.1.10",
          "form": "남친이 해외유학 중이에요ㅠㅠ",
          "original_form": "남친이 해외유학 중이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100040133.1.11",
          "form": "인생을 좌우하는 가장큰일~~",
          "original_form": "인생을 좌우하는 가장큰일~~",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100040133.1.12",
          "form": "자주못보겠네요",
          "original_form": "자주못보겠네요",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100040133.1.13",
          "form": "코로나라 더욱더",
          "original_form": "코로나라 더욱더",
          "speaker_id": "1",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100040133.1.14",
          "form": "구쵸ㅠㅠ 못봐요",
          "original_form": "구쵸ㅠㅠ 못봐요",
          "speaker_id": "2",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100040133.1.15",
          "form": "저 나이도 많은데 얘 기다리다가",
          "original_form": "저 나이도 많은데 얘 기다리다가",
          "speaker_id": "2",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100040133.1.16",
          "form": "노처녀 될거같아요",
          "original_form": "노처녀 될거같아요",
          "speaker_id": "2",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100040133.1.17",
          "form": "남친이 더 어린가봐요",
          "original_form": "남친이 더 어린가봐요",
          "speaker_id": "1",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100040133.1.18",
          "form": "때를 놓치면 힘들어요.여자들은",
          "original_form": "때를 놓치면 힘들어요.여자들은",
          "speaker_id": "1",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100040133.1.19",
          "form": "아뇨동갑인데",
          "original_form": "아뇨동갑인데",
          "speaker_id": "2",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100040133.1.20",
          "form": "아무래도 여자가 때가 더 즁요하잖아요ㅠㅠ",
          "original_form": "아무래도 여자가 때가 더 즁요하잖아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100040133.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100040133.1.22",
          "form": "그래서 걱정이에요",
