      "utterance": [
        {
          "id": "MDRW2100029890.1.1",
          "form": "오 제주도 사세요?",
          "original_form": "오 제주도 사세요?",
          "speaker_id": "1",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100029890.1.2",
          "form": "네 안녕하세여",
          "original_form": "네 안녕하세여",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100029890.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100029890.1.4",
          "form": "요즘 운동하구있는데 코로나라 하기가어려운거갘아요ㅠㅠ",
          "original_form": "요즘 운동하구있는데 코로나라 하기가어려운거갘아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100029890.1.5",
          "form": "저도 헬스장 끊었는데 안가고 있어요",
          "original_form": "저도 헬스장 끊었는데 안가고 있어요",
          "speaker_id": "2",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100029890.1.6",
          "form": "아진짜요? 예전에 제주도갔을때 제주도 종주했었는데",
          "original_form": "아진짜요? 예전에 제주도갔을때 제주도 종주했었는데",
          "speaker_id": "1",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100029890.1.7",
          "form": "엄청 건강해진거같았어요",
          "original_form": "엄청 건강해진거같았어요",
          "speaker_id": "1",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100029890.1.8",
          "form": "제주도 사시니까 필요성못느끼실수도있으시겠지만 한번 도전해보시는것도 나쁘지않을거같아요!",
          "original_form": "제주도 사시니까 필요성못느끼실수도있으시겠지만 한번 도전해보시는것도 나쁘지않을거같아요!",
          "speaker_id": "1",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100029890.1.9",
          "form": "여건이 된다면요 ㅎㅎ",
          "original_form": "여건이 된다면요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100029890.1.10",
          "form": "종주가 올레길을 다 걸으신거예요?",
          "original_form": "종주가 올레길을 다 걸으신거예요?",
          "speaker_id": "2",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100029890.1.11",
          "form": "아 넵 올레길 전부 다걸었어요",
          "original_form": "아 넵 올레길 전부 다걸었어요",
          "speaker_id": "1",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100029890.1.12",
          "form": "2016년도쯤에!",
          "original_form": "2016년도쯤에!",
          "speaker_id": "1",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100029890.1.13",
          "form": "오 가장 추천하는 코스 있어요? 다 걸으면 다이어트 되나요?",
          "original_form": "오 가장 추천하는 코스 있어요? 다 걸으면 다이어트 되나요?",
          "speaker_id": "2",
          "time": "20210903 16:25"
        },
        {
          "id": "MDRW2100029890.1.14",
          "form": "다걸으면 다이어트는 최고죠... 근데 아시다시피 맛있는게ㅜ너무많아서ㅠㅠ",
          "original_form": "다걸으면 다이어트는 최고죠... 근데 아시다시피 맛있는게ㅜ너무많아서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029890.1.15",
          "form": "쇠소깍 가는 그코스",
          "original_form": "쇠소깍 가는 그코스",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029890.1.16",
          "form": "5코스였을까요 정확히 기억은 나지않는데그쪽이",
          "original_form": "5코스였을까요 정확히 기억은 나지않는데그쪽이",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029890.1.17",
          "form": "이쁘고 좋았던거같아요",
          "original_form": "이쁘고 좋았던거같아요",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029890.1.18",
          "form": "쇠소깍이 추천이 많네요",
          "original_form": "쇠소깍이 추천이 많네요",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100029890.1.19",
          "form": "홈트는 힘들더라구요",
          "original_form": "홈트는 힘들더라구요",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100029890.1.20",
          "form": "맞아요 그쪽이 정말 이쁜것 같아요",
          "original_form": "맞아요 그쪽이 정말 이쁜것 같아요",
          "speaker_id": "1",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100029890.1.21",
          "form": "종주하실때 옷은 어떻게 입으셨어요?",
          "original_form": "종주하실때 옷은 어떻게 입으셨어요?",
          "speaker_id": "2",
          "time": "20210903 16:56"
        },
        {
          "id": "MDRW2100029890.1.22",
          "form": "되게 가볍게ㅜ가려고했었는데 날이너무 추웠어서",
          "original_form": "되게 가볍게ㅜ가려고했었는데 날이너무 추웠어서",
          "speaker_id": "1",
          "time": "20210903 17:08"
        }
      ]
    }
