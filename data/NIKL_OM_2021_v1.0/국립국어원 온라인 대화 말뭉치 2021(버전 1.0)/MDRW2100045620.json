      "utterance": [
        {
          "id": "MDRW2100045620.1.1",
          "form": "굴소스 넣어요!",
          "original_form": "굴소스 넣어요!",
          "speaker_id": "2",
          "time": "20210908 04:18"
        },
        {
          "id": "MDRW2100045620.1.2",
          "form": "중국요리는 대부분 간장이나 굴소스로 간을 하는것같아요",
          "original_form": "중국요리는 대부분 간장이나 굴소스로 간을 하는것같아요",
          "speaker_id": "2",
          "time": "20210908 04:18"
        },
        {
          "id": "MDRW2100045620.1.3",
          "form": "오 소금이 아니라 굴소스였군요",
          "original_form": "오 소금이 아니라 굴소스였군요",
          "speaker_id": "1",
          "time": "20210908 04:19"
        },
        {
          "id": "MDRW2100045620.1.4",
          "form": "볶음밥에 굴소스 넣으면 쫀맛인데",
          "original_form": "볶음밥에 굴소스 넣으면 쫀맛인데",
          "speaker_id": "1",
          "time": "20210908 04:19"
        },
        {
          "id": "MDRW2100045620.1.5",
          "form": "맞아요 굴소스는 정말 여기저기 다 넣기에 좋죠!!",
          "original_form": "맞아요 굴소스는 정말 여기저기 다 넣기에 좋죠!!",
          "speaker_id": "2",
          "time": "20210908 04:21"
        },
        {
          "id": "MDRW2100045620.1.6",
          "form": "애매하다 싶을때 굴소스 넣으면 되더라구요 ㅎㅎ",
          "original_form": "애매하다 싶을때 굴소스 넣으면 되더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 04:21"
        },
        {
          "id": "MDRW2100045620.1.7",
          "form": "맞아요 그리고 치킨스톡도 있으면 국물요리는 끝입니다",
          "original_form": "맞아요 그리고 치킨스톡도 있으면 국물요리는 끝입니다",
          "speaker_id": "1",
          "time": "20210908 04:23"
        },
        {
          "id": "MDRW2100045620.1.8",
          "form": "맞아요..ㅜㅜ",
          "original_form": "맞아요..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 04:24"
        },
        {
          "id": "MDRW2100045620.1.9",
          "form": "요리도 잘하시는분이셧군뇨..",
          "original_form": "요리도 잘하시는분이셧군뇨..",
          "speaker_id": "2",
          "time": "20210908 04:24"
        },
        {
          "id": "MDRW2100045620.1.10",
          "form": "예에? 그냥 템빨이죠",
          "original_form": "예에? 그냥 템빨이죠",
          "speaker_id": "1",
          "time": "20210908 04:24"
        },
        {
          "id": "MDRW2100045620.1.11",
          "form": "캐시템이라고 해도 될 정도인 무기아닌가용",
          "original_form": "캐시템이라고 해도 될 정도인 무기아닌가용",
          "speaker_id": "1",
          "time": "20210908 04:25"
        },
        {
          "id": "MDRW2100045620.1.12",
          "form": "ㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210908 04:25"
        },
        {
          "id": "MDRW2100045620.1.13",
          "form": "그치만 요리 못하는 사람은 그런것도 쓸줄몰라요",
          "original_form": "그치만 요리 못하는 사람은 그런것도 쓸줄몰라요",
          "speaker_id": "2",
          "time": "20210908 04:25"
        },
        {
          "id": "MDRW2100045620.1.14",
          "form": "ㅋㅋㅋㅋ 꼼수죵",
          "original_form": "ㅋㅋㅋㅋ 꼼수죵",
          "speaker_id": "1",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045620.1.15",
          "form": "name2님 안주무실거에요?",
          "original_form": "&name2&님 안주무실거에요?",
          "speaker_id": "1",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045620.1.16",
          "form": "저 지금 약간 졸려서",
          "original_form": "저 지금 약간 졸려서",
          "speaker_id": "2",
          "time": "20210908 04:29"
        },
        {
          "id": "MDRW2100045620.1.17",
          "form": "누워있습니다...ㅡ.",
          "original_form": "누워있습니다...ㅡ.",
          "speaker_id": "2",
          "time": "20210908 04:29"
        },
        {
          "id": "MDRW2100045620.1.18",
          "form": "아가부터 누워계시자나여",
          "original_form": "아가부터 누워계시자나여",
          "speaker_id": "1",
          "time": "20210908 04:29"
        },
        {
          "id": "MDRW2100045620.1.19",
          "form": "에이 아니에요 진짜 방금누움",
          "original_form": "에이 아니에요 진짜 방금누움",
          "speaker_id": "2",
          "time": "20210908 04:30"
        },
        {
          "id": "MDRW2100045620.1.20",
          "form": "진짜요? 여태 앉아계셨어요? 그게 더신기한데요",
          "original_form": "진짜요? 여태 앉아계셨어요? 그게 더신기한데요",
          "speaker_id": "1",
          "time": "20210908 04:30"
        },
        {
          "id": "MDRW2100045620.1.21",
          "form": "아놔 ㅡㅡ",
          "original_form": "아놔 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045620.1.22",
          "form": "저를 어떻게 생각하고 있으신거에요!",
          "original_form": "저를 어떻게 생각하고 있으신거에요!",
          "speaker_id": "2",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045620.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045620.1.24",
          "form": "저는 계속 누워있었거든요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 계속 누워있었거든요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
