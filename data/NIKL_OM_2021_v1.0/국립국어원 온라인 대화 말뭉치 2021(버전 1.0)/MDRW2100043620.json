      },
      "utterance": [
        {
          "id": "MDRW2100043620.1.1",
          "form": "ㅋㅋㅋㅋㅋ피피엘에 당해버렸다...",
          "original_form": "ㅋㅋㅋㅋㅋ피피엘에 당해버렸다...",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043620.1.2",
          "form": "이번편에는 점심미션에서",
          "original_form": "이번편에는 점심미션에서",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043620.1.3",
          "form": "피피엘 못하는 사람들만 이겨가지고 망했는데",
          "original_form": "피피엘 못하는 사람들만 이겨가지고 망했는데",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043620.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ피피엘 벌칙이냐고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ피피엘 벌칙이냐고",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043620.1.5",
          "form": "이긴사람들이 피피엘 치킨 먹었는데",
          "original_form": "이긴사람들이 피피엘 치킨 먹었는데",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043620.1.6",
          "form": "못하고 먹기만 함ㅋㅋㅋ",
          "original_form": "못하고 먹기만 함ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043620.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ무슨 치킨인데?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ무슨 치킨인데?",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043620.1.8",
          "form": "육십계 치킨 신메뉴",
          "original_form": "육십계 치킨 신메뉴",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043620.1.9",
          "form": "호랑이치킨인가? 그거",
          "original_form": "호랑이치킨인가? 그거",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043620.1.10",
          "form": "호랑이치킨이 머야",
          "original_form": "호랑이치킨이 머야",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043620.1.11",
          "form": "첨들어",
          "original_form": "첨들어",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043620.1.12",
          "form": "신메뉴 이름이 그거였어",
          "original_form": "신메뉴 이름이 그거였어",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043620.1.13",
          "form": "광고도 호랑이 나오고 그랬음",
          "original_form": "광고도 호랑이 나오고 그랬음",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043620.1.14",
          "form": "호랑이랑 치킨이 무슨 관계여",
          "original_form": "호랑이랑 치킨이 무슨 관계여",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043620.1.15",
          "form": "모르겠음",
          "original_form": "모르겠음",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043620.1.16",
          "form": "광고에 호랑이 나오고 치킨 나온다음에 끝나",
          "original_form": "광고에 호랑이 나오고 치킨 나온다음에 끝나",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043620.1.17",
          "form": "무슨맛인지 모르겠음ㅋㅋㅋ",
          "original_form": "무슨맛인지 모르겠음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043620.1.18",
          "form": "극한의 컨셉광고ㅋㅋㅋㅋㅋ",
          "original_form": "극한의 컨셉광고ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043620.1.19",
          "form": "육십계 별로였어가지고 안 먹을건데",
          "original_form": "육십계 별로였어가지고 안 먹을건데",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043620.1.20",
          "form": "궁금하긴 하다",
          "original_form": "궁금하긴 하다",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043620.1.21",
          "form": "육십계 먼가 매장도 없자너",
          "original_form": "육십계 먼가 매장도 없자너",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043620.1.22",
          "form": "그렇긴 함ㅋㅋ 마케팅만 잘하지...",
          "original_form": "그렇긴 함ㅋㅋ 마케팅만 잘하지...",
          "speaker_id": "2",
          "time": "20210907 20:08"
        }
      ]
