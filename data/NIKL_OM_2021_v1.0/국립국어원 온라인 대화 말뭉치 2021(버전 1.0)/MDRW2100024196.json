      "utterance": [
        {
          "id": "MDRW2100024196.1.1",
          "form": "최근에 산 상품은 ㅁ 엇인가요",
          "original_form": "최근에 산 상품은 ㅁ 엇인가요",
          "speaker_id": "1",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100024196.1.2",
          "form": "최근에는 옷이요",
          "original_form": "최근에는 옷이요",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024196.1.3",
          "form": "약간 충동구매로 사서 지금 살짝 후회되기도해요",
          "original_form": "약간 충동구매로 사서 지금 살짝 후회되기도해요",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024196.1.4",
          "form": "오 어떤 옷 사셨어요?",
          "original_form": "오 어떤 옷 사셨어요?",
          "speaker_id": "1",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100024196.1.5",
          "form": "그",
          "original_form": "그",
          "speaker_id": "2",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100024196.1.6",
          "form": "김연경선수 유니폼이요...",
          "original_form": "김연경선수 유니폼이요...",
          "speaker_id": "2",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100024196.1.7",
          "form": "한때 도쿄올임픽에 미쳤죠",
          "original_form": "한때 도쿄올임픽에 미쳤죠",
          "speaker_id": "2",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100024196.1.8",
          "form": "아 그거 알아요",
          "original_form": "아 그거 알아요",
          "speaker_id": "1",
          "time": "20210901 16:31"
        },
        {
          "id": "MDRW2100024196.1.9",
          "form": "저도 고민하다가...충동소비일것 같아서...안 샀어오..",
          "original_form": "저도 고민하다가...충동소비일것 같아서...안 샀어오..",
          "speaker_id": "1",
          "time": "20210901 16:31"
        },
        {
          "id": "MDRW2100024196.1.10",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100024196.1.11",
          "form": "충동소비가 맞았어요",
          "original_form": "충동소비가 맞았어요",
          "speaker_id": "2",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100024196.1.12",
          "form": "이제 선수가 국대은퇴해서 가치 있긴한데",
          "original_form": "이제 선수가 국대은퇴해서 가치 있긴한데",
          "speaker_id": "2",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100024196.1.13",
          "form": "그냥 소장할 생각이었는데",
          "original_form": "그냥 소장할 생각이었는데",
          "speaker_id": "2",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100024196.1.14",
          "form": "입고 운동하려구요",
          "original_form": "입고 운동하려구요",
          "speaker_id": "2",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100024196.1.15",
          "form": "오 그러게요 운동복으로 쓰면 그건 이제",
          "original_form": "오 그러게요 운동복으로 쓰면 그건 이제",
          "speaker_id": "1",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100024196.1.16",
          "form": "충동소비 아니에오!!",
          "original_form": "충동소비 아니에오!!",
          "speaker_id": "1",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100024196.1.17",
          "form": "충동소비가 되지 않기위해",
          "original_form": "충동소비가 되지 않기위해",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100024196.1.18",
          "form": "열심히 운동 해야겠어요!",
          "original_form": "열심히 운동 해야겠어요!",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100024196.1.19",
          "form": "내가 김연경선수다!",
          "original_form": "내가 김연경선수다!",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100024196.1.20",
          "form": "그럼요~!~!~ 전 요즘...제트플립이 너무 갖고싶어요",
          "original_form": "그럼요~!~!~ 전 요즘...제트플립이 너무 갖고싶어요",
          "speaker_id": "1",
          "time": "20210903 08:30"
        },
        {
          "id": "MDRW2100024196.1.21",
          "form": "제트플립",
          "original_form": "제트플립",
          "speaker_id": "2",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100024196.1.22",
          "form": "예쁘더라구요",
          "original_form": "예쁘더라구요",
          "speaker_id": "2",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100024196.1.23",
          "form": "삼성이 다른 제품 디자인은 버렸는데",
          "original_form": "삼성이 다른 제품 디자인은 버렸는데",
          "speaker_id": "2",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100024196.1.24",
          "form": "플립만 신경쓰나봐요!!!@",
          "original_form": "플립만 신경쓰나봐요!!!@",
          "speaker_id": "2",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100024196.1.25",
          "form": "라고 말할만큼 제트플립 예뻐요ㅋㅋㅋ",
          "original_form": "라고 말할만큼 제트플립 예뻐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:09"
        },
        {
          "id": "MDRW2100024196.1.26",
          "form": "아 그쵸!!! 아직은 바꿀 시기가 안돼서..조금만 더 기다렸다가 그때도 사고싶으면 사려구요",
          "original_form": "아 그쵸!!! 아직은 바꿀 시기가 안돼서..조금만 더 기다렸다가 그때도 사고싶으면 사려구요",
          "speaker_id": "1",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100024196.1.27",
          "form": "갤럭시 워치도ㅎㅎ..",
          "original_form": "갤럭시 워치도ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100024196.1.28",
          "form": "갤럭시워치",
          "original_form": "갤럭시워치",
          "speaker_id": "2",
          "time": "20210904 11:40"
        },
        {
          "id": "MDRW2100024196.1.29",
          "form": "다들 워치 많이 사더라구요",
          "original_form": "다들 워치 많이 사더라구요",
          "speaker_id": "2",
          "time": "20210904 11:40"
        },
        {
          "id": "MDRW2100024196.1.30",
          "form": "많은 제품이 디지털로 바뀌는 모습이 신기하기도하고",
          "original_form": "많은 제품이 디지털로 바뀌는 모습이 신기하기도하고",
          "speaker_id": "2",
          "time": "20210904 11:40"
        },
        {
          "id": "MDRW2100024196.1.31",
          "form": "흥미로워요ㅋㅋㅋ",
          "original_form": "흥미로워요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:40"
        },
        {
          "id": "MDRW2100024196.1.32",
          "form": "맞아요...점점 일상기능에 전자제품의 비중이 더 커지는 것 같아요",
          "original_form": "맞아요...점점 일상기능에 전자제품의 비중이 더 커지는 것 같아요",
          "speaker_id": "1",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100024196.1.33",
          "form": "그 갤럭시워치의 인바디기능이..너무 매력적이에요 제게",
          "original_form": "그 갤럭시워치의 인바디기능이..너무 매력적이에요 제게",
          "speaker_id": "1",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100024196.1.34",
          "form": "인바디기능",
          "original_form": "인바디기능",
          "speaker_id": "2",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100024196.1.35",
          "form": "운동하시는 분들이 좋아하더라구요",
          "original_form": "운동하시는 분들이 좋아하더라구요",
          "speaker_id": "2",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100024196.1.36",
          "form": "확실히 그런면에선 편해보이긴해요ㅋㅋㅋ",
          "original_form": "확실히 그런면에선 편해보이긴해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100024196.1.37",
          "form": "그쵸ㅋㅋㅋㅋ저도 요즘 운동 찍먹중이라 너무 갖고싶어요..그래도 참고..후기들 보고 내년에 사려구요",
          "original_form": "그쵸ㅋㅋㅋㅋ저도 요즘 운동 찍먹중이라 너무 갖고싶어요..그래도 참고..후기들 보고 내년에 사려구요",
          "speaker_id": "1",
          "time": "20210904 18:34"
        },
        {
          "id": "MDRW2100024196.1.38",
          "form": "오 계획을 잡으셨군요",
          "original_form": "오 계획을 잡으셨군요",
          "speaker_id": "2",
          "time": "20210904 21:03"
        },
        {
          "id": "MDRW2100024196.1.39",
          "form": "무사기 겟하실수있길 바랍니다",
          "original_form": "무사기 겟하실수있길 바랍니다",
          "speaker_id": "2",
          "time": "20210904 21:03"
        },
        {
          "id": "MDRW2100024196.1.40",
          "form": "그때까지 갖고싶으면 진짜 갖고싶은거일테니까 사려구요 허허",
