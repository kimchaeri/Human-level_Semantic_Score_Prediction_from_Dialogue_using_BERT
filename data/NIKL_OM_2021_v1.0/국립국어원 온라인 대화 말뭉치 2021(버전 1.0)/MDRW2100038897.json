      "utterance": [
        {
          "id": "MDRW2100038897.1.1",
          "form": "김지현은 백화점 코스메틱 매니저",
          "original_form": "김지현은 백화점 코스메틱 매니저",
          "speaker_id": "2",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038897.1.2",
          "form": "김지현도 얼굴이 헬갈리네요. 검색해보께요",
          "original_form": "김지현도 얼굴이 헬갈리네요. 검색해보께요",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038897.1.3",
          "form": "김지현 이분도 연극과 뮤지컬에서 알아준다는데",
          "original_form": "김지현 이분도 연극과 뮤지컬에서 알아준다는데",
          "speaker_id": "2",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100038897.1.4",
          "form": "처음보는 얼굴이네요",
          "original_form": "처음보는 얼굴이네요",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100038897.1.5",
          "form": "12부작인데 시즌제 할려고 빨리 끝내나",
          "original_form": "12부작인데 시즌제 할려고 빨리 끝내나",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100038897.1.6",
          "form": "이것도 시즌제ㅠ",
          "original_form": "이것도 시즌제ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100038897.1.7",
          "form": "편의점 샛별이에서 나왔데 김지현",
          "original_form": "편의점 샛별이에서 나왔데 김지현",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100038897.1.8",
          "form": "재미 없음 시즌제 안할듯",
          "original_form": "재미 없음 시즌제 안할듯",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100038897.1.9",
          "form": "제중원에서도 나오고",
          "original_form": "제중원에서도 나오고",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100038897.1.10",
          "form": "편의점 샛별이 봤는데 얼굴을 모르겠어요",
          "original_form": "편의점 샛별이 봤는데 얼굴을 모르겠어요",
          "speaker_id": "1",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100038897.1.11",
          "form": "일단 방송하고 재밌으면 시즌제로 갈려고 하는 추세",
          "original_form": "일단 방송하고 재밌으면 시즌제로 갈려고 하는 추세",
          "speaker_id": "2",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100038897.1.12",
          "form": "펜트하우스가 제일 심함",
          "original_form": "펜트하우스가 제일 심함",
          "speaker_id": "1",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100038897.1.13",
          "form": "그래 맞다",
          "original_form": "그래 맞다",
          "speaker_id": "2",
          "time": "20210902 22:46"
        }
