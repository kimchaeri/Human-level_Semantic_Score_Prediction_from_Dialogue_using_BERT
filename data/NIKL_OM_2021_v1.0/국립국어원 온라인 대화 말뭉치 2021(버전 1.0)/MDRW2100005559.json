      "utterance": [
        {
          "id": "MDRW2100005559.1.1",
          "form": "헤이 걸 자네도 '죠죠' 하지 않겠는가 (고고고고",
          "original_form": "헤이 걸 자네도 '죠죠' 하지 않겠는가 (고고고고",
          "speaker_id": "1",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005559.1.2",
          "form": "허나 거절한다!!!!",
          "original_form": "허나 거절한다!!!!",
          "speaker_id": "2",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005559.1.3",
          "form": "사실 죠죠 영업 쉽게 되도 이상한장르라",
          "original_form": "사실 죠죠 영업 쉽게 되도 이상한장르라",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005559.1.4",
          "form": "기대치는 높지 않다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "기대치는 높지 않다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005559.1.5",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005559.1.6",
          "form": "나 방금 넷플릭스로",
          "original_form": "나 방금 넷플릭스로",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005559.1.7",
          "form": "파프리카 봤는데",
          "original_form": "파프리카 봤는데",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005559.1.8",
          "form": "이것도 기묘함...",
          "original_form": "이것도 기묘함...",
          "speaker_id": "2",
          "time": "20210821 23:06"
        },
        {
          "id": "MDRW2100005559.1.9",
          "form": "나만해도 몇년전에 선배가 보자보자 할때 전혀 흔들리지않았는데",
          "original_form": "나만해도 몇년전에 선배가 보자보자 할때 전혀 흔들리지않았는데",
          "speaker_id": "1",
          "time": "20210821 23:06"
        },
        {
          "id": "MDRW2100005559.1.10",
          "form": "지금 스스로 퍼먹고있음 ....ㅋㅋㅋ",
          "original_form": "지금 스스로 퍼먹고있음 ....ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:06"
        },
        {
          "id": "MDRW2100005559.1.11",
          "form": "파프리카는 뭐냐",
          "original_form": "파프리카는 뭐냐",
          "speaker_id": "1",
          "time": "20210821 23:06"
        },
        {
          "id": "MDRW2100005559.1.12",
          "form": "아 나이거 영화리뷰에서 봤던것 같음",
          "original_form": "아 나이거 영화리뷰에서 봤던것 같음",
          "speaker_id": "1",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005559.1.13",
          "form": "인셉션이랑 비슷하대서 봤는데",
          "original_form": "인셉션이랑 비슷하대서 봤는데",
          "speaker_id": "2",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005559.1.14",
          "form": "인셉션보다 기묘..",
          "original_form": "인셉션보다 기묘..",
          "speaker_id": "2",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005559.1.15",
          "form": "근데 별 관심은안가드라",
          "original_form": "근데 별 관심은안가드라",
          "speaker_id": "1",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005559.1.16",
          "form": "이..이게뭐지?(두시간 동안 반복)",
          "original_form": "이..이게뭐지?(두시간 동안 반복)",
          "speaker_id": "2",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005559.1.17",
          "form": "야 파프리카 볼 시간에 죠죠 6편은 볼 수 있었겠다",
          "original_form": "야 파프리카 볼 시간에 죠죠 6편은 볼 수 있었겠다",
          "speaker_id": "1",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005559.1.18",
          "form": "1부 뚝딱",
          "original_form": "1부 뚝딱",
          "speaker_id": "1",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005559.1.19",
          "form": "아 개웃곀ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 개웃곀ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005559.1.20",
          "form": "1부 9편밖에 없음",
          "original_form": "1부 9편밖에 없음",
          "speaker_id": "1",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005559.1.21",
          "form": "죠죠로 계산되는 시간",
          "original_form": "죠죠로 계산되는 시간",
          "speaker_id": "2",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005559.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005559.1.23",
          "form": "두시간=9죠죠",
          "original_form": "두시간=9죠죠",
          "speaker_id": "2",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005559.1.24",
          "form": "1부 금방봐욧...",
          "original_form": "1부 금방봐욧...",
          "speaker_id": "1",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005559.1.25",
          "form": "와 오프닝엔딩 3분 빼면 딱 한편에 20분이네",
          "original_form": "와 오프닝엔딩 3분 빼면 딱 한편에 20분이네",
          "speaker_id": "1",
          "time": "20210821 23:09"
        },
        {
          "id": "MDRW2100005559.1.26",
          "form": "두시간=6죠죠",
          "original_form": "두시간=6죠죠",
          "speaker_id": "1",
          "time": "20210821 23:09"
        },
        {
          "id": "MDRW2100005559.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 늘었엌ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 늘었엌ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:09"
        },
        {
          "id": "MDRW2100005559.1.28",
          "form": "아 근데 이렇게 말한이유가",
          "original_form": "아 근데 이렇게 말한이유가",
          "speaker_id": "1",
          "time": "20210821 23:09"
        },
        {
          "id": "MDRW2100005559.1.29",
          "form": "1부는 좀 루즈해서 알아서 스킵하면서 보기때문이다",
          "original_form": "1부는 좀 루즈해서 알아서 스킵하면서 보기때문이다",
          "speaker_id": "1",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005559.1.30",
          "form": "이렇게 말하는 저도 아직 3부 다 못봄",
          "original_form": "이렇게 말하는 저도 아직 3부 다 못봄",
          "speaker_id": "1",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005559.1.31",
          "form": "그럼 다 본게 아니잖아요..",
          "original_form": "그럼 다 본게 아니잖아요..",
          "speaker_id": "2",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005559.1.32",
          "form": "3부는 좀 많다..",
          "original_form": "3부는 좀 많다..",
