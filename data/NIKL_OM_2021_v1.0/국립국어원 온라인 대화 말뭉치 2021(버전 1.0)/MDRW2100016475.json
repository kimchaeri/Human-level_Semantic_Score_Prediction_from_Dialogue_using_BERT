      "utterance": [
        {
          "id": "MDRW2100016475.1.1",
          "form": "어디나 완벽한 곳은 없군요",
          "original_form": "어디나 완벽한 곳은 없군요",
          "speaker_id": "2",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100016475.1.2",
          "form": "맞아요 ㅜ",
          "original_form": "맞아요 ㅜ",
          "speaker_id": "1",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100016475.1.3",
          "form": "이럴땐 유교사상 빡센 한국이 나아보이네요",
          "original_form": "이럴땐 유교사상 빡센 한국이 나아보이네요",
          "speaker_id": "2",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100016475.1.4",
          "form": "여기도 만만치는 않지만요",
          "original_form": "여기도 만만치는 않지만요",
          "speaker_id": "2",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100016475.1.5",
          "form": "ㄹㅇ 적의 적은 동지에요",
          "original_form": "ㄹㅇ 적의 적은 동지에요",
          "speaker_id": "1",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100016475.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100016475.1.7",
          "form": "프랑스랑 독일 국경 닿아잇어서주말마다",
          "original_form": "프랑스랑 독일 국경 닿아잇어서주말마다",
          "speaker_id": "1",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100016475.1.8",
          "form": "프랑스 남자들이 독일로 성ㅇ매매 여행 온대요 ㅋㅋㅋ",
          "original_form": "프랑스 남자들이 독일로 성ㅇ매매 여행 온대요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100016475.1.9",
          "form": "와 역겨워욬ㅋㅋㅋㅋ",
          "original_form": "와 역겨워욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100016475.1.10",
          "form": "그래서 국경마을이 성ㅁ매매가 그렇게 활발하다고",
          "original_form": "그래서 국경마을이 성ㅁ매매가 그렇게 활발하다고",
          "speaker_id": "1",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100016475.1.11",
          "form": "역시 국적 안가리고 남들은",
          "original_form": "역시 국적 안가리고 남들은",
          "speaker_id": "2",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100016475.1.12",
          "form": "놀랍네요",
          "original_form": "놀랍네요",
          "speaker_id": "2",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100016475.1.13",
          "form": "깨끗한척하지만 본질은 같은 것 같아요",
          "original_form": "깨끗한척하지만 본질은 같은 것 같아요",
          "speaker_id": "2",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016475.1.14",
          "form": "비혼 결심 +1",
          "original_form": "비혼 결심 +1",
          "speaker_id": "1",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016475.1.15",
          "form": "맞아요 왜 그모양일까요",
          "original_form": "맞아요 왜 그모양일까요",
          "speaker_id": "1",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016475.1.16",
          "form": "저는 성ㅇ매매 돈아까워서라도 안할거같은데",
          "original_form": "저는 성ㅇ매매 돈아까워서라도 안할거같은데",
          "speaker_id": "1",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016475.1.17",
          "form": "쪽팔림이 없는것도 신기하고",
          "original_form": "쪽팔림이 없는것도 신기하고",
          "speaker_id": "1",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016475.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100016475.1.19",
          "form": "공공연하게 다 하고 다녀서그런지",
          "original_form": "공공연하게 다 하고 다녀서그런지",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100016475.1.20",
          "form": "그게 수치스럽지 않은걸까요",
          "original_form": "그게 수치스럽지 않은걸까요",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016475.1.21",
          "form": "단체로 돌았죠",
          "original_form": "단체로 돌았죠",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016475.1.22",
          "form": "단체로 돌아서 그런거군요 ㅋㅋㅋㅋ 그리고",
          "original_form": "단체로 돌아서 그런거군요 ㅋㅋㅋㅋ 그리고",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016475.1.23",
          "form": "인류의 가장 오래된 직업이 성ㅇ매매다",
          "original_form": "인류의 가장 오래된 직업이 성ㅇ매매다",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016475.1.24",
          "form": "이럴때도 빡쳐요",
          "original_form": "이럴때도 빡쳐요",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016475.1.25",
          "form": "어차피 성매매하는 여성들더 있고",
          "original_form": "어차피 성매매하는 여성들더 있고",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016475.1.26",
          "form": "아 진짜요",
          "original_form": "아 진짜요",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016475.1.27",
          "form": "자신들은 상품을 구매하는거다",
          "original_form": "자신들은 상품을 구매하는거다",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016475.1.28",
          "form": "이러는거 아닐까요?",
          "original_form": "이러는거 아닐까요?",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016475.1.29",
          "form": "맞아요 무슨 상품평 쓰듯이",
          "original_form": "맞아요 무슨 상품평 쓰듯이",
          "speaker_id": "1",
          "time": "20210826 13:07"
        }
      ]
