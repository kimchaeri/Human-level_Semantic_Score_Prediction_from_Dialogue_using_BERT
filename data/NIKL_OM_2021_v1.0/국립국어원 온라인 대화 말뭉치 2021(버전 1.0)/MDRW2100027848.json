      "utterance": [
        {
          "id": "MDRW2100027848.1.1",
          "form": "그나저나 복숭아면",
          "original_form": "그나저나 복숭아면",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027848.1.2",
          "form": "딱딱한거랑",
          "original_form": "딱딱한거랑",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027848.1.3",
          "form": "물렁한거 있잖아요",
          "original_form": "물렁한거 있잖아요",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027848.1.4",
          "form": "그 중에선...?",
          "original_form": "그 중에선...?",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027848.1.5",
          "form": "둘다좋지만",
          "original_form": "둘다좋지만",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027848.1.6",
          "form": "물복이요!",
          "original_form": "물복이요!",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027848.1.7",
          "form": "확실히",
          "original_form": "확실히",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100027848.1.8",
          "form": "물복이 딱복보다",
          "original_form": "물복이 딱복보다",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100027848.1.9",
          "form": "더 달다는",
          "original_form": "더 달다는",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100027848.1.10",
          "form": "느낌이 있긴 하죠",
          "original_form": "느낌이 있긴 하죠",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100027848.1.11",
          "form": "그러고보니 예전에는",
          "original_form": "그러고보니 예전에는",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027848.1.12",
          "form": "그래서 너믄 아쉬워요",
          "original_form": "그래서 너믄 아쉬워요",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027848.1.13",
          "form": "아 아쉬우신건...?",
          "original_form": "아 아쉬우신건...?",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027848.1.14",
          "form": "딱복이 더달면 좋을텐데요",
          "original_form": "딱복이 더달면 좋을텐데요",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027848.1.15",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027848.1.16",
          "form": "확실히 딱복이",
          "original_form": "확실히 딱복이",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027848.1.17",
          "form": "식감도 좋고 그래서",
          "original_form": "식감도 좋고 그래서",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027848.1.18",
          "form": "저도 좋아하는데",
          "original_form": "저도 좋아하는데",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027848.1.19",
          "form": "자두보단 약하다는 느낌이;;",
          "original_form": "자두보단 약하다는 느낌이;;",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027848.1.20",
          "form": "자두는 강력하죠ㅋㅋㅋ",
          "original_form": "자두는 강력하죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100027848.1.21",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100027848.1.22",
          "form": "아까 청포도도 그렇고",
          "original_form": "아까 청포도도 그렇고",
          "speaker_id": "2",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100027848.1.23",
          "form": "새삼 생각해보니 과일 선호에",
          "original_form": "새삼 생각해보니 과일 선호에",
          "speaker_id": "2",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100027848.1.24",
          "form": "사탕이 큰 비중을 차지하고 있네요",
          "original_form": "사탕이 큰 비중을 차지하고 있네요",
          "speaker_id": "2",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100027848.1.25",
          "form": "그런 의미에서 포도나 복숭아도",
          "original_form": "그런 의미에서 포도나 복숭아도",
          "speaker_id": "2",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027848.1.26",
          "form": "달달한게 최고니깐요ㅋㅋ",
          "original_form": "달달한게 최고니깐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027848.1.27",
          "form": "예예",
          "original_form": "예예",
          "speaker_id": "2",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027848.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027848.1.29",
          "form": "그런 의미에서",
          "original_form": "그런 의미에서",
          "speaker_id": "2",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027848.1.30",
          "form": "딸기도 좋아하긴 합니다",
          "original_form": "딸기도 좋아하긴 합니다",
          "speaker_id": "2",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027848.1.31",
          "form": "딸기는 겨울딸기가 최고죠",
          "original_form": "딸기는 겨울딸기가 최고죠",
          "speaker_id": "1",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027848.1.32",
          "form": "어...순간",
          "original_form": "어...순간",
          "speaker_id": "2",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027848.1.33",
          "form": "딸기철이 언제지?",
          "original_form": "딸기철이 언제지?",
          "speaker_id": "2",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027848.1.34",
          "form": "했네요",
          "original_form": "했네요",
          "speaker_id": "2",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027848.1.35",
          "form": "겨울에 맛있게 먹은 기억이 있긴 있는데",
          "original_form": "겨울에 맛있게 먹은 기억이 있긴 있는데",
          "speaker_id": "2",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027848.1.36",
          "form": "어? 했네요",
          "original_form": "어? 했네요",
          "speaker_id": "2",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027848.1.37",
          "form": "거의 아무때나 다먹을수있죠ㅋㅋㅋ",
          "original_form": "거의 아무때나 다먹을수있죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100027848.1.38",
          "form": "겨울이 제일 달달하지만요",
          "original_form": "겨울이 제일 달달하지만요",
          "speaker_id": "1",
          "time": "20210902 15:57"
        }
      ]
    }
