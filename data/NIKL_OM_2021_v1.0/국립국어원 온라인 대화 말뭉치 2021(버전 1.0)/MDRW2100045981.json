      "utterance": [
        {
          "id": "MDRW2100045981.1.1",
          "form": "우도환 나오지않나요??",
          "original_form": "우도환 나오지않나요??",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045981.1.2",
          "form": "그 구교환이요!!!!",
          "original_form": "그 구교환이요!!!!",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045981.1.3",
          "form": "정해인이랑 ㅋㅋㅋㅋ",
          "original_form": "정해인이랑 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045981.1.4",
          "form": "아하!!!",
          "original_form": "아하!!!",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045981.1.5",
          "form": "정해인이구나",
          "original_form": "정해인이구나",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045981.1.6",
          "form": "얼굴이다했다하더라구욬ㅋㅋㅋ",
          "original_form": "얼굴이다했다하더라구욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045981.1.7",
          "form": "네네 구교환 매력 쩔두라구욬ㅋㅋ",
          "original_form": "네네 구교환 매력 쩔두라구욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045981.1.8",
          "form": "드라마 보면 빠지게 되요 ㅋㅋ",
          "original_form": "드라마 보면 빠지게 되요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045981.1.9",
          "form": "맞아요 걔가 캐리햿다하더라규요",
          "original_form": "맞아요 걔가 캐리햿다하더라규요",
          "speaker_id": "2",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045981.1.10",
          "form": "그 좀비 나오는 영화 아세요?",
          "original_form": "그 좀비 나오는 영화 아세요?",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045981.1.11",
          "form": "강동원 나오는",
          "original_form": "강동원 나오는",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045981.1.12",
          "form": "아니요 ㅠㅠㅠ 저좀비 무서워해서",
          "original_form": "아니요 ㅠㅠㅠ 저좀비 무서워해서",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
