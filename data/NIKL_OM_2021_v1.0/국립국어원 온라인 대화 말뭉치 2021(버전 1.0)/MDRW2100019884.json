      "utterance": [
        {
          "id": "MDRW2100019884.1.1",
          "form": "야식 드시나요?",
          "original_form": "야식 드시나요?",
          "speaker_id": "1",
          "time": "20210827 22:39"
        },
        {
          "id": "MDRW2100019884.1.2",
          "form": "앗 애가 소세지 먹자해서",
          "original_form": "앗 애가 소세지 먹자해서",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019884.1.3",
          "form": "소세지를 주고오다보니\r\n대화가 바뀌었습니다",
          "original_form": "소세지를 주고오다보니\r\n대화가 바뀌었습니다",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019884.1.4",
          "form": "오~~소세지 진리죠",
          "original_form": "오~~소세지 진리죠",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019884.1.5",
          "form": "아이 몇개주고 마늘도 같이 구워서 ...",
          "original_form": "아이 몇개주고 마늘도 같이 구워서 ...",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019884.1.6",
          "form": "아 맥주안주가 되겠네요",
          "original_form": "아 맥주안주가 되겠네요",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019884.1.7",
          "form": "이야 마늘 소세지는 부럽네요",
          "original_form": "이야 마늘 소세지는 부럽네요",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019884.1.8",
          "form": "역시 소세지는 케찹에 볶는거보다",
          "original_form": "역시 소세지는 케찹에 볶는거보다",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019884.1.9",
          "form": "그냥 구워서 머스타드랑 먹는게 최고같습니다",
          "original_form": "그냥 구워서 머스타드랑 먹는게 최고같습니다",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019884.1.10",
          "form": "군침도는군요",
          "original_form": "군침도는군요",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019884.1.11",
          "form": "하하 시간이 늦었는데",
          "original_form": "하하 시간이 늦었는데",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019884.1.12",
          "form": "지금 이렇게 먹으면...안될텐데",
          "original_form": "지금 이렇게 먹으면...안될텐데",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019884.1.13",
          "form": "꼭 밤만 되면..",
          "original_form": "꼭 밤만 되면..",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019884.1.14",
          "form": "소세지가 비앤나??",
          "original_form": "소세지가 비앤나??",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019884.1.15",
          "form": "먹네요..",
          "original_form": "먹네요..",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019884.1.16",
          "form": "오 아니요 비엔나 스타일인데",
          "original_form": "오 아니요 비엔나 스타일인데",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019884.1.17",
          "form": "마트에 수제소세지처럼",
          "original_form": "마트에 수제소세지처럼",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019884.1.18",
          "form": "조금 두툼하고 고기함량이 높아요",
          "original_form": "조금 두툼하고 고기함량이 높아요",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019884.1.19",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019884.1.20",
          "form": "잉(?) 그런게있나요(?(",
          "original_form": "잉(?) 그런게있나요(?(",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019884.1.21",
          "form": "그릴 비엔나였나",
          "original_form": "그릴 비엔나였나",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019884.1.22",
          "form": "통 그릴비엔나..?",
          "original_form": "통 그릴비엔나..?",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019884.1.23",
          "form": "비싸보여요",
          "original_form": "비싸보여요",
          "speaker_id": "1",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019884.1.24",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210827 22:58"
        }
      ]
