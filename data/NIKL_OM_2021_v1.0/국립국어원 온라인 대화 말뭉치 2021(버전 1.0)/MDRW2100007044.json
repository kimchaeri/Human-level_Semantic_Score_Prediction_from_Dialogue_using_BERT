      "utterance": [
        {
          "id": "MDRW2100007044.1.1",
          "form": "대구??",
          "original_form": "대구??",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007044.1.2",
          "form": "맞아요 address 제니스에 사시더라구요",
          "original_form": "맞아요 &address& 제니스에 사시더라구요",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007044.1.3",
          "form": "에궁 address 맛집 많지요",
          "original_form": "에궁 &address& 맛집 많지요",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007044.1.4",
          "form": "호식이 치킨 처음 나왓을때 혁명 이어쬬",
          "original_form": "호식이 치킨 처음 나왓을때 혁명 이어쬬",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007044.1.5",
          "form": "돈 많이 버셔서 좋은데 사시네요 ㅋㅋㅋ",
          "original_form": "돈 많이 버셔서 좋은데 사시네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007044.1.6",
          "form": "ㅋㅋㅋ할머니들이 특히 손주들오면",
          "original_form": "ㅋㅋㅋ할머니들이 특히 손주들오면",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007044.1.7",
          "form": "많이들 먹였던거같아요 따스한기억ㅎ",
          "original_form": "많이들 먹였던거같아요 따스한기억ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007044.1.8",
          "form": "그분은입건되셨지만ㅠ",
          "original_form": "그분은입건되셨지만ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007044.1.9",
          "form": "양 많고 맛있고 ㅋㅋㅋㅋ",
          "original_form": "양 많고 맛있고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007044.1.10",
          "form": "아 ㅋㅋㅋㅋㅋㅋ 이런",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋ 이런",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007044.1.11",
          "form": "어렴풋이 기억 나네요 ㅋㅋㅋ",
          "original_form": "어렴풋이 기억 나네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007044.1.12",
          "form": "호식이는 근데",
          "original_form": "호식이는 근데",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007044.1.13",
          "form": "양념은 진짜 아니에요",
          "original_form": "양념은 진짜 아니에요",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007044.1.14",
          "form": "저는 두마리 치킨은 티바가 맛있더라구요",
          "original_form": "저는 두마리 치킨은 티바가 맛있더라구요",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007044.1.15",
          "form": "간장이죠",
          "original_form": "간장이죠",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007044.1.16",
          "form": "오 저는 티바는 한번두안먹어봤어요",
          "original_form": "오 저는 티바는 한번두안먹어봤어요",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007044.1.17",
          "form": "양념은 티바가 진리입니다",
          "original_form": "양념은 티바가 진리입니다",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007044.1.18",
          "form": "근데 그것도 매장마다 다른지 맛이 살짝 달라요 장난질은 하시는지",
          "original_form": "근데 그것도 매장마다 다른지 맛이 살짝 달라요 장난질은 하시는지",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007044.1.19",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007044.1.20",
          "form": "근데 name1님",
