      "utterance": [
        {
          "id": "MDRW2100029679.1.1",
          "form": "점심 뭐드실거에요?",
          "original_form": "점심 뭐드실거에요?",
          "speaker_id": "2",
          "time": "20210903 11:38"
        },
        {
          "id": "MDRW2100029679.1.2",
          "form": "오늘도 쉬는김에 회와 소주를 마시려 합니다",
          "original_form": "오늘도 쉬는김에 회와 소주를 마시려 합니다",
          "speaker_id": "1",
          "time": "20210903 11:39"
        },
        {
          "id": "MDRW2100029679.1.3",
          "form": "ㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210903 11:39"
        },
        {
          "id": "MDRW2100029679.1.4",
          "form": "백신 맞고 멀쩡하니",
          "original_form": "백신 맞고 멀쩡하니",
          "speaker_id": "1",
          "time": "20210903 11:39"
        },
        {
          "id": "MDRW2100029679.1.5",
          "form": "회쏘는",
          "original_form": "회쏘는",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029679.1.6",
          "form": "오늘은 괜찮겠쥬?",
          "original_form": "오늘은 괜찮겠쥬?",
          "speaker_id": "1",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029679.1.7",
          "form": "진리입니다",
          "original_form": "진리입니다",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029679.1.8",
          "form": "아주 훌륭하신 인품을 가지셨습니다",
          "original_form": "아주 훌륭하신 인품을 가지셨습니다",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029679.1.9",
          "form": "회쏘!",
          "original_form": "회쏘!",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029679.1.10",
          "form": "ㅎㅎ 안목이 높으시군요ㅎㅎ",
          "original_form": "ㅎㅎ 안목이 높으시군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029679.1.11",
          "form": "제가 좀 그렇습니다 ㅋㅋ",
          "original_form": "제가 좀 그렇습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029679.1.12",
          "form": "저 사실 소주 안 좋아하는데",
          "original_form": "저 사실 소주 안 좋아하는데",
          "speaker_id": "1",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029679.1.13",
          "form": "오 진짜요?",
          "original_form": "오 진짜요?",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029679.1.14",
          "form": "소주를 안좋아하시다니",
          "original_form": "소주를 안좋아하시다니",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029679.1.15",
          "form": "회에 맥주는 용납 못하겠어요ㅋㅋ",
          "original_form": "회에 맥주는 용납 못하겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029679.1.16",
          "form": "ㅋㅋㅋㅋㅋ 맞습니다",
          "original_form": "ㅋㅋㅋㅋㅋ 맞습니다",
          "speaker_id": "2",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029679.1.17",
          "form": "소주 향도 맛도 시름ㅜ",
          "original_form": "소주 향도 맛도 시름ㅜ",
          "speaker_id": "1",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029679.1.18",
          "form": "와인 이런거 어울리실것 같습니다",
          "original_form": "와인 이런거 어울리실것 같습니다",
          "speaker_id": "2",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029679.1.19",
          "form": "아니먄 헤네시 같은것도",
          "original_form": "아니먄 헤네시 같은것도",
          "speaker_id": "2",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029679.1.20",
          "form": "근데 회, 특히 멍게에 소주는 최고 조합이쥬",
          "original_form": "근데 회, 특히 멍게에 소주는 최고 조합이쥬",
          "speaker_id": "1",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029679.1.21",
          "form": "아니 이 부르주아가  무슨 말이람",
          "original_form": "아니 이 부르주아가  무슨 말이람",
          "speaker_id": "1",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029679.1.22",
          "form": "정말 눈높이 안맞네요ㅉㅉㅉ",
          "original_form": "정말 눈높이 안맞네요ㅉㅉㅉ",
          "speaker_id": "1",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029679.1.23",
          "form": "그래서 님은 우아하게 와인에 하몽과 치즈를 곁들여 드시나요!!",
          "original_form": "그래서 님은 우아하게 와인에 하몽과 치즈를 곁들여 드시나요!!",
          "speaker_id": "1",
          "time": "20210903 11:43"
        }
      ]
