      "utterance": [
        {
          "id": "MDRW2100009659.1.1",
          "form": "오마이스? 인걸로 알고있어요",
          "original_form": "오마이스? 인걸로 알고있어요",
          "speaker_id": "2",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009659.1.2",
          "form": "헉 이름도 잇었ㄱ나",
          "original_form": "헉 이름도 잇었ㄱ나",
          "speaker_id": "1",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009659.1.3",
          "form": "조용히 끝났으면 좋겠네요ㅠㅠㅠ",
          "original_form": "조용히 끝났으면 좋겠네요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009659.1.4",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210823 02:40"
        },
        {
          "id": "MDRW2100009659.1.5",
          "form": "근데 부산쪽이 항상 영향을 많이 받아서..ㅋㅋㅋㅋㅋ",
          "original_form": "근데 부산쪽이 항상 영향을 많이 받아서..ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:41"
        },
        {
          "id": "MDRW2100009659.1.6",
          "form": "전 사실 조용할 거라는 기대를 거의 안하는.. 편입니다",
          "original_form": "전 사실 조용할 거라는 기대를 거의 안하는.. 편입니다",
          "speaker_id": "2",
          "time": "20210823 02:41"
        },
        {
          "id": "MDRW2100009659.1.7",
          "form": "그런가요 ㅠㅠ 저는 이제 산지 2년되서 몰랐네요",
          "original_form": "그런가요 ㅠㅠ 저는 이제 산지 2년되서 몰랐네요",
          "speaker_id": "1",
          "time": "20210823 02:42"
        },
        {
          "id": "MDRW2100009659.1.8",
          "form": "월화수는 나가지 말아야 겠어요!",
          "original_form": "월화수는 나가지 말아야 겠어요!",
          "speaker_id": "1",
          "time": "20210823 02:42"
        },
        {
          "id": "MDRW2100009659.1.9",
          "form": "아아 오신 지 얼마 안되셨군요..",
          "original_form": "아아 오신 지 얼마 안되셨군요..",
          "speaker_id": "2",
          "time": "20210823 02:42"
        },
        {
          "id": "MDRW2100009659.1.10",
          "form": "그게 제일 좋습니다",
          "original_form": "그게 제일 좋습니다",
          "speaker_id": "2",
          "time": "20210823 02:42"
        },
        {
          "id": "MDRW2100009659.1.11",
          "form": "진짜 집에있는게 제일 나아요..",
          "original_form": "진짜 집에있는게 제일 나아요..",
          "speaker_id": "2",
          "time": "20210823 02:42"
        },
        {
          "id": "MDRW2100009659.1.12",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ 배달도 못시키겠네요 ㅠㅠㅠ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ 배달도 못시키겠네요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 02:42"
        },
        {
          "id": "MDRW2100009659.1.13",
          "form": "경남 사세요?",
          "original_form": "경남 사세요?",
          "speaker_id": "1",
          "time": "20210823 02:42"
        },
        {
          "id": "MDRW2100009659.1.14",
          "form": "넵 저 경남살아요",
          "original_form": "넵 저 경남살아요",
          "speaker_id": "2",
          "time": "20210823 02:43"
        },
        {
          "id": "MDRW2100009659.1.15",
          "form": "배달.. 미안해서 아마 못시킬것 같아요..ㅋㅋㅋ",
          "original_form": "배달.. 미안해서 아마 못시킬것 같아요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:43"
        },
        {
          "id": "MDRW2100009659.1.16",
          "form": "그렇군요 저 이번주 수요일에 거제도 놀러가요 ㅎㅎ",
          "original_form": "그렇군요 저 이번주 수요일에 거제도 놀러가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:43"
        },
        {
          "id": "MDRW2100009659.1.17",
          "form": "비 안오면 좋겠는데ㅠㅠㅠㅠ",
          "original_form": "비 안오면 좋겠는데ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 02:43"
        },
        {
          "id": "MDRW2100009659.1.18",
          "form": "헉 거제도.. 비 올 것 같은데..",
          "original_form": "헉 거제도.. 비 올 것 같은데..",
          "speaker_id": "2",
          "time": "20210823 02:45"
        },
        {
          "id": "MDRW2100009659.1.19",
          "form": "ㅠㅠㅠㅠ 숙소에서 갇혀있어야겠지ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ 숙소에서 갇혀있어야겠지ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 02:46"
        },
        {
          "id": "MDRW2100009659.1.20",
          "form": "비야 내리지 말아야~~~",
          "original_form": "비야 내리지 말아야~~~",
          "speaker_id": "1",
          "time": "20210823 02:47"
        },
        {
          "id": "MDRW2100009659.1.21",
          "form": "아마 강제호캉스를 즐기지 않을까 싶습니다..",
          "original_form": "아마 강제호캉스를 즐기지 않을까 싶습니다..",
          "speaker_id": "2",
          "time": "20210823 02:47"
        },
        {
          "id": "MDRW2100009659.1.22",
          "form": "호캉스로 쓰고 자가격리라고 읽습니다..",
          "original_form": "호캉스로 쓰고 자가격리라고 읽습니다..",
          "speaker_id": "1",
          "time": "20210823 02:48"
        },
        {
          "id": "MDRW2100009659.1.23",
          "form": "태풍 끝나면 추워지려나요?",
          "original_form": "태풍 끝나면 추워지려나요?",
          "speaker_id": "1",
          "time": "20210823 02:48"
        },
        {
          "id": "MDRW2100009659.1.24",
          "form": "막 그렇게 추워지진 않을걸요..?",
          "original_form": "막 그렇게 추워지진 않을걸요..?",
          "speaker_id": "2",
          "time": "20210823 02:48"
        },
        {
          "id": "MDRW2100009659.1.25",
          "form": "근데 그 전부터 밤 날씨가 선선해지고 있어서..",
          "original_form": "근데 그 전부터 밤 날씨가 선선해지고 있어서..",
          "speaker_id": "2",
          "time": "20210823 02:48"
        },
        {
          "id": "MDRW2100009659.1.26",
          "form": "비슷할 것 같아요",
          "original_form": "비슷할 것 같아요",
          "speaker_id": "2",
          "time": "20210823 02:49"
        }
      ]
