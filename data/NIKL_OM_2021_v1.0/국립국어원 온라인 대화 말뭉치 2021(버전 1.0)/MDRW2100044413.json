      "utterance": [
        {
          "id": "MDRW2100044413.1.1",
          "form": "많이 밀려서 해야 할 것 같아",
          "original_form": "많이 밀려서 해야 할 것 같아",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044413.1.2",
          "form": "뭐 뭐 할 거에요",
          "original_form": "뭐 뭐 할 거에요",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044413.1.3",
          "form": "일단 일반 빨래",
          "original_form": "일단 일반 빨래",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044413.1.4",
          "form": "수건은 저번에 다 빵 거죠",
          "original_form": "수건은 저번에 다 빵 거죠",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044413.1.5",
          "form": "수거는 다 빨았지",
          "original_form": "수거는 다 빨았지",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044413.1.6",
          "form": "이불 빨래는 언제 해요",
          "original_form": "이불 빨래는 언제 해요",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044413.1.7",
          "form": "이불은 계절 지날 때",
          "original_form": "이불은 계절 지날 때",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044413.1.8",
          "form": "이불 빨래는 그냥 세탁소에 맡기는 거 어때요",
          "original_form": "이불 빨래는 그냥 세탁소에 맡기는 거 어때요",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044413.1.9",
          "form": "그거 비쌀 걸",
          "original_form": "그거 비쌀 걸",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044413.1.10",
          "form": "이불 하나당 얼만데",
          "original_form": "이불 하나당 얼만데",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044413.1.11",
          "form": "만원 넘을 거야",
          "original_form": "만원 넘을 거야",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044413.1.12",
          "form": "에 이 그러면 나 같으면은 그냥 하겠다",
          "original_form": "에 이 그러면 나 같으면은 그냥 하겠다",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044413.1.13",
          "form": "너무 비싸 집에서 하면 되는데",
          "original_form": "너무 비싸 집에서 하면 되는데",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044413.1.14",
          "form": "집에서 하면 너무 힘들잖아",
          "original_form": "집에서 하면 너무 힘들잖아",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044413.1.15",
          "form": "하나하나 하면 돼",
          "original_form": "하나하나 하면 돼",
