      "utterance": [
        {
          "id": "MDRW2100017806.1.1",
          "form": "날씨가 뭔가 땡기게 만드네요ㅋㅋ",
          "original_form": "날씨가 뭔가 땡기게 만드네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:14"
        },
        {
          "id": "MDRW2100017806.1.2",
          "form": "ㅋㅋㅋㅋ비와서 칼국수 먹고싶어요",
          "original_form": "ㅋㅋㅋㅋ비와서 칼국수 먹고싶어요",
          "speaker_id": "1",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100017806.1.3",
          "form": "아 좋죠",
          "original_form": "아 좋죠",
          "speaker_id": "2",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100017806.1.4",
          "form": "무슨 칼국수?",
          "original_form": "무슨 칼국수?",
          "speaker_id": "2",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100017806.1.5",
          "form": "바지락칼국수...",
          "original_form": "바지락칼국수...",
          "speaker_id": "1",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100017806.1.6",
          "form": "정답!! ㅋㅋ",
          "original_form": "정답!! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100017806.1.7",
          "form": "저도 바지락최고",
          "original_form": "저도 바지락최고",
          "speaker_id": "2",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100017806.1.8",
          "form": "ㅋㅋㅋㅋ그거도 맛있어요",
          "original_form": "ㅋㅋㅋㅋ그거도 맛있어요",
          "speaker_id": "1",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100017806.1.9",
          "form": "샤브샤브먹고 칼국수",
          "original_form": "샤브샤브먹고 칼국수",
          "speaker_id": "1",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100017806.1.10",
          "form": "와 죽도 해야져ㅋㅋ",
          "original_form": "와 죽도 해야져ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100017806.1.11",
          "form": "완전",
          "original_form": "완전",
          "speaker_id": "1",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100017806.1.12",
          "form": "최고의 조합이죠",
          "original_form": "최고의 조합이죠",
          "speaker_id": "1",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100017806.1.13",
          "form": "여름이 갈듯 잘 안가네요. 평냉도 드시나요?",
          "original_form": "여름이 갈듯 잘 안가네요. 평냉도 드시나요?",
          "speaker_id": "2",
          "time": "20210830 14:01"
        },
        {
          "id": "MDRW2100017806.1.14",
          "form": "저 평양냉면 완전 좋아해요!!!",
          "original_form": "저 평양냉면 완전 좋아해요!!!",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100017806.1.15",
          "form": "평양냉면 맛집 아는곳 있어요?",
          "original_form": "평양냉면 맛집 아는곳 있어요?",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100017806.1.16",
          "form": "전 을밀대요ㅎㅎ",
          "original_form": "전 을밀대요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100017806.1.17",
          "form": "막 유명한 곳 줄서서 먹진 않아요ㅋㅋ",
          "original_form": "막 유명한 곳 줄서서 먹진 않아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:43"
        },
        {
          "id": "MDRW2100017806.1.18",
          "form": "오 을밀대 맛있죠",
          "original_form": "오 을밀대 맛있죠",
          "speaker_id": "1",
          "time": "20210830 18:44"
        },
        {
          "id": "MDRW2100017806.1.19",
          "form": "친구가 생크림빵맛난다그러던데ㅋㅋㅋ",
          "original_form": "친구가 생크림빵맛난다그러던데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:44"
        },
        {
          "id": "MDRW2100017806.1.20",
          "form": "ㅋㅋ 독특한 비유이시네요ㅋ",
          "original_form": "ㅋㅋ 독특한 비유이시네요ㅋ",
          "speaker_id": "2",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100017806.1.21",
          "form": "손 씻은 물이라고 많이 하는데ㅋ",
          "original_form": "손 씻은 물이라고 많이 하는데ㅋ",
          "speaker_id": "2",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100017806.1.22",
          "form": "ㅋㄱㅋㅋㄱㅋㄱ그쵸",
          "original_form": "ㅋㄱㅋㅋㄱㅋㄱ그쵸",
          "speaker_id": "1",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100017806.1.23",
          "form": "특이한 맛이 있는뎅",
          "original_form": "특이한 맛이 있는뎅",
          "speaker_id": "1",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100017806.1.24",
          "form": "을지면옥도 맛있어요!",
          "original_form": "을지면옥도 맛있어요!",
