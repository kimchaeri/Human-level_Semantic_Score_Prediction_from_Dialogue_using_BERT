      "utterance": [
        {
          "id": "MDRW2100023883.1.1",
          "form": "넵~~~~~~~~~라면 안좋아하는 사람이 있을까요ㅎㅎㅎ",
          "original_form": "넵~~~~~~~~~라면 안좋아하는 사람이 있을까요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100023883.1.2",
          "form": "ㅋㅋㅋㅋ 최근에",
          "original_form": "ㅋㅋㅋㅋ 최근에",
          "speaker_id": "1",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100023883.1.3",
          "form": "열라면 순두부찌개 맛있다고 유행하던데",
          "original_form": "열라면 순두부찌개 맛있다고 유행하던데",
          "speaker_id": "1",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100023883.1.4",
          "form": "들어보셨어요?",
          "original_form": "들어보셨어요?",
          "speaker_id": "1",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100023883.1.5",
          "form": "네 들어는 봤어요ㅋㅋㅋ\n먹어보진 못했지만ㅎ",
          "original_form": "네 들어는 봤어요ㅋㅋㅋ\n먹어보진 못했지만ㅎ",
          "speaker_id": "2",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100023883.1.6",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100023883.1.7",
          "form": "네네 누가 맛있다고해서",
          "original_form": "네네 누가 맛있다고해서",
          "speaker_id": "1",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100023883.1.8",
          "form": "막 중독이라고해서 먹어봤는데",
          "original_form": "막 중독이라고해서 먹어봤는데",
          "speaker_id": "1",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100023883.1.9",
          "form": "라면 한봉지 끓여먹은것보다 소화가 잘되고 맛있어요",
          "original_form": "라면 한봉지 끓여먹은것보다 소화가 잘되고 맛있어요",
          "speaker_id": "1",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100023883.1.10",
          "form": "오~",
          "original_form": "오~",
          "speaker_id": "2",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100023883.1.11",
          "form": "저도 먹어봐야겠어요ㅋㅋㅋㅋ",
          "original_form": "저도 먹어봐야겠어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100023883.1.12",
          "form": "꼭 드셔보세용",
          "original_form": "꼭 드셔보세용",
          "speaker_id": "1",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100023883.1.13",
          "form": "열라면이 라면중에서 좀 매운편이라",
          "original_form": "열라면이 라면중에서 좀 매운편이라",
          "speaker_id": "1",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100023883.1.14",
          "form": "다른 라면으로 대체하셔도 괜찮아요!",
          "original_form": "다른 라면으로 대체하셔도 괜찮아요!",
          "speaker_id": "1",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100023883.1.15",
          "form": "다른 라면도 괜찮군요~",
          "original_form": "다른 라면도 괜찮군요~",
          "speaker_id": "2",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100023883.1.16",
          "form": "저는 최근에 그 크림진짬뽕? 먹어봤는데 맛나더라구요",
          "original_form": "저는 최근에 그 크림진짬뽕? 먹어봤는데 맛나더라구요",
          "speaker_id": "2",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100023883.1.17",
          "form": "아 이거!ㅋㅋㅋㅋ",
          "original_form": "아 이거!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100023883.1.18",
          "form": "저도 컵라면으로 먹었는데",
          "original_form": "저도 컵라면으로 먹었는데",
          "speaker_id": "1",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100023883.1.19",
          "form": "양이 너무 적었어요ㅠㅠ",
          "original_form": "양이 너무 적었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100023883.1.20",
          "form": "맞아요ㅠ 양은 적더라구요",
          "original_form": "맞아요ㅠ 양은 적더라구요",
          "speaker_id": "2",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100023883.1.21",
          "form": "맛있긴 진짜 맛있었어요",
          "original_form": "맛있긴 진짜 맛있었어요",
          "speaker_id": "1",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100023883.1.22",
          "form": "살짝 달지만...?ㅎㅎ",
          "original_form": "살짝 달지만...?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100023883.1.23",
          "form": "봉지라면은 더 맛있다고 하더라구요",
          "original_form": "봉지라면은 더 맛있다고 하더라구요",
          "speaker_id": "2",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100023883.1.24",
          "form": "아 봉지가 더 맛있대요?",
          "original_form": "아 봉지가 더 맛있대요?",
          "speaker_id": "1",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100023883.1.25",
          "form": "한봉지사면 다 먹어야해서",
          "original_form": "한봉지사면 다 먹어야해서",
          "speaker_id": "1",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100023883.1.26",
          "form": "컵라면으로 하나만 먹어본거였거든요ㅋㅋㅋ",
          "original_form": "컵라면으로 하나만 먹어본거였거든요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100023883.1.27",
          "form": "네 아무래도 끓여먹는게 더 맛있나봐요",
          "original_form": "네 아무래도 끓여먹는게 더 맛있나봐요",
          "speaker_id": "2",
          "time": "20210830 19:48"
        }
      ]
    }
