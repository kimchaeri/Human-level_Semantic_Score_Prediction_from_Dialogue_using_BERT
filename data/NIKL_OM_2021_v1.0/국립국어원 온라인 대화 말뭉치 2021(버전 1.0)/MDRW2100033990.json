      "utterance": [
        {
          "id": "MDRW2100033990.1.1",
          "form": "그럼 어떤장르보시나요",
          "original_form": "그럼 어떤장르보시나요",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100033990.1.2",
          "form": "음 저 잔인하지 않은 선에서 액션도 좋아하고",
          "original_form": "음 저 잔인하지 않은 선에서 액션도 좋아하고",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100033990.1.3",
          "form": "스릴러도 좋아요 ㅋㅋㅋㅋㅋ",
          "original_form": "스릴러도 좋아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100033990.1.4",
          "form": "오오 잔인한거 나오잖아요 스릴러",
          "original_form": "오오 잔인한거 나오잖아요 스릴러",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100033990.1.5",
          "form": "맞아요...그런 스토리는 좋아하는데",
          "original_form": "맞아요...그런 스토리는 좋아하는데",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100033990.1.6",
          "form": "청불붙으면 그래서 못봐요 ㅎㅎ...",
          "original_form": "청불붙으면 그래서 못봐요 ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100033990.1.7",
          "form": "ㅋㅋㅋ빨리감기하면서  봐야조ㅎㅎ",
          "original_form": "ㅋㅋㅋ빨리감기하면서  봐야조ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100033990.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ큐ㅠ 근데 영화관가면 빨리감기 못하잖아욬ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ큐ㅠ 근데 영화관가면 빨리감기 못하잖아욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100033990.1.9",
          "form": "ㅋㅋㅋㄱ음 넷플에서...",
          "original_form": "ㅋㅋㅋㄱ음 넷플에서...",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100033990.1.10",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100033990.1.11",
          "form": "저 근데 영화관에서 유전은 봤었어요...",
          "original_form": "저 근데 영화관에서 유전은 봤었어요...",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100033990.1.12",
          "form": "제 의지로 보러간건 아니었지만...",
          "original_form": "제 의지로 보러간건 아니었지만...",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100033990.1.13",
          "form": "안돼",
          "original_form": "안돼",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100033990.1.14",
          "form": "친구손에 끌려가서 랑종같은거보면",
          "original_form": "친구손에 끌려가서 랑종같은거보면",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100033990.1.15",
          "form": "도망쳐야해요",
          "original_form": "도망쳐야해요",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100033990.1.16",
          "form": "랑종만큼은 진짜...",
          "original_form": "랑종만큼은 진짜...",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100033990.1.17",
          "form": "유전은 안봤는데 공포아닌가여..ㅜㅜㅠ",
          "original_form": "유전은 안봤는데 공포아닌가여..ㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100033990.1.18",
          "form": "안돼요...",
          "original_form": "안돼요...",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100033990.1.19",
          "form": "유전 공포맞고요...",
          "original_form": "유전 공포맞고요...",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100033990.1.20",
          "form": "저만 볼 수 없어서 주변에 그렇게 추천하고다니는데",
          "original_form": "저만 볼 수 없어서 주변에 그렇게 추천하고다니는데",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100033990.1.21",
          "form": "아무도 안봐줘요",
          "original_form": "아무도 안봐줘요",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100033990.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100033990.1.23",
          "form": "으음 다들현명하시네요",
          "original_form": "으음 다들현명하시네요",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100033990.1.24",
          "form": "ㅠㅠ...",
          "original_form": "ㅠㅠ...",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100033990.1.25",
          "form": "왓챠에 있던데..봐..봐야하나 그걸..ㅜㅜㅜ",
          "original_form": "왓챠에 있던데..봐..봐야하나 그걸..ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100033990.1.26",
          "form": "그치만... 배우들이...연기를 잘합니다...",
          "original_form": "그치만... 배우들이...연기를 잘합니다...",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100033990.1.27",
          "form": "솔직히 내용 자체는 안무서워요",
          "original_form": "솔직히 내용 자체는 안무서워요",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100033990.1.28",
          "form": "서양 공포라서 기승전 악마라",
          "original_form": "서양 공포라서 기승전 악마라",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100033990.1.29",
          "form": "갑자기 후반에 판타지돼요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "갑자기 후반에 판타지돼요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100033990.1.30",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100033990.1.31",
          "form": "근데 초중반 줄거리랑 배우들 연기가 무척 좋습니다...",
          "original_form": "근데 초중반 줄거리랑 배우들 연기가 무척 좋습니다...",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100033990.1.32",
          "form": "연기 중요하죠",
          "original_form": "연기 중요하죠",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100033990.1.33",
          "form": "저처럼 잔인한거 못보시는거 아니믄 보실만해요",
          "original_form": "저처럼 잔인한거 못보시는거 아니믄 보실만해요",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100033990.1.34",
          "form": "잔인한장면도 그렇게 많이 나오는건 아닌데",
          "original_form": "잔인한장면도 그렇게 많이 나오는건 아닌데",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100033990.1.35",
          "form": "아 서양의 공포는 동양과 다른거 같아요",
          "original_form": "아 서양의 공포는 동양과 다른거 같아요",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100033990.1.36",
          "form": "제기준이...",
          "original_form": "제기준이...",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100033990.1.37",
          "form": "개념이",
          "original_form": "개념이",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100033990.1.38",
          "form": "12세이용가 정도의 기준이라서...",
          "original_form": "12세이용가 정도의 기준이라서...",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100033990.1.39",
          "form": "제기준 잔인했고요...",
          "original_form": "제기준 잔인했고요...",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100033990.1.40",
          "form": "좋은거라고 생각합니다 디즈니도 얼마나 좋은데요",
          "original_form": "좋은거라고 생각합니다 디즈니도 얼마나 좋은데요",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100033990.1.41",
          "form": "디즈니 사랑해요",
          "original_form": "디즈니 사랑해요",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100033990.1.42",
          "form": "맞아요 디즈니 좋아요 ㅠㅠㅋㅋㅋ",
          "original_form": "맞아요 디즈니 좋아요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100033990.1.43",
          "form": "토이스토리 명작이라구욧",
          "original_form": "토이스토리 명작이라구욧",
          "speaker_id": "2",
