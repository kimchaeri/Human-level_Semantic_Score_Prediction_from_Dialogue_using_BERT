      "utterance": [
        {
          "id": "MDRW2100015051.1.1",
          "form": "아 오늘 2차 맞는 날이었구나",
          "original_form": "아 오늘 2차 맞는 날이었구나",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015051.1.2",
          "form": "안 아프대??",
          "original_form": "안 아프대??",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015051.1.3",
          "form": "응. 팔은 안 아프고 멀쩡하대.",
          "original_form": "응. 팔은 안 아프고 멀쩡하대.",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015051.1.4",
          "form": "안 아프면 다행이지~",
          "original_form": "안 아프면 다행이지~",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100015051.1.5",
          "form": "주사 맞고 며칠동안은 무리한 운동 하지 말라고 해.",
          "original_form": "주사 맞고 며칠동안은 무리한 운동 하지 말라고 해.",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100015051.1.6",
          "form": "안 그래도 그냥 누워 있어.",
          "original_form": "안 그래도 그냥 누워 있어.",
          "speaker_id": "1",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100015051.1.7",
          "form": "좀 피곤한가 봐.",
          "original_form": "좀 피곤한가 봐.",
          "speaker_id": "1",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100015051.1.8",
          "form": "내가 영양제 준 거 잘 챙기고~엄마도 같이 먹어!",
          "original_form": "내가 영양제 준 거 잘 챙기고~엄마도 같이 먹어!",
          "speaker_id": "2",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100015051.1.9",
          "form": "응 꾸준히 비타민이랑 루테인 먹고 있어.",
          "original_form": "응 꾸준히 비타민이랑 루테인 먹고 있어.",
          "speaker_id": "1",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100015051.1.10",
          "form": "할머니는 병원 언제 가셔?ㅡ",
          "original_form": "할머니는 병원 언제 가셔?ㅡ",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100015051.1.11",
          "form": "다음주. 이번에 mri 찍기로 했어.",
          "original_form": "다음주. 이번에 mri 찍기로 했어.",
          "speaker_id": "1",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100015051.1.12",
          "form": "그래 퇴원한지 1년 정도 됐으니까 찍을 때 됐지",
          "original_form": "그래 퇴원한지 1년 정도 됐으니까 찍을 때 됐지",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015051.1.13",
          "form": "요즘 너한테 아프시단 얘긴 안하셔?",
          "original_form": "요즘 너한테 아프시단 얘긴 안하셔?",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015051.1.14",
          "form": "가끔 머리 아프고 다리에 힘이 없다고 하시지",
          "original_form": "가끔 머리 아프고 다리에 힘이 없다고 하시지",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015051.1.15",
          "form": "뇌경색 증상이 다 그런 거라고 하긴 하는데",
          "original_form": "뇌경색 증상이 다 그런 거라고 하긴 하는데",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015051.1.16",
          "form": "그래도 걱정 되시나 보ㅓ",
          "original_form": "그래도 걱정 되시나 보ㅓ",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015051.1.17",
          "form": "응. 요새 약도 꼬박꼬박 드시고 계시니까 걱정 말아.",
          "original_form": "응. 요새 약도 꼬박꼬박 드시고 계시니까 걱정 말아.",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015051.1.18",
          "form": "응 엄마가 잘 챙겨드려~",
          "original_form": "응 엄마가 잘 챙겨드려~",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015051.1.19",
          "form": "요새 피부에 또 알러지 올라오기 시작했어ㅠㅠ",
          "original_form": "요새 피부에 또 알러지 올라오기 시작했어ㅠㅠ",
          "speaker_id": "2",
