      "utterance": [
        {
          "id": "MDRW2100011132.1.1",
          "form": "백화점 가면 주로 뭐해?",
          "original_form": "백화점 가면 주로 뭐해?",
          "speaker_id": "2",
          "time": "20210824 01:27"
        },
        {
          "id": "MDRW2100011132.1.2",
          "form": "나는 옷 구경 보다는 밥먹으로 자주 가는 것 같아 ㅋㅋㅋㅋㅋ",
          "original_form": "나는 옷 구경 보다는 밥먹으로 자주 가는 것 같아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 01:28"
        },
        {
          "id": "MDRW2100011132.1.3",
          "form": "친구는 가방 사러 간대~ 옷이랑",
          "original_form": "친구는 가방 사러 간대~ 옷이랑",
          "speaker_id": "1",
          "time": "20210824 01:28"
        },
        {
          "id": "MDRW2100011132.1.4",
          "form": "백화점 자주 다녀?",
          "original_form": "백화점 자주 다녀?",
          "speaker_id": "1",
          "time": "20210824 01:28"
        },
        {
          "id": "MDRW2100011132.1.5",
          "form": "ㅋㅋ 주로 어떤거 먹어?",
          "original_form": "ㅋㅋ 주로 어떤거 먹어?",
          "speaker_id": "2",
          "time": "20210824 01:28"
        },
        {
          "id": "MDRW2100011132.1.6",
          "form": "옷이랑 가방 사면 좋겠다",
          "original_form": "옷이랑 가방 사면 좋겠다",
          "speaker_id": "2",
          "time": "20210824 01:29"
        },
        {
          "id": "MDRW2100011132.1.7",
          "form": "푸드코트에 맛있는 간식있잖아 그런거 먹으러 가!",
          "original_form": "푸드코트에 맛있는 간식있잖아 그런거 먹으러 가!",
          "speaker_id": "1",
          "time": "20210824 01:29"
        },
        {
          "id": "MDRW2100011132.1.8",
          "form": "백화점 식품관 가봣어?",
          "original_form": "백화점 식품관 가봣어?",
          "speaker_id": "1",
          "time": "20210824 01:30"
        },
        {
          "id": "MDRW2100011132.1.9",
          "form": "나는 저번에 센텀백화점 가서 인도 음식 먹었어",
          "original_form": "나는 저번에 센텀백화점 가서 인도 음식 먹었어",
          "speaker_id": "2",
          "time": "20210824 01:30"
        },
        {
          "id": "MDRW2100011132.1.10",
          "form": "우와 그거 맛있어???",
          "original_form": "우와 그거 맛있어???",
          "speaker_id": "1",
          "time": "20210824 01:30"
        },
        {
          "id": "MDRW2100011132.1.11",
          "form": "백화점 식품관 가봤는데",
          "original_form": "백화점 식품관 가봤는데",
          "speaker_id": "2",
          "time": "20210824 01:30"
        },
        {
          "id": "MDRW2100011132.1.12",
          "form": "음식은 많이 안먹어봤어",
          "original_form": "음식은 많이 안먹어봤어",
          "speaker_id": "2",
          "time": "20210824 01:30"
        },
        {
          "id": "MDRW2100011132.1.13",
          "form": "너는 뭐 먹어봤어?",
          "original_form": "너는 뭐 먹어봤어?",
          "speaker_id": "2",
          "time": "20210824 01:31"
        },
        {
          "id": "MDRW2100011132.1.14",
          "form": "나는 센텀가서는",
          "original_form": "나는 센텀가서는",
          "speaker_id": "1",
          "time": "20210824 01:31"
        },
        {
          "id": "MDRW2100011132.1.15",
          "form": "아웃백 밖에 안가봐서 지금 좀 기대 돼!",
          "original_form": "아웃백 밖에 안가봐서 지금 좀 기대 돼!",
          "speaker_id": "1",
          "time": "20210824 01:31"
        },
        {
          "id": "MDRW2100011132.1.16",
          "form": "우왕~~",
          "original_form": "우왕~~",
          "speaker_id": "2",
          "time": "20210824 01:32"
        },
        {
          "id": "MDRW2100011132.1.17",
          "form": "나중에 같이 가~~",
          "original_form": "나중에 같이 가~~",
          "speaker_id": "1",
          "time": "20210824 01:32"
        },
        {
          "id": "MDRW2100011132.1.18",
          "form": "맛난거 먹고와!",
          "original_form": "맛난거 먹고와!",
          "speaker_id": "2",
          "time": "20210824 01:32"
        },
        {
          "id": "MDRW2100011132.1.19",
          "form": "그냥 놀러가자 백화점 놀러가도 좋잖아 ㅎㅎ",
          "original_form": "그냥 놀러가자 백화점 놀러가도 좋잖아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:32"
        },
        {
          "id": "MDRW2100011132.1.20",
          "form": "나도 데려가줘! ㅋㅋ",
          "original_form": "나도 데려가줘! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 01:32"
        },
