      "utterance": [
        {
          "id": "MDRW2100039629.1.1",
          "form": "주로 낚시대 장난감으로 놀아줘요",
          "original_form": "주로 낚시대 장난감으로 놀아줘요",
          "speaker_id": "2",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039629.1.2",
          "form": "강아지는 던지고 물어와~ 시키잖아요 고양이는 그런건 안물어오나요?",
          "original_form": "강아지는 던지고 물어와~ 시키잖아요 고양이는 그런건 안물어오나요?",
          "speaker_id": "1",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039629.1.3",
          "form": "저희애들은 약간 개냥이과라",
          "original_form": "저희애들은 약간 개냥이과라",
          "speaker_id": "2",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100039629.1.4",
          "form": "던저달라고 물어오긴해요",
          "original_form": "던저달라고 물어오긴해요",
          "speaker_id": "2",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100039629.1.5",
          "form": "완전 고양이 과는 안물어오나보군요",
          "original_form": "완전 고양이 과는 안물어오나보군요",
          "speaker_id": "1",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100039629.1.6",
          "form": "그렇지 않을까요 ㅎㅎ  보통 강쥐가 할거같아요",
          "original_form": "그렇지 않을까요 ㅎㅎ  보통 강쥐가 할거같아요",
          "speaker_id": "2",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100039629.1.7",
          "form": "고양이는 혼자서도 잘논다고 들었거든요",
          "original_form": "고양이는 혼자서도 잘논다고 들었거든요",
          "speaker_id": "1",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100039629.1.8",
          "form": "놀아달라고 안하나요?",
          "original_form": "놀아달라고 안하나요?",
          "speaker_id": "1",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100039629.1.9",
          "form": "고양이도 강아지 산책가듯 사냥놀이가 필수에요",
          "original_form": "고양이도 강아지 산책가듯 사냥놀이가 필수에요",
          "speaker_id": "2",
          "time": "20210903 20:01"
        },
        {
          "id": "MDRW2100039629.1.10",
          "form": "안놀아주면 자꾸 울어요 ㅜㅜ",
          "original_form": "안놀아주면 자꾸 울어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 20:01"
        },
        {
          "id": "MDRW2100039629.1.11",
          "form": "사냥놀이가 낚시??",
          "original_form": "사냥놀이가 낚시??",
          "speaker_id": "1",
          "time": "20210903 20:02"
        },
        {
          "id": "MDRW2100039629.1.12",
          "form": "고양이 장난감은 낚시대 모양이거든요 ㅎㅎ",
          "original_form": "고양이 장난감은 낚시대 모양이거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:02"
        },
        {
          "id": "MDRW2100039629.1.13",
          "form": "낚시대를 막 흔들어줘요",
          "original_form": "낚시대를 막 흔들어줘요",
          "speaker_id": "2",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039629.1.14",
          "form": "고양이도 놀아달라고 하는군요 왠지 도도한 이미지인데요",
          "original_form": "고양이도 놀아달라고 하는군요 왠지 도도한 이미지인데요",
          "speaker_id": "1",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039629.1.15",
          "form": "막..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "막..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039629.1.16",
          "form": "우리집 애들은 저 일하고오면 문앞에  대기하고 있어요 ㅎㅎ",
          "original_form": "우리집 애들은 저 일하고오면 문앞에  대기하고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039629.1.17",
          "form": "강아지처럼  좋아서 발라당 해요",
          "original_form": "강아지처럼  좋아서 발라당 해요",
          "speaker_id": "2",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100039629.1.18",
          "form": "어머머~ 넘 예쁘겠어요",
          "original_form": "어머머~ 넘 예쁘겠어요",
          "speaker_id": "1",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100039629.1.19",
          "form": "키우기 전후가 이미지가 다르네요 ㅎㅎ",
          "original_form": "키우기 전후가 이미지가 다르네요 ㅎㅎ",
