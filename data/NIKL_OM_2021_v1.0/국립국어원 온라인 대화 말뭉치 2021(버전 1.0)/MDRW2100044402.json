      "utterance": [
        {
          "id": "MDRW2100044402.1.1",
          "form": "사실 거의 5일정도니깤ㅋㅋㅋㅋㅋ",
          "original_form": "사실 거의 5일정도니깤ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044402.1.2",
          "form": "ㅋㅋㅋㅋ 진짜 급 대본삘",
          "original_form": "ㅋㅋㅋㅋ 진짜 급 대본삘",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044402.1.3",
          "form": "쟤들이 진짜 바보도 아니고 ㅋㅋ",
          "original_form": "쟤들이 진짜 바보도 아니고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044402.1.4",
          "form": "그니까요 ㅋㅋ제가 체인지 데이즈 마지막에 기본커플 찾아가면 대본일거라햇죠!!!",
          "original_form": "그니까요 ㅋㅋ제가 체인지 데이즈 마지막에 기본커플 찾아가면 대본일거라햇죠!!!",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044402.1.5",
          "form": "님도 같은 기분을 느끼셨군요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "님도 같은 기분을 느끼셨군요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044402.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 진짜 ㅋㅋㅋㅋㅋ 농락당함",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 진짜 ㅋㅋㅋㅋㅋ 농락당함",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044402.1.7",
          "form": "갑자기 마지막에 눈빛 애틋",
          "original_form": "갑자기 마지막에 눈빛 애틋",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044402.1.8",
          "form": "ㅋㅋㅋ아 맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ아 맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044402.1.9",
          "form": "마지막 눈빛 애틋이 진짜 대본같았어요",
          "original_form": "마지막 눈빛 애틋이 진짜 대본같았어요",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044402.1.10",
          "form": "ㅋㅋㅋㅋ 다들 홍보 잘햇네요",
          "original_form": "ㅋㅋㅋㅋ 다들 홍보 잘햇네요",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044402.1.11",
          "form": "아 진짜 ㅋㅋㅋㅋ시청자가 농락당했어요",
