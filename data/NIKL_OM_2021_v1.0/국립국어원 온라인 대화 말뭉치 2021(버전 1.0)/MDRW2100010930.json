      "utterance": [
        {
          "id": "MDRW2100010930.1.1",
          "form": "아 그때 무료로 샀다던 그 사과?",
          "original_form": "아 그때 무료로 샀다던 그 사과?",
          "speaker_id": "2",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100010930.1.2",
          "form": "ㅇㅇ 맛있을까 기대중",
          "original_form": "ㅇㅇ 맛있을까 기대중",
          "speaker_id": "1",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100010930.1.3",
          "form": "맛있겠지 ㅎㅎ 요즘 사과 철이라",
          "original_form": "맛있겠지 ㅎㅎ 요즘 사과 철이라",
          "speaker_id": "2",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100010930.1.4",
          "form": "오면 샐러드에도 넣고 평소에도 많이 먹어야겠다",
          "original_form": "오면 샐러드에도 넣고 평소에도 많이 먹어야겠다",
          "speaker_id": "1",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100010930.1.5",
          "form": "사과는 언제든 환영이지",
          "original_form": "사과는 언제든 환영이지",
          "speaker_id": "2",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100010930.1.6",
          "form": "맛도 좋고 건강에도 좋으니까 ㅎㅎ",
          "original_form": "맛도 좋고 건강에도 좋으니까 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100010930.1.7",
          "form": "꼬맹이들 많이 먹여",
          "original_form": "꼬맹이들 많이 먹여",
          "speaker_id": "2",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100010930.1.8",
          "form": "ㅇㅇ 요즘 엄청 잘 먹어서 영양간식으로 사과만한 게 없어",
          "original_form": "ㅇㅇ 요즘 엄청 잘 먹어서 영양간식으로 사과만한 게 없어",
          "speaker_id": "1",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100010930.1.9",
          "form": "기왕이면 껍질까지",
          "original_form": "기왕이면 껍질까지",
          "speaker_id": "2",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100010930.1.10",
          "form": "이번에 오는 사과는 세척사과라 껍질까지 먹는거야",
          "original_form": "이번에 오는 사과는 세척사과라 껍질까지 먹는거야",
          "speaker_id": "1",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100010930.1.11",
          "form": "잘됐네 ㅎㅎ 껍질에 영양분이 많으니",
          "original_form": "잘됐네 ㅎㅎ 껍질에 영양분이 많으니",
          "speaker_id": "2",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100010930.1.12",
          "form": "ㅇㅇ 꾸준히 먹여야겠다",
          "original_form": "ㅇㅇ 꾸준히 먹여야겠다",
          "speaker_id": "1",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100010930.1.13",
          "form": "맛 좋으면 또 시키자 ㅎㅎ",
          "original_form": "맛 좋으면 또 시키자 ㅎㅎ",
          "speaker_id": "2",
