      "utterance": [
        {
          "id": "MDRW2100024667.1.1",
          "form": "못하고있어요",
          "original_form": "못하고있어요",
          "speaker_id": "2",
          "time": "20210831 16:55"
        },
        {
          "id": "MDRW2100024667.1.2",
          "form": "에어로빅과 수영다녔었는데 다쉬고있어요",
          "original_form": "에어로빅과 수영다녔었는데 다쉬고있어요",
          "speaker_id": "2",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100024667.1.3",
          "form": "아... 그쵸 코로나 때문이 다 문 닫았겠구나...",
          "original_form": "아... 그쵸 코로나 때문이 다 문 닫았겠구나...",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024667.1.4",
          "form": "특별히 아픈곳은 없나요?",
          "original_form": "특별히 아픈곳은 없나요?",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100024667.1.5",
          "form": "저는 허리디스크가 있어서요",
          "original_form": "저는 허리디스크가 있어서요",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100024667.1.6",
          "form": "특별히 아픈 곳은 없어요",
          "original_form": "특별히 아픈 곳은 없어요",
          "speaker_id": "1",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100024667.1.7",
          "form": "디스크 많이 심하신가요?ㅠㅠ",
          "original_form": "디스크 많이 심하신가요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100024667.1.8",
          "form": "디스크가 가끔 재발해서 그때 주사치료 받아야해요",
          "original_form": "디스크가 가끔 재발해서 그때 주사치료 받아야해요",
          "speaker_id": "2",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100024667.1.9",
          "form": "운동 필수로 하셔야겠네요",
          "original_form": "운동 필수로 하셔야겠네요",
          "speaker_id": "1",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100024667.1.10",
          "form": "자격있는 강사있는 헬스장은 어떠세요?",
          "original_form": "자격있는 강사있는 헬스장은 어떠세요?",
          "speaker_id": "1",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100024667.1.11",
          "form": "헬스는 재미가 없어서 안가게되더라구요",
          "original_form": "헬스는 재미가 없어서 안가게되더라구요",
          "speaker_id": "2",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100024667.1.12",
          "form": "필라테스를 할까해요",
          "original_form": "필라테스를 할까해요",
          "speaker_id": "2",
          "time": "20210902 11:07"
        },
        {
          "id": "MDRW2100024667.1.13",
          "form": "그것도 좋죠! 꼭 자격증 있는 전문강사로 고르세요",
          "original_form": "그것도 좋죠! 꼭 자격증 있는 전문강사로 고르세요",
          "speaker_id": "1",
          "time": "20210902 11:07"
        },
        {
          "id": "MDRW2100024667.1.14",
          "form": "기구필라테스는 많이 생기는데도 비싸더라구요",
          "original_form": "기구필라테스는 많이 생기는데도 비싸더라구요",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100024667.1.15",
          "form": "강사도 잘 봐야겠네요",
          "original_form": "강사도 잘 봐야겠네요",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100024667.1.16",
          "form": "그런 재활 치료같은... 그런쪽 자격증 있는 사람이랑 해야 아픈데에 무리가 안 가게 할 수 있어요",
          "original_form": "그런 재활 치료같은... 그런쪽 자격증 있는 사람이랑 해야 아픈데에 무리가 안 가게 할 수 있어요",
          "speaker_id": "1",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100024667.1.17",
          "form": "치료와 다이어트 다 되면 좋을것같아요^^",
          "original_form": "치료와 다이어트 다 되면 좋을것같아요^^",
          "speaker_id": "2",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100024667.1.18",
          "form": "근육 만들면 둘 다 될 거에요!",
          "original_form": "근육 만들면 둘 다 될 거에요!",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100024667.1.19",
          "form": "맞아요!  병원에서도 그얘기를 했거든요",
          "original_form": "맞아요!  병원에서도 그얘기를 했거든요",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100024667.1.20",
          "form": "연예인 김종국이 허리가 안 좋은데 근육으로 버티고 있는 거래요 ㅇㅇ",
          "original_form": "연예인 김종국이 허리가 안 좋은데 근육으로 버티고 있는 거래요 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 14:29"
        }
      ]
