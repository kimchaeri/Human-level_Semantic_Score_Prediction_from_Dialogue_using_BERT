          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100033929.1.3",
          "form": "비 와?",
          "original_form": "비 와?",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100033929.1.4",
          "form": "오전에 좀 왔어",
          "original_form": "오전에 좀 왔어",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100033929.1.5",
          "form": "너희는?",
          "original_form": "너희는?",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100033929.1.6",
          "form": "우리는 오늘은 비 안 왔어",
          "original_form": "우리는 오늘은 비 안 왔어",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100033929.1.7",
          "form": "어제는 비 왔어?",
          "original_form": "어제는 비 왔어?",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100033929.1.8",
          "form": "응 많이 왔어",
          "original_form": "응 많이 왔어",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100033929.1.9",
          "form": "우리도",
          "original_form": "우리도",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100033929.1.10",
          "form": "태풍 피해는 없었어?",
          "original_form": "태풍 피해는 없었어?",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100033929.1.11",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100033929.1.12",
          "form": "너희는 괜찮았어?",
          "original_form": "너희는 괜찮았어?",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100033929.1.13",
          "form": "응 큰 피해 없었어",
          "original_form": "응 큰 피해 없었어",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100033929.1.14",
          "form": "비 좋아해?",
          "original_form": "비 좋아해?",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100033929.1.15",
          "form": "실내에서 보는 건 좋아해",
          "original_form": "실내에서 보는 건 좋아해",
          "speaker_id": "2",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100033929.1.16",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100033929.1.17",
          "form": "나도 실내에서는 좋아해",
          "original_form": "나도 실내에서는 좋아해",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100033929.1.18",
          "form": "옷 젖어서 찝찝한 게 싫어서",
          "original_form": "옷 젖어서 찝찝한 게 싫어서",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100033929.1.19",
          "form": "나도 양말 젖는 거 너무 싫어",
          "original_form": "나도 양말 젖는 거 너무 싫어",
          "speaker_id": "2",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100033929.1.20",
          "form": "아 진짜 끔찍해ㅠㅠ",
          "original_form": "아 진짜 끔찍해ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100033929.1.21",
          "form": "바지 밑단도ㅠㅠ",
          "original_form": "바지 밑단도ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100033929.1.22",
          "form": "맞아 바지도 많이 젖지",
          "original_form": "맞아 바지도 많이 젖지",
          "speaker_id": "2",
          "time": "20210825 16:08"
        }
