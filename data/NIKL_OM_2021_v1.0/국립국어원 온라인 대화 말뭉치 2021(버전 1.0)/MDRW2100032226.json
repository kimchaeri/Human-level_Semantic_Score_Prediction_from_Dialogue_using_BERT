      "utterance": [
        {
          "id": "MDRW2100032226.1.1",
          "form": "배바지 너무 귀여워요 ㅋㅋㅋㅋ",
          "original_form": "배바지 너무 귀여워요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032226.1.2",
          "form": "그쵸ㅋㅋㅋ 짱귀여워요",
          "original_form": "그쵸ㅋㅋㅋ 짱귀여워요",
          "speaker_id": "2",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032226.1.3",
          "form": "아, 그리고 아까는 라인프렌즈샵에서 장패드 하나 샀어요",
          "original_form": "아, 그리고 아까는 라인프렌즈샵에서 장패드 하나 샀어요",
          "speaker_id": "2",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032226.1.4",
          "form": "근데 춘식이는 뭐에요?",
          "original_form": "근데 춘식이는 뭐에요?",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032226.1.5",
          "form": "곰은 아닌거같고",
          "original_form": "곰은 아닌거같고",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032226.1.6",
          "form": "야옹이에요",
          "original_form": "야옹이에요",
          "speaker_id": "2",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032226.1.7",
          "form": "뚱냥이?",
          "original_form": "뚱냥이?",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032226.1.8",
          "form": "임티 이름이 춘식이는 야옹",
          "original_form": "임티 이름이 춘식이는 야옹",
          "speaker_id": "2",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032226.1.9",
          "form": "헉 맞네요 ㅋㅋㅋㅋㅋ",
          "original_form": "헉 맞네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032226.1.10",
          "form": "둔둔한 야옹이였군요",
          "original_form": "둔둔한 야옹이였군요",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032226.1.11",
          "form": "장패드 그 키보드 밑에 까는거요?",
          "original_form": "장패드 그 키보드 밑에 까는거요?",
          "speaker_id": "1",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032226.1.12",
          "form": "네네, 맞아요! 민트색 좋아하는데 하나남은 거 세일 중이라 샀어요",
          "original_form": "네네, 맞아요! 민트색 좋아하는데 하나남은 거 세일 중이라 샀어요",
          "speaker_id": "2",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032226.1.13",
          "form": "샐리래요 노란 병아리 그려진ㅎㅎ",
          "original_form": "샐리래요 노란 병아리 그려진ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032226.1.14",
          "form": "저희집에 샐리 머그컵있어요!ㅋㅋㅋ 별거 아닌데 반갑네요ㅋㅎㅎ",
          "original_form": "저희집에 샐리 머그컵있어요!ㅋㅋㅋ 별거 아닌데 반갑네요ㅋㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100032226.1.15",
          "form": "저희집 키보드 밑에도 장패드 깔려있는데 용도는 모르겠어요 ㅋㅋㅋㅋ",
          "original_form": "저희집 키보드 밑에도 장패드 깔려있는데 용도는 모르겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032226.1.16",
          "form": "이제 샐리 동지네요ㅋㅋㅋ",
          "original_form": "이제 샐리 동지네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032226.1.17",
          "form": "마우스 패드를 따로 안 깔아도 되고 피부를 책상으로부터 보호해주는(?)",
          "original_form": "마우스 패드를 따로 안 깔아도 되고 피부를 책상으로부터 보호해주는(?)",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032226.1.18",
          "form": "아 그러네요 마우스 패드 안까는구나 ㅋㅋㅋ 이제야 인식했어요",
          "original_form": "아 그러네요 마우스 패드 안까는구나 ㅋㅋㅋ 이제야 인식했어요",
          "speaker_id": "1",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032226.1.19",
          "form": "하임님은 작업하실때 팔뜍에 책상 자국 안나세요?",
          "original_form": "하임님은 작업하실때 팔뜍에 책상 자국 안나세요?",
          "speaker_id": "1",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100032226.1.20",
          "form": "저는 항상 나요ㅋㅋㅋㅋㅋ",
          "original_form": "저는 항상 나요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100032226.1.21",
          "form": "왜죠 왜 보호받지 못하죠",
          "original_form": "왜죠 왜 보호받지 못하죠",
          "speaker_id": "1",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100032226.1.22",
          "form": "자국 나요ㅋㅋㅋㅋㅋ 자국 안나려면 모서리에 패드를 붙여야하나??",
          "original_form": "자국 나요ㅋㅋㅋㅋㅋ 자국 안나려면 모서리에 패드를 붙여야하나??",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032226.1.23",
          "form": "역시!ㅋㅋㅋㅋㅋ 패드 붙이는건 번거롭고 그냥 그냥저냥 아프지 않으니까 괜찮아요 ㅎㅎㅋㅋㅋ",
          "original_form": "역시!ㅋㅋㅋㅋㅋ 패드 붙이는건 번거롭고 그냥 그냥저냥 아프지 않으니까 괜찮아요 ㅎㅎㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:37"
        }
      ]
    }
