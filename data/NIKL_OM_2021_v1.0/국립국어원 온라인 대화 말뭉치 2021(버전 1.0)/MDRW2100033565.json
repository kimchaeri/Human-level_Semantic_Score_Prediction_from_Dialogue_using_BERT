      "utterance": [
        {
          "id": "MDRW2100033565.1.1",
          "form": "요즘 다이어트하고계세요?",
          "original_form": "요즘 다이어트하고계세요?",
          "speaker_id": "1",
          "time": "20210824 14:30"
        },
        {
          "id": "MDRW2100033565.1.2",
          "form": "다이어트아닌 다이어트^^",
          "original_form": "다이어트아닌 다이어트^^",
          "speaker_id": "2",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100033565.1.3",
          "form": "풀먹어도 잘차려먹으니 더 찌네요^^",
          "original_form": "풀먹어도 잘차려먹으니 더 찌네요^^",
          "speaker_id": "2",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100033565.1.4",
          "form": "ㅎㅎ 맛있게 먹음 0칼로리",
          "original_form": "ㅎㅎ 맛있게 먹음 0칼로리",
          "speaker_id": "1",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100033565.1.5",
          "form": "요즘 맛있는녀석들 안보니 야식이 줄어서 좀 빠질려고 폼잡고있어요",
          "original_form": "요즘 맛있는녀석들 안보니 야식이 줄어서 좀 빠질려고 폼잡고있어요",
          "speaker_id": "1",
          "time": "20210824 14:34"
        },
        {
          "id": "MDRW2100033565.1.6",
          "form": "ㅎㅎ 오~~ 전 맛있는녀석들도 안보는데 왜 자꾸 찔까요",
          "original_form": "ㅎㅎ 오~~ 전 맛있는녀석들도 안보는데 왜 자꾸 찔까요",
          "speaker_id": "2",
          "time": "20210824 14:34"
        },
        {
          "id": "MDRW2100033565.1.7",
          "form": "다른 먹방을 보시나요ㅎㅎ",
          "original_form": "다른 먹방을 보시나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:35"
        },
        {
          "id": "MDRW2100033565.1.8",
          "form": "걍 잘드시는게 건강에도 좋죠ㅎ",
          "original_form": "걍 잘드시는게 건강에도 좋죠ㅎ",
          "speaker_id": "1",
          "time": "20210824 14:35"
        },
        {
          "id": "MDRW2100033565.1.9",
          "form": "ㅎㅎ 아뇨~~ 먹방은 될수있음 멀리해요. ㅋ",
          "original_form": "ㅎㅎ 아뇨~~ 먹방은 될수있음 멀리해요. ㅋ",
          "speaker_id": "2",
          "time": "20210824 14:36"
        },
        {
          "id": "MDRW2100033565.1.10",
          "form": "살뺀다고 스트레스받음 정신건강에 안좋아요",
          "original_form": "살뺀다고 스트레스받음 정신건강에 안좋아요",
          "speaker_id": "1",
          "time": "20210824 14:36"
        },
        {
          "id": "MDRW2100033565.1.11",
          "form": "네~~ 그래서 적당히 맛있게 먹을라고해요",
          "original_form": "네~~ 그래서 적당히 맛있게 먹을라고해요",
          "speaker_id": "2",
          "time": "20210824 14:38"
        },
        {
          "id": "MDRW2100033565.1.12",
          "form": "이제 선선해졌으니 걷기해야죠",
          "original_form": "이제 선선해졌으니 걷기해야죠",
          "speaker_id": "2",
          "time": "20210824 14:38"
        },
        {
          "id": "MDRW2100033565.1.13",
          "form": "비가 얼렁 그쳐야할텐데요~~",
          "original_form": "비가 얼렁 그쳐야할텐데요~~",
          "speaker_id": "2",
          "time": "20210824 14:39"
        },
        {
          "id": "MDRW2100033565.1.14",
          "form": "저도 운동하고싶긴한데 홈트말고",
          "original_form": "저도 운동하고싶긴한데 홈트말고",
          "speaker_id": "1",
          "time": "20210824 14:39"
        },
        {
          "id": "MDRW2100033565.1.15",
          "form": "점핑이나 번지피지오 이런신나는걸로",
          "original_form": "점핑이나 번지피지오 이런신나는걸로",
          "speaker_id": "1",
          "time": "20210824 14:40"
        },
        {
          "id": "MDRW2100033565.1.16",
          "form": "점핑은 저도 해보고싶어요. 무릎에 무리안간다고 하더라구요",
          "original_form": "점핑은 저도 해보고싶어요. 무릎에 무리안간다고 하더라구요",
          "speaker_id": "2",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100033565.1.17",
          "form": "엄청운동될듯 해요 신나고",
          "original_form": "엄청운동될듯 해요 신나고",
          "speaker_id": "1",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100033565.1.18",
          "form": "재미있게하면 운동도되고 살도금방 빠질꺼같아요",
          "original_form": "재미있게하면 운동도되고 살도금방 빠질꺼같아요",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100033565.1.19",
          "form": "줌바도 잼나요ㅎ",
          "original_form": "줌바도 잼나요ㅎ",
          "speaker_id": "1",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100033565.1.20",
          "form": "몸치라 움직여주려나 모르겠어요^^  재미는있어보이던데요",
          "original_form": "몸치라 움직여주려나 모르겠어요^^  재미는있어보이던데요",
          "speaker_id": "2",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100033565.1.21",
          "form": "저도 몸치인데 할만하더라구요",
          "original_form": "저도 몸치인데 할만하더라구요",
