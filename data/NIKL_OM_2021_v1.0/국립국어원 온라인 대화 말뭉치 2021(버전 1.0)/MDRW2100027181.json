      "utterance": [
        {
          "id": "MDRW2100027181.1.1",
          "form": "전시회 좋아하시나요?",
          "original_form": "전시회 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027181.1.2",
          "form": "관심있는 아티스트전은 가보는 편이예요",
          "original_form": "관심있는 아티스트전은 가보는 편이예요",
          "speaker_id": "1",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100027181.1.3",
          "form": "자주가세요?",
          "original_form": "자주가세요?",
          "speaker_id": "1",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027181.1.4",
          "form": "잘 몰라서 한번씩 가는데",
          "original_form": "잘 몰라서 한번씩 가는데",
          "speaker_id": "2",
          "time": "20210902 11:23"
        },
        {
          "id": "MDRW2100027181.1.5",
          "form": "최근에 에바알머슨 보고왔어요",
          "original_form": "최근에 에바알머슨 보고왔어요",
          "speaker_id": "2",
          "time": "20210902 11:23"
        },
        {
          "id": "MDRW2100027181.1.6",
          "form": "마음이 따뜻해지는 기분이 들어서 좋았습니다",
          "original_form": "마음이 따뜻해지는 기분이 들어서 좋았습니다",
          "speaker_id": "2",
          "time": "20210902 11:24"
        },
        {
          "id": "MDRW2100027181.1.7",
          "form": "방금 검색해봤는데 귀엽네요",
          "original_form": "방금 검색해봤는데 귀엽네요",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027181.1.8",
          "form": "창원에서 보신거예요?",
          "original_form": "창원에서 보신거예요?",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027181.1.9",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 11:34"
        },
        {
          "id": "MDRW2100027181.1.10",
          "form": "최근에 가신곳 어디실까요?",
          "original_form": "최근에 가신곳 어디실까요?",
          "speaker_id": "2",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027181.1.11",
          "form": "올초 압구정에서 있었던 전시회가 가장최근이네요",
          "original_form": "올초 압구정에서 있었던 전시회가 가장최근이네요",
          "speaker_id": "1",
          "time": "20210902 12:02"
        },
        {
          "id": "MDRW2100027181.1.12",
          "form": "오 어떤전시회였어요?ㅎㅎ",
          "original_form": "오 어떤전시회였어요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027181.1.13",
          "form": "크래피티 하는 일러스트레이터 전시회였는데",
          "original_form": "크래피티 하는 일러스트레이터 전시회였는데",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100027181.1.14",
          "form": "재밌었어요 md도 몇개사고",
          "original_form": "재밌었어요 md도 몇개사고",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100027181.1.15",
          "form": "서울은 문화를 다양하게 접할수있어서 좋은거 같아요",
          "original_form": "서울은 문화를 다양하게 접할수있어서 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027181.1.16",
          "form": "부럽기도 하구요",
          "original_form": "부럽기도 하구요",
          "speaker_id": "2",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027181.1.17",
          "form": "있어도 잘은 안가요 저도 서울은 아니구요",
          "original_form": "있어도 잘은 안가요 저도 서울은 아니구요",
          "speaker_id": "1",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100027181.1.18",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100027181.1.19",
          "form": "최근 영화관 가보셧나요?",
          "original_form": "최근 영화관 가보셧나요?",
