      "utterance": [
        {
          "id": "MDRW2100039079.1.1",
          "form": "청소기 돌리는거 담당하고 있어요",
          "original_form": "청소기 돌리는거 담당하고 있어요",
          "speaker_id": "2",
          "time": "20210903 09:21"
        },
        {
          "id": "MDRW2100039079.1.2",
          "form": "당번이 따로 있죠",
          "original_form": "당번이 따로 있죠",
          "speaker_id": "2",
          "time": "20210903 09:21"
        },
        {
          "id": "MDRW2100039079.1.3",
          "form": "ㅋㅋㅋㅋ오",
          "original_form": "ㅋㅋㅋㅋ오",
          "speaker_id": "1",
          "time": "20210903 09:21"
        },
        {
          "id": "MDRW2100039079.1.4",
          "form": "동생도해요? 고딩인데",
          "original_form": "동생도해요? 고딩인데",
          "speaker_id": "1",
          "time": "20210903 09:21"
        },
        {
          "id": "MDRW2100039079.1.5",
          "form": "동생은 학교 가는 길에 쓰레기 버려요",
          "original_form": "동생은 학교 가는 길에 쓰레기 버려요",
          "speaker_id": "2",
          "time": "20210903 09:22"
        },
        {
          "id": "MDRW2100039079.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ 귀엽네유...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 귀엽네유...",
          "speaker_id": "1",
          "time": "20210903 09:22"
        },
        {
          "id": "MDRW2100039079.1.7",
          "form": "요린 안하시나요",
          "original_form": "요린 안하시나요",
          "speaker_id": "1",
          "time": "20210903 09:22"
        },
        {
          "id": "MDRW2100039079.1.8",
          "form": "밀키트로 먹는 날에는 제가 해요",
          "original_form": "밀키트로 먹는 날에는 제가 해요",
          "speaker_id": "2",
          "time": "20210903 09:24"
        },
        {
          "id": "MDRW2100039079.1.9",
          "form": "직접 사시나요 ㅋㅋ",
          "original_form": "직접 사시나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:29"
        },
        {
          "id": "MDRW2100039079.1.10",
          "form": "직접 사는 편이죠ㅋㅋ 제가 먹고싶은 메뉴로다가",
          "original_form": "직접 사는 편이죠ㅋㅋ 제가 먹고싶은 메뉴로다가",
          "speaker_id": "2",
          "time": "20210903 09:30"
        },
        {
          "id": "MDRW2100039079.1.11",
          "form": "ㅋㅋㅋ요리담당분이 조아하시겟네요",
          "original_form": "ㅋㅋㅋ요리담당분이 조아하시겟네요",
          "speaker_id": "1",
          "time": "20210903 09:32"
        },
        {
          "id": "MDRW2100039079.1.12",
          "form": "맞아요ㅋㅋ 부모님이 엄청 좋아하십니다",
          "original_form": "맞아요ㅋㅋ 부모님이 엄청 좋아하십니다",
          "speaker_id": "2",
          "time": "20210903 09:35"
        },
        {
          "id": "MDRW2100039079.1.13",
          "form": "설거지도하시나요",
          "original_form": "설거지도하시나요",
          "speaker_id": "1",
          "time": "20210903 09:35"
        },
        {
          "id": "MDRW2100039079.1.14",
          "form": "설거지는 아부지가 하고 있어요",
          "original_form": "설거지는 아부지가 하고 있어요",
          "speaker_id": "2",
          "time": "20210903 09:37"
        },
        {
          "id": "MDRW2100039079.1.15",
          "form": "ㅋㅋㅋㅋ 담당 확실하네요",
          "original_form": "ㅋㅋㅋㅋ 담당 확실하네요",
          "speaker_id": "1",
          "time": "20210903 09:37"
        },
        {
