      "utterance": [
        {
          "id": "MDRW2100023380.1.1",
          "form": "아이가 엄청 활발해요ㅎ 성격은 내성적인데 웃는거 좋아하고 아빠엄청 좋아하구요ㄹ",
          "original_form": "아이가 엄청 활발해요ㅎ 성격은 내성적인데 웃는거 좋아하고 아빠엄청 좋아하구요ㄹ",
          "speaker_id": "2",
          "time": "20210830 01:13"
        },
        {
          "id": "MDRW2100023380.1.2",
          "form": "우와ㅎ",
          "original_form": "우와ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:13"
        },
        {
          "id": "MDRW2100023380.1.3",
          "form": "혼자서요?",
          "original_form": "혼자서요?",
          "speaker_id": "2",
          "time": "20210830 01:13"
        },
        {
          "id": "MDRW2100023380.1.4",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:13"
        },
        {
          "id": "MDRW2100023380.1.5",
          "form": "우와ㅠㅠ",
          "original_form": "우와ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 01:13"
        },
        {
          "id": "MDRW2100023380.1.6",
          "form": "부럽네요ㅠ",
          "original_form": "부럽네요ㅠ",
          "speaker_id": "2",
          "time": "20210830 01:13"
        },
        {
          "id": "MDRW2100023380.1.7",
          "form": "전4살인데 아직도 아빠엄마가 없으면 안자요ㅠ",
          "original_form": "전4살인데 아직도 아빠엄마가 없으면 안자요ㅠ",
          "speaker_id": "2",
          "time": "20210830 01:14"
        },
        {
          "id": "MDRW2100023380.1.8",
          "form": "조금더 크면  ㅎㅎㅎ",
          "original_form": "조금더 크면  ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:14"
        },
        {
          "id": "MDRW2100023380.1.9",
          "form": "가서 꼭 무서운이야기 재밋는이야기 해줘야해요ㅎ",
          "original_form": "가서 꼭 무서운이야기 재밋는이야기 해줘야해요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:14"
        },
        {
          "id": "MDRW2100023380.1.10",
          "form": "혼자 잘거에요",
          "original_form": "혼자 잘거에요",
          "speaker_id": "1",
          "time": "20210830 01:14"
        },
        {
          "id": "MDRW2100023380.1.11",
          "form": "이제 그런것도 못하겟네요ㅠ",
          "original_form": "이제 그런것도 못하겟네요ㅠ",
          "speaker_id": "2",
          "time": "20210830 01:14"
        },
        {
          "id": "MDRW2100023380.1.12",
          "form": "아이랑 따로 살아야해서ㅎ",
          "original_form": "아이랑 따로 살아야해서ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:14"
        },
        {
          "id": "MDRW2100023380.1.13",
          "form": "재우는건 나중에ㅎㅎ",
          "original_form": "재우는건 나중에ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:14"
        },
        {
          "id": "MDRW2100023380.1.14",
          "form": "에공",
          "original_form": "에공",
          "speaker_id": "1",
          "time": "20210830 01:14"
        },
        {
          "id": "MDRW2100023380.1.15",
          "form": "할머니랑 살아야 하나요?",
          "original_form": "할머니랑 살아야 하나요?",
          "speaker_id": "1",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100023380.1.16",
          "form": "ㅎㅎ 괜찮아요ㅎ 어짜피 일어날일이어서ㅎ",
          "original_form": "ㅎㅎ 괜찮아요ㅎ 어짜피 일어날일이어서ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100023380.1.17",
          "form": "아내랑 어머님이랑 같이 살아야하는데",
          "original_form": "아내랑 어머님이랑 같이 살아야하는데",
          "speaker_id": "2",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100023380.1.18",
          "form": "아내가 좀 그건 싫다고하네요ㅎ",
          "original_form": "아내가 좀 그건 싫다고하네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100023380.1.19",
          "form": "아내랑 어머님이랑 좀 자주싸워서ㅎ",
          "original_form": "아내랑 어머님이랑 좀 자주싸워서ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100023380.1.20",
          "form": "아하 그러시구낭",
          "original_form": "아하 그러시구낭",
          "speaker_id": "1",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100023380.1.21",
          "form": "육아문제로 더싸울것같아서ㅎ 아내도 싫은가봐요",
          "original_form": "육아문제로 더싸울것같아서ㅎ 아내도 싫은가봐요",
          "speaker_id": "2",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100023380.1.22",
          "form": "그래서 저도 고민이에요ㅎ 당장에 이혼하면 위자료도 줘야하고ㄹ",
          "original_form": "그래서 저도 고민이에요ㅎ 당장에 이혼하면 위자료도 줘야하고ㄹ",
          "speaker_id": "2",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100023380.1.23",
          "form": "빚도값아야하는데ㅎ 아내는 대출받아서 이사간다고하지",
          "original_form": "빚도값아야하는데ㅎ 아내는 대출받아서 이사간다고하지",
          "speaker_id": "2",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100023380.1.24",
          "form": "집이 30평인데 그짐가지고 내가 위자료 줘도 지금보다 더 줄수도없는 노릇이고ㅎ 자존심굽히고 그냥 어머님댁에 들어가면 되는데... 참 걱정이에요",
          "original_form": "집이 30평인데 그짐가지고 내가 위자료 줘도 지금보다 더 줄수도없는 노릇이고ㅎ 자존심굽히고 그냥 어머님댁에 들어가면 되는데... 참 걱정이에요",
          "speaker_id": "2",
          "time": "20210830 01:17"
        },
        {
          "id": "MDRW2100023380.1.25",
          "form": "아내분도 일을 하시는거에요?",
          "original_form": "아내분도 일을 하시는거에요?",
          "speaker_id": "1",
          "time": "20210830 01:17"
        },
        {
          "id": "MDRW2100023380.1.26",
          "form": "이제 일을해요ㅎ",
          "original_form": "이제 일을해요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:17"
        },
        {
          "id": "MDRW2100023380.1.27",
          "form": "그동안 육아휴직3년쉬었어요ㅎ",
          "original_form": "그동안 육아휴직3년쉬었어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:18"
        },
        {
          "id": "MDRW2100023380.1.28",
          "form": "아 근데.실질적으로 이혼이 쉽진 않을거에요",
          "original_form": "아 근데.실질적으로 이혼이 쉽진 않을거에요",
          "speaker_id": "1",
          "time": "20210830 01:18"
        },
        {
          "id": "MDRW2100023380.1.29",
          "form": "육아휴직하고 세금내면 당장에 3개월동안은 월급없을꺼애요 그리고 위자료로 집구하기도 힘들고ㅎ",
          "original_form": "육아휴직하고 세금내면 당장에 3개월동안은 월급없을꺼애요 그리고 위자료로 집구하기도 힘들고ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:19"
        },
        {
          "id": "MDRW2100023380.1.30",
          "form": "근데도 꼭 해야한다고 하니깐 어쩔수가없어요",
          "original_form": "근데도 꼭 해야한다고 하니깐 어쩔수가없어요",
          "speaker_id": "2",
          "time": "20210830 01:19"
        }
      ]
