      "utterance": [
        {
          "id": "MDRW2100027984.1.1",
          "form": "네! 저 한참 당근마켓에 꽂혀서",
          "original_form": "네! 저 한참 당근마켓에 꽂혀서",
          "speaker_id": "2",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027984.1.2",
          "form": "당근에서 뮤ㅓ 많이 샀던거 같아요",
          "original_form": "당근에서 뮤ㅓ 많이 샀던거 같아요",
          "speaker_id": "2",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027984.1.3",
          "form": "ㅎㅎ팔기도 많이 팔구!",
          "original_form": "ㅎㅎ팔기도 많이 팔구!",
          "speaker_id": "2",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027984.1.4",
          "form": "주로 뭐 사셨어요?",
          "original_form": "주로 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210902 16:41"
        },
        {
          "id": "MDRW2100027984.1.5",
          "form": "옷 많이 샀엉ㅅ!",
          "original_form": "옷 많이 샀엉ㅅ!",
          "speaker_id": "2",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027984.1.6",
          "form": "근데 살이 많이쪄서",
          "original_form": "근데 살이 많이쪄서",
          "speaker_id": "2",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027984.1.7",
          "form": "그때 샀던 옷도 하나도 안맞는..ㅎㅎ",
          "original_form": "그때 샀던 옷도 하나도 안맞는..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027984.1.8",
          "form": "ㅇㅅㅠㅠ",
          "original_form": "ㅇㅅㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100027984.1.9",
          "form": "당근으로 사면 물건 괜찮은가요?",
          "original_form": "당근으로 사면 물건 괜찮은가요?",
          "speaker_id": "1",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100027984.1.10",
          "form": "저는 되게 싸게 바지 샀었는데",
          "original_form": "저는 되게 싸게 바지 샀었는데",
          "speaker_id": "2",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100027984.1.11",
          "form": "약간 원래 옷 좋아하시구 옷 많이 사시는 분이 크게 입고싶어서",
          "original_form": "약간 원래 옷 좋아하시구 옷 많이 사시는 분이 크게 입고싶어서",
          "speaker_id": "2",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100027984.1.12",
          "form": "사셨던걸 한번도 안입고 파는거였어서",
          "original_form": "사셨던걸 한번도 안입고 파는거였어서",
          "speaker_id": "2",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100027984.1.13",
          "form": "엄청 만족했었어요",
          "original_form": "엄청 만족했었어요",
          "speaker_id": "2",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100027984.1.14",
          "form": "와 정망 횡재하셨네요ㅋㅋㅋ 저는 왠지 못미더워서 앱 삭제했어요ㅠ",
          "original_form": "와 정망 횡재하셨네요ㅋㅋㅋ 저는 왠지 못미더워서 앱 삭제했어요ㅠ",
          "speaker_id": "1",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100027984.1.15",
          "form": "헐 이런. ! 저도 요즘은 다시 안쓰긴 해요",
          "original_form": "헐 이런. ! 저도 요즘은 다시 안쓰긴 해요",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100027984.1.16",
          "form": "생각보다 커뮤니티 기능..? 이런게 있어서 엄마아빠가 더 조아하능거 같기도 해요",
          "original_form": "생각보다 커뮤니티 기능..? 이런게 있어서 엄마아빠가 더 조아하능거 같기도 해요",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100027984.1.17",
          "form": "그거 저희 지역은 별로 활성화 안 되어있어서... 별로 메리트가 없었어요",
          "original_form": "그거 저희 지역은 별로 활성화 안 되어있어서... 별로 메리트가 없었어요",
          "speaker_id": "1",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100027984.1.18",
          "form": "아하..~! 그럼 그럴만두해요",
          "original_form": "아하..~! 그럼 그럴만두해요",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100027984.1.19",
          "form": "운좋으면 싸게 건질수도 있고 해서 좋긴한데",
          "original_form": "운좋으면 싸게 건질수도 있고 해서 좋긴한데",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100027984.1.20",
          "form": "또 생각보다 귀찮기도 하구..",
          "original_form": "또 생각보다 귀찮기도 하구..",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100027984.1.21",
          "form": "당근 말고 딴데서 중고 팔아 봤는데",
          "original_form": "당근 말고 딴데서 중고 팔아 봤는데",
          "speaker_id": "1",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100027984.1.22",
          "form": "글 쓰고 사진 찍고하는게 큰 일이더라구여",
          "original_form": "글 쓰고 사진 찍고하는게 큰 일이더라구여",
          "speaker_id": "1",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100027984.1.23",
          "form": "앳 맞아요",
          "original_form": "앳 맞아요",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100027984.1.24",
          "form": "상세사진 요구하는 사람도 있어서 은근 맞추기 까다롭기두 하구여",
          "original_form": "상세사진 요구하는 사람도 있어서 은근 맞추기 까다롭기두 하구여",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100027984.1.25",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100027984.1.26",
          "form": "맞아요 막 네고해달라 그러고...",
          "original_form": "맞아요 막 네고해달라 그러고...",
          "speaker_id": "1",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100027984.1.27",
          "form": "앗 맞아요!!",
          "original_form": "앗 맞아요!!",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100027984.1.28",
          "form": "전.. 안해주고싶은데요..",
          "original_form": "전.. 안해주고싶은데요..",
