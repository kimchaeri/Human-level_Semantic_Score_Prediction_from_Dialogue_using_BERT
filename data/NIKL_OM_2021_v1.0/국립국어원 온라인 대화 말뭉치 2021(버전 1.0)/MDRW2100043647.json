      "utterance": [
        {
          "id": "MDRW2100043647.1.1",
          "form": "콜라요!!",
          "original_form": "콜라요!!",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043647.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043647.1.3",
          "form": "앗!! 전 사이다 파입니다. ㅋ",
          "original_form": "앗!! 전 사이다 파입니다. ㅋ",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043647.1.4",
          "form": "주로 어떤 음식드실 때 탄산드시나용",
          "original_form": "주로 어떤 음식드실 때 탄산드시나용",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043647.1.5",
          "form": "치킨이나~ 오늘같은 버터 들어간 음식 먹을때요?",
          "original_form": "치킨이나~ 오늘같은 버터 들어간 음식 먹을때요?",
          "speaker_id": "1",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043647.1.6",
          "form": "오오 저두요 ㅎㅎ",
          "original_form": "오오 저두요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043647.1.7",
          "form": "아니면 피자?",
          "original_form": "아니면 피자?",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043647.1.8",
          "form": "다이어트 식사 추천부탁드려요~",
          "original_form": "다이어트 식사 추천부탁드려요~",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043647.1.9",
          "form": "아 피자 먹을때 무조건 사이다 있어야죠",
          "original_form": "아 피자 먹을때 무조건 사이다 있어야죠",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043647.1.10",
          "form": "샐러드요~ 닭가슴살 샐러드",
          "original_form": "샐러드요~ 닭가슴살 샐러드",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043647.1.11",
          "form": "맞죠맞죠",
          "original_form": "맞죠맞죠",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043647.1.12",
          "form": "샐러드랑 샌드위치. 밥에 고기",
          "original_form": "샐러드랑 샌드위치. 밥에 고기",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043647.1.13",
          "form": "이렇게만 먹으니까 지치더라구요ㅠㅠ",
          "original_form": "이렇게만 먹으니까 지치더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043647.1.14",
          "form": "근데 샐러드 먹으면 금방 배고파서요",
          "original_form": "근데 샐러드 먹으면 금방 배고파서요",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043647.1.15",
          "form": "스테이크 먹으면 좀 든든하잖아요",
          "original_form": "스테이크 먹으면 좀 든든하잖아요",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043647.1.16",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043647.1.17",
          "form": "소고기 종종먹는데 참 눈물나는게 넘 비싸네요ㅠㅠ",
          "original_form": "소고기 종종먹는데 참 눈물나는게 넘 비싸네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043647.1.18",
          "form": "맞아요 ㅠ 왜케 비싸죠?",
          "original_form": "맞아요 ㅠ 왜케 비싸죠?",
          "speaker_id": "1",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043647.1.19",
          "form": "소고기 먹으면 든든해서 전 다음끼 거르거든요. 이것도 다이어트에 도움이 되지 않을까요?? ㅋ",
          "original_form": "소고기 먹으면 든든해서 전 다음끼 거르거든요. 이것도 다이어트에 도움이 되지 않을까요?? ㅋ",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043647.1.20",
          "form": "아 정말요??",
          "original_form": "아 정말요??",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043647.1.21",
          "form": "저는 100그람이다보니... 금방 고파요 ㅎㅎ",
          "original_form": "저는 100그람이다보니... 금방 고파요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043647.1.22",
          "form": "고기를 100그람만 드신다고요???",
          "original_form": "고기를 100그람만 드신다고요???",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043647.1.23",
          "form": "한끼에요!",
          "original_form": "한끼에요!",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043647.1.24",
          "form": "악. ㅠ 삶의 낙이 너무 없는데요 ㅠ",
          "original_form": "악. ㅠ 삶의 낙이 너무 없는데요 ㅠ",
