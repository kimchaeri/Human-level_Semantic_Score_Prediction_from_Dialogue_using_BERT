      "utterance": [
        {
          "id": "MDRW2100015958.1.1",
          "form": "동호회 하시나요?",
          "original_form": "동호회 하시나요?",
          "speaker_id": "2",
          "time": "20210826 04:10"
        },
        {
          "id": "MDRW2100015958.1.2",
          "form": "아니요ㅠㅠ",
          "original_form": "아니요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 04:10"
        },
        {
          "id": "MDRW2100015958.1.3",
          "form": "요즘 코로나때문에",
          "original_form": "요즘 코로나때문에",
          "speaker_id": "1",
          "time": "20210826 04:10"
        },
        {
          "id": "MDRW2100015958.1.4",
          "form": "하고는 싶은데",
          "original_form": "하고는 싶은데",
          "speaker_id": "1",
          "time": "20210826 04:10"
        },
        {
          "id": "MDRW2100015958.1.5",
          "form": "못하네용",
          "original_form": "못하네용",
          "speaker_id": "1",
          "time": "20210826 04:10"
        },
        {
          "id": "MDRW2100015958.1.6",
          "form": "맞죠 ㅠㅠ 상황이 여의치않네요",
          "original_form": "맞죠 ㅠㅠ 상황이 여의치않네요",
          "speaker_id": "2",
          "time": "20210826 04:11"
        },
        {
          "id": "MDRW2100015958.1.7",
          "form": "활동적인 모임은 어렵구 덜 활동적인건 비대면으로 많이 진행하는 것 같아요",
          "original_form": "활동적인 모임은 어렵구 덜 활동적인건 비대면으로 많이 진행하는 것 같아요",
          "speaker_id": "2",
          "time": "20210826 04:12"
        },
        {
          "id": "MDRW2100015958.1.8",
          "form": "오오 맞아요",
          "original_form": "오오 맞아요",
          "speaker_id": "1",
          "time": "20210826 04:12"
        },
        {
          "id": "MDRW2100015958.1.9",
          "form": "코로나 풀리면 하고싶은 동호회가 있나요?",
          "original_form": "코로나 풀리면 하고싶은 동호회가 있나요?",
          "speaker_id": "2",
          "time": "20210826 04:12"
        },
        {
          "id": "MDRW2100015958.1.10",
          "form": "저는 자전거 동호회나 산악 동호회 하고싶은데",
          "original_form": "저는 자전거 동호회나 산악 동호회 하고싶은데",
          "speaker_id": "1",
          "time": "20210826 04:12"
        },
        {
          "id": "MDRW2100015958.1.11",
          "form": "산악은 부적절한 만남이 자주 이루어진다길래",
          "original_form": "산악은 부적절한 만남이 자주 이루어진다길래",
          "speaker_id": "1",
          "time": "20210826 04:13"
        },
        {
          "id": "MDRW2100015958.1.12",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 04:13"
        },
        {
          "id": "MDRW2100015958.1.13",
          "form": "오옹 활동적인 취미를 즐기시네요 ㅋㅋㅋㅋ",
          "original_form": "오옹 활동적인 취미를 즐기시네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 04:13"
        },
        {
          "id": "MDRW2100015958.1.14",
          "form": "name2 님은요? 해보신거 있으세요?",
          "original_form": "&name2& 님은요? 해보신거 있으세요?",
          "speaker_id": "1",
          "time": "20210826 04:13"
        },
        {
          "id": "MDRW2100015958.1.15",
          "form": "저는 막 모르는 사람들이랑 하는 동호회는 안해봤구",
          "original_form": "저는 막 모르는 사람들이랑 하는 동호회는 안해봤구",
          "speaker_id": "2",
          "time": "20210826 04:13"
        },
        {
          "id": "MDRW2100015958.1.16",
          "form": "교내 동아리같은거만 해봤어요",
          "original_form": "교내 동아리같은거만 해봤어요",
          "speaker_id": "2",
          "time": "20210826 04:14"
        },
        {
          "id": "MDRW2100015958.1.17",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210826 04:14"
        },
        {
          "id": "MDRW2100015958.1.18",
          "form": "동아리 저는 못해봐서",
          "original_form": "동아리 저는 못해봐서",
          "speaker_id": "1",
          "time": "20210826 04:14"
        },
        {
          "id": "MDRW2100015958.1.19",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "1",
          "time": "20210826 04:14"
        },
        {
          "id": "MDRW2100015958.1.20",
          "form": "헉 많이 바쁘셨나요?ㅠㅠ",
          "original_form": "헉 많이 바쁘셨나요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 04:14"
        },
        {
          "id": "MDRW2100015958.1.21",
          "form": "아쉽네용 ㅠㅠ",
          "original_form": "아쉽네용 ㅠㅠ",
