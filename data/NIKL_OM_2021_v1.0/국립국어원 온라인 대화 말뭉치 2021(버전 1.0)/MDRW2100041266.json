      "utterance": [
        {
          "id": "MDRW2100041266.1.1",
          "form": "최근에 산 물건 있어3",
          "original_form": "최근에 산 물건 있어3",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100041266.1.2",
          "form": "오늘 구두산거 신어봤어",
          "original_form": "오늘 구두산거 신어봤어",
          "speaker_id": "1",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100041266.1.3",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100041266.1.4",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100041266.1.5",
          "form": "단화야?",
          "original_form": "단화야?",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100041266.1.6",
          "form": "응 갈색 로퍼 맘에들어",
          "original_form": "응 갈색 로퍼 맘에들어",
          "speaker_id": "1",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100041266.1.7",
          "form": "오 이쁘겠다",
          "original_form": "오 이쁘겠다",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100041266.1.8",
          "form": "나도 로퍼사야되는데",
          "original_form": "나도 로퍼사야되는데",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100041266.1.9",
          "form": "살게너무많아",
          "original_form": "살게너무많아",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100041266.1.10",
          "form": "사이즈도 좋고 편하네",
          "original_form": "사이즈도 좋고 편하네",
          "speaker_id": "1",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100041266.1.11",
          "form": "순대야 어디갔오",
          "original_form": "순대야 어디갔오",
          "speaker_id": "1",
          "time": "20210906 15:18"
        },
        {
          "id": "MDRW2100041266.1.12",
          "form": "순대야ㅠㅠ가버렸니",
          "original_form": "순대야ㅠㅠ가버렸니",
          "speaker_id": "1",
          "time": "20210906 19:24"
        },
        {
          "id": "MDRW2100041266.1.13",
          "form": "헐 아냐ㅋㅋㅋ",
          "original_form": "헐 아냐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100041266.1.14",
          "form": "나 어제 출근준비한다고 못봤네 미안",
          "original_form": "나 어제 출근준비한다고 못봤네 미안",
          "speaker_id": "2",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100041266.1.15",
          "form": "이제 퇴근했어ㅋㅋㅋ",
          "original_form": "이제 퇴근했어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100041266.1.16",
          "form": "오 순대 왔구나",
          "original_form": "오 순대 왔구나",
          "speaker_id": "1",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100041266.1.17",
          "form": "고생했어ㅠㅠ 집 도착했니",
          "original_form": "고생했어ㅠㅠ 집 도착했니",
          "speaker_id": "1",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100041266.1.18",
          "form": "지금 가는 중ㅋㅋㅋ",
          "original_form": "지금 가는 중ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100041266.1.19",
          "form": "집가는중에",
          "original_form": "집가는중에",
          "speaker_id": "2",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100041266.1.20",
          "form": "떡볶이 사간다",
          "original_form": "떡볶이 사간다",
          "speaker_id": "2",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100041266.1.21",
          "form": "뭐 사갈거야?",
          "original_form": "뭐 사갈거야?",
          "speaker_id": "1",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100041266.1.22",
          "form": "우와 떡볶이 벌써 샀어?",
          "original_form": "우와 떡볶이 벌써 샀어?",
          "speaker_id": "1",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100041266.1.23",
          "form": "ㅋㅋㅋ응 받는중에 알람와서 봤어",
          "original_form": "ㅋㅋㅋ응 받는중에 알람와서 봤어",
          "speaker_id": "2",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100041266.1.24",
          "form": "우와 그랬구나",
          "original_form": "우와 그랬구나",
          "speaker_id": "1",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100041266.1.25",
          "form": "얼른 집에가서 먹어",
          "original_form": "얼른 집에가서 먹어",
          "speaker_id": "1",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100041266.1.26",
          "form": "나도 떡볶이 사고 싶다",
          "original_form": "나도 떡볶이 사고 싶다",
          "speaker_id": "1",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100041266.1.27",
          "form": "아직 저녁 안먹었으면 떡볶이 어때?!",
          "original_form": "아직 저녁 안먹었으면 떡볶이 어때?!",
          "speaker_id": "2",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100041266.1.28",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100041266.1.29",
          "form": "나 벌써 먹얶지 뭐여",
          "original_form": "나 벌써 먹얶지 뭐여",
          "speaker_id": "1",
          "time": "20210906 19:48"
        }
      ]
