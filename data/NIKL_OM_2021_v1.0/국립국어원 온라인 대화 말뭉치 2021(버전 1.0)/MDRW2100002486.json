      "utterance": [
        {
          "id": "MDRW2100002486.1.1",
          "form": "해외여행 많이 가봤어요?",
          "original_form": "해외여행 많이 가봤어요?",
          "speaker_id": "2",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002486.1.2",
          "form": "저는 두번가봤습니다ㅎㅎ 가려다가",
          "original_form": "저는 두번가봤습니다ㅎㅎ 가려다가",
          "speaker_id": "1",
          "time": "20210820 12:53"
        },
        {
          "id": "MDRW2100002486.1.3",
          "form": "코로나땜에 막혀서 못가는중이네요ㅠㅠ",
          "original_form": "코로나땜에 막혀서 못가는중이네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 12:53"
        },
        {
          "id": "MDRW2100002486.1.4",
          "form": "어디어디 가봤나요?",
          "original_form": "어디어디 가봤나요?",
          "speaker_id": "2",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002486.1.5",
          "form": "대만이랑 일본이요ㅋㅋㅋ",
          "original_form": "대만이랑 일본이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:27"
        },
        {
          "id": "MDRW2100002486.1.6",
          "form": "대만이 좋았답니다!",
          "original_form": "대만이 좋았답니다!",
          "speaker_id": "1",
          "time": "20210820 14:27"
        },
        {
          "id": "MDRW2100002486.1.7",
          "form": "오 저 대만 한번 가보고싶어요",
          "original_form": "오 저 대만 한번 가보고싶어요",
          "speaker_id": "2",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002486.1.8",
          "form": "진짜좋아요ㅋㅋㅋ",
          "original_form": "진짜좋아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002486.1.9",
          "form": "코로나끝나면한번가보세요!",
          "original_form": "코로나끝나면한번가보세요!",
          "speaker_id": "1",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002486.1.10",
          "form": "그래야겠어요",
          "original_form": "그래야겠어요",
          "speaker_id": "2",
          "time": "20210820 15:17"
        },
        {
          "id": "MDRW2100002486.1.11",
          "form": "일본은 어디 가보셨어요?",
          "original_form": "일본은 어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210820 15:17"
        },
        {
          "id": "MDRW2100002486.1.12",
          "form": "후쿠오카요ㅋㅋㅋ",
          "original_form": "후쿠오카요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002486.1.13",
          "form": "근데일본은진짜예전에갔어가지고",
          "original_form": "근데일본은진짜예전에갔어가지고",
          "speaker_id": "1",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002486.1.14",
          "form": "국내 최근에가신곳있으신가요?",
          "original_form": "국내 최근에가신곳있으신가요?",
          "speaker_id": "1",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002486.1.15",
          "form": "저는 최근에는 가본곳이 없어요",
          "original_form": "저는 최근에는 가본곳이 없어요",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002486.1.16",
          "form": "아 4월달에 청벚꽃보러 개심사 다녀왔어요",
          "original_form": "아 4월달에 청벚꽃보러 개심사 다녀왔어요",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002486.1.17",
          "form": "개심사요??",
          "original_form": "개심사요??",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002486.1.18",
          "form": "어디지역이죠?!",
          "original_form": "어디지역이죠?!",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002486.1.19",
          "form": "충남이요",
          "original_form": "충남이요",
          "speaker_id": "2",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002486.1.20",
          "form": "서산시에 있어요",
          "original_form": "서산시에 있어요",
          "speaker_id": "2",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002486.1.21",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002486.1.22",
          "form": "저도여행가고싶어요ㅠㅠ",
          "original_form": "저도여행가고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 15:38"
        }
      ]
