      "utterance": [
        {
          "id": "MDRW2100044698.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ2차까지 맞았나봐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ2차까지 맞았나봐",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044698.1.2",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044698.1.3",
          "form": "어제 따끔이라고 하면서",
          "original_form": "어제 따끔이라고 하면서",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044698.1.4",
          "form": "쌍따봉올림",
          "original_form": "쌍따봉올림",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044698.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ부럽다...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ부럽다...",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044698.1.6",
          "form": "민기도 헤드윅하면서",
          "original_form": "민기도 헤드윅하면서",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044698.1.7",
          "form": "이제 방송 녹화같은거 가도",
          "original_form": "이제 방송 녹화같은거 가도",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044698.1.8",
          "form": "티엠아를 뿌려주길바랬는데",
          "original_form": "티엠아를 뿌려주길바랬는데",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044698.1.9",
          "form": "긴장 덜할듯",
          "original_form": "긴장 덜할듯",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044698.1.10",
          "form": "민기는 긴장했는지 애드립을 하지않았어...",
          "original_form": "민기는 긴장했는지 애드립을 하지않았어...",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044698.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044698.1.12",
          "form": "오늘도 조드윅은 3시간을 꽊꽉채웠는데....",
          "original_form": "오늘도 조드윅은 3시간을 꽊꽉채웠는데....",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044698.1.13",
          "form": "55분이었음 끝나고 나오니 ㅋㅋㅋㅋㅋ",
          "original_form": "55분이었음 끝나고 나오니 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044698.1.14",
          "form": "ㅋㅋㅋㅋㅋ오늘 드러머가 백신맞았다는 티엠아와 함께...",
          "original_form": "ㅋㅋㅋㅋㅋ오늘 드러머가 백신맞았다는 티엠아와 함께...",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044698.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ드러머분 연주하실때마다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ드러머분 연주하실때마다",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044698.1.16",
          "form": "팔 울리셨겠는데",
          "original_form": "팔 울리셨겠는데",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044698.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044698.1.18",
          "form": "오늘 스티커 자랑하심",
          "original_form": "오늘 스티커 자랑하심",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044698.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044698.1.20",
          "form": "동호는 잔여로 맞았나 신기하네",
          "original_form": "동호는 잔여로 맞았나 신기하네",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044698.1.21",
          "form": "스티커?",
          "original_form": "스티커?",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044698.1.22",
          "form": "아 주사맞으면",
          "original_form": "아 주사맞으면",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044698.1.23",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044698.1.24",
          "form": "붙여주는 그거",
          "original_form": "붙여주는 그거",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044698.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044698.1.26",
          "form": "동호 예약 안했대?",
          "original_form": "동호 예약 안했대?",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044698.1.27",
          "form": "반창고",
          "original_form": "반창고",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044698.1.28",
          "form": "아 스티커 주는구나",
          "original_form": "아 스티커 주는구나",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044698.1.29",
          "form": "아 뽀로로같은거?",
          "original_form": "아 뽀로로같은거?",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044698.1.30",
          "form": "모르게어 시기상 벌써 2차를 맞는거면",
          "original_form": "모르게어 시기상 벌써 2차를 맞는거면",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044698.1.31",
          "form": "잔여일듯",
          "original_form": "잔여일듯",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044698.1.32",
          "form": "네 그거 ㅋㅋㅋ",
          "original_form": "네 그거 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044698.1.33",
          "form": "뽀로로 스티커 받으면 뿌듯하지...",
          "original_form": "뽀로로 스티커 받으면 뿌듯하지...",
          "speaker_id": "1",
