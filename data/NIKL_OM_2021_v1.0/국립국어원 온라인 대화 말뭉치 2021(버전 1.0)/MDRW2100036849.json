      "utterance": [
        {
          "id": "MDRW2100036849.1.1",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210831 13:40"
        },
        {
          "id": "MDRW2100036849.1.2",
          "form": "네 간단히 먹었어요 점심 드셨어요?",
          "original_form": "네 간단히 먹었어요 점심 드셨어요?",
          "speaker_id": "1",
          "time": "20210831 13:40"
        },
        {
          "id": "MDRW2100036849.1.3",
          "form": "네 저는 우육탕면 먹었어요",
          "original_form": "네 저는 우육탕면 먹었어요",
          "speaker_id": "2",
          "time": "20210831 13:42"
        },
        {
          "id": "MDRW2100036849.1.4",
          "form": "우육탕면 좋아하시나요?",
          "original_form": "우육탕면 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210831 13:42"
        },
        {
          "id": "MDRW2100036849.1.5",
          "form": "딱 한번 먹어봤어요 대만여행갔을때요",
          "original_form": "딱 한번 먹어봤어요 대만여행갔을때요",
          "speaker_id": "1",
          "time": "20210831 13:42"
        },
        {
          "id": "MDRW2100036849.1.6",
          "form": "꽤괜찮던데요 고기가많아서요",
          "original_form": "꽤괜찮던데요 고기가많아서요",
          "speaker_id": "1",
          "time": "20210831 13:42"
        },
        {
          "id": "MDRW2100036849.1.7",
          "form": "전 양념장 한가득 풀어서 먹는데",
          "original_form": "전 양념장 한가득 풀어서 먹는데",
          "speaker_id": "2",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100036849.1.8",
          "form": "맛있는 것 같아요 ㅎㅎ",
          "original_form": "맛있는 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100036849.1.9",
          "form": "우육탕을 직접 만드신거에요?",
          "original_form": "우육탕을 직접 만드신거에요?",
          "speaker_id": "1",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100036849.1.10",
          "form": "아뇨 식당에서 먹는데 양념장이 비치되어있어요!",
          "original_form": "아뇨 식당에서 먹는데 양념장이 비치되어있어요!",
          "speaker_id": "2",
          "time": "20210831 13:44"
        },
        {
          "id": "MDRW2100036849.1.11",
          "form": "아아아 넹 ㅎㅎ",
          "original_form": "아아아 넹 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 13:45"
        },
        {
          "id": "MDRW2100036849.1.12",
          "form": "여튼 고기가많아서 두툼한게 좋았어요",
          "original_form": "여튼 고기가많아서 두툼한게 좋았어요",
          "speaker_id": "1",
          "time": "20210831 13:45"
        },
        {
          "id": "MDRW2100036849.1.13",
          "form": "맞아요 두툼하죠 ㅎㅎ",
          "original_form": "맞아요 두툼하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:46"
        },
        {
          "id": "MDRW2100036849.1.14",
          "form": "네 맛도 무난하고",
          "original_form": "네 맛도 무난하고",
          "speaker_id": "1",
          "time": "20210831 13:47"
        },
        {
          "id": "MDRW2100036849.1.15",
          "form": "우리입맛에 딱히 거슬리진 않더라구요",
          "original_form": "우리입맛에 딱히 거슬리진 않더라구요",
          "speaker_id": "1",
          "time": "20210831 13:47"
        },
        {
          "id": "MDRW2100036849.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 13:49"
        },
        {
          "id": "MDRW2100036849.1.17",
          "form": "군만듀도 육즙가득하구요 ㅎㅎ",
          "original_form": "군만듀도 육즙가득하구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:49"
        },
        {
          "id": "MDRW2100036849.1.18",
          "form": "군만두 넘 먹고싶어요",
          "original_form": "군만두 넘 먹고싶어요",
          "speaker_id": "1",
          "time": "20210831 13:51"
