        {
          "id": "MDRW2100005994.1.1",
          "form": "운동하시나요",
          "original_form": "운동하시나요",
          "speaker_id": "1",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100005994.1.2",
          "form": "빨리 걷기를 합니다",
          "original_form": "빨리 걷기를 합니다",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100005994.1.3",
          "form": "얼마나 걸으시나여?",
          "original_form": "얼마나 걸으시나여?",
          "speaker_id": "1",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100005994.1.4",
          "form": "하루 1시간을 걸어요",
          "original_form": "하루 1시간을 걸어요",
          "speaker_id": "2",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100005994.1.5",
          "form": "주 5일 해요",
          "original_form": "주 5일 해요",
          "speaker_id": "2",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100005994.1.6",
          "form": "와 대단하시네요 주로  몇시에 걸으시나요",
          "original_form": "와 대단하시네요 주로  몇시에 걸으시나요",
          "speaker_id": "1",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100005994.1.7",
          "form": "저녁먹고9시에서 10시 까지 걸어요",
          "original_form": "저녁먹고9시에서 10시 까지 걸어요",
          "speaker_id": "2",
          "time": "20210907 18:40"
        },
        {
          "id": "MDRW2100005994.1.8",
          "form": "저녁 소화 잘 되시겠네요",
          "original_form": "저녁 소화 잘 되시겠네요",
          "speaker_id": "1",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100005994.1.9",
          "form": "네 소화도 잘되고 운동후 잠도 잘 와요",
          "original_form": "네 소화도 잘되고 운동후 잠도 잘 와요",
          "speaker_id": "2",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100005994.1.10",
          "form": "건강에 좋은 습관이네요 걸으신진 오래되셨나요?",
          "original_form": "건강에 좋은 습관이네요 걸으신진 오래되셨나요?",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100005994.1.11",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100005994.1.12",
          "form": "올초부터 시작 했어요",
          "original_form": "올초부터 시작 했어요",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100005994.1.13",
          "form": "그럼 벌써 반년이 지났네요",
          "original_form": "그럼 벌써 반년이 지났네요",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100005994.1.14",
          "form": "네 시간이 참 빠르네요",
          "original_form": "네 시간이 참 빠르네요",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100005994.1.15",
          "form": "꾸준하게 하는게 쉽지않으신데 알찬 한해를 보내십니다",
