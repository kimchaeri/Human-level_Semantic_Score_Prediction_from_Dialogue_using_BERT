        {
          "id": "MDRW2100046047.1.1",
          "form": "나 소개팅 할까봐",
          "original_form": "나 소개팅 할까봐",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046047.1.2",
          "form": "저번엔어땠어",
          "original_form": "저번엔어땠어",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046047.1.3",
          "form": "잘 안됐어",
          "original_form": "잘 안됐어",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046047.1.4",
          "form": "이번에 다시 해봐",
          "original_form": "이번에 다시 해봐",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046047.1.5",
          "form": "도전해볼까",
          "original_form": "도전해볼까",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046047.1.6",
          "form": "응원할게",
          "original_form": "응원할게",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046047.1.7",
          "form": "친한 친구가 소개시켜줬어",
          "original_form": "친한 친구가 소개시켜줬어",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046047.1.8",
          "form": "너의 취향을 잘 알겠다",
          "original_form": "너의 취향을 잘 알겠다",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046047.1.9",
          "form": "맞아 어떤 사람이 나올지 궁금해",
          "original_form": "맞아 어떤 사람이 나올지 궁금해",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046047.1.10",
          "form": "좋은 사람이면 좋겠어",
          "original_form": "좋은 사람이면 좋겠어",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046047.1.11",
          "form": "대화를 잘 나눠볼게",
          "original_form": "대화를 잘 나눠볼게",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046047.1.12",
          "form": "소개팅은 따를 거 같아",
          "original_form": "소개팅은 따를 거 같아",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046047.1.13",
          "form": "어려운 점이 많지",
          "original_form": "어려운 점이 많지",
