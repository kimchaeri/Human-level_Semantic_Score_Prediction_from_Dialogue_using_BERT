      "utterance": [
        {
          "id": "MDRW2100041648.1.1",
          "form": "name2님 혹시 반려동물을 기르시나요?",
          "original_form": "&name2&님 혹시 반려동물을 기르시나요?",
          "speaker_id": "1",
          "time": "20210906 13:50"
        },
        {
          "id": "MDRW2100041648.1.2",
          "form": "아니요! name1님은요??",
          "original_form": "아니요! &name1&님은요??",
          "speaker_id": "2",
          "time": "20210906 13:52"
        },
        {
          "id": "MDRW2100041648.1.3",
          "form": "저두요 ㅋㅋ",
          "original_form": "저두요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:52"
        },
        {
          "id": "MDRW2100041648.1.4",
          "form": "저는 사진보는건 좋아하는데",
          "original_form": "저는 사진보는건 좋아하는데",
          "speaker_id": "1",
          "time": "20210906 13:52"
        },
        {
          "id": "MDRW2100041648.1.5",
          "form": "실제론 동물을 무서워해요ㅠ",
          "original_form": "실제론 동물을 무서워해요ㅠ",
          "speaker_id": "1",
          "time": "20210906 13:52"
        },
        {
          "id": "MDRW2100041648.1.6",
          "form": "오, 저는 개한테 물린 적 있어서 무서워해요",
          "original_form": "오, 저는 개한테 물린 적 있어서 무서워해요",
          "speaker_id": "2",
          "time": "20210906 13:53"
        },
        {
          "id": "MDRW2100041648.1.7",
          "form": "사진은 저도 좋아해요ㅋㅋㅋ",
          "original_form": "사진은 저도 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:53"
        },
        {
          "id": "MDRW2100041648.1.8",
          "form": "헐!! 저도 개한테 물릴뻔! 한적있어서 무서워해요ㅠ",
          "original_form": "헐!! 저도 개한테 물릴뻔! 한적있어서 무서워해요ㅠ",
          "speaker_id": "1",
          "time": "20210906 13:55"
        },
        {
          "id": "MDRW2100041648.1.9",
          "form": "크게 다치시진않으셨나요? ㅠㅠㅠ",
          "original_form": "크게 다치시진않으셨나요? ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 13:55"
        },
        {
          "id": "MDRW2100041648.1.10",
          "form": "작은 개여서 손가락 살짝 물렸어요ㅋㅋㅋ",
          "original_form": "작은 개여서 손가락 살짝 물렸어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:55"
        },
        {
          "id": "MDRW2100041648.1.11",
          "form": "오우ㅜㅜㅜ그래두 다행이긴하지만",
          "original_form": "오우ㅜㅜㅜ그래두 다행이긴하지만",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041648.1.12",
          "form": "무서웠겠어요ㅠㅠ",
          "original_form": "무서웠겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041648.1.13",
          "form": "네, 그래서 그뒤로 무섭더라구요ㅜㅜ",
          "original_form": "네, 그래서 그뒤로 무섭더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041648.1.14",
          "form": "저도 어렸을때 옆집 강아지가",
          "original_form": "저도 어렸을때 옆집 강아지가",
          "speaker_id": "1",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041648.1.15",
          "form": "푸들인데 겁나 싸나운 푸들이어서",
          "original_form": "푸들인데 겁나 싸나운 푸들이어서",
          "speaker_id": "1",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041648.1.16",
          "form": "너무 짖고 맨날 쫓아와서 물려고하고 그래서ㅠ",
          "original_form": "너무 짖고 맨날 쫓아와서 물려고하고 그래서ㅠ",
          "speaker_id": "1",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041648.1.17",
          "form": "어렸을때 기억이 너무 강렬해서 지금도 무서워요ㅠㅠ",
          "original_form": "어렸을때 기억이 너무 강렬해서 지금도 무서워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041648.1.18",
          "form": "개물림사고나 사건이 많잖아요ㅜㅜ 주인들이 주의 좀 했으면",
          "original_form": "개물림사고나 사건이 많잖아요ㅜㅜ 주인들이 주의 좀 했으면",
          "speaker_id": "2",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041648.1.19",
          "form": "그니까요!!!! 가끔 정말 목숨을 잃는 사람도 나오잖아요",
          "original_form": "그니까요!!!! 가끔 정말 목숨을 잃는 사람도 나오잖아요",
          "speaker_id": "1",
          "time": "20210906 17:17"
        },
        {
          "id": "MDRW2100041648.1.20",
          "form": "너무 안타까워요ㅠㅠ",
          "original_form": "너무 안타까워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 17:17"
        },
        {
          "id": "MDRW2100041648.1.21",
          "form": "맞아요ㅜㅜ 개가 아무리 귀해도 사람에게 상처입히면 안되죠",
          "original_form": "맞아요ㅜㅜ 개가 아무리 귀해도 사람에게 상처입히면 안되죠",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100041648.1.22",
          "form": "맞아요ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100041648.1.23",
          "form": "조심히 잘 내려가셨나요? 비가 꽤많이오네요ㅠ",
          "original_form": "조심히 잘 내려가셨나요? 비가 꽤많이오네요ㅠ",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100041648.1.24",
          "form": "네네, 비오기 전에 내려왔어요",
