      "utterance": [
        {
          "id": "MDRW2100044737.1.1",
          "form": "약간 본보기식으로",
          "original_form": "약간 본보기식으로",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044737.1.2",
          "form": "가세연 잡으면",
          "original_form": "가세연 잡으면",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044737.1.3",
          "form": "다른 인간들도",
          "original_form": "다른 인간들도",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044737.1.4",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044737.1.5",
          "form": "좀 눈치보고 사리지 않을까",
          "original_form": "좀 눈치보고 사리지 않을까",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044737.1.6",
          "form": "제발....",
          "original_form": "제발....",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044737.1.7",
          "form": "그랬으면ㅡㅡ",
          "original_form": "그랬으면ㅡㅡ",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044737.1.8",
          "form": "아니 진짜 너무 아님말고 하는거",
          "original_form": "아니 진짜 너무 아님말고 하는거",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044737.1.9",
          "form": "너무싫어",
          "original_form": "너무싫어",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044737.1.10",
          "form": "그러고보니 김용호는 룸싸롱에서 갑질하던거 터져서",
          "original_form": "그러고보니 김용호는 룸싸롱에서 갑질하던거 터져서",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044737.1.11",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044737.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044737.1.13",
          "form": "눈믈의 ㅁㅁ쇼를 하고",
          "original_form": "눈믈의 ㅁㅁ쇼를 하고",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044737.1.14",
          "form": "그거 기사 본거같아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그거 기사 본거같아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044737.1.15",
          "form": "난리를 치면서",
          "original_form": "난리를 치면서",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044737.1.16",
          "form": "자숙을 한다니 어쩌니",
          "original_form": "자숙을 한다니 어쩌니",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044737.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ그리고 김건모 한예슬 이근 박수홍 그외 기타 거의 두자리수로",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그리고 김건모 한예슬 이근 박수홍 그외 기타 거의 두자리수로",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044737.1.18",
          "form": "어휴....",
          "original_form": "어휴....",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044737.1.19",
          "form": "연예인들한테 변호사끼고",
          "original_form": "연예인들한테 변호사끼고",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044737.1.20",
          "form": "줄고소 당했는데",
          "original_form": "줄고소 당했는데",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044737.1.21",
          "form": "김용호는 솔직히",
          "original_form": "김용호는 솔직히",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044737.1.22",
          "form": "다 억대고",
          "original_form": "다 억대고",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044737.1.23",
          "form": "지금까지 멀쩡히 활동한게",
          "original_form": "지금까지 멀쩡히 활동한게",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044737.1.24",
          "form": "이상할정도",
          "original_form": "이상할정도",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044737.1.25",
          "form": "김건모만해도 30억대래",
          "original_form": "김건모만해도 30억대래",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044737.1.26",
          "form": "아니 왜 30이야",
          "original_form": "아니 왜 30이야",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
