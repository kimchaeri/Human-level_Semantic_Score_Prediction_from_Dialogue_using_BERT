        },
        {
          "id": "MDRW2100043571.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043571.1.3",
          "form": "식사하셧어요?",
          "original_form": "식사하셧어요?",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043571.1.4",
          "form": "이제 하려고요 드셨나요?",
          "original_form": "이제 하려고요 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043571.1.5",
          "form": "네 저는 먹엇어요",
          "original_form": "네 저는 먹엇어요",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043571.1.6",
          "form": "뭐 드실거예요?",
          "original_form": "뭐 드실거예요?",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043571.1.7",
          "form": "저는 만둣국 먹을까 싶어요 ㅎㅎ",
          "original_form": "저는 만둣국 먹을까 싶어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043571.1.8",
          "form": "뭐 드셨나요",
          "original_form": "뭐 드셨나요",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043571.1.9",
          "form": "저는 비빔밥먹엇어요",
          "original_form": "저는 비빔밥먹엇어요",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043571.1.10",
          "form": "만두 좋아하세요?",
          "original_form": "만두 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043571.1.11",
          "form": "아 만두 얼마전에 인터넷으로 시켰더니 넘 많아서 ㅋㅋ",
          "original_form": "아 만두 얼마전에 인터넷으로 시켰더니 넘 많아서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043571.1.12",
          "form": "먹어없애려고요 ㅎㅎ",
          "original_form": "먹어없애려고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043571.1.13",
          "form": "ㅋㅋㅋㅋㅋ무슨만두 시키셧어요?",
          "original_form": "ㅋㅋㅋㅋㅋ무슨만두 시키셧어요?",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043571.1.14",
          "form": "얇은피만두 아시나요 풀무원꺼",
          "original_form": "얇은피만두 아시나요 풀무원꺼",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043571.1.15",
          "form": "네 알아요",
          "original_form": "네 알아요",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043571.1.16",
          "form": "만둣국에 딱이던데요",
          "original_form": "만둣국에 딱이던데요",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043571.1.17",
          "form": "그거 김치랑 고기 시켰는데 넘 많아요 근데 맛나요 ㅋㅋ",
          "original_form": "그거 김치랑 고기 시켰는데 넘 많아요 근데 맛나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043571.1.18",
          "form": "피얇고 간이조금세서 ㅋㅋㅋㅋ",
          "original_form": "피얇고 간이조금세서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043571.1.19",
          "form": "맞아요 짭짤해요",
          "original_form": "맞아요 짭짤해요",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043571.1.20",
          "form": "맞아요 전 비비고 주로 먹는데",
          "original_form": "맞아요 전 비비고 주로 먹는데",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043571.1.21",
          "form": "얇은피가 땡겨서 시킬 때가 잇어요 ㅋㅋㅋㅋ",
          "original_form": "얇은피가 땡겨서 시킬 때가 잇어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043571.1.22",
          "form": "아 비비고도 맛있죠 비비고야 뭐 워낙 잘 팔리니까 ㅋㅋ",
          "original_form": "아 비비고도 맛있죠 비비고야 뭐 워낙 잘 팔리니까 ㅋㅋ",
