      "utterance": [
        {
          "id": "MDRW2100034759.1.1",
          "form": "이따 좀 더 캄캄해지면 가려구요",
          "original_form": "이따 좀 더 캄캄해지면 가려구요",
          "speaker_id": "2",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100034759.1.2",
          "form": "온천천가나요",
          "original_form": "온천천가나요",
          "speaker_id": "1",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100034759.1.3",
          "form": "네네넹",
          "original_form": "네네넹",
          "speaker_id": "2",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100034759.1.4",
          "form": "저는 아파트 한바퀴 다녀왓어요 너무덥네요",
          "original_form": "저는 아파트 한바퀴 다녀왓어요 너무덥네요",
          "speaker_id": "1",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100034759.1.5",
          "form": "할머니오셔서 운동기구도 하고",
          "original_form": "할머니오셔서 운동기구도 하고",
          "speaker_id": "1",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100034759.1.6",
          "form": "오 할머니랑 같이 운동하셨군여",
          "original_form": "오 할머니랑 같이 운동하셨군여",
          "speaker_id": "2",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100034759.1.7",
          "form": "어머니 건강은 괜찮으신가요",
          "original_form": "어머니 건강은 괜찮으신가요",
          "speaker_id": "2",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100034759.1.8",
          "form": "네 엄마오셨어요",
          "original_form": "네 엄마오셨어요",
          "speaker_id": "1",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100034759.1.9",
          "form": "다행이네요! 별일없으셨죠?",
          "original_form": "다행이네요! 별일없으셨죠?",
          "speaker_id": "2",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100034759.1.10",
          "form": "네 하지만 지켜봐여할것같아요ㅠ",
          "original_form": "네 하지만 지켜봐여할것같아요ㅠ",
          "speaker_id": "1",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100034759.1.11",
          "form": "이제운동가세요? 아무래도 가족력인거같아요 당뇨 쪽",
          "original_form": "이제운동가세요? 아무래도 가족력인거같아요 당뇨 쪽",
          "speaker_id": "1",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100034759.1.12",
          "form": "여덟시에 갈거랍니다",
          "original_form": "여덟시에 갈거랍니다",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100034759.1.13",
          "form": "오 34분남았네요",
          "original_form": "오 34분남았네요",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100034759.1.14",
          "form": "그러네욬ㅋㅋㅋ 운동의 귀찮음이 있으나 건강해지기 위해...일어나야겠네요 ...",
          "original_form": "그러네욬ㅋㅋㅋ 운동의 귀찮음이 있으나 건강해지기 위해...일어나야겠네요 ...",
          "speaker_id": "2",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100034759.1.15",
          "form": "맞아요 꾸준해서 멋져요 name2님",
          "original_form": "맞아요 꾸준해서 멋져요 &name2&님",
          "speaker_id": "1",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100034759.1.16",
          "form": "하다보니 습관되네요 꾸준히 산책 더 잘다니는 name1님 ㅋㅋㅋㅋ",
          "original_form": "하다보니 습관되네요 꾸준히 산책 더 잘다니는 &name1&님 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100034759.1.17",
          "form": "산책 맬 가죠ㅠ 짧긴하지만",
          "original_form": "산책 맬 가죠ㅠ 짧긴하지만",
          "speaker_id": "1",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034759.1.18",
          "form": "넹 그것 역시 꾸준함",
          "original_form": "넹 그것 역시 꾸준함",
          "speaker_id": "2",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034759.1.19",
          "form": "아 여덟시 됐어요",
          "original_form": "아 여덟시 됐어요",
          "speaker_id": "2",
          "time": "20210828 19:59"
        }
      ]
