      "utterance": [
        {
          "id": "MDRW2100024081.1.1",
          "form": "아니 저 근데 전공이 사실 지리거든요",
          "original_form": "아니 저 근데 전공이 사실 지리거든요",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100024081.1.2",
          "form": "어머 지리...",
          "original_form": "어머 지리...",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100024081.1.3",
          "form": "지리시네요",
          "original_form": "지리시네요",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100024081.1.4",
          "form": "근데 전공 벌써 다 까먹었어요",
          "original_form": "근데 전공 벌써 다 까먹었어요",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100024081.1.5",
          "form": "괜찮아요",
          "original_form": "괜찮아요",
          "speaker_id": "1",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024081.1.6",
          "form": "뭐 주상절리 이런 것만 알고 있음 되죠",
          "original_form": "뭐 주상절리 이런 것만 알고 있음 되죠",
          "speaker_id": "1",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024081.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ와 얘기하는데 군데군데 모르겟네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ와 얘기하는데 군데군데 모르겟네",
          "speaker_id": "2",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024081.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024081.1.9",
          "form": "세계지리 가르치고 그랬는데",
          "original_form": "세계지리 가르치고 그랬는데",
          "speaker_id": "2",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024081.1.10",
          "form": "앗 한국지리 세계지리 전부다 전공인가요",
          "original_form": "앗 한국지리 세계지리 전부다 전공인가요",
          "speaker_id": "1",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024081.1.11",
          "form": "중학교에서 잇엇더니 바보되버렷네..",
          "original_form": "중학교에서 잇엇더니 바보되버렷네..",
          "speaker_id": "2",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024081.1.12",
          "form": "지리과면 한국지리 세계지리 다 가르쳐용!",
          "original_form": "지리과면 한국지리 세계지리 다 가르쳐용!",
          "speaker_id": "2",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024081.1.13",
          "form": "헐 ㅜ 몰랐어요... 그렇구나 대박",
          "original_form": "헐 ㅜ 몰랐어요... 그렇구나 대박",
          "speaker_id": "1",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024081.1.14",
          "form": "그럼 여행 다니시면요",
          "original_form": "그럼 여행 다니시면요",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024081.1.15",
          "form": "지리 지식 떠오르고 보이고 해요?",
          "original_form": "지리 지식 떠오르고 보이고 해요?",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024081.1.16",
          "form": "원래 다들",
          "original_form": "원래 다들",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024081.1.17",
          "form": "하천 같은데 가면 에헴 여기가 하안단구이군!",
          "original_form": "하천 같은데 가면 에헴 여기가 하안단구이군!",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024081.1.18",
          "form": "아는만큼 보이는거자나요",
          "original_form": "아는만큼 보이는거자나요",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024081.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024081.1.20",
          "form": "약간 좀 그래요",
          "original_form": "약간 좀 그래요",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024081.1.21",
          "form": "제주도에 막 동굴 들어갔을 때도",
          "original_form": "제주도에 막 동굴 들어갔을 때도",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024081.1.22",
          "form": "좋은 경치봐도 여긴 어떻게 형성된거겠군",
          "original_form": "좋은 경치봐도 여긴 어떻게 형성된거겠군",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024081.1.23",
          "form": "되게 잘 보이시겠다",
          "original_form": "되게 잘 보이시겠다",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024081.1.24",
          "form": "와 용암이 흐른 자국~",
          "original_form": "와 용암이 흐른 자국~",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024081.1.25",
          "form": "이러면서",
          "original_form": "이러면서",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024081.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ전 선생님한테 이상한 질문 많이하고 그랬어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ전 선생님한테 이상한 질문 많이하고 그랬어요",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024081.1.27",
          "form": "무슨 질문이여",
          "original_form": "무슨 질문이여",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024081.1.28",
          "form": "진짜 범상치 않다",
          "original_form": "진짜 범상치 않다",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024081.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024081.1.30",
          "form": "선생님! 제주도는 현무암 때문에 물 잘빠진다면서요? 그럼 사람들 식수나 용수는 어떻게 얻었어요? 농사는 못지었어요? 삼다수는 근데 왜 이리 많이 나요?",
          "original_form": "선생님! 제주도는 현무암 때문에 물 잘빠진다면서요? 그럼 사람들 식수나 용수는 어떻게 얻었어요? 농사는 못지었어요? 삼다수는 근데 왜 이리 많이 나요?",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024081.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ이런거...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ이런거...",
          "speaker_id": "1",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100024081.1.32",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100024081.1.33",
          "form": "근데 다 질문이 좋네요",
          "original_form": "근데 다 질문이 좋네요",
          "speaker_id": "2",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100024081.1.34",
          "form": "쌤들이 좋아하셧을듯",
          "original_form": "쌤들이 좋아하셧을듯",
          "speaker_id": "2",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100024081.1.35",
          "form": "사회랑 과학선생님 엄청 진땀 빼시고 그러셨는데",
          "original_form": "사회랑 과학선생님 엄청 진땀 빼시고 그러셨는데",
          "speaker_id": "1",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100024081.1.36",
          "form": "ㅋㅋㅋㅋㅋ앜ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ앜ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100024081.1.37",
          "form": "다 교과서에 있는 내용 질문하신건데",
          "original_form": "다 교과서에 있는 내용 질문하신건데",
          "speaker_id": "2",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100024081.1.38",
          "form": "제가 원하는 모범생 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "제가 원하는 모범생 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100024081.1.39",
          "form": "선생님! CA 2+랑 CO3 2- 이 탄산칼슘이면은 콜라랑 우유랑 섞으면 탄산칼슘 생겨요?",
          "original_form": "선생님! CA 2+랑 CO3 2- 이 탄산칼슘이면은 콜라랑 우유랑 섞으면 탄산칼슘 생겨요?",
          "speaker_id": "1",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100024081.1.40",
          "form": "이런거 막 물어보고 그랬는데 저보고 직접 사서 해보라고 하시던 선생님..ㅠㅠ",
          "original_form": "이런거 막 물어보고 그랬는데 저보고 직접 사서 해보라고 하시던 선생님..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100024081.1.41",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100024081.1.42",
          "form": "진짜 저 호기심천국이였어요 헿",
          "original_form": "진짜 저 호기심천국이였어요 헿",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100024081.1.43",
          "form": "그래서 이런 것들이 여행갈 때마다도 호기심도 생기고 궁금하기도 하고 그러더라구요~",
          "original_form": "그래서 이런 것들이 여행갈 때마다도 호기심도 생기고 궁금하기도 하고 그러더라구요~",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100024081.1.44",
          "form": "원래 질문이 좋은 사람들이 공부 잘하는 거잖아여",
          "original_form": "원래 질문이 좋은 사람들이 공부 잘하는 거잖아여",
          "speaker_id": "2",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100024081.1.45",
          "form": "ㅋㅋㅋㅋㅋ전 항상 왜를 달고 살면서도 딱히 궁금한게 없는데",
          "original_form": "ㅋㅋㅋㅋㅋ전 항상 왜를 달고 살면서도 딱히 궁금한게 없는데",
          "speaker_id": "2",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100024081.1.46",
          "form": "저는 아직 먼 것 같아요 ㅜㅜㅜ",
          "original_form": "저는 아직 먼 것 같아요 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 22:05"
        }
      ]
