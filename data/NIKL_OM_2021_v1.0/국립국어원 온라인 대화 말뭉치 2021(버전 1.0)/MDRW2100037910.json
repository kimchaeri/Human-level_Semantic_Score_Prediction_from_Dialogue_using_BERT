      "utterance": [
        {
          "id": "MDRW2100037910.1.1",
          "form": "나 휴학했잖아",
          "original_form": "나 휴학했잖아",
          "speaker_id": "2",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100037910.1.2",
          "form": "요즘도 대학교 비대면 해?",
          "original_form": "요즘도 대학교 비대면 해?",
          "speaker_id": "2",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100037910.1.3",
          "form": "응 나는 쫙 다 비대면이야",
          "original_form": "응 나는 쫙 다 비대면이야",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100037910.1.4",
          "form": "헐~ 부럽다",
          "original_form": "헐~ 부럽다",
          "speaker_id": "2",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100037910.1.5",
          "form": "근데 중간고사 치고나면",
          "original_form": "근데 중간고사 치고나면",
          "speaker_id": "1",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037910.1.6",
          "form": "두 개 정도는 대면으로 바뀔 것 같아",
          "original_form": "두 개 정도는 대면으로 바뀔 것 같아",
          "speaker_id": "1",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037910.1.7",
          "form": "아~ 그럼 부산 있어야겠네",
          "original_form": "아~ 그럼 부산 있어야겠네",
          "speaker_id": "2",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100037910.1.8",
          "form": "그치 그래서 10월쯤에 다시 자취방 갈 것 같아",
          "original_form": "그치 그래서 10월쯤에 다시 자취방 갈 것 같아",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037910.1.9",
          "form": "나도 빨리 휴학하고 싶다ㅎㅎ",
          "original_form": "나도 빨리 휴학하고 싶다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037910.1.10",
          "form": "그치 대학로망은",
          "original_form": "그치 대학로망은",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100037910.1.11",
          "form": "cc 이딴게 아니라 휴학이지",
          "original_form": "cc 이딴게 아니라 휴학이지",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100037910.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ그치 cc는 진짜 아니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그치 cc는 진짜 아니다",
          "speaker_id": "1",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100037910.1.13",
          "form": "학과나 동아리 같은 사람이랑 연애하는 건 진짜 아님",
          "original_form": "학과나 동아리 같은 사람이랑 연애하는 건 진짜 아님",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100037910.1.14",
          "form": "그치 cc는 아니지",
          "original_form": "그치 cc는 아니지",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100037910.1.15",
          "form": "경험자가 말하니까 또",
          "original_form": "경험자가 말하니까 또",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100037910.1.16",
          "form": "무게감이 다르고",
          "original_form": "무게감이 다르고",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100037910.1.17",
          "form": "ㅋㅋㅋㅋㅋ 슬프네...",
          "original_form": "ㅋㅋㅋㅋㅋ 슬프네...",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100037910.1.18",
          "form": "첨엔 뭐 상관 없지 않나? 했는데",
          "original_form": "첨엔 뭐 상관 없지 않나? 했는데",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100037910.1.19",
          "form": "엄청 상관 있더라...ㅎㅎ",
          "original_form": "엄청 상관 있더라...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100037910.1.20",
          "form": "역시 교내 커플은 좀",
          "original_form": "역시 교내 커플은 좀",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100037910.1.21",
          "form": "그렇지...",
          "original_form": "그렇지...",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100037910.1.22",
          "form": "수강신청 잘했어?",
          "original_form": "수강신청 잘했어?",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100037910.1.23",
          "form": "두 과목인가 잡고 나머지는 다 망했었는데",
          "original_form": "두 과목인가 잡고 나머지는 다 망했었는데",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100037910.1.24",
          "form": "과사에 전화해서 겨우겨우 채웠어ㅋㅋㅋ",
          "original_form": "과사에 전화해서 겨우겨우 채웠어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100037910.1.25",
          "form": "다행이다ㅋㅋㅋㅋ 전필이었어??",
          "original_form": "다행이다ㅋㅋㅋㅋ 전필이었어??",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037910.1.26",
          "form": "응응 망한 게 전필들 ㅎㅎ",
          "original_form": "응응 망한 게 전필들 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037910.1.27",
          "form": "아니 전화했는데 있잖아?",
          "original_form": "아니 전화했는데 있잖아?",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037910.1.28",
          "form": "받으시는 분이 증원 못 해드릴 수 있다~ 하면서 겁줘서",
          "original_form": "받으시는 분이 증원 못 해드릴 수 있다~ 하면서 겁줘서",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037910.1.29",
          "form": "쫄았었어",
          "original_form": "쫄았었어",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037910.1.30",
          "form": "다들 밑밥깔고 시작하시는구나",
          "original_form": "다들 밑밥깔고 시작하시는구나",
          "speaker_id": "2",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100037910.1.31",
          "form": "우리 조교님도 그러셨다고 들었어",
          "original_form": "우리 조교님도 그러셨다고 들었어",
          "speaker_id": "2",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100037910.1.32",
          "form": "그래? 그냥 열어주겠다 했다가",
          "original_form": "그래? 그냥 열어주겠다 했다가",
          "speaker_id": "1",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100037910.1.33",
          "form": "나중에 못 열어주실까봐 그러시나",
          "original_form": "나중에 못 열어주실까봐 그러시나",
          "speaker_id": "1",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100037910.1.34",
          "form": "그렇겠지... 조교님들도 고생이시다",
          "original_form": "그렇겠지... 조교님들도 고생이시다",
          "speaker_id": "2",
          "time": "20210901 21:00"
        }
      ]
