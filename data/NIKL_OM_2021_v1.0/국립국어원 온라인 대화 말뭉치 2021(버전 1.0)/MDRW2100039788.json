      "utterance": [
        {
          "id": "MDRW2100039788.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039788.1.2",
          "form": "좋아해요!",
          "original_form": "좋아해요!",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039788.1.3",
          "form": "안좋아하는 사람이 있을까요?",
          "original_form": "안좋아하는 사람이 있을까요?",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039788.1.4",
          "form": "가끔 있더라고요 ㅎㅎ 제친구중에 있어서 ㅎㅎㅎㅎㅎ",
          "original_form": "가끔 있더라고요 ㅎㅎ 제친구중에 있어서 ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039788.1.5",
          "form": "코로나 전에 놀러다녀오신 곳 기억나세요?",
          "original_form": "코로나 전에 놀러다녀오신 곳 기억나세요?",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039788.1.6",
          "form": "오우 그 친구 생각이 궁금한데요?",
          "original_form": "오우 그 친구 생각이 궁금한데요?",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039788.1.7",
          "form": "코로나가 길어서....기억이안나네요",
          "original_form": "코로나가 길어서....기억이안나네요",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039788.1.8",
          "form": "코로나 시기에 제주도를 갔다오긴했어요",
          "original_form": "코로나 시기에 제주도를 갔다오긴했어요",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039788.1.9",
          "form": "ㅋㅋㅋ 저도 궁금한 애에요 ...",
          "original_form": "ㅋㅋㅋ 저도 궁금한 애에요 ...",
          "speaker_id": "1",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039788.1.10",
          "form": "아 제주도 다녀오셨구나!",
          "original_form": "아 제주도 다녀오셨구나!",
          "speaker_id": "1",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039788.1.11",
          "form": "사람 많았나요? 제 친구는 코로나에 제주도 갔다가 잠만 자고 왔다고 그러더라고요 ㅋㅋㅋㅋ",
          "original_form": "사람 많았나요? 제 친구는 코로나에 제주도 갔다가 잠만 자고 왔다고 그러더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039788.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ네 궁금한 친구 ㅋㅋㅋㅋ 물어보시지 ㅎㅎㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ네 궁금한 친구 ㅋㅋㅋㅋ 물어보시지 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039788.1.13",
          "form": "네.....와 사람 많더라구요",
          "original_form": "네.....와 사람 많더라구요",
          "speaker_id": "2",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039788.1.14",
          "form": "다들 제주도만 오나 생각했어요",
          "original_form": "다들 제주도만 오나 생각했어요",
          "speaker_id": "2",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039788.1.15",
          "form": "갔다온지 별로안되긴했는데",
          "original_form": "갔다온지 별로안되긴했는데",
          "speaker_id": "2",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039788.1.16",
          "form": "신혼여행도 거의 제주도로 갈거 같고 제주도가 잘될수밖에 없겠네요 ㅋㅋㅋㅋㅋ",
          "original_form": "신혼여행도 거의 제주도로 갈거 같고 제주도가 잘될수밖에 없겠네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039788.1.17",
          "form": "맞아욬ㅋㅋㅋ 갈때가 제주도뿐이라...신혼여행은 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋ 갈때가 제주도뿐이라...신혼여행은 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039788.1.18",
          "form": "해외여행 두번밖에 못가봤는데 코로나 터져서 아쉬워요 ㅠㅠ",
          "original_form": "해외여행 두번밖에 못가봤는데 코로나 터져서 아쉬워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039788.1.19",
          "form": "ㅠㅠㅠㅠㅠ내년에 가면되죠 ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ내년에 가면되죠 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039788.1.20",
          "form": "내년에 갈수 있을까여 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "내년에 갈수 있을까여 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039788.1.21",
          "form": "유럽 한번 가보고 싶었는데",
          "original_form": "유럽 한번 가보고 싶었는데",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039788.1.22",
          "form": "사실 아니요..",
          "original_form": "사실 아니요..",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039788.1.23",
          "form": "동양인 혐오할거 같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "동양인 혐오할거 같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039788.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039788.1.25",
          "form": "내년도 못갈거같은데요",
          "original_form": "내년도 못갈거같은데요",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039788.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039788.1.27",
          "form": "오우..중국인덕분에 그럴거같기도해요",
          "original_form": "오우..중국인덕분에 그럴거같기도해요",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039788.1.28",
          "form": "한국인이라고 써붙이고다닐수도럾고",
          "original_form": "한국인이라고 써붙이고다닐수도럾고",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039788.1.29",
          "form": "그러니까요 일생에 도움이 안되는 중국인들",
          "original_form": "그러니까요 일생에 도움이 안되는 중국인들",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039788.1.30",
          "form": "흑흑 어쩔수 없지만 그렇네요",
          "original_form": "흑흑 어쩔수 없지만 그렇네요",
