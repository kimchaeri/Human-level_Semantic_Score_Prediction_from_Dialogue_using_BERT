      "utterance": [
        {
          "id": "MDRW2100039572.1.1",
          "form": "혹시 여행 가고 싶은 곳 있으신가용",
          "original_form": "혹시 여행 가고 싶은 곳 있으신가용",
          "speaker_id": "1",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039572.1.2",
          "form": "저 여수 가고싶어요!",
          "original_form": "저 여수 가고싶어요!",
          "speaker_id": "2",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039572.1.3",
          "form": "여수 가보셨어요?",
          "original_form": "여수 가보셨어요?",
          "speaker_id": "2",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039572.1.4",
          "form": "저는 아직 여수 한번도 안가봣어요",
          "original_form": "저는 아직 여수 한번도 안가봣어요",
          "speaker_id": "2",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039572.1.5",
          "form": "저도 못가봤어요ㅠ",
          "original_form": "저도 못가봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039572.1.6",
          "form": "혹시 해외로는 가보고 싶은 곳 없으세요?",
          "original_form": "혹시 해외로는 가보고 싶은 곳 없으세요?",
          "speaker_id": "1",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039572.1.7",
          "form": "아하! 저는 해외는 미국이요!!",
          "original_form": "아하! 저는 해외는 미국이요!!",
          "speaker_id": "2",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039572.1.8",
          "form": "갈뻔했었는데 못가니까 너무 가보고싶어요",
          "original_form": "갈뻔했었는데 못가니까 너무 가보고싶어요",
          "speaker_id": "2",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039572.1.9",
          "form": "가보고싶은 여행지있으세요?",
          "original_form": "가보고싶은 여행지있으세요?",
          "speaker_id": "2",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039572.1.10",
          "form": "저는 옛날에 태국 한번 갔었는데 다시 가고 싶네요 ㅠ",
          "original_form": "저는 옛날에 태국 한번 갔었는데 다시 가고 싶네요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 19:42"
        },
        {
          "id": "MDRW2100039572.1.11",
          "form": "미국 어디로 가려고 하셨어요?",
          "original_form": "미국 어디로 가려고 하셨어요?",
          "speaker_id": "1",
          "time": "20210903 19:42"
        },
        {
          "id": "MDRW2100039572.1.12",
          "form": "막 정해놓은건 아닌데 친구가 학교를 미국쪽으로 가려고 했어서",
          "original_form": "막 정해놓은건 아닌데 친구가 학교를 미국쪽으로 가려고 했어서",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100039572.1.13",
          "form": "나갈 기회가 있었는데 바로 직후에 코로나가 터져서 못나갛었어요",
          "original_form": "나갈 기회가 있었는데 바로 직후에 코로나가 터져서 못나갛었어요",
          "speaker_id": "2",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039572.1.14",
          "form": "아이고ㅠ",
          "original_form": "아이고ㅠ",
          "speaker_id": "1",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100039572.1.15",
          "form": "코로나 터지기 전에 여행을 많이 다녀놔야 됐어요..",
          "original_form": "코로나 터지기 전에 여행을 많이 다녀놔야 됐어요..",
          "speaker_id": "1",
          "time": "20210903 20:11"
        },
        {
          "id": "MDRW2100039572.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 20:14"
        },
        {
          "id": "MDRW2100039572.1.17",
          "form": "저도 그땐 여행 별로 암좋아해서 많이 안다녓거든요ㅠㅠ",
          "original_form": "저도 그땐 여행 별로 암좋아해서 많이 안다녓거든요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 20:14"
        },
        {
          "id": "MDRW2100039572.1.18",
          "form": "ㅋㅋ ㅠㅠ",
          "original_form": "ㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039572.1.19",
          "form": "코로나 풀려서 여수 가면",
          "original_form": "코로나 풀려서 여수 가면",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039572.1.20",
          "form": "뭐 해보고 싶으세요?",
          "original_form": "뭐 해보고 싶으세요?",
          "speaker_id": "1",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039572.1.21",
          "form": "호텔",
          "original_form": "호텔",
          "speaker_id": "2",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039572.1.22",
          "form": "전 그냥 좋은 호텔에 숙박하면서 바다를 바라보고 싶습니다..",
          "original_form": "전 그냥 좋은 호텔에 숙박하면서 바다를 바라보고 싶습니다..",
          "speaker_id": "2",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039572.1.23",
          "form": "너무나 코시국에도 가능한 시나리오일까요?",
          "original_form": "너무나 코시국에도 가능한 시나리오일까요?",
          "speaker_id": "2",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039572.1.24",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039572.1.25",
          "form": "좀 잠잠해지면 가능할지도 모르겠네요!",
          "original_form": "좀 잠잠해지면 가능할지도 모르겠네요!",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100039572.1.26",
          "form": "그런게 힐링이죠 ㅎㅎ",
          "original_form": "그런게 힐링이죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100039572.1.27",
          "form": "근데 휴가철에 오션뷰 예약  잡기 참 힘들더라구요..ㅋㅋ",
          "original_form": "근데 휴가철에 오션뷰 예약  잡기 참 힘들더라구요..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100039572.1.28",
          "form": "맞아요! 그래도 이제 9월이라 극성수기는 지나가지않았나요?",
          "original_form": "맞아요! 그래도 이제 9월이라 극성수기는 지나가지않았나요?",
          "speaker_id": "2",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100039572.1.29",
          "form": "그죠 지금 잡으면 잘 잡힐지도 모르겠네요?",
          "original_form": "그죠 지금 잡으면 잘 잡힐지도 모르겠네요?",
          "speaker_id": "1",
          "time": "20210904 14:37"
        },
        {
          "id": "MDRW2100039572.1.30",
          "form": "근데 지금은 학생이라 더 이상 가기 힘들어서..ㅋㅋ 슬프네요ㅠ",
          "original_form": "근데 지금은 학생이라 더 이상 가기 힘들어서..ㅋㅋ 슬프네요ㅠ",
          "speaker_id": "1",
          "time": "20210904 14:37"
        },
        {
          "id": "MDRW2100039572.1.31",
          "form": "앗 대학생이세요?",
          "original_form": "앗 대학생이세요?",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100039572.1.32",
          "form": "대학생때 가는 여행이 진짜 좋은거같아요",
          "original_form": "대학생때 가는 여행이 진짜 좋은거같아요",
