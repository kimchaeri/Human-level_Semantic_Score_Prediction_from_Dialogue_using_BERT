      "utterance": [
        {
          "id": "MDRW2100008688.1.1",
          "form": "당분간 해외여행은 힘들겠죠?",
          "original_form": "당분간 해외여행은 힘들겠죠?",
          "speaker_id": "2",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008688.1.2",
          "form": "아무래도 코로나 종식 될것 같지도 않고",
          "original_form": "아무래도 코로나 종식 될것 같지도 않고",
          "speaker_id": "1",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008688.1.3",
          "form": "어려울 것 같아요",
          "original_form": "어려울 것 같아요",
          "speaker_id": "1",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008688.1.4",
          "form": "가장 최근 여행한 곳은 어디에요",
          "original_form": "가장 최근 여행한 곳은 어디에요",
          "speaker_id": "1",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008688.1.5",
          "form": "가까운곳은 부산이요",
          "original_form": "가까운곳은 부산이요",
          "speaker_id": "2",
          "time": "20210822 17:35"
        },
        {
          "id": "MDRW2100008688.1.6",
          "form": "동생이 살아서요 ㅎㅎ",
          "original_form": "동생이 살아서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:35"
        },
        {
          "id": "MDRW2100008688.1.7",
          "form": "아하 저도 부산은 자주가요",
          "original_form": "아하 저도 부산은 자주가요",
          "speaker_id": "1",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008688.1.8",
          "form": "부산 근처에 살아서",
          "original_form": "부산 근처에 살아서",
          "speaker_id": "1",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008688.1.9",
          "form": "이번 휴가도 부산으로 갈 예정",
          "original_form": "이번 휴가도 부산으로 갈 예정",
          "speaker_id": "1",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008688.1.10",
          "form": "그냥 바다 한번 보고와도 속이 시원하더라고요",
          "original_form": "그냥 바다 한번 보고와도 속이 시원하더라고요",
          "speaker_id": "2",
          "time": "20210822 17:43"
        },
        {
          "id": "MDRW2100008688.1.11",
          "form": "저는 제가 사는 동네에 출렁다리가생겨서 요새거기댕겨요",
          "original_form": "저는 제가 사는 동네에 출렁다리가생겨서 요새거기댕겨요",
          "speaker_id": "2",
          "time": "20210822 17:44"
        },
        {
          "id": "MDRW2100008688.1.12",
          "form": "국내 최장 출렁다리라고 지자체에서 엄청 밀고 있어요 ㅋㅋ",
          "original_form": "국내 최장 출렁다리라고 지자체에서 엄청 밀고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:44"
        },
        {
          "id": "MDRW2100008688.1.13",
          "form": "울산이세요?",
          "original_form": "울산이세요?",
          "speaker_id": "1",
          "time": "20210822 17:45"
        },
        {
          "id": "MDRW2100008688.1.14",
          "form": "오 어캐 아셨어요?",
          "original_form": "오 어캐 아셨어요?",
          "speaker_id": "2",
          "time": "20210822 17:52"
        },
        {
          "id": "MDRW2100008688.1.15",
          "form": "은근 밀고있는게 유명해진건가 드디어 전국구로",
          "original_form": "은근 밀고있는게 유명해진건가 드디어 전국구로",
          "speaker_id": "2",
          "time": "20210822 17:52"
        },
        {
          "id": "MDRW2100008688.1.16",
          "form": "저도 울산 살아서 알아요",
          "original_form": "저도 울산 살아서 알아요",
          "speaker_id": "1",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008688.1.17",
          "form": "대왕암 흔들바위",
          "original_form": "대왕암 흔들바위",
          "speaker_id": "1",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008688.1.18",
          "form": "흔들바위란다 출렁다리",
          "original_form": "흔들바위란다 출렁다리",
          "speaker_id": "1",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008688.1.19",
          "form": "세상 ㅋㅋㅋ 참좁군요",
          "original_form": "세상 ㅋㅋㅋ 참좁군요",
          "speaker_id": "2",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008688.1.20",
          "form": "사실 오픈빨이라 사람많은가 싶기도 ㅋㅋ",
          "original_form": "사실 오픈빨이라 사람많은가 싶기도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008688.1.21",
          "form": "지금 무료개방 아닌가요?",
          "original_form": "지금 무료개방 아닌가요?",
          "speaker_id": "1",
          "time": "20210823 08:03"
        },
        {
          "id": "MDRW2100008688.1.22",
          "form": "유료 전환하면 줄어들지 않을까요",
          "original_form": "유료 전환하면 줄어들지 않을까요",
          "speaker_id": "1",
          "time": "20210823 08:04"
        },
        {
          "id": "MDRW2100008688.1.23",
          "form": "어떤 스타일의 여행 좋아하세요?",
          "original_form": "어떤 스타일의 여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 08:04"
        },
        {
          "id": "MDRW2100008688.1.24",
          "form": "엑티비티? 휴양? 문화재 관람?",
          "original_form": "엑티비티? 휴양? 문화재 관람?",
          "speaker_id": "1",
          "time": "20210823 08:05"
        },
        {
          "id": "MDRW2100008688.1.25",
          "form": "액티비티는 진짜 아닌것같아요 제스타일",
          "original_form": "액티비티는 진짜 아닌것같아요 제스타일",
          "speaker_id": "2",
          "time": "20210823 09:45"
        },
        {
          "id": "MDRW2100008688.1.26",
          "form": "쉬거나",
          "original_form": "쉬거나",
          "speaker_id": "2",
          "time": "20210823 09:45"
        },
        {
          "id": "MDRW2100008688.1.27",
          "form": "문화재 관람 좋아해요!",
          "original_form": "문화재 관람 좋아해요!",
          "speaker_id": "2",
          "time": "20210823 09:45"
        },
        {
          "id": "MDRW2100008688.1.28",
          "form": "저도 박물관이나 미술관가는거 좋아해요",
          "original_form": "저도 박물관이나 미술관가는거 좋아해요",
          "speaker_id": "1",
          "time": "20210823 10:12"
        }
      ]
