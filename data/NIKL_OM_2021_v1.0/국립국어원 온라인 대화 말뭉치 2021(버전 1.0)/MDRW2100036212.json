      "utterance": [
        {
          "id": "MDRW2100036212.1.1",
          "form": "안녕하세요 저녁 식사는 하셨어요~?",
          "original_form": "안녕하세요 저녁 식사는 하셨어요~?",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036212.1.2",
          "form": "안녕하세요 소고기뭇국 방금 먹었어요",
          "original_form": "안녕하세요 소고기뭇국 방금 먹었어요",
          "speaker_id": "2",
          "time": "20210830 19:07"
        },
        {
          "id": "MDRW2100036212.1.3",
          "form": "뭐드셨나요",
          "original_form": "뭐드셨나요",
          "speaker_id": "2",
          "time": "20210830 19:07"
        },
        {
          "id": "MDRW2100036212.1.4",
          "form": "우와 너무 맛있는 메뉴네요~",
          "original_form": "우와 너무 맛있는 메뉴네요~",
          "speaker_id": "1",
          "time": "20210830 19:07"
        },
        {
          "id": "MDRW2100036212.1.5",
          "form": "저는 카레 먹었어요!",
          "original_form": "저는 카레 먹었어요!",
          "speaker_id": "1",
          "time": "20210830 19:07"
        },
        {
          "id": "MDRW2100036212.1.6",
          "form": "오 카레 내일은 감자사서 카레해야쥐",
          "original_form": "오 카레 내일은 감자사서 카레해야쥐",
          "speaker_id": "2",
          "time": "20210830 19:24"
        },
        {
          "id": "MDRW2100036212.1.7",
          "form": "카레에 뭐뭐 넣으세요?",
          "original_form": "카레에 뭐뭐 넣으세요?",
          "speaker_id": "1",
          "time": "20210830 19:26"
        },
        {
          "id": "MDRW2100036212.1.8",
          "form": "전 브로콜리 넣는데 다들 싫어하네요ㅋㅋㅋ",
          "original_form": "전 브로콜리 넣는데 다들 싫어하네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:26"
        },
        {
          "id": "MDRW2100036212.1.9",
          "form": "아 좋을듯해요",
          "original_form": "아 좋을듯해요",
          "speaker_id": "2",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100036212.1.10",
          "form": "전 고기 양파 감자 당근용",
          "original_form": "전 고기 양파 감자 당근용",
          "speaker_id": "2",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100036212.1.11",
          "form": "전 고기 양파 감자 양송이버섯 브로콜리 넣었어용ㅋㅋㅋ",
          "original_form": "전 고기 양파 감자 양송이버섯 브로콜리 넣었어용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100036212.1.12",
          "form": "오오 맛있겠다",
          "original_form": "오오 맛있겠다",
          "speaker_id": "2",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100036212.1.13",
          "form": "브로콜리 마니먹어야해요",
          "original_form": "브로콜리 마니먹어야해요",
          "speaker_id": "2",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100036212.1.14",
          "form": "넹넹 생으로 먹어도 맛있자나영!ㅋㅋㅋㅋ",
          "original_form": "넹넹 생으로 먹어도 맛있자나영!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100036212.1.15",
          "form": "저는 브로콜리 초장 조합 젤 조아해요",
          "original_form": "저는 브로콜리 초장 조합 젤 조아해요",
          "speaker_id": "2",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100036212.1.16",
          "form": "카레는 ...음 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "카레는 ...음 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100036212.1.17",
          "form": "음ㅋㅋㅋㅋ",
          "original_form": "음ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036212.1.18",
          "form": "파스타 좋아하세요~?",
          "original_form": "파스타 좋아하세요~?",
          "speaker_id": "1",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036212.1.19",
          "form": "네네 조아해요",
          "original_form": "네네 조아해요",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036212.1.20",
          "form": "크림파수터에 브로콜리도 조아요",
          "original_form": "크림파수터에 브로콜리도 조아요",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036212.1.21",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036212.1.22",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036212.1.23",
          "form": "브로콜리러버네용 ! ㅋㅋ",
          "original_form": "브로콜리러버네용 ! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036212.1.24",
          "form": "파스타 면 삶는게",
          "original_form": "파스타 면 삶는게",
          "speaker_id": "1",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036212.1.25",
          "form": "건강하고 좋아요",
          "original_form": "건강하고 좋아요",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036212.1.26",
          "form": "너무 힘들어서",
          "original_form": "너무 힘들어서",
          "speaker_id": "1",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036212.1.27",
          "form": "파스타면 쿠커를 샀는데",
          "original_form": "파스타면 쿠커를 샀는데",
          "speaker_id": "1",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036212.1.28",
          "form": "이거사고 파스타 진짜 많이 해먹어요 ㅡㅋㅋㅋ",
          "original_form": "이거사고 파스타 진짜 많이 해먹어요 ㅡㅋㅋㅋ",
