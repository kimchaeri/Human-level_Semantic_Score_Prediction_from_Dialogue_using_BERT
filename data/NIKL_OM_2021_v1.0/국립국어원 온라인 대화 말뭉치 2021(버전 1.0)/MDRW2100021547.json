      "utterance": [
        {
          "id": "MDRW2100021547.1.1",
          "form": "영화 자주 보세요?",
          "original_form": "영화 자주 보세요?",
          "speaker_id": "1",
          "time": "20210828 20:14"
        },
        {
          "id": "MDRW2100021547.1.2",
          "form": "가끔 보고싶은 영화 생기면 보는편이에요!",
          "original_form": "가끔 보고싶은 영화 생기면 보는편이에요!",
          "speaker_id": "2",
          "time": "20210828 20:22"
        },
        {
          "id": "MDRW2100021547.1.3",
          "form": "최근본 영화는 뭔가요?",
          "original_form": "최근본 영화는 뭔가요?",
          "speaker_id": "1",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100021547.1.4",
          "form": "크루엘라 봤어요",
          "original_form": "크루엘라 봤어요",
          "speaker_id": "2",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100021547.1.5",
          "form": "오 저도 크루엘라요ㅎ",
          "original_form": "오 저도 크루엘라요ㅎ",
          "speaker_id": "1",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100021547.1.6",
          "form": "지루한 장면이 하나도 없을정도로 재밌었어요!",
          "original_form": "지루한 장면이 하나도 없을정도로 재밌었어요!",
          "speaker_id": "2",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100021547.1.7",
          "form": "오 name1도 크루엘라 보셨군요",
          "original_form": "오 &name1&도 크루엘라 보셨군요",
          "speaker_id": "2",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100021547.1.8",
          "form": "디즈니 영화 좋아하세요?",
          "original_form": "디즈니 영화 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100021547.1.9",
          "form": "저도 크루엘라 재밌게봤어요ㅎ 디즈니영화 정말 좋아해요",
          "original_form": "저도 크루엘라 재밌게봤어요ㅎ 디즈니영화 정말 좋아해요",
          "speaker_id": "1",
          "time": "20210828 20:46"
        },
        {
          "id": "MDRW2100021547.1.10",
          "form": "저도요!! 디즈니랑 픽사 영화 정말 재밌어요",
          "original_form": "저도요!! 디즈니랑 픽사 영화 정말 재밌어요",
          "speaker_id": "2",
          "time": "20210828 20:52"
        },
        {
          "id": "MDRW2100021547.1.11",
          "form": "가장 재미있게 본 디즈니영화는 뭔가요??",
          "original_form": "가장 재미있게 본 디즈니영화는 뭔가요??",
          "speaker_id": "2",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021547.1.12",
          "form": "최애 디즈니영화는 라이온킹 2d버전이요ㅎ 라푼젤 3d도 좋아해요",
          "original_form": "최애 디즈니영화는 라이온킹 2d버전이요ㅎ 라푼젤 3d도 좋아해요",
          "speaker_id": "1",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021547.1.13",
          "form": "저는 인사이드아웃이랑 라따뚜이 업 토이스토리 너무 좋아해요ㅎㅎ",
          "original_form": "저는 인사이드아웃이랑 라따뚜이 업 토이스토리 너무 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:00"
        },
        {
          "id": "MDRW2100021547.1.14",
          "form": "다 재밌게본 영화들이네요ㅎㅎ",
          "original_form": "다 재밌게본 영화들이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100021547.1.15",
          "form": "토이스토리4는 아직안봤지만요ㅎ",
          "original_form": "토이스토리4는 아직안봤지만요ㅎ",
          "speaker_id": "1",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021547.1.16",
          "form": "저도 토이스토리4 아직 안봤어요",
          "original_form": "저도 토이스토리4 아직 안봤어요",
          "speaker_id": "2",
          "time": "20210828 21:14"
        },
        {
          "id": "MDRW2100021547.1.17",
          "form": "뭔가 못보겠더라고요ㅎㅎ",
          "original_form": "뭔가 못보겠더라고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:14"
        },
        {
          "id": "MDRW2100021547.1.18",
          "form": "3 엔딩이 너무 좋아서 그런것같아요",
          "original_form": "3 엔딩이 너무 좋아서 그런것같아요",
          "speaker_id": "2",
          "time": "20210828 21:14"
        },
        {
          "id": "MDRW2100021547.1.19",
          "form": "저도요 전 3이 마지막이라니까 보면서 펑펑 울었거든요",
          "original_form": "저도요 전 3이 마지막이라니까 보면서 펑펑 울었거든요",
