      "utterance": [
        {
          "id": "MDRW2100003578.1.1",
          "form": "최근 영화보신거 있나요?",
          "original_form": "최근 영화보신거 있나요?",
          "speaker_id": "2",
          "time": "20210821 02:33"
        },
        {
          "id": "MDRW2100003578.1.2",
          "form": "저 최근에 모가디슈 봤어요!",
          "original_form": "저 최근에 모가디슈 봤어요!",
          "speaker_id": "1",
          "time": "20210821 02:34"
        },
        {
          "id": "MDRW2100003578.1.3",
          "form": "name2님은 영화 보신거있나요?",
          "original_form": "&name2&님은 영화 보신거있나요?",
          "speaker_id": "1",
          "time": "20210821 02:34"
        },
        {
          "id": "MDRW2100003578.1.4",
          "form": "오 저도 그거봤어요 일욜에",
          "original_form": "오 저도 그거봤어요 일욜에",
          "speaker_id": "2",
          "time": "20210821 02:34"
        },
        {
          "id": "MDRW2100003578.1.5",
          "form": "우와 진짜 재밌었죠!!",
          "original_form": "우와 진짜 재밌었죠!!",
          "speaker_id": "1",
          "time": "20210821 02:35"
        },
        {
          "id": "MDRW2100003578.1.6",
          "form": "오랜만에 재밌는 영화 나온거같아요 ㅋㅋㅋ",
          "original_form": "오랜만에 재밌는 영화 나온거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 02:35"
        },
        {
          "id": "MDRW2100003578.1.7",
          "form": "볼거없어서 봤는데 생각보다 잼있더라구요 ㅎ",
          "original_form": "볼거없어서 봤는데 생각보다 잼있더라구요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 02:35"
        },
        {
          "id": "MDRW2100003578.1.8",
          "form": "지루할틈없이 총쏘고ㅎ",
          "original_form": "지루할틈없이 총쏘고ㅎ",
          "speaker_id": "2",
          "time": "20210821 02:35"
        },
        {
          "id": "MDRW2100003578.1.9",
          "form": "아 ㅋㅋㅋㅋㅋ맞아요",
          "original_form": "아 ㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210821 02:36"
        },
        {
          "id": "MDRW2100003578.1.10",
          "form": "생각보다 재미있다는게 진짜 딱 맞는말같아요",
          "original_form": "생각보다 재미있다는게 진짜 딱 맞는말같아요",
          "speaker_id": "1",
          "time": "20210821 02:36"
        },
        {
          "id": "MDRW2100003578.1.11",
          "form": "그리고 조인성 잘생겼어요 ㅋㅋㅋㅋ!!",
          "original_form": "그리고 조인성 잘생겼어요 ㅋㅋㅋㅋ!!",
          "speaker_id": "1",
          "time": "20210821 02:36"
        },
        {
          "id": "MDRW2100003578.1.12",
          "form": "맞아요ㅎ 연기도 잘하던데요",
          "original_form": "맞아요ㅎ 연기도 잘하던데요",
          "speaker_id": "2",
          "time": "20210821 02:36"
        },
        {
          "id": "MDRW2100003578.1.13",
          "form": "그러니까요 ㅋㅋㅋ",
          "original_form": "그러니까요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 02:37"
        },
        {
          "id": "MDRW2100003578.1.14",
          "form": "조인성 잘생긴줄만 알았는데",
          "original_form": "조인성 잘생긴줄만 알았는데",
          "speaker_id": "1",
          "time": "20210821 02:37"
        },
        {
          "id": "MDRW2100003578.1.15",
          "form": "연기도 잘해서 더 좋아졌어요",
          "original_form": "연기도 잘해서 더 좋아졌어요",
          "speaker_id": "1",
          "time": "20210821 02:37"
        },
        {
          "id": "MDRW2100003578.1.16",
          "form": "보고만 있어도 흐뭇하죠ㅎㅎ",
          "original_form": "보고만 있어도 흐뭇하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 02:38"
        },
        {
          "id": "MDRW2100003578.1.17",
          "form": "싱크홀은 보셨어요?",
          "original_form": "싱크홀은 보셨어요?",
          "speaker_id": "2",
          "time": "20210821 02:38"
        },
        {
          "id": "MDRW2100003578.1.18",
          "form": "싱크홀도 영화인가요?",
          "original_form": "싱크홀도 영화인가요?",
          "speaker_id": "1",
          "time": "20210821 02:38"
        },
        {
          "id": "MDRW2100003578.1.19",
          "form": "예매권 하나있어서 다른것좀 볼까하는데",
          "original_form": "예매권 하나있어서 다른것좀 볼까하는데",
          "speaker_id": "2",
          "time": "20210821 02:38"
        },
        {
          "id": "MDRW2100003578.1.20",
          "form": "네 차승원이랑 이광수나와요",
          "original_form": "네 차승원이랑 이광수나와요",
          "speaker_id": "2",
          "time": "20210821 02:38"
        },
        {
          "id": "MDRW2100003578.1.21",
          "form": "아 ! 우와 처음 알았어요",
          "original_form": "아 ! 우와 처음 알았어요",
          "speaker_id": "1",
          "time": "20210821 02:39"
        },
        {
          "id": "MDRW2100003578.1.22",
          "form": "약간 코믹하고 감동도 있고 그런가봐요",
          "original_form": "약간 코믹하고 감동도 있고 그런가봐요",
          "speaker_id": "2",
          "time": "20210821 02:40"
        },
        {
          "id": "MDRW2100003578.1.23",
          "form": "우와 둘 다 연기 잘하니까",
          "original_form": "우와 둘 다 연기 잘하니까",
          "speaker_id": "1",
          "time": "20210821 02:40"
        },
        {
          "id": "MDRW2100003578.1.24",
          "form": "보고싶어지네요",
          "original_form": "보고싶어지네요",
          "speaker_id": "1",
          "time": "20210821 02:40"
        },
        {
          "id": "MDRW2100003578.1.25",
          "form": "막 추천은 아니고 킬링타임용으로 괜찮대요",
          "original_form": "막 추천은 아니고 킬링타임용으로 괜찮대요",
          "speaker_id": "2",
          "time": "20210821 02:41"
        },
        {
          "id": "MDRW2100003578.1.26",
          "form": "우와 추천 감사해요!",
          "original_form": "우와 추천 감사해요!",
          "speaker_id": "1",
          "time": "20210821 02:42"
        }
      ]
