      "utterance": [
        {
          "id": "MDRW2100032441.1.1",
          "form": "책 자주 읽으시나요?!",
          "original_form": "책 자주 읽으시나요?!",
          "speaker_id": "1",
          "time": "20210905 17:41"
        },
        {
          "id": "MDRW2100032441.1.2",
          "form": "아뇨 ㅎ 웹소설만봐요",
          "original_form": "아뇨 ㅎ 웹소설만봐요",
          "speaker_id": "2",
          "time": "20210905 17:41"
        },
        {
          "id": "MDRW2100032441.1.3",
          "form": "책 좋아하세요?",
          "original_form": "책 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 17:41"
        },
        {
          "id": "MDRW2100032441.1.4",
          "form": "웹소설 뭐 보세요?",
          "original_form": "웹소설 뭐 보세요?",
          "speaker_id": "1",
          "time": "20210905 17:41"
        },
        {
          "id": "MDRW2100032441.1.5",
          "form": "네 저는 좋아합니다",
          "original_form": "네 저는 좋아합니다",
          "speaker_id": "1",
          "time": "20210905 17:41"
        },
        {
          "id": "MDRW2100032441.1.6",
          "form": "카카오페이지에서",
          "original_form": "카카오페이지에서",
          "speaker_id": "2",
          "time": "20210905 17:42"
        },
        {
          "id": "MDRW2100032441.1.7",
          "form": "로판같은거만 주로봐요",
          "original_form": "로판같은거만 주로봐요",
          "speaker_id": "2",
          "time": "20210905 17:42"
        },
        {
          "id": "MDRW2100032441.1.8",
          "form": "어떤책 좋아하세요?",
          "original_form": "어떤책 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 17:42"
        },
        {
          "id": "MDRW2100032441.1.9",
          "form": "저는 추리소설이요",
          "original_form": "저는 추리소설이요",
          "speaker_id": "1",
          "time": "20210905 17:43"
        },
        {
          "id": "MDRW2100032441.1.10",
          "form": "저도 로판 자주 보는데 최애작 있으신가요?",
          "original_form": "저도 로판 자주 보는데 최애작 있으신가요?",
          "speaker_id": "1",
          "time": "20210905 17:43"
        },
        {
          "id": "MDRW2100032441.1.11",
          "form": "맨날 바뀌어요",
          "original_form": "맨날 바뀌어요",
          "speaker_id": "2",
          "time": "20210905 17:43"
        },
        {
          "id": "MDRW2100032441.1.12",
          "form": "맨날연재작보고",
          "original_form": "맨날연재작보고",
          "speaker_id": "2",
          "time": "20210905 17:43"
        },
        {
          "id": "MDRW2100032441.1.13",
          "form": "다음 내용 못기다려서 딴거보고해서 ㅋㅋ",
          "original_form": "다음 내용 못기다려서 딴거보고해서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:44"
        },
        {
          "id": "MDRW2100032441.1.14",
          "form": "최근에 어떤 작품 읽으셨어요?",
          "original_form": "최근에 어떤 작품 읽으셨어요?",
          "speaker_id": "1",
          "time": "20210905 17:44"
        },
        {
          "id": "MDRW2100032441.1.15",
          "form": "무슨 게임이랑 왔다갔다하는 내용이었어요",
          "original_form": "무슨 게임이랑 왔다갔다하는 내용이었어요",
          "speaker_id": "2",
          "time": "20210905 17:45"
        },
        {
          "id": "MDRW2100032441.1.16",
          "form": "추리소설은 뭐 좋아하세요?",
          "original_form": "추리소설은 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 17:45"
        },
        {
          "id": "MDRW2100032441.1.17",
          "form": "다 보는 편이에요",
          "original_form": "다 보는 편이에요",
          "speaker_id": "1",
          "time": "20210905 17:45"
        },
        {
          "id": "MDRW2100032441.1.18",
          "form": "싫어하는 장르 있으세요?",
          "original_form": "싫어하는 장르 있으세요?",
          "speaker_id": "1",
          "time": "20210905 17:46"
        },
        {
          "id": "MDRW2100032441.1.19",
          "form": "공포요 ㅋㅋㄱㄱ",
          "original_form": "공포요 ㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210905 17:46"
        },
        {
          "id": "MDRW2100032441.1.20",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
          "time": "20210905 17:46"
        },
        {
          "id": "MDRW2100032441.1.21",
          "form": "시도 좋아하세요?",
          "original_form": "시도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 17:46"
        },
        {
          "id": "MDRW2100032441.1.22",
          "form": "시는 서울시 같은거 ㅋㅋㅋ",
          "original_form": "시는 서울시 같은거 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:47"
        },
        {
          "id": "MDRW2100032441.1.23",
          "form": "그런거 좋아합니다",
          "original_form": "그런거 좋아합니다",
          "speaker_id": "2",
          "time": "20210905 17:47"
        }
