      "utterance": [
        {
          "id": "MDRW2100040998.1.1",
          "form": "저녁머드실건가요",
          "original_form": "저녁머드실건가요",
          "speaker_id": "2",
          "time": "20210905 16:32"
        },
        {
          "id": "MDRW2100040998.1.2",
          "form": "저는 토스트해먹었어요!",
          "original_form": "저는 토스트해먹었어요!",
          "speaker_id": "1",
          "time": "20210905 20:16"
        },
        {
          "id": "MDRW2100040998.1.3",
          "form": "저녁 뭐 드셧어요?",
          "original_form": "저녁 뭐 드셧어요?",
          "speaker_id": "1",
          "time": "20210905 20:16"
        },
        {
          "id": "MDRW2100040998.1.4",
          "form": "저짜장면먹었습니덩",
          "original_form": "저짜장면먹었습니덩",
          "speaker_id": "2",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100040998.1.5",
          "form": "토스트맛있겠네용",
          "original_form": "토스트맛있겠네용",
          "speaker_id": "2",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100040998.1.6",
          "form": "오 짜장면맛잇겟네요 탕수육도 드셧어요?",
          "original_form": "오 짜장면맛잇겟네요 탕수육도 드셧어요?",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100040998.1.7",
          "form": "당연하죠 사천짜장먹었는데",
          "original_form": "당연하죠 사천짜장먹었는데",
          "speaker_id": "2",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100040998.1.8",
          "form": "맵더라구요 ㅠㅠ",
          "original_form": "맵더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100040998.1.9",
          "form": "오 사천짜장 맛잇겟네요",
          "original_form": "오 사천짜장 맛잇겟네요",
          "speaker_id": "1",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100040998.1.10",
          "form": "매운거 잘 못드세요?",
          "original_form": "매운거 잘 못드세요?",
          "speaker_id": "1",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100040998.1.11",
          "form": "넹넹맵찔이입니당...ㅠㅠㅠ",
          "original_form": "넹넹맵찔이입니당...ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:31"
        },
        {
          "id": "MDRW2100040998.1.12",
          "form": "앗 그렇군요 ㅠㅠ",
          "original_form": "앗 그렇군요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 20:31"
        },
        {
          "id": "MDRW2100040998.1.13",
          "form": "탕수육은 찍먹이세여?",
          "original_form": "탕수육은 찍먹이세여?",
          "speaker_id": "1",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100040998.1.14",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100040998.1.15",
          "form": "찍든 붓든",
          "original_form": "찍든 붓든",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100040998.1.16",
          "form": "잘먹어요",
          "original_form": "잘먹어요",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100040998.1.17",
          "form": "저두에요 ㅋㅋㅋㅋ둘다맛잇어요",
          "original_form": "저두에요 ㅋㅋㅋㅋ둘다맛잇어요",
          "speaker_id": "1",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100040998.1.18",
          "form": "머든맛있어요 ㅠㅠ",
          "original_form": "머든맛있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 21:20"
        },
        {
          "id": "MDRW2100040998.1.19",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:21"
        },
        {
          "id": "MDRW2100040998.1.20",
          "form": "배달 최애 메뉴가 몬가요?",
          "original_form": "배달 최애 메뉴가 몬가요?",
          "speaker_id": "1",
          "time": "20210905 21:21"
        },
        {
          "id": "MDRW2100040998.1.21",
          "form": "저는 아주 다양하게 써요",
          "original_form": "저는 아주 다양하게 써요",
          "speaker_id": "2",
          "time": "20210905 21:22"
        },
        {
          "id": "MDRW2100040998.1.22",
          "form": "요근래에는 와플이 땡기더라구요 ㅠㅠㅠ",
          "original_form": "요근래에는 와플이 땡기더라구요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 21:22"
        },
        {
          "id": "MDRW2100040998.1.23",
          "form": "크 와플 맛잇죠 요즘에 엄청 다양하더라구요",
          "original_form": "크 와플 맛잇죠 요즘에 엄청 다양하더라구요",
          "speaker_id": "1",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100040998.1.24",
          "form": "맞어요 크로플이 유향이자나요",
          "original_form": "맞어요 크로플이 유향이자나요",
          "speaker_id": "2",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100040998.1.25",
          "form": "저는 그옛날느낌와플이 조터라구요!",
          "original_form": "저는 그옛날느낌와플이 조터라구요!",
          "speaker_id": "2",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100040998.1.26",
          "form": "저두요 ㅋㅋㅋㅋ얇은와플에 사과쨈 생크림!",
          "original_form": "저두요 ㅋㅋㅋㅋ얇은와플에 사과쨈 생크림!",
          "speaker_id": "1",
          "time": "20210905 21:27"
        }
      ]
    }
