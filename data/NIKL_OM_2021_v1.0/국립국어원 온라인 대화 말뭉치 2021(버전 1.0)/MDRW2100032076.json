      "utterance": [
        {
          "id": "MDRW2100032076.1.1",
          "form": "청약에 관심 있으신가요?",
          "original_form": "청약에 관심 있으신가요?",
          "speaker_id": "1",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100032076.1.2",
          "form": "아 청약 관심은 있는데",
          "original_form": "아 청약 관심은 있는데",
          "speaker_id": "2",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100032076.1.3",
          "form": "제가 조건이 안될 것 같아서",
          "original_form": "제가 조건이 안될 것 같아서",
          "speaker_id": "2",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100032076.1.4",
          "form": "청약도  공부를 많이해야한다고 하네요",
          "original_form": "청약도  공부를 많이해야한다고 하네요",
          "speaker_id": "1",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100032076.1.5",
          "form": "네 청약도 잘 알아보고",
          "original_form": "네 청약도 잘 알아보고",
          "speaker_id": "2",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100032076.1.6",
          "form": "또 운도 좋아야 한다고",
          "original_form": "또 운도 좋아야 한다고",
          "speaker_id": "2",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100032076.1.7",
          "form": "저는 유튜브보면서 열심히 알아보는데",
          "original_form": "저는 유튜브보면서 열심히 알아보는데",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100032076.1.8",
          "form": "쉽지는않네요",
          "original_form": "쉽지는않네요",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100032076.1.9",
          "form": "아 신혼부부신가요?",
          "original_form": "아 신혼부부신가요?",
          "speaker_id": "2",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100032076.1.10",
          "form": "신혼부부 특공 아니면 어렵다고 해서요ㅜ",
          "original_form": "신혼부부 특공 아니면 어렵다고 해서요ㅜ",
          "speaker_id": "2",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100032076.1.11",
          "form": "신혼부부도 자녀2명부터 가능성이 있다고 하드라구요",
          "original_form": "신혼부부도 자녀2명부터 가능성이 있다고 하드라구요",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100032076.1.12",
          "form": "제 지인은 아이 1명인데 특공 되기는 했어요",
          "original_form": "제 지인은 아이 1명인데 특공 되기는 했어요",
          "speaker_id": "2",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100032076.1.13",
          "form": "아~정말 운이좋으셨네요",
          "original_form": "아~정말 운이좋으셨네요",
          "speaker_id": "1",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100032076.1.14",
          "form": "청약되는분들 정말 부러워요",
          "original_form": "청약되는분들 정말 부러워요",
          "speaker_id": "1",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100032076.1.15",
          "form": "네 눈치싸움에 성공한 것 같다고",
          "original_form": "네 눈치싸움에 성공한 것 같다고",
          "speaker_id": "2",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100032076.1.16",
          "form": "청약 되면야 너무 좋죠~",
          "original_form": "청약 되면야 너무 좋죠~",
          "speaker_id": "2",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032076.1.17",
          "form": "요즘 내집 마련이 너무 힘드니까요ㅜ",
          "original_form": "요즘 내집 마련이 너무 힘드니까요ㅜ",
          "speaker_id": "2",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032076.1.18",
          "form": "부동산 가격도 많이 오르고",
          "original_form": "부동산 가격도 많이 오르고",
          "speaker_id": "2",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032076.1.19",
          "form": "제가사는지역의 사전청약은 경쟁률도 너무높아요ㅜㅠ",
          "original_form": "제가사는지역의 사전청약은 경쟁률도 너무높아요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032076.1.20",
          "form": "사전청약 당첨되기 쉽지않을듯요",
          "original_form": "사전청약 당첨되기 쉽지않을듯요",
          "speaker_id": "1",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032076.1.21",
          "form": "하루빨리 내집마련의 시간이 왔으면 좋겠어요ㅠ",
          "original_form": "하루빨리 내집마련의 시간이 왔으면 좋겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032076.1.22",
          "form": "요즘집값정말 미친듯요",
          "original_form": "요즘집값정말 미친듯요",
          "speaker_id": "1",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100032076.1.23",
          "form": "저도 수도권에 살고 있어서",
          "original_form": "저도 수도권에 살고 있어서",
          "speaker_id": "2",
          "time": "20210905 12:16"
        },
        {
          "id": "MDRW2100032076.1.24",
          "form": "직장도 수도권이고",
          "original_form": "직장도 수도권이고",
          "speaker_id": "2",
          "time": "20210905 12:16"
        },
        {
          "id": "MDRW2100032076.1.25",
          "form": "요즘 집 구하기 정말 어려워요",
          "original_form": "요즘 집 구하기 정말 어려워요",
          "speaker_id": "2",
          "time": "20210905 12:16"
        },
        {
          "id": "MDRW2100032076.1.26",
          "form": "사전청약 경쟁률도 어마어마하고요",
          "original_form": "사전청약 경쟁률도 어마어마하고요",
          "speaker_id": "2",
          "time": "20210905 12:16"
        },
        {
          "id": "MDRW2100032076.1.27",
          "form": "전세도 너무비싸드라구요",
          "original_form": "전세도 너무비싸드라구요",
          "speaker_id": "1",
          "time": "20210905 12:16"
        }
