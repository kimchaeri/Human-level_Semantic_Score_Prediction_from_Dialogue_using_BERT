      "utterance": [
        {
          "id": "MDRW2100022504.1.1",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022504.1.2",
          "form": "오늘은 점심을 일찍먹었어요!",
          "original_form": "오늘은 점심을 일찍먹었어요!",
          "speaker_id": "1",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022504.1.3",
          "form": "곱창전골에 라면사리넣고 밥까지!",
          "original_form": "곱창전골에 라면사리넣고 밥까지!",
          "speaker_id": "1",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022504.1.4",
          "form": "우와 낮부터 곱창전골 부러워요 ㅋㅋㅋㅋ",
          "original_form": "우와 낮부터 곱창전골 부러워요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022504.1.5",
          "form": "코스트코에서 곱창전골 사와서",
          "original_form": "코스트코에서 곱창전골 사와서",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022504.1.6",
          "form": "그대로 끓이기만 했어요ㅋㅋㅋㅋㅋ",
          "original_form": "그대로 끓이기만 했어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022504.1.7",
          "form": "점심 뭐 드셨어요?",
          "original_form": "점심 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022504.1.8",
          "form": "전 빨갛게 양념한 돼지갈비찜에 어제먹고 남은 소불고기요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 빨갛게 양념한 돼지갈비찜에 어제먹고 남은 소불고기요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022504.1.9",
          "form": "고기 더하기 고기ㅋㅋㅋㅋ 최곤데요?",
          "original_form": "고기 더하기 고기ㅋㅋㅋㅋ 최곤데요?",
          "speaker_id": "1",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100022504.1.10",
          "form": "아임리얼 주스 아세여??",
          "original_form": "아임리얼 주스 아세여??",
          "speaker_id": "1",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100022504.1.11",
          "form": "아 그거 딸기맛 좋아요 ㅜㅜ",
          "original_form": "아 그거 딸기맛 좋아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 15:17"
        },
        {
          "id": "MDRW2100022504.1.12",
          "form": "지금 마시고 있어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "지금 마시고 있어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:18"
        },
        {
          "id": "MDRW2100022504.1.13",
          "form": "저한테 진짜 비싼거같아서 큰맘먹고 하나 사왔는데",
          "original_form": "저한테 진짜 비싼거같아서 큰맘먹고 하나 사왔는데",
          "speaker_id": "1",
          "time": "20210829 15:18"
        },
        {
          "id": "MDRW2100022504.1.14",
          "form": "오랜만에 마셔서 그런가 엄청엄청 진해요ㅠㅠㅠ",
          "original_form": "오랜만에 마셔서 그런가 엄청엄청 진해요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022504.1.15",
          "form": "무슨맛사셨어요? ㅋㅋ",
          "original_form": "무슨맛사셨어요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022504.1.16",
          "form": "딸기요ㅋㅋㅋㅋ",
          "original_form": "딸기요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022504.1.17",
          "form": "딸기가 진리죠",
          "original_form": "딸기가 진리죠",
          "speaker_id": "1",
          "time": "20210829 15:19"
        }
      ]
