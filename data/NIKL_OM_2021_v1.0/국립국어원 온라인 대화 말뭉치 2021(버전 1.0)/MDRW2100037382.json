      "utterance": [
        {
          "id": "MDRW2100037382.1.1",
          "form": "너 에어팟 고장났자고했지?",
          "original_form": "너 에어팟 고장났자고했지?",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037382.1.2",
          "form": "나 며칠전에 갤버라 샀어",
          "original_form": "나 며칠전에 갤버라 샀어",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037382.1.3",
          "form": "응응 고장나서 산거야",
          "original_form": "응응 고장나서 산거야",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037382.1.4",
          "form": "우와 그건 써보니까 어때?",
          "original_form": "우와 그건 써보니까 어때?",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037382.1.5",
          "form": "갤버라는 갤럭시버즈라이브라고 삼성에서 나온거야",
          "original_form": "갤버라는 갤럭시버즈라이브라고 삼성에서 나온거야",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037382.1.6",
          "form": "좀 특이하게 생겼어",
          "original_form": "좀 특이하게 생겼어",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037382.1.7",
          "form": "노이즈캔슬링에 대해 호평이 많더라",
          "original_form": "노이즈캔슬링에 대해 호평이 많더라",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037382.1.8",
          "form": "콩모양이야",
          "original_form": "콩모양이야",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037382.1.9",
          "form": "애플 에어팟프로보다는 덜 답답해",
          "original_form": "애플 에어팟프로보다는 덜 답답해",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037382.1.10",
          "form": "귀엽겠다 다음에 실물 보여줘!",
          "original_form": "귀엽겠다 다음에 실물 보여줘!",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037382.1.11",
          "form": "너무 답답하면 머리아프거든",
          "original_form": "너무 답답하면 머리아프거든",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100037382.1.12",
          "form": "좋아!",
          "original_form": "좋아!",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100037382.1.13",
          "form": "맞아 이어폰을 자기에게 맞는 상품을 써야해",
          "original_form": "맞아 이어폰을 자기에게 맞는 상품을 써야해",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100037382.1.14",
          "form": "애플 제품은 너무 비싼데",
          "original_form": "애플 제품은 너무 비싼데",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100037382.1.15",
          "form": "합리적인 소비릉 한 거 같야",
          "original_form": "합리적인 소비릉 한 거 같야",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100037382.1.16",
          "form": "너는 이어폰을 오래 끼는편이야??",
          "original_form": "너는 이어폰을 오래 끼는편이야??",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100037382.1.17",
          "form": "응 난 이어폰쟁이야",
          "original_form": "응 난 이어폰쟁이야",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100037382.1.18",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100037382.1.19",
          "form": "ㅋㅋㅋㅋㅋ귀여운 별명이네",
          "original_form": "ㅋㅋㅋㅋㅋ귀여운 별명이네",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100037382.1.20",
          "form": "나는 요즘은 자주 안끼는 것 같아",
          "original_form": "나는 요즘은 자주 안끼는 것 같아",
          "speaker_id": "1",
          "time": "20210831 23:03"
        }
      ]
