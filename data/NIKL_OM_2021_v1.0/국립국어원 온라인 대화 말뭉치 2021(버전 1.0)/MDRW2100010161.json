        {
          "id": "MDRW2100010161.1.1",
          "form": "ㅇㅇ 새로 산 스피커 좋더라 음질이",
          "original_form": "ㅇㅇ 새로 산 스피커 좋더라 음질이",
          "speaker_id": "2",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010161.1.2",
          "form": "고럼 내가 열심히 검색해서 산 건데",
          "original_form": "고럼 내가 열심히 검색해서 산 건데",
          "speaker_id": "1",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010161.1.3",
          "form": "근데 음량이 플러스 마이너스 되어 있는 건 좀 불편해",
          "original_form": "근데 음량이 플러스 마이너스 되어 있는 건 좀 불편해",
          "speaker_id": "2",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010161.1.4",
          "form": "왜? 누르기만 하면 되는데",
          "original_form": "왜? 누르기만 하면 되는데",
          "speaker_id": "1",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010161.1.5",
          "form": "음량이 어디까지 올라간건지 표시가 안돼서..",
          "original_form": "음량이 어디까지 올라간건지 표시가 안돼서..",
          "speaker_id": "2",
          "time": "20210823 14:59"
        },
        {
          "id": "MDRW2100010161.1.6",
          "form": "아~ 그게 좀 그렇긴 하지",
          "original_form": "아~ 그게 좀 그렇긴 하지",
          "speaker_id": "1",
          "time": "20210823 14:59"
        },
        {
          "id": "MDRW2100010161.1.7",
          "form": "음량 돌리는 건 어디가 최대인지 알 수 있는데",
          "original_form": "음량 돌리는 건 어디가 최대인지 알 수 있는데",
          "speaker_id": "2",
          "time": "20210823 14:59"
        },
        {
          "id": "MDRW2100010161.1.8",
          "form": "ㅎㅎㅎ 아무것도 안보이니 답답하구나",
          "original_form": "ㅎㅎㅎ 아무것도 안보이니 답답하구나",
          "speaker_id": "1",
          "time": "20210823 14:59"
        },
        {
          "id": "MDRW2100010161.1.9",
          "form": "ㅇㅇ 음량이 몇에 가 있는건지 알 수가 없어 ㅋ",
          "original_form": "ㅇㅇ 음량이 몇에 가 있는건지 알 수가 없어 ㅋ",
          "speaker_id": "2",
          "time": "20210823 14:59"
        },
        {
          "id": "MDRW2100010161.1.10",
          "form": "그래도 브리츠 스피커가 음질은 좋으니까",
          "original_form": "그래도 브리츠 스피커가 음질은 좋으니까",
          "speaker_id": "1",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010161.1.11",
          "form": "웅 음질은 너무 만족해 ㅎㅎ",
          "original_form": "웅 음질은 너무 만족해 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010161.1.12",
          "form": "그리고 작아서 깔끔하잖아",
          "original_form": "그리고 작아서 깔끔하잖아",
          "speaker_id": "1",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010161.1.13",
          "form": "모니터에 살짝 걸치니 편한 것 같아",
          "original_form": "모니터에 살짝 걸치니 편한 것 같아",
          "speaker_id": "2",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010161.1.14",
          "form": "요즘 비가 너무 자주 와서 우산도 몇 개 더 사야겠어",
