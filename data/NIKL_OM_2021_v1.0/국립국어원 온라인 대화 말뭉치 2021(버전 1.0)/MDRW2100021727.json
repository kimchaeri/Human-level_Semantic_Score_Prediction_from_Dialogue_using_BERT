      "utterance": [
        {
          "id": "MDRW2100021727.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100021727.1.2",
          "form": "요즘 뭐 보시는거 있으신가요?",
          "original_form": "요즘 뭐 보시는거 있으신가요?",
          "speaker_id": "2",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021727.1.3",
          "form": "혹시 침착맨아니라구 아시나요",
          "original_form": "혹시 침착맨아니라구 아시나요",
          "speaker_id": "1",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100021727.1.4",
          "form": "유튜버아닌가요?",
          "original_form": "유튜버아닌가요?",
          "speaker_id": "2",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100021727.1.5",
          "form": "웹툰작가 이말년씨가 하는 방송이요",
          "original_form": "웹툰작가 이말년씨가 하는 방송이요",
          "speaker_id": "1",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100021727.1.6",
          "form": "맞아요 유튜버",
          "original_form": "맞아요 유튜버",
          "speaker_id": "1",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100021727.1.7",
          "form": "아 맞아요 맞아요! 알아요 얼굴도!",
          "original_form": "아 맞아요 맞아요! 알아요 얼굴도!",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021727.1.8",
          "form": "전 요즘 그거에 빠져있어요 ㅎ",
          "original_form": "전 요즘 그거에 빠져있어요 ㅎ",
          "speaker_id": "1",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021727.1.9",
          "form": "주로 어떤 컨텐츠에요?",
          "original_form": "주로 어떤 컨텐츠에요?",
          "speaker_id": "2",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021727.1.10",
          "form": "음 게임방송많이하시던데 전 게임안해서 관심없구",
          "original_form": "음 게임방송많이하시던데 전 게임안해서 관심없구",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021727.1.11",
          "form": "신과함께 주호민 작가님이랑",
          "original_form": "신과함께 주호민 작가님이랑",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021727.1.12",
          "form": "김풍 작가님이랑 같이 나오셔서",
          "original_form": "김풍 작가님이랑 같이 나오셔서",
          "speaker_id": "1",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021727.1.13",
          "form": "맞아요 두분 같이 하시던데!",
          "original_form": "맞아요 두분 같이 하시던데!",
          "speaker_id": "2",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021727.1.14",
          "form": "김풍도나와요? ㅋㅋ",
          "original_form": "김풍도나와요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021727.1.15",
          "form": "이런저런 이야기 하는거 재미나요",
          "original_form": "이런저런 이야기 하는거 재미나요",
          "speaker_id": "1",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021727.1.16",
          "form": "유투브 많이 보세요?",
          "original_form": "유투브 많이 보세요?",
          "speaker_id": "2",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021727.1.17",
          "form": "네 그래서 침펄풍이라고 하던데요",
          "original_form": "네 그래서 침펄풍이라고 하던데요",
          "speaker_id": "1",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021727.1.18",
          "form": "아! 헐 ㅋㅋㅋㅋ 침펄풍 들어봤는데",
          "original_form": "아! 헐 ㅋㅋㅋㅋ 침펄풍 들어봤는데",
          "speaker_id": "2",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021727.1.19",
          "form": "침착맨이 이말년씨구요",
          "original_form": "침착맨이 이말년씨구요",
          "speaker_id": "1",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021727.1.20",
          "form": "주호민작가님 민머리라",
          "original_form": "주호민작가님 민머리라",
          "speaker_id": "1",
