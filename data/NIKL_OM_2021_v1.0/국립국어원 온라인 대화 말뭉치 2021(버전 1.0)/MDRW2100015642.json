      "utterance": [
        {
          "id": "MDRW2100015642.1.1",
          "form": "내일 점심 뭐먹을까",
          "original_form": "내일 점심 뭐먹을까",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015642.1.2",
          "form": "먹고싶은거 있어?",
          "original_form": "먹고싶은거 있어?",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015642.1.3",
          "form": "나는 보쌈 먹고싶다ㅋㅋ",
          "original_form": "나는 보쌈 먹고싶다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015642.1.4",
          "form": "김치볶음밥 (내가먹고싶은거^^)",
          "original_form": "김치볶음밥 (내가먹고싶은거^^)",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015642.1.5",
          "form": ".엄마 김치볶음밥 잘만들잖아",
          "original_form": ".엄마 김치볶음밥 잘만들잖아",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015642.1.6",
          "form": "그니까 내가 요리할때 잘 보고 배우랬잖아~~^^",
          "original_form": "그니까 내가 요리할때 잘 보고 배우랬잖아~~^^",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015642.1.7",
          "form": "나 그 레시피 좀 알려줘 ㅎㅎ",
          "original_form": "나 그 레시피 좀 알려줘 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015642.1.8",
          "form": "나중에 알려줄게~~~",
          "original_form": "나중에 알려줄게~~~",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015642.1.9",
          "form": "아님 그냥 엄마가 해줘",
          "original_form": "아님 그냥 엄마가 해줘",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015642.1.10",
          "form": "너 이러면  나중에 요리 바보된다",
          "original_form": "너 이러면  나중에 요리 바보된다",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015642.1.11",
          "form": "괜찮아ㅎㅎ 천천히 배우면 돼지",
          "original_form": "괜찮아ㅎㅎ 천천히 배우면 돼지",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015642.1.12",
          "form": "나는 라면 잘끓여",
          "original_form": "나는 라면 잘끓여",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015642.1.13",
          "form": "맛있는거 한개라도 배워 엄마한테 해줘",
          "original_form": "맛있는거 한개라도 배워 엄마한테 해줘",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015642.1.14",
          "form": "엄마한테 라면을 대접하겠다는거야~?^^",
          "original_form": "엄마한테 라면을 대접하겠다는거야~?^^",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015642.1.15",
          "form": "라면 맛있잖아",
          "original_form": "라면 맛있잖아",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015642.1.16",
          "form": "알겠어 365일 라면만 해줄게",
          "original_form": "알겠어 365일 라면만 해줄게",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015642.1.17",
