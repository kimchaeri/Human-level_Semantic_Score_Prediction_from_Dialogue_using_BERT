      "utterance": [
        {
          "id": "MDRW2100026679.1.1",
          "form": "해외는 어디가보셧어용!",
          "original_form": "해외는 어디가보셧어용!",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026679.1.2",
          "form": "저는 동남아만 다닌거 같아요ㅎ",
          "original_form": "저는 동남아만 다닌거 같아요ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026679.1.3",
          "form": "동남아 ㅠㅠ 휴양하기넘조은거같앙6ㅇ",
          "original_form": "동남아 ㅠㅠ 휴양하기넘조은거같앙6ㅇ",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026679.1.4",
          "form": "전 베트남 가봣어용",
          "original_form": "전 베트남 가봣어용",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026679.1.5",
          "form": "오 저 베트남 좋아해여",
          "original_form": "오 저 베트남 좋아해여",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026679.1.6",
          "form": "다낭 가봤어요",
          "original_form": "다낭 가봤어요",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026679.1.7",
          "form": "어디갔다 오셨어요?",
          "original_form": "어디갔다 오셨어요?",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026679.1.8",
          "form": "저도 다낭 !그 근처랑 ㅋㅋㅋ",
          "original_form": "저도 다낭 !그 근처랑 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026679.1.9",
          "form": "또가고파요",
          "original_form": "또가고파요",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026679.1.10",
          "form": "호이안이요?ㅋ",
          "original_form": "호이안이요?ㅋ",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026679.1.11",
          "form": "앗마자요 ㅋㅋㅋ",
          "original_form": "앗마자요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026679.1.12",
          "form": "너무 좋져ㅋ",
          "original_form": "너무 좋져ㅋ",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026679.1.13",
          "form": "반미 너무 맛잇엇어요",
          "original_form": "반미 너무 맛잇엇어요",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026679.1.14",
          "form": "저는 음식이 맘에 들더라구여",
          "original_form": "저는 음식이 맘에 들더라구여",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026679.1.15",
          "form": "쌀국수랑..",
          "original_form": "쌀국수랑..",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026679.1.16",
          "form": "진짜 다 맛잇더라구요ㅋㅋ 한국인입맛",
          "original_form": "진짜 다 맛잇더라구요ㅋㅋ 한국인입맛",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026679.1.17",
          "form": "태국보다 베트남이 더 맛나더라구여",
          "original_form": "태국보다 베트남이 더 맛나더라구여",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026679.1.18",
          "form": "진짜진짜",
          "original_form": "진짜진짜",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026679.1.19",
          "form": "아뇨ㅋㅋ",
          "original_form": "아뇨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026679.1.20",
          "form": "태국은 재밌어여",
          "original_form": "태국은 재밌어여",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026679.1.21",
          "form": "다낭은 쉬기 너무 좋고",
          "original_form": "다낭은 쉬기 너무 좋고",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026679.1.22",
          "form": "진짜 ㅠ 태국은 재밋는게머가잇나요",
          "original_form": "진짜 ㅠ 태국은 재밋는게머가잇나요",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026679.1.23",
          "form": "저는 클럽 이런데 1도 안좋은데",
          "original_form": "저는 클럽 이런데 1도 안좋은데",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026679.1.24",
          "form": "거기 카오산로드에",
          "original_form": "거기 카오산로드에",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026679.1.25",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026679.1.26",
          "form": "술마시는 바?펍?",
          "original_form": "술마시는 바?펍?",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026679.1.27",
          "form": "카오산로드 말만들어본..",
          "original_form": "카오산로드 말만들어본..",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026679.1.28",
          "form": "오 ...",
          "original_form": "오 ...",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026679.1.29",
          "form": "어디가 좋은지 몰라서 몇군데 갔는데",
          "original_form": "어디가 좋은지 몰라서 몇군데 갔는데",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026679.1.30",
          "form": "럭키바라고 거기서 술마셨는데",
          "original_form": "럭키바라고 거기서 술마셨는데",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026679.1.31",
          "form": "서양인 동양인 태국사람 중국사람",
          "original_form": "서양인 동양인 태국사람 중국사람",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026679.1.32",
          "form": "다 만나거든요",
          "original_form": "다 만나거든요",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026679.1.33",
          "form": "ㅋㅋㅋㅋㅋ 다모엿네요",
          "original_form": "ㅋㅋㅋㅋㅋ 다모엿네요",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026679.1.34",
          "form": "근데 서양인은 우리 무시하고ㅋㅋ",
          "original_form": "근데 서양인은 우리 무시하고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026679.1.35",
          "form": "태국사람 중국사람이랑 놀았는데",
          "original_form": "태국사람 중국사람이랑 놀았는데",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026679.1.36",
          "form": "다들 한국말 가능하고",
          "original_form": "다들 한국말 가능하고",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026679.1.37",
          "form": "노래는 한국노래 나오고",
          "original_form": "노래는 한국노래 나오고",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026679.1.38",
          "form": "어떻게그러짘ㅋㅋㅋ",
          "original_form": "어떻게그러짘ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026679.1.39",
          "form": "무슨 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "무슨 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026679.1.40",
          "form": "그러니까요ㅋㅋ",
          "original_form": "그러니까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026679.1.41",
          "form": "무슨노래나와요",
          "original_form": "무슨노래나와요",
