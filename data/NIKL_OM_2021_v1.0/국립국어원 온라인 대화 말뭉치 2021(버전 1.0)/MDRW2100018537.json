      "utterance": [
        {
          "id": "MDRW2100018537.1.1",
          "form": "떡볶이 좋아해요?",
          "original_form": "떡볶이 좋아해요?",
          "speaker_id": "2",
          "time": "20210827 07:57"
        },
        {
          "id": "MDRW2100018537.1.2",
          "form": "저는 환장합니다",
          "original_form": "저는 환장합니다",
          "speaker_id": "2",
          "time": "20210827 07:57"
        },
        {
          "id": "MDRW2100018537.1.3",
          "form": "단것을 별로 안 좋아해서ㅜ",
          "original_form": "단것을 별로 안 좋아해서ㅜ",
          "speaker_id": "1",
          "time": "20210827 07:57"
        },
        {
          "id": "MDRW2100018537.1.4",
          "form": "매운 거 좋아하시나봐요",
          "original_form": "매운 거 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210827 07:57"
        },
        {
          "id": "MDRW2100018537.1.5",
          "form": "다이어트 중인데 회사 식사로 떡볶이 나오면 폭식각이죠",
          "original_form": "다이어트 중인데 회사 식사로 떡볶이 나오면 폭식각이죠",
          "speaker_id": "2",
          "time": "20210827 07:57"
        },
        {
          "id": "MDRW2100018537.1.6",
          "form": "그럼요ㅜㅜ 매운것도 좋아해요",
          "original_form": "그럼요ㅜㅜ 매운것도 좋아해요",
          "speaker_id": "2",
          "time": "20210827 07:57"
        },
        {
          "id": "MDRW2100018537.1.7",
          "form": "아ㅋㅋ  완전 다이어트의 적이라던데",
          "original_form": "아ㅋㅋ  완전 다이어트의 적이라던데",
          "speaker_id": "1",
          "time": "20210827 07:58"
        },
        {
          "id": "MDRW2100018537.1.8",
          "form": "회사가 잘못했네요",
          "original_form": "회사가 잘못했네요",
          "speaker_id": "1",
          "time": "20210827 07:58"
        },
        {
          "id": "MDRW2100018537.1.9",
          "form": "어느 브랜드 떡볶이 좋아해요?",
          "original_form": "어느 브랜드 떡볶이 좋아해요?",
          "speaker_id": "1",
          "time": "20210827 07:58"
        },
        {
          "id": "MDRW2100018537.1.10",
          "form": "맛있게 먹으면 0칼로리..",
          "original_form": "맛있게 먹으면 0칼로리..",
          "speaker_id": "2",
          "time": "20210827 07:58"
        },
        {
          "id": "MDRW2100018537.1.11",
          "form": "엄마표..",
          "original_form": "엄마표..",
          "speaker_id": "2",
          "time": "20210827 07:58"
        },
        {
          "id": "MDRW2100018537.1.12",
          "form": "어머니 요리솜씨가 좋은가봐요",
          "original_form": "어머니 요리솜씨가 좋은가봐요",
          "speaker_id": "1",
          "time": "20210827 07:58"
        },
        {
          "id": "MDRW2100018537.1.13",
          "form": "떡볶이는 맛있는 브랜드가 없었네요ㅜ",
          "original_form": "떡볶이는 맛있는 브랜드가 없었네요ㅜ",
          "speaker_id": "2",
          "time": "20210827 07:58"
        },
        {
          "id": "MDRW2100018537.1.14",
          "form": "엄마 요리가 최고에요ㅎㅎ",
          "original_form": "엄마 요리가 최고에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 07:59"
        },
        {
          "id": "MDRW2100018537.1.15",
          "form": "ㅎㅎ엽떡 많이 찾던데 거기는 별로인가봐요",
          "original_form": "ㅎㅎ엽떡 많이 찾던데 거기는 별로인가봐요",
          "speaker_id": "1",
          "time": "20210827 07:59"
        },
        {
          "id": "MDRW2100018537.1.16",
          "form": "네.. 개별로",
          "original_form": "네.. 개별로",
          "speaker_id": "2",
          "time": "20210827 07:59"
        },
        {
          "id": "MDRW2100018537.1.17",
          "form": "네. 별로입니다ㅜㅜ",
          "original_form": "네. 별로입니다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 07:59"
        },
        {
          "id": "MDRW2100018537.1.18",
          "form": "아. 걸작떡볶이 괜찮았어요",
          "original_form": "아. 걸작떡볶이 괜찮았어요",
          "speaker_id": "2",
          "time": "20210827 08:00"
        },
        {
          "id": "MDRW2100018537.1.19",
          "form": "아 토핑은 뭐 곁들이세요?",
          "original_form": "아 토핑은 뭐 곁들이세요?",
          "speaker_id": "1",
          "time": "20210827 08:00"
        },
        {
          "id": "MDRW2100018537.1.20",
          "form": "저번에 또봉이떡볶이였나.. 레토르트식품 주문해서 먹었는데 그건 맛났네요",
          "original_form": "저번에 또봉이떡볶이였나.. 레토르트식품 주문해서 먹었는데 그건 맛났네요",
          "speaker_id": "2",
          "time": "20210827 08:00"
        },
        {
          "id": "MDRW2100018537.1.21",
          "form": "오오 레토르트도 잘나오나봐요",
          "original_form": "오오 레토르트도 잘나오나봐요",
          "speaker_id": "1",
          "time": "20210827 08:00"
        },
        {
          "id": "MDRW2100018537.1.22",
          "form": "라면 오뎅 마늘편 납작만두",
          "original_form": "라면 오뎅 마늘편 납작만두",
          "speaker_id": "2",
          "time": "20210827 08:01"
        },
        {
          "id": "MDRW2100018537.1.23",
          "form": "배우신 분이네요",
          "original_form": "배우신 분이네요",
          "speaker_id": "1",
          "time": "20210827 08:01"
        },
        {
          "id": "MDRW2100018537.1.24",
          "form": "네 잘 나와요 추천추천",
          "original_form": "네 잘 나와요 추천추천",
          "speaker_id": "2",
          "time": "20210827 08:01"
        },
        {
          "id": "MDRW2100018537.1.25",
          "form": "전 옛날떡볶이가 그리워요",
          "original_form": "전 옛날떡볶이가 그리워요",
          "speaker_id": "1",
          "time": "20210827 08:01"
        }
      ]
    }
