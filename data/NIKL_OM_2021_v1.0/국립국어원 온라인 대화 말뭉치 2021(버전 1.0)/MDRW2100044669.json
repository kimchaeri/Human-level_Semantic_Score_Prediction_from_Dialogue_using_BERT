        {
          "id": "MDRW2100044669.1.1",
          "form": "털 알러지 있으세요?",
          "original_form": "털 알러지 있으세요?",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044669.1.2",
          "form": "없는 것 같아요",
          "original_form": "없는 것 같아요",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044669.1.3",
          "form": "루꼴라님은요?",
          "original_form": "루꼴라님은요?",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044669.1.4",
          "form": "저도 없어요",
          "original_form": "저도 없어요",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044669.1.5",
          "form": "저는 알러지 검사도 해봤어요",
          "original_form": "저는 알러지 검사도 해봤어요",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044669.1.6",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044669.1.7",
          "form": "반려 동물 키우시나요?",
          "original_form": "반려 동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044669.1.8",
          "form": "아니요 루꼴라님은 키우시나요?",
          "original_form": "아니요 루꼴라님은 키우시나요?",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044669.1.9",
          "form": "저두 없어요",
          "original_form": "저두 없어요",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044669.1.10",
          "form": "반려 동물 있으셔서 이 주제 고른 줄 알았어요",
          "original_form": "반려 동물 있으셔서 이 주제 고른 줄 알았어요",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044669.1.11",
          "form": "제가 고른 주제가 아니라 랜덤으로 주제가 정해지는거에요 ㅎㅎㅎ",
          "original_form": "제가 고른 주제가 아니라 랜덤으로 주제가 정해지는거에요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044669.1.12",
          "form": "아항ㅋㅋㅋㅋㅋ",
          "original_form": "아항ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044669.1.13",
          "form": "어릴 때 키운 동물도 없으신가요?",
          "original_form": "어릴 때 키운 동물도 없으신가요?",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044669.1.14",
          "form": "네ㅠㅠ",
          "original_form": "네ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044669.1.15",
          "form": "큰 일이네요",
          "original_form": "큰 일이네요",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044669.1.16",
          "form": "키우고 싶은 동물 있으신가요?",
          "original_form": "키우고 싶은 동물 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044669.1.17",
          "form": "저는 리트리버 같은 대형견 기르고 싶어요",
          "original_form": "저는 리트리버 같은 대형견 기르고 싶어요",
          "speaker_id": "1",
