      "utterance": [
        {
          "id": "MDRW2100043910.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043910.1.2",
          "form": "안녕하세요~~~~저녁드셨어요~~~~?",
          "original_form": "안녕하세요~~~~저녁드셨어요~~~~?",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043910.1.3",
          "form": "네! 닭볶음탕 먹엇습니다",
          "original_form": "네! 닭볶음탕 먹엇습니다",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043910.1.4",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043910.1.5",
          "form": "오 맛있는거 드셨네요!",
          "original_form": "오 맛있는거 드셨네요!",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043910.1.6",
          "form": "저는 닭꼬치 조금이랑 냉동 탕수육 먹었어요",
          "original_form": "저는 닭꼬치 조금이랑 냉동 탕수육 먹었어요",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043910.1.7",
          "form": "오 닭꼬치도 맛있으셨겠어요",
          "original_form": "오 닭꼬치도 맛있으셨겠어요",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043910.1.8",
          "form": "네네!!!",
          "original_form": "네네!!!",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043910.1.9",
          "form": "지금 유혹을 못참고",
          "original_form": "지금 유혹을 못참고",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043910.1.10",
          "form": "딸기요거트롤 먹고있어요",
          "original_form": "딸기요거트롤 먹고있어요",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043910.1.11",
          "form": "오 요거트 좋아하시나요?",
          "original_form": "오 요거트 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043910.1.12",
          "form": "눼눼",
          "original_form": "눼눼",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100043910.1.13",
          "form": "요거트 완전좋아해요!!!!!!",
          "original_form": "요거트 완전좋아해요!!!!!!",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100043910.1.14",
          "form": "그릭요거트도 잘먹어요",
          "original_form": "그릭요거트도 잘먹어요",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100043910.1.15",
          "form": "헉 그릭요거트는",
          "original_form": "헉 그릭요거트는",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043910.1.16",
          "form": "일반요거트와 맛이 다른가요?",
          "original_form": "일반요거트와 맛이 다른가요?",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043910.1.17",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043910.1.18",
          "form": "네 조금?",
          "original_form": "네 조금?",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043910.1.19",
          "form": "식감도 그렇고",
          "original_form": "식감도 그렇고",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043910.1.20",
          "form": "덜 시큼해요",
          "original_form": "덜 시큼해요",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043910.1.21",
          "form": "저는 요거트 시큼한 맛 때문에",
          "original_form": "저는 요거트 시큼한 맛 때문에",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043910.1.22",
          "form": "별로 안좋아했는데",
          "original_form": "별로 안좋아했는데",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043910.1.23",
          "form": "한번 먹어봐야겠네요",
          "original_form": "한번 먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043910.1.24",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043910.1.25",
          "form": "그게 유청(?) 그걸 뺀거라",
          "original_form": "그게 유청(?) 그걸 뺀거라",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043910.1.26",
          "form": "덜 시큼해요",
          "original_form": "덜 시큼해요",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043910.1.27",
          "form": "꾸덕하고",
          "original_form": "꾸덕하고",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043910.1.28",
          "form": "유청이 시큼한 거였군요",
          "original_form": "유청이 시큼한 거였군요",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100043910.1.29",
          "form": "그럼 치즈맛이 나나요?",
          "original_form": "그럼 치즈맛이 나나요?",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100043910.1.30",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100043910.1.31",
          "form": "그렇진않아요ㅎ",
          "original_form": "그렇진않아요ㅎ",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100043910.1.32",
          "form": "호불호가 갈리긴해요",
          "original_form": "호불호가 갈리긴해요",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100043910.1.33",
          "form": "요즘 복숭아랑 그릭요거트 같이 먹는 거 유행하더라고요",
          "original_form": "요즘 복숭아랑 그릭요거트 같이 먹는 거 유행하더라고요",
