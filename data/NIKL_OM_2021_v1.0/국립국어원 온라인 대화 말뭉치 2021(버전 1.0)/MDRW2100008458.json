      "utterance": [
        {
          "id": "MDRW2100008458.1.1",
          "form": "수강신청 잘됏어?",
          "original_form": "수강신청 잘됏어?",
          "speaker_id": "2",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008458.1.2",
          "form": "불행히도 잘 됐어",
          "original_form": "불행히도 잘 됐어",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008458.1.3",
          "form": "나는 왜 수강신청을 잘하는걸까",
          "original_form": "나는 왜 수강신청을 잘하는걸까",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008458.1.4",
          "form": "오오 공강은 언제야?",
          "original_form": "오오 공강은 언제야?",
          "speaker_id": "2",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008458.1.5",
          "form": "당연히 금요일이지",
          "original_form": "당연히 금요일이지",
          "speaker_id": "1",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008458.1.6",
          "form": "너는 수강신청 했어?",
          "original_form": "너는 수강신청 했어?",
          "speaker_id": "1",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008458.1.7",
          "form": "그렇군",
          "original_form": "그렇군",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008458.1.8",
          "form": "했지 나도 금공강",
          "original_form": "했지 나도 금공강",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008458.1.9",
          "form": "역시 공강은 금요일이지",
          "original_form": "역시 공강은 금요일이지",
          "speaker_id": "1",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008458.1.10",
          "form": "다음주 개강이라는 사실 믿겨지냐고",
          "original_form": "다음주 개강이라는 사실 믿겨지냐고",
          "speaker_id": "1",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008458.1.11",
          "form": "믿을수 없어",
          "original_form": "믿을수 없어",
          "speaker_id": "2",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008458.1.12",
          "form": "일주일이면 또 그 지옥같은 학교생활이 시작이라니..",
          "original_form": "일주일이면 또 그 지옥같은 학교생활이 시작이라니..",
          "speaker_id": "2",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008458.1.13",
          "form": "두번씩이나 이 거지같은 대학교에 버려지다니",
          "original_form": "두번씩이나 이 거지같은 대학교에 버려지다니",
          "speaker_id": "1",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008458.1.14",
          "form": "아효 근데 나는 비대면이라 또 집에서 수업들을듯",
          "original_form": "아효 근데 나는 비대면이라 또 집에서 수업들을듯",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008458.1.15",
          "form": "와 부럽다 나는 대면인데",
          "original_form": "와 부럽다 나는 대면인데",
          "speaker_id": "1",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008458.1.16",
          "form": "이론 수업같은건 좀 비대면 해주지",
          "original_form": "이론 수업같은건 좀 비대면 해주지",
          "speaker_id": "1",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008458.1.17",
          "form": "그건 그래 교수님들 정신 차렷!",
          "original_form": "그건 그래 교수님들 정신 차렷!",
          "speaker_id": "2",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008458.1.18",
          "form": "교수님들 눈치챙겨~!",
          "original_form": "교수님들 눈치챙겨~!",
          "speaker_id": "1",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008458.1.19",
          "form": "마스크도 똑바로 챙겨~!",
          "original_form": "마스크도 똑바로 챙겨~!",
          "speaker_id": "1",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008458.1.20",
          "form": "교수님들도 코로나라서 그런지 수업하는거에 어려움 겪는것 같아...",
          "original_form": "교수님들도 코로나라서 그런지 수업하는거에 어려움 겪는것 같아...",
          "speaker_id": "2",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008458.1.21",
          "form": "수업 들으면 맨날 이상한게 한두가지가 아녀 ㅋㅋㅋ",
          "original_form": "수업 들으면 맨날 이상한게 한두가지가 아녀 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:00"
        }
      ]
