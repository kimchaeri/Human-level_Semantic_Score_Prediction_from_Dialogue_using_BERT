      "utterance": [
        {
          "id": "MDRW2100002191.1.1",
          "form": "학교 다니니",
          "original_form": "학교 다니니",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002191.1.2",
          "form": "웅 대학생이야 넌?",
          "original_form": "웅 대학생이야 넌?",
          "speaker_id": "2",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002191.1.3",
          "form": "글쿠나 나는 이제 졸업해서 대학교 그립다 ㅠㅠ",
          "original_form": "글쿠나 나는 이제 졸업해서 대학교 그립다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002191.1.4",
          "form": "곧 개강하는데 학교가기싫어ㅠㅠ",
          "original_form": "곧 개강하는데 학교가기싫어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002191.1.5",
          "form": "2학기 시간표 짜는중인데 망한거같아",
          "original_form": "2학기 시간표 짜는중인데 망한거같아",
          "speaker_id": "2",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002191.1.6",
          "form": "그치 요즘 다 온라인수업하지?",
          "original_form": "그치 요즘 다 온라인수업하지?",
          "speaker_id": "1",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002191.1.7",
          "form": "온라인이랑 대면이랑 섞어서 해",
          "original_form": "온라인이랑 대면이랑 섞어서 해",
          "speaker_id": "2",
          "time": "20210820 00:26"
        },
        {
          "id": "MDRW2100002191.1.8",
          "form": "그렇구나 그럼 기숙사 살아?",
          "original_form": "그렇구나 그럼 기숙사 살아?",
          "speaker_id": "1",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002191.1.9",
          "form": "웅 기숙사!",
          "original_form": "웅 기숙사!",
          "speaker_id": "2",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002191.1.10",
          "form": "기숙사생활해본적있어?",
          "original_form": "기숙사생활해본적있어?",
          "speaker_id": "2",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002191.1.11",
          "form": "나는 시험기간엔 좀 불편한데 그거빼곤 좋은거같아",
          "original_form": "나는 시험기간엔 좀 불편한데 그거빼곤 좋은거같아",
          "speaker_id": "2",
          "time": "20210820 00:43"
        },
        {
          "id": "MDRW2100002191.1.12",
          "form": "기숙사 생활 안해봤어 ㅠㅠ",
          "original_form": "기숙사 생활 안해봤어 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 00:43"
        },
        {
          "id": "MDRW2100002191.1.13",
          "form": "시험기간엔 왜불편해?",
          "original_form": "시험기간엔 왜불편해?",
          "speaker_id": "1",
          "time": "20210820 00:43"
        },
        {
          "id": "MDRW2100002191.1.14",
          "form": "공부하다보면 자는시간이 좀 안맞아지더라구",
          "original_form": "공부하다보면 자는시간이 좀 안맞아지더라구",
          "speaker_id": "2",
          "time": "20210820 00:44"
        },
        {
          "id": "MDRW2100002191.1.15",
          "form": "근데 그것도 룸메나름이겠지?",
          "original_form": "근데 그것도 룸메나름이겠지?",
          "speaker_id": "2",
          "time": "20210820 00:44"
        },
        {
          "id": "MDRW2100002191.1.16",
          "form": "아 칭구랑 같이쓰니까? ㅠㅠ",
          "original_form": "아 칭구랑 같이쓰니까? ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 00:44"
        },
