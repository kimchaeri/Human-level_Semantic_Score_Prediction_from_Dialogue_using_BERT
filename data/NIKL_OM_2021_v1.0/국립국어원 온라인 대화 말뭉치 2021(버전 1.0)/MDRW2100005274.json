      "utterance": [
        {
          "id": "MDRW2100005274.1.1",
          "form": "제주도 좋니?",
          "original_form": "제주도 좋니?",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005274.1.2",
          "form": "제주도 여행지로 어때?",
          "original_form": "제주도 여행지로 어때?",
          "speaker_id": "1",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005274.1.3",
          "form": "제주도!! 좋지",
          "original_form": "제주도!! 좋지",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005274.1.4",
          "form": "한라산 가봤니?",
          "original_form": "한라산 가봤니?",
          "speaker_id": "1",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005274.1.5",
          "form": "아니 힘들다던데??",
          "original_form": "아니 힘들다던데??",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005274.1.6",
          "form": "그러면 어디가 좋아?",
          "original_form": "그러면 어디가 좋아?",
          "speaker_id": "1",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005274.1.7",
          "form": "제주도의 어디가 좋냐구?",
          "original_form": "제주도의 어디가 좋냐구?",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005274.1.8",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005274.1.9",
          "form": "흠 나도 가족들이 가자는 곳을 가서 잘 몰라",
          "original_form": "흠 나도 가족들이 가자는 곳을 가서 잘 몰라",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005274.1.10",
          "form": "올레길 좋다던데",
          "original_form": "올레길 좋다던데",
          "speaker_id": "1",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005274.1.11",
          "form": "너는 역시.. 걷는걸 좋아하는군",
          "original_form": "너는 역시.. 걷는걸 좋아하는군",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005274.1.12",
          "form": "안가봤니?",
          "original_form": "안가봤니?",
          "speaker_id": "1",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005274.1.13",
          "form": "차타고만 다녀서 안가본듯?",
          "original_form": "차타고만 다녀서 안가본듯?",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005274.1.14",
          "form": "차타는게 더 좋다! ㅎㅎ",
          "original_form": "차타는게 더 좋다! ㅎㅎ",
          "speaker_id": "1",
