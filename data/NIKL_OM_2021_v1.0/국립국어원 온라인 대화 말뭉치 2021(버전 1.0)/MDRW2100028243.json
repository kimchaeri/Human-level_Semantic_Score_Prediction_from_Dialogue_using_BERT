        {
          "id": "MDRW2100028243.1.1",
          "form": "넹 해봤어요",
          "original_form": "넹 해봤어요",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028243.1.2",
          "form": "ㅡ전 당근매니아",
          "original_form": "ㅡ전 당근매니아",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028243.1.3",
          "form": "중고나라도요",
          "original_form": "중고나라도요",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028243.1.4",
          "form": "지금은 라이언빔",
          "original_form": "지금은 라이언빔",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028243.1.5",
          "form": "온도 높으시나요",
          "original_form": "온도 높으시나요",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028243.1.6",
          "form": "사려고 보고잇어요",
          "original_form": "사려고 보고잇어요",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028243.1.7",
          "form": "저는 향수요",
          "original_form": "저는 향수요",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028243.1.8",
          "form": "43도인가그래요",
          "original_form": "43도인가그래요",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028243.1.9",
          "form": "거래가 없어 저는 낮아요",
          "original_form": "거래가 없어 저는 낮아요",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028243.1.10",
          "form": "향수는 그냥 모기향 훈연해서쓰세요",
          "original_form": "향수는 그냥 모기향 훈연해서쓰세요",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028243.1.11",
          "form": "주로 사기만 해요",
          "original_form": "주로 사기만 해요",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028243.1.12",
          "form": "사기만친다구요?",
          "original_form": "사기만친다구요?",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028243.1.13",
          "form": "팔진 않아요",
          "original_form": "팔진 않아요",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028243.1.14",
          "form": "전ㅈ판매도많이해요",
          "original_form": "전ㅈ판매도많이해요",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028243.1.15",
          "form": "판매할때 힘든건 없나요",
          "original_form": "판매할때 힘든건 없나요",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028243.1.16",
          "form": "최근에는 ㅁ을팔았어요",
          "original_form": "최근에는 ㅁ을팔았어요",
          "speaker_id": "1",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028243.1.17",
          "form": "진상이나",
          "original_form": "진상이나",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028243.1.18",
          "form": "음 깎아요 오늘도당했어요",
          "original_form": "음 깎아요 오늘도당했어요",
          "speaker_id": "1",
