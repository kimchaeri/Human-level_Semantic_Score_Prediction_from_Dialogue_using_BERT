      "utterance": [
        {
          "id": "MDRW2100005183.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005183.1.2",
          "form": "동호회나 봉사활동 해보신적 있으세요?",
          "original_form": "동호회나 봉사활동 해보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005183.1.3",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005183.1.4",
          "form": "봉사동아리 해본 적 있어요",
          "original_form": "봉사동아리 해본 적 있어요",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005183.1.5",
          "form": "우와 멋있어요! 저는 한번도 없는데",
          "original_form": "우와 멋있어요! 저는 한번도 없는데",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005183.1.6",
          "form": "어떤 활동 하셨어요?",
          "original_form": "어떤 활동 하셨어요?",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005183.1.7",
          "form": "학생들을 직접 가르치기도 하고",
          "original_form": "학생들을 직접 가르치기도 하고",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005183.1.8",
          "form": "멘토랑 멘티를",
          "original_form": "멘토랑 멘티를",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005183.1.9",
          "form": "매칭하는 역할을 하기도 했어요~",
          "original_form": "매칭하는 역할을 하기도 했어요~",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005183.1.10",
          "form": "아하! 저는 동아리 말고 혼자 봉사활동은 해봣는데",
          "original_form": "아하! 저는 동아리 말고 혼자 봉사활동은 해봣는데",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005183.1.11",
          "form": "나중에 여유가되면 봉사 동호회 만들어서 유기견 봉사 하고싶어요",
          "original_form": "나중에 여유가되면 봉사 동호회 만들어서 유기견 봉사 하고싶어요",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005183.1.12",
          "form": "혼자 봉사활동은 어떤 거 해보셨어요?",
          "original_form": "혼자 봉사활동은 어떤 거 해보셨어요?",
          "speaker_id": "2",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005183.1.13",
          "form": "애완동물 키우시나요?",
          "original_form": "애완동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005183.1.14",
          "form": "봉사활동은 요양원 같은 곳 가서 할머니 할아버지들 도와주는 활동했었어요!",
          "original_form": "봉사활동은 요양원 같은 곳 가서 할머니 할아버지들 도와주는 활동했었어요!",
          "speaker_id": "1",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005183.1.15",
          "form": "반려견은 안키우고 반려묘만 키우고있어요",
          "original_form": "반려견은 안키우고 반려묘만 키우고있어요",
          "speaker_id": "1",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005183.1.16",
          "form": "등산 동호회는 해보실 생각 있으세요?",
          "original_form": "등산 동호회는 해보실 생각 있으세요?",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005183.1.17",
          "form": "오, 요양원에서 봉사하셨구나",
          "original_form": "오, 요양원에서 봉사하셨구나",
          "speaker_id": "2",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005183.1.18",
          "form": "저는 나이먹으면 등산동호회 나가고싶어요",
          "original_form": "저는 나이먹으면 등산동호회 나가고싶어요",
          "speaker_id": "1",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005183.1.19",
          "form": "대단하다...!",
          "original_form": "대단하다...!",
          "speaker_id": "2",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005183.1.20",
          "form": "저는 체력이 저질이라서...",
          "original_form": "저는 체력이 저질이라서...",
          "speaker_id": "2",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005183.1.21",
          "form": "운동 좋아하시나 봐요!",
          "original_form": "운동 좋아하시나 봐요!",
          "speaker_id": "2",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005183.1.22",
          "form": "네 싫어하지않는 편이에요!",
          "original_form": "네 싫어하지않는 편이에요!",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005183.1.23",
          "form": "그리고 사람들 사귀는거 좋아해서",
          "original_form": "그리고 사람들 사귀는거 좋아해서",
          "speaker_id": "1",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005183.1.24",
          "form": "여러사람 만나면서 활동적이고 싶어서요!",
          "original_form": "여러사람 만나면서 활동적이고 싶어서요!",
          "speaker_id": "1",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005183.1.25",
          "form": "아하, 사교적이시네요",
          "original_form": "아하, 사교적이시네요",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005183.1.26",
          "form": "저는 집순이라서",
          "original_form": "저는 집순이라서",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005183.1.27",
          "form": "동호회를 한다고 해도",
          "original_form": "동호회를 한다고 해도",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005183.1.28",
          "form": "독서모임 정도?",
          "original_form": "독서모임 정도?",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005183.1.29",
          "form": "오오 독서모임도 좋은거 같아요",
          "original_form": "오오 독서모임도 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005183.1.30",
          "form": "책도 많이 읽으면 좋잖아요!",
          "original_form": "책도 많이 읽으면 좋잖아요!",
          "speaker_id": "1",
          "time": "20210821 21:09"
        }
      ]
    }
  ]
