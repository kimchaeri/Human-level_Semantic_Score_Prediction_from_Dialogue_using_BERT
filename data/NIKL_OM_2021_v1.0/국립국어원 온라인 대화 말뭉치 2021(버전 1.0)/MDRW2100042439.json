      "utterance": [
        {
          "id": "MDRW2100042439.1.1",
          "form": "미술교육요.",
          "original_form": "미술교육요.",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042439.1.2",
          "form": "고등학교때전공은 한국화요",
          "original_form": "고등학교때전공은 한국화요",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042439.1.3",
          "form": "와....저 무슨 부스체험에서 한국화 그림 '색칠'해봤는데 !!",
          "original_form": "와....저 무슨 부스체험에서 한국화 그림 '색칠'해봤는데 !!",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042439.1.4",
          "form": "붓이..ㅠㅠㅠ농담표현이...",
          "original_form": "붓이..ㅠㅠㅠ농담표현이...",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042439.1.5",
          "form": "오..잘아시네요",
          "original_form": "오..잘아시네요",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042439.1.6",
          "form": "한국화는 농담표현을잘하면돼요",
          "original_form": "한국화는 농담표현을잘하면돼요",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042439.1.7",
          "form": "필력도 중요하구요",
          "original_form": "필력도 중요하구요",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042439.1.8",
          "form": "저는 안된다는걸 알았죠.. 와  진짜 되게 기술 필요해요",
          "original_form": "저는 안된다는걸 알았죠.. 와  진짜 되게 기술 필요해요",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042439.1.9",
          "form": "좀 오래 해야돼요",
          "original_form": "좀 오래 해야돼요",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042439.1.10",
          "form": "전 도배지 풀바르듯 했는데 설명하는 분은 꽃이 와....색이 연하고 진하고 모양도 꽃이고요",
          "original_form": "전 도배지 풀바르듯 했는데 설명하는 분은 꽃이 와....색이 연하고 진하고 모양도 꽃이고요",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042439.1.11",
          "form": "붓이 진짜 어려웠어요",
          "original_form": "붓이 진짜 어려웠어요",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042439.1.12",
          "form": "전 오래해도 안될사람인걸로 ㅎㅎ",
          "original_form": "전 오래해도 안될사람인걸로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042439.1.13",
          "form": "전공 동양화라니 넘 멋져요",
          "original_form": "전공 동양화라니 넘 멋져요",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042439.1.14",
          "form": "저두그림안그린지 오래됐네요..애들그림만그리고",
          "original_form": "저두그림안그린지 오래됐네요..애들그림만그리고",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042439.1.15",
          "form": "누군가 가르치는게 본인이 직접하는거보다 더힘들고 월등한 실력있어야 가능한거죠",
          "original_form": "누군가 가르치는게 본인이 직접하는거보다 더힘들고 월등한 실력있어야 가능한거죠",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042439.1.16",
          "form": "필요헌 코치랑 조언해주시는거 전문가만 가능하고요?",
          "original_form": "필요헌 코치랑 조언해주시는거 전문가만 가능하고요?",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042439.1.17",
          "form": "그정도까지는아니고요~~",
          "original_form": "그정도까지는아니고요~~",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042439.1.18",
          "form": "애들도 동양화 가르치시는건가요 아님 다른 그림?",
          "original_form": "애들도 동양화 가르치시는건가요 아님 다른 그림?",
          "speaker_id": "1",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042439.1.19",
          "form": "졸업하고 바로 애들가르치는일하다보니",
          "original_form": "졸업하고 바로 애들가르치는일하다보니",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042439.1.20",
          "form": "초등미술요",
          "original_form": "초등미술요",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042439.1.21",
          "form": "오 학생이 초등이면  난이도 더 상승이에요 !!!",
          "original_form": "오 학생이 초등이면  난이도 더 상승이에요 !!!",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042439.1.22",
          "form": "쉬워요..초등은",
          "original_form": "쉬워요..초등은",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042439.1.23",
          "form": "초등이라 집중시간짧으니 이것저것 다양하게+안그래도 손많이 가는 준비나 뒷정리도 글코\r\n저는 저혼자 그리고 치우라해도 힘들텐데ㅠㅠ\r\n완전 천사시네요",
          "original_form": "초등이라 집중시간짧으니 이것저것 다양하게+안그래도 손많이 가는 준비나 뒷정리도 글코\r\n저는 저혼자 그리고 치우라해도 힘들텐데ㅠㅠ\r\n완전 천사시네요",
