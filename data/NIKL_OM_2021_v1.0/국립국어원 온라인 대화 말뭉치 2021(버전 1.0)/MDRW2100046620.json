      "utterance": [
        {
          "id": "MDRW2100046620.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 12:14"
        },
        {
          "id": "MDRW2100046620.1.2",
          "form": "안녕하세요! 늦은 시간인데 안주무시네요!",
          "original_form": "안녕하세요! 늦은 시간인데 안주무시네요!",
          "speaker_id": "2",
          "time": "20211104 12:15"
        },
        {
          "id": "MDRW2100046620.1.3",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20211104 12:16"
        },
        {
          "id": "MDRW2100046620.1.4",
          "form": "이제 자려구요",
          "original_form": "이제 자려구요",
          "speaker_id": "1",
          "time": "20211104 12:16"
        },
        {
          "id": "MDRW2100046620.1.5",
          "form": "늦은시간까지",
          "original_form": "늦은시간까지",
          "speaker_id": "1",
          "time": "20211104 12:17"
        },
        {
          "id": "MDRW2100046620.1.6",
          "form": "일하시고계인건가요?",
          "original_form": "일하시고계인건가요?",
          "speaker_id": "1",
          "time": "20211104 12:17"
        },
        {
          "id": "MDRW2100046620.1.7",
          "form": "저두 자려고 누웠어요!",
          "original_form": "저두 자려고 누웠어요!",
          "speaker_id": "2",
          "time": "20211104 12:17"
        },
        {
          "id": "MDRW2100046620.1.8",
          "form": "그럼 새벽에는 대화방 중단인가요",
          "original_form": "그럼 새벽에는 대화방 중단인가요",
          "speaker_id": "1",
          "time": "20211104 12:18"
        },
        {
          "id": "MDRW2100046620.1.9",
          "form": "아니요! ㅎㅎ 뭐하고 계세요?",
          "original_form": "아니요! ㅎㅎ 뭐하고 계세요?",
          "speaker_id": "2",
          "time": "20211104 12:19"
        },
        {
          "id": "MDRW2100046620.1.10",
          "form": "잘려고 이  닦고 있네요",
          "original_form": "잘려고 이  닦고 있네요",
          "speaker_id": "1",
          "time": "20211104 12:19"
        },
        {
          "id": "MDRW2100046620.1.11",
          "form": "낼 몇시에 일어나세요?",
          "original_form": "낼 몇시에 일어나세요?",
          "speaker_id": "2",
          "time": "20211104 12:20"
        },
        {
          "id": "MDRW2100046620.1.12",
          "form": "6시에 일어나야하네요",
          "original_form": "6시에 일어나야하네요",
          "speaker_id": "1",
          "time": "20211104 12:20"
        },
        {
          "id": "MDRW2100046620.1.13",
          "form": "샤워하고 일찍 출근해야",
          "original_form": "샤워하고 일찍 출근해야",
          "speaker_id": "1",
          "time": "20211104 12:20"
        },
        {
          "id": "MDRW2100046620.1.14",
          "form": "여유가 있네요",
          "original_form": "여유가 있네요",
          "speaker_id": "1",
          "time": "20211104 12:21"
        },
        {
          "id": "MDRW2100046620.1.15",
          "form": "헉 얼른 자세요! 아침은 드세요?!",
          "original_form": "헉 얼른 자세요! 아침은 드세요?!",
          "speaker_id": "2",
          "time": "20211104 12:21"
        },
        {
          "id": "MDRW2100046620.1.16",
          "form": "간단하게 먹네요",
          "original_form": "간단하게 먹네요",
          "speaker_id": "1",
          "time": "20211104 12:21"
        },
        {
          "id": "MDRW2100046620.1.17",
          "form": "보통 뭐 드세요?!",
          "original_form": "보통 뭐 드세요?!",
          "speaker_id": "2",
          "time": "20211104 12:21"
        },
        {
          "id": "MDRW2100046620.1.18",
          "form": "집밥이네요 흔한 집반찬ㅜㅜ",
          "original_form": "집밥이네요 흔한 집반찬ㅜㅜ",
          "speaker_id": "1",
          "time": "20211104 12:21"
        },
        {
          "id": "MDRW2100046620.1.19",
          "form": "버섯나물위주로 먹네요",
          "original_form": "버섯나물위주로 먹네요",
          "speaker_id": "1",
          "time": "20211104 12:22"
        },
        {
          "id": "MDRW2100046620.1.20",
          "form": "아 그래두 든든하겠어요! 내일 출근하시려면 얼른 주무세요!!",
          "original_form": "아 그래두 든든하겠어요! 내일 출근하시려면 얼른 주무세요!!",
          "speaker_id": "2",
          "time": "20211104 12:22"
        }
      ]
