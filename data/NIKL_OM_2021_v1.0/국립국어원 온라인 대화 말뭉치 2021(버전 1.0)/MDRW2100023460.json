      "utterance": [
        {
          "id": "MDRW2100023460.1.1",
          "form": "네 김영갑 기억해둘게요",
          "original_form": "네 김영갑 기억해둘게요",
          "speaker_id": "1",
          "time": "20210830 04:05"
        },
        {
          "id": "MDRW2100023460.1.2",
          "form": "name1은 코시국 끝나면 여행가고 싶으신 데 있으신가요",
          "original_form": "&name1&은 코시국 끝나면 여행가고 싶으신 데 있으신가요",
          "speaker_id": "2",
          "time": "20210830 04:07"
        },
        {
          "id": "MDRW2100023460.1.3",
          "form": "일본 가보고 싶어요",
          "original_form": "일본 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210830 04:08"
        },
        {
          "id": "MDRW2100023460.1.4",
          "form": "좋쵸",
          "original_form": "좋쵸",
          "speaker_id": "2",
          "time": "20210830 04:09"
        },
        {
          "id": "MDRW2100023460.1.5",
          "form": "저는 오사카 도쿄 홋카이도 가봤어요",
          "original_form": "저는 오사카 도쿄 홋카이도 가봤어요",
          "speaker_id": "2",
          "time": "20210830 04:09"
        },
        {
          "id": "MDRW2100023460.1.6",
          "form": "지역은요?",
          "original_form": "지역은요?",
          "speaker_id": "2",
          "time": "20210830 04:09"
        },
        {
          "id": "MDRW2100023460.1.7",
          "form": "지역은 잘 몰라서요",
          "original_form": "지역은 잘 몰라서요",
          "speaker_id": "1",
          "time": "20210830 04:10"
        },
        {
          "id": "MDRW2100023460.1.8",
          "form": "북쪽에 게 많이 나는 곳이 홋카이도인가요?",
          "original_form": "북쪽에 게 많이 나는 곳이 홋카이도인가요?",
          "speaker_id": "1",
          "time": "20210830 04:10"
        },
        {
          "id": "MDRW2100023460.1.9",
          "form": "네 위에 섬이 홋카이도",
          "original_form": "네 위에 섬이 홋카이도",
          "speaker_id": "2",
          "time": "20210830 04:10"
        },
        {
          "id": "MDRW2100023460.1.10",
          "form": "유명한데가 삿포로",
          "original_form": "유명한데가 삿포로",
          "speaker_id": "2",
          "time": "20210830 04:10"
        },
        {
          "id": "MDRW2100023460.1.11",
          "form": "일본 초밥이 한국보다 확실히 맛있나요?",
          "original_form": "일본 초밥이 한국보다 확실히 맛있나요?",
          "speaker_id": "1",
          "time": "20210830 04:11"
        },
        {
          "id": "MDRW2100023460.1.12",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210830 04:19"
        },
        {
          "id": "MDRW2100023460.1.13",
          "form": "완전요",
          "original_form": "완전요",
          "speaker_id": "2",
          "time": "20210830 04:19"
        },
        {
          "id": "MDRW2100023460.1.14",
          "form": "별로 안비싼 집도 맛있어요?",
          "original_form": "별로 안비싼 집도 맛있어요?",
          "speaker_id": "1",
          "time": "20210830 04:20"
        },
        {
          "id": "MDRW2100023460.1.15",
          "form": "그게 이유가뭐에요? 만드는 기술이 좋은거에요?",
          "original_form": "그게 이유가뭐에요? 만드는 기술이 좋은거에요?",
          "speaker_id": "1",
          "time": "20210830 04:20"
        },
        {
          "id": "MDRW2100023460.1.16",
          "form": "그랬던기억이나요",
          "original_form": "그랬던기억이나요",
          "speaker_id": "2",
          "time": "20210830 04:20"
        },
        {
          "id": "MDRW2100023460.1.17",
          "form": "신선하고 싼집도 유명한데가 많았는데",
          "original_form": "신선하고 싼집도 유명한데가 많았는데",
          "speaker_id": "2",
          "time": "20210830 04:21"
        },
        {
          "id": "MDRW2100023460.1.18",
          "form": "수요가 많아서 그런것 같아요",
          "original_form": "수요가 많아서 그런것 같아요",
          "speaker_id": "2",
          "time": "20210830 04:21"
        },
        {
          "id": "MDRW2100023460.1.19",
          "form": "츠키시시장같은 곳은 관관명소이기도 하고요",
          "original_form": "츠키시시장같은 곳은 관관명소이기도 하고요",
          "speaker_id": "2",
          "time": "20210830 04:21"
        },
        {
          "id": "MDRW2100023460.1.20",
          "form": "그렇군요... 우리는 비싸게 줘야 그나마 맛있는거같아요",
          "original_form": "그렇군요... 우리는 비싸게 줘야 그나마 맛있는거같아요",
          "speaker_id": "1",
          "time": "20210830 04:22"
        },
        {
          "id": "MDRW2100023460.1.21",
          "form": "제주도 마트가보면 오징어 전복 회 다싼데 가게가면 또 비싸더라구요",
          "original_form": "제주도 마트가보면 오징어 전복 회 다싼데 가게가면 또 비싸더라구요",
          "speaker_id": "2",
          "time": "20210830 04:23"
        },
        {
          "id": "MDRW2100023460.1.22",
          "form": "그럼 마트서 사서 집에서 먹으면 되나봐요?",
          "original_form": "그럼 마트서 사서 집에서 먹으면 되나봐요?",
          "speaker_id": "1",
          "time": "20210830 04:25"
        },
        {
          "id": "MDRW2100023460.1.23",
          "form": "마트꺼도 싱싱해요?",
          "original_form": "마트꺼도 싱싱해요?",
          "speaker_id": "1",
          "time": "20210830 04:25"
        },
        {
          "id": "MDRW2100023460.1.24",
          "form": "제주도가니 삼다수 엄청 싸던데요 ㅋㅋㅋ",
          "original_form": "제주도가니 삼다수 엄청 싸던데요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 04:25"
        },
        {
          "id": "MDRW2100023460.1.25",
          "form": "네에 싱싱하구요",
          "original_form": "네에 싱싱하구요",
          "speaker_id": "2",
          "time": "20210830 04:25"
        },
        {
          "id": "MDRW2100023460.1.26",
          "form": "횟감 포장해서 만원에 팔아요",
          "original_form": "횟감 포장해서 만원에 팔아요",
          "speaker_id": "2",
          "time": "20210830 04:25"
        },
        {
          "id": "MDRW2100023460.1.27",
          "form": "삼다수 ㅋㅋㅋㅋ",
          "original_form": "삼다수 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 04:25"
        },
        {
          "id": "MDRW2100023460.1.28",
          "form": "전 브리타정수기로 내려서먹죠",
          "original_form": "전 브리타정수기로 내려서먹죠",
          "speaker_id": "2",
          "time": "20210830 04:26"
        }
      ]
    }
  ]
