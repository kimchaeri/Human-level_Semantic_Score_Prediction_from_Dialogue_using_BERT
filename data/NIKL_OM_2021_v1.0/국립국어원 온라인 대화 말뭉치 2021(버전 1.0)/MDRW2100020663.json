        },
        {
          "id": "MDRW2100020663.1.2",
          "form": "네 간단히 베이글먹었어요~",
          "original_form": "네 간단히 베이글먹었어요~",
          "speaker_id": "2",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020663.1.3",
          "form": "혹시 커뮤하시나요?",
          "original_form": "혹시 커뮤하시나요?",
          "speaker_id": "2",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020663.1.4",
          "form": "흠..  커뮤니티가 어떤걸까요?",
          "original_form": "흠..  커뮤니티가 어떤걸까요?",
          "speaker_id": "1",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020663.1.5",
          "form": "저도 베이글먹었어요!!",
          "original_form": "저도 베이글먹었어요!!",
          "speaker_id": "1",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020663.1.6",
          "form": "오오 통했네요!",
          "original_form": "오오 통했네요!",
          "speaker_id": "2",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020663.1.7",
          "form": "음 에타라던지 뽐뿌 이런거 말하는거같은데요?",
          "original_form": "음 에타라던지 뽐뿌 이런거 말하는거같은데요?",
          "speaker_id": "2",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020663.1.8",
          "form": "저도 커뮤 안해서 잘은 몰라요 ㅋㅋ",
          "original_form": "저도 커뮤 안해서 잘은 몰라요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020663.1.9",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020663.1.10",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020663.1.11",
          "form": "전 사실 인스타만해서요ㅋㅋ",
          "original_form": "전 사실 인스타만해서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020663.1.12",
          "form": "하하....",
          "original_form": "하하....",
          "speaker_id": "1",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020663.1.13",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020663.1.14",
          "form": "전 인스타하다가 해킹당해서 그이후론 안해여 ㅋㅋ",
          "original_form": "전 인스타하다가 해킹당해서 그이후론 안해여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020663.1.15",
          "form": "오호 그렇군요ㅠㅠ",
          "original_form": "오호 그렇군요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020663.1.16",
          "form": "그나저나 오늘 날씨 좋네요",
          "original_form": "그나저나 오늘 날씨 좋네요",
          "speaker_id": "1",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020663.1.17",
          "form": "저는 오랜만에 친구보러나와서요",
          "original_form": "저는 오랜만에 친구보러나와서요",
          "speaker_id": "1",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020663.1.18",
          "form": "오오 오늘 친구만나시군요!",
          "original_form": "오오 오늘 친구만나시군요!",
          "speaker_id": "2",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020663.1.19",
          "form": "친구만나면 주로 어디가거나 뭐하시나요?",
          "original_form": "친구만나면 주로 어디가거나 뭐하시나요?",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020663.1.20",
          "form": "먹방투어",
          "original_form": "먹방투어",
          "speaker_id": "1",
          "time": "20210828 13:01"
        },
        {
          "id": "MDRW2100020663.1.21",
          "form": "다녀요",
          "original_form": "다녀요",
          "speaker_id": "1",
          "time": "20210828 13:01"
        },
        {
          "id": "MDRW2100020663.1.22",
          "form": "이것저것 먹으러 다닙니다",
          "original_form": "이것저것 먹으러 다닙니다",
          "speaker_id": "1",
          "time": "20210828 13:01"
        },
        {
          "id": "MDRW2100020663.1.23",
          "form": "오오! 저두 먹방투어좋아해요",
          "original_form": "오오! 저두 먹방투어좋아해요",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020663.1.24",
          "form": "특히 빵집투어",
          "original_form": "특히 빵집투어",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020663.1.25",
          "form": "오호 name2님 저는",
          "original_form": "오호 &name2&님 저는",
          "speaker_id": "1",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100020663.1.26",
          "form": "1차로 떡볶이. 2차로 빈대떡",
          "original_form": "1차로 떡볶이. 2차로 빈대떡",
          "speaker_id": "1",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100020663.1.27",
          "form": "밀면,냉면",
          "original_form": "밀면,냉면",
          "speaker_id": "1",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100020663.1.28",
          "form": "3차로",
          "original_form": "3차로",
          "speaker_id": "1",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100020663.1.29",
          "form": "조각피자 먹었어요.",
          "original_form": "조각피자 먹었어요.",
          "speaker_id": "1",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100020663.1.30",
          "form": "워우!! 대박알찬데요??",
          "original_form": "워우!! 대박알찬데요??",
          "speaker_id": "2",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100020663.1.31",
          "form": "혹시 지역이 어디에요???",
          "original_form": "혹시 지역이 어디에요???",
          "speaker_id": "2",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100020663.1.32",
          "form": "대전 놀러왔어요",
          "original_form": "대전 놀러왔어요",
