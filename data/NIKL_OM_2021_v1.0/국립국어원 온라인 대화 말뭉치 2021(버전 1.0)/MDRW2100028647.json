      "utterance": [
        {
          "id": "MDRW2100028647.1.1",
          "form": "직장 사람 스트레스 있으신가요",
          "original_form": "직장 사람 스트레스 있으신가요",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028647.1.2",
          "form": "새로 오신분 교육 때문에",
          "original_form": "새로 오신분 교육 때문에",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028647.1.3",
          "form": "최근에 가볍게 스트레스가 있네요",
          "original_form": "최근에 가볍게 스트레스가 있네요",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028647.1.4",
          "form": "잘 알겠습니다",
          "original_form": "잘 알겠습니다",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028647.1.5",
          "form": "인문?",
          "original_form": "인문?",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028647.1.6",
          "form": "과학?",
          "original_form": "과학?",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028647.1.7",
          "form": "음?",
          "original_form": "음?",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028647.1.8",
          "form": "아뇨 과학 기술 새로 오신 분...",
          "original_form": "아뇨 과학 기술 새로 오신 분...",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028647.1.9",
          "form": "제가 직접 교육하는지라...",
          "original_form": "제가 직접 교육하는지라...",
          "speaker_id": "1",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028647.1.10",
          "form": "부사수 때문에 회사 생활이 힘들다는 거군요",
          "original_form": "부사수 때문에 회사 생활이 힘들다는 거군요",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028647.1.11",
          "form": "어...",
          "original_form": "어...",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028647.1.12",
          "form": "선임으로서의 책임감이라고 해주심이..",
          "original_form": "선임으로서의 책임감이라고 해주심이..",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028647.1.13",
          "form": "책임감 있으시네요ㅎㅎ",
          "original_form": "책임감 있으시네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 09:03"
        },
        {
          "id": "MDRW2100028647.1.14",
          "form": "이거 완전 엎드려 절받기...",
          "original_form": "이거 완전 엎드려 절받기...",
          "speaker_id": "1",
          "time": "20210903 09:22"
        },
        {
          "id": "MDRW2100028647.1.15",
          "form": "근데 오늘 마침 아침에",
          "original_form": "근데 오늘 마침 아침에",
          "speaker_id": "1",
          "time": "20210903 09:22"
        },
        {
          "id": "MDRW2100028647.1.16",
          "form": "부사수 분 첫 발표가 있는지라",
          "original_form": "부사수 분 첫 발표가 있는지라",
          "speaker_id": "1",
          "time": "20210903 09:22"
        },
        {
          "id": "MDRW2100028647.1.17",
          "form": "(점독)",
          "original_form": "(점독)",
          "speaker_id": "1",
          "time": "20210903 09:22"
        },
        {
          "id": "MDRW2100028647.1.18",
          "form": "옆에서 커버쳐 주세요",
          "original_form": "옆에서 커버쳐 주세요",
          "speaker_id": "2",
          "time": "20210903 09:31"
        },
        {
          "id": "MDRW2100028647.1.19",
          "form": "멋진 사수",
          "original_form": "멋진 사수",
          "speaker_id": "2",
          "time": "20210903 09:31"
        },
        {
          "id": "MDRW2100028647.1.20",
          "form": "그럴 예정인지라...",
          "original_form": "그럴 예정인지라...",
          "speaker_id": "1",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100028647.1.21",
          "form": "안 그럼 그는 떠날 거예요",
          "original_form": "안 그럼 그는 떠날 거예요",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100028647.1.22",
          "form": "아 그러고보니",
          "original_form": "아 그러고보니",
          "speaker_id": "1",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100028647.1.23",
          "form": "프리랜서분",
          "original_form": "프리랜서분",
          "speaker_id": "1",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100028647.1.24",
          "form": "그만두신거 아셔요?",
          "original_form": "그만두신거 아셔요?",
          "speaker_id": "1",
          "time": "20210903 09:55"
        }
      ]
