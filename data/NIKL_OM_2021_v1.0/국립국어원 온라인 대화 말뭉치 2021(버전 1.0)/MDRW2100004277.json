      "utterance": [
        {
          "id": "MDRW2100004277.1.1",
          "form": "안녕하세요 혹시 믿는 종교 있으신가요?",
          "original_form": "안녕하세요 혹시 믿는 종교 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004277.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004277.1.3",
          "form": "예전에 성당을 다녔어요",
          "original_form": "예전에 성당을 다녔어요",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004277.1.4",
          "form": "지금은 안나가고요",
          "original_form": "지금은 안나가고요",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004277.1.5",
          "form": "오 저도 성당나녀요 지금은 코로나때문에 못가고 있지만..",
          "original_form": "오 저도 성당나녀요 지금은 코로나때문에 못가고 있지만..",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004277.1.6",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004277.1.7",
          "form": "예전에 성당 다닐때는",
          "original_form": "예전에 성당 다닐때는",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004277.1.8",
          "form": "마리아 학교 그런곳도 많이 다녔었늗데",
          "original_form": "마리아 학교 그런곳도 많이 다녔었늗데",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004277.1.9",
          "form": "이제는 추억이네요",
          "original_form": "이제는 추억이네요",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004277.1.10",
          "form": "종교활동 관련해서",
          "original_form": "종교활동 관련해서",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004277.1.11",
          "form": "특별히 기억에 나는거",
          "original_form": "특별히 기억에 나는거",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004277.1.12",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004277.1.13",
          "form": "겨울에 성탄절에 성당에서 행사할때 공연으로  친구들이랑언니오빠들이랑  연극했던게 기억나네요",
          "original_form": "겨울에 성탄절에 성당에서 행사할때 공연으로  친구들이랑언니오빠들이랑  연극했던게 기억나네요",
          "speaker_id": "1",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004277.1.14",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004277.1.15",
          "form": "무슨 연극 하셨나요?",
          "original_form": "무슨 연극 하셨나요?",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004277.1.16",
          "form": "재밌었을 것 같아요",
          "original_form": "재밌었을 것 같아요",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004277.1.17",
          "form": "성탈절에 하는 일은 대게 특별하게 느껴지는 것같아요",
          "original_form": "성탈절에 하는 일은 대게 특별하게 느껴지는 것같아요",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004277.1.18",
          "form": "더 특별했니겠네요",
          "original_form": "더 특별했니겠네요",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004277.1.19",
          "form": "네!  스크루지 연극했어요!",
          "original_form": "네!  스크루지 연극했어요!",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004277.1.20",
          "form": "무슨 내용인가요?",
          "original_form": "무슨 내용인가요?",
          "speaker_id": "2",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004277.1.21",
          "form": "스쿠르지가 구두쇠인데 유령들만나면서 과거 현재 미래의 처지를 살펴보면서 자신의 죄를 반성하고 착하게 사는 이야기 에요",
          "original_form": "스쿠르지가 구두쇠인데 유령들만나면서 과거 현재 미래의 처지를 살펴보면서 자신의 죄를 반성하고 착하게 사는 이야기 에요",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004277.1.22",
          "form": "설명 감사해요 ㅎㅎ",
          "original_form": "설명 감사해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004277.1.23",
          "form": "유령들이랑 만난다는 점이 매력적이네요",
          "original_form": "유령들이랑 만난다는 점이 매력적이네요",
          "speaker_id": "2",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004277.1.24",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004277.1.25",
          "form": "저도 성당에서 연극을 한 적이 있는데",
          "original_form": "저도 성당에서 연극을 한 적이 있는데",
          "speaker_id": "2",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004277.1.26",
          "form": "그때 예수님 역할을 맡았었어요 ㅎㅎㅎ",
          "original_form": "그때 예수님 역할을 맡았었어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004277.1.27",
          "form": "내용은 하나도 기억 안나지만요",
          "original_form": "내용은 하나도 기억 안나지만요",
          "speaker_id": "2",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004277.1.28",
          "form": "오오 멋져요!",
          "original_form": "오오 멋져요!",
          "speaker_id": "1",
          "time": "20210821 16:13"
        }
      ]
    }
  ]
