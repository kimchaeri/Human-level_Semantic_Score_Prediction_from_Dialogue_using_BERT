      "utterance": [
        {
          "id": "MDRW2100025409.1.1",
          "form": "거긴 비오나요??",
          "original_form": "거긴 비오나요??",
          "speaker_id": "2",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025409.1.2",
          "form": "여긴 비가 계속 옵니다",
          "original_form": "여긴 비가 계속 옵니다",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025409.1.3",
          "form": "여긴 계속 흐리고",
          "original_form": "여긴 계속 흐리고",
          "speaker_id": "1",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025409.1.4",
          "form": "조금 왔다가 그치고 반복이네요ㅜ",
          "original_form": "조금 왔다가 그치고 반복이네요ㅜ",
          "speaker_id": "1",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025409.1.5",
          "form": "오늘은 해가 나네요",
          "original_form": "오늘은 해가 나네요",
          "speaker_id": "2",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100025409.1.6",
          "form": "날씨가 간만에 맑은거같아요^^",
          "original_form": "날씨가 간만에 맑은거같아요^^",
          "speaker_id": "2",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100025409.1.7",
          "form": "앗 부러워요ㅜㅜ",
          "original_form": "앗 부러워요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100025409.1.8",
          "form": "여긴 오늘도 흐림 ㅜ",
          "original_form": "여긴 오늘도 흐림 ㅜ",
          "speaker_id": "1",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100025409.1.9",
          "form": "아 흐리군요 ㅠ",
          "original_form": "아 흐리군요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100025409.1.10",
          "form": "여기도 해가 났다가 흐렸다가 하긴 하는데",
          "original_form": "여기도 해가 났다가 흐렸다가 하긴 하는데",
          "speaker_id": "2",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100025409.1.11",
          "form": "오랜만에 해를 보는 느낌이예요",
          "original_form": "오랜만에 해를 보는 느낌이예요",
          "speaker_id": "2",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100025409.1.12",
          "form": "해가 부끄러워서 계속 구름 속에 숨네요ㅋㅋㅋ",
          "original_form": "해가 부끄러워서 계속 구름 속에 숨네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100025409.1.13",
          "form": "많이 봐주세요 비타민D!",
          "original_form": "많이 봐주세요 비타민D!",
          "speaker_id": "1",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100025409.1.14",
          "form": "ㅋㅋㅋㅋㅋ 네 오늘은 햇빛 받으며 좀 걸어야겠어요",
          "original_form": "ㅋㅋㅋㅋㅋ 네 오늘은 햇빛 받으며 좀 걸어야겠어요",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100025409.1.15",
          "form": "요새는 미세먼지 없으니 너무 좋아요",
          "original_form": "요새는 미세먼지 없으니 너무 좋아요",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100025409.1.16",
          "form": "창문열어놔도 시원하고",
          "original_form": "창문열어놔도 시원하고",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100025409.1.17",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100025409.1.18",
          "form": "ㅎㅎ 잘때도 시원하더라구요",
          "original_form": "ㅎㅎ 잘때도 시원하더라구요",
          "speaker_id": "2",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100025409.1.19",
          "form": "여긴 근데 추워요ㅜㅜ",
          "original_form": "여긴 근데 추워요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 09:39"
        },
        {
          "id": "MDRW2100025409.1.20",
          "form": "전기장판을 벌써꺼냈어요ㅋㅋ",
          "original_form": "전기장판을 벌써꺼냈어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:40"
        },
        {
          "id": "MDRW2100025409.1.21",
          "form": "우와 벌써 장판 꺼내셨어요??",
          "original_form": "우와 벌써 장판 꺼내셨어요??",
          "speaker_id": "2",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100025409.1.22",
          "form": "저도 창문 열어 놓고 자니 추워서",
          "original_form": "저도 창문 열어 놓고 자니 추워서",
          "speaker_id": "2",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100025409.1.23",
          "form": "창문 살짝 열고 이불 덮고 잔 답니다",
          "original_form": "창문 살짝 열고 이불 덮고 잔 답니다",
          "speaker_id": "2",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100025409.1.24",
          "form": "여기가 아침 저녁이 쌀쌀해서ㅎㅎㅎ",
          "original_form": "여기가 아침 저녁이 쌀쌀해서ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100025409.1.25",
          "form": "습한데 장판켜니 뽀송한 느낌이더라구요ㅎ",
          "original_form": "습한데 장판켜니 뽀송한 느낌이더라구요ㅎ",
