      "utterance": [
        {
          "id": "MDRW2100023459.1.1",
          "form": "타이벡감귤 떠오르네요",
          "original_form": "타이벡감귤 떠오르네요",
          "speaker_id": "1",
          "time": "20210830 03:56"
        },
        {
          "id": "MDRW2100023459.1.2",
          "form": "효돈감귤 거기서 유명한거맞아요?",
          "original_form": "효돈감귤 거기서 유명한거맞아요?",
          "speaker_id": "1",
          "time": "20210830 03:56"
        },
        {
          "id": "MDRW2100023459.1.3",
          "form": "하 저도 잘 모르겠어요",
          "original_form": "하 저도 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210830 03:57"
        },
        {
          "id": "MDRW2100023459.1.4",
          "form": "맛있는데 엄청비쌌어요",
          "original_form": "맛있는데 엄청비쌌어요",
          "speaker_id": "1",
          "time": "20210830 03:57"
        },
        {
          "id": "MDRW2100023459.1.5",
          "form": "5킬로에 5-6만원 하더라구요 ㅡㅡ",
          "original_form": "5킬로에 5-6만원 하더라구요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210830 03:57"
        },
        {
          "id": "MDRW2100023459.1.6",
          "form": "제주와서 여행하시고 사드셨군요",
          "original_form": "제주와서 여행하시고 사드셨군요",
          "speaker_id": "2",
          "time": "20210830 03:58"
        },
        {
          "id": "MDRW2100023459.1.7",
          "form": "품질은 어땠어요?",
          "original_form": "품질은 어땠어요?",
          "speaker_id": "2",
          "time": "20210830 03:58"
        },
        {
          "id": "MDRW2100023459.1.8",
          "form": "아뇨 여기서 맛있는 과일이라며 파는 사람한테 사먹었어요",
          "original_form": "아뇨 여기서 맛있는 과일이라며 파는 사람한테 사먹었어요",
          "speaker_id": "1",
          "time": "20210830 03:58"
        },
        {
          "id": "MDRW2100023459.1.9",
          "form": "맛있기는 엄청 맛있었어요",
          "original_form": "맛있기는 엄청 맛있었어요",
          "speaker_id": "1",
          "time": "20210830 03:58"
        },
        {
          "id": "MDRW2100023459.1.10",
          "form": "여행이 다 먹는건데 맛있었음 됐쥬",
          "original_form": "여행이 다 먹는건데 맛있었음 됐쥬",
          "speaker_id": "2",
          "time": "20210830 03:59"
        },
        {
          "id": "MDRW2100023459.1.11",
          "form": "친구들이랑 오셨어요?",
          "original_form": "친구들이랑 오셨어요?",
          "speaker_id": "2",
          "time": "20210830 04:00"
        },
        {
          "id": "MDRW2100023459.1.12",
          "form": "아뇨 가족이랑요 이중섭 기념관 좋았던 기억나요",
          "original_form": "아뇨 가족이랑요 이중섭 기념관 좋았던 기억나요",
          "speaker_id": "1",
          "time": "20210830 04:00"
        },
        {
          "id": "MDRW2100023459.1.13",
          "form": "오 이중섭기념관 저도 가봐야겠네요",
          "original_form": "오 이중섭기념관 저도 가봐야겠네요",
          "speaker_id": "2",
          "time": "20210830 04:00"
        },
        {
          "id": "MDRW2100023459.1.14",
          "form": "거기 기념관에 부인이랑 주고받은 편지가",
          "original_form": "거기 기념관에 부인이랑 주고받은 편지가",
          "speaker_id": "1",
          "time": "20210830 04:01"
        },
        {
          "id": "MDRW2100023459.1.15",
          "form": "읽어보면 무지 슬퍼요",
          "original_form": "읽어보면 무지 슬퍼요",
          "speaker_id": "1",
          "time": "20210830 04:01"
        },
        {
          "id": "MDRW2100023459.1.16",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210830 04:01"
        },
        {
          "id": "MDRW2100023459.1.17",
          "form": "제주시절이 가난하면서도 행복했던 시절이라",
          "original_form": "제주시절이 가난하면서도 행복했던 시절이라",
          "speaker_id": "2",
          "time": "20210830 04:02"
        },
        {
          "id": "MDRW2100023459.1.18",
          "form": "이중섭기념관이 있나보네요",
          "original_form": "이중섭기념관이 있나보네요",
          "speaker_id": "2",
          "time": "20210830 04:02"
        },
        {
          "id": "MDRW2100023459.1.19",
          "form": "네 그런 설명이 적혀있어요",
          "original_form": "네 그런 설명이 적혀있어요",
          "speaker_id": "1",
          "time": "20210830 04:02"
        },
        {
          "id": "MDRW2100023459.1.20",
          "form": "살았던 방 보여주는데",
          "original_form": "살았던 방 보여주는데",
          "speaker_id": "1",
          "time": "20210830 04:02"
        },
        {
          "id": "MDRW2100023459.1.21",
          "form": "방이 진짜 작아요",
          "original_form": "방이 진짜 작아요",
          "speaker_id": "1",
          "time": "20210830 04:02"
        },
        {
          "id": "MDRW2100023459.1.22",
          "form": "근데 거기서 젤 행복했다하네요",
          "original_form": "근데 거기서 젤 행복했다하네요",
          "speaker_id": "1",
          "time": "20210830 04:02"
        },
        {
          "id": "MDRW2100023459.1.23",
          "form": "창도없고 그랬던 것 같네요",
          "original_form": "창도없고 그랬던 것 같네요",
          "speaker_id": "2",
          "time": "20210830 04:02"
        },
        {
          "id": "MDRW2100023459.1.24",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "1",
          "time": "20210830 04:03"
        },
        {
          "id": "MDRW2100023459.1.25",
          "form": "신기한게 제주는 바다가 아름다운데",
          "original_form": "신기한게 제주는 바다가 아름다운데",
          "speaker_id": "2",
          "time": "20210830 04:03"
        },
        {
          "id": "MDRW2100023459.1.26",
          "form": "그런 풍경화는 기억이 안나네여",
          "original_form": "그런 풍경화는 기억이 안나네여",
          "speaker_id": "2",
          "time": "20210830 04:03"
        },
        {
          "id": "MDRW2100023459.1.27",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "1",
          "time": "20210830 04:03"
        },
        {
          "id": "MDRW2100023459.1.28",
          "form": "김영갑인가",
          "original_form": "김영갑인가",
          "speaker_id": "2",
          "time": "20210830 04:04"
        },
        {
          "id": "MDRW2100023459.1.29",
          "form": "오름사짐만 찍다가 돌아가신 분 갤러리도 좋거든요",
          "original_form": "오름사짐만 찍다가 돌아가신 분 갤러리도 좋거든요",
          "speaker_id": "2",
          "time": "20210830 04:04"
        }
      ]
