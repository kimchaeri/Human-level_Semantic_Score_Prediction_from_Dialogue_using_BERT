      "utterance": [
        {
          "id": "MDRW2100040767.1.1",
          "form": "언니다행히2주만에 음성떠서",
          "original_form": "언니다행히2주만에 음성떠서",
          "speaker_id": "2",
          "time": "20210905 04:08"
        },
        {
          "id": "MDRW2100040767.1.2",
          "form": "나왔는데",
          "original_form": "나왔는데",
          "speaker_id": "2",
          "time": "20210905 04:08"
        },
        {
          "id": "MDRW2100040767.1.3",
          "form": "저는 그냥 음성...",
          "original_form": "저는 그냥 음성...",
          "speaker_id": "2",
          "time": "20210905 04:08"
        },
        {
          "id": "MDRW2100040767.1.4",
          "form": "멀쩡한음성...",
          "original_form": "멀쩡한음성...",
          "speaker_id": "2",
          "time": "20210905 04:08"
        },
        {
          "id": "MDRW2100040767.1.5",
          "form": "건강맨...인지 언니가 걸린게 진짜 약한 코로나였던건지",
          "original_form": "건강맨...인지 언니가 걸린게 진짜 약한 코로나였던건지",
          "speaker_id": "2",
          "time": "20210905 04:08"
        },
        {
          "id": "MDRW2100040767.1.6",
          "form": "와....",
          "original_form": "와....",
          "speaker_id": "1",
          "time": "20210905 04:08"
        },
        {
          "id": "MDRW2100040767.1.7",
          "form": "아니진짜",
          "original_form": "아니진짜",
          "speaker_id": "1",
          "time": "20210905 04:08"
        },
        {
          "id": "MDRW2100040767.1.8",
          "form": "다행이네요 name2님 그럼 그 이후로",
          "original_form": "다행이네요 &name2&님 그럼 그 이후로",
          "speaker_id": "1",
          "time": "20210905 04:08"
        },
        {
          "id": "MDRW2100040767.1.9",
          "form": "그래서 만약 1차도 아플수도있겠구나싶네요 저는",
          "original_form": "그래서 만약 1차도 아플수도있겠구나싶네요 저는",
          "speaker_id": "2",
          "time": "20210905 04:08"
        },
        {
          "id": "MDRW2100040767.1.10",
          "form": "아프신 적은 없는거고요?",
          "original_form": "아프신 적은 없는거고요?",
          "speaker_id": "1",
          "time": "20210905 04:08"
        },
        {
          "id": "MDRW2100040767.1.11",
          "form": "네네퓨ㅠ",
          "original_form": "네네퓨ㅠ",
          "speaker_id": "2",
          "time": "20210905 04:08"
        },
        {
          "id": "MDRW2100040767.1.12",
          "form": "그냥",
          "original_form": "그냥",
          "speaker_id": "2",
          "time": "20210905 04:08"
        },
        {
          "id": "MDRW2100040767.1.13",
          "form": "하루에똥2번씩싸면서",
          "original_form": "하루에똥2번씩싸면서",
          "speaker_id": "2",
          "time": "20210905 04:08"
        },
        {
          "id": "MDRW2100040767.1.14",
          "form": "그냥잘먹고잘싸는",
          "original_form": "그냥잘먹고잘싸는",
          "speaker_id": "2",
          "time": "20210905 04:09"
        },
        {
          "id": "MDRW2100040767.1.15",
          "form": "너무 신기해요....!!!! 네네",
          "original_form": "너무 신기해요....!!!! 네네",
          "speaker_id": "1",
          "time": "20210905 04:09"
        },
        {
          "id": "MDRW2100040767.1.16",
          "form": "신기허네용",
          "original_form": "신기허네용",
          "speaker_id": "2",
          "time": "20210905 04:09"
        },
        {
          "id": "MDRW2100040767.1.17",
          "form": "언니는그때 다 낫고나서 항체생겼다네요",
          "original_form": "언니는그때 다 낫고나서 항체생겼다네요",
          "speaker_id": "2",
          "time": "20210905 04:09"
        },
        {
          "id": "MDRW2100040767.1.18",
          "form": "신기한....경험..",
          "original_form": "신기한....경험..",
          "speaker_id": "2",
          "time": "20210905 04:09"
        },
        {
          "id": "MDRW2100040767.1.19",
          "form": "와 항체 생긴거는",
          "original_form": "와 항체 생긴거는",
          "speaker_id": "1",
          "time": "20210905 04:10"
        },
        {
          "id": "MDRW2100040767.1.20",
          "form": "미국에서 피검사 하면 알 수 있는건가요?!",
          "original_form": "미국에서 피검사 하면 알 수 있는건가요?!",
          "speaker_id": "1",
          "time": "20210905 04:10"
        },
        {
          "id": "MDRW2100040767.1.21",
          "form": "헉 아니요 한국와서",
          "original_form": "헉 아니요 한국와서",
          "speaker_id": "2",
          "time": "20210905 04:10"
        },
        {
          "id": "MDRW2100040767.1.22",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 04:10"
        },
        {
          "id": "MDRW2100040767.1.23",
          "form": "그때 검사랑 다 한거였어용",
          "original_form": "그때 검사랑 다 한거였어용",
          "speaker_id": "2",
          "time": "20210905 04:10"
        },
        {
          "id": "MDRW2100040767.1.24",
          "form": "오 그 때 항체 유무도 알 수 있는거군요",
          "original_form": "오 그 때 항체 유무도 알 수 있는거군요",
          "speaker_id": "1",
          "time": "20210905 04:10"
        },
        {
          "id": "MDRW2100040767.1.25",
          "form": "네네그렇더라구요",
          "original_form": "네네그렇더라구요",
          "speaker_id": "2",
          "time": "20210905 04:10"
        },
        {
          "id": "MDRW2100040767.1.26",
          "form": "그나저나 언니가 쾌차하셔서 정말 다행이에요",
          "original_form": "그나저나 언니가 쾌차하셔서 정말 다행이에요",
          "speaker_id": "1",
          "time": "20210905 04:10"
        },
        {
          "id": "MDRW2100040767.1.27",
          "form": "그렇다면 name2님은",
          "original_form": "그렇다면 &name2&님은",
          "speaker_id": "1",
          "time": "20210905 04:11"
        },
        {
          "id": "MDRW2100040767.1.28",
          "form": "항체가 이미",
          "original_form": "항체가 이미",
          "speaker_id": "1",
          "time": "20210905 04:11"
        },
        {
          "id": "MDRW2100040767.1.29",
          "form": "ㅎㅎ다행이죵",
          "original_form": "ㅎㅎ다행이죵",
          "speaker_id": "2",
          "time": "20210905 04:11"
        },
        {
          "id": "MDRW2100040767.1.30",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210905 04:11"
        },
        {
          "id": "MDRW2100040767.1.31",
          "form": "있는거 아닐까요?",
          "original_form": "있는거 아닐까요?",
          "speaker_id": "1",
          "time": "20210905 04:11"
        },
        {
          "id": "MDRW2100040767.1.32",
          "form": "저는잘모르겠네요",
          "original_form": "저는잘모르겠네요",
          "speaker_id": "2",
          "time": "20210905 04:11"
        }
      ]
    }
