      "utterance": [
        {
          "id": "MDRW2100019808.1.1",
          "form": "이번에 올림픽 보셨어요??",
          "original_form": "이번에 올림픽 보셨어요??",
          "speaker_id": "1",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019808.1.2",
          "form": "네 봤죠!",
          "original_form": "네 봤죠!",
          "speaker_id": "2",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019808.1.3",
          "form": "배구랑  양궁 진짜 재밌게 봤어요",
          "original_form": "배구랑  양궁 진짜 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019808.1.4",
          "form": "너무 재밌더라구요",
          "original_form": "너무 재밌더라구요",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019808.1.5",
          "form": "오 저도 양궁 봤어요 ㅋㅋㅋ",
          "original_form": "오 저도 양궁 봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019808.1.6",
          "form": "역시 주몽의 후예답게 잘 하더라구요",
          "original_form": "역시 주몽의 후예답게 잘 하더라구요",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019808.1.7",
          "form": "그니까요ㅋㅋㅋ",
          "original_form": "그니까요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019808.1.8",
          "form": "안산선수 넘 잘하더라구요",
          "original_form": "안산선수 넘 잘하더라구요",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019808.1.9",
          "form": "안산선수랑 김제덕선수 케미가 너무  웃기더라구요ㅋㅋ",
          "original_form": "안산선수랑 김제덕선수 케미가 너무  웃기더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019808.1.10",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019808.1.11",
          "form": "마자요 최고예요",
          "original_form": "마자요 최고예요",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019808.1.12",
          "form": "제덕쿵야 ㅋㅋㅋ 너무 귀여워요",
          "original_form": "제덕쿵야 ㅋㅋㅋ 너무 귀여워요",
          "speaker_id": "1",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019808.1.13",
          "form": "화이팅!!!!",
          "original_form": "화이팅!!!!",
          "speaker_id": "1",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019808.1.14",
          "form": "ㅋㅋㅋ그니까요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ그니까요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019808.1.15",
          "form": "원래 김제덕선수 못나올뻔한거 아세요?",
          "original_form": "원래 김제덕선수 못나올뻔한거 아세요?",
          "speaker_id": "1",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019808.1.16",
          "form": "안산선수 귀막는것도 귀엽고ㅋㅋ",
          "original_form": "안산선수 귀막는것도 귀엽고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019808.1.17",
          "form": "너무웃겨요ㅠㅋㅋ",
          "original_form": "너무웃겨요ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019808.1.18",
          "form": "연기 안됐으면",
          "original_form": "연기 안됐으면",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019808.1.19",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019808.1.20",
          "form": "못뽑힐뻔했다그러더라구요",
          "original_form": "못뽑힐뻔했다그러더라구요",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019808.1.21",
          "form": "코로나덕에 볼수있었던 선수같아요",
