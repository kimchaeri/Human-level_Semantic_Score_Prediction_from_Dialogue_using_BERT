      "utterance": [
        {
          "id": "MDRW2100024317.1.1",
          "form": "요즘은 없어요 ㅋㅋㅋ 옛날에 개와늑대의시간도 정주행 진짜 많이했었는데..",
          "original_form": "요즘은 없어요 ㅋㅋㅋ 옛날에 개와늑대의시간도 정주행 진짜 많이했었는데..",
          "speaker_id": "2",
          "time": "20210831 00:41"
        },
        {
          "id": "MDRW2100024317.1.2",
          "form": "아 그 정유미나오는거 뭐더라ㅠㅠ 시리즈로된거 2탄",
          "original_form": "아 그 정유미나오는거 뭐더라ㅠㅠ 시리즈로된거 2탄",
          "speaker_id": "2",
          "time": "20210831 00:41"
        },
        {
          "id": "MDRW2100024317.1.3",
          "form": "그거도 엄청보고 ..",
          "original_form": "그거도 엄청보고 ..",
          "speaker_id": "2",
          "time": "20210831 00:41"
        },
        {
          "id": "MDRW2100024317.1.4",
          "form": "로맨스가 필요해?!",
          "original_form": "로맨스가 필요해?!",
          "speaker_id": "1",
          "time": "20210831 00:42"
        },
        {
          "id": "MDRW2100024317.1.5",
          "form": "아로맨스가필요해2",
          "original_form": "아로맨스가필요해2",
          "speaker_id": "2",
          "time": "20210831 00:42"
        },
        {
          "id": "MDRW2100024317.1.6",
          "form": "네네넼ㅋㅋㅋ",
          "original_form": "네네넼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:42"
        },
        {
          "id": "MDRW2100024317.1.7",
          "form": "아아 이거 3도 나왔어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아아 이거 3도 나왔어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:42"
        },
        {
          "id": "MDRW2100024317.1.8",
          "form": "김소연이랑 성준배우요",
          "original_form": "김소연이랑 성준배우요",
          "speaker_id": "1",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100024317.1.9",
          "form": "그거도봤어요! ㅋㅋㅋㅋ",
          "original_form": "그거도봤어요! ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100024317.1.10",
          "form": "아ㅋㅋㅋㅋㅋ2는 완전 여름여름해서 풋풋한데",
          "original_form": "아ㅋㅋㅋㅋㅋ2는 완전 여름여름해서 풋풋한데",
          "speaker_id": "1",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100024317.1.11",
          "form": "3은 으른미가 아주ㅋㅋ",
          "original_form": "3은 으른미가 아주ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100024317.1.12",
          "form": "아니다 1도 장난아니었지",
          "original_form": "아니다 1도 장난아니었지",
          "speaker_id": "1",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100024317.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 마자욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 마자욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:44"
        },
        {
          "id": "MDRW2100024317.1.14",
          "form": "다 어렸을때본거라 2를 젤 조아햇습니닼ㅋㅋㅋㅋㅋ",
          "original_form": "다 어렸을때본거라 2를 젤 조아햇습니닼ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:44"
        },
        {
          "id": "MDRW2100024317.1.15",
          "form": "그럼 그것도 보셨어요?",
          "original_form": "그럼 그것도 보셨어요?",
          "speaker_id": "1",
          "time": "20210831 00:45"
        },
        {
          "id": "MDRW2100024317.1.16",
          "form": "연애의 발견!",
          "original_form": "연애의 발견!",
          "speaker_id": "1",
          "time": "20210831 00:45"
        },
        {
          "id": "MDRW2100024317.1.17",
          "form": "아당연하쥬...",
          "original_form": "아당연하쥬...",
          "speaker_id": "2",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100024317.1.18",
          "form": "중간중간 넘 화가났지만ㅋㅋㅋㅋ",
          "original_form": "중간중간 넘 화가났지만ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100024317.1.19",
          "form": "정유미배우는",
          "original_form": "정유미배우는",
          "speaker_id": "1",
          "time": "20210831 00:48"
        },
        {
          "id": "MDRW2100024317.1.20",
          "form": "로코가 진짜 찰떡같아요ㅋㅋㅋㅋ",
          "original_form": "로코가 진짜 찰떡같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:48"
        },
        {
          "id": "MDRW2100024317.1.21",
          "form": "그냥 한여름 그 자체",
          "original_form": "그냥 한여름 그 자체",
          "speaker_id": "1",
          "time": "20210831 00:48"
        },
        {
          "id": "MDRW2100024317.1.22",
          "form": "맞어요 필모 절대놓치지않구다봄 ㅠㅋㅋ",
          "original_form": "맞어요 필모 절대놓치지않구다봄 ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024317.1.23",
          "form": "케세라세라로 첨봤었는데 ㅋㅋ  그건진짜피폐했는뎈ㅋㅋㅋㅋㅋ",
          "original_form": "케세라세라로 첨봤었는데 ㅋㅋ  그건진짜피폐했는뎈ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:50"
        },
        {
          "id": "MDRW2100024317.1.24",
          "form": "오 저 그건안봣어요",
          "original_form": "오 저 그건안봣어요",
          "speaker_id": "1",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100024317.1.25",
          "form": "그거 리메이크 한다는 얘기있던데 맞죠?",
          "original_form": "그거 리메이크 한다는 얘기있던데 맞죠?",
          "speaker_id": "1",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100024317.1.26",
          "form": "헉 맞아요 얘기가 없넹",
          "original_form": "헉 맞아요 얘기가 없넹",
          "speaker_id": "2",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024317.1.27",
          "form": "그거 ...도진짜 ㅋㅋ 마라맛이에요 그건",
          "original_form": "그거 ...도진짜 ㅋㅋ 마라맛이에요 그건",
          "speaker_id": "2",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024317.1.28",
          "form": "연발은 순한맛 그건 마라맛ㅋㅋㅋㅋ",
          "original_form": "연발은 순한맛 그건 마라맛ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024317.1.29",
          "form": "오진짜요?ㅋㅋㅋㅋㅋ",
          "original_form": "오진짜요?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024317.1.30",
          "form": "혹하는데요?",
          "original_form": "혹하는데요?",
          "speaker_id": "1",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024317.1.31",
          "form": "지금봐도 안 촌스러울까요?",
          "original_form": "지금봐도 안 촌스러울까요?",
          "speaker_id": "1",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024317.1.32",
          "form": "아 잠깐,,,그거 에릭나와요?",
          "original_form": "아 잠깐,,,그거 에릭나와요?",
          "speaker_id": "1",
          "time": "20210831 00:57"
        },
        {
          "id": "MDRW2100024317.1.33",
          "form": "네네네!에릭나옴",
          "original_form": "네네네!에릭나옴",
          "speaker_id": "2",
          "time": "20210831 00:58"
        },
        {
          "id": "MDRW2100024317.1.34",
          "form": "촌스럽지만 지금도 볼만해요ㅋㅋㅋㅋㅋ",
          "original_form": "촌스럽지만 지금도 볼만해요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:59"
        },
        {
          "id": "MDRW2100024317.1.35",
          "form": "명작임 ㅋㅋㅋ",
          "original_form": "명작임 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:59"
        },
        {
          "id": "MDRW2100024317.1.36",
          "form": "어 그러면 에릭이랑 정유미는",
          "original_form": "어 그러면 에릭이랑 정유미는",
          "speaker_id": "1",
          "time": "20210831 01:06"
        },
        {
          "id": "MDRW2100024317.1.37",
          "form": "두번 같이 찍었네요?? 케세라랑 연발까지",
          "original_form": "두번 같이 찍었네요?? 케세라랑 연발까지",
