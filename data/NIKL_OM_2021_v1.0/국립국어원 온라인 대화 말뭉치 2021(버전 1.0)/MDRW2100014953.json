      "utterance": [
        {
          "id": "MDRW2100014953.1.1",
          "form": "안녕하세요 혹시 종교가 있으세요?",
          "original_form": "안녕하세요 혹시 종교가 있으세요?",
          "speaker_id": "1",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014953.1.2",
          "form": "불교요",
          "original_form": "불교요",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100014953.1.3",
          "form": "저도요 반갑습니당",
          "original_form": "저도요 반갑습니당",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100014953.1.4",
          "form": "종교활동 열씸히 하는 편이세요,.",
          "original_form": "종교활동 열씸히 하는 편이세요,.",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100014953.1.5",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100014953.1.6",
          "form": "그렇진 않아요.",
          "original_form": "그렇진 않아요.",
          "speaker_id": "2",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100014953.1.7",
          "form": "그러시군용 저도 그냥 마음으로만 기도드리는 정도에요",
          "original_form": "그러시군용 저도 그냥 마음으로만 기도드리는 정도에요",
          "speaker_id": "1",
          "time": "20210826 09:49"
        },
        {
          "id": "MDRW2100014953.1.8",
          "form": "초파일 연등점화정도하는 듯 해요",
          "original_form": "초파일 연등점화정도하는 듯 해요",
          "speaker_id": "1",
          "time": "20210826 09:49"
        },
        {
          "id": "MDRW2100014953.1.9",
          "form": "저도 비슷해요.",
          "original_form": "저도 비슷해요.",
          "speaker_id": "2",
          "time": "20210826 11:43"
        },
        {
          "id": "MDRW2100014953.1.10",
          "form": "저는 불경읽는것도 좋아해요",
          "original_form": "저는 불경읽는것도 좋아해요",
          "speaker_id": "1",
          "time": "20210826 13:39"
        },
        {
          "id": "MDRW2100014953.1.11",
          "form": "오. 정말요?",
          "original_form": "오. 정말요?",
          "speaker_id": "2",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100014953.1.12",
          "form": "네 마음에 평안을 주더라구요",
          "original_form": "네 마음에 평안을 주더라구요",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100014953.1.13",
          "form": "독경듣고 있음 잠도 잘 오고",
          "original_form": "독경듣고 있음 잠도 잘 오고",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100014953.1.14",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100014953.1.15",
          "form": "저희 엄마는 저보고 좀 읽으라시더라구요",
          "original_form": "저희 엄마는 저보고 좀 읽으라시더라구요",
          "speaker_id": "2",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100014953.1.16",
          "form": "그러셨군요 저도 강추강추",
          "original_form": "그러셨군요 저도 강추강추",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100014953.1.17",
          "form": "어머님께서도 불교쪽이신가봐요",
          "original_form": "어머님께서도 불교쪽이신가봐요",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100014953.1.18",
          "form": "네. 아주 신실하시죠.",
          "original_form": "네. 아주 신실하시죠.",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100014953.1.19",
          "form": "매일 절에 가세요",
          "original_form": "매일 절에 가세요",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100014953.1.20",
          "form": "우와 싑지않은 일일텐데 대단하시네요",
          "original_form": "우와 싑지않은 일일텐데 대단하시네요",
          "speaker_id": "1",
          "time": "20210826 18:42"
        }
      ]
