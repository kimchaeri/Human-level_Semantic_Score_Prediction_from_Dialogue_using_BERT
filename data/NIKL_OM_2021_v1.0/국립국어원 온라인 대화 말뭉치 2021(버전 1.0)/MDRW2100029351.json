      "utterance": [
        {
          "id": "MDRW2100029351.1.1",
          "form": "넷플가튼거 보세요?",
          "original_form": "넷플가튼거 보세요?",
          "speaker_id": "1",
          "time": "20210903 02:27"
        },
        {
          "id": "MDRW2100029351.1.2",
          "form": "아뇨ㅎㅎ 본방재방 유튜브만 봅니다",
          "original_form": "아뇨ㅎㅎ 본방재방 유튜브만 봅니다",
          "speaker_id": "2",
          "time": "20210903 02:28"
        },
        {
          "id": "MDRW2100029351.1.3",
          "form": "아하 어떤거조아하세요",
          "original_form": "아하 어떤거조아하세요",
          "speaker_id": "1",
          "time": "20210903 02:29"
        },
        {
          "id": "MDRW2100029351.1.4",
          "form": "돈도 돈이고 시간까지 어마어마하게 먹을거라ㅎㅎ",
          "original_form": "돈도 돈이고 시간까지 어마어마하게 먹을거라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 02:29"
        },
        {
          "id": "MDRW2100029351.1.5",
          "form": "예능도 보고 드라마도 보고 가끔 뉴스도봐요ㅎㅎ",
          "original_form": "예능도 보고 드라마도 보고 가끔 뉴스도봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 02:29"
        },
        {
          "id": "MDRW2100029351.1.6",
          "form": "아하.. 드라마어떤거요",
          "original_form": "아하.. 드라마어떤거요",
          "speaker_id": "1",
          "time": "20210903 02:30"
        },
        {
          "id": "MDRW2100029351.1.7",
          "form": "요즘은 슬의생2보고 홍천기 새로 보고있어요",
          "original_form": "요즘은 슬의생2보고 홍천기 새로 보고있어요",
          "speaker_id": "2",
          "time": "20210903 02:31"
        },
        {
          "id": "MDRW2100029351.1.8",
          "form": "아 슬의2 잼죠",
          "original_form": "아 슬의2 잼죠",
          "speaker_id": "1",
          "time": "20210903 02:31"
        },
        {
          "id": "MDRW2100029351.1.9",
          "form": "조아하는 에피나 캐릭은 ?",
          "original_form": "조아하는 에피나 캐릭은 ?",
          "speaker_id": "1",
          "time": "20210903 02:31"
        },
        {
          "id": "MDRW2100029351.1.10",
          "form": "일단 추미나 선생님, 그리고 전미도 배우님..ㅠ",
          "original_form": "일단 추미나 선생님, 그리고 전미도 배우님..ㅠ",
          "speaker_id": "2",
          "time": "20210903 02:32"
        },
        {
          "id": "MDRW2100029351.1.11",
          "form": "왜용?",
          "original_form": "왜용?",
          "speaker_id": "1",
          "time": "20210903 02:32"
        },
        {
          "id": "MDRW2100029351.1.12",
          "form": "장겨울선생님도 좋구..",
          "original_form": "장겨울선생님도 좋구..",
          "speaker_id": "2",
          "time": "20210903 02:32"
        },
        {
          "id": "MDRW2100029351.1.13",
          "form": "여자캐릭 거의 다네요 ㅋㅋㅋ",
          "original_form": "여자캐릭 거의 다네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 02:33"
        },
        {
          "id": "MDRW2100029351.1.14",
          "form": "누구하나 빼놓을수없죠ㅋㅋㅋㅋ",
          "original_form": "누구하나 빼놓을수없죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 02:33"
        },
        {
          "id": "MDRW2100029351.1.15",
          "form": "만민평등...ㅎ",
          "original_form": "만민평등...ㅎ",
          "speaker_id": "2",
          "time": "20210903 02:33"
        },
        {
          "id": "MDRW2100029351.1.16",
          "form": "남자는 왜빼용",
          "original_form": "남자는 왜빼용",
          "speaker_id": "1",
          "time": "20210903 02:33"
        },
        {
          "id": "MDRW2100029351.1.17",
          "form": "다 매력포인트가 다른걸요ㅠㅠ",
          "original_form": "다 매력포인트가 다른걸요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 02:33"
        },
        {
          "id": "MDRW2100029351.1.18",
          "form": "아이.. 그럼 다넣어야하는데요?",
          "original_form": "아이.. 그럼 다넣어야하는데요?",
          "speaker_id": "2",
          "time": "20210903 02:34"
        },
        {
          "id": "MDRW2100029351.1.19",
          "form": "ㅋㅋㅋㅋ 홍천기는 누구나와요",
          "original_form": "ㅋㅋㅋㅋ 홍천기는 누구나와요",
          "speaker_id": "1",
          "time": "20210903 02:34"
        },
        {
          "id": "MDRW2100029351.1.20",
          "form": "도재학쌤, 드래곤, 윤복이까지 넣으면 안되잖아요",
          "original_form": "도재학쌤, 드래곤, 윤복이까지 넣으면 안되잖아요",
          "speaker_id": "2",
          "time": "20210903 02:34"
        },
        {
          "id": "MDRW2100029351.1.21",
          "form": "홍천기는 안효섭배우랑 곽시양배우, 김유정배우 나옵니다",
          "original_form": "홍천기는 안효섭배우랑 곽시양배우, 김유정배우 나옵니다",
          "speaker_id": "2",
          "time": "20210903 02:35"
        },
        {
          "id": "MDRW2100029351.1.22",
          "form": "공명 배우도 나오구요ㅎㅎ",
          "original_form": "공명 배우도 나오구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 02:35"
        }
      ]
    }
