      "utterance": [
        {
          "id": "MDRW2100024539.1.1",
          "form": "언니 오늘 공모주넣었어요?",
          "original_form": "언니 오늘 공모주넣었어요?",
          "speaker_id": "1",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100024539.1.2",
          "form": "헉 맞다 잠만",
          "original_form": "헉 맞다 잠만",
          "speaker_id": "2",
          "time": "20210831 13:40"
        },
        {
          "id": "MDRW2100024539.1.3",
          "form": "했다ㅋ",
          "original_form": "했다ㅋ",
          "speaker_id": "2",
          "time": "20210831 13:42"
        },
        {
          "id": "MDRW2100024539.1.4",
          "form": "이제 했어요?ㅋ",
          "original_form": "이제 했어요?ㅋ",
          "speaker_id": "1",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100024539.1.5",
          "form": "방금했다ㅋ",
          "original_form": "방금했다ㅋ",
          "speaker_id": "2",
          "time": "20210831 13:49"
        },
        {
          "id": "MDRW2100024539.1.6",
          "form": "메타버스ㅋㅋ",
          "original_form": "메타버스ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:50"
        },
        {
          "id": "MDRW2100024539.1.7",
          "form": "얍얍 잘햇어유 ㅋㅋ",
          "original_form": "얍얍 잘햇어유 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:50"
        },
        {
          "id": "MDRW2100024539.1.8",
          "form": "SK리츠도 들어갔어요?",
          "original_form": "SK리츠도 들어갔어요?",
          "speaker_id": "1",
          "time": "20210831 13:50"
        },
        {
          "id": "MDRW2100024539.1.9",
          "form": "아니 그건 패스했다",
          "original_form": "아니 그건 패스했다",
          "speaker_id": "2",
          "time": "20210831 13:50"
        },
        {
          "id": "MDRW2100024539.1.10",
          "form": "니는?",
          "original_form": "니는?",
          "speaker_id": "2",
          "time": "20210831 13:50"
        },
        {
          "id": "MDRW2100024539.1.11",
          "form": "저도 고민고민 ㅋㅋ",
          "original_form": "저도 고민고민 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:50"
        },
        {
          "id": "MDRW2100024539.1.12",
          "form": "그거는 내일까지래서 ㅋㅋ 일단 보고 들어갈라고요 근데 뭐 올라도 몇백원인것같길래 ㅋㅋ 안할까도 싶어유",
          "original_form": "그거는 내일까지래서 ㅋㅋ 일단 보고 들어갈라고요 근데 뭐 올라도 몇백원인것같길래 ㅋㅋ 안할까도 싶어유",
          "speaker_id": "1",
          "time": "20210831 13:51"
        },
        {
          "id": "MDRW2100024539.1.13",
          "form": "ㅋㅋ 아바타이야기해야되는데",
          "original_form": "ㅋㅋ 아바타이야기해야되는데",
          "speaker_id": "2",
          "time": "20210831 13:51"
        },
        {
          "id": "MDRW2100024539.1.14",
          "form": "아바타??ㅋㅋ 뭔 아바타??",
          "original_form": "아바타??ㅋㅋ 뭔 아바타??",
          "speaker_id": "1",
          "time": "20210831 13:51"
        },
        {
          "id": "MDRW2100024539.1.15",
          "form": "친한아바타있나, ㅋㅋㅋ",
          "original_form": "친한아바타있나, ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:51"
        },
        {
          "id": "MDRW2100024539.1.16",
          "form": "메타버스ㅋㅋ",
          "original_form": "메타버스ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:51"
        },
        {
          "id": "MDRW2100024539.1.17",
          "form": "친한 아바타?? 그게 뭔데요??ㅋㅋ",
          "original_form": "친한 아바타?? 그게 뭔데요??ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:51"
        },
        {
          "id": "MDRW2100024539.1.18",
          "form": "뭐라도 던져라ㅋㅋ",
          "original_form": "뭐라도 던져라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:51"
        },
        {
          "id": "MDRW2100024539.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ언니 미안한데요 ㅋ ㅋ나 이제 폰이랑 좀 떨어질 시간이 되엇어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ언니 미안한데요 ㅋ ㅋ나 이제 폰이랑 좀 떨어질 시간이 되엇어요",
          "speaker_id": "1",
          "time": "20210831 13:51"
        },
        {
          "id": "MDRW2100024539.1.20",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210831 13:51"
        },
        {
          "id": "MDRW2100024539.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:51"
        },
        {
          "id": "MDRW2100024539.1.22",
          "form": "어디가나?",
          "original_form": "어디가나?",
          "speaker_id": "2",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100024539.1.23",
