      "utterance": [
        {
          "id": "MDRW2100036438.1.1",
          "form": "응? 아니..",
          "original_form": "응? 아니..",
          "speaker_id": "2",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100036438.1.2",
          "form": "타이머콘센트가 뭐야??",
          "original_form": "타이머콘센트가 뭐야??",
          "speaker_id": "2",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100036438.1.3",
          "form": "아까부터 자꾸 말하던대..",
          "original_form": "아까부터 자꾸 말하던대..",
          "speaker_id": "2",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100036438.1.4",
          "form": "내가 다른사람이랑 착각한건가..?",
          "original_form": "내가 다른사람이랑 착각한건가..?",
          "speaker_id": "1",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100036438.1.5",
          "form": "으응.. 그게 무슨 물건인데?",
          "original_form": "으응.. 그게 무슨 물건인데?",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036438.1.6",
          "form": "그건 시간이 지나면 전기를 차단해준대",
          "original_form": "그건 시간이 지나면 전기를 차단해준대",
          "speaker_id": "1",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036438.1.7",
          "form": "그걸 갖고싶은겅8?",
          "original_form": "그걸 갖고싶은겅8?",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036438.1.8",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036438.1.9",
          "form": "근데ㅜ전기차단이아니라",
          "original_form": "근데ㅜ전기차단이아니라",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036438.1.10",
          "form": "정해진시간에 전기 공급과 차단을 맘대로 할 수 있는 제품이네",
          "original_form": "정해진시간에 전기 공급과 차단을 맘대로 할 수 있는 제품이네",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036438.1.11",
          "form": "대단하다!",
          "original_form": "대단하다!",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036438.1.12",
          "form": "아하 나보다 더 잘아는구나",
          "original_form": "아하 나보다 더 잘아는구나",
          "speaker_id": "1",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036438.1.13",
          "form": "근데 아날로그형은 소음이 꽤 있대",
          "original_form": "근데 아날로그형은 소음이 꽤 있대",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036438.1.14",
          "form": "방금 슥삭 찾아봤지",
          "original_form": "방금 슥삭 찾아봤지",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036438.1.15",
          "form": "네이버에 검색하면 금방잉8",
          "original_form": "네이버에 검색하면 금방잉8",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036438.1.16",
          "form": "그럼 보통 어떤 가전제품을 꼽아서 쓰는거야?",
          "original_form": "그럼 보통 어떤 가전제품을 꼽아서 쓰는거야?",
          "speaker_id": "1",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036438.1.17",
          "form": "쇼핑은 네이버 아니면 쿠팡이지",
          "original_form": "쇼핑은 네이버 아니면 쿠팡이지",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036438.1.18",
          "form": "음 난로를 그렇게 하기도 하더라",
          "original_form": "음 난로를 그렇게 하기도 하더라",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036438.1.19",
          "form": "아 난로를 그렇게하면",
          "original_form": "아 난로를 그렇게하면",
          "speaker_id": "1",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036438.1.20",
          "form": "반려동물에 쫘어주면 좋대",
          "original_form": "반려동물에 쫘어주면 좋대",
          "speaker_id": "2",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036438.1.21",
          "form": "선풍기도 그렇게들 하더라",
          "original_form": "선풍기도 그렇게들 하더라",
          "speaker_id": "2",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036438.1.22",
          "form": "아 그런 용도로 쓰는구나",
          "original_form": "아 그런 용도로 쓰는구나",
          "speaker_id": "1",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036438.1.23",
          "form": "신기하다!",
          "original_form": "신기하다!",
          "speaker_id": "2",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036438.1.24",
          "form": "나는 반려동물이 없어서 몰랐어",
          "original_form": "나는 반려동물이 없어서 몰랐어",
          "speaker_id": "1",
          "time": "20210830 23:10"
        }
