          "speaker_id": "2",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025681.1.41",
          "form": "진심도덕심으로",
          "original_form": "진심도덕심으로",
          "speaker_id": "1",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025681.1.42",
          "form": "자기검열하고사니까",
          "original_form": "자기검열하고사니까",
          "speaker_id": "1",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025681.1.43",
          "form": "남자들이더그러지",
          "original_form": "남자들이더그러지",
          "speaker_id": "1",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025681.1.44",
          "form": "검열해야 할 것들이 안하고",
          "original_form": "검열해야 할 것들이 안하고",
          "speaker_id": "2",
          "time": "20210901 16:11"
