      "utterance": [
        {
          "id": "MDRW2100025681.1.1",
          "form": "나 세상에서 제일 어이없는 초콜렛있음",
          "original_form": "나 세상에서 제일 어이없는 초콜렛있음",
          "speaker_id": "2",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025681.1.2",
          "form": "뭐?",
          "original_form": "뭐?",
          "speaker_id": "1",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025681.1.3",
          "form": "\"여자의 그날을 위한 초콜렛\"",
          "original_form": "\"여자의 그날을 위한 초콜렛\"",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025681.1.4",
          "form": "싸지도 않음 거의 만원대에서 삼만원대 사이임",
          "original_form": "싸지도 않음 거의 만원대에서 삼만원대 사이임",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025681.1.5",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025681.1.6",
          "form": "그거",
          "original_form": "그거",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025681.1.7",
          "form": "네이버쇼핑인가에",
          "original_form": "네이버쇼핑인가에",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025681.1.8",
          "form": "있자ㅣ않음?",
          "original_form": "있자ㅣ않음?",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025681.1.9",
          "form": "ㅇㅇㅇㅇㄴ",
          "original_form": "ㅇㅇㅇㅇㄴ",
          "speaker_id": "2",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025681.1.10",
          "form": "그냥 초콜릿",
          "original_form": "그냥 초콜릿",
          "speaker_id": "2",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025681.1.11",
          "form": "그냥 해외 초콜릿 이름바꿔서 팔고 있는 것도 있음",
          "original_form": "그냥 해외 초콜릿 이름바꿔서 팔고 있는 것도 있음",
          "speaker_id": "2",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025681.1.12",
          "form": "어떻게든",
          "original_form": "어떻게든",
          "speaker_id": "1",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025681.1.13",
          "form": "여자돈",
          "original_form": "여자돈",
          "speaker_id": "1",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025681.1.14",
          "form": "뺏으려고",
          "original_form": "뺏으려고",
          "speaker_id": "1",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025681.1.15",
          "form": "ㄱㄴㄲ 아주그냥",
          "original_form": "ㄱㄴㄲ 아주그냥",
          "speaker_id": "2",
          "time": "20210901 15:59"
        },
        {
          "id": "MDRW2100025681.1.16",
          "form": "눈에 불을 키고 찾아",
          "original_form": "눈에 불을 키고 찾아",
          "speaker_id": "2",
          "time": "20210901 15:59"
        },
        {
          "id": "MDRW2100025681.1.17",
          "form": "그것도 있잖아 여자인 척해서 물건 팔거나 여자 대리인 세워놓는거",
          "original_form": "그것도 있잖아 여자인 척해서 물건 팔거나 여자 대리인 세워놓는거",
          "speaker_id": "2",
          "time": "20210901 16:09"
        },
        {
          "id": "MDRW2100025681.1.18",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210901 16:09"
        },
        {
          "id": "MDRW2100025681.1.19",
          "form": "그거진짜싫어",
          "original_form": "그거진짜싫어",
          "speaker_id": "1",
          "time": "20210901 16:09"
        },
        {
          "id": "MDRW2100025681.1.20",
          "form": "여자대포인으로",
          "original_form": "여자대포인으로",
          "speaker_id": "1",
          "time": "20210901 16:09"
