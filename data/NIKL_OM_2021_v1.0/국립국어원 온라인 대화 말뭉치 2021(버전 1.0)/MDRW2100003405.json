      "utterance": [
        {
          "id": "MDRW2100003405.1.1",
          "form": "일이나 알바하시나요~?",
          "original_form": "일이나 알바하시나요~?",
          "speaker_id": "1",
          "time": "20210821 11:00"
        },
        {
          "id": "MDRW2100003405.1.2",
          "form": "저 회사다니고있어요 ㅎㅎ",
          "original_form": "저 회사다니고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:43"
        },
        {
          "id": "MDRW2100003405.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 19:43"
        },
        {
          "id": "MDRW2100003405.1.4",
          "form": "우와~ 저는 프리랜서로 일하고 있습니다~ㅎㅎ",
          "original_form": "우와~ 저는 프리랜서로 일하고 있습니다~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 10:47"
        },
        {
          "id": "MDRW2100003405.1.5",
          "form": "헐 프리랜서 멋있어요!!",
          "original_form": "헐 프리랜서 멋있어요!!",
          "speaker_id": "2",
          "time": "20210823 10:57"
        },
        {
          "id": "MDRW2100003405.1.6",
          "form": "무슨일하시는지 여쭤봐도될까요?",
          "original_form": "무슨일하시는지 여쭤봐도될까요?",
          "speaker_id": "2",
          "time": "20210823 10:57"
        },
        {
          "id": "MDRW2100003405.1.7",
          "form": "아 저는 데이터라벨링 검수하고있습니다~",
          "original_form": "아 저는 데이터라벨링 검수하고있습니다~",
          "speaker_id": "1",
          "time": "20210823 19:22"
        },
        {
          "id": "MDRW2100003405.1.8",
          "form": "혹시 데이터라벨링아시나용~?",
          "original_form": "혹시 데이터라벨링아시나용~?",
          "speaker_id": "1",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100003405.1.9",
          "form": "아뇨! 어떤일이에요? 처음들어봐요",
          "original_form": "아뇨! 어떤일이에요? 처음들어봐요",
          "speaker_id": "2",
          "time": "20210823 19:26"
        },
        {
          "id": "MDRW2100003405.1.10",
          "form": "아~ 예를 들면 음성 녹음을 한 부분을 제가 맞게했는지 검수하는일? 이에요~",
          "original_form": "아~ 예를 들면 음성 녹음을 한 부분을 제가 맞게했는지 검수하는일? 이에요~",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100003405.1.11",
          "form": "우와 신기하네요~",
          "original_form": "우와 신기하네요~",
          "speaker_id": "2",
          "time": "20210824 10:22"
        },
        {
          "id": "MDRW2100003405.1.12",
          "form": "ars 이런거 하시는건가요?",
          "original_form": "ars 이런거 하시는건가요?",
          "speaker_id": "2",
          "time": "20210824 10:22"
        },
        {
          "id": "MDRW2100003405.1.13",
          "form": "잘몰라서,,",
          "original_form": "잘몰라서,,",
          "speaker_id": "2",
          "time": "20210824 10:23"
        },
        {
          "id": "MDRW2100003405.1.14",
          "form": "음 사람들이 녹음해서 보내주면 저는 잘 맞게했는지 체크하는일이에요~ 그냥 녹음을 평가하는 부분?!",
          "original_form": "음 사람들이 녹음해서 보내주면 저는 잘 맞게했는지 체크하는일이에요~ 그냥 녹음을 평가하는 부분?!",
          "speaker_id": "1",
          "time": "20210824 10:24"
        },
        {
          "id": "MDRW2100003405.1.15",
          "form": "신기해요! 자격증이 따로 있는건가요?",
          "original_form": "신기해요! 자격증이 따로 있는건가요?",
          "speaker_id": "2",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100003405.1.16",
          "form": "아니요~ 이거는 우연히 제가 라벨링 작업자로 참여하다가",
          "original_form": "아니요~ 이거는 우연히 제가 라벨링 작업자로 참여하다가",
          "speaker_id": "1",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100003405.1.17",
          "form": "검수자까지 하게 되었어요~",
          "original_form": "검수자까지 하게 되었어요~",
          "speaker_id": "1",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100003405.1.18",
          "form": "근데 계약기간이 정해져서 아마 올해까지는 할것같아요~",
          "original_form": "근데 계약기간이 정해져서 아마 올해까지는 할것같아요~",
          "speaker_id": "1",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100003405.1.19",
          "form": "잘됐네요 ㅎㅎ",
          "original_form": "잘됐네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 09:18"
        },
        {
          "id": "MDRW2100003405.1.20",
          "form": "출근하자마자 퇴근하고싶어서 큰일났어요 ㅠㅠㅋㅋ",
          "original_form": "출근하자마자 퇴근하고싶어서 큰일났어요 ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:19"
        },
        {
          "id": "MDRW2100003405.1.21",
          "form": "흐아ㅜㅠ 이건 폭풍공감이에여ㅠ",
          "original_form": "흐아ㅜㅠ 이건 폭풍공감이에여ㅠ",
          "speaker_id": "1",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100003405.1.22",
          "form": "출근도하기싫은데 일하려니ㅠㅠ",
          "original_form": "출근도하기싫은데 일하려니ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100003405.1.23",
          "form": "그니까여 ㅜㅜ 비도오고",
          "original_form": "그니까여 ㅜㅜ 비도오고",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100003405.1.24",
          "form": "출근길에도 비왓는데 퇴근길도 비오게생겼네요 ㅠ",
          "original_form": "출근길에도 비왓는데 퇴근길도 비오게생겼네요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100003405.1.25",
          "form": "오늘은 아주 쨍쨍하네요",
          "original_form": "오늘은 아주 쨍쨍하네요",
          "speaker_id": "1",
          "time": "20210826 10:20"
        },
        {
          "id": "MDRW2100003405.1.26",
          "form": "오늘은 비가 간만에 안오는것같아요~",
          "original_form": "오늘은 비가 간만에 안오는것같아요~",
          "speaker_id": "1",
          "time": "20210826 10:20"
        },
        {
          "id": "MDRW2100003405.1.27",
          "form": "다시 매미소리가 나네용ㅋㅋ",
          "original_form": "다시 매미소리가 나네용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:20"
        },
        {
          "id": "MDRW2100003405.1.28",
          "form": "여기는 약간흐린데 선선하니 좋네요 ㅎ",
          "original_form": "여기는 약간흐린데 선선하니 좋네요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100003405.1.29",
          "form": "여름끝난거같아서 너무좋아용 ㅜ",
          "original_form": "여름끝난거같아서 너무좋아용 ㅜ",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100003405.1.30",
          "form": "맞아여!! 이제 진짜 가을!!",
          "original_form": "맞아여!! 이제 진짜 가을!!",
          "speaker_id": "1",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100003405.1.31",
          "form": "일하시는데도 날씨 괜찮나요~?",
          "original_form": "일하시는데도 날씨 괜찮나요~?",
          "speaker_id": "1",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100003405.1.32",
          "form": "넹넹 사무실에만있어서 뭐 ㅎㅎ",
          "original_form": "넹넹 사무실에만있어서 뭐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100003405.1.33",
          "form": "내일 드디어금요일이네요ㅠㅠ",
          "original_form": "내일 드디어금요일이네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100003405.1.34",
          "form": "흐아 맞아요~!!",
          "original_form": "흐아 맞아요~!!",
          "speaker_id": "1",
          "time": "20210827 10:20"
        },
        {
          "id": "MDRW2100003405.1.35",
          "form": "금요일만되면 설레는 부분들이 있어용ㅋㅋ",
          "original_form": "금요일만되면 설레는 부분들이 있어용ㅋㅋ",
