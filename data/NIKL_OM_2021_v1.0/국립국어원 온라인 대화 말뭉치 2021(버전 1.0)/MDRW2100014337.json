      "utterance": [
        {
          "id": "MDRW2100014337.1.1",
          "form": "커뮤니티 뭐 하시는거 있으세요?",
          "original_form": "커뮤니티 뭐 하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 18:53"
        },
        {
          "id": "MDRW2100014337.1.2",
          "form": "저는 음 인스타도 커뮤니티인가요 아니겠져?",
          "original_form": "저는 음 인스타도 커뮤니티인가요 아니겠져?",
          "speaker_id": "1",
          "time": "20210825 18:53"
        },
        {
          "id": "MDRW2100014337.1.3",
          "form": "인스타도 커뮤니티죠",
          "original_form": "인스타도 커뮤니티죠",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014337.1.4",
          "form": "저 인스타해요",
          "original_form": "저 인스타해요",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014337.1.5",
          "form": "인스타에서는 주로 업로드를 하시나요 피드를 구경하시나요",
          "original_form": "인스타에서는 주로 업로드를 하시나요 피드를 구경하시나요",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014337.1.6",
          "form": "저는 피드 구경요! 근데 그것도 자주하는건 아니고",
          "original_form": "저는 피드 구경요! 근데 그것도 자주하는건 아니고",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100014337.1.7",
          "form": "그냥 카톡이랑 유튭을 제일 많이 하는것같아요",
          "original_form": "그냥 카톡이랑 유튭을 제일 많이 하는것같아요",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100014337.1.8",
          "form": "피드구경 재밌죠",
          "original_form": "피드구경 재밌죠",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100014337.1.9",
          "form": "유튜브로는 뭐 보시나요",
          "original_form": "유튜브로는 뭐 보시나요",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100014337.1.10",
          "form": "저 게임영상도 보고 요리하는것도 보고 그럽니당",
          "original_form": "저 게임영상도 보고 요리하는것도 보고 그럽니당",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100014337.1.11",
          "form": "좋아하는 유튜버 있어오ㅡ?",
          "original_form": "좋아하는 유튜버 있어오ㅡ?",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100014337.1.12",
          "form": "딱히 없는데 요즘엔 우왁굳 옛날?영상...? 좀 보는것같아여",
          "original_form": "딱히 없는데 요즘엔 우왁굳 옛날?영상...? 좀 보는것같아여",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100014337.1.13",
          "form": "음..  그러시구나",
          "original_form": "음..  그러시구나",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100014337.1.14",
          "form": "요즘 틱톡 유행하던데 틱톡같은거 하시나오",
          "original_form": "요즘 틱톡 유행하던데 틱톡같은거 하시나오",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100014337.1.15",
          "form": "전 구경만요 ㅋㅋㅋㅋ",
          "original_form": "전 구경만요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100014337.1.16",
          "form": "name2은여",
          "original_form": "&name2&은여",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100014337.1.17",
          "form": "저도 가끔 뜰 때? 구경하는 거 같아요",
          "original_form": "저도 가끔 뜰 때? 구경하는 거 같아요",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100014337.1.18",
          "form": "어때요 감성에 맞으시나요",
          "original_form": "어때요 감성에 맞으시나요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100014337.1.19",
          "form": "그냥 이쁜 언니들 얼굴만 감성에 맞는듯해요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그냥 이쁜 언니들 얼굴만 감성에 맞는듯해요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100014337.1.20",
          "form": "전 약간 틱톡세대가 아니라 그런가 뭐랄까..",
          "original_form": "전 약간 틱톡세대가 아니라 그런가 뭐랄까..",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100014337.1.21",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100014337.1.22",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100014337.1.23",
          "form": "나이가 어떻게되시는데요?",
          "original_form": "나이가 어떻게되시는데요?",
