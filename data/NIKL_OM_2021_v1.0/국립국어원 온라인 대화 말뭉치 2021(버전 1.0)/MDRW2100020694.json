      "utterance": [
        {
          "id": "MDRW2100020694.1.1",
          "form": "그냥 제가 먹는거면 다 좋아한다는 건줄ㅋㅋㅋㅋㅋㅋ알고ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그냥 제가 먹는거면 다 좋아한다는 건줄ㅋㅋㅋㅋㅋㅋ알고ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020694.1.2",
          "form": "혹시.. 삼겹살 드시고 마지막에 밥 볶아드시나요",
          "original_form": "혹시.. 삼겹살 드시고 마지막에 밥 볶아드시나요",
          "speaker_id": "1",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020694.1.3",
          "form": "와 그건 아직 못해봤는데",
          "original_form": "와 그건 아직 못해봤는데",
          "speaker_id": "2",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020694.1.4",
          "form": "짱맛있을거같네요",
          "original_form": "짱맛있을거같네요",
          "speaker_id": "2",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020694.1.5",
          "form": "아 안타깝다",
          "original_form": "아 안타깝다",
          "speaker_id": "1",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020694.1.6",
          "form": "아직 경험해보시지 못하셨다니",
          "original_form": "아직 경험해보시지 못하셨다니",
          "speaker_id": "1",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020694.1.7",
          "form": "약간 눌러붙게 볶아먹어야겠어요",
          "original_form": "약간 눌러붙게 볶아먹어야겠어요",
          "speaker_id": "2",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020694.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020694.1.9",
          "form": "아시는구나",
          "original_form": "아시는구나",
          "speaker_id": "1",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020694.1.10",
          "form": "눌러야 꼬숩잖아요",
          "original_form": "눌러야 꼬숩잖아요",
          "speaker_id": "1",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020694.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020694.1.12",
          "form": "안눌러붙으면 아쉽고",
          "original_form": "안눌러붙으면 아쉽고",
          "speaker_id": "2",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020694.1.13",
          "form": "박박 긁어먹어야한다구요",
          "original_form": "박박 긁어먹어야한다구요",
          "speaker_id": "1",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020694.1.14",
          "form": "그럼 닭갈비드실 때 볶음밥해드시나",
          "original_form": "그럼 닭갈비드실 때 볶음밥해드시나",
          "speaker_id": "1",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020694.1.15",
          "form": "네 물론!",
          "original_form": "네 물론!",
          "speaker_id": "2",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020694.1.16",
          "form": "닭갈비집은 가면 다 볶아먹어서",
          "original_form": "닭갈비집은 가면 다 볶아먹어서",
          "speaker_id": "2",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020694.1.17",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020694.1.18",
          "form": "저도 자연스레 배웠답니다",
          "original_form": "저도 자연스레 배웠답니다",
          "speaker_id": "2",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020694.1.19",
          "form": "밥을 안볶는것 유죄..",
          "original_form": "밥을 안볶는것 유죄..",
          "speaker_id": "1",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020694.1.20",
          "form": "하 볶는다고 하니까 급",
          "original_form": "하 볶는다고 하니까 급",
          "speaker_id": "2",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020694.1.21",
          "form": "밥볶는거 식사아니고 디져트잖아요",
          "original_form": "밥볶는거 식사아니고 디져트잖아요",
          "speaker_id": "1",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020694.1.22",
          "form": "콩불도 먹고 싶네요",
          "original_form": "콩불도 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020694.1.23",
          "form": "아 콩불",
          "original_form": "아 콩불",
          "speaker_id": "1",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020694.1.24",
          "form": "아닌데여",
          "original_form": "아닌데여",
          "speaker_id": "2",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020694.1.25",
          "form": "너무좋아",
          "original_form": "너무좋아",
          "speaker_id": "1",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020694.1.26",
          "form": "아니라구요?",
          "original_form": "아니라구요?",
          "speaker_id": "1",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020694.1.27",
          "form": "디저트는 따로 또 먹어야는데요",
          "original_form": "디저트는 따로 또 먹어야는데요",
