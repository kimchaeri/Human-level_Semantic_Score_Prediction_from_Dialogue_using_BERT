      "utterance": [
        {
          "id": "MDRW2100033068.1.1",
          "form": "오 꿀물 좋지~",
          "original_form": "오 꿀물 좋지~",
          "speaker_id": "2",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100033068.1.2",
          "form": "애들 학교 다녀와서 피곤할 것 같아서 피로회복겸 ㅎㅎ",
          "original_form": "애들 학교 다녀와서 피곤할 것 같아서 피로회복겸 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100033068.1.3",
          "form": "꿀이 순수 당이라 체력회복에 좋지",
          "original_form": "꿀이 순수 당이라 체력회복에 좋지",
          "speaker_id": "2",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100033068.1.4",
          "form": "시원하게 타서 얼음 넣어주니 좋아하네",
          "original_form": "시원하게 타서 얼음 넣어주니 좋아하네",
          "speaker_id": "1",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100033068.1.5",
          "form": "집에 있는 꿀 자체가 좋은거라 ㅎㅎ",
          "original_form": "집에 있는 꿀 자체가 좋은거라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100033068.1.6",
          "form": "엄마가 매년 주셔서 감사하네",
          "original_form": "엄마가 매년 주셔서 감사하네",
          "speaker_id": "1",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100033068.1.7",
          "form": "그러게 매번 꿀 주셔서 잘 먹네",
          "original_form": "그러게 매번 꿀 주셔서 잘 먹네",
          "speaker_id": "2",
          "time": "20210906 15:23"
        },
        {
          "id": "MDRW2100033068.1.8",
          "form": "청룡골 벌꿀이 품질이 좋은 것 같아",
          "original_form": "청룡골 벌꿀이 품질이 좋은 것 같아",
          "speaker_id": "1",
          "time": "20210906 15:23"
        },
        {
          "id": "MDRW2100033068.1.9",
          "form": "확실히 마트 꿀이랑은 다르더라",
          "original_form": "확실히 마트 꿀이랑은 다르더라",
          "speaker_id": "2",
          "time": "20210906 15:23"
        },
        {
          "id": "MDRW2100033068.1.10",
          "form": "ㅇㅇ 숟가락으로 퍼보면 확실히 품질이 달라",
          "original_form": "ㅇㅇ 숟가락으로 퍼보면 확실히 품질이 달라",
          "speaker_id": "1",
          "time": "20210906 15:23"
        },
        {
          "id": "MDRW2100033068.1.11",
          "form": "맛도 완전히 다르더만 ㅎㅎ",
          "original_form": "맛도 완전히 다르더만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100033068.1.12",
          "form": "애들 건강을 위해서 매일 물에 타서 줘야겠다",
          "original_form": "애들 건강을 위해서 매일 물에 타서 줘야겠다",
          "speaker_id": "1",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100033068.1.13",
          "form": "좋지~ 다른 음료수 먹는 것보다는",
          "original_form": "좋지~ 다른 음료수 먹는 것보다는",
          "speaker_id": "2",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100033068.1.14",
          "form": "이번 명절에 갈 때 회 좀 사살까?",
          "original_form": "이번 명절에 갈 때 회 좀 사살까?",
          "speaker_id": "1",
          "time": "20210906 15:40"
