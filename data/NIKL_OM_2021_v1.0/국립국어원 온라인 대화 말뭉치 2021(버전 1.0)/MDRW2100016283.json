      "utterance": [
        {
          "id": "MDRW2100016283.1.1",
          "form": "이번달에 쇼핑했어?",
          "original_form": "이번달에 쇼핑했어?",
          "speaker_id": "1",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100016283.1.2",
          "form": "난 이번에 거의 공부세트 맞췄어ㅎ",
          "original_form": "난 이번에 거의 공부세트 맞췄어ㅎ",
          "speaker_id": "2",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016283.1.3",
          "form": "독서대랑 의자랑 키보드마우스 받침대",
          "original_form": "독서대랑 의자랑 키보드마우스 받침대",
          "speaker_id": "2",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016283.1.4",
          "form": "장비만 갖췄지",
          "original_form": "장비만 갖췄지",
          "speaker_id": "2",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016283.1.5",
          "form": "너는 뭐 샀어?",
          "original_form": "너는 뭐 샀어?",
          "speaker_id": "2",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016283.1.6",
          "form": "오 전문적인데",
          "original_form": "오 전문적인데",
          "speaker_id": "1",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100016283.1.7",
          "form": "키보드 마우스는 뭐샀어?",
          "original_form": "키보드 마우스는 뭐샀어?",
          "speaker_id": "1",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100016283.1.8",
          "form": "난노트북 거치대 휴대용!",
          "original_form": "난노트북 거치대 휴대용!",
          "speaker_id": "1",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100016283.1.9",
          "form": "그리고 계랑컵이랑.. 머리삔이랑..",
          "original_form": "그리고 계랑컵이랑.. 머리삔이랑..",
          "speaker_id": "1",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100016283.1.10",
          "form": "아 키보드하고 마우스가 아니라 그것들의 받침대ㅋㅋㅋ",
          "original_form": "아 키보드하고 마우스가 아니라 그것들의 받침대ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100016283.1.11",
          "form": "오 거치대 편해? 휴대할만해?",
          "original_form": "오 거치대 편해? 휴대할만해?",
          "speaker_id": "2",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100016283.1.12",
          "form": "아아 ㅋㅋ ㅋ의자도 새로샀으면 돈 많이 썼겠다",
          "original_form": "아아 ㅋㅋ ㅋ의자도 새로샀으면 돈 많이 썼겠다",
          "speaker_id": "1",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100016283.1.13",
          "form": "오늘..도착했어.ㅓ.^^..",
          "original_form": "오늘..도착했어.ㅓ.^^..",
          "speaker_id": "1",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016283.1.14",
          "form": "계량컵..은 뭐 만드려고?",
          "original_form": "계량컵..은 뭐 만드려고?",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016283.1.15",
          "form": "앗..그렇구나",
          "original_form": "앗..그렇구나",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016283.1.16",
          "form": "거치대..오늘 도착..ㅎㅎ",
          "original_form": "거치대..오늘 도착..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016283.1.17",
          "form": "계량컵은 그냥 유리로 된거 사봤어 뜨거운 물도 담을 수 있게",
          "original_form": "계량컵은 그냥 유리로 된거 사봤어 뜨거운 물도 담을 수 있게",
          "speaker_id": "1",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016283.1.18",
          "form": "의자 23만원짜리라 내 몸을 최대한 의자에 맞추고 있어",
          "original_form": "의자 23만원짜리라 내 몸을 최대한 의자에 맞추고 있어",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016283.1.19",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016283.1.20",
          "form": "계량컵으로 계량래서 베이킹해줘",
          "original_form": "계량컵으로 계량래서 베이킹해줘",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016283.1.21",
          "form": "맛있겠다^^",
          "original_form": "맛있겠다^^",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016283.1.22",
          "form": "오 투자 쫌 했네",
          "original_form": "오 투자 쫌 했네",
          "speaker_id": "1",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016283.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ 근데 계량컵 잘못산듯 1리터짜리샀거든",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 근데 계량컵 잘못산듯 1리터짜리샀거든",
          "speaker_id": "1",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100016283.1.24",
          "form": "의자 편해",
          "original_form": "의자 편해",
          "speaker_id": "1",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100016283.1.25",
          "form": "1리터면 뭘 계량하는 거니..?",
          "original_form": "1리터면 뭘 계량하는 거니..?",
          "speaker_id": "2",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100016283.1.26",
          "form": "음,, 모르겠어,,, 500리터 샀어야 하나봐.",
          "original_form": "음,, 모르겠어,,, 500리터 샀어야 하나봐.",
          "speaker_id": "1",
          "time": "20210826 11:43"
        },
        {
          "id": "MDRW2100016283.1.27",
          "form": "다시보니가 1리터짜리가 제일 싸더라고ㅎㅎ..",
          "original_form": "다시보니가 1리터짜리가 제일 싸더라고ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210826 11:43"
        },
        {
          "id": "MDRW2100016283.1.28",
          "form": "사람들이 제일 안사는 거였나봐 ㅎ",
          "original_form": "사람들이 제일 안사는 거였나봐 ㅎ",
          "speaker_id": "1",
          "time": "20210826 11:43"
        },
        {
          "id": "MDRW2100016283.1.29",
          "form": "500리터는 너무 크지 않을까..?",
          "original_form": "500리터는 너무 크지 않을까..?",
          "speaker_id": "2",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100016283.1.30",
          "form": "1리터면 그냥 물병으로 써도 괜찮을 것 같아",
          "original_form": "1리터면 그냥 물병으로 써도 괜찮을 것 같아",
          "speaker_id": "2",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100016283.1.31",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016283.1.32",
          "form": "장난하니?",
          "original_form": "장난하니?",
          "speaker_id": "1",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016283.1.33",
          "form": "근데 물병이 아니고 걍 큰 그릇같아...ㅎㅎ",
          "original_form": "근데 물병이 아니고 걍 큰 그릇같아...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016283.1.34",
          "form": "조번에 팬케이크만들 때 씀 ㅋㅋ",
          "original_form": "조번에 팬케이크만들 때 씀 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016283.1.35",
