      "utterance": [
        {
          "id": "MDRW2100029315.1.1",
          "form": "계획있으세요?",
          "original_form": "계획있으세요?",
          "speaker_id": "2",
          "time": "20210903 01:24"
        },
        {
          "id": "MDRW2100029315.1.2",
          "form": "무슨 계획이요?",
          "original_form": "무슨 계획이요?",
          "speaker_id": "1",
          "time": "20210903 01:24"
        },
        {
          "id": "MDRW2100029315.1.3",
          "form": "결혼!",
          "original_form": "결혼!",
          "speaker_id": "2",
          "time": "20210903 01:24"
        },
        {
          "id": "MDRW2100029315.1.4",
          "form": "ㅎㅎ 전 비혼주의자입니다",
          "original_form": "ㅎㅎ 전 비혼주의자입니다",
          "speaker_id": "1",
          "time": "20210903 01:24"
        },
        {
          "id": "MDRW2100029315.1.5",
          "form": "요즘 거의 안하는추세고 해도 딩크라",
          "original_form": "요즘 거의 안하는추세고 해도 딩크라",
          "speaker_id": "2",
          "time": "20210903 01:24"
        },
        {
          "id": "MDRW2100029315.1.6",
          "form": "그것도 좋은것 같습니다",
          "original_form": "그것도 좋은것 같습니다",
          "speaker_id": "2",
          "time": "20210903 01:24"
        },
        {
          "id": "MDRW2100029315.1.7",
          "form": "큰 신념이 있는 건 아니고ㅎ",
          "original_form": "큰 신념이 있는 건 아니고ㅎ",
          "speaker_id": "1",
          "time": "20210903 01:26"
        },
        {
          "id": "MDRW2100029315.1.8",
          "form": "같이 살면 힘듭니다 ㅋㅋ",
          "original_form": "같이 살면 힘듭니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:26"
        },
        {
          "id": "MDRW2100029315.1.9",
          "form": "작은 무언가 있나요?",
          "original_form": "작은 무언가 있나요?",
          "speaker_id": "2",
          "time": "20210903 01:26"
        },
        {
          "id": "MDRW2100029315.1.10",
          "form": "아ㅋㅋ 그래도 좋쥬?",
          "original_form": "아ㅋㅋ 그래도 좋쥬?",
          "speaker_id": "1",
          "time": "20210903 01:26"
        },
        {
          "id": "MDRW2100029315.1.11",
          "form": "반반?",
          "original_form": "반반?",
          "speaker_id": "2",
          "time": "20210903 01:26"
        },
        {
          "id": "MDRW2100029315.1.12",
          "form": "무 성 애 자 가 왜 나쁜말이죠ㅜ",
          "original_form": "무 성 애 자 가 왜 나쁜말이죠ㅜ",
          "speaker_id": "1",
          "time": "20210903 01:26"
        },
        {
          "id": "MDRW2100029315.1.13",
          "form": "암튼 그래서 연애가 안 맞아요",
          "original_form": "암튼 그래서 연애가 안 맞아요",
          "speaker_id": "1",
          "time": "20210903 01:26"
        },
        {
          "id": "MDRW2100029315.1.14",
          "form": "무 성 애 자가 뭐죠?",
          "original_form": "무 성 애 자가 뭐죠?",
          "speaker_id": "2",
          "time": "20210903 01:27"
        },
        {
          "id": "MDRW2100029315.1.15",
          "form": "만나는 게 너무 스트레스라 관뒀죠ㅎ",
          "original_form": "만나는 게 너무 스트레스라 관뒀죠ㅎ",
          "speaker_id": "1",
          "time": "20210903 01:27"
        },
        {
          "id": "MDRW2100029315.1.16",
          "form": "그냥 남자에게도 여자에게도 생각이 없는 거요!",
          "original_form": "그냥 남자에게도 여자에게도 생각이 없는 거요!",
          "speaker_id": "1",
          "time": "20210903 01:28"
        },
        {
          "id": "MDRW2100029315.1.17",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 01:28"
        },
        {
          "id": "MDRW2100029315.1.18",
          "form": "사람들 만나고 그러는건 좋은데",
          "original_form": "사람들 만나고 그러는건 좋은데",
          "speaker_id": "1",
          "time": "20210903 01:28"
        },
        {
          "id": "MDRW2100029315.1.19",
          "form": "연애는 별로?",
          "original_form": "연애는 별로?",
          "speaker_id": "2",
          "time": "20210903 01:28"
        },
        {
          "id": "MDRW2100029315.1.20",
          "form": "사귀는 단계가 되면 엄청 스트레스더라고요ㅎㅎ",
          "original_form": "사귀는 단계가 되면 엄청 스트레스더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:28"
        },
        {
          "id": "MDRW2100029315.1.21",
          "form": "네 친구단계까지는 남녀노소 안 가리고 좋아요",
          "original_form": "네 친구단계까지는 남녀노소 안 가리고 좋아요",
          "speaker_id": "1",
          "time": "20210903 01:29"
        },
        {
          "id": "MDRW2100029315.1.22",
          "form": "음 그럴수도 있겠습니다",
          "original_form": "음 그럴수도 있겠습니다",
          "speaker_id": "2",
          "time": "20210903 01:29"
        }
      ]
