      "utterance": [
        {
          "id": "MDRW2100035758.1.1",
          "form": "요즘은 후라이드도 매운게 많아서요ㅜ",
          "original_form": "요즘은 후라이드도 매운게 많아서요ㅜ",
          "speaker_id": "2",
          "time": "20210830 00:52"
        },
        {
          "id": "MDRW2100035758.1.2",
          "form": "아 근데 저희애들이 치킨자체를 잘안먹어요^^;;",
          "original_form": "아 근데 저희애들이 치킨자체를 잘안먹어요^^;;",
          "speaker_id": "2",
          "time": "20210830 00:52"
        },
        {
          "id": "MDRW2100035758.1.3",
          "form": "아 맞아요ㅜㅜ 후라이드 참 잘하는집 아세요? 거기 매워요",
          "original_form": "아 맞아요ㅜㅜ 후라이드 참 잘하는집 아세요? 거기 매워요",
          "speaker_id": "1",
          "time": "20210830 00:52"
        },
        {
          "id": "MDRW2100035758.1.4",
          "form": "아하ㅎㅎ 건강식 위주로 챙겨주세용?",
          "original_form": "아하ㅎㅎ 건강식 위주로 챙겨주세용?",
          "speaker_id": "1",
          "time": "20210830 00:53"
        },
        {
          "id": "MDRW2100035758.1.5",
          "form": "아니욬ㅋㅋㅋㅋ  애들이 잘안먹더라구요 치킨피자를",
          "original_form": "아니욬ㅋㅋㅋㅋ  애들이 잘안먹더라구요 치킨피자를",
          "speaker_id": "2",
          "time": "20210830 00:54"
        },
        {
          "id": "MDRW2100035758.1.6",
          "form": "요즘엔 피자는 좀 먹네요",
          "original_form": "요즘엔 피자는 좀 먹네요",
          "speaker_id": "2",
          "time": "20210830 00:54"
        },
        {
          "id": "MDRW2100035758.1.7",
          "form": "와 좋으시겠어요",
          "original_form": "와 좋으시겠어요",
          "speaker_id": "1",
          "time": "20210830 00:55"
        },
        {
          "id": "MDRW2100035758.1.8",
          "form": "전 그냥 그때그때 아무거나 시키는편이에용 남편이랑ㅎㅎ",
          "original_form": "전 그냥 그때그때 아무거나 시키는편이에용 남편이랑ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:55"
        },
        {
          "id": "MDRW2100035758.1.9",
          "form": "엄마 입장에서는 아이들이 그런거 안찾으면 넘 좋죠",
          "original_form": "엄마 입장에서는 아이들이 그런거 안찾으면 넘 좋죠",
          "speaker_id": "1",
          "time": "20210830 00:55"
        },
        {
          "id": "MDRW2100035758.1.10",
          "form": "저 얼마전에 뽀로로 떡볶이 먹었는데",
          "original_form": "저 얼마전에 뽀로로 떡볶이 먹었는데",
          "speaker_id": "1",
          "time": "20210830 00:55"
        },
        {
          "id": "MDRW2100035758.1.11",
          "form": "ㅋㅋㄱㅋ  과자나젤리 초코 사탕 좋아하는건 똑같은 아이들이라^^;;",
          "original_form": "ㅋㅋㄱㅋ  과자나젤리 초코 사탕 좋아하는건 똑같은 아이들이라^^;;",
          "speaker_id": "2",
          "time": "20210830 00:55"
        },
        {
          "id": "MDRW2100035758.1.12",
          "form": "너무 맛있더라구요ㅋㅋㅋ",
          "original_form": "너무 맛있더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:55"
        },
        {
          "id": "MDRW2100035758.1.13",
          "form": "뽀로로 떡볶이 맛나나요? 안먹어봐서ㅜ",
          "original_form": "뽀로로 떡볶이 맛나나요? 안먹어봐서ㅜ",
          "speaker_id": "2",
          "time": "20210830 00:55"
        },
        {
          "id": "MDRW2100035758.1.14",
          "form": "아하 ㅋㅋㅋㅋ 그래도 그게 어딘가용!",
          "original_form": "아하 ㅋㅋㅋㅋ 그래도 그게 어딘가용!",
          "speaker_id": "1",
          "time": "20210830 00:55"
        },
        {
          "id": "MDRW2100035758.1.15",
          "form": "네네 애기들꺼라",
          "original_form": "네네 애기들꺼라",
          "speaker_id": "1",
          "time": "20210830 00:55"
        },
        {
          "id": "MDRW2100035758.1.16",
          "form": "간도 심심하고",
          "original_form": "간도 심심하고",
          "speaker_id": "1",
          "time": "20210830 00:55"
        },
        {
          "id": "MDRW2100035758.1.17",
          "form": "자극적이지않고 맛있어요",
          "original_form": "자극적이지않고 맛있어요",
          "speaker_id": "1",
          "time": "20210830 00:56"
        },
        {
          "id": "MDRW2100035758.1.18",
          "form": "오 저희아이들은  조금만 매워도 잘 못먹어서요ㅠ",
          "original_form": "오 저희아이들은  조금만 매워도 잘 못먹어서요ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:56"
        },
        {
          "id": "MDRW2100035758.1.19",
          "form": "저도 어릴때 김치도 물에 씻어먹고",
          "original_form": "저도 어릴때 김치도 물에 씻어먹고",
          "speaker_id": "1",
          "time": "20210830 00:56"
        },
        {
          "id": "MDRW2100035758.1.20",
          "form": "뽀로로 짜장도 있지않나요",
          "original_form": "뽀로로 짜장도 있지않나요",
          "speaker_id": "2",
          "time": "20210830 00:56"
        },
        {
          "id": "MDRW2100035758.1.21",
          "form": "라면도 매워서 씻어먹고 그랬네요ㅋㅋㅋ",
          "original_form": "라면도 매워서 씻어먹고 그랬네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:56"
        },
        {
          "id": "MDRW2100035758.1.22",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210830 00:56"
        },
        {
          "id": "MDRW2100035758.1.23",
          "form": "맛없었던걸로 기억하는...",
          "original_form": "맛없었던걸로 기억하는...",
          "speaker_id": "2",
          "time": "20210830 00:56"
        },
        {
          "id": "MDRW2100035758.1.24",
          "form": "전 괜찮던데용?ㅎㅎ",
          "original_form": "전 괜찮던데용?ㅎㅎ",
