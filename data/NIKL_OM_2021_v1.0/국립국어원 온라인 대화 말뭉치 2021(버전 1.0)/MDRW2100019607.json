      "utterance": [
        {
          "id": "MDRW2100019607.1.1",
          "form": "선선하네요",
          "original_form": "선선하네요",
          "speaker_id": "2",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019607.1.2",
          "form": "땀복입고 마스크 끼고 걷는 중이라 겁나 덥긴해요",
          "original_form": "땀복입고 마스크 끼고 걷는 중이라 겁나 덥긴해요",
          "speaker_id": "2",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019607.1.3",
          "form": "나중에 비 안올까요?",
          "original_form": "나중에 비 안올까요?",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019607.1.4",
          "form": "헐  산책하고계시나요?",
          "original_form": "헐  산책하고계시나요?",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019607.1.5",
          "form": "버즈라이브끼셨나요?",
          "original_form": "버즈라이브끼셨나요?",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019607.1.6",
          "form": "네네 잘되네요 아주",
          "original_form": "네네 잘되네요 아주",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019607.1.7",
          "form": "폰을 껐다 키는게 답이엇다니",
          "original_form": "폰을 껐다 키는게 답이엇다니",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019607.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ황당하지만 정확한답을주셨구나",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ황당하지만 정확한답을주셨구나",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019607.1.9",
          "form": "우산은안들고나갔지?",
          "original_form": "우산은안들고나갔지?",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019607.1.10",
          "form": "넹 비올거 같지 않은데",
          "original_form": "넹 비올거 같지 않은데",
          "speaker_id": "2",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019607.1.11",
          "form": "밤공기가좋겠구려   사실 마스크벗어야 밤공기를 느낄수있눈뎅",
          "original_form": "밤공기가좋겠구려   사실 마스크벗어야 밤공기를 느낄수있눈뎅",
          "speaker_id": "1",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019607.1.12",
          "form": "엉 공기가 안느껴지는구려 내 숨밖에",
          "original_form": "엉 공기가 안느껴지는구려 내 숨밖에",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019607.1.13",
          "form": "슬프당",
          "original_form": "슬프당",
          "speaker_id": "1",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100019607.1.14",
          "form": "여름을제일좋아하시나요?",
          "original_form": "여름을제일좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100019607.1.15",
          "form": "넹 여름밤 너무 좋은",
          "original_form": "넹 여름밤 너무 좋은",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100019607.1.16",
          "form": "여름..시로요..빨래잘마르는거하나좋네..",
          "original_form": "여름..시로요..빨래잘마르는거하나좋네..",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019607.1.17",
          "form": "마자요 살기엔 가을이 젤 좋아여",
          "original_form": "마자요 살기엔 가을이 젤 좋아여",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019607.1.18",
          "form": "근데 우리나라는 가을 짧고 여름 기니까",
          "original_form": "근데 우리나라는 가을 짧고 여름 기니까",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019607.1.19",
          "form": "여름 좋아하면 행복할 수 있는 기간이 늘어나여",
          "original_form": "여름 좋아하면 행복할 수 있는 기간이 늘어나여",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019607.1.20",
          "form": "오 되게 현명한말이었어요..",
          "original_form": "오 되게 현명한말이었어요..{emoji:♡}",
          "speaker_id": "1",
          "time": "20210827 20:56"
        },
        {
          "id": "MDRW2100019607.1.21",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210827 20:56"
        },
        {
          "id": "MDRW2100019607.1.22",
          "form": "생각의전환이당",
          "original_form": "생각의전환이당",
          "speaker_id": "1",
          "time": "20210827 20:56"
        },
        {
          "id": "MDRW2100019607.1.23",
          "form": "명언이져?",
          "original_form": "명언이져?",
          "speaker_id": "2",
          "time": "20210827 20:59"
        },
        {
          "id": "MDRW2100019607.1.24",
          "form": "네 진짜요  \r\n또감동먹었어요",
          "original_form": "네 진짜요  \r\n또감동먹었어요",
          "speaker_id": "1",
          "time": "20210827 21:04"
        },
        {
          "id": "MDRW2100019607.1.25",
          "form": "무조건 배척하지말아야겠어요",
          "original_form": "무조건 배척하지말아야겠어요",
          "speaker_id": "1",
          "time": "20210827 21:04"
        },
        {
          "id": "MDRW2100019607.1.26",
          "form": "로보트처럼말해서그런데\r\n진심이예요",
          "original_form": "로보트처럼말해서그런데\r\n진심이예요",
          "speaker_id": "1",
          "time": "20210827 21:05"
        },
        {
          "id": "MDRW2100019607.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100019607.1.28",
          "form": "아름다운계절이된거같다",
          "original_form": "아름다운계절이된거같다",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019607.1.29",
          "form": "그치만 내년 여름에 또 화냄",
          "original_form": "그치만 내년 여름에 또 화냄",
