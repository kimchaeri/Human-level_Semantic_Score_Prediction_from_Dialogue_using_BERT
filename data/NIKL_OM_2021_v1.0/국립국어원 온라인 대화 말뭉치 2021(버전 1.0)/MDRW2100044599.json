      "utterance": [
        {
          "id": "MDRW2100044599.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044599.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044599.1.3",
          "form": "미용과 건강에 관심있으세요?",
          "original_form": "미용과 건강에 관심있으세요?",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044599.1.4",
          "form": "없지는 않은 것 같아요",
          "original_form": "없지는 않은 것 같아요",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044599.1.5",
          "form": "아하 다행이네요",
          "original_form": "아하 다행이네요",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044599.1.6",
          "form": "저도 자기관리같은 걸 좋아하는 편이거든요!",
          "original_form": "저도 자기관리같은 걸 좋아하는 편이거든요!",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044599.1.7",
          "form": "주로 어떤 걸 하시나요?",
          "original_form": "주로 어떤 걸 하시나요?",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044599.1.8",
          "form": "다이어트도하고 운동도 하고",
          "original_form": "다이어트도하고 운동도 하고",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044599.1.9",
          "form": "근데 아직 학생이라 돈이 많이 드는 건 안하고",
          "original_form": "근데 아직 학생이라 돈이 많이 드는 건 안하고",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044599.1.10",
          "form": "직접 찾아서 해보는 편이예요",
          "original_form": "직접 찾아서 해보는 편이예요",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044599.1.11",
          "form": "홈트하시는건가요?",
          "original_form": "홈트하시는건가요?",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044599.1.12",
          "form": "네네 홈트로 하고있어요",
          "original_form": "네네 홈트로 하고있어요",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044599.1.13",
          "form": "주로 뭐 보시면서 따라하시나요?",
          "original_form": "주로 뭐 보시면서 따라하시나요?",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044599.1.14",
          "form": "처음 시작할때는 유튜브보면서 했는데",
          "original_form": "처음 시작할때는 유튜브보면서 했는데",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044599.1.15",
          "form": "오래하다보니까 그냥 저한테 맞는 운동으로 맞춰서 해요",
          "original_form": "오래하다보니까 그냥 저한테 맞는 운동으로 맞춰서 해요",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044599.1.16",
          "form": "운동루틴짜기가 쉽지가 않을텐데",
          "original_form": "운동루틴짜기가 쉽지가 않을텐데",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044599.1.17",
          "form": "괜찮으신가요?",
          "original_form": "괜찮으신가요?",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044599.1.18",
          "form": "음 저는 오래하다보니까 익숙해져서",
          "original_form": "음 저는 오래하다보니까 익숙해져서",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044599.1.19",
          "form": "하던대로 하게되는 거 같아용",
          "original_form": "하던대로 하게되는 거 같아용",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044599.1.20",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210907 23:56"
        }
      ]
