      "utterance": [
        {
          "id": "MDRW2100022591.1.1",
          "form": "동물 키우세요?",
          "original_form": "동물 키우세요?",
          "speaker_id": "1",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100022591.1.2",
          "form": "전 엄청 좋아하는데.. 키우는건..",
          "original_form": "전 엄청 좋아하는데.. 키우는건..",
          "speaker_id": "2",
          "time": "20210829 16:09"
        },
        {
          "id": "MDRW2100022591.1.3",
          "form": "혼자 냅두면..  불쌍하기도하고..",
          "original_form": "혼자 냅두면..  불쌍하기도하고..",
          "speaker_id": "2",
          "time": "20210829 16:09"
        },
        {
          "id": "MDRW2100022591.1.4",
          "form": "그렇긴하죠..",
          "original_form": "그렇긴하죠..",
          "speaker_id": "1",
          "time": "20210829 16:13"
        },
        {
          "id": "MDRW2100022591.1.5",
          "form": "그건 진짜 미안해요",
          "original_form": "그건 진짜 미안해요",
          "speaker_id": "1",
          "time": "20210829 16:13"
        },
        {
          "id": "MDRW2100022591.1.6",
          "form": "그래도 혼자사는게 아니라 다행이다 싶어요",
          "original_form": "그래도 혼자사는게 아니라 다행이다 싶어요",
          "speaker_id": "1",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100022591.1.7",
          "form": "아.. 그럼 걱정 엄죠..",
          "original_form": "아.. 그럼 걱정 엄죠..",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022591.1.8",
          "form": "고양이도 더 좋아하는 주인있어요?",
          "original_form": "고양이도 더 좋아하는 주인있어요?",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022591.1.9",
          "form": "있는거 같아요!",
          "original_form": "있는거 같아요!",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022591.1.10",
          "form": "밥줄때 놀때 나뉘는거 같더라구요ㅣㄱㅋ",
          "original_form": "밥줄때 놀때 나뉘는거 같더라구요ㅣㄱㅋ",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022591.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100022591.1.12",
          "form": "고양이는 영리해서.. ㅋㅋ \r\n매일매일 놀아주시나요?",
          "original_form": "고양이는 영리해서.. ㅋㅋ \r\n매일매일 놀아주시나요?",
          "speaker_id": "2",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022591.1.13",
          "form": "놀긴하는데 만족하는지 모르겠어오 ㅠㅜ",
          "original_form": "놀긴하는데 만족하는지 모르겠어오 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100022591.1.14",
          "form": "오래기르면 표정 어느정도 보이지 않아요?",
          "original_form": "오래기르면 표정 어느정도 보이지 않아요?",
          "speaker_id": "2",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022591.1.15",
          "form": "늘 아쉬워보여서요..ㅜㅠ",
          "original_form": "늘 아쉬워보여서요..ㅜㅠ",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022591.1.16",
          "form": "ㅋㅋㅋ 그냥 고양이 성격이 안좋은걸로..",
          "original_form": "ㅋㅋㅋ 그냥 고양이 성격이 안좋은걸로..",
          "speaker_id": "2",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100022591.1.17",
          "form": "츄.르.는 자주 주세요?",
          "original_form": "츄.르.는 자주 주세요?",
          "speaker_id": "2",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100022591.1.18",
          "form": "아앗.. 제가 못놀아주는걸로..",
          "original_form": "아앗.. 제가 못놀아주는걸로..",
          "speaker_id": "1",
          "time": "20210829 17:19"
        },
        {
          "id": "MDRW2100022591.1.19",
          "form": "발톱깎거나 털깎을때 줘요!",
          "original_form": "발톱깎거나 털깎을때 줘요!",
          "speaker_id": "1",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100022591.1.20",
          "form": "자주주면 밥을 안먹더라구요ㅜ",
          "original_form": "자주주면 밥을 안먹더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100022591.1.21",
          "form": "ㅋㅋㅋ 털도 깍아용? 단모인뎅?",
          "original_form": "ㅋㅋㅋ 털도 깍아용? 단모인뎅?",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100022591.1.22",
          "form": "발바닥이랑 뒷다리쪽이요!",
          "original_form": "발바닥이랑 뒷다리쪽이요!",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022591.1.23",
          "form": "미끄러지더라구요",
          "original_form": "미끄러지더라구요",
          "speaker_id": "1",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022591.1.24",
          "form": "아.. ㅋㅋㅋ 털 깍고 발 바닥 냄새도 맡으시나요? \r\n엄청 구수하다던데..",
          "original_form": "아.. ㅋㅋㅋ 털 깍고 발 바닥 냄새도 맡으시나요? \r\n엄청 구수하다던데..",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022591.1.25",
          "form": "넹ㅋㅋㅋ",
          "original_form": "넹ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022591.1.26",
          "form": "진짜 구수해요ㅋㅋ",
          "original_form": "진짜 구수해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022591.1.27",
          "form": "그리고 발로차죠ㅋㅋ",
          "original_form": "그리고 발로차죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:26"
        }
      ]
