      "utterance": [
        {
          "id": "MDRW2100006776.1.1",
          "form": "동유럽은",
          "original_form": "동유럽은",
          "speaker_id": "1",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100006776.1.2",
          "form": "맥주가 싸서 정신 못차렸습니다ㅋㅋ",
          "original_form": "맥주가 싸서 정신 못차렸습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100006776.1.3",
          "form": "헉 그렇구나 ㅋㅋㅋ",
          "original_form": "헉 그렇구나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100006776.1.4",
          "form": "일단 그 동유럽에 강이 주는 느낌이 커요",
          "original_form": "일단 그 동유럽에 강이 주는 느낌이 커요",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100006776.1.5",
          "form": "서유럽에 비해 약간의 산지도 있고",
          "original_form": "서유럽에 비해 약간의 산지도 있고",
          "speaker_id": "1",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100006776.1.6",
          "form": "유럽여행 갈 때 좀 더 길게 있고 싶었는데",
          "original_form": "유럽여행 갈 때 좀 더 길게 있고 싶었는데",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100006776.1.7",
          "form": "짧았던게 너무 아쉬워요",
          "original_form": "짧았던게 너무 아쉬워요",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100006776.1.8",
          "form": "가 본 나라도 적고...",
          "original_form": "가 본 나라도 적고...",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100006776.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100006776.1.10",
          "form": "저도 여러 나라는 못가봤어요",
          "original_form": "저도 여러 나라는 못가봤어요",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100006776.1.11",
          "form": "코로나 끝나고 기회 되면 또 가보고싶네요...",
          "original_form": "코로나 끝나고 기회 되면 또 가보고싶네요...",
          "speaker_id": "2",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100006776.1.12",
          "form": "여행 가고 싶으신 국가 있으신가요??",
          "original_form": "여행 가고 싶으신 국가 있으신가요??",
          "speaker_id": "2",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100006776.1.13",
          "form": "지금 말씀 듣고",
          "original_form": "지금 말씀 듣고",
          "speaker_id": "1",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100006776.1.14",
          "form": "아일랜드 가고싶네요",
          "original_form": "아일랜드 가고싶네요",
          "speaker_id": "1",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100006776.1.15",
          "form": "아일랜드 진짜 좋아요",
          "original_form": "아일랜드 진짜 좋아요",
          "speaker_id": "2",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100006776.1.16",
          "form": "네 거긴 기네스.. ㅋㅋㅋ",
          "original_form": "네 거긴 기네스.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100006776.1.17",
          "form": "사실 북유럽쪽도 가보고 싶네요",
          "original_form": "사실 북유럽쪽도 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100006776.1.18",
          "form": "아 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100006776.1.19",
          "form": "북유럽은 물가가 기절한대서ㅠ",
          "original_form": "북유럽은 물가가 기절한대서ㅠ",
          "speaker_id": "1",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100006776.1.20",
          "form": "아 그런가요??",
          "original_form": "아 그런가요??",
          "speaker_id": "2",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100006776.1.21",
          "form": "물가는 영국 갔을때도 장난 아니던데...",
          "original_form": "물가는 영국 갔을때도 장난 아니던데...",
          "speaker_id": "2",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100006776.1.22",
          "form": "영국은 실제로",
          "original_form": "영국은 실제로",
          "speaker_id": "1",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100006776.1.23",
          "form": "맛없던가요ㅋㅋ",
          "original_form": "맛없던가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100006776.1.24",
          "form": "아 네 ㅋㅋㅋ",
          "original_form": "아 네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100006776.1.25",
          "form": "룸서비스로 샌드위치를 시켰는데",
          "original_form": "룸서비스로 샌드위치를 시켰는데",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100006776.1.26",
          "form": "안에 와사비가 발려 있어서 식겁했어요...",
          "original_form": "안에 와사비가 발려 있어서 식겁했어요...",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100006776.1.27",
          "form": "저 와사비 진짜 싫어하는데...",
          "original_form": "저 와사비 진짜 싫어하는데...",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100006776.1.28",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100006776.1.29",
          "form": "진짜 와사비 샌드위치라니",
          "original_form": "진짜 와사비 샌드위치라니",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100006776.1.30",
