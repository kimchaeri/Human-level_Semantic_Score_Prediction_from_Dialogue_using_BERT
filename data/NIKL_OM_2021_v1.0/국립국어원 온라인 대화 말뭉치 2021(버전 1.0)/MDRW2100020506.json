      "utterance": [
        {
          "id": "MDRW2100020506.1.1",
          "form": "클래식이 영원하죠",
          "original_form": "클래식이 영원하죠",
          "speaker_id": "2",
          "time": "20210828 08:37"
        },
        {
          "id": "MDRW2100020506.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 08:37"
        },
        {
          "id": "MDRW2100020506.1.3",
          "form": "전 사실 가리지않습니다",
          "original_form": "전 사실 가리지않습니다",
          "speaker_id": "1",
          "time": "20210828 08:37"
        },
        {
          "id": "MDRW2100020506.1.4",
          "form": "올라운더시군요",
          "original_form": "올라운더시군요",
          "speaker_id": "2",
          "time": "20210828 08:37"
        },
        {
          "id": "MDRW2100020506.1.5",
          "form": "그래도 구황작물 올라간게 아무래도",
          "original_form": "그래도 구황작물 올라간게 아무래도",
          "speaker_id": "1",
          "time": "20210828 08:37"
        },
        {
          "id": "MDRW2100020506.1.6",
          "form": "탄수화물 탄수화물 조합이라ㅋㅋㅋㅋ",
          "original_form": "탄수화물 탄수화물 조합이라ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 08:37"
        },
        {
          "id": "MDRW2100020506.1.7",
          "form": "맛이 두배..",
          "original_form": "맛이 두배..",
          "speaker_id": "1",
          "time": "20210828 08:37"
        },
        {
          "id": "MDRW2100020506.1.8",
          "form": "아 역시 맛잘알",
          "original_form": "아 역시 맛잘알",
          "speaker_id": "2",
          "time": "20210828 08:37"
        },
        {
          "id": "MDRW2100020506.1.9",
          "form": "고구마도 감자도 모두 어울리죠",
          "original_form": "고구마도 감자도 모두 어울리죠",
          "speaker_id": "2",
          "time": "20210828 08:37"
        },
        {
          "id": "MDRW2100020506.1.10",
          "form": "핫소스는 뿌리시나요?",
          "original_form": "핫소스는 뿌리시나요?",
          "speaker_id": "2",
          "time": "20210828 08:38"
        },
        {
          "id": "MDRW2100020506.1.11",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "1",
          "time": "20210828 08:38"
        },
        {
          "id": "MDRW2100020506.1.12",
          "form": "그 약간 시큼매큼한 맛에 중독되어",
          "original_form": "그 약간 시큼매큼한 맛에 중독되어",
          "speaker_id": "1",
          "time": "20210828 08:38"
        },
        {
          "id": "MDRW2100020506.1.13",
          "form": "느끼함을 잡아주는 매콤함",
          "original_form": "느끼함을 잡아주는 매콤함",
          "speaker_id": "2",
          "time": "20210828 08:38"
        },
        {
          "id": "MDRW2100020506.1.14",
          "form": "팍팍 뿌리고싶은데",
          "original_form": "팍팍 뿌리고싶은데",
          "speaker_id": "1",
          "time": "20210828 08:38"
        },
        {
          "id": "MDRW2100020506.1.15",
          "form": "늘 양이 부족해요",
          "original_form": "늘 양이 부족해요",
          "speaker_id": "2",
          "time": "20210828 08:38"
        },
        {
          "id": "MDRW2100020506.1.16",
          "form": "포장지 너무 쥐알탱이만해요",
          "original_form": "포장지 너무 쥐알탱이만해요",
          "speaker_id": "1",
          "time": "20210828 08:38"
        },
        {
          "id": "MDRW2100020506.1.17",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210828 08:38"
        },
        {
          "id": "MDRW2100020506.1.18",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "1",
          "time": "20210828 08:38"
        },
        {
          "id": "MDRW2100020506.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ어휴 넘 작아요 진짜",
          "original_form": "ㅋㅋㅋㅋㅋㅋ어휴 넘 작아요 진짜",
          "speaker_id": "2",
          "time": "20210828 08:39"
        },
        {
          "id": "MDRW2100020506.1.20",
          "form": "파마산 치즈도 뿌리시나요",
          "original_form": "파마산 치즈도 뿌리시나요",
          "speaker_id": "2",
          "time": "20210828 08:39"
        },
        {
          "id": "MDRW2100020506.1.21",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "1",
          "time": "20210828 08:39"
        },
        {
          "id": "MDRW2100020506.1.22",
          "form": "치즈에 치즈를 더해서",
          "original_form": "치즈에 치즈를 더해서",
          "speaker_id": "1",
          "time": "20210828 08:39"
        },
        {
          "id": "MDRW2100020506.1.23",
          "form": "어머 깜짝야",
          "original_form": "어머 깜짝야",
          "speaker_id": "1",
          "time": "20210828 08:39"
        },
        {
          "id": "MDRW2100020506.1.24",
          "form": "역시 주는건 다뿌려야죠",
          "original_form": "역시 주는건 다뿌려야죠",
