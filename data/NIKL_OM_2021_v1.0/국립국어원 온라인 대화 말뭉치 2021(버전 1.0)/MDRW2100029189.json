      "utterance": [
        {
          "id": "MDRW2100029189.1.1",
          "form": "그래야 쓰겄네ㅋㅋㅋ",
          "original_form": "그래야 쓰겄네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029189.1.2",
          "form": "확실히 새옷 입는 그 맛이 있으니 ㅋㅋㅋ",
          "original_form": "확실히 새옷 입는 그 맛이 있으니 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029189.1.3",
          "form": "지금 반팔티 하나로  견디고 있다ㅋㅋㅋㅋ",
          "original_form": "지금 반팔티 하나로  견디고 있다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029189.1.4",
          "form": "다 셔츠나 블라우스 뿐이라ㅋㅋㅋ",
          "original_form": "다 셔츠나 블라우스 뿐이라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029189.1.5",
          "form": "아 하나는 쩜 힘겹네 ㅋㅋㅋㅋ",
          "original_form": "아 하나는 쩜 힘겹네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029189.1.6",
          "form": "응 그래서 고민듕ㅋㅋㅋ",
          "original_form": "응 그래서 고민듕ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029189.1.7",
          "form": "하나정도는 사도 되겠는걸 ㅋㅋㅋㅋ",
          "original_form": "하나정도는 사도 되겠는걸 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100029189.1.8",
          "form": "그럼 반팔 사까ㅋㅋㅋㅋ",
          "original_form": "그럼 반팔 사까ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:51"
        },
        {
          "id": "MDRW2100029189.1.9",
          "form": "고려해보거라 ㅋㅋㅋㅋ",
          "original_form": "고려해보거라 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029189.1.10",
          "form": "맴에 드는거 두갠데 하나만 사까ㅋㅋ",
          "original_form": "맴에 드는거 두갠데 하나만 사까ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100029189.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 사서 개봉은 내년에 해도 괜찮을 듯 ?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 사서 개봉은 내년에 해도 괜찮을 듯 ?",
          "speaker_id": "1",
          "time": "20210903 13:00"
        },
        {
          "id": "MDRW2100029189.1.12",
          "form": "그런...가?ㅋㅋㅋ 추워지니 여름옷 세일 많이 하네ㅎㅎ",
          "original_form": "그런...가?ㅋㅋㅋ 추워지니 여름옷 세일 많이 하네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 13:52"
        },
        {
          "id": "MDRW2100029189.1.13",
          "form": "나도 새 반팔 다섯개있음 ㅋㅋㅋㅋㅋ",
          "original_form": "나도 새 반팔 다섯개있음 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:57"
        },
        {
          "id": "MDRW2100029189.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ 너에게 설득당했다ㅋㅋ 결제하마ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 너에게 설득당했다ㅋㅋ 결제하마ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:05"
        },
        {
          "id": "MDRW2100029189.1.15",
          "form": "어차피 유행타는거 살것도 아니고 ㅋㅋㅋ",
          "original_form": "어차피 유행타는거 살것도 아니고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:06"
        },
        {
          "id": "MDRW2100029189.1.16",
          "form": "응 기본 스탈 사야지ㅋㅋㅋ",
          "original_form": "응 기본 스탈 사야지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:48"
        },
        {
          "id": "MDRW2100029189.1.17",
          "form": "그럼 언제 사도 괜찮지 뭐 ㅋㅋㅋ",
          "original_form": "그럼 언제 사도 괜찮지 뭐 ㅋㅋㅋ",
