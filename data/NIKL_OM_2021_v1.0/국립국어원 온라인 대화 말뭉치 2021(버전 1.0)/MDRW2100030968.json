      "utterance": [
        {
          "id": "MDRW2100030968.1.1",
          "form": "종교 있으신가요?",
          "original_form": "종교 있으신가요?",
          "speaker_id": "2",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030968.1.2",
          "form": "오 저도 다녀본적 있어요",
          "original_form": "오 저도 다녀본적 있어요",
          "speaker_id": "2",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030968.1.3",
          "form": "네 어릴때는 조금 다녔어요",
          "original_form": "네 어릴때는 조금 다녔어요",
          "speaker_id": "2",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030968.1.4",
          "form": "부모님이 교회를 다니시거든요",
          "original_form": "부모님이 교회를 다니시거든요",
          "speaker_id": "2",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030968.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030968.1.6",
          "form": "학교앞에서 홍보도 하잖아요",
          "original_form": "학교앞에서 홍보도 하잖아요",
          "speaker_id": "1",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100030968.1.7",
          "form": "맞아요 오면 선물준다고",
          "original_form": "맞아요 오면 선물준다고",
          "speaker_id": "2",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100030968.1.8",
          "form": "맞아요ㅋㅋㅋ 이벤트 하고",
          "original_form": "맞아요ㅋㅋㅋ 이벤트 하고",
          "speaker_id": "1",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100030968.1.9",
          "form": "그거때문에 가본 초등학생들 많을걸요",
          "original_form": "그거때문에 가본 초등학생들 많을걸요",
          "speaker_id": "2",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100030968.1.10",
          "form": "공감합니다 ㅎㅎ",
          "original_form": "공감합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100030968.1.11",
          "form": "추억이네요",
          "original_form": "추억이네요",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100030968.1.12",
          "form": "문상도 준다하고 그랬었죠",
          "original_form": "문상도 준다하고 그랬었죠",
          "speaker_id": "1",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100030968.1.13",
          "form": "교회 나름 재밌었던 기억나네요",
          "original_form": "교회 나름 재밌었던 기억나네요",
          "speaker_id": "2",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100030968.1.14",
          "form": "교회 다녀보세요 ㅎㅎ",
          "original_form": "교회 다녀보세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100030968.1.15",
          "form": "여유되면 그래볼까 생각중이에요",
          "original_form": "여유되면 그래볼까 생각중이에요",
          "speaker_id": "2",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030968.1.16",
          "form": "환영합니다 ㅎㅎ",
          "original_form": "환영합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030968.1.17",
          "form": "찬송가도 되게 좋잖아요",
          "original_form": "찬송가도 되게 좋잖아요",
