      "utterance": [
        {
          "id": "MDRW2100038213.1.1",
          "form": "과자 좋아하세요??",
          "original_form": "과자 좋아하세요??",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100038213.1.2",
          "form": "디저트류라던지 좋아하시나요??",
          "original_form": "디저트류라던지 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100038213.1.3",
          "form": "과자 좋아하죠",
          "original_form": "과자 좋아하죠",
          "speaker_id": "2",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100038213.1.4",
          "form": "안먹어야하는데 회사에",
          "original_form": "안먹어야하는데 회사에",
          "speaker_id": "2",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100038213.1.5",
          "form": "ㅋㅋㅋㅋ 그렇죠",
          "original_form": "ㅋㅋㅋㅋ 그렇죠",
          "speaker_id": "1",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100038213.1.6",
          "form": "간식 과자들에 자꾸 손이 가요",
          "original_form": "간식 과자들에 자꾸 손이 가요",
          "speaker_id": "2",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100038213.1.7",
          "form": "맛있는거 너무 많아요",
          "original_form": "맛있는거 너무 많아요",
          "speaker_id": "1",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100038213.1.8",
          "form": "근데 간식 안먹는 직원은 진짜 안먹더라구요",
          "original_form": "근데 간식 안먹는 직원은 진짜 안먹더라구요",
          "speaker_id": "2",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100038213.1.9",
          "form": "촉촉한 초코칩",
          "original_form": "촉촉한 초코칩",
          "speaker_id": "2",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100038213.1.10",
          "form": "빼빼로 이런거요 ㅎ",
          "original_form": "빼빼로 이런거요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100038213.1.11",
          "form": "그런분들 보면 신기해요 단거 안좋아하시나",
          "original_form": "그런분들 보면 신기해요 단거 안좋아하시나",
          "speaker_id": "1",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100038213.1.12",
          "form": "촉촉한 초코칩 진짜 맛있어요",
          "original_form": "촉촉한 초코칩 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100038213.1.13",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100038213.1.14",
          "form": "저는 아몬드빼빼로 좋아해요",
          "original_form": "저는 아몬드빼빼로 좋아해요",
          "speaker_id": "1",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100038213.1.15",
          "form": "포키도 맛있더라고요",
          "original_form": "포키도 맛있더라고요",
          "speaker_id": "1",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100038213.1.16",
          "form": "저두요 그거 주로 먹어요",
          "original_form": "저두요 그거 주로 먹어요",
          "speaker_id": "2",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100038213.1.17",
          "form": "얇은거",
          "original_form": "얇은거",
          "speaker_id": "1",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100038213.1.18",
          "form": "누드도 좋구요",
          "original_form": "누드도 좋구요",
          "speaker_id": "2",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100038213.1.19",
          "form": "맞아요 맛있어요",
          "original_form": "맞아요 맛있어요",
          "speaker_id": "1",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100038213.1.20",
          "form": "요즘은 바프 사다놔서",
          "original_form": "요즘은 바프 사다놔서",
          "speaker_id": "2",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100038213.1.21",
          "form": "빼먹고 있어요 ㅋ",
          "original_form": "빼먹고 있어요 ㅋ",
          "speaker_id": "2",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100038213.1.22",
          "form": "오 맛있는거 사놓으셨네",
          "original_form": "오 맛있는거 사놓으셨네",
          "speaker_id": "1",
          "time": "20210902 00:58"
        }
      ]
