        },
        {
          "id": "MDRW2100015096.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015096.1.3",
          "form": "종교가 있으신가요",
          "original_form": "종교가 있으신가요",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015096.1.4",
          "form": "저는 무교에요",
          "original_form": "저는 무교에요",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015096.1.5",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015096.1.6",
          "form": "저는 무교인듯 불교같은 불교아닌 사람입니다",
          "original_form": "저는 무교인듯 불교같은 불교아닌 사람입니다",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015096.1.7",
          "form": "절은 좋아하지만... 잘 가진 않는...",
          "original_form": "절은 좋아하지만... 잘 가진 않는...",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015096.1.8",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015096.1.9",
          "form": "가끔..관광지처럼 찾아가는 그런느낌인가요",
          "original_form": "가끔..관광지처럼 찾아가는 그런느낌인가요",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015096.1.10",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015096.1.11",
          "form": "그치만 교회나 성당은 가끔도 안가니까",
          "original_form": "그치만 교회나 성당은 가끔도 안가니까",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015096.1.12",
          "form": "불교인듯 불교아닌 무교같은 삶...",
          "original_form": "불교인듯 불교아닌 무교같은 삶...",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015096.1.13",
          "form": "ㅋㅋㅋㅋ그렇네요",
          "original_form": "ㅋㅋㅋㅋ그렇네요",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015096.1.14",
          "form": "저는 절도 끌려가보고 교회도 끌려만 가봤어요",
          "original_form": "저는 절도 끌려가보고 교회도 끌려만 가봤어요",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015096.1.15",
          "form": "근데 차라리 종교학 학문을 배우는게 낫더라고요ㅋㅋㅋ",
          "original_form": "근데 차라리 종교학 학문을 배우는게 낫더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015096.1.16",
          "form": "ㅋㅋㅋㅋ맞아요 종교는 학문적으로 들어가면 오히려 흥미진진한 점이 있더라구요",
          "original_form": "ㅋㅋㅋㅋ맞아요 종교는 학문적으로 들어가면 오히려 흥미진진한 점이 있더라구요",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015096.1.17",
          "form": "저는 성경을 소설책처럼 읽어보려다가 그 특유의 어투가 힘들어서",
          "original_form": "저는 성경을 소설책처럼 읽어보려다가 그 특유의 어투가 힘들어서",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015096.1.18",
          "form": "포기한적이 있습니다....",
          "original_form": "포기한적이 있습니다....",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015096.1.19",
          "form": "저랑 비슷하시네요ㅋㅋㅋ",
          "original_form": "저랑 비슷하시네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015096.1.20",
          "form": "학문적으로 접근했다가 그냥 도망가버린..",
          "original_form": "학문적으로 접근했다가 그냥 도망가버린..",
          "speaker_id": "2",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015096.1.21",
          "form": "그리고 완전 기원전구시대 얘기고",
          "original_form": "그리고 완전 기원전구시대 얘기고",
          "speaker_id": "2",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015096.1.22",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015096.1.23",
          "form": "호기롭게 도전했다가 뛰쳐나왔습니다",
          "original_form": "호기롭게 도전했다가 뛰쳐나왔습니다",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015096.1.24",
          "form": "불경은 뭔 소린지 알수도 없고",
          "original_form": "불경은 뭔 소린지 알수도 없고",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015096.1.25",
          "form": "경전들은 어찌나 그렇게 다들 접근성이 안좋은지..",
          "original_form": "경전들은 어찌나 그렇게 다들 접근성이 안좋은지..",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015096.1.26",
          "form": "해설자가 꼭 필요해요 ㅋㅋ",
          "original_form": "해설자가 꼭 필요해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015096.1.27",
          "form": "불경도 보셨나요ㅋㅋㅋㅋ",
          "original_form": "불경도 보셨나요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015096.1.28",
          "form": "머져 원전 언어가",
          "original_form": "머져 원전 언어가",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015096.1.29",
          "form": "산스크리트어??!!!",
          "original_form": "산스크리트어??!!!",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015096.1.30",
          "form": "해설도 막 몇천년치 분량이라...",
          "original_form": "해설도 막 몇천년치 분량이라...",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015096.1.31",
          "form": "그냥 탈주각",
          "original_form": "그냥 탈주각",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015096.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ 한자가 수두룩빽빽쓰",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 한자가 수두룩빽빽쓰",
          "speaker_id": "1",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015096.1.33",
          "form": "머리가 지끈지끈",
          "original_form": "머리가 지끈지끈",
          "speaker_id": "1",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015096.1.34",
          "form": "종교는 참 쉽지않네요",
          "original_form": "종교는 참 쉽지않네요",
          "speaker_id": "1",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015096.1.35",
          "form": "그걸 보고 신앙심이 생기다니",
          "original_form": "그걸 보고 신앙심이 생기다니",
