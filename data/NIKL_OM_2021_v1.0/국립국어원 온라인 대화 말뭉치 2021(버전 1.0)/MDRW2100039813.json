      "utterance": [
        {
          "id": "MDRW2100039813.1.1",
          "form": "ㅋㅋㅋㅋ내가 상세샷을 덜 보고 와서",
          "original_form": "ㅋㅋㅋㅋ내가 상세샷을 덜 보고 와서",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039813.1.2",
          "form": "투웨이인지 몰랐는데",
          "original_form": "투웨이인지 몰랐는데",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039813.1.3",
          "form": "내가 근 4년간 옷을 안샀거든",
          "original_form": "내가 근 4년간 옷을 안샀거든",
          "speaker_id": "1",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039813.1.4",
          "form": "어떻게 투웨이지",
          "original_form": "어떻게 투웨이지",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039813.1.5",
          "form": "4년 존버했니",
          "original_form": "4년 존버했니",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039813.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039813.1.7",
          "form": "그냥 옷을 좋아하지만",
          "original_form": "그냥 옷을 좋아하지만",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039813.1.8",
          "form": "사고싶은 마음은 없었거든",
          "original_form": "사고싶은 마음은 없었거든",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039813.1.9",
          "form": "응응응",
          "original_form": "응응응",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039813.1.10",
          "form": "그런데 몸과 마음이 좀 건강해지니까",
          "original_form": "그런데 몸과 마음이 좀 건강해지니까",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039813.1.11",
          "form": "자기애가 생겼나봐",
          "original_form": "자기애가 생겼나봐",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039813.1.12",
          "form": "내가 '입을 수있는'이 아니라",
          "original_form": "내가 '입을 수있는'이 아니라",
          "speaker_id": "1",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039813.1.13",
          "form": "내가 갖고싶은 옷을 사게됐어",
          "original_form": "내가 갖고싶은 옷을 사게됐어",
          "speaker_id": "1",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039813.1.14",
          "form": "아 맞아 니가 무슨 느낌인지 알 것 같다",
          "original_form": "아 맞아 니가 무슨 느낌인지 알 것 같다",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039813.1.15",
          "form": "패션계에 도는 말이",
          "original_form": "패션계에 도는 말이",
          "speaker_id": "2",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100039813.1.16",
          "form": "패완얼 패완몸 이런 말이잖아",
          "original_form": "패완얼 패완몸 이런 말이잖아",
          "speaker_id": "2",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100039813.1.17",
          "form": "막 모델같은 사람들만 '입을 자격' 있다는 인식",
          "original_form": "막 모델같은 사람들만 '입을 자격' 있다는 인식",
          "speaker_id": "2",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100039813.1.18",
          "form": "예쁜^^,, 사람 아니면 좀만 튀게 입어도 비웃는 분위기 있었잖아",
          "original_form": "예쁜^^,, 사람 아니면 좀만 튀게 입어도 비웃는 분위기 있었잖아",
          "speaker_id": "2",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039813.1.19",
          "form": "그것때문에 나도 '내가 입을 수 있는' 옷만 입어야 할 것 같았는데",
          "original_form": "그것때문에 나도 '내가 입을 수 있는' 옷만 입어야 할 것 같았는데",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039813.1.20",
          "form": "자기애 생기고 줏대 생기면서 내가 입을 수 있는가보다는 내가 그 옷이 마음에 드는지를 우선하게 됐어",
          "original_form": "자기애 생기고 줏대 생기면서 내가 입을 수 있는가보다는 내가 그 옷이 마음에 드는지를 우선하게 됐어",
          "speaker_id": "2",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100039813.1.21",
          "form": "내가 갖고 싶고 어쨌든 내가 입으면 그게 내 옷이지",
          "original_form": "내가 갖고 싶고 어쨌든 내가 입으면 그게 내 옷이지",
          "speaker_id": "2",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039813.1.22",
          "form": "옷에 나를 맞추려고 자기검열하지 않게 됐음",
          "original_form": "옷에 나를 맞추려고 자기검열하지 않게 됐음",
          "speaker_id": "2",
          "time": "20210903 21:58"
        },
        {
          "id": "MDRW2100039813.1.23",
          "form": "사이즈도 옷를 나한테 맞추게 됐어",
          "original_form": "사이즈도 옷를 나한테 맞추게 됐어",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039813.1.24",
          "form": "맞아 사실 저옷도",
          "original_form": "맞아 사실 저옷도",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039813.1.25",
          "form": "나한테는 살짝 낄텐대",
          "original_form": "나한테는 살짝 낄텐대",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039813.1.26",
          "form": "그냥 샀어",
          "original_form": "그냥 샀어",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039813.1.27",
          "form": "내가 입고싶어서",
          "original_form": "내가 입고싶어서",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039813.1.28",
          "form": "니트야 뭐 입다보면 늘어나겠지",
          "original_form": "니트야 뭐 입다보면 늘어나겠지",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039813.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039813.1.30",
          "form": "크면 대충 걷어입고",
          "original_form": "크면 대충 걷어입고",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039813.1.31",
          "form": "맞아 지가 내몸에 맞추라그래",
          "original_form": "맞아 지가 내몸에 맞추라그래",
          "speaker_id": "1",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039813.1.32",
          "form": "작으면 옷 능지처참해서 늘려",
          "original_form": "작으면 옷 능지처참해서 늘려",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039813.1.33",
          "form": "그래야겠어 어딜감히",
          "original_form": "그래야겠어 어딜감히",
          "speaker_id": "1",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039813.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039813.1.35",
          "form": "나는 옷을 꽤 좋아하는 사람이란걸",
          "original_form": "나는 옷을 꽤 좋아하는 사람이란걸",
          "speaker_id": "1",
          "time": "20210903 22:17"
        },
