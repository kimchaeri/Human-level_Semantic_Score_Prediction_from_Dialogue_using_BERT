      "utterance": [
        {
          "id": "MDRW2100024925.1.1",
          "form": "name2 똑똑",
          "original_form": "&name2& 똑똑",
          "speaker_id": "1",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100024925.1.2",
          "form": "name1 안녕하세요",
          "original_form": "&name1& 안녕하세요",
          "speaker_id": "2",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100024925.1.3",
          "form": "최근에 구매한물건있나여",
          "original_form": "최근에 구매한물건있나여",
          "speaker_id": "2",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100024925.1.4",
          "form": "오늘은 꽃을 샀어여",
          "original_form": "오늘은 꽃을 샀어여",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024925.1.5",
          "form": "화병에 꽂아서 테이블에 두면 인테리어 굿...",
          "original_form": "화병에 꽂아서 테이블에 두면 인테리어 굿...",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024925.1.6",
          "form": "name2은 오늘 쇼핑 안하셨나요",
          "original_form": "&name2&은 오늘 쇼핑 안하셨나요",
          "speaker_id": "1",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100024925.1.7",
          "form": "오 꽃을 자주 사시나봐요",
          "original_form": "오 꽃을 자주 사시나봐요",
          "speaker_id": "2",
          "time": "20210831 21:10"
        },
        {
          "id": "MDRW2100024925.1.8",
          "form": "생화가 분위기 전환에 좋더라구여",
          "original_form": "생화가 분위기 전환에 좋더라구여",
          "speaker_id": "1",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100024925.1.9",
          "form": "오 그렇군여 좋아하는 꽃 종류가 뭐야?",
          "original_form": "오 그렇군여 좋아하는 꽃 종류가 뭐야?",
          "speaker_id": "2",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024925.1.10",
          "form": "아직 크게 좋아하는 건 없는데...",
          "original_form": "아직 크게 좋아하는 건 없는데...",
          "speaker_id": "1",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024925.1.11",
          "form": "프리지아가 좀 예뻤다",
          "original_form": "프리지아가 좀 예뻤다",
          "speaker_id": "1",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024925.1.12",
          "form": "걔는 말려도 노랑노랑해",
          "original_form": "걔는 말려도 노랑노랑해",
          "speaker_id": "1",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024925.1.13",
          "form": "오 그렇구나",
          "original_form": "오 그렇구나",
          "speaker_id": "2",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024925.1.14",
          "form": "나는 어제 원두 샀어",
          "original_form": "나는 어제 원두 샀어",
          "speaker_id": "2",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024925.1.15",
          "form": "오 어디서 인터넷?",
          "original_form": "오 어디서 인터넷?",
          "speaker_id": "1",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024925.1.16",
          "form": "스벅?..",
          "original_form": "스벅?..",
          "speaker_id": "1",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024925.1.17",
          "form": "인터넷에서 샀어 네이버쇼핑!",
          "original_form": "인터넷에서 샀어 네이버쇼핑!",
          "speaker_id": "2",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024925.1.18",
          "form": "스벅이 맛잇긴한데 다른데보다 비싸 ㅠ",
          "original_form": "스벅이 맛잇긴한데 다른데보다 비싸 ㅠ",
          "speaker_id": "2",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024925.1.19",
          "form": "그렇긴 하지...",
          "original_form": "그렇긴 하지...",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024925.1.20",
          "form": "오늘 내일 네이버플러스멤버십데이야",
          "original_form": "오늘 내일 네이버플러스멤버십데이야",
          "speaker_id": "2",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024925.1.21",
          "form": "멤버십 가입함?",
          "original_form": "멤버십 가입함?",
          "speaker_id": "1",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024925.1.22",
          "form": "아니다 어제오늘이야 적립 많이돼",
          "original_form": "아니다 어제오늘이야 적립 많이돼",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024925.1.23",
          "form": "난 한달 무료만 해보고 끝냈는데",
          "original_form": "난 한달 무료만 해보고 끝냈는데",
          "speaker_id": "1",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024925.1.24",
          "form": "옹 첫달 무료길래 이번달에 첨써봤어",
          "original_form": "옹 첫달 무료길래 이번달에 첨써봤어",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024925.1.25",
          "form": "오 ㅇㅇ 나름 적립 잘된다 아녀 ㅋㅋㅋ",
          "original_form": "오 ㅇㅇ 나름 적립 잘된다 아녀 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024925.1.26",
          "form": "네이버쇼핑 잘 안해서 ㅋㅋㅋㅋㅋ",
          "original_form": "네이버쇼핑 잘 안해서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024925.1.27",
          "form": "그니까 적립 꽤되도라",
          "original_form": "그니까 적립 꽤되도라",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024925.1.28",
          "form": "그때 막 배민도 네이버페이로 했었는데",
          "original_form": "그때 막 배민도 네이버페이로 했었는데",
          "speaker_id": "1",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024925.1.29",
          "form": "최근에 뭐 구매했어",
          "original_form": "최근에 뭐 구매했어",
          "speaker_id": "2",
          "time": "20210902 14:11"
        }
      ]
    }
