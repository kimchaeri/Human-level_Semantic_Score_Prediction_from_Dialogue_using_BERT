        {
          "id": "MDRW2100013013.1.1",
          "form": "겜 좋아하시나요",
          "original_form": "겜 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 13:02"
        },
        {
          "id": "MDRW2100013013.1.2",
          "form": "가끔해요 하시는 겜있으세요?",
          "original_form": "가끔해요 하시는 겜있으세요?",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013013.1.3",
          "form": "피파온라인이요",
          "original_form": "피파온라인이요",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100013013.1.4",
          "form": "아 많이들 하더라구요ㅎ현질도 하세요?",
          "original_form": "아 많이들 하더라구요ㅎ현질도 하세요?",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100013013.1.5",
          "form": "현질은 안합니다ㅎㅎ",
          "original_form": "현질은 안합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100013013.1.6",
          "form": "아ㅎㅎ안하는게 좋죠",
          "original_form": "아ㅎㅎ안하는게 좋죠",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100013013.1.7",
          "form": "마자요 돈아까워요",
          "original_form": "마자요 돈아까워요",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100013013.1.8",
          "form": "가끔 하고싶을때있는데 참으면 또 참아지더라구요",
          "original_form": "가끔 하고싶을때있는데 참으면 또 참아지더라구요",
          "speaker_id": "2",
          "time": "20210826 10:27"
        },
        {
          "id": "MDRW2100013013.1.9",
          "form": "겜 뭐하시나요",
          "original_form": "겜 뭐하시나요",
          "speaker_id": "1",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100013013.1.10",
          "form": "저 쿠키런해요ㄹ",
          "original_form": "저 쿠키런해요ㄹ",
          "speaker_id": "2",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100013013.1.11",
          "form": "와 추억의게임",
          "original_form": "와 추억의게임",
          "speaker_id": "1",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100013013.1.12",
          "form": "올해나왔는데 원래있던건가요",
          "original_form": "올해나왔는데 원래있던건가요",
