      "utterance": [
        {
          "id": "MDRW2100034889.1.1",
          "form": "그러게요 클렌징워터 같은 걸 써야 하는지",
          "original_form": "그러게요 클렌징워터 같은 걸 써야 하는지",
          "speaker_id": "1",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100034889.1.2",
          "form": "피부관리가 참 어려운 것 같아요",
          "original_form": "피부관리가 참 어려운 것 같아요",
          "speaker_id": "1",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100034889.1.3",
          "form": "맞아요ㅠㅠ 저 언제 화장품 잘못쓰고",
          "original_form": "맞아요ㅠㅠ 저 언제 화장품 잘못쓰고",
          "speaker_id": "2",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100034889.1.4",
          "form": "접촉성피부염 생긴적 있는데",
          "original_form": "접촉성피부염 생긴적 있는데",
          "speaker_id": "2",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100034889.1.5",
          "form": "진짜 엄청 고생했어요ㅠㅠㅠ",
          "original_form": "진짜 엄청 고생했어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100034889.1.6",
          "form": "흉터도 오래가고ㅠ",
          "original_form": "흉터도 오래가고ㅠ",
          "speaker_id": "2",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100034889.1.7",
          "form": "헉 ㅠㅠ 진짜 그럼 너무 슬퍼요",
          "original_form": "헉 ㅠㅠ 진짜 그럼 너무 슬퍼요",
          "speaker_id": "1",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100034889.1.8",
          "form": "피부과 다니고 약먹고 주사맞고 그랬어요ㅠㅠ",
          "original_form": "피부과 다니고 약먹고 주사맞고 그랬어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034889.1.9",
          "form": "그래서 뭘 발라도 트러블안나는 분들이 부러워요",
          "original_form": "그래서 뭘 발라도 트러블안나는 분들이 부러워요",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034889.1.10",
          "form": "아이고 고생 많이 하셨네요ㅠㅜㅜ",
          "original_form": "아이고 고생 많이 하셨네요ㅠㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100034889.1.11",
          "form": "아이크림은 바르세요?",
          "original_form": "아이크림은 바르세요?",
          "speaker_id": "2",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100034889.1.12",
          "form": "아뇨ㅠ",
          "original_form": "아뇨ㅠ",
          "speaker_id": "1",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100034889.1.13",
          "form": "근데 발라야 된다 하더라고요",
          "original_form": "근데 발라야 된다 하더라고요",
          "speaker_id": "1",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100034889.1.14",
          "form": "맞아요 20대 초반부터 바르라고 하더라구요",
          "original_form": "맞아요 20대 초반부터 바르라고 하더라구요",
          "speaker_id": "2",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100034889.1.15",
          "form": "근데 또 어떤분은 아이크림이라고 다른 크림이랑 다른게 아니다",
          "original_form": "근데 또 어떤분은 아이크림이라고 다른 크림이랑 다른게 아니다",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100034889.1.16",
          "form": "맞아요 그냥 수분크림을 발라도 된다?? 하시더라고요",
          "original_form": "맞아요 그냥 수분크림을 발라도 된다?? 하시더라고요",
          "speaker_id": "1",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100034889.1.17",
          "form": "수분크림인지 영양크림인지 아무튼...",
          "original_form": "수분크림인지 영양크림인지 아무튼...",
          "speaker_id": "1",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100034889.1.18",
          "form": "선크림도 잘 바르고 다니시나요?",
          "original_form": "선크림도 잘 바르고 다니시나요?",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100034889.1.19",
          "form": "아뇨ㅋㅋㅋㅋ ㅠ 발라야 되는데 귀찮아서",
          "original_form": "아뇨ㅋㅋㅋㅋ ㅠ 발라야 되는데 귀찮아서",
          "speaker_id": "1",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100034889.1.20",
          "form": "안끈적이는 선크림 딱 하나 아는거 있는데",
          "original_form": "안끈적이는 선크림 딱 하나 아는거 있는데",
          "speaker_id": "2",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100034889.1.21",
          "form": "일본 제품이라",
          "original_form": "일본 제품이라",
          "speaker_id": "2",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100034889.1.22",
          "form": "다른거 찾아봐야하는데 슬프네요ㅠㅠ",
          "original_form": "다른거 찾아봐야하는데 슬프네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100034889.1.23",
          "form": "헉 그렇군요ㅠ",
          "original_form": "헉 그렇군요ㅠ",
