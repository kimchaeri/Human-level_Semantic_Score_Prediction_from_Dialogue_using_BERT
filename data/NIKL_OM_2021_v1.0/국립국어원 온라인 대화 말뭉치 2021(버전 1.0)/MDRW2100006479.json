      "utterance": [
        {
          "id": "MDRW2100006479.1.1",
          "form": "마스크 쓰는건 안힘드시나요",
          "original_form": "마스크 쓰는건 안힘드시나요",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100006479.1.2",
          "form": "엄청 힘드네요ㅠㅠ",
          "original_form": "엄청 힘드네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100006479.1.3",
          "form": "어떤게 힘드시나요",
          "original_form": "어떤게 힘드시나요",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100006479.1.4",
          "form": "아무래도 습기가ㅠㅠ",
          "original_form": "아무래도 습기가ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100006479.1.5",
          "form": "그렇죠 피부트러블은요?",
          "original_form": "그렇죠 피부트러블은요?",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100006479.1.6",
          "form": "트러블도 문제죠",
          "original_form": "트러블도 문제죠",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100006479.1.7",
          "form": "이게 다 마스크 문베죠ㅠㅠ",
          "original_form": "이게 다 마스크 문베죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100006479.1.8",
          "form": "문제죠ㅠㅠ",
          "original_form": "문제죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100006479.1.9",
          "form": "마스크 쓴 이후 피부질환도 많아졌다더라구요",
          "original_form": "마스크 쓴 이후 피부질환도 많아졌다더라구요",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100006479.1.10",
          "form": "맞아요 아무래도 마스크쓸리고 피부 습해지고 하니까요",
          "original_form": "맞아요 아무래도 마스크쓸리고 피부 습해지고 하니까요",
          "speaker_id": "2",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100006479.1.11",
          "form": "여름/겨울 언제가 더 힘드신가요",
          "original_form": "여름/겨울 언제가 더 힘드신가요",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100006479.1.12",
          "form": "저는 여름이요",
          "original_form": "저는 여름이요",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100006479.1.13",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100006479.1.14",
          "form": "저는 안경을 써서 겨울이 더 불편하더라구요",
          "original_form": "저는 안경을 써서 겨울이 더 불편하더라구요",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100006479.1.15",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100006479.1.16",
          "form": "마스크 벗는 날이 올련지요ㅡ..",
          "original_form": "마스크 벗는 날이 올련지요ㅡ..",
