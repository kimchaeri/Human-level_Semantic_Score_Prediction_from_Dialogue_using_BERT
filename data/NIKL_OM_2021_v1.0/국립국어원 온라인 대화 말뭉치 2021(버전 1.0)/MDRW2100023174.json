      "utterance": [
        {
          "id": "MDRW2100023174.1.1",
          "form": "어디서 맨날",
          "original_form": "어디서 맨날",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023174.1.2",
          "form": "우리 직원이 아는 사람 누가 만들었다고",
          "original_form": "우리 직원이 아는 사람 누가 만들었다고",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023174.1.3",
          "form": "무화과잼만",
          "original_form": "무화과잼만",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023174.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ불쌍한 무화과잼",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ불쌍한 무화과잼",
          "speaker_id": "1",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023174.1.5",
          "form": "냉장고에 한칸 가득 채워놓고",
          "original_form": "냉장고에 한칸 가득 채워놓고",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023174.1.6",
          "form": "하필 무화과가 너무 많은 집에 가서",
          "original_form": "하필 무화과가 너무 많은 집에 가서",
          "speaker_id": "1",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023174.1.7",
          "form": "문전박대를....",
          "original_form": "문전박대를....",
          "speaker_id": "1",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023174.1.8",
          "form": "내가 딸기잼 먹고싶다고 하면",
          "original_form": "내가 딸기잼 먹고싶다고 하면",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023174.1.9",
          "form": "아니 집에 잼이",
          "original_form": "아니 집에 잼이",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023174.1.10",
          "form": "이렇게 많은데",
          "original_form": "이렇게 많은데",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023174.1.11",
          "form": "무슨 잼을 사서 먹냐고",
          "original_form": "무슨 잼을 사서 먹냐고",
          "speaker_id": "2",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100023174.1.12",
          "form": "아ㅋㅋㅋㅋㅋㅋ 그거 뭔지 알지",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ 그거 뭔지 알지",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100023174.1.13",
          "form": "빠꾸먹고",
          "original_form": "빠꾸먹고",
          "speaker_id": "2",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100023174.1.14",
          "form": "우리집도 어디서 오디 받아왔을때 한창 그랬음",
          "original_form": "우리집도 어디서 오디 받아왔을때 한창 그랬음",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100023174.1.15",
          "form": "그래서 내가 한 중학교 2학년때까지",
          "original_form": "그래서 내가 한 중학교 2학년때까지",
          "speaker_id": "2",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100023174.1.16",
          "form": "딸기잼을 못사먹어봣어",
          "original_form": "딸기잼을 못사먹어봣어",
          "speaker_id": "2",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100023174.1.17",
          "form": "맨날 무화과잼만 먹엇어",
          "original_form": "맨날 무화과잼만 먹엇어",
          "speaker_id": "2",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100023174.1.18",
          "form": "그래서 질렸어....",
          "original_form": "그래서 질렸어....",
          "speaker_id": "2",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100023174.1.19",
          "form": "아니 빵사러가면",
          "original_form": "아니 빵사러가면",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100023174.1.20",
          "form": "아닠ㅋㅋㅋㅋㅋ 중2때까지는 너무하네...",
          "original_form": "아닠ㅋㅋㅋㅋㅋ 중2때까지는 너무하네...",
          "speaker_id": "1",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100023174.1.21",
          "form": "딸기잼 포도잼 마멀레이드",
          "original_form": "딸기잼 포도잼 마멀레이드",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100023174.1.22",
          "form": "딱딱 진열이 되어잇는데",
          "original_form": "딱딱 진열이 되어잇는데",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100023174.1.23",
          "form": "한번만 사주래도",
          "original_form": "한번만 사주래도",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100023174.1.24",
          "form": "ㅋㅋㅋㅋㅋ 너무하시네",
          "original_form": "ㅋㅋㅋㅋㅋ 너무하시네",
          "speaker_id": "1",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100023174.1.25",
          "form": "이미 무화과잼이 꽉차잇어서",
          "original_form": "이미 무화과잼이 꽉차잇어서",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100023174.1.26",
          "form": "여튼 그래서 무화과를",
          "original_form": "여튼 그래서 무화과를",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100023174.1.27",
          "form": "돈주고사먹을일은",
          "original_form": "돈주고사먹을일은",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100023174.1.28",
          "form": "없을거야....",
          "original_form": "없을거야....",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023174.1.29",
          "form": "우리도 한때 오디가 그랬어서",
          "original_form": "우리도 한때 오디가 그랬어서",
          "speaker_id": "1",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023174.1.30",
          "form": "엄마가 넘치는 오디를 주체하지 못하고",
          "original_form": "엄마가 넘치는 오디를 주체하지 못하고",
          "speaker_id": "1",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023174.1.31",
          "form": "냉장고에 잔뜩 얼려두고",
          "original_form": "냉장고에 잔뜩 얼려두고",
          "speaker_id": "1",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023174.1.32",
          "form": "오디 잼에 오디청?만들어서",
          "original_form": "오디 잼에 오디청?만들어서",
          "speaker_id": "1",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023174.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023174.1.34",
          "form": "강제 배식함 ㅋㅋㅋㅋㅋ",
          "original_form": "강제 배식함 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023174.1.35",
          "form": "그러고선 내가",
          "original_form": "그러고선 내가",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023174.1.36",
          "form": "계속 딸기잼 먹고십다그러닉가",
          "original_form": "계속 딸기잼 먹고십다그러닉가",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023174.1.37",
          "form": "어디선가",
          "original_form": "어디선가",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023174.1.38",
          "form": "딸기를 가득 사오고",
          "original_form": "딸기를 가득 사오고",
          "speaker_id": "2",
          "time": "20210829 22:31"
