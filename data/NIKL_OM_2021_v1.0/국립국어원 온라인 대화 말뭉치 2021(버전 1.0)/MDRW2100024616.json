        {
          "id": "MDRW2100024616.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 15:46"
        },
        {
          "id": "MDRW2100024616.1.3",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 15:46"
        },
        {
          "id": "MDRW2100024616.1.4",
          "form": "아뇨 저는 집순이라 ㅎ",
          "original_form": "아뇨 저는 집순이라 ㅎ",
          "speaker_id": "1",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100024616.1.5",
          "form": "여행 자주 다니시나요?",
          "original_form": "여행 자주 다니시나요?",
          "speaker_id": "1",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100024616.1.6",
          "form": "아뇨...저도 집순이입니다ㅋㅋㅋㅋ",
          "original_form": "아뇨...저도 집순이입니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:48"
        },
        {
          "id": "MDRW2100024616.1.7",
          "form": "요즘 또 코시국이라고 더 하죠...",
          "original_form": "요즘 또 코시국이라고 더 하죠...",
          "speaker_id": "2",
          "time": "20210831 15:48"
        },
        {
          "id": "MDRW2100024616.1.8",
          "form": "맞아요.. 어디 나갔다가 코로나 걸릴까봐 무서워요 ㅋㅋㅋ",
          "original_form": "맞아요.. 어디 나갔다가 코로나 걸릴까봐 무서워요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:50"
        },
        {
          "id": "MDRW2100024616.1.9",
          "form": "저는 그냥.. 여행사진 고화질로만 보면 대충 만족되더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 그냥.. 여행사진 고화질로만 보면 대충 만족되더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:50"
        },
        {
          "id": "MDRW2100024616.1.10",
          "form": "예전에 어디 여행 가셨었나요?",
          "original_form": "예전에 어디 여행 가셨었나요?",
          "speaker_id": "2",
          "time": "20210831 15:52"
        },
        {
          "id": "MDRW2100024616.1.11",
          "form": "음 일본 한번 가봤어요",
          "original_form": "음 일본 한번 가봤어요",
          "speaker_id": "1",
          "time": "20210831 15:53"
        },
        {
          "id": "MDRW2100024616.1.12",
          "form": "일본 가보셨나요.",
          "original_form": "일본 가보셨나요.",
          "speaker_id": "1",
          "time": "20210831 15:53"
        },
        {
          "id": "MDRW2100024616.1.13",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210831 15:53"
        },
        {
          "id": "MDRW2100024616.1.14",
          "form": "아뇨ㅜ 전 해외여행 가본 적이 없어요",
          "original_form": "아뇨ㅜ 전 해외여행 가본 적이 없어요",
          "speaker_id": "2",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100024616.1.15",
          "form": "일본 어떠셨어요?",
          "original_form": "일본 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100024616.1.16",
          "form": "한국이랑 되게 비슷해서 다니기 편하구요",
          "original_form": "한국이랑 되게 비슷해서 다니기 편하구요",
          "speaker_id": "1",
          "time": "20210831 15:57"
        },
        {
          "id": "MDRW2100024616.1.17",
          "form": "근데 또 외국분위기는 나서 좋더라구요 코로나끝나면 또 가고싶어요",
          "original_form": "근데 또 외국분위기는 나서 좋더라구요 코로나끝나면 또 가고싶어요",
          "speaker_id": "1",
          "time": "20210831 15:57"
        },
        {
          "id": "MDRW2100024616.1.18",
          "form": "가고 싶은 여행지 있으신가요?",
          "original_form": "가고 싶은 여행지 있으신가요?",
          "speaker_id": "1",
          "time": "20210831 15:58"
        },
        {
          "id": "MDRW2100024616.1.19",
          "form": "저는 제주도요!",
          "original_form": "저는 제주도요!",
          "speaker_id": "2",
          "time": "20210831 16:00"
        },
        {
          "id": "MDRW2100024616.1.20",
          "form": "제주도 오랜만에 다시 가보고파요 많이 변했을거같아서오",
          "original_form": "제주도 오랜만에 다시 가보고파요 많이 변했을거같아서오",
          "speaker_id": "2",
          "time": "20210831 16:01"
        },
        {
          "id": "MDRW2100024616.1.21",
          "form": "아 제주도 저도요 가고싶네요",
          "original_form": "아 제주도 저도요 가고싶네요",
          "speaker_id": "1",
          "time": "20210831 16:03"
        },
        {
          "id": "MDRW2100024616.1.22",
          "form": "생각보다 비행기표가 싸더라고요 타이밍만 잘 보면?",
          "original_form": "생각보다 비행기표가 싸더라고요 타이밍만 잘 보면?",
          "speaker_id": "1",
          "time": "20210831 16:03"
        },
        {
          "id": "MDRW2100024616.1.23",
          "form": "비행기 타는거 재밌지 않나요?..ㅋㅋㅋ",
          "original_form": "비행기 타는거 재밌지 않나요?..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:03"
        },
        {
          "id": "MDRW2100024616.1.24",
          "form": "ㅋㅋㅋ저는 겁이 많아가지구ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ저는 겁이 많아가지구ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:04"
        },
        {
          "id": "MDRW2100024616.1.25",
          "form": "아하 그럼 놀이공원 자주안가시나요?",
          "original_form": "아하 그럼 놀이공원 자주안가시나요?",
          "speaker_id": "1",
          "time": "20210831 16:05"
        },
        {
          "id": "MDRW2100024616.1.26",
          "form": "디즈니 랜드? 넘 궁금해요",
          "original_form": "디즈니 랜드? 넘 궁금해요",
          "speaker_id": "1",
          "time": "20210831 16:06"
        },
        {
          "id": "MDRW2100024616.1.27",
          "form": "저도 디즈니랜드가고파서 가까운 일본디즈니랜드라도 가볼까했었어요",
          "original_form": "저도 디즈니랜드가고파서 가까운 일본디즈니랜드라도 가볼까했었어요",
          "speaker_id": "2",
          "time": "20210831 16:14"
        },
        {
          "id": "MDRW2100024616.1.28",
          "form": "좋을거 같아요 으 빨리 코로나 끝나면 좋겠네요",
          "original_form": "좋을거 같아요 으 빨리 코로나 끝나면 좋겠네요",
          "speaker_id": "1",
          "time": "20210831 16:32"
        }
      ]
