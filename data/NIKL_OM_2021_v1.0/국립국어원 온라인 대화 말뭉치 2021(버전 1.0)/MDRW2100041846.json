      "utterance": [
        {
          "id": "MDRW2100041846.1.1",
          "form": "동물 좋아하시나요?",
          "original_form": "동물 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041846.1.2",
          "form": "네 동물 좋아하죠!",
          "original_form": "네 동물 좋아하죠!",
          "speaker_id": "2",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041846.1.3",
          "form": "제가 키우진않고 본가에서 키우고있어요",
          "original_form": "제가 키우진않고 본가에서 키우고있어요",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041846.1.4",
          "form": "강아지용?",
          "original_form": "강아지용?",
          "speaker_id": "1",
          "time": "20210906 18:50"
        },
        {
          "id": "MDRW2100041846.1.5",
          "form": "네 강아지 키우고있어요!",
          "original_form": "네 강아지 키우고있어요!",
          "speaker_id": "2",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100041846.1.6",
          "form": "키우시는거 있으세요?",
          "original_form": "키우시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100041846.1.7",
          "form": "아뇨 전 없어요 ㅠ",
          "original_form": "아뇨 전 없어요 ㅠ",
          "speaker_id": "1",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100041846.1.8",
          "form": "강아지들 밥은 뭐먹어요? 사료?",
          "original_form": "강아지들 밥은 뭐먹어요? 사료?",
          "speaker_id": "1",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100041846.1.9",
          "form": "네 사료도 먹고 가끔 고기먹으면 고기 남는것도 좀 줘요",
          "original_form": "네 사료도 먹고 가끔 고기먹으면 고기 남는것도 좀 줘요",
          "speaker_id": "2",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100041846.1.10",
          "form": "키우고 싶은 동물 있으세요?",
          "original_form": "키우고 싶은 동물 있으세요?",
          "speaker_id": "2",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100041846.1.11",
          "form": "저도 강아지 키우고싶어요 ㅎㅎ 진돗개 넘 예쁘더라구요",
          "original_form": "저도 강아지 키우고싶어요 ㅎㅎ 진돗개 넘 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100041846.1.12",
          "form": "마자요 진돗개 예쁘죠ㅠ",
          "original_form": "마자요 진돗개 예쁘죠ㅠ",
          "speaker_id": "2",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100041846.1.13",
          "form": "강아지 말고는  없으세요?",
          "original_form": "강아지 말고는  없으세요?",
          "speaker_id": "2",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100041846.1.14",
          "form": "네네 아직은요 님은요?",
          "original_form": "네네 아직은요 님은요?",
          "speaker_id": "1",
          "time": "20210906 19:17"
        },
        {
          "id": "MDRW2100041846.1.15",
          "form": "저는 고양이나 고슴도치도 한번 키워보고싶어요",
          "original_form": "저는 고양이나 고슴도치도 한번 키워보고싶어요",
          "speaker_id": "2",
          "time": "20210906 20:06"
        },
        {
          "id": "MDRW2100041846.1.16",
          "form": "오 고슴고치 특이해여",
          "original_form": "오 고슴고치 특이해여",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100041846.1.17",
          "form": "고슴도치도 귀엽고 좋을거같아요",
          "original_form": "고슴도치도 귀엽고 좋을거같아요",
          "speaker_id": "2",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100041846.1.18",
          "form": "요새 은근 키우는사람 많더라구요",
          "original_form": "요새 은근 키우는사람 많더라구요",
          "speaker_id": "2",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100041846.1.19",
          "form": "귀여울거같아요 ㅋㅋ",
          "original_form": "귀여울거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:25"
        }
      ]
