      "utterance": [
        {
          "id": "MDRW2100023091.1.1",
          "form": "건강한 편이에요?",
          "original_form": "건강한 편이에요?",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023091.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ넼ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ넼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023091.1.3",
          "form": "운동 꾸준히 해요",
          "original_form": "운동 꾸준히 해요",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023091.1.4",
          "form": "오 운동을 꾸준히 하시는구나",
          "original_form": "오 운동을 꾸준히 하시는구나",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023091.1.5",
          "form": "그리고 병원가서 검사도 자주 받고",
          "original_form": "그리고 병원가서 검사도 자주 받고",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023091.1.6",
          "form": "한여름에도 꾸준히 했어요?",
          "original_form": "한여름에도 꾸준히 했어요?",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023091.1.7",
          "form": "검사는 주로 어떤거 받아요?",
          "original_form": "검사는 주로 어떤거 받아요?",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023091.1.8",
          "form": "네네 필라테스 학원 다니거든요",
          "original_form": "네네 필라테스 학원 다니거든요",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023091.1.9",
          "form": "그냥 뭐 초음파검사같은거",
          "original_form": "그냥 뭐 초음파검사같은거",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023091.1.10",
          "form": "어머 필라테스! 저도 집에서 그거해요 ㅋㅋㅋ",
          "original_form": "어머 필라테스! 저도 집에서 그거해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023091.1.11",
          "form": "위내시경이나 ㅋㅋㅋ",
          "original_form": "위내시경이나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023091.1.12",
          "form": "오 필라테스 집에서 해요?",
          "original_form": "오 필라테스 집에서 해요?",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023091.1.13",
          "form": "네~ 뭐 특별한건 아니구 가볍게 해요",
          "original_form": "네~ 뭐 특별한건 아니구 가볍게 해요",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023091.1.14",
          "form": "우와 대단하시네요 집에서 하는건 쉽지않은데",
          "original_form": "우와 대단하시네요 집에서 하는건 쉽지않은데",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023091.1.15",
          "form": "폼블러랑 요가링도 필라테스 맞아요?",
          "original_form": "폼블러랑 요가링도 필라테스 맞아요?",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023091.1.16",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023091.1.17",
          "form": "크크 요가매트 깔아놓고 가끔 해요",
          "original_form": "크크 요가매트 깔아놓고 가끔 해요",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023091.1.18",
          "form": "종아리 시원하더라구요",
          "original_form": "종아리 시원하더라구요",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023091.1.19",
          "form": "오 의지의 한국인",
          "original_form": "오 의지의 한국인",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023091.1.20",
          "form": "전 집에서 혼자선 잘 안해서",
          "original_form": "전 집에서 혼자선 잘 안해서",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023091.1.21",
          "form": "ㅋㅋㅋㅋ 남자가 필라테스 하는거 처음보셨죠?",
          "original_form": "ㅋㅋㅋㅋ 남자가 필라테스 하는거 처음보셨죠?",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023091.1.22",
          "form": "억지로 배우러 가야해요",
          "original_form": "억지로 배우러 가야해요",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023091.1.23",
          "form": "뭐 학원에 종종 있긴 한데",
          "original_form": "뭐 학원에 종종 있긴 한데",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023091.1.24",
          "form": "집에서 한다는 분은 첨봐요",
          "original_form": "집에서 한다는 분은 첨봐요",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023091.1.25",
          "form": "ㅋㅋㅋ 오 진짜요?",
          "original_form": "ㅋㅋㅋ 오 진짜요?",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023091.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023091.1.27",
          "form": "전 학원에도 남성분 있을 줄 몰랐어요",
          "original_form": "전 학원에도 남성분 있을 줄 몰랐어요",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023091.1.28",
          "form": "제 친구들은 남자가 뭐 그런거 하냐고 다 욕하던데 ㅜㅜ",
          "original_form": "제 친구들은 남자가 뭐 그런거 하냐고 다 욕하던데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023091.1.29",
          "form": "어쩌다가 시작을 핬어요",
          "original_form": "어쩌다가 시작을 핬어요",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023091.1.30",
          "form": "저 이벤트 응모했는데 필라테스 당첨 됐어요!",
          "original_form": "저 이벤트 응모했는데 필라테스 당첨 됐어요!",
          "speaker_id": "1",
          "time": "20210829 21:40"
        }
      ]
    }
