      "utterance": [
        {
          "id": "MDRW2100009507.1.1",
          "form": "여행은 계획을 짜고 가세요? 아니면 즉흥으로?",
          "original_form": "여행은 계획을 짜고 가세요? 아니면 즉흥으로?",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009507.1.2",
          "form": "반반이요 ㅎㅎ",
          "original_form": "반반이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009507.1.3",
          "form": "혼자가면 거의 즉흥인데",
          "original_form": "혼자가면 거의 즉흥인데",
          "speaker_id": "2",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009507.1.4",
          "form": "동행자 있음 아무래도 계획을 짜야 ㅎㅎ",
          "original_form": "동행자 있음 아무래도 계획을 짜야 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009507.1.5",
          "form": "동행자가 어떤 스타일에 따라 다르죠ㅎㅎ",
          "original_form": "동행자가 어떤 스타일에 따라 다르죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009507.1.6",
          "form": "ㅎㅎ 맞아요",
          "original_form": "ㅎㅎ 맞아요",
          "speaker_id": "2",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009507.1.7",
          "form": "전 거의 제 친구들이 저한테 맡기는 스타일이라ㅎㅎ 맨날 즉흥에 복불복ㅎㅎ",
          "original_form": "전 거의 제 친구들이 저한테 맡기는 스타일이라ㅎㅎ 맨날 즉흥에 복불복ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009507.1.8",
          "form": "카드 뽑기 맨날하고 그래요ㄹㅎ",
          "original_form": "카드 뽑기 맨날하고 그래요ㄹㅎ",
          "speaker_id": "1",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009507.1.9",
          "form": "재미겠어요 ㅎㅎ",
          "original_form": "재미겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009507.1.10",
          "form": "여자들이더  계획적인건 좋아하는거 같아요",
          "original_form": "여자들이더  계획적인건 좋아하는거 같아요",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009507.1.11",
          "form": "글죠ㅎ 아무래도 남자들보다는 여자가 신경써야할 부분이 많으니깐요ㅎ",
          "original_form": "글죠ㅎ 아무래도 남자들보다는 여자가 신경써야할 부분이 많으니깐요ㅎ",
          "speaker_id": "1",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009507.1.12",
          "form": "여행갈때 가장 중요시하는건 있으세요?",
          "original_form": "여행갈때 가장 중요시하는건 있으세요?",
          "speaker_id": "1",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009507.1.13",
          "form": "뭐 예를들면 숙소라든지?",
          "original_form": "뭐 예를들면 숙소라든지?",
          "speaker_id": "1",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009507.1.14",
          "form": "숙소요",
          "original_form": "숙소요",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009507.1.15",
          "form": "확실히 깔끔 한게 ㅎㅎ",
          "original_form": "확실히 깔끔 한게 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009507.1.16",
          "form": "예전에 즉흥으로 강원도 갓다가 ㅋ",
          "original_form": "예전에 즉흥으로 강원도 갓다가 ㅋ",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009507.1.17",
          "form": "차키 잊어버리고 추위에 2시간 떨고 \r\n숙소 없어서 아무곳이나 갔는데 ㅎㅎㅎ",
          "original_form": "차키 잊어버리고 추위에 2시간 떨고 \r\n숙소 없어서 아무곳이나 갔는데 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009507.1.18",
          "form": "ㅎㅎ 이벤트가있었네요ㅎ",
          "original_form": "ㅎㅎ 이벤트가있었네요ㅎ",
          "speaker_id": "1",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009507.1.19",
          "form": "ㅠㅠ 숙소가정말  방에 커피포트도 없고",
          "original_form": "ㅠㅠ 숙소가정말  방에 커피포트도 없고",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009507.1.20",
          "form": "숙소는 진짜 깔끔하긴해야해여ㅠ",
          "original_form": "숙소는 진짜 깔끔하긴해야해여ㅠ",
          "speaker_id": "1",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009507.1.21",
          "form": "정수기가 ㅋㅋ 1층에 잇엇어요 ㅎㅎ",
          "original_form": "정수기가 ㅋㅋ 1층에 잇엇어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009507.1.22",
          "form": "잘못하면 여행을 망칠수가 있어서ㅠ",
          "original_form": "잘못하면 여행을 망칠수가 있어서ㅠ",
          "speaker_id": "1",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009507.1.23",
          "form": "숙소가 가장중요하죠ㅎ 정말ㅎㅎ",
          "original_form": "숙소가 가장중요하죠ㅎ 정말ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009507.1.24",
          "form": "엄청불편하셨겟네요ㅎㅎ",
          "original_form": "엄청불편하셨겟네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009507.1.25",
          "form": "그다음으로는요?ㅎㅎ",
          "original_form": "그다음으로는요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009507.1.26",
          "form": "그거 빼곤  괜찮은거 같아요 ㅎㅎ",
          "original_form": "그거 빼곤  괜찮은거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009507.1.27",
          "form": "숨은 맛집좀 많았음 하는 바램 정도~~",
          "original_form": "숨은 맛집좀 많았음 하는 바램 정도~~",
          "speaker_id": "2",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009507.1.28",
          "form": "보통 가서 맛집 검색하잖아요ㅎㅎ",
          "original_form": "보통 가서 맛집 검색하잖아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100009507.1.29",
          "form": "가보면 실망하고가고ㅎㅎ",
          "original_form": "가보면 실망하고가고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100009507.1.30",
          "form": "그마마 그렇게 실망 한곳은 없엇던거 같아요 ㅎㅎ",
          "original_form": "그마마 그렇게 실망 한곳은 없엇던거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009507.1.31",
          "form": "그래서 저는 아에 검색안하고 현지인에게 물어보고 들어가요ㅎ 그게 더 재밌고 맛집을 찾을 확률이 높더라구요ㅎ",
          "original_form": "그래서 저는 아에 검색안하고 현지인에게 물어보고 들어가요ㅎ 그게 더 재밌고 맛집을 찾을 확률이 높더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009507.1.32",
          "form": "택시 아저씨한테 물어보면 최고에요 ㅎㅎ",
          "original_form": "택시 아저씨한테 물어보면 최고에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009507.1.33",
          "form": "맞아요ㅎㅎ 현지인에게물어보는게 최고ㄹㅎ",
          "original_form": "맞아요ㅎㅎ 현지인에게물어보는게 최고ㄹㅎ",
          "speaker_id": "1",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009507.1.34",
          "form": "음료수 사서 드리면서 알려달라 하면 엄청 친절 ㅎㅎ",
          "original_form": "음료수 사서 드리면서 알려달라 하면 엄청 친절 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009507.1.35",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009507.1.36",
          "form": "정말 친절하게 알려주세요ㄹ",
          "original_form": "정말 친절하게 알려주세요ㄹ",
          "speaker_id": "1",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009507.1.37",
          "form": "여행갈때 기념품 같은건 사세요?ㅎ",
          "original_form": "여행갈때 기념품 같은건 사세요?ㅎ",
          "speaker_id": "1",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009507.1.38",
          "form": "음 잘 안사는거 같아요",
          "original_form": "음 잘 안사는거 같아요",
          "speaker_id": "2",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100009507.1.39",
          "form": "맛있는건 택배예약해요",
          "original_form": "맛있는건 택배예약해요",
          "speaker_id": "2",
          "time": "20210823 00:18"
        },
        {
          "id": "MDRW2100009507.1.40",
          "form": "기념품이라기보단 제주도같은대서 택배보내는게 좋죠ㄹ",
          "original_form": "기념품이라기보단 제주도같은대서 택배보내는게 좋죠ㄹ",
          "speaker_id": "1",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100009507.1.41",
          "form": "제주도는 근데 택배해야해요ㅎ 안그러면 불편해서ㅍ",
          "original_form": "제주도는 근데 택배해야해요ㅎ 안그러면 불편해서ㅍ",
