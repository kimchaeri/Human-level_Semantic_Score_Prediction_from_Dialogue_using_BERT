      "utterance": [
        {
          "id": "MDRW2100005963.1.1",
          "form": "나 시월 초 이 싶어요",
          "original_form": "나 시월 초 이 싶어요",
          "speaker_id": "2",
          "time": "20210907 17:41"
        },
        {
          "id": "MDRW2100005963.1.2",
          "form": "10월 초??",
          "original_form": "10월 초??",
          "speaker_id": "2",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.3",
          "form": "나는 6주로 미뤄져서 오늘이었는데",
          "original_form": "나는 6주로 미뤄져서 오늘이었는데",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.4",
          "form": "텀이",
          "original_form": "텀이",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.5",
          "form": "일주일 정도 당길 수 있다던데ㅋㅋㅋㅋ",
          "original_form": "일주일 정도 당길 수 있다던데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.6",
          "form": "4주? 6주??",
          "original_form": "4주? 6주??",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.7",
          "form": "어 맞다",
          "original_form": "어 맞다",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.8",
          "form": "우리도련님도 일주일 당겼더라",
          "original_form": "우리도련님도 일주일 당겼더라",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.9",
          "form": "이제 또 독감도 맞아야 되고",
          "original_form": "이제 또 독감도 맞아야 되고",
          "speaker_id": "2",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.10",
          "form": "미리 당겨서 예약해놔라",
          "original_form": "미리 당겨서 예약해놔라",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.11",
          "form": "주사맞다 볼 일 다 보겠네요",
          "original_form": "주사맞다 볼 일 다 보겠네요",
          "speaker_id": "2",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.12",
          "form": "독감 맞을꺼가??",
          "original_form": "독감 맞을꺼가??",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.13",
          "form": "나는 희안하게",
          "original_form": "나는 희안하게",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.14",
          "form": "안 맞을래요 ㅋㅋㅋ",
          "original_form": "안 맞을래요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.15",
          "form": "독감 접종한 해에만",
          "original_form": "독감 접종한 해에만",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.16",
          "form": "독감 걸려가지고",
          "original_form": "독감 걸려가지고",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.17",
          "form": "안맞을란다",
          "original_form": "안맞을란다",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.18",
          "form": "아프잖아요ㅋㅋㅋㅋ",
          "original_form": "아프잖아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100005963.1.19",
          "form": "ㅋㄱㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㄱㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:43"
        },
        {
          "id": "MDRW2100005963.1.20",
          "form": "그러이 아프지",
          "original_form": "그러이 아프지",
          "speaker_id": "1",
          "time": "20210907 17:43"
        },
        {
          "id": "MDRW2100005963.1.21",
          "form": "나도 작년인가 안 맞았던 듯ㅋㅋㅋ",
