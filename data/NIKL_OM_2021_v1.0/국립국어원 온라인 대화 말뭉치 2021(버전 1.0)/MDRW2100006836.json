      "utterance": [
        {
          "id": "MDRW2100006836.1.1",
          "form": "빙수먹고싶다",
          "original_form": "빙수먹고싶다",
          "speaker_id": "2",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100006836.1.2",
          "form": "설빙",
          "original_form": "설빙",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006836.1.3",
          "form": "굿?",
          "original_form": "굿?",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006836.1.4",
          "form": "메론빙수 장난아니던데",
          "original_form": "메론빙수 장난아니던데",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100006836.1.5",
          "form": "먹어봄?",
          "original_form": "먹어봄?",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100006836.1.6",
          "form": "설빙을",
          "original_form": "설빙을",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100006836.1.7",
          "form": "안가봄",
          "original_form": "안가봄",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100006836.1.8",
          "form": "님은?",
          "original_form": "님은?",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100006836.1.9",
          "form": "꼭먹어보셈 요거통통",
          "original_form": "꼭먹어보셈 요거통통",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100006836.1.10",
          "form": "배달됨",
          "original_form": "배달됨",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100006836.1.11",
          "form": "요거통통?",
          "original_form": "요거통통?",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100006836.1.12",
          "form": "요거트임?",
          "original_form": "요거트임?",
          "speaker_id": "1",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100006836.1.13",
          "form": "요거트아이스크림에 메론",
          "original_form": "요거트아이스크림에 메론",
          "speaker_id": "2",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100006836.1.14",
          "form": "존맛임 추천",
          "original_form": "존맛임 추천",
          "speaker_id": "2",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100006836.1.15",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "1",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100006836.1.16",
          "form": "가야겠다",
          "original_form": "가야겠다",
          "speaker_id": "1",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100006836.1.17",
          "form": "메론 철이니까 빨리먹어야해",
          "original_form": "메론 철이니까 빨리먹어야해",
          "speaker_id": "2",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100006836.1.18",
          "form": "점심뭐먹?",
          "original_form": "점심뭐먹?",
          "speaker_id": "2",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100006836.1.19",
          "form": "굶고 저녁먹을거임",
          "original_form": "굶고 저녁먹을거임",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100006836.1.20",
          "form": "설빙 주말에가봐야겠다",
          "original_form": "설빙 주말에가봐야겠다",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100006836.1.21",
          "form": "불닭에 머먹음",
          "original_form": "불닭에 머먹음",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100006836.1.22",
          "form": "불닭만먹음 흑... 저녁뭐먹음?",
          "original_form": "불닭만먹음 흑... 저녁뭐먹음?",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100006836.1.23",
          "form": "건강에 안좋음",
          "original_form": "건강에 안좋음",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100006836.1.24",
          "form": "김밥이랑 드셈",
          "original_form": "김밥이랑 드셈",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100006836.1.25",
          "form": "저녁은 고구마 먹을까 생각중",
          "original_form": "저녁은 고구마 먹을까 생각중",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100006836.1.26",
          "form": "챙겨주니 감동...그치만 편의점이멀어서 아쉽..",
          "original_form": "챙겨주니 감동...그치만 편의점이멀어서 아쉽..",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100006836.1.27",
          "form": "점심도안먹고?",
          "original_form": "점심도안먹고?",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100006836.1.28",
          "form": "아침 식빵 딸기잼",
          "original_form": "아침 식빵 딸기잼",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006836.1.29",
          "form": "고구마",
          "original_form": "고구마",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006836.1.30",
          "form": "김밥 먹음요 헉..",
          "original_form": "김밥 먹음요 헉..",
          "speaker_id": "1",
          "time": "20210908 10:45"
        }
      ]
    }
