      "utterance": [
        {
          "id": "MDRW2100017368.1.1",
          "form": "버거킹vs맥도날드는 어떠세요?",
          "original_form": "버거킹vs맥도날드는 어떠세요?",
          "speaker_id": "1",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017368.1.2",
          "form": "버거킹이 짱이죠",
          "original_form": "버거킹이 짱이죠",
          "speaker_id": "2",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017368.1.3",
          "form": "맥도날드는 한물 간 느낌이에요 ㅎㅎ",
          "original_form": "맥도날드는 한물 간 느낌이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017368.1.4",
          "form": "이런",
          "original_form": "이런",
          "speaker_id": "1",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017368.1.5",
          "form": "저랑 반대네요ㅋㅋㅋ",
          "original_form": "저랑 반대네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017368.1.6",
          "form": "맥도날드 언제부턴가 레시피 바뀐 것 같아요 ㅜㅜ",
          "original_form": "맥도날드 언제부턴가 레시피 바뀐 것 같아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017368.1.7",
          "form": "어떤 메뉴 좋아하세요?",
          "original_form": "어떤 메뉴 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017368.1.8",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017368.1.9",
          "form": "슈슈랑 슈비요",
          "original_form": "슈슈랑 슈비요",
          "speaker_id": "1",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017368.1.10",
          "form": "맥모닝도요",
          "original_form": "맥모닝도요",
          "speaker_id": "1",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017368.1.11",
          "form": "아 슈슈랑 슈비는 좋아요 ㅋㅋ",
          "original_form": "아 슈슈랑 슈비는 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017368.1.12",
          "form": "근데 생각보다 기본인 빅맥도 넘 좋더라구요",
          "original_form": "근데 생각보다 기본인 빅맥도 넘 좋더라구요",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017368.1.13",
          "form": "ㅋㅋㅋ저랑 입맛이 약간다르신듯요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ저랑 입맛이 약간다르신듯요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017368.1.14",
          "form": "그러니 세상에는 다양한 음식이 있는 것 같아요",
          "original_form": "그러니 세상에는 다양한 음식이 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100017368.1.15",
          "form": "전 맘스터치 싸이버거도 좋더라구요",
          "original_form": "전 맘스터치 싸이버거도 좋더라구요",
          "speaker_id": "2",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100017368.1.16",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017368.1.17",
          "form": "싸이버거는 저도좋아요",
          "original_form": "싸이버거는 저도좋아요",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017368.1.18",
          "form": "그건진짜맛있더라고요",
          "original_form": "그건진짜맛있더라고요",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017368.1.19",
          "form": "ㅋㅋㅋ싸이버거 매운맛도 먹어보셨어요?",
          "original_form": "ㅋㅋㅋ싸이버거 매운맛도 먹어보셨어요?",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017368.1.20",
          "form": "싸이가",
          "original_form": "싸이가",
          "speaker_id": "1",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100017368.1.21",
          "form": "매운맛도있나요?!",
          "original_form": "매운맛도있나요?!",
          "speaker_id": "1",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100017368.1.22",
          "form": "네 불닭소스 넣은 거 있더라구요",
          "original_form": "네 불닭소스 넣은 거 있더라구요",
          "speaker_id": "2",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100017368.1.23",
          "form": "전 그런 인위적인 매운맛 안좋아해서 안먹어봤어요",
          "original_form": "전 그런 인위적인 매운맛 안좋아해서 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100017368.1.24",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100017368.1.25",
          "form": "매운맛러버들을",
          "original_form": "매운맛러버들을",
          "speaker_id": "1",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100017368.1.26",
          "form": "위한버거군요",
          "original_form": "위한버거군요",
          "speaker_id": "1",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100017368.1.27",
          "form": "네 ㅋㅋ 그거 먹으면 폭풍설사할 것 같아요",
          "original_form": "네 ㅋㅋ 그거 먹으면 폭풍설사할 것 같아요",
          "speaker_id": "2",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100017368.1.28",
          "form": "엄두도 못내겠습니다 저는 ㅜㅜ",
          "original_form": "엄두도 못내겠습니다 저는 ㅜㅜ",
