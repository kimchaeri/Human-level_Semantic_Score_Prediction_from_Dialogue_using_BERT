      "utterance": [
        {
          "id": "MDRW2100040328.1.1",
          "form": "악 마지막 힌트 못 봤어요 뼈 붙은 고기예요?",
          "original_form": "악 마지막 힌트 못 봤어요 뼈 붙은 고기예요?",
          "speaker_id": "2",
          "time": "20210904 16:22"
        },
        {
          "id": "MDRW2100040328.1.2",
          "form": "그거 되게 애매해요 뼈 붙어 있기도 한 거 같고 아닌 것도 있고...?",
          "original_form": "그거 되게 애매해요 뼈 붙어 있기도 한 거 같고 아닌 것도 있고...?",
          "speaker_id": "1",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100040328.1.3",
          "form": "앗...ㅋㅋㅌㅌㅌ 어렵다",
          "original_form": "앗...ㅋㅋㅌㅌㅌ 어렵다",
          "speaker_id": "2",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100040328.1.4",
          "form": "매운 음식이예요?",
          "original_form": "매운 음식이예요?",
          "speaker_id": "2",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100040328.1.5",
          "form": "맵진 않아요!",
          "original_form": "맵진 않아요!",
          "speaker_id": "1",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100040328.1.6",
          "form": "그럼 양념이 되어있는 요리인가요?",
          "original_form": "그럼 양념이 되어있는 요리인가요?",
          "speaker_id": "2",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100040328.1.7",
          "form": "예 양념이 되어 있습니다!  다왔다 다왔다",
          "original_form": "예 양념이 되어 있습니다!  다왔다 다왔다",
          "speaker_id": "1",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100040328.1.8",
          "form": "구워먹는 음식이죠!",
          "original_form": "구워먹는 음식이죠!",
          "speaker_id": "2",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100040328.1.9",
          "form": "맞아요 맞아요!!! 구워먹기!",
          "original_form": "맞아요 맞아요!!! 구워먹기!",
          "speaker_id": "1",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100040328.1.10",
          "form": "정답! 돼지갈비!!!!!",
          "original_form": "정답! 돼지갈비!!!!!",
          "speaker_id": "2",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100040328.1.11",
          "form": "돼지갈비 맞습니다!! 와아아",
          "original_form": "돼지갈비 맞습니다!! 와아아",
          "speaker_id": "1",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100040328.1.12",
          "form": "제 동생의 최애 외식 메뉴에요",
          "original_form": "제 동생의 최애 외식 메뉴에요",
          "speaker_id": "1",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100040328.1.13",
          "form": "와아아아 진짜 맛있겠다 갈비 너무 맛있죠",
          "original_form": "와아아아 진짜 맛있겠다 갈비 너무 맛있죠",
          "speaker_id": "2",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100040328.1.14",
          "form": "힌트 중 뼈가 있는 거랑 없는 게 섞여있다는 게 결정적이었어요",
          "original_form": "힌트 중 뼈가 있는 거랑 없는 게 섞여있다는 게 결정적이었어요",
          "speaker_id": "2",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100040328.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저녁 거리두기 풀리니까 바로 외식...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저녁 거리두기 풀리니까 바로 외식...",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040328.1.16",
          "form": "이래서 거리두기 안풀어주는 거 알면서도 참...",
          "original_form": "이래서 거리두기 안풀어주는 거 알면서도 참...",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040328.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ사람들 다 벼르고 있었을 것..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ사람들 다 벼르고 있었을 것..",
          "speaker_id": "2",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100040328.1.18",
          "form": "코로나 조심히 잘 다녀오세요",
          "original_form": "코로나 조심히 잘 다녀오세요",
