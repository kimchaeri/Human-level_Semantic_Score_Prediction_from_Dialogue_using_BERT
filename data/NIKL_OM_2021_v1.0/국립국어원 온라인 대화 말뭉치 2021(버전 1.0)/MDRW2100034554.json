      "utterance": [
        {
          "id": "MDRW2100034554.1.1",
          "form": "산책하고 오셨나요?ㅎㅎ",
          "original_form": "산책하고 오셨나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 18:24"
        },
        {
          "id": "MDRW2100034554.1.2",
          "form": "저 이따 해지면 가려구요 ㅋㅋㅋㅋ",
          "original_form": "저 이따 해지면 가려구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100034554.1.3",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "2",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100034554.1.4",
          "form": "산책할때 땀복입고 걸어서 좀 잘보이면 부끄러워요",
          "original_form": "산책할때 땀복입고 걸어서 좀 잘보이면 부끄러워요",
          "speaker_id": "1",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100034554.1.5",
          "form": "저녁 산책 좋죠",
          "original_form": "저녁 산책 좋죠",
          "speaker_id": "2",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100034554.1.6",
          "form": "건강도 챙기고",
          "original_form": "건강도 챙기고",
          "speaker_id": "2",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100034554.1.7",
          "form": "아 땀복까지 입고요?!!",
          "original_form": "아 땀복까지 입고요?!!",
          "speaker_id": "2",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100034554.1.8",
          "form": "넼ㅋㅋㅋ뭔가 좀 더 운동 느낌 나려고요",
          "original_form": "넼ㅋㅋㅋ뭔가 좀 더 운동 느낌 나려고요",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100034554.1.9",
          "form": "오 다이어트 효과도 있겠네요",
          "original_form": "오 다이어트 효과도 있겠네요",
          "speaker_id": "2",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100034554.1.10",
          "form": "네 요즘 진짜 살찌는 기분이 들어서 ㅠㅠㅎㅎㅎ",
          "original_form": "네 요즘 진짜 살찌는 기분이 들어서 ㅠㅠㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100034554.1.11",
          "form": "살찌면 몸이 무겁고 건강이 안좋아지는 느낌이 있죠ㅜ",
          "original_form": "살찌면 몸이 무겁고 건강이 안좋아지는 느낌이 있죠ㅜ",
          "speaker_id": "2",
          "time": "20210828 18:35"
        },
        {
          "id": "MDRW2100034554.1.12",
          "form": "그쳐 이제 주말 밤인데 뭐하세요",
          "original_form": "그쳐 이제 주말 밤인데 뭐하세요",
          "speaker_id": "1",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100034554.1.13",
          "form": "저는 저녁 먹고 오랜만에 뒹굴거리며 정신건강을 챙기려고요",
          "original_form": "저는 저녁 먹고 오랜만에 뒹굴거리며 정신건강을 챙기려고요",
          "speaker_id": "2",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100034554.1.14",
          "form": "이번주에는 퇴근후에도 다음날 준비하며 일했거든요",
          "original_form": "이번주에는 퇴근후에도 다음날 준비하며 일했거든요",
          "speaker_id": "2",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100034554.1.15",
          "form": "와 저도 대략 그렇게 지내서",
          "original_form": "와 저도 대략 그렇게 지내서",
          "speaker_id": "1",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100034554.1.16",
          "form": "오늘 하루종일 뒹굴거려요",
          "original_form": "오늘 하루종일 뒹굴거려요",
          "speaker_id": "1",
          "time": "20210828 18:45"
        },
        {
          "id": "MDRW2100034554.1.17",
          "form": "화이팅합시다..ㅋㅋㅋ",
          "original_form": "화이팅합시다..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:45"
        },
        {
          "id": "MDRW2100034554.1.18",
          "form": "네네 정신건강도 중요하니까요",
          "original_form": "네네 정신건강도 중요하니까요",
