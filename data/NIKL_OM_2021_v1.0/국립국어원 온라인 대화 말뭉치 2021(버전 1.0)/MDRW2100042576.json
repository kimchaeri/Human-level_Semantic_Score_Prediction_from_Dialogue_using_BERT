      },
      "utterance": [
        {
          "id": "MDRW2100042576.1.1",
          "form": "맛있긴 한데 매번 번거로긴 하겠네요",
          "original_form": "맛있긴 한데 매번 번거로긴 하겠네요",
          "speaker_id": "2",
          "time": "20210907 00:19"
        },
        {
          "id": "MDRW2100042576.1.2",
          "form": "생수 사는게 너무 힘드니까 어쩔수 없어",
          "original_form": "생수 사는게 너무 힘드니까 어쩔수 없어",
          "speaker_id": "1",
          "time": "20210907 00:19"
        },
        {
          "id": "MDRW2100042576.1.3",
          "form": "배달 시키면 오지 않나요 ㅠ 배달아저씨도 힘들겠지만",
          "original_form": "배달 시키면 오지 않나요 ㅠ 배달아저씨도 힘들겠지만",
          "speaker_id": "2",
          "time": "20210907 00:20"
        },
        {
          "id": "MDRW2100042576.1.4",
          "form": "생수 시키는 놈이 제일 나쁜 놈이라더라",
          "original_form": "생수 시키는 놈이 제일 나쁜 놈이라더라",
          "speaker_id": "1",
          "time": "20210907 00:22"
        },
        {
          "id": "MDRW2100042576.1.5",
          "form": "ㅋㅋㅋㅋ 그렇게까지야 ㅠ 그래도 뭐 힘들긴 하겠지만요",
          "original_form": "ㅋㅋㅋㅋ 그렇게까지야 ㅠ 그래도 뭐 힘들긴 하겠지만요",
          "speaker_id": "2",
          "time": "20210907 00:22"
        },
        {
          "id": "MDRW2100042576.1.6",
          "form": "아빠랑 같이 매번 사오는것도 힘들어서 그냥 보리차 먹어 보리차 맛있잖아",
          "original_form": "아빠랑 같이 매번 사오는것도 힘들어서 그냥 보리차 먹어 보리차 맛있잖아",
          "speaker_id": "1",
          "time": "20210907 00:23"
        },
        {
          "id": "MDRW2100042576.1.7",
          "form": "정수기 사시면 될텐데 그것도 싫다고 하시고",
          "original_form": "정수기 사시면 될텐데 그것도 싫다고 하시고",
          "speaker_id": "2",
          "time": "20210907 00:23"
        },
        {
          "id": "MDRW2100042576.1.8",
          "form": "정수기 렌탈비 비싸잖아",
          "original_form": "정수기 렌탈비 비싸잖아",
          "speaker_id": "1",
          "time": "20210907 00:23"
        },
        {
          "id": "MDRW2100042576.1.9",
          "form": "그거 뭐 얼마나 한다고 그래요 제가 해드릴까보다",
          "original_form": "그거 뭐 얼마나 한다고 그래요 제가 해드릴까보다",
          "speaker_id": "2",
          "time": "20210907 00:23"
        },
        {
          "id": "MDRW2100042576.1.10",
          "form": "돈 아무데나 막 쓰지 마",
          "original_form": "돈 아무데나 막 쓰지 마",
          "speaker_id": "1",
          "time": "20210907 00:23"
        },
        {
          "id": "MDRW2100042576.1.11",
          "form": "효도하는데 스는거거든요 ㅋㅋㅋㅋ",
          "original_form": "효도하는데 스는거거든요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 00:23"
        },
        {
          "id": "MDRW2100042576.1.12",
          "form": "정수기도 요즘은 비싸지 않냐",
          "original_form": "정수기도 요즘은 비싸지 않냐",
          "speaker_id": "1",
          "time": "20210907 00:24"
        },
        {
          "id": "MDRW2100042576.1.13",
          "form": "옛날에도 비쌌지만",
          "original_form": "옛날에도 비쌌지만",
          "speaker_id": "1",
          "time": "20210907 00:24"
        },
        {
          "id": "MDRW2100042576.1.14",
          "form": "보리차가 맛있긴 하죠 취직해서 사드릴게요 건조기랑 정수기랑",
          "original_form": "보리차가 맛있긴 하죠 취직해서 사드릴게요 건조기랑 정수기랑",
          "speaker_id": "2",
          "time": "20210907 00:25"
        },
        {
          "id": "MDRW2100042576.1.15",
          "form": "아빠도 보리차가 좋대",
          "original_form": "아빠도 보리차가 좋대",
          "speaker_id": "1",
          "time": "20210907 00:25"
        },
        {
          "id": "MDRW2100042576.1.16",
          "form": "아빠는 대추차 싫다고 하셨어˜",
          "original_form": "아빠는 대추차 싫다고 하셨어˜",
          "speaker_id": "2",
          "time": "20210907 00:25"
        },
        {
          "id": "MDRW2100042576.1.17",
          "form": "대추차 맛있는데 왜 다들 싫어하지",
          "original_form": "대추차 맛있는데 왜 다들 싫어하지",
          "speaker_id": "1",
          "time": "20210907 00:25"
        },
        {
          "id": "MDRW2100042576.1.18",
          "form": "껍질때문에 대추맛 장난아니라고 들었는데 ㅋㅋ",
          "original_form": "껍질때문에 대추맛 장난아니라고 들었는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 00:25"
        }
