      "utterance": [
        {
          "id": "MDRW2100042612.1.1",
          "form": "운동하시나요?",
          "original_form": "운동하시나요?",
          "speaker_id": "2",
          "time": "20210906 23:43"
        },
        {
          "id": "MDRW2100042612.1.2",
          "form": "네 지금응 쉬지망요",
          "original_form": "네 지금응 쉬지망요",
          "speaker_id": "1",
          "time": "20210906 23:43"
        },
        {
          "id": "MDRW2100042612.1.3",
          "form": "아하 저도 쉬고 있어요! 백신맞았거든요!",
          "original_form": "아하 저도 쉬고 있어요! 백신맞았거든요!",
          "speaker_id": "2",
          "time": "20210906 23:43"
        },
        {
          "id": "MDRW2100042612.1.4",
          "form": "아 저도요 똑같에요",
          "original_form": "아 저도요 똑같에요",
          "speaker_id": "1",
          "time": "20210906 23:43"
        },
        {
          "id": "MDRW2100042612.1.5",
          "form": "원래 어떤운동하셨어요?",
          "original_form": "원래 어떤운동하셨어요?",
          "speaker_id": "2",
          "time": "20210906 23:44"
        },
        {
          "id": "MDRW2100042612.1.6",
          "form": "집에사 홈트를 했았어요",
          "original_form": "집에사 홈트를 했았어요",
          "speaker_id": "1",
          "time": "20210906 23:44"
        },
        {
          "id": "MDRW2100042612.1.7",
          "form": "헐 저도요!저도 홈투했었어요!",
          "original_form": "헐 저도요!저도 홈투했었어요!",
          "speaker_id": "2",
          "time": "20210906 23:44"
        },
        {
          "id": "MDRW2100042612.1.8",
          "form": "원래는 밖에서 걸었는데 코로나때문에 힘드네요",
          "original_form": "원래는 밖에서 걸었는데 코로나때문에 힘드네요",
          "speaker_id": "1",
          "time": "20210906 23:44"
        },
        {
          "id": "MDRW2100042612.1.9",
          "form": "아하ㅠ 그쵸ㅠㅠ 요즘 나가는게  진짜 어려우니까...",
          "original_form": "아하ㅠ 그쵸ㅠㅠ 요즘 나가는게  진짜 어려우니까...",
          "speaker_id": "2",
          "time": "20210906 23:45"
        },
        {
          "id": "MDRW2100042612.1.10",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210906 23:45"
        },
        {
          "id": "MDRW2100042612.1.11",
          "form": "그럼 식단관리도 하세요?아니면 건강챙기는 용으로 운동만??",
          "original_form": "그럼 식단관리도 하세요?아니면 건강챙기는 용으로 운동만??",
          "speaker_id": "2",
          "time": "20210906 23:45"
        },
        {
          "id": "MDRW2100042612.1.12",
          "form": "건강챙기면서 식단도 해요",
          "original_form": "건강챙기면서 식단도 해요",
          "speaker_id": "1",
          "time": "20210906 23:46"
        },
        {
          "id": "MDRW2100042612.1.13",
          "form": "헐 저도요! 전 저녁에만 샐러드를 먹어요!",
          "original_form": "헐 저도요! 전 저녁에만 샐러드를 먹어요!",
          "speaker_id": "2",
          "time": "20210906 23:46"
        },
        {
          "id": "MDRW2100042612.1.14",
          "form": "저도 그래요 똑같네요",
          "original_form": "저도 그래요 똑같네요",
          "speaker_id": "1",
          "time": "20210906 23:46"
