      "utterance": [
        {
          "id": "MDRW2100038449.1.1",
          "form": "제일 슬픈건..",
          "original_form": "제일 슬픈건..",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100038449.1.2",
          "form": "맥날은 아예없어요 ㅜㅜ",
          "original_form": "맥날은 아예없어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100038449.1.3",
          "form": "헉....",
          "original_form": "헉....",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100038449.1.4",
          "form": "흑흑 제가 진짜 빅맥 좋아하는데",
          "original_form": "흑흑 제가 진짜 빅맥 좋아하는데",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100038449.1.5",
          "form": "맥날이 없다니ㅠㅠㅠㅠ",
          "original_form": "맥날이 없다니ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100038449.1.6",
          "form": "먹고싶을때마다",
          "original_form": "먹고싶을때마다",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100038449.1.7",
          "form": "30분 버스타고 나가서",
          "original_form": "30분 버스타고 나가서",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100038449.1.8",
          "form": "먹고들어오는 집념을 보여줬죠..",
          "original_form": "먹고들어오는 집념을 보여줬죠..",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100038449.1.9",
          "form": "빅맥사랑 찐이시네...",
          "original_form": "빅맥사랑 찐이시네...",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100038449.1.10",
          "form": "그럼 롯데리아뿐인가요...",
          "original_form": "그럼 롯데리아뿐인가요...",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100038449.1.11",
          "form": "롯데랑 맘터는 있어요",
          "original_form": "롯데랑 맘터는 있어요",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100038449.1.12",
          "form": "맘터는 감튀먹으러가는",
          "original_form": "맘터는 감튀먹으러가는",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100038449.1.13",
          "form": "맘터 요즘 맛이 변했다는데",
          "original_form": "맘터 요즘 맛이 변했다는데",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100038449.1.14",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100038449.1.15",
          "form": "저도 최근에는 안먹어봤는대",
          "original_form": "저도 최근에는 안먹어봤는대",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100038449.1.16",
          "form": "신제품들은 다 별로였어요 ㅜ",
          "original_form": "신제품들은 다 별로였어요 ㅜ",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100038449.1.17",
          "form": "헉.... ㅠㅠ 싸이버거만 먹어야하는군요...",
          "original_form": "헉.... ㅠㅠ 싸이버거만 먹어야하는군요...",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100038449.1.18",
          "form": "슬퍼요..",
          "original_form": "슬퍼요..",
          "speaker_id": "2",
          "time": "20210903 08:27"
        },
        {
          "id": "MDRW2100038449.1.19",
          "form": "햄버거 어디거 좋아하새요",
          "original_form": "햄버거 어디거 좋아하새요",
          "speaker_id": "2",
          "time": "20210903 08:28"
        },
        {
          "id": "MDRW2100038449.1.20",
          "form": "저는 버거킹 좋아해요",
          "original_form": "저는 버거킹 좋아해요",
          "speaker_id": "1",
          "time": "20210903 08:28"
        },
        {
          "id": "MDRW2100038449.1.21",
          "form": "그다음 맥날 슈비버거요!",
          "original_form": "그다음 맥날 슈비버거요!",
          "speaker_id": "1",
          "time": "20210903 08:28"
        },
        {
          "id": "MDRW2100038449.1.22",
          "form": "버거킹! 한번도 안먹어봣어요",
          "original_form": "버거킹! 한번도 안먹어봣어요",
          "speaker_id": "2",
          "time": "20210903 08:31"
        },
        {
          "id": "MDRW2100038449.1.23",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "2",
          "time": "20210903 08:31"
        },
        {
          "id": "MDRW2100038449.1.24",
          "form": "패티에서 숯불향이나는게 매력이예요",
          "original_form": "패티에서 숯불향이나는게 매력이예요",
          "speaker_id": "1",
          "time": "20210903 08:32"
        }
