      "utterance": [
        {
          "id": "MDRW2100033120.1.1",
          "form": "배달 좋아하시나",
          "original_form": "배달 좋아하시나",
          "speaker_id": "2",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100033120.1.2",
          "form": "전 자주",
          "original_form": "전 자주",
          "speaker_id": "2",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100033120.1.3",
          "form": "저도 배달음식 많이 먹죠",
          "original_form": "저도 배달음식 많이 먹죠",
          "speaker_id": "1",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100033120.1.4",
          "form": "주로 어떤 거 시키세요",
          "original_form": "주로 어떤 거 시키세요",
          "speaker_id": "2",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100033120.1.5",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "1",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100033120.1.6",
          "form": "전 쌀국수",
          "original_form": "전 쌀국수",
          "speaker_id": "2",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100033120.1.7",
          "form": "저도 옛날에는 쌀국수 많이 먹었죠",
          "original_form": "저도 옛날에는 쌀국수 많이 먹었죠",
          "speaker_id": "1",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100033120.1.8",
          "form": "엥 입맛이 바뀌셨어요?",
          "original_form": "엥 입맛이 바뀌셨어요?",
          "speaker_id": "2",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100033120.1.9",
          "form": "네 그런 셈이죠",
          "original_form": "네 그런 셈이죠",
          "speaker_id": "1",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100033120.1.10",
          "form": "글쿠나.. 근데 전 치킨 비싸서ㅜㅠ",
          "original_form": "글쿠나.. 근데 전 치킨 비싸서ㅜㅠ",
          "speaker_id": "2",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100033120.1.11",
          "form": "그래서 저도 웬만하면 치킨 잘 안시켜요",
          "original_form": "그래서 저도 웬만하면 치킨 잘 안시켜요",
          "speaker_id": "1",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100033120.1.12",
          "form": "아이고.. 좀 더 싸면 좋겠네요",
          "original_form": "아이고.. 좀 더 싸면 좋겠네요",
          "speaker_id": "2",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100033120.1.13",
          "form": "너무 옛날에 비해 가격이 올라갔어요",
          "original_form": "너무 옛날에 비해 가격이 올라갔어요",
          "speaker_id": "1",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100033120.1.14",
          "form": "그니까요 옛날통닭은 싼데.",
          "original_form": "그니까요 옛날통닭은 싼데.",
          "speaker_id": "2",
          "time": "20210906 16:54"
        }
