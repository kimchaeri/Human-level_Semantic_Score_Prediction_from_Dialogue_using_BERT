      "utterance": [
        {
          "id": "MDRW2100012048.1.1",
          "form": "밖에 비가 많이 오죠ㅠ",
          "original_form": "밖에 비가 많이 오죠ㅠ",
          "speaker_id": "1",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100012048.1.2",
          "form": "태풍땜에ㅠ",
          "original_form": "태풍땜에ㅠ",
          "speaker_id": "1",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100012048.1.3",
          "form": "앗 맞아요ㅜㅜ",
          "original_form": "앗 맞아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100012048.1.4",
          "form": "그래도 저희 지역은 이젠 그쳤네요!",
          "original_form": "그래도 저희 지역은 이젠 그쳤네요!",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100012048.1.5",
          "form": "저희 지역도 지금 보니까 잠깐 안오는 것같아요",
          "original_form": "저희 지역도 지금 보니까 잠깐 안오는 것같아요",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100012048.1.6",
          "form": "비 그치니까 다시 조금 더워진 것 같네요",
          "original_form": "비 그치니까 다시 조금 더워진 것 같네요",
          "speaker_id": "2",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100012048.1.7",
          "form": "생각해보니 비올때 좀 더 쌀쌀함에 가까웠던 것같긴 해요",
          "original_form": "생각해보니 비올때 좀 더 쌀쌀함에 가까웠던 것같긴 해요",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100012048.1.8",
          "form": "그쵸! 그래도 비 올때 나가면 물 튀어서ㅠㅠ 이런 날씨가 더 나은 것 같아요",
          "original_form": "그쵸! 그래도 비 올때 나가면 물 튀어서ㅠㅠ 이런 날씨가 더 나은 것 같아요",
          "speaker_id": "2",
          "time": "20210827 00:29"
        },
        {
          "id": "MDRW2100012048.1.9",
          "form": "물 튀는거 별로에요 맞아요ㅠ 오늘도 비오네요",
          "original_form": "물 튀는거 별로에요 맞아요ㅠ 오늘도 비오네요",
          "speaker_id": "1",
          "time": "20210827 16:21"
        },
        {
          "id": "MDRW2100012048.1.10",
          "form": "헉 비가 왔었군요 저희 지역엔 안 왔었어요!",
          "original_form": "헉 비가 왔었군요 저희 지역엔 안 왔었어요!",
          "speaker_id": "2",
          "time": "20210828 01:43"
        },
        {
          "id": "MDRW2100012048.1.11",
          "form": "아 진짜요? 이제 전국적으로 내리진 않나봐요",
          "original_form": "아 진짜요? 이제 전국적으로 내리진 않나봐요",
          "speaker_id": "1",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100012048.1.12",
          "form": "저희 지역은 틈틈히 계속 비오더라구요",
          "original_form": "저희 지역은 틈틈히 계속 비오더라구요",
          "speaker_id": "1",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100012048.1.13",
          "form": "앗 그러면 오늘도 왔나요?",
          "original_form": "앗 그러면 오늘도 왔나요?",
          "speaker_id": "2",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100012048.1.14",
          "form": "오늘은 안왔는데",
          "original_form": "오늘은 안왔는데",
          "speaker_id": "1",
          "time": "20210829 01:12"
        },
        {
          "id": "MDRW2100012048.1.15",
          "form": "내일은 온대요!",
          "original_form": "내일은 온대요!",
          "speaker_id": "1",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100012048.1.16",
          "form": "오늘 안오긴 했지만 하루종일 흐리고 안개꼈어요ㅎ",
          "original_form": "오늘 안오긴 했지만 하루종일 흐리고 안개꼈어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100012048.1.17",
          "form": "와 날씨가 진짜 다르네요 어느 쪽 지역이신가요?",
          "original_form": "와 날씨가 진짜 다르네요 어느 쪽 지역이신가요?",
          "speaker_id": "2",
          "time": "20210829 01:28"
        },
        {
          "id": "MDRW2100012048.1.18",
          "form": "이쪽은 충남쪽이에요ㅎㅎ",
          "original_form": "이쪽은 충남쪽이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 19:38"
        }
      ]
