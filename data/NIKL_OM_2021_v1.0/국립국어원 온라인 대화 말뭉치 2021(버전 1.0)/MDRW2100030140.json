      "utterance": [
        {
          "id": "MDRW2100030140.1.1",
          "form": "우와~ 백신 맞으셨나요?",
          "original_form": "우와~ 백신 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100030140.1.2",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100030140.1.3",
          "form": "아직 한번도 안 맞으셨어요?",
          "original_form": "아직 한번도 안 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100030140.1.4",
          "form": "아뇨 저는 1차 접종만 겨우 며칠전 했어요!!",
          "original_form": "아뇨 저는 1차 접종만 겨우 며칠전 했어요!!",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030140.1.5",
          "form": "2차도 빨리 맞고싶어요ㅠㅠ",
          "original_form": "2차도 빨리 맞고싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030140.1.6",
          "form": "아아 저도 1차만 맞았어요",
          "original_form": "아아 저도 1차만 맞았어요",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030140.1.7",
          "form": "부작용 없으셨어요?",
          "original_form": "부작용 없으셨어요?",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030140.1.8",
          "form": "아 전 3일이 지나고서도 너무 피곤해서^^제가 게으른 건가 이게 부작용인가 아리까리하더라구요ㅋㅋㅋㅋ",
          "original_form": "아 전 3일이 지나고서도 너무 피곤해서^^제가 게으른 건가 이게 부작용인가 아리까리하더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030140.1.9",
          "form": "앗 두통도 있었어요",
          "original_form": "앗 두통도 있었어요",
          "speaker_id": "2",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030140.1.10",
          "form": "근데 여성은 부작용이 좀 심하잖아요",
          "original_form": "근데 여성은 부작용이 좀 심하잖아요",
          "speaker_id": "2",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030140.1.11",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030140.1.12",
          "form": "맞아요 그렇다고 하더라구요",
          "original_form": "맞아요 그렇다고 하더라구요",
          "speaker_id": "1",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030140.1.13",
          "form": "저는 근육통 있었어요",
          "original_form": "저는 근육통 있었어요",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100030140.1.14",
          "form": "ㅠㅠ 죽는줄 알았네요 ㅠㅠ",
          "original_form": "ㅠㅠ 죽는줄 알았네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100030140.1.15",
          "form": "아이고ㅠㅠㅠㅠㅠ고생하셨군요ㅠㅠㅠㅜㅜ",
          "original_form": "아이고ㅠㅠㅠㅠㅠ고생하셨군요ㅠㅠㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030140.1.16",
          "form": "2차가 두렵네요...",
          "original_form": "2차가 두렵네요...",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030140.1.17",
          "form": "화이자 맞으셨어요?",
          "original_form": "화이자 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030140.1.18",
          "form": "네넹 저 화이자!",
          "original_form": "네넹 저 화이자!",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100030140.1.19",
          "form": "뭐 맞으셨나용?",
          "original_form": "뭐 맞으셨나용?",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100030140.1.20",
          "form": "저도 화이자요!",
          "original_form": "저도 화이자요!",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100030140.1.21",
          "form": "화이자는 2차가 더 아프다는데 걱정이예요 ㅠㅠ",
          "original_form": "화이자는 2차가 더 아프다는데 걱정이예요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100030140.1.22",
          "form": "마자요ㅠㅠㅠㅠ그래서 2차가 두렵더라구요",
          "original_form": "마자요ㅠㅠㅠㅠ그래서 2차가 두렵더라구요",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030140.1.23",
          "form": "제발 무난히 넘겼으면 좋겠어요",
          "original_form": "제발 무난히 넘겼으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030140.1.24",
          "form": "ㅠㅠㅠ그니까요",
          "original_form": "ㅠㅠㅠ그니까요",
          "speaker_id": "1",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100030140.1.25",
          "form": "요즘에 백신 부작용 뉴스가 너무 많은 거 같아요",
          "original_form": "요즘에 백신 부작용 뉴스가 너무 많은 거 같아요",
          "speaker_id": "1",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100030140.1.26",
          "form": "맞아요ㅠㅠㅠㅠ무섭죠",
          "original_form": "맞아요ㅠㅠㅠㅠ무섭죠",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030140.1.27",
          "form": "뉴스에 나오는 사건이 벌어질 거라곤 생각하지 않지만",
          "original_form": "뉴스에 나오는 사건이 벌어질 거라곤 생각하지 않지만",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030140.1.28",
          "form": "생리부작용은 좀 두려워요ㅠㅠ",
          "original_form": "생리부작용은 좀 두려워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030140.1.29",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030140.1.30",
          "form": "제 친구 생리통이랑 겹쳐서 맞았는데",
          "original_form": "제 친구 생리통이랑 겹쳐서 맞았는데",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030140.1.31",
          "form": "타이레놀 5알 먹고도 힘들었데요",
          "original_form": "타이레놀 5알 먹고도 힘들었데요",
