      "utterance": [
        {
          "id": "MDRW2100035843.1.1",
          "form": "학교 졸업한지 2년이나 지났어요ㅋㅋ",
          "original_form": "학교 졸업한지 2년이나 지났어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 08:43"
        },
        {
          "id": "MDRW2100035843.1.2",
          "form": "저도 한 5년되었네요",
          "original_form": "저도 한 5년되었네요",
          "speaker_id": "1",
          "time": "20210830 08:46"
        },
        {
          "id": "MDRW2100035843.1.3",
          "form": "동아리 활동 하셨나요?",
          "original_form": "동아리 활동 하셨나요?",
          "speaker_id": "1",
          "time": "20210830 08:46"
        },
        {
          "id": "MDRW2100035843.1.4",
          "form": "아뇨! 전 안했어요",
          "original_form": "아뇨! 전 안했어요",
          "speaker_id": "2",
          "time": "20210830 18:35"
        },
        {
          "id": "MDRW2100035843.1.5",
          "form": "동아리 하셨었나요?",
          "original_form": "동아리 하셨었나요?",
          "speaker_id": "2",
          "time": "20210830 18:35"
        },
        {
          "id": "MDRW2100035843.1.6",
          "form": "새내기때 배드민턴이나 볼링 조금 하다 그만뒀어요",
          "original_form": "새내기때 배드민턴이나 볼링 조금 하다 그만뒀어요",
          "speaker_id": "1",
          "time": "20210830 19:58"
        },
        {
          "id": "MDRW2100035843.1.7",
          "form": "아하ㅎㅎ운동 좋아하시나봐요",
          "original_form": "아하ㅎㅎ운동 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210831 19:02"
        },
        {
          "id": "MDRW2100035843.1.8",
          "form": "팀운동을 해보고싶었어요",
          "original_form": "팀운동을 해보고싶었어요",
          "speaker_id": "1",
          "time": "20210831 20:52"
        },
        {
          "id": "MDRW2100035843.1.9",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "2",
          "time": "20210901 08:52"
        },
        {
          "id": "MDRW2100035843.1.10",
          "form": "왜 조금 하다 그만뒀어요?",
          "original_form": "왜 조금 하다 그만뒀어요?",
          "speaker_id": "2",
          "time": "20210901 08:52"
        },
        {
          "id": "MDRW2100035843.1.11",
          "form": "술을 안좋아하는데 술을 너무많이 먹는 동아리여서요",
          "original_form": "술을 안좋아하는데 술을 너무많이 먹는 동아리여서요",
          "speaker_id": "1",
          "time": "20210901 08:53"
        },
        {
          "id": "MDRW2100035843.1.12",
          "form": "아 진짜 술 너무 많이 먹이죠ㅠ",
          "original_form": "아 진짜 술 너무 많이 먹이죠ㅠ",
          "speaker_id": "2",
          "time": "20210901 08:54"
        },
        {
          "id": "MDRW2100035843.1.13",
          "form": "그죠....그래도 지금생각해보면 술동아리아닌곳 찾아서 가볼걸 싶어요",
          "original_form": "그죠....그래도 지금생각해보면 술동아리아닌곳 찾아서 가볼걸 싶어요",
          "speaker_id": "1",
          "time": "20210901 08:57"
        },
        {
          "id": "MDRW2100035843.1.14",
          "form": "근데 비공식적으로도 술자리가 너무 많아요ㅠ",
          "original_form": "근데 비공식적으로도 술자리가 너무 많아요ㅠ",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100035843.1.15",
          "form": "맞아요...대학생활 내내 술때문에 힘들었어요",
          "original_form": "맞아요...대학생활 내내 술때문에 힘들었어요",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100035843.1.16",
          "form": "술없이 학교생활하면 안되는건지 참",
          "original_form": "술없이 학교생활하면 안되는건지 참",
          "speaker_id": "2",
          "time": "20210901 18:48"
        }
