      "utterance": [
        {
          "id": "MDRW2100026243.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100026243.1.2",
          "form": "연애 하세요?",
          "original_form": "연애 하세요?",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026243.1.3",
          "form": "네네 안녕하세요",
          "original_form": "네네 안녕하세요",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026243.1.4",
          "form": "지금연애중이예요",
          "original_form": "지금연애중이예요",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026243.1.5",
          "form": "와 얼마나 되셨어요?",
          "original_form": "와 얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026243.1.6",
          "form": "사귄지얼마안댓어요!",
          "original_form": "사귄지얼마안댓어요!",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026243.1.7",
          "form": "우와 대박 축하드려요~~",
          "original_form": "우와 대박 축하드려요~~",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026243.1.8",
          "form": "완전 제일 설레시겠다",
          "original_form": "완전 제일 설레시겠다",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026243.1.9",
          "form": "하지만 너무 어려워요 연애",
          "original_form": "하지만 너무 어려워요 연애",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026243.1.10",
          "form": "그쵸ㅠㅠ 진짜 연애가 제일 어려워요",
          "original_form": "그쵸ㅠㅠ 진짜 연애가 제일 어려워요",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026243.1.11",
          "form": "사람 마음을 읽을 수 있음 좋은데ㅠㅠ 그게 안되니...",
          "original_form": "사람 마음을 읽을 수 있음 좋은데ㅠㅠ 그게 안되니...",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026243.1.12",
          "form": "맞아요 진짜 너무어려워요..연애하시나요?",
          "original_form": "맞아요 진짜 너무어려워요..연애하시나요?",
          "speaker_id": "2",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100026243.1.13",
          "form": "아뇨ㅠㅠ 안해요 지금은",
          "original_form": "아뇨ㅠㅠ 안해요 지금은",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026243.1.14",
          "form": "사람 만나고 싶어도 인연이 없네요ㅠ",
          "original_form": "사람 만나고 싶어도 인연이 없네요ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026243.1.15",
          "form": "맞아죠 좋은데 어려워서 너무 힘드네요!",
          "original_form": "맞아죠 좋은데 어려워서 너무 힘드네요!",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026243.1.16",
          "form": "그쵸ㅠㅠ 코로나라 더 누구 만나기 어려워요!",
          "original_form": "그쵸ㅠㅠ 코로나라 더 누구 만나기 어려워요!",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026243.1.17",
          "form": "만날 장소도 딱히 모르겠고ㅠㅠ",
          "original_form": "만날 장소도 딱히 모르겠고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026243.1.18",
          "form": "맙아요 그래도 저는 혼자라도 놀려고 하는거같아요",
          "original_form": "맙아요 그래도 저는 혼자라도 놀려고 하는거같아요",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026243.1.19",
          "form": "애인분이랑 노셔야죵!",
          "original_form": "애인분이랑 노셔야죵!",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026243.1.20",
          "form": "애인분이랑 데이트 ㅎㅎ",
          "original_form": "애인분이랑 데이트 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026243.1.21",
          "form": "남자친구랑 놀기도 하는데",
          "original_form": "남자친구랑 놀기도 하는데",
          "speaker_id": "2",
          "time": "20210902 06:44"
        },
        {
          "id": "MDRW2100026243.1.22",
          "form": "일마치구의 여유는 혼자가 편안..ㅎ",
          "original_form": "일마치구의 여유는 혼자가 편안..ㅎ",
          "speaker_id": "2",
          "time": "20210902 06:44"
        },
        {
          "id": "MDRW2100026243.1.23",
          "form": "오오오 혼자만의 시간도 필요한 타입이구나",
          "original_form": "오오오 혼자만의 시간도 필요한 타입이구나",
          "speaker_id": "1",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100026243.1.24",
          "form": "남친이랑은 소개팅으로 만난 거야?",
          "original_form": "남친이랑은 소개팅으로 만난 거야?",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100026243.1.25",
          "form": "요?ㅋㅋㅋ 존댓말 했었네요 ㅋㅋㅋㅋ 헷갈려요 ㅋㅋㅋ",
          "original_form": "요?ㅋㅋㅋ 존댓말 했었네요 ㅋㅋㅋㅋ 헷갈려요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100026243.1.26",
          "form": "아니요 아니요원래 알던사이였어요",
          "original_form": "아니요 아니요원래 알던사이였어요",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100026243.1.27",
          "form": "헉 친구였다가 연인으로...? 완전 로망이에요 ㅋㅋㅋ",
          "original_form": "헉 친구였다가 연인으로...? 완전 로망이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:07"
        }
      ]
    }
