        },
        {
          "id": "MDRW2100029543.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029543.1.3",
          "form": "형제자매 있으세요?",
          "original_form": "형제자매 있으세요?",
          "speaker_id": "1",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029543.1.4",
          "form": "네 있습니다",
          "original_form": "네 있습니다",
          "speaker_id": "2",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029543.1.5",
          "form": "사이 좋아요?",
          "original_form": "사이 좋아요?",
          "speaker_id": "1",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029543.1.6",
          "form": "잘지내고있습니당ㅋㅋ",
          "original_form": "잘지내고있습니당ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029543.1.7",
          "form": "좋네요ㅎㅎ",
          "original_form": "좋네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:12"
        },
        {
          "id": "MDRW2100029543.1.8",
          "form": "하도 사이 안좋다고 하시는 분들이 많아서요",
          "original_form": "하도 사이 안좋다고 하시는 분들이 많아서요",
          "speaker_id": "1",
          "time": "20210903 10:12"
        },
        {
          "id": "MDRW2100029543.1.9",
          "form": "제친구들도 반반이더라구요ㅠㅠ",
          "original_form": "제친구들도 반반이더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 10:13"
        },
        {
          "id": "MDRW2100029543.1.10",
          "form": "사이좋으신가요?",
          "original_form": "사이좋으신가요?",
          "speaker_id": "2",
          "time": "20210903 10:14"
        },
        {
          "id": "MDRW2100029543.1.11",
          "form": "저는 엄청 좋아요",
          "original_form": "저는 엄청 좋아요",
          "speaker_id": "1",
          "time": "20210903 10:15"
        },
        {
          "id": "MDRW2100029543.1.12",
          "form": "거의 친구 사이ㅎㅎ",
          "original_form": "거의 친구 사이ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:15"
        },
        {
          "id": "MDRW2100029543.1.13",
          "form": "우와아 좋네요",
          "original_form": "우와아 좋네요",
          "speaker_id": "2",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100029543.1.14",
          "form": "하긴 저도 첫짼데 동생들이랑 그냥 친구처럼 살아요ㅋㅋ",
          "original_form": "하긴 저도 첫짼데 동생들이랑 그냥 친구처럼 살아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100029543.1.15",
          "form": "이름부르고ㅋㅋ",
          "original_form": "이름부르고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100029543.1.16",
          "form": "와 이름도 부르는 정도이시군요",
          "original_form": "와 이름도 부르는 정도이시군요",
          "speaker_id": "1",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100029543.1.17",
          "form": "오히려 나이드니까 형제사이가 더 좋아지더라구요",
          "original_form": "오히려 나이드니까 형제사이가 더 좋아지더라구요",
          "speaker_id": "2",
          "time": "20210903 10:19"
        },
        {
          "id": "MDRW2100029543.1.18",
          "form": "그런가요ㅎㅎ",
          "original_form": "그런가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:22"
        },
        {
          "id": "MDRW2100029543.1.19",
          "form": "어릴땐 별로였어서 그런가봐요ㅋㅋ",
          "original_form": "어릴땐 별로였어서 그런가봐요ㅋㅋ",
