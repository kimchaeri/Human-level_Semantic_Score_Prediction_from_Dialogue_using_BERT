      "utterance": [
        {
          "id": "MDRW2100015447.1.1",
          "form": "저희 대화 빨리 제출하네요 되게 빨리빨리 ㅋㅋ",
          "original_form": "저희 대화 빨리 제출하네요 되게 빨리빨리 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015447.1.2",
          "form": "오늘 하신 집안일 있으세요?",
          "original_form": "오늘 하신 집안일 있으세요?",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015447.1.3",
          "form": "집안일은 해도해도 끝이없죠 ㅋㅋㅋㅋㅋ",
          "original_form": "집안일은 해도해도 끝이없죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015447.1.4",
          "form": "오늘은 집안일 파업했어요",
          "original_form": "오늘은 집안일 파업했어요",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015447.1.5",
          "form": "너무피곤해서요ㅠ",
          "original_form": "너무피곤해서요ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015447.1.6",
          "form": "ㅋㅋㅋㅋㅋ파업할때도잇어야죠",
          "original_form": "ㅋㅋㅋㅋㅋ파업할때도잇어야죠",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015447.1.7",
          "form": "어떻게 365일 집안일을 합니까 ㅠㅠ",
          "original_form": "어떻게 365일 집안일을 합니까 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015447.1.8",
          "form": "그쵸ㅠㅠ 이번여름에 안그래도 더위약한데 너무 돌렸네요",
          "original_form": "그쵸ㅠㅠ 이번여름에 안그래도 더위약한데 너무 돌렸네요",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015447.1.9",
          "form": "가족들과 다함께 하시나요?",
          "original_form": "가족들과 다함께 하시나요?",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015447.1.10",
          "form": "혼자 살고 있어요 ㅋㅋㅋㅋ",
          "original_form": "혼자 살고 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015447.1.11",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015447.1.12",
          "form": "저희집은 어지르는 사람 셋 치우는 사람 저라서",
          "original_form": "저희집은 어지르는 사람 셋 치우는 사람 저라서",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015447.1.13",
          "form": "힘들어요 ㅋㅋ",
          "original_form": "힘들어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015447.1.14",
          "form": "아니 어지르는 사람 셋은 집안일 안하고 뭐합니까!!",
          "original_form": "아니 어지르는 사람 셋은 집안일 안하고 뭐합니까!!",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015447.1.15",
          "form": "분노 ㅋㅋㅋㅋ",
          "original_form": "분노 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015447.1.16",
          "form": "그니까요 분노",
          "original_form": "그니까요 분노",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015447.1.17",
          "form": "더위도 약한데 말이죠",
          "original_form": "더위도 약한데 말이죠",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015447.1.18",
          "form": "전 설거지가 젤 싫어요 name2님은요?",
          "original_form": "전 설거지가 젤 싫어요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015447.1.19",
          "form": "청소기 빨래는 괜찮아요",
          "original_form": "청소기 빨래는 괜찮아요",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015447.1.20",
          "form": "전 빨래가 제일 싫어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 빨래가 제일 싫어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015447.1.21",
          "form": "빨래개는걸 그렇게싫어해요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "빨래개는걸 그렇게싫어해요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015447.1.22",
          "form": "우와 정말 사람마다 다르구나",
          "original_form": "우와 정말 사람마다 다르구나",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015447.1.23",
          "form": "전빨래개는건 또 괜찮아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전빨래개는건 또 괜찮아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015447.1.24",
          "form": "아 너무 웃기다",
          "original_form": "아 너무 웃기다",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015447.1.25",
