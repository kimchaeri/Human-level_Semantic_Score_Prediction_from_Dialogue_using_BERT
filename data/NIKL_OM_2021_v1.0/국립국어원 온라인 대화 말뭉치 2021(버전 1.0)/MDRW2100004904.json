      "utterance": [
        {
          "id": "MDRW2100004904.1.1",
          "form": "좋아하는 프로그램 있으세요?",
          "original_form": "좋아하는 프로그램 있으세요?",
          "speaker_id": "1",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004904.1.2",
          "form": "TV는 요즘 잘 안 보고 유튜브 자주 봐요",
          "original_form": "TV는 요즘 잘 안 보고 유튜브 자주 봐요",
          "speaker_id": "2",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004904.1.3",
          "form": "오 어떤거 많이보세요?",
          "original_form": "오 어떤거 많이보세요?",
          "speaker_id": "1",
          "time": "20210821 19:33"
        },
        {
          "id": "MDRW2100004904.1.4",
          "form": "경제랑 일상 영상들 보는 거 같아요",
          "original_form": "경제랑 일상 영상들 보는 거 같아요",
          "speaker_id": "2",
          "time": "20210821 19:34"
        },
        {
          "id": "MDRW2100004904.1.5",
          "form": "저도 유튜브 즐겨보긴하는데",
          "original_form": "저도 유튜브 즐겨보긴하는데",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004904.1.6",
          "form": "너무 알고리즘 따라봐서 좀 줄이고있네요",
          "original_form": "너무 알고리즘 따라봐서 좀 줄이고있네요",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004904.1.7",
          "form": "저도 그래서 구독한 거 위주로 보고 있어요",
          "original_form": "저도 그래서 구독한 거 위주로 보고 있어요",
          "speaker_id": "2",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004904.1.8",
          "form": "알고리즘 떠라가면 정말 하루종일 유튜브만 볼 거 같아서요ㅋㅋㅋ",
          "original_form": "알고리즘 떠라가면 정말 하루종일 유튜브만 볼 거 같아서요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004904.1.9",
          "form": "ㅋㅋㅋㅋ제 경험담이네요",
          "original_form": "ㅋㅋㅋㅋ제 경험담이네요",
          "speaker_id": "1",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004904.1.10",
          "form": "구독위주로 보는거 좋은 방법인것같아요",
          "original_form": "구독위주로 보는거 좋은 방법인것같아요",
          "speaker_id": "1",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004904.1.11",
          "form": "저도 그렇개 해봐야겠어요",
          "original_form": "저도 그렇개 해봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 19:49"
        },
        {
          "id": "MDRW2100004904.1.12",
          "form": "넷플릭스나 왓챠같은 구독서비스는 안즐겨보시나요?",
          "original_form": "넷플릭스나 왓챠같은 구독서비스는 안즐겨보시나요?",
          "speaker_id": "1",
          "time": "20210821 19:49"
        },
        {
          "id": "MDRW2100004904.1.13",
          "form": "둘 다 몇 개월 구독했었는데 지금은 안 하고 있어요",
          "original_form": "둘 다 몇 개월 구독했었는데 지금은 안 하고 있어요",
          "speaker_id": "2",
          "time": "20210821 19:52"
        },
        {
          "id": "MDRW2100004904.1.14",
          "form": "최근에 본 것 중에는 스위트홈 재밌게 봤어요",
          "original_form": "최근에 본 것 중에는 스위트홈 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210821 19:52"
        },
        {
          "id": "MDRW2100004904.1.15",
          "form": "아 스위트홈 재밌죠",
          "original_form": "아 스위트홈 재밌죠",
          "speaker_id": "1",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100004904.1.16",
          "form": "저도 진짜 재밌게봤어요",
          "original_form": "저도 진짜 재밌게봤어요",
          "speaker_id": "1",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100004904.1.17",
          "form": "잘만들었더라고요",
          "original_form": "잘만들었더라고요",
          "speaker_id": "1",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100004904.1.18",
          "form": "재밌게 본 작품 있으세요?",
          "original_form": "재밌게 본 작품 있으세요?",
          "speaker_id": "2",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100004904.1.19",
          "form": "지금 슬의생 되게 재밌게 보고있어요",
          "original_form": "지금 슬의생 되게 재밌게 보고있어요",
          "speaker_id": "1",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100004904.1.20",
          "form": "가장 좋아하는 작품은 빨간머리앤이랑 굿플레이스고요",
          "original_form": "가장 좋아하는 작품은 빨간머리앤이랑 굿플레이스고요",
          "speaker_id": "1",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100004904.1.21",
          "form": "슬의생 다들 재밌게 보시더라고요",
          "original_form": "슬의생 다들 재밌게 보시더라고요",
          "speaker_id": "2",
          "time": "20210821 20:02"
        },
        {
          "id": "MDRW2100004904.1.22",
          "form": "지금 나오는게 시즌2인가요?",
          "original_form": "지금 나오는게 시즌2인가요?",
          "speaker_id": "2",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100004904.1.23",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100004904.1.24",
          "form": "다음주 결방이래서 너무 슬펐어요 ..",
          "original_form": "다음주 결방이래서 너무 슬펐어요 ..",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100004904.1.25",
          "form": "잔잔한 얘긴데 왜이렇게 재밌는지",
          "original_form": "잔잔한 얘긴데 왜이렇게 재밌는지",
          "speaker_id": "1",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100004904.1.26",
          "form": "저도 다음에 시즌 1부터 정주행 해야겠어요",
