      "utterance": [
        {
          "id": "MDRW2100034572.1.1",
          "form": "이제 춤도 좀 따라할 수 있게 된거 같아요 ㅋㅋㅋ",
          "original_form": "이제 춤도 좀 따라할 수 있게 된거 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100034572.1.2",
          "form": "정말 열심히 보셧나봐요 ㅋㅋㅋㅋㅋ",
          "original_form": "정말 열심히 보셧나봐요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100034572.1.3",
          "form": "전 몸치라서 춤은 전혀 못춥니다",
          "original_form": "전 몸치라서 춤은 전혀 못춥니다",
          "speaker_id": "2",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100034572.1.4",
          "form": "그 각진 안무가 되나 안되나 정도 ㅋㅋㅋㅋㅋ",
          "original_form": "그 각진 안무가 되나 안되나 정도 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100034572.1.5",
          "form": "어우 저 손아파서 이따가 다시 돌아올게요 ㅋㅋㅋㅋㅋ",
          "original_form": "어우 저 손아파서 이따가 다시 돌아올게요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100034572.1.6",
          "form": "너무 열심히했네요 오늘",
          "original_form": "너무 열심히했네요 오늘",
          "speaker_id": "1",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100034572.1.7",
          "form": "이따 봬요 ㅎㅎ",
          "original_form": "이따 봬요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100034572.1.8",
          "form": "지난주 대탈출을 보고 돌아왔어요 ㅋㅋㅋ",
          "original_form": "지난주 대탈출을 보고 돌아왔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100034572.1.9",
          "form": "재밋게 보셧나요 ㅋㅋㅋ",
          "original_form": "재밋게 보셧나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100034572.1.10",
          "form": "엄청 스케일이 크네요 ㅋㅋ세트장 만든거 자체가 너무 신기한 편이었어요 ㅋㅋ",
          "original_form": "엄청 스케일이 크네요 ㅋㅋ세트장 만든거 자체가 너무 신기한 편이었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100034572.1.11",
          "form": "대탈출이 스케일 되게 크다고 들엇어요",
          "original_form": "대탈출이 스케일 되게 크다고 들엇어요",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034572.1.12",
          "form": "영화에선 cg 처리될 것들이 여긴 진짜 세트장이니까 어마어마하네요",
          "original_form": "영화에선 cg 처리될 것들이 여긴 진짜 세트장이니까 어마어마하네요",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100034572.1.13",
          "form": "제작비가 많이들엇겟네요 ㅋㅋㅋㅋ",
          "original_form": "제작비가 많이들엇겟네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100034572.1.14",
          "form": "나영석보고 제작비 벌어오라 하잖아여 ㅋㅋㅋ",
          "original_form": "나영석보고 제작비 벌어오라 하잖아여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100034572.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ돈은 나영석이 벌고",
          "original_form": "ㅋㅋㅋㅋㅋㅋ돈은 나영석이 벌고",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100034572.1.16",
          "form": "ㅋㅋㅋㅋㅋ피디가 엄청 뿌듯할거같아여 프로그램 찍고나면",
          "original_form": "ㅋㅋㅋㅋㅋ피디가 엄청 뿌듯할거같아여 프로그램 찍고나면",
          "speaker_id": "1",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100034572.1.17",
          "form": "이것이 내가 만든 세트장이다...!!!",
          "original_form": "이것이 내가 만든 세트장이다...!!!",
          "speaker_id": "2",
          "time": "20210828 17:13"
        },
        {
          "id": "MDRW2100034572.1.18",
          "form": "이렇게요? ㅋㅋㅋㅋㅋ",
          "original_form": "이렇게요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:13"
        },
        {
          "id": "MDRW2100034572.1.19",
          "form": "네ㅋㅋㅋ 진짜 영화찍어도 되겠던데요",
          "original_form": "네ㅋㅋㅋ 진짜 영화찍어도 되겠던데요",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034572.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ대탈출 세트장은 만들고나서 도로 없애나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ대탈출 세트장은 만들고나서 도로 없애나요?",
          "speaker_id": "2",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100034572.1.21",
          "form": "잠깐 장소 대여해서 만드는 것 같더라구요",
          "original_form": "잠깐 장소 대여해서 만드는 것 같더라구요",
          "speaker_id": "1",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100034572.1.22",
          "form": "뭔가 박물관처럼 놔둬도 관광지될 것 같은데 ㅋㅋㅋ",
          "original_form": "뭔가 박물관처럼 놔둬도 관광지될 것 같은데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100034572.1.23",
          "form": "그러게요 아깝네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그러게요 아깝네요 ㅋㅋㅋㅋㅋㅋ",
