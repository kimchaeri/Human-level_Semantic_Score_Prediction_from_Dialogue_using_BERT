      "utterance": [
        {
          "id": "MDRW2100023583.1.1",
          "form": "군필이신가요?",
          "original_form": "군필이신가요?",
          "speaker_id": "1",
          "time": "20210830 18:25"
        },
        {
          "id": "MDRW2100023583.1.2",
          "form": "네ㅋㅋㅋ 군필입니다",
          "original_form": "네ㅋㅋㅋ 군필입니다",
          "speaker_id": "2",
          "time": "20210830 18:32"
        },
        {
          "id": "MDRW2100023583.1.3",
          "form": "복무중이세요?",
          "original_form": "복무중이세요?",
          "speaker_id": "2",
          "time": "20210830 18:33"
        },
        {
          "id": "MDRW2100023583.1.4",
          "form": "아니용ㅋㅋ\r\n저는 주부입니다만...ㅋㅋㅋ",
          "original_form": "아니용ㅋㅋ\r\n저는 주부입니다만...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100023583.1.5",
          "form": "육군이셨나요?",
          "original_form": "육군이셨나요?",
          "speaker_id": "1",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100023583.1.6",
          "form": "앗ㅋㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100023583.1.7",
          "form": "아뇨 해군이었어요",
          "original_form": "아뇨 해군이었어요",
          "speaker_id": "2",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100023583.1.8",
          "form": "평범한건 싫어서ㅋㅋ",
          "original_form": "평범한건 싫어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100023583.1.9",
          "form": "오 해군!! 멋지네요ㅋㅋ",
          "original_form": "오 해군!! 멋지네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023583.1.10",
          "form": "어디에 계셨었어요?",
          "original_form": "어디에 계셨었어요?",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023583.1.11",
          "form": "인천에 있었어요",
          "original_form": "인천에 있었어요",
          "speaker_id": "2",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100023583.1.12",
          "form": "대부분 다 밑으로 지원하더라구요",
          "original_form": "대부분 다 밑으로 지원하더라구요",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100023583.1.13",
          "form": "절반이 남쪽 친구들이라",
          "original_form": "절반이 남쪽 친구들이라",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100023583.1.14",
          "form": "오? 인천이요? ㅋㅋ\r\n인천 어디용?",
          "original_form": "오? 인천이요? ㅋㅋ\r\n인천 어디용?",
          "speaker_id": "1",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100023583.1.15",
          "form": "말해도 되나 모르겠네ㅋㅋㅋ",
          "original_form": "말해도 되나 모르겠네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100023583.1.16",
          "form": "인천해역방어사령부인데",
          "original_form": "인천해역방어사령부인데",
          "speaker_id": "2",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100023583.1.17",
          "form": "연안부두쪽에 있어요",
          "original_form": "연안부두쪽에 있어요",
          "speaker_id": "2",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100023583.1.18",
          "form": "연안부두 잘 알죠:)\r\n다시 돌아가고 싶지는 않지만\r\n그리운 시간들인가요? ㅋㅋ",
          "original_form": "연안부두 잘 알죠:)\r\n다시 돌아가고 싶지는 않지만\r\n그리운 시간들인가요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:59"
        },
        {
          "id": "MDRW2100023583.1.19",
          "form": "음.. 제가 군대 체질이 아니라ㅋㅋㅋ 그냥 한번쯤 갈만하다? 정도ㅋㅋ네요",
          "original_form": "음.. 제가 군대 체질이 아니라ㅋㅋㅋ 그냥 한번쯤 갈만하다? 정도ㅋㅋ네요",
          "speaker_id": "2",
          "time": "20210831 15:18"
        },
        {
          "id": "MDRW2100023583.1.20",
          "form": "ㅋㅋ군대체질인분들도 있나영?",
          "original_form": "ㅋㅋ군대체질인분들도 있나영?",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100023583.1.21",
          "form": "있어요 본인 입으로 말해요",
          "original_form": "있어요 본인 입으로 말해요",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100023583.1.22",
          "form": "이게 무지성으로 시키는거만 하면 되는데",
          "original_form": "이게 무지성으로 시키는거만 하면 되는데",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100023583.1.23",
          "form": "저는 언제나 why를 외치는 사람이더라구요",
          "original_form": "저는 언제나 why를 외치는 사람이더라구요",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100023583.1.24",
          "form": "까.라면 까.라는게 안됨",
          "original_form": "까.라면 까.라는게 안됨",
          "speaker_id": "2",
          "time": "20210831 22:42"
        }
