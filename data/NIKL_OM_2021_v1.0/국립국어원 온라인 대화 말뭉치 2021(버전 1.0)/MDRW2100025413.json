      "utterance": [
        {
          "id": "MDRW2100025413.1.1",
          "form": "그 놀뭐 김연경도 웃겼다곸ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그 놀뭐 김연경도 웃겼다곸ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025413.1.2",
          "form": "햇빛과 맞바꾼 낯빛",
          "original_form": "햇빛과 맞바꾼 낯빛",
          "speaker_id": "2",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025413.1.3",
          "form": "ㅋㅋㄱㅋㄱㅋㄱㅋㅋㄱㅋㅋㄱㅋㅋ 오 그건뭐야",
          "original_form": "ㅋㅋㄱㅋㄱㅋㄱㅋㅋㄱㅋㅋㄱㅋㅋ 오 그건뭐야",
          "speaker_id": "1",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025413.1.4",
          "form": "그건못본듯",
          "original_form": "그건못본듯",
          "speaker_id": "1",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025413.1.5",
          "form": "아 그거 진짜 웃긴데 잠만요",
          "original_form": "아 그거 진짜 웃긴데 잠만요",
          "speaker_id": "2",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025413.1.6",
          "form": "ㅋㅋㅋㄱㅋㄱㅋㅋㅋ 키워드알려주면 검색해보께",
          "original_form": "ㅋㅋㅋㄱㅋㄱㅋㅋㅋ 키워드알려주면 검색해보께",
          "speaker_id": "1",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025413.1.7",
          "form": "김연경 놀면뭐하니",
          "original_form": "김연경 놀면뭐하니",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025413.1.8",
          "form": "치면",
          "original_form": "치면",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025413.1.9",
          "form": "내리다보면 식빵귀신나와",
          "original_form": "내리다보면 식빵귀신나와",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025413.1.10",
          "form": "오 놀뭐에나왔어???",
          "original_form": "오 놀뭐에나왔어???",
          "speaker_id": "1",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025413.1.11",
          "form": "예전에",
          "original_form": "예전에",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025413.1.12",
          "form": "치킨만들때 잠깐 나왔었음",
          "original_form": "치킨만들때 잠깐 나왔었음",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025413.1.13",
          "form": "오 ㅋㅋㅋㅋㄱㅋㅋ 식빵귀신ㅋㅋㅋㄱㅋㅋ",
          "original_form": "오 ㅋㅋㅋㅋㄱㅋㅋ 식빵귀신ㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025413.1.14",
          "form": "찾아보껰ㅋㅋㄱㅋ",
          "original_form": "찾아보껰ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025413.1.15",
          "form": "그때 명수옹이랑 케미도 너무웃겼엌ㅋㅋ",
          "original_form": "그때 명수옹이랑 케미도 너무웃겼엌ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025413.1.16",
          "form": "오 명수옹이랑도 나왔구나",
          "original_form": "오 명수옹이랑도 나왔구나",
          "speaker_id": "1",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025413.1.17",
          "form": "ㅋㅋㄱㅋㄱㅋㅋㄱㄱㄱ 꼭봐야디",
          "original_form": "ㅋㅋㄱㅋㄱㅋㅋㄱㄱㄱ 꼭봐야디",
          "speaker_id": "1",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025413.1.18",
          "form": "그러고보니 이제놀뭐플러스 나온다며",
          "original_form": "그러고보니 이제놀뭐플러스 나온다며",
          "speaker_id": "1",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025413.1.19",
          "form": "약간 무도시즌2처럼",
          "original_form": "약간 무도시즌2처럼",
          "speaker_id": "1",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025413.1.20",
          "form": "그니갘ㅋㅋㅋ나는 너무좋음",
          "original_form": "그니갘ㅋㅋㅋ나는 너무좋음",
