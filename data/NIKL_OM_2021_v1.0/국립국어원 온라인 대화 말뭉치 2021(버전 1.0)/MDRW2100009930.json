      "utterance": [
        {
          "id": "MDRW2100009930.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100009930.1.2",
          "form": "안녕하세요 학생이신가요?",
          "original_form": "안녕하세요 학생이신가요?",
          "speaker_id": "2",
          "time": "20210823 11:53"
        },
        {
          "id": "MDRW2100009930.1.3",
          "form": "대학생입니다! 별님은요?",
          "original_form": "대학생입니다! 별님은요?",
          "speaker_id": "1",
          "time": "20210823 11:54"
        },
        {
          "id": "MDRW2100009930.1.4",
          "form": "저는 졸업했어요!",
          "original_form": "저는 졸업했어요!",
          "speaker_id": "2",
          "time": "20210823 11:56"
        },
        {
          "id": "MDRW2100009930.1.5",
          "form": "아직 방학이죠...?",
          "original_form": "아직 방학이죠...?",
          "speaker_id": "2",
          "time": "20210823 11:56"
        },
        {
          "id": "MDRW2100009930.1.6",
          "form": "네! 다음주 개강이에요",
          "original_form": "네! 다음주 개강이에요",
          "speaker_id": "1",
          "time": "20210823 11:57"
        },
        {
          "id": "MDRW2100009930.1.7",
          "form": "전 이번이 막학기..ㅎ",
          "original_form": "전 이번이 막학기..ㅎ",
          "speaker_id": "1",
          "time": "20210823 11:57"
        },
        {
          "id": "MDRW2100009930.1.8",
          "form": "그럼 수강신청하셨나요?",
          "original_form": "그럼 수강신청하셨나요?",
          "speaker_id": "2",
          "time": "20210823 11:58"
        },
        {
          "id": "MDRW2100009930.1.9",
          "form": "지금 수강신청 기간이에요 그래서 에타 완전 난리에요ㅎㅎ",
          "original_form": "지금 수강신청 기간이에요 그래서 에타 완전 난리에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 12:03"
        },
        {
          "id": "MDRW2100009930.1.10",
          "form": "곰무님은 잘 마무리하셨나요?",
          "original_form": "곰무님은 잘 마무리하셨나요?",
          "speaker_id": "2",
          "time": "20210823 12:05"
        },
        {
          "id": "MDRW2100009930.1.11",
          "form": "저는 한 과목만 수강하면 돼서 할게 없네요..",
          "original_form": "저는 한 과목만 수강하면 돼서 할게 없네요..",
          "speaker_id": "1",
          "time": "20210823 12:07"
        },
        {
          "id": "MDRW2100009930.1.12",
          "form": "그동안 성실하게 학점 쌓으셨군요",
          "original_form": "그동안 성실하게 학점 쌓으셨군요",
          "speaker_id": "2",
          "time": "20210823 12:08"
        },
        {
          "id": "MDRW2100009930.1.13",
          "form": "동아리활동은 하신거 있으세요?",
          "original_form": "동아리활동은 하신거 있으세요?",
          "speaker_id": "2",
          "time": "20210823 12:08"
        },
        {
          "id": "MDRW2100009930.1.14",
          "form": "아니요 별님은 동아리 활동 하셨나요?",
          "original_form": "아니요 별님은 동아리 활동 하셨나요?",
          "speaker_id": "1",
          "time": "20210823 12:12"
        },
        {
          "id": "MDRW2100009930.1.15",
          "form": "저는 새내기때 잠깐잠깐 발만 들여봤어요",
          "original_form": "저는 새내기때 잠깐잠깐 발만 들여봤어요",
