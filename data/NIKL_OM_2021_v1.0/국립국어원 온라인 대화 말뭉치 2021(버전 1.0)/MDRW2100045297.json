        },
        {
          "id": "MDRW2100045297.1.3",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045297.1.4",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045297.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045297.1.6",
          "form": "저도!",
          "original_form": "저도!",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045297.1.7",
          "form": "어디가 제일 좋으셨어오?",
          "original_form": "어디가 제일 좋으셨어오?",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045297.1.8",
          "form": "전 라오스요!",
          "original_form": "전 라오스요!",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045297.1.9",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045297.1.10",
          "form": "전 홍콩!",
          "original_form": "전 홍콩!",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045297.1.11",
          "form": "라오스 가보고싶네요",
          "original_form": "라오스 가보고싶네요",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045297.1.12",
          "form": "홍콩 안가봣어요",
          "original_form": "홍콩 안가봣어요",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045297.1.13",
          "form": "홍콩에서 뭐가 제일 좋으셧어요?",
          "original_form": "홍콩에서 뭐가 제일 좋으셧어요?",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045297.1.14",
          "form": "피크랑 음식이요!",
          "original_form": "피크랑 음식이요!",
          "speaker_id": "1",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045297.1.15",
          "form": "남산타워같지만ㅋㅋ",
          "original_form": "남산타워같지만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045297.1.16",
          "form": "어떤 음식이 맛잇엇어요?",
          "original_form": "어떤 음식이 맛잇엇어요?",
          "speaker_id": "2",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045297.1.17",
          "form": "딤섬이요!!",
          "original_form": "딤섬이요!!",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045297.1.18",
          "form": "라오스는요?",
          "original_form": "라오스는요?",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045297.1.19",
          "form": "라오스는 카오삐약이라고 쌀국수같은 국수가잇어요",
          "original_form": "라오스는 카오삐약이라고 쌀국수같은 국수가잇어요",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045297.1.20",
          "form": "아쌀국수 너무 좋아요!",
          "original_form": "아쌀국수 너무 좋아요!",
          "speaker_id": "1",
