      "utterance": [
        {
          "id": "MDRW2100039297.1.1",
          "form": "그 짤방 중에",
          "original_form": "그 짤방 중에",
          "speaker_id": "1",
          "time": "20210903 15:47"
        },
        {
          "id": "MDRW2100039297.1.2",
          "form": "고기와 밀가루를 멀리하면 오래 살 수 있지만",
          "original_form": "고기와 밀가루를 멀리하면 오래 살 수 있지만",
          "speaker_id": "1",
          "time": "20210903 15:48"
        },
        {
          "id": "MDRW2100039297.1.3",
          "form": "그렇다면 딱히 오래 살 이유가 없다는 말이 생각나요",
          "original_form": "그렇다면 딱히 오래 살 이유가 없다는 말이 생각나요",
          "speaker_id": "1",
          "time": "20210903 15:48"
        },
        {
          "id": "MDRW2100039297.1.4",
          "form": "아 그거 누가한 말이었죠",
          "original_form": "아 그거 누가한 말이었죠",
          "speaker_id": "2",
          "time": "20210903 16:00"
        },
        {
          "id": "MDRW2100039297.1.5",
          "form": "너무 존경스러운 분",
          "original_form": "너무 존경스러운 분",
          "speaker_id": "2",
          "time": "20210903 16:00"
        },
        {
          "id": "MDRW2100039297.1.6",
          "form": "아이돌이었던 것 같은데 라비였나",
          "original_form": "아이돌이었던 것 같은데 라비였나",
          "speaker_id": "1",
          "time": "20210903 16:01"
        },
        {
          "id": "MDRW2100039297.1.7",
          "form": "짤로만 봤어서 ㅋㅋ ㅋ",
          "original_form": "짤로만 봤어서 ㅋㅋ ㅋ",
          "speaker_id": "1",
          "time": "20210903 16:01"
        },
        {
          "id": "MDRW2100039297.1.8",
          "form": "건강관리는 해야하지만 역시 끊기 어려운 것들...",
          "original_form": "건강관리는 해야하지만 역시 끊기 어려운 것들...",
          "speaker_id": "1",
          "time": "20210903 16:01"
        },
        {
          "id": "MDRW2100039297.1.9",
          "form": "저는 그럼 짧고 굵게 가겠습니다",
          "original_form": "저는 그럼 짧고 굵게 가겠습니다",
          "speaker_id": "2",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100039297.1.10",
          "form": "꿀만 빨다 가고 싶어요",
          "original_form": "꿀만 빨다 가고 싶어요",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100039297.1.11",
          "form": "운동이라도 해야 하는데",
          "original_form": "운동이라도 해야 하는데",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100039297.1.12",
          "form": "홈트 안하세요?",
          "original_form": "홈트 안하세요?",
          "speaker_id": "2",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100039297.1.13",
          "form": "링피트 해요",
          "original_form": "링피트 해요",
          "speaker_id": "1",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100039297.1.14",
          "form": "근데 요즘은 집가면 와식생활 하고 있어요",
          "original_form": "근데 요즘은 집가면 와식생활 하고 있어요",
          "speaker_id": "1",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100039297.1.15",
          "form": "링피트 하시는군요",
          "original_form": "링피트 하시는군요",
          "speaker_id": "2",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100039297.1.16",
          "form": "남도형님이 링 성우라 해보고 싶은데",
          "original_form": "남도형님이 링 성우라 해보고 싶은데",
          "speaker_id": "2",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100039297.1.17",
          "form": "너무 비싸요ㅠ",
          "original_form": "너무 비싸요ㅠ",
          "speaker_id": "2",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100039297.1.18",
          "form": "스위치는 있으세요?",
          "original_form": "스위치는 있으세요?",
          "speaker_id": "1",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100039297.1.19",
          "form": "스위치도 없어요...ㅎㅎ",
          "original_form": "스위치도 없어요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100039297.1.20",
          "form": "링피트 하나 보고 스위치에 게임까지 사긴 부담스럽죠...",
          "original_form": "링피트 하나 보고 스위치에 게임까지 사긴 부담스럽죠...",
          "speaker_id": "1",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100039297.1.21",
          "form": "전 샀지만...",
          "original_form": "전 샀지만...",
          "speaker_id": "1",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100039297.1.22",
          "form": "구매하신 보람이 있나요",
          "original_form": "구매하신 보람이 있나요",
          "speaker_id": "2",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100039297.1.23",
          "form": "저 자신의 게으름을 깨닫는 계기가 되었어요",
          "original_form": "저 자신의 게으름을 깨닫는 계기가 되었어요",
          "speaker_id": "1",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100039297.1.24",
          "form": "시작이 귀찮아서 그렇지 하면 재밌긴 해요",
          "original_form": "시작이 귀찮아서 그렇지 하면 재밌긴 해요",
          "speaker_id": "1",
          "time": "20210903 16:17"
        },
        {
          "id": "MDRW2100039297.1.25",
          "form": "ㅋㅋ 링이 사람을 노예처럼 부려먹는다던데",
          "original_form": "ㅋㅋ 링이 사람을 노예처럼 부려먹는다던데",
          "speaker_id": "2",
          "time": "20210903 16:18"
        },
        {
          "id": "MDRW2100039297.1.26",
          "form": "링과 드래고 사이에서 새우등 터지는",
          "original_form": "링과 드래고 사이에서 새우등 터지는",
          "speaker_id": "1",
          "time": "20210903 16:19"
        },
        {
          "id": "MDRW2100039297.1.27",
          "form": "그런거죠",
          "original_form": "그런거죠",
          "speaker_id": "1",
          "time": "20210903 16:19"
        },
        {
          "id": "MDRW2100039297.1.28",
          "form": "ㅋㅋㅋㅋㅋ 새우였군요!",
          "original_form": "ㅋㅋㅋㅋㅋ 새우였군요!",
          "speaker_id": "2",
          "time": "20210903 16:20"
        }
      ]
    }
