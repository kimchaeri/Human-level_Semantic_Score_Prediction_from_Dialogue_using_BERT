      "utterance": [
        {
          "id": "MDRW2100020451.1.1",
          "form": "국낸 어디요?",
          "original_form": "국낸 어디요?",
          "speaker_id": "2",
          "time": "20210828 06:13"
        },
        {
          "id": "MDRW2100020451.1.2",
          "form": "저번에 여수 갔을 때 좋았는데.. 언제였지?",
          "original_form": "저번에 여수 갔을 때 좋았는데.. 언제였지?",
          "speaker_id": "1",
          "time": "20210828 06:14"
        },
        {
          "id": "MDRW2100020451.1.3",
          "form": "아 가족여행이오? 19년도에 갔죠 코로나 전해요",
          "original_form": "아 가족여행이오? 19년도에 갔죠 코로나 전해요",
          "speaker_id": "2",
          "time": "20210828 06:14"
        },
        {
          "id": "MDRW2100020451.1.4",
          "form": "그것도 마지막 국내여행이 되었구나",
          "original_form": "그것도 마지막 국내여행이 되었구나",
          "speaker_id": "1",
          "time": "20210828 06:14"
        },
        {
          "id": "MDRW2100020451.1.5",
          "form": "네 여수밤바다 진짜 종았는데요",
          "original_form": "네 여수밤바다 진짜 종았는데요",
          "speaker_id": "2",
          "time": "20210828 06:36"
        },
        {
          "id": "MDRW2100020451.1.6",
          "form": "그래 전망대도 올라가서 보고.. 케이블카도 타보고",
          "original_form": "그래 전망대도 올라가서 보고.. 케이블카도 타보고",
          "speaker_id": "1",
          "time": "20210828 06:36"
        },
        {
          "id": "MDRW2100020451.1.7",
          "form": "거긴 음식들도 맛있었잖아요",
          "original_form": "거긴 음식들도 맛있었잖아요",
          "speaker_id": "2",
          "time": "20210828 06:36"
        },
        {
          "id": "MDRW2100020451.1.8",
          "form": "그랬지.. 확실히 전라도쪽은 음식이 맛있더라",
          "original_form": "그랬지.. 확실히 전라도쪽은 음식이 맛있더라",
          "speaker_id": "1",
          "time": "20210828 06:36"
        },
        {
          "id": "MDRW2100020451.1.9",
          "form": "그때 잠깐 옆동네 갔는데 길건너 경상도 쪽은 확실히 달랐어요",
          "original_form": "그때 잠깐 옆동네 갔는데 길건너 경상도 쪽은 확실히 달랐어요",
          "speaker_id": "2",
          "time": "20210828 06:37"
        },
        {
          "id": "MDRW2100020451.1.10",
          "form": "길하나 차인데 어째서 그랬을까",
          "original_form": "길하나 차인데 어째서 그랬을까",
          "speaker_id": "1",
          "time": "20210828 06:37"
        },
        {
          "id": "MDRW2100020451.1.11",
          "form": "저도 궁금해요",
          "original_form": "저도 궁금해요",
          "speaker_id": "2",
          "time": "20210828 06:37"
        },
        {
          "id": "MDRW2100020451.1.12",
          "form": "다음 여행도 전라도로 가자",
          "original_form": "다음 여행도 전라도로 가자",
          "speaker_id": "1",
          "time": "20210828 06:38"
        },
        {
          "id": "MDRW2100020451.1.13",
          "form": "코로나 끝나고 꼭 여행가요",
          "original_form": "코로나 끝나고 꼭 여행가요",
          "speaker_id": "2",
          "time": "20210828 06:38"
        },
        {
          "id": "MDRW2100020451.1.14",
          "form": "경주 갔을때도 생각나요",
          "original_form": "경주 갔을때도 생각나요",
          "speaker_id": "2",
          "time": "20210828 06:38"
        }
