        {
          "id": "MDRW2100029504.1.2",
          "form": "습하죠?",
          "original_form": "습하죠?",
          "speaker_id": "1",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100029504.1.3",
          "form": "습도 적당해요!",
          "original_form": "습도 적당해요!",
          "speaker_id": "2",
          "time": "20210903 09:58"
        },
        {
          "id": "MDRW2100029504.1.4",
          "form": "보통 가을날 정도로 뽀송합니당",
          "original_form": "보통 가을날 정도로 뽀송합니당",
          "speaker_id": "2",
          "time": "20210903 09:59"
        },
        {
          "id": "MDRW2100029504.1.5",
          "form": "빨래하기가 좋겟네여",
          "original_form": "빨래하기가 좋겟네여",
          "speaker_id": "1",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100029504.1.6",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "2",
          "time": "20210903 10:05"
        },
        {
          "id": "MDRW2100029504.1.7",
          "form": "계신곳은 비오나요?",
          "original_form": "계신곳은 비오나요?",
          "speaker_id": "2",
          "time": "20210903 10:06"
        },
        {
          "id": "MDRW2100029504.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 10:12"
        },
        {
          "id": "MDRW2100029504.1.9",
          "form": "꿉꿉하시겠어요ㅜㅜ",
          "original_form": "꿉꿉하시겠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 10:14"
        },
        {
          "id": "MDRW2100029504.1.10",
          "form": "그래오",
          "original_form": "그래오",
          "speaker_id": "1",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100029504.1.11",
          "form": "비오면 신발 뭐신으세요?",
          "original_form": "비오면 신발 뭐신으세요?",
          "speaker_id": "2",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100029504.1.12",
          "form": "운동화여",
          "original_form": "운동화여",
          "speaker_id": "1",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100029504.1.13",
          "form": "아 저두요",
          "original_form": "아 저두요",
          "speaker_id": "2",
          "time": "20210903 10:21"
        },
        {
          "id": "MDRW2100029504.1.14",
          "form": "그게 짱이에요",
          "original_form": "그게 짱이에요",
          "speaker_id": "1",
          "time": "20210903 10:24"
        },
        {
          "id": "MDRW2100029504.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 10:25"
        },
        {
          "id": "MDRW2100029504.1.16",
          "form": "안젖구",
