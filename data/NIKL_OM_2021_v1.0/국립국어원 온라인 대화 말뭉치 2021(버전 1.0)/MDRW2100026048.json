      "utterance": [
        {
          "id": "MDRW2100026048.1.1",
          "form": "진정한 히키코모리가 되고있더라고요 ㅋㅋ",
          "original_form": "진정한 히키코모리가 되고있더라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:15"
        },
        {
          "id": "MDRW2100026048.1.2",
          "form": "그래서 힘들게 끊었던",
          "original_form": "그래서 힘들게 끊었던",
          "speaker_id": "1",
          "time": "20210901 19:15"
        },
        {
          "id": "MDRW2100026048.1.3",
          "form": "ㅠㅠ 대단해요 끊으셨었다니",
          "original_form": "ㅠㅠ 대단해요 끊으셨었다니",
          "speaker_id": "2",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100026048.1.4",
          "form": "근데 저는 다른분들이 본인들 반려동물 자랑하시는거 듣는거 좋아해서요 ㅋㅋ",
          "original_form": "근데 저는 다른분들이 본인들 반려동물 자랑하시는거 듣는거 좋아해서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100026048.1.5",
          "form": "파이프 애기 구조하는건 꼭봐야해요",
          "original_form": "파이프 애기 구조하는건 꼭봐야해요",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100026048.1.6",
          "form": "엄마냥이가 따라오라해요 ㄷㄷ",
          "original_form": "엄마냥이가 따라오라해요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100026048.1.7",
          "form": "제가 적격이네요 ㅋㅋㅋㅋ",
          "original_form": "제가 적격이네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100026048.1.8",
          "form": "더 막 자랑해주세요",
          "original_form": "더 막 자랑해주세요",
          "speaker_id": "1",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100026048.1.9",
          "form": "저는 너무 키우고싶은데",
          "original_form": "저는 너무 키우고싶은데",
          "speaker_id": "1",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100026048.1.10",
          "form": "제가 알레르기있어요",
          "original_form": "제가 알레르기있어요",
          "speaker_id": "1",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100026048.1.11",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:18"
        },
        {
          "id": "MDRW2100026048.1.12",
          "form": "ㅠㅠ 헐 그런 안타까운일이",
          "original_form": "ㅠㅠ 헐 그런 안타까운일이",
          "speaker_id": "2",
          "time": "20210901 19:18"
        },
        {
          "id": "MDRW2100026048.1.13",
          "form": "고양이는 진짜 우주 최곤데...",
          "original_form": "고양이는 진짜 우주 최곤데...",
          "speaker_id": "2",
          "time": "20210901 19:18"
        },
        {
          "id": "MDRW2100026048.1.14",
          "form": "사실 강쥐는 일주일도 안넘었는데 특유의 냄새가 있거든요",
          "original_form": "사실 강쥐는 일주일도 안넘었는데 특유의 냄새가 있거든요",
          "speaker_id": "2",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100026048.1.15",
          "form": "고양이는 냄새가 안나서 넘 좋아요",
          "original_form": "고양이는 냄새가 안나서 넘 좋아요",
          "speaker_id": "2",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100026048.1.16",
          "form": "몸에서 ㅋㅋ",
          "original_form": "몸에서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100026048.1.17",
          "form": "앗 그래요? 그런 그루밍 열심히하는 ㅋㅋ",
          "original_form": "앗 그래요? 그런 그루밍 열심히하는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100026048.1.18",
          "form": "둘이 친한가요?",
          "original_form": "둘이 친한가요?",
          "speaker_id": "1",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100026048.1.19",
          "form": "외동들은 가끔 잘몰라서 세게문다던데",
          "original_form": "외동들은 가끔 잘몰라서 세게문다던데",
          "speaker_id": "1",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100026048.1.20",
          "form": "같이키우면 그런건없다더라고요",
          "original_form": "같이키우면 그런건없다더라고요",
          "speaker_id": "1",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100026048.1.21",
          "form": "형제고 엄마가 가르쳤는지 괜찮더라구요",
          "original_form": "형제고 엄마가 가르쳤는지 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210901 19:21"
        },
        {
          "id": "MDRW2100026048.1.22",
          "form": "근데 첫째 고등어가 요즘 불만이 많아서 저를 쫌 물어요 ㅜㅜㅋ",
          "original_form": "근데 첫째 고등어가 요즘 불만이 많아서 저를 쫌 물어요 ㅜㅜㅋ",
          "speaker_id": "2",
          "time": "20210901 19:21"
        },
        {
          "id": "MDRW2100026048.1.23",
          "form": "그럼 진짜완벽한대요? ㅋㅋ",
          "original_form": "그럼 진짜완벽한대요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:21"
        },
        {
          "id": "MDRW2100026048.1.24",
          "form": "아니 뭐가 불만이시랍니까",
          "original_form": "아니 뭐가 불만이시랍니까",
          "speaker_id": "1",
          "time": "20210901 19:21"
        },
        {
          "id": "MDRW2100026048.1.25",
          "form": "많이 안놀아줘서 인거같아요 ㅋㅋㅋ",
          "original_form": "많이 안놀아줘서 인거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:22"
        },
        {
          "id": "MDRW2100026048.1.26",
          "form": "말도 진짜 많아요... 하루종일 냥냥 거리심",
          "original_form": "말도 진짜 많아요... 하루종일 냥냥 거리심",
          "speaker_id": "2",
          "time": "20210901 19:22"
        },
        {
          "id": "MDRW2100026048.1.27",
          "form": "ㅋㅋ딱활발하고 개구진 그런성격일거같아요",
          "original_form": "ㅋㅋ딱활발하고 개구진 그런성격일거같아요",
          "speaker_id": "1",
          "time": "20210901 19:23"
        },
        {
          "id": "MDRW2100026048.1.28",
          "form": "개냥이스탈인가요?",
          "original_form": "개냥이스탈인가요?",
          "speaker_id": "1",
          "time": "20210901 19:23"
        },
        {
          "id": "MDRW2100026048.1.29",
          "form": "저말고 다른 사람은 무서워하긴하는데 개냥이에요ㅎㅎ",
          "original_form": "저말고 다른 사람은 무서워하긴하는데 개냥이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 19:23"
        },
        {
          "id": "MDRW2100026048.1.30",
          "form": "집오면 마중나와서 드러누워요",
          "original_form": "집오면 마중나와서 드러누워요",
          "speaker_id": "2",
          "time": "20210901 19:23"
        },
        {
          "id": "MDRW2100026048.1.31",
          "form": "근데 둘째는 쫌 더 쫄보라 겁쟁이에요 ㅋㅋ 얘도 마중은 같이 나와요",
          "original_form": "근데 둘째는 쫌 더 쫄보라 겁쟁이에요 ㅋㅋ 얘도 마중은 같이 나와요",
          "speaker_id": "2",
          "time": "20210901 19:24"
        },
        {
          "id": "MDRW2100026048.1.32",
          "form": "근데 첫째 둘째는 어캐정하셨어요ㄱㄱㅋ",
          "original_form": "근데 첫째 둘째는 어캐정하셨어요ㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210901 19:24"
        },
        {
          "id": "MDRW2100026048.1.33",
          "form": "모든답은 옷장안에있다 ㅋㅋ나니아인데여 ㅋㅋ",
          "original_form": "모든답은 옷장안에있다 ㅋㅋ나니아인데여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:25"
        },
        {
          "id": "MDRW2100026048.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋ 사실 그럴 못봐서 너무 아쉬운데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 사실 그럴 못봐서 너무 아쉬운데",
          "speaker_id": "2",
          "time": "20210901 19:25"
        }
      ]
