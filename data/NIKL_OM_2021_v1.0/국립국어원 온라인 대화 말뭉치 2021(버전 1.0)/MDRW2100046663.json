      "utterance": [
        {
          "id": "MDRW2100046663.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211105 00:28"
        },
        {
          "id": "MDRW2100046663.1.2",
          "form": "식사중이신가요",
          "original_form": "식사중이신가요",
          "speaker_id": "1",
          "time": "20211105 00:28"
        },
        {
          "id": "MDRW2100046663.1.3",
          "form": "안녕하세요~ 저는 방금 다 먹었습니다 ㅎㅎ",
          "original_form": "안녕하세요~ 저는 방금 다 먹었습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 00:33"
        },
        {
          "id": "MDRW2100046663.1.4",
          "form": "뭐드셨나용",
          "original_form": "뭐드셨나용",
          "speaker_id": "1",
          "time": "20211105 00:33"
        },
        {
          "id": "MDRW2100046663.1.5",
          "form": "저는 청년다방 가서 오징어 떡볶이 먹었어요! 간만에 먹었더니 맛있네요",
          "original_form": "저는 청년다방 가서 오징어 떡볶이 먹었어요! 간만에 먹었더니 맛있네요",
          "speaker_id": "2",
          "time": "20211105 00:34"
        },
        {
          "id": "MDRW2100046663.1.6",
          "form": "오호 ㅜㅜ",
          "original_form": "오호 ㅜㅜ",
          "speaker_id": "1",
          "time": "20211105 00:34"
        },
        {
          "id": "MDRW2100046663.1.7",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20211105 00:34"
        },
        {
          "id": "MDRW2100046663.1.8",
          "form": "오징어떡볶이도 있군요",
          "original_form": "오징어떡볶이도 있군요",
          "speaker_id": "1",
          "time": "20211105 00:34"
        },
        {
          "id": "MDRW2100046663.1.9",
          "form": "네네 맛있더라구요 오징어 튀김 들어간 떡볶이!",
          "original_form": "네네 맛있더라구요 오징어 튀김 들어간 떡볶이!",
          "speaker_id": "2",
          "time": "20211105 00:34"
        },
        {
          "id": "MDRW2100046663.1.10",
          "form": "저는 편의점에서 라면 먹었네요",
          "original_form": "저는 편의점에서 라면 먹었네요",
          "speaker_id": "1",
          "time": "20211105 00:34"
        },
        {
          "id": "MDRW2100046663.1.11",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "1",
          "time": "20211105 00:34"
        },
        {
          "id": "MDRW2100046663.1.12",
          "form": "줄이면",
          "original_form": "줄이면",
          "speaker_id": "1",
          "time": "20211105 00:35"
        },
        {
          "id": "MDRW2100046663.1.13",
          "form": "뭐라고 부르죠?",
          "original_form": "뭐라고 부르죠?",
          "speaker_id": "1",
          "time": "20211105 00:35"
        },
        {
          "id": "MDRW2100046663.1.14",
          "form": "오떡인가",
          "original_form": "오떡인가",
          "speaker_id": "1",
          "time": "20211105 00:35"
        },
        {
          "id": "MDRW2100046663.1.15",
          "form": "음 딱히 줄여서 부르진 않았어요 ㅋㅋ",
          "original_form": "음 딱히 줄여서 부르진 않았어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 00:35"
        },
        {
          "id": "MDRW2100046663.1.16",
          "form": "줄이면 오떡이라고 할 수 있겠네여",
          "original_form": "줄이면 오떡이라고 할 수 있겠네여",
          "speaker_id": "2",
          "time": "20211105 00:35"
        },
        {
          "id": "MDRW2100046663.1.17",
          "form": "라면도 맛있었겠어요",
          "original_form": "라면도 맛있었겠어요",
          "speaker_id": "2",
          "time": "20211105 00:37"
        },
        {
          "id": "MDRW2100046663.1.18",
          "form": "날씨가 참 애매하네요",
          "original_form": "날씨가 참 애매하네요",
          "speaker_id": "1",
          "time": "20211105 00:37"
        },
        {
          "id": "MDRW2100046663.1.19",
          "form": "네 맛은있는데",
          "original_form": "네 맛은있는데",
          "speaker_id": "1",
          "time": "20211105 00:37"
        },
        {
          "id": "MDRW2100046663.1.20",
          "form": "건강엔 안좋은느낌",
          "original_form": "건강엔 안좋은느낌",
          "speaker_id": "1",
          "time": "20211105 00:37"
        },
        {
          "id": "MDRW2100046663.1.21",
          "form": "그래도 돈이 없으니 가성비로는 최고죠",
          "original_form": "그래도 돈이 없으니 가성비로는 최고죠",
          "speaker_id": "1",
          "time": "20211105 00:38"
        },
        {
          "id": "MDRW2100046663.1.22",
          "form": "음 맞아요 가성비 최고인 국물음식인데",
          "original_form": "음 맞아요 가성비 최고인 국물음식인데",
          "speaker_id": "2",
          "time": "20211105 00:39"
        },
        {
          "id": "MDRW2100046663.1.23",
          "form": "건강엔 좋다고 할 수가 없죠 ㅠ",
          "original_form": "건강엔 좋다고 할 수가 없죠 ㅠ",
          "speaker_id": "2",
          "time": "20211105 00:39"
        }
      ]
