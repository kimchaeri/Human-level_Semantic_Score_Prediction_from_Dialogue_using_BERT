      "utterance": [
        {
          "id": "MDRW2100008546.1.1",
          "form": "마카롱 맛있겠어요ㅠ",
          "original_form": "마카롱 맛있겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008546.1.2",
          "form": "간짜장이 더 먹고싶은데요ㅎ",
          "original_form": "간짜장이 더 먹고싶은데요ㅎ",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008546.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ서로 냠냠하는 건가요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ서로 냠냠하는 건가요",
          "speaker_id": "1",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008546.1.4",
          "form": "지금 늦은점심 먹고있거든요",
          "original_form": "지금 늦은점심 먹고있거든요",
          "speaker_id": "2",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008546.1.5",
          "form": "저도 3시쯤 일어나서 늦은 아점 먹었어요ㅋㅋㅋㅋ",
          "original_form": "저도 3시쯤 일어나서 늦은 아점 먹었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008546.1.6",
          "form": "어떤 거 드세요?",
          "original_form": "어떤 거 드세요?",
          "speaker_id": "1",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008546.1.7",
          "form": "제육볶음이랑 계란탕이요",
          "original_form": "제육볶음이랑 계란탕이요",
          "speaker_id": "2",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008546.1.8",
          "form": "김자반이랑ㅎ",
          "original_form": "김자반이랑ㅎ",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008546.1.9",
          "form": "어우 맛있겠네요ㅠㅠ",
          "original_form": "어우 맛있겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008546.1.10",
          "form": "뭐드셨어요?ㅎ",
          "original_form": "뭐드셨어요?ㅎ",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008546.1.11",
          "form": "저는 스팸김치볶음밥 먹었어요!!ㅎ",
          "original_form": "저는 스팸김치볶음밥 먹었어요!!ㅎ",
          "speaker_id": "1",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008546.1.12",
          "form": "오  맛있겠어요",
          "original_form": "오  맛있겠어요",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008546.1.13",
          "form": "직접요리해서요?",
          "original_form": "직접요리해서요?",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008546.1.14",
          "form": "네네 그냥 간단하게 햄 김치만 넣고 해여",
          "original_form": "네네 그냥 간단하게 햄 김치만 넣고 해여",
          "speaker_id": "1",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008546.1.15",
          "form": "스팸만 들어가도 맛있죠ㅎ",
          "original_form": "스팸만 들어가도 맛있죠ㅎ",
          "speaker_id": "2",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008546.1.16",
          "form": "김치까지 맛있음 더~ㅎ",
          "original_form": "김치까지 맛있음 더~ㅎ",
          "speaker_id": "2",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008546.1.17",
          "form": "그쵸 ㅎㅎ 한국인은 김치~",
          "original_form": "그쵸 ㅎㅎ 한국인은 김치~",
