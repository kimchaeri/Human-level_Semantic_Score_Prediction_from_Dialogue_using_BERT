      "utterance": [
        {
          "id": "MDRW2100030248.1.1",
          "form": "봉펑메밀면유명하죠",
          "original_form": "봉펑메밀면유명하죠",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030248.1.2",
          "form": "대신동에 있지요",
          "original_form": "대신동에 있지요",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030248.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030248.1.4",
          "form": "꿩만두랑요",
          "original_form": "꿩만두랑요",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030248.1.5",
          "form": "전에먹고",
          "original_form": "전에먹고",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030248.1.6",
          "form": "동네  맛집이지요",
          "original_form": "동네  맛집이지요",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030248.1.7",
          "form": "체하지않았나요?",
          "original_form": "체하지않았나요?",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030248.1.8",
          "form": "기억안나네요  존맛이어서",
          "original_form": "기억안나네요  존맛이어서",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030248.1.9",
          "form": "전기억나네요",
          "original_form": "전기억나네요",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030248.1.10",
          "form": "냉면도 좋아요",
          "original_form": "냉면도 좋아요",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030248.1.11",
          "form": "존맛인거 인정합니다",
          "original_form": "존맛인거 인정합니다",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030248.1.12",
          "form": "엎에 고기파는곳도 맛나요",
          "original_form": "엎에 고기파는곳도 맛나요",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030248.1.13",
          "form": "기억하지마세요",
          "original_form": "기억하지마세요",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030248.1.14",
          "form": "고기는 꽂분이네",
          "original_form": "고기는 꽂분이네",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030248.1.15",
          "form": "화화도 맛나요",
          "original_form": "화화도 맛나요",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100030248.1.16",
          "form": "대신동시장안에요",
          "original_form": "대신동시장안에요",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100030248.1.17",
          "form": "화화 그립네요",
