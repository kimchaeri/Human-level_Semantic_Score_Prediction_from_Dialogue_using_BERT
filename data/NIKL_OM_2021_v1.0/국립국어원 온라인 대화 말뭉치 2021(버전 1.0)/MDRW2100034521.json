      "utterance": [
        {
          "id": "MDRW2100034521.1.1",
          "form": "진짜 노벨상줘야합니다",
          "original_form": "진짜 노벨상줘야합니다",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100034521.1.2",
          "form": "위에 캬라멜라이징돼있어서 쫜-득한데 기절했어요",
          "original_form": "위에 캬라멜라이징돼있어서 쫜-득한데 기절했어요",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100034521.1.3",
          "form": "노벨상 쩝쩝박사부문 필요합니다",
          "original_form": "노벨상 쩝쩝박사부문 필요합니다",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100034521.1.4",
          "form": "먹어보지도 않앗는데 침흐르네요",
          "original_form": "먹어보지도 않앗는데 침흐르네요",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100034521.1.5",
          "form": "제가 주고싶을정도에요",
          "original_form": "제가 주고싶을정도에요",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100034521.1.6",
          "form": "안드시는 음식 있으세여?",
          "original_form": "안드시는 음식 있으세여?",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100034521.1.7",
          "form": "음 거의 없어요",
          "original_form": "음 거의 없어요",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100034521.1.8",
          "form": "아 홍어못먹어요",
          "original_form": "아 홍어못먹어요",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100034521.1.9",
          "form": "아 저두요 ㅋㅋㅋ",
          "original_form": "아 저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100034521.1.10",
          "form": "전 닭발 곱창 간 이런거 다 못 먹어요",
          "original_form": "전 닭발 곱창 간 이런거 다 못 먹어요",
          "speaker_id": "1",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100034521.1.11",
          "form": "순대 간도 못드세요?",
          "original_form": "순대 간도 못드세요?",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100034521.1.12",
          "form": "전 생간은 못먹어도 순대 간은 잘먹거든요 ㅋㅋㅋㅋ",
          "original_form": "전 생간은 못먹어도 순대 간은 잘먹거든요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100034521.1.13",
          "form": "못 먹어요 ㅋㅋㅋ ㅠㅠ",
          "original_form": "못 먹어요 ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100034521.1.14",
          "form": "그냥 순대만 먹어요 ㅋㅋㅋ",
          "original_form": "그냥 순대만 먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100034521.1.15",
          "form": "그래도 순대는 잘 드시나봐요",
          "original_form": "그래도 순대는 잘 드시나봐요",
          "speaker_id": "2",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100034521.1.16",
          "form": "순대도 못먹는 분들 있더라구요",
          "original_form": "순대도 못먹는 분들 있더라구요",
          "speaker_id": "2",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100034521.1.17",
          "form": "순대는 맛있어요",
          "original_form": "순대는 맛있어요",
          "speaker_id": "1",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100034521.1.18",
          "form": "떡볶이 국물에 딱..!!",
          "original_form": "떡볶이 국물에 딱..!!",
          "speaker_id": "1",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100034521.1.19",
          "form": "그렇죠 떡볶이 국물!!",
          "original_form": "그렇죠 떡볶이 국물!!",
          "speaker_id": "2",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100034521.1.20",
          "form": "튀김도 먹어줘야해요",
          "original_form": "튀김도 먹어줘야해요",
          "speaker_id": "2",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100034521.1.21",
          "form": "캬 최고죠",
          "original_form": "캬 최고죠",
          "speaker_id": "1",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100034521.1.22",
          "form": "당연합니다!!",
          "original_form": "당연합니다!!",
          "speaker_id": "1",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100034521.1.23",
          "form": "오징어튀김!",
          "original_form": "오징어튀김!",
          "speaker_id": "1",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100034521.1.24",
          "form": "크으 오징어튀김 받고 김말이!!",
          "original_form": "크으 오징어튀김 받고 김말이!!",
          "speaker_id": "2",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100034521.1.25",
          "form": "하 극공감합니다",
          "original_form": "하 극공감합니다",
          "speaker_id": "1",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100034521.1.26",
          "form": "바삭바삭 ㅜㅜ",
          "original_form": "바삭바삭 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100034521.1.27",
          "form": "야채튀김도 너무 맛잇죠",
          "original_form": "야채튀김도 너무 맛잇죠",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100034521.1.28",
