      "utterance": [
        {
          "id": "MDRW2100002184.1.1",
          "form": "최근에 구매한 물건 있으세오?!",
          "original_form": "최근에 구매한 물건 있으세오?!",
          "speaker_id": "2",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002184.1.2",
          "form": "저 뭐샀지 음",
          "original_form": "저 뭐샀지 음",
          "speaker_id": "1",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002184.1.3",
          "form": "아니 근데 제 책상을 생각도 못하고 잘못산거 있죠? ㅎㅎ..;",
          "original_form": "아니 근데 제 책상을 생각도 못하고 잘못산거 있죠? ㅎㅎ..;",
          "speaker_id": "1",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002184.1.4",
          "form": "그래서 그냥 책상 위에 놔두고 쓰고있어요ㅠ",
          "original_form": "그래서 그냥 책상 위에 놔두고 쓰고있어요ㅠ",
          "speaker_id": "1",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002184.1.5",
          "form": "재프리님은 최근에 뭐 산 거 있으신가요?",
          "original_form": "재프리님은 최근에 뭐 산 거 있으신가요?",
          "speaker_id": "1",
          "time": "20210820 00:26"
        },
        {
          "id": "MDRW2100002184.1.6",
          "form": "전 프린터 샀는데 저도 똑같이 약간 후회하고 있어요ㅋㅋㅋㅋㅋ!",
          "original_form": "전 프린터 샀는데 저도 똑같이 약간 후회하고 있어요ㅋㅋㅋㅋㅋ!",
          "speaker_id": "2",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002184.1.7",
          "form": "엌ㅋㅋㅋㅋ 크기가 너무 커서 자리차지가 큰가요??!",
          "original_form": "엌ㅋㅋㅋㅋ 크기가 너무 커서 자리차지가 큰가요??!",
          "speaker_id": "1",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002184.1.8",
          "form": "그렇기도 하고 너무 즉흥적으로 사버린ㅋㅋㅋㅋㅋㅋㅠ",
          "original_form": "그렇기도 하고 너무 즉흥적으로 사버린ㅋㅋㅋㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002184.1.9",
          "form": "할인을 하루만 해가지고 촉박해서 그냥 긁었더니 하하",
          "original_form": "할인을 하루만 해가지고 촉박해서 그냥 긁었더니 하하",
          "speaker_id": "2",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002184.1.10",
          "form": "앗.. 진짜 그럴때 충동적으로 구매하죠ㅠ ㅋㅋㅋㅋㅋ",
          "original_form": "앗.. 진짜 그럴때 충동적으로 구매하죠ㅠ ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002184.1.11",
          "form": "일단 급하니까 결제하고 생각하고 해놓고",
          "original_form": "일단 급하니까 결제하고 생각하고 해놓고",
          "speaker_id": "1",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002184.1.12",
          "form": "진짜 그래서 후회중입니다......",
          "original_form": "진짜 그래서 후회중입니다......",
          "speaker_id": "2",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002184.1.13",
          "form": "제가 한 방법을 그대로 말해주셨네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "제가 한 방법을 그대로 말해주셨네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:38"
        },
        {
          "id": "MDRW2100002184.1.14",
          "form": "막상오면 아.. 내가 왜그랬지?이러고 막 ㅠㅠ ㅋㅋㅋㅋㅋ 처치곤란 ㅠㅠ",
          "original_form": "막상오면 아.. 내가 왜그랬지?이러고 막 ㅠㅠ ㅋㅋㅋㅋㅋ 처치곤란 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 00:38"
        },
        {
          "id": "MDRW2100002184.1.15",
          "form": "ㅋㅋㅋㅋㅋ 쓸 일이 많을거에요....",
          "original_form": "ㅋㅋㅋㅋㅋ 쓸 일이 많을거에요....",
          "speaker_id": "1",
          "time": "20210820 00:38"
        },
        {
          "id": "MDRW2100002184.1.16",
          "form": "너무 안써버리면 당근이나 중고로 팔아버리는것도 답입니다!",
          "original_form": "너무 안써버리면 당근이나 중고로 팔아버리는것도 답입니다!",
          "speaker_id": "1",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002184.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ 그래야겠어요 좀 지켜보고 안돠면",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그래야겠어요 좀 지켜보고 안돠면",
          "speaker_id": "2",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002184.1.18",
          "form": "당근을 좀 이용해야죠 ㅎ",
          "original_form": "당근을 좀 이용해야죠 ㅎ",
          "speaker_id": "2",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002184.1.19",
          "form": "당근 진짜 편리하고 좋아요!",
          "original_form": "당근 진짜 편리하고 좋아요!",
          "speaker_id": "1",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002184.1.20",
          "form": "당근에 내보내기 전에 프린터가 열일하기를..!",
          "original_form": "당근에 내보내기 전에 프린터가 열일하기를..!",
          "speaker_id": "1",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002184.1.21",
          "form": "돈 쓴 만큼 꼭 사용을 해봐야죠 ㅎㅎ",
          "original_form": "돈 쓴 만큼 꼭 사용을 해봐야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002184.1.22",
          "form": "많은 것들을 뽑아봐야겠어요",
          "original_form": "많은 것들을 뽑아봐야겠어요",
          "speaker_id": "2",
