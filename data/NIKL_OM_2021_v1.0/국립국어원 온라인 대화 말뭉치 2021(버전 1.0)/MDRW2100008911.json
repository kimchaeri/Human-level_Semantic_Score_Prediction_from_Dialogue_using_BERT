      "utterance": [
        {
          "id": "MDRW2100008911.1.1",
          "form": "반려동물은 있으세요?",
          "original_form": "반려동물은 있으세요?",
          "speaker_id": "1",
          "time": "20210822 19:36"
        },
        {
          "id": "MDRW2100008911.1.2",
          "form": "아뇨 여건이 안되서 못키우네요ㅠㅠ",
          "original_form": "아뇨 여건이 안되서 못키우네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 19:44"
        },
        {
          "id": "MDRW2100008911.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 19:44"
        },
        {
          "id": "MDRW2100008911.1.4",
          "form": "저도요 ㅠㅠ",
          "original_form": "저도요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100008911.1.5",
          "form": "동물 정말 좋아하지만..",
          "original_form": "동물 정말 좋아하지만..",
          "speaker_id": "1",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100008911.1.6",
          "form": "책임이 너무 큰것같아서요",
          "original_form": "책임이 너무 큰것같아서요",
          "speaker_id": "1",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100008911.1.7",
          "form": "오 맞아요 .. 예전에 햄스터를 키운 적있는데",
          "original_form": "오 맞아요 .. 예전에 햄스터를 키운 적있는데",
          "speaker_id": "2",
          "time": "20210822 20:32"
        },
        {
          "id": "MDRW2100008911.1.8",
          "form": "동물 키우는게 쉽지 않은 일이더라구요ㅠ",
          "original_form": "동물 키우는게 쉽지 않은 일이더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210822 20:32"
        },
        {
          "id": "MDRW2100008911.1.9",
          "form": "햄스터 같은 작은 동물들이 은근히 더 힘들것같아요 ㅠㅠ",
          "original_form": "햄스터 같은 작은 동물들이 은근히 더 힘들것같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 20:41"
        },
        {
          "id": "MDRW2100008911.1.10",
          "form": "대단하세요 ㅠㅠ",
          "original_form": "대단하세요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 20:41"
        },
        {
          "id": "MDRW2100008911.1.11",
          "form": "맞아요 ㅠㅠ 아플때도 병원가기가참 그렇더라구요 ..",
          "original_form": "맞아요 ㅠㅠ 아플때도 병원가기가참 그렇더라구요 ..",
          "speaker_id": "2",
          "time": "20210822 21:03"
        },
        {
          "id": "MDRW2100008911.1.12",
          "form": "작은 동물이라서..",
          "original_form": "작은 동물이라서..",
          "speaker_id": "2",
          "time": "20210822 21:03"
        },
        {
          "id": "MDRW2100008911.1.13",
          "form": "아이고 ㅠㅠ 쪼꼬매서 잡고있기도 힘들겠어요",
          "original_form": "아이고 ㅠㅠ 쪼꼬매서 잡고있기도 힘들겠어요",
          "speaker_id": "1",
          "time": "20210822 21:07"
        },
        {
          "id": "MDRW2100008911.1.14",
          "form": "그래도 같이있으면 힐링됐죠?",
          "original_form": "그래도 같이있으면 힐링됐죠?",
          "speaker_id": "1",
          "time": "20210822 21:08"
        },
        {
          "id": "MDRW2100008911.1.15",
          "form": "네네 그럼요 제 손에 올라와있을때 너무 귀여웠어요ㅎㅎ",
          "original_form": "네네 그럼요 제 손에 올라와있을때 너무 귀여웠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 21:08"
        },
        {
          "id": "MDRW2100008911.1.16",
          "form": "아이구 ㅠㅠ 너무 사랑스럽겠어요",
          "original_form": "아이구 ㅠㅠ 너무 사랑스럽겠어요",
          "speaker_id": "1",
          "time": "20210822 21:25"
        },
        {
          "id": "MDRW2100008911.1.17",
          "form": "털이 어떤 색이었나요?",
          "original_form": "털이 어떤 색이었나요?",
          "speaker_id": "1",
          "time": "20210822 21:25"
        },
        {
          "id": "MDRW2100008911.1.18",
          "form": "펄햄스터여서 흰색털에 발바닥이 핑크색이었어요 ..!",
          "original_form": "펄햄스터여서 흰색털에 발바닥이 핑크색이었어요 ..!",
          "speaker_id": "2",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100008911.1.19",
          "form": "정말 귀여웠죠ㅎㅎ",
          "original_form": "정말 귀여웠죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100008911.1.20",
          "form": "ㅠㅠㅠ 이야기만 들어도 너무 사랑스러워요",
          "original_form": "ㅠㅠㅠ 이야기만 들어도 너무 사랑스러워요",
          "speaker_id": "1",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100008911.1.21",
          "form": "행복하셨겠어요",
          "original_form": "행복하셨겠어요",
          "speaker_id": "1",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100008911.1.22",
          "form": "맞아요 ..너무 귀여워서 바라만봐도 좋더라구요 ..",
          "original_form": "맞아요 ..너무 귀여워서 바라만봐도 좋더라구요 ..",
          "speaker_id": "2",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100008911.1.23",
          "form": "저도 그런 친구가 있었으면 좋겠내요..",
          "original_form": "저도 그런 친구가 있었으면 좋겠내요..",
