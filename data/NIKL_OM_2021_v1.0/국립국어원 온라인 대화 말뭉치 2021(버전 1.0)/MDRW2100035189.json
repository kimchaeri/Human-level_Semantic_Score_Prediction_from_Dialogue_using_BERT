      "utterance": [
        {
          "id": "MDRW2100035189.1.1",
          "form": "청소 좋아하세요?",
          "original_form": "청소 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 11:17"
        },
        {
          "id": "MDRW2100035189.1.2",
          "form": "아뇨.. 좋아하시나요?",
          "original_form": "아뇨.. 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100035189.1.3",
          "form": "방청소는 꽤 좋아해요",
          "original_form": "방청소는 꽤 좋아해요",
          "speaker_id": "1",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100035189.1.4",
          "form": "설거지는 싫지만요..ㅎㅎ",
          "original_form": "설거지는 싫지만요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100035189.1.5",
          "form": "우와!! 드디어 청소 좋아하는 사람을 만났네요~ 전 설거지는 안귀찮아 합니다^^",
          "original_form": "우와!! 드디어 청소 좋아하는 사람을 만났네요~ 전 설거지는 안귀찮아 합니다^^",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100035189.1.6",
          "form": "오오!!",
          "original_form": "오오!!",
          "speaker_id": "1",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100035189.1.7",
          "form": "저도 설거지 안귀찮으신분 처음 만났어요!!",
          "original_form": "저도 설거지 안귀찮으신분 처음 만났어요!!",
          "speaker_id": "1",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100035189.1.8",
          "form": "설거지하실때 장갑 끼시나요??",
          "original_form": "설거지하실때 장갑 끼시나요??",
          "speaker_id": "1",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100035189.1.9",
          "form": "진짜요? ㅋㅋ",
          "original_form": "진짜요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100035189.1.10",
          "form": "장갑 끼면 기름이 다 닦였나 아닌가 잘 몰라서 그냥 손으로 해요",
          "original_form": "장갑 끼면 기름이 다 닦였나 아닌가 잘 몰라서 그냥 손으로 해요",
          "speaker_id": "2",
          "time": "20210829 11:31"
        },
        {
          "id": "MDRW2100035189.1.11",
          "form": "네넹!",
          "original_form": "네넹!",
          "speaker_id": "1",
          "time": "20210829 11:51"
        },
        {
          "id": "MDRW2100035189.1.12",
          "form": "아 그쵸 장갑은 그게 단점이죠ㅠㅠ",
          "original_form": "아 그쵸 장갑은 그게 단점이죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 11:51"
        },
        {
          "id": "MDRW2100035189.1.13",
          "form": "저는 손끝 피부가 약해서인지 맨손으로하면 벗겨져서..흑",
          "original_form": "저는 손끝 피부가 약해서인지 맨손으로하면 벗겨져서..흑",
          "speaker_id": "1",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100035189.1.14",
          "form": "저도 벗겨지는데 맨손으로 해서인가보군요. 하지만 장갑끼고 하고 나중에 보면 기름이 그대로 ㅠ",
          "original_form": "저도 벗겨지는데 맨손으로 해서인가보군요. 하지만 장갑끼고 하고 나중에 보면 기름이 그대로 ㅠ",
          "speaker_id": "2",
          "time": "20210829 11:57"
        },
        {
          "id": "MDRW2100035189.1.15",
          "form": "아앗ㅠ",
          "original_form": "아앗ㅠ",
          "speaker_id": "1",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100035189.1.16",
          "form": "맞아요.. 그래서 저는 설거지와 친해지기 힘들어져요ㅠ",
          "original_form": "맞아요.. 그래서 저는 설거지와 친해지기 힘들어져요ㅠ",
          "speaker_id": "1",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100035189.1.17",
          "form": "식기세척기 사고싶어진달까요ㅋㅋ",
          "original_form": "식기세척기 사고싶어진달까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100035189.1.18",
          "form": "청소할때 요령이 있을까요. 전 시간만 엄청 걸리고 안깨끗해져요",
          "original_form": "청소할때 요령이 있을까요. 전 시간만 엄청 걸리고 안깨끗해져요",
          "speaker_id": "2",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100035189.1.19",
          "form": "전 일단 버려요!",
          "original_form": "전 일단 버려요!",
          "speaker_id": "1",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100035189.1.20",
          "form": "이게 뭐지? 쓸까? 음 안쓸거같은데.. 싶으면 버려요!",
          "original_form": "이게 뭐지? 쓸까? 음 안쓸거같은데.. 싶으면 버려요!",
          "speaker_id": "1",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100035189.1.21",
          "form": "아.. 제가 잘 못버리거든요. 이건 언젠간 필요할것 같은데 하면서요 ㅠ",
          "original_form": "아.. 제가 잘 못버리거든요. 이건 언젠간 필요할것 같은데 하면서요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 12:01"
        },
        {
          "id": "MDRW2100035189.1.22",
          "form": "아.. 공감가요.. 저도 그래서 못버리는것들이 구석에 있긴해요..",
          "original_form": "아.. 공감가요.. 저도 그래서 못버리는것들이 구석에 있긴해요..",
          "speaker_id": "1",
          "time": "20210829 12:05"
        },
        {
          "id": "MDRW2100035189.1.23",
          "form": "그래도 다음에도 거기 있음 버리려고해요",
          "original_form": "그래도 다음에도 거기 있음 버리려고해요",
          "speaker_id": "1",
          "time": "20210829 12:05"
        },
        {
          "id": "MDRW2100035189.1.24",
          "form": "아님 중고거래요!",
          "original_form": "아님 중고거래요!",
          "speaker_id": "1",
          "time": "20210829 12:05"
        }
      ]
