      "utterance": [
        {
          "id": "MDRW2100015400.1.1",
          "form": "종교가 있으신가요?",
          "original_form": "종교가 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015400.1.2",
          "form": "무교입니다",
          "original_form": "무교입니다",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015400.1.3",
          "form": "요즘 유교 단어를 아냐는 질문이 핫하던데",
          "original_form": "요즘 유교 단어를 아냐는 질문이 핫하던데",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015400.1.4",
          "form": "유교를 무교 반대말로 아는 사람들이 있더라구요",
          "original_form": "유교를 무교 반대말로 아는 사람들이 있더라구요",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015400.1.5",
          "form": "그게무슨말이에요 헝",
          "original_form": "그게무슨말이에요 헝",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015400.1.6",
          "form": "헤이헤이 유교걸",
          "original_form": "헤이헤이 유교걸",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015400.1.7",
          "form": "유교걸을 무교걸의 반대말로 안단말인가요?",
          "original_form": "유교걸을 무교걸의 반대말로 안단말인가요?",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015400.1.8",
          "form": "네 유교를 모른대요",
          "original_form": "네 유교를 모른대요",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015400.1.9",
          "form": "유교? 무교 반댓말 아니야? 이런ㄱ다고",
          "original_form": "유교? 무교 반댓말 아니야? 이런ㄱ다고",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015400.1.10",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015400.1.11",
          "form": "뭐 틀린말은 아니지만",
          "original_form": "뭐 틀린말은 아니지만",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015400.1.12",
          "form": "헤이헤이 유교걸의 유교인데ㅠㅠ",
          "original_form": "헤이헤이 유교걸의 유교인데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015400.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015400.1.14",
          "form": "종교 좋아하시나요",
          "original_form": "종교 좋아하시나요",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015400.1.15",
          "form": "교회 오빠 어때요",
          "original_form": "교회 오빠 어때요",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015400.1.16",
          "form": "유무야 반대말로 어디다가 붙이면 되지만 ㅠㅠ",
          "original_form": "유무야 반대말로 어디다가 붙이면 되지만 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015400.1.17",
          "form": "아 교회 오빠 좋아요 친구중에도 있는데",
          "original_form": "아 교회 오빠 좋아요 친구중에도 있는데",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015400.1.18",
          "form": "애가 너무 괜찮은데",
          "original_form": "애가 너무 괜찮은데",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015400.1.19",
          "form": "저는 무교가 좋아요",
          "original_form": "저는 무교가 좋아요",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015400.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015400.1.21",
          "form": "무교면서 사찰을 좋아하는 게 좋아요",
          "original_form": "무교면서 사찰을 좋아하는 게 좋아요",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015400.1.22",
          "form": "불교좋아요",
          "original_form": "불교좋아요",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015400.1.23",
          "form": "교회 오빠 친구 여자친구 교회 언니여야해요",
          "original_form": "교회 오빠 친구 여자친구 교회 언니여야해요",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015400.1.24",
          "form": "name2님도 그렇죠?",
          "original_form": "&name2&님도 그렇죠?",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015400.1.25",
          "form": "성당 언니도 안만나더라구요",
          "original_form": "성당 언니도 안만나더라구요",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015400.1.26",
          "form": "맞아요ㅠㅠㅠ 유유상종입니다",
          "original_form": "맞아요ㅠㅠㅠ 유유상종입니다",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015400.1.27",
          "form": "나두 절 좋아해요",
          "original_form": "나두 절 좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015400.1.28",
          "form": "아 다른 파라 그런가요",
          "original_form": "아 다른 파라 그런가요",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015400.1.29",
          "form": "절 좋아요 그쵸",
          "original_form": "절 좋아요 그쵸",
          "speaker_id": "1",
          "time": "20210825 23:00"
        }
      ]
