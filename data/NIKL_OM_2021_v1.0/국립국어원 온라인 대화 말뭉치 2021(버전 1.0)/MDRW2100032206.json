      "utterance": [
        {
          "id": "MDRW2100032206.1.1",
          "form": "잉글리쉬 머핀 입니다",
          "original_form": "잉글리쉬 머핀 입니다",
          "speaker_id": "2",
          "time": "20210905 14:02"
        },
        {
          "id": "MDRW2100032206.1.2",
          "form": "맥모닝 같은 동그란 빵",
          "original_form": "맥모닝 같은 동그란 빵",
          "speaker_id": "2",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100032206.1.3",
          "form": "커피랑 드신 건가요",
          "original_form": "커피랑 드신 건가요",
          "speaker_id": "1",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100032206.1.4",
          "form": "햄이랑 치즈랑 계란이 들어가 있어요",
          "original_form": "햄이랑 치즈랑 계란이 들어가 있어요",
          "speaker_id": "2",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100032206.1.5",
          "form": "아 그거 말씀하시는군요",
          "original_form": "아 그거 말씀하시는군요",
          "speaker_id": "1",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100032206.1.6",
          "form": "간단하게 먹기에는 좋은데 밥이 될 수는 없지요",
          "original_form": "간단하게 먹기에는 좋은데 밥이 될 수는 없지요",
          "speaker_id": "2",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100032206.1.7",
          "form": "정말 맛있지요 간단한 한끼 요리지요",
          "original_form": "정말 맛있지요 간단한 한끼 요리지요",
          "speaker_id": "1",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100032206.1.8",
          "form": "이걸로 배를 채우면 아쉽지요",
          "original_form": "이걸로 배를 채우면 아쉽지요",
          "speaker_id": "2",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100032206.1.9",
          "form": "고모가 요즘 토스트에 푹 빠졌다고 한 거 말씀드린 적 있나요",
          "original_form": "고모가 요즘 토스트에 푹 빠졌다고 한 거 말씀드린 적 있나요",
          "speaker_id": "1",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100032206.1.10",
          "form": "커피랑 먹고 있습니다 다 먹었네요",
          "original_form": "커피랑 먹고 있습니다 다 먹었네요",
          "speaker_id": "2",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100032206.1.11",
          "form": "계속 샌드위치 기계 사고 싶어 하세요",
          "original_form": "계속 샌드위치 기계 사고 싶어 하세요",
          "speaker_id": "1",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100032206.1.12",
          "form": "이삭토스트 말씀인가요",
          "original_form": "이삭토스트 말씀인가요",
          "speaker_id": "2",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100032206.1.13",
          "form": "샌드위치 기계를 사서 만들어 먹는다고 하더라도",
          "original_form": "샌드위치 기계를 사서 만들어 먹는다고 하더라도",
          "speaker_id": "1",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100032206.1.14",
          "form": "해밀턴 기계 말인가요",
          "original_form": "해밀턴 기계 말인가요",
          "speaker_id": "2",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100032206.1.15",
          "form": "이삭 이랑은 소스가 다른데 그 맛을 안 나오는데 어떻게 할까요",
          "original_form": "이삭 이랑은 소스가 다른데 그 맛을 안 나오는데 어떻게 할까요",
          "speaker_id": "1",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100032206.1.16",
          "form": "아니면 파니니 기계",
          "original_form": "아니면 파니니 기계",
          "speaker_id": "2",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100032206.1.17",
          "form": "아마 소스 팔 거예요",
          "original_form": "아마 소스 팔 거예요",
          "speaker_id": "2",
          "time": "20210905 14:04"
        }
      ]
