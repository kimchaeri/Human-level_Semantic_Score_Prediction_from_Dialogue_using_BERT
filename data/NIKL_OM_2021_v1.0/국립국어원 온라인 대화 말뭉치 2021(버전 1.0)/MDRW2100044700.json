      "utterance": [
        {
          "id": "MDRW2100044700.1.1",
          "form": "그 시대 남자들 육체적 사랑은 여자랑 하고",
          "original_form": "그 시대 남자들 육체적 사랑은 여자랑 하고",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044700.1.2",
          "form": "정신적 사랑은 남자랑하잖아",
          "original_form": "정신적 사랑은 남자랑하잖아",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044700.1.3",
          "form": "(ㅈㄴ",
          "original_form": "(ㅈㄴ",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044700.1.4",
          "form": "근데 어쩌면 그렇게 볼수도 있는거 아닐까",
          "original_form": "근데 어쩌면 그렇게 볼수도 있는거 아닐까",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044700.1.5",
          "form": "죠죠네 친구들은 언제나 여자가ㅠ많았자나",
          "original_form": "죠죠네 친구들은 언제나 여자가ㅠ많았자나",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044700.1.6",
          "form": "그래서 인셀남인 디오는",
          "original_form": "그래서 인셀남인 디오는",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044700.1.7",
          "form": "거기에 열등감을 가진 나머지 죠죠가에 집착한 것이 아닐까?",
          "original_form": "거기에 열등감을 가진 나머지 죠죠가에 집착한 것이 아닐까?",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044700.1.8",
          "form": "앗... 열등감 가진 건 맞지 개웃기다 갑작스럽게 그럴듯한 캐해석",
          "original_form": "앗... 열등감 가진 건 맞지 개웃기다 갑작스럽게 그럴듯한 캐해석",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044700.1.9",
          "form": "맞아 약간 질투와 열등감과 집착이 섞인......그런거지(ㅈㄴ",
          "original_form": "맞아 약간 질투와 열등감과 집착이 섞인......그런거지(ㅈㄴ",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044700.1.10",
          "form": "그저 그 많은 죠죠 중에",
          "original_form": "그저 그 많은 죠죠 중에",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044700.1.11",
          "form": "한명이라도",
          "original_form": "한명이라도",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044700.1.12",
          "form": "연애를 못하는 유전자가 있었다면",
          "original_form": "연애를 못하는 유전자가 있었다면",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044700.1.13",
          "form": "끝나는 악순환이었던거지",
          "original_form": "끝나는 악순환이었던거지",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044700.1.14",
          "form": "첨에두 걍 거의 그거 아냐?",
          "original_form": "첨에두 걍 거의 그거 아냐?",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044700.1.15",
          "form": "아노ㅏ 개어이없어",
          "original_form": "아노ㅏ 개어이없어",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044700.1.16",
          "form": "진짜 어이없어  ㅠㅠ ㅠㅠ ㅠ ㅠㅠ  ㅠㅇ응응ㅇ",
          "original_form": "진짜 어이없어  ㅠㅠ ㅠㅠ ㅠ ㅠㅠ  ㅠㅇ응응ㅇ",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044700.1.17",
          "form": "죠스타 여친 내거 하구 시펏는데...",
          "original_form": "죠스타 여친 내거 하구 시펏는데...",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044700.1.18",
          "form": "부들부들",
          "original_form": "부들부들",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044700.1.19",
          "form": "헐...맞아진짜완전그거임",
          "original_form": "헐...맞아진짜완전그거임",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044700.1.20",
          "form": "난...도둑 키스한다!",
          "original_form": "난...도둑 키스한다!",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044700.1.21",
          "form": "쭈왑쭈왑",
          "original_form": "쭈왑쭈왑",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044700.1.22",
          "form": "(뺨맞기(",
          "original_form": "(뺨맞기(",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044700.1.23",
          "form": "날 좋아해줄 여성은 없는것인가...",
          "original_form": "날 좋아해줄 여성은 없는것인가...",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044700.1.24",
          "form": "에리 나 : 셔츠 쫘아악",
          "original_form": "에리 나 : 셔츠 쫘아악",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044700.1.25",
          "form": "그럼 나는 인간을 포기하겠다 죠죠!",
          "original_form": "그럼 나는 인간을 포기하겠다 죠죠!",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044700.1.26",
          "form": "와....................... 갑자기 엄청 사회학적인 주제 되네",
          "original_form": "와....................... 갑자기 엄청 사회학적인 주제 되네",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044700.1.27",
          "form": "죠스타:헤에...",
          "original_form": "죠스타:헤에...",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044700.1.28",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044700.1.29",
          "form": "인셀남의 정신세계에 대해서..",
          "original_form": "인셀남의 정신세계에 대해서..",
