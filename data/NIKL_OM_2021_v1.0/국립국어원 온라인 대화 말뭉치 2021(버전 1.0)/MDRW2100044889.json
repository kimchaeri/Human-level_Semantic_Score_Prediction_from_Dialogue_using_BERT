      "utterance": [
        {
          "id": "MDRW2100044889.1.1",
          "form": "그 유니버셜이",
          "original_form": "그 유니버셜이",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044889.1.2",
          "form": "음반쪽도 하시는거 아시죠",
          "original_form": "음반쪽도 하시는거 아시죠",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044889.1.3",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044889.1.4",
          "form": "유니버셜 거기를 통해서",
          "original_form": "유니버셜 거기를 통해서",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044889.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ앨범도내신?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ앨범도내신?",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044889.1.6",
          "form": "팬텀싱어 (우리나라) 음감한테",
          "original_form": "팬텀싱어 (우리나라) 음감한테",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044889.1.7",
          "form": "....?",
          "original_form": "....?",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044889.1.8",
          "form": "연락옴ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "연락옴ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044889.1.9",
          "form": "음감한테서",
          "original_form": "음감한테서",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044889.1.10",
          "form": "곡을 꼭 받고싶다고ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "곡을 꼭 받고싶다고ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044889.1.11",
          "form": "진짜 제정신인가",
          "original_form": "진짜 제정신인가",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044889.1.12",
          "form": "정신이 혼미해",
          "original_form": "정신이 혼미해",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044889.1.13",
          "form": "이거 음감이 인라에서 직접 풀어준 얘기ㅇㅇ",
          "original_form": "이거 음감이 인라에서 직접 풀어준 얘기ㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044889.1.14",
          "form": "저승으로가!!",
          "original_form": "저승으로가!!",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044889.1.15",
          "form": "우승팀이",
          "original_form": "우승팀이",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044889.1.16",
          "form": "그지경임",
          "original_form": "그지경임",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044889.1.17",
          "form": "저승으로 가버려",
          "original_form": "저승으로 가버려",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044889.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저승가면 꼭 지옥가라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저승가면 꼭 지옥가라",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044889.1.19",
          "form": "처음에 그",
          "original_form": "처음에 그",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044889.1.20",
          "form": "아이구....",
          "original_form": "아이구....",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044889.1.21",
          "form": "가면 힘들겠다",
          "original_form": "가면 힘들겠다",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044889.1.22",
          "form": "해외에서 못보게 막아서",
          "original_form": "해외에서 못보게 막아서",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044889.1.23",
          "form": "중국이 원조인줄 안다는 얘기만 들었을얀",
          "original_form": "중국이 원조인줄 안다는 얘기만 들었을얀",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044889.1.24",
          "form": "땐",
          "original_form": "땐",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044889.1.25",
          "form": "몇억이넘는 중국인들만 바글바글할테니",
          "original_form": "몇억이넘는 중국인들만 바글바글할테니",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044889.1.26",
          "form": "저 썰 들으니까",
          "original_form": "저 썰 들으니까",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044889.1.27",
          "form": "....아득해지네여",
          "original_form": "....아득해지네여",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044889.1.28",
          "form": "그냥 웃음만...",
          "original_form": "그냥 웃음만...",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044889.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
