      "utterance": [
        {
          "id": "MDRW2100027314.1.1",
          "form": "나 시대물은 묘사가 어려워서 잘못봤응",
          "original_form": "나 시대물은 묘사가 어려워서 잘못봤응",
          "speaker_id": "2",
          "time": "20210902 11:23"
        },
        {
          "id": "MDRW2100027314.1.2",
          "form": "한권읽었당",
          "original_form": "한권읽었당",
          "speaker_id": "2",
          "time": "20210902 11:23"
        },
        {
          "id": "MDRW2100027314.1.3",
          "form": "현대물은 다읽음",
          "original_form": "현대물은 다읽음",
          "speaker_id": "2",
          "time": "20210902 11:23"
        },
        {
          "id": "MDRW2100027314.1.4",
          "form": "모방범 화차 낙원 스텝파더스텝은 책도있음",
          "original_form": "모방범 화차 낙원 스텝파더스텝은 책도있음",
          "speaker_id": "2",
          "time": "20210902 11:23"
        },
        {
          "id": "MDRW2100027314.1.5",
          "form": "난 평소 일드 많이봐서",
          "original_form": "난 평소 일드 많이봐서",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027314.1.6",
          "form": "거기나오는 단어들 익숙함",
          "original_form": "거기나오는 단어들 익숙함",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027314.1.7",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027314.1.8",
          "form": "도신이나 오캇피키나 파수막 나가야같은거",
          "original_form": "도신이나 오캇피키나 파수막 나가야같은거",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027314.1.9",
          "form": "미디어가 낳은 천재 ㅋ",
          "original_form": "미디어가 낳은 천재 ㅋ",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027314.1.10",
          "form": "엥 하나도몰게쏘ㅜ",
          "original_form": "엥 하나도몰게쏘ㅜ",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027314.1.11",
          "form": "궁금해서 나무위키도 찾아서 읽어보고했었음",
          "original_form": "궁금해서 나무위키도 찾아서 읽어보고했었음",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027314.1.12",
          "form": "와 열정보소",
          "original_form": "와 열정보소",
          "speaker_id": "2",
          "time": "20210902 11:27"
        },
        {
          "id": "MDRW2100027314.1.13",
          "form": "책은 주석이 잘되있어서좋음 좀모를만한단어에",
          "original_form": "책은 주석이 잘되있어서좋음 좀모를만한단어에",
          "speaker_id": "1",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100027314.1.14",
          "form": "드라마는 그걸영상으로볼수있어서좋고",
          "original_form": "드라마는 그걸영상으로볼수있어서좋고",
          "speaker_id": "1",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027314.1.15",
          "form": "ㅇㅇ 드라마는 책이설명하는거 딱보여주자너",
          "original_form": "ㅇㅇ 드라마는 책이설명하는거 딱보여주자너",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027314.1.16",
          "form": "책은 주석봐도모름",
          "original_form": "책은 주석봐도모름",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027314.1.17",
          "form": "난 드라마로 생김새를보고 주석읽으니까 찰떡같이이해감",
          "original_form": "난 드라마로 생김새를보고 주석읽으니까 찰떡같이이해감",
          "speaker_id": "1",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027314.1.18",
          "form": "요새 고전 수사물에 끌리고있음",
          "original_form": "요새 고전 수사물에 끌리고있음",
          "speaker_id": "1",
          "time": "20210902 12:16"
        },
        {
