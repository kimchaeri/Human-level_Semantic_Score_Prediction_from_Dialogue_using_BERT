      "utterance": [
        {
          "id": "MDRW2100046446.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211022 15:52"
        },
        {
          "id": "MDRW2100046446.1.2",
          "form": "안녕하세요! 선선한 오후네요!",
          "original_form": "안녕하세요! 선선한 오후네요!",
          "speaker_id": "2",
          "time": "20211022 15:52"
        },
        {
          "id": "MDRW2100046446.1.3",
          "form": "맞아요 날씨가 좋아요",
          "original_form": "맞아요 날씨가 좋아요",
          "speaker_id": "1",
          "time": "20211022 15:53"
        },
        {
          "id": "MDRW2100046446.1.4",
          "form": "이런날 놀러다녀야 하는데 코로나 때문에 제한적이라 슬프네요 ㅠ",
          "original_form": "이런날 놀러다녀야 하는데 코로나 때문에 제한적이라 슬프네요 ㅠ",
          "speaker_id": "2",
          "time": "20211022 15:53"
        },
        {
          "id": "MDRW2100046446.1.5",
          "form": "저도 너무 슬퍼요",
          "original_form": "저도 너무 슬퍼요",
          "speaker_id": "1",
          "time": "20211022 15:54"
        },
        {
          "id": "MDRW2100046446.1.6",
          "form": "코로나 끝나면 어디 여행 계획 있으신가요?",
          "original_form": "코로나 끝나면 어디 여행 계획 있으신가요?",
          "speaker_id": "2",
          "time": "20211022 15:56"
        },
        {
          "id": "MDRW2100046446.1.7",
          "form": "캐나다 가고 싶어요",
          "original_form": "캐나다 가고 싶어요",
          "speaker_id": "1",
          "time": "20211022 15:58"
        },
        {
          "id": "MDRW2100046446.1.8",
          "form": "헉 저도 전에 드라마 도깨비 보고 캐나다 가보고싶었는데!",
          "original_form": "헉 저도 전에 드라마 도깨비 보고 캐나다 가보고싶었는데!",
          "speaker_id": "2",
          "time": "20211022 15:58"
        },
        {
          "id": "MDRW2100046446.1.9",
          "form": "저도 가보진 못했어요",
          "original_form": "저도 가보진 못했어요",
          "speaker_id": "1",
          "time": "20211022 15:59"
        },
        {
          "id": "MDRW2100046446.1.10",
          "form": "캐나다에서 꼭 가보고싶은 곳이 있으신건가요?",
          "original_form": "캐나다에서 꼭 가보고싶은 곳이 있으신건가요?",
          "speaker_id": "2",
          "time": "20211022 16:01"
        },
        {
          "id": "MDRW2100046446.1.11",
          "form": "나이아가라  폭포요",
          "original_form": "나이아가라  폭포요",
          "speaker_id": "1",
          "time": "20211022 16:02"
        },
        {
          "id": "MDRW2100046446.1.12",
          "form": "캐나다는 자연이 예쁜것같아요",
          "original_form": "캐나다는 자연이 예쁜것같아요",
          "speaker_id": "2",
          "time": "20211022 16:04"
        },
        {
          "id": "MDRW2100046446.1.13",
          "form": "저는 퀘벡 가보고싶었거든요",
          "original_form": "저는 퀘벡 가보고싶었거든요",
          "speaker_id": "2",
          "time": "20211022 16:04"
        },
        {
          "id": "MDRW2100046446.1.14",
          "form": "언젠가 꼭 가야죠",
          "original_form": "언젠가 꼭 가야죠",
          "speaker_id": "1",
          "time": "20211022 16:04"
        },
        {
          "id": "MDRW2100046446.1.15",
          "form": "해외여행 자유로워지면 꼭 가야겠어요 ㅎㅎ",
          "original_form": "해외여행 자유로워지면 꼭 가야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211022 16:05"
        }
      ]
