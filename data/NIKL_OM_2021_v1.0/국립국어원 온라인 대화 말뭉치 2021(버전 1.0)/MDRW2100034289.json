      "utterance": [
        {
          "id": "MDRW2100034289.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100034289.1.2",
          "form": "하이여~~",
          "original_form": "하이여~~",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100034289.1.3",
          "form": "이젠 밤에는 선선하네요",
          "original_form": "이젠 밤에는 선선하네요",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100034289.1.4",
          "form": "비로니 선선보다 꿉꿉해요 ㅠㅠ",
          "original_form": "비로니 선선보다 꿉꿉해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100034289.1.5",
          "form": "아 습도가 높군요",
          "original_form": "아 습도가 높군요",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100034289.1.6",
          "form": "저도 어젯밤엔 습도가 높아 끈적거렸는데",
          "original_form": "저도 어젯밤엔 습도가 높아 끈적거렸는데",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100034289.1.7",
          "form": "네네 에어콘 껏다 켯다 반복 후 ㅜ",
          "original_form": "네네 에어콘 껏다 켯다 반복 후 ㅜ",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100034289.1.8",
          "form": "오늘은 괜찮아요",
          "original_form": "오늘은 괜찮아요",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100034289.1.9",
          "form": "다행이네여 ㅎㅎ 여긴  습도 넘높아요",
          "original_form": "다행이네여 ㅎㅎ 여긴  습도 넘높아요",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100034289.1.10",
          "form": "ㅠㅠ 습하면 기온 낮아도 선풍기나 에어컨 틀어야죠",
          "original_form": "ㅠㅠ 습하면 기온 낮아도 선풍기나 에어컨 틀어야죠",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100034289.1.11",
          "form": "네 ㅎㅎ 이제 에어컨 꺼야겟네요 춥 ㅜㅜㅋ",
          "original_form": "네 ㅎㅎ 이제 에어컨 꺼야겟네요 춥 ㅜㅜㅋ",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100034289.1.12",
          "form": "제습으로 하면 되지 않나요?ㅎㅎ",
          "original_form": "제습으로 하면 되지 않나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100034289.1.13",
          "form": "제습  그닥 모르겟더라구요 ㅎㅎ",
          "original_form": "제습  그닥 모르겟더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100034289.1.14",
          "form": "아 그다지 효과가 없으셨나봐요",
          "original_form": "아 그다지 효과가 없으셨나봐요",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100034289.1.15",
          "form": "내일도 비가 오려나",
          "original_form": "내일도 비가 오려나",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034289.1.16",
          "form": "내일은 구름만요 저희쪽은",
          "original_form": "내일은 구름만요 저희쪽은",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100034289.1.17",
          "form": "아 흐리기만 할 예정인가보네요",
          "original_form": "아 흐리기만 할 예정인가보네요",
