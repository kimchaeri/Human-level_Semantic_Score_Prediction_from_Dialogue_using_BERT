      "utterance": [
        {
          "id": "MDRW2100036123.1.1",
          "form": "군만두 물만두",
          "original_form": "군만두 물만두",
          "speaker_id": "2",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100036123.1.2",
          "form": "뭘 더 좋아하세요",
          "original_form": "뭘 더 좋아하세요",
          "speaker_id": "2",
          "time": "20210830 16:41"
        },
        {
          "id": "MDRW2100036123.1.3",
          "form": "전 둘다 좋아하는데 때에 따라 달라요 중국집 군만두는 싫구요",
          "original_form": "전 둘다 좋아하는데 때에 따라 달라요 중국집 군만두는 싫구요",
          "speaker_id": "1",
          "time": "20210830 16:43"
        },
        {
          "id": "MDRW2100036123.1.4",
          "form": "일본식 교자 굽는 스타일은 좋아해요",
          "original_form": "일본식 교자 굽는 스타일은 좋아해요",
          "speaker_id": "1",
          "time": "20210830 16:43"
        },
        {
          "id": "MDRW2100036123.1.5",
          "form": "일본식.. 잘 몰라서요..",
          "original_form": "일본식.. 잘 몰라서요..",
          "speaker_id": "2",
          "time": "20210830 17:14"
        },
        {
          "id": "MDRW2100036123.1.6",
          "form": "그럼 혹시",
          "original_form": "그럼 혹시",
          "speaker_id": "2",
          "time": "20210830 17:14"
        },
        {
          "id": "MDRW2100036123.1.7",
          "form": "감자떡만두 아세요?",
          "original_form": "감자떡만두 아세요?",
          "speaker_id": "2",
          "time": "20210830 17:14"
        },
        {
          "id": "MDRW2100036123.1.8",
          "form": "감자전분피로 만든 만두요? 알아요",
          "original_form": "감자전분피로 만든 만두요? 알아요",
          "speaker_id": "1",
          "time": "20210830 17:16"
        },
        {
          "id": "MDRW2100036123.1.9",
          "form": "좋아하시나요? 전 살짝 밀가루 파 입니다 ㅋㅋ",
          "original_form": "좋아하시나요? 전 살짝 밀가루 파 입니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100036123.1.10",
          "form": "네.. 그거 매콤한버전",
          "original_form": "네.. 그거 매콤한버전",
          "speaker_id": "2",
          "time": "20210830 17:18"
        },
        {
          "id": "MDRW2100036123.1.11",
          "form": "진짜 맛있오요",
          "original_form": "진짜 맛있오요",
          "speaker_id": "2",
          "time": "20210830 17:18"
        },
        {
          "id": "MDRW2100036123.1.12",
          "form": "쫄깃한 전분피인거죠? 한 번 도전해볼께요",
          "original_form": "쫄깃한 전분피인거죠? 한 번 도전해볼께요",
          "speaker_id": "1",
          "time": "20210830 17:19"
        },
        {
          "id": "MDRW2100036123.1.13",
          "form": "알려주셔서 감사해요",
          "original_form": "알려주셔서 감사해요",
          "speaker_id": "1",
          "time": "20210830 17:19"
        },
        {
          "id": "MDRW2100036123.1.14",
          "form": "네.. 맛있으니까 드셔보세요 ㅣ",
          "original_form": "네.. 맛있으니까 드셔보세요 ㅣ",
          "speaker_id": "2",
          "time": "20210830 17:20"
        },
        {
          "id": "MDRW2100036123.1.15",
          "form": "전 마트에서 요즘 못찾아서 슬펐거든요ㅋㅋ",
          "original_form": "전 마트에서 요즘 못찾아서 슬펐거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:21"
        },
        {
          "id": "MDRW2100036123.1.16",
          "form": "감자떡이 좀 옛날에 유행했던 기억이,, 온라인으로 사드셔야겠네요 ㅠ",
          "original_form": "감자떡이 좀 옛날에 유행했던 기억이,, 온라인으로 사드셔야겠네요 ㅠ",
          "speaker_id": "1",
          "time": "20210830 17:24"
        },
        {
          "id": "MDRW2100036123.1.17",
          "form": "냉동식품 온라인배송 편하긴한뎁  배송비가 아무래도 좀,,",
          "original_form": "냉동식품 온라인배송 편하긴한뎁  배송비가 아무래도 좀,,",
          "speaker_id": "1",
          "time": "20210830 17:24"
        },
        {
          "id": "MDRW2100036123.1.18",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 19:10"
        },
        {
          "id": "MDRW2100036123.1.19",
          "form": "배송비.....",
          "original_form": "배송비.....",
          "speaker_id": "2",
          "time": "20210830 19:10"
        },
        {
          "id": "MDRW2100036123.1.20",
          "form": "그래서 저 쿠팡을 애용해요",
          "original_form": "그래서 저 쿠팡을 애용해요",
          "speaker_id": "2",
          "time": "20210830 19:10"
        },
        {
          "id": "MDRW2100036123.1.21",
          "form": "전 그 때 마다 하나씩 비교해서 사요 ㅋㅋ",
          "original_form": "전 그 때 마다 하나씩 비교해서 사요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100036123.1.22",
          "form": "쇼핑난민입니다",
          "original_form": "쇼핑난민입니다",
          "speaker_id": "1",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100036123.1.23",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036123.1.24",
          "form": "ㅋㅋㅋㅋ그게 가성비 최고져!",
          "original_form": "ㅋㅋㅋㅋ그게 가성비 최고져!",
          "speaker_id": "2",
          "time": "20210830 22:30"
        },
        {
          "id": "MDRW2100036123.1.25",
          "form": "네이버 페이가 적립되는 네이버 플러스는 혹시 하시나요?",
          "original_form": "네이버 페이가 적립되는 네이버 플러스는 혹시 하시나요?",
          "speaker_id": "1",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100036123.1.26",
          "form": "엇 잘은 안하능데 해요",
          "original_form": "엇 잘은 안하능데 해요",
          "speaker_id": "2",
          "time": "20210831 13:53"
        },
        {
          "id": "MDRW2100036123.1.27",
          "form": "그거 오늘까지 이벤트죠?",
          "original_form": "그거 오늘까지 이벤트죠?",
          "speaker_id": "2",
          "time": "20210831 13:53"
        },
        {
          "id": "MDRW2100036123.1.28",
          "form": "이벤트는 잘 몰라요. 쿠팡 무료배송. 지마켓 스마일배송. 네이버페이회원? 이렇게 세개 고민하다",
          "original_form": "이벤트는 잘 몰라요. 쿠팡 무료배송. 지마켓 스마일배송. 네이버페이회원? 이렇게 세개 고민하다",
          "speaker_id": "1",
          "time": "20210831 16:16"
        },
        {
          "id": "MDRW2100036123.1.29",
          "form": "아직 하나도 신청안했어요",
          "original_form": "아직 하나도 신청안했어요",
          "speaker_id": "1",
          "time": "20210831 16:16"
        },
        {
          "id": "MDRW2100036123.1.30",
          "form": "식재료를 한군데서만 사는게 아니다보니 고르기애매해요",
          "original_form": "식재료를 한군데서만 사는게 아니다보니 고르기애매해요",
          "speaker_id": "1",
          "time": "20210831 16:16"
        }
      ]
