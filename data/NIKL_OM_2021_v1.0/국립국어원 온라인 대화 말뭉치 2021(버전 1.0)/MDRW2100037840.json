      "utterance": [
        {
          "id": "MDRW2100037840.1.1",
          "form": "운동 하시는 거 있으세요?",
          "original_form": "운동 하시는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100037840.1.2",
          "form": "저는 홈트해요!",
          "original_form": "저는 홈트해요!",
          "speaker_id": "2",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100037840.1.3",
          "form": "운동 뭐하세요?",
          "original_form": "운동 뭐하세요?",
          "speaker_id": "2",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100037840.1.4",
          "form": "저 이번주에 클라이밍 가요!",
          "original_form": "저 이번주에 클라이밍 가요!",
          "speaker_id": "1",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100037840.1.5",
          "form": "오 클라이밍 자주하세요?",
          "original_form": "오 클라이밍 자주하세요?",
          "speaker_id": "2",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100037840.1.6",
          "form": "코로나 이후엔 처음이에요 ㅋㅋ",
          "original_form": "코로나 이후엔 처음이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100037840.1.7",
          "form": "원래 클라이밍을 해보셧나보네요!",
          "original_form": "원래 클라이밍을 해보셧나보네요!",
          "speaker_id": "2",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100037840.1.8",
          "form": "몇번 해봤었어요! 재밌고 \r\n다른 사람이랑 접촉하지도 않아서 좋아요!",
          "original_form": "몇번 해봤었어요! 재밌고 \r\n다른 사람이랑 접촉하지도 않아서 좋아요!",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100037840.1.9",
          "form": "오오 그렇군요 그 높이 올라갈때 안무서우세요?",
          "original_form": "오오 그렇군요 그 높이 올라갈때 안무서우세요?",
          "speaker_id": "2",
          "time": "20210901 19:31"
        },
        {
          "id": "MDRW2100037840.1.10",
          "form": "저는 무서워서 낮은데로만 다녔어요 ㅋㅋㅋㅋ",
          "original_form": "저는 무서워서 낮은데로만 다녔어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:31"
        },
        {
          "id": "MDRW2100037840.1.11",
          "form": "사실 떨어져도 바닥 푹신해서 괜찮은데..",
          "original_form": "사실 떨어져도 바닥 푹신해서 괜찮은데..",
          "speaker_id": "1",
          "time": "20210901 19:31"
        },
        {
          "id": "MDRW2100037840.1.12",
          "form": "아그래요? 그래도몬가 무서울거같아요 ㅋㅋㅋㅋ",
          "original_form": "아그래요? 그래도몬가 무서울거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100037840.1.13",
          "form": "저두 괜히 무서워요 ㅋㅋㅋㅋ",
          "original_form": "저두 괜히 무서워요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100037840.1.14",
          "form": "클라이밍은 횟수로 가격받나요??",
          "original_form": "클라이밍은 횟수로 가격받나요??",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100037840.1.15",
          "form": "월 요금으로 내거나",
          "original_form": "월 요금으로 내거나",
          "speaker_id": "1",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100037840.1.16",
          "form": "횟수로 내거나 하는거같아요!",
          "original_form": "횟수로 내거나 하는거같아요!",
          "speaker_id": "1",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100037840.1.17",
          "form": "회당 2-3만ㄴ원정도인듯해요",
          "original_form": "회당 2-3만ㄴ원정도인듯해요",
          "speaker_id": "1",
