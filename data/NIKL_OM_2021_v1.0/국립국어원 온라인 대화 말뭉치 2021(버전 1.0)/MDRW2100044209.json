      "utterance": [
        {
          "id": "MDRW2100044209.1.1",
          "form": "네 초심자의 행운으로 따기도 한대요",
          "original_form": "네 초심자의 행운으로 따기도 한대요",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044209.1.2",
          "form": "같이 갔던 분들도 10만원 들고갔는데",
          "original_form": "같이 갔던 분들도 10만원 들고갔는데",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044209.1.3",
          "form": "땄대요?",
          "original_form": "땄대요?",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044209.1.4",
          "form": "한두번씩은 땄다고 하더라구요",
          "original_form": "한두번씩은 땄다고 하더라구요",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044209.1.5",
          "form": "한두번 땄을때 그만 해야하는군요",
          "original_form": "한두번 땄을때 그만 해야하는군요",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044209.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044209.1.7",
          "form": "근데 보통 처음에 2~3천원 걸고 한게 몇만원이 되니까",
          "original_form": "근데 보통 처음에 2~3천원 걸고 한게 몇만원이 되니까",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044209.1.8",
          "form": "하지만 그렇게 안되겠죠?",
          "original_form": "하지만 그렇게 안되겠죠?",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044209.1.9",
          "form": "멈추는게 안된대요",
          "original_form": "멈추는게 안된대요",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044209.1.10",
          "form": "아!!",
          "original_form": "아!!",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044209.1.11",
          "form": "그렇게 소액으로도 하는군요",
          "original_form": "그렇게 소액으로도 하는군요",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044209.1.12",
          "form": "게임 규칙을 모르는 상태로 가도 다 할 수 있나요?",
          "original_form": "게임 규칙을 모르는 상태로 가도 다 할 수 있나요?",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044209.1.13",
          "form": "오히려 규칙 알아야하는 카드게임들은",
          "original_form": "오히려 규칙 알아야하는 카드게임들은",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044209.1.14",
          "form": "거기에 오래 있는? 사람들이 테이블을 안떠나서 잘 못하고",
          "original_form": "거기에 오래 있는? 사람들이 테이블을 안떠나서 잘 못하고",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044209.1.15",
          "form": "구경 온 사람들은 보통",
          "original_form": "구경 온 사람들은 보통",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044209.1.16",
          "form": "레버만 당기면 되는 룰렛이나",
          "original_form": "레버만 당기면 되는 룰렛이나",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044209.1.17",
          "form": "구슬에 돈거는? 게임 한대요",
          "original_form": "구슬에 돈거는? 게임 한대요",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044209.1.18",
          "form": "아... 오래 한 사람들만 할 수 있군요",
          "original_form": "아... 오래 한 사람들만 할 수 있군요",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044209.1.19",
          "form": "우린 확률게임만~",
          "original_form": "우린 확률게임만~",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044209.1.20",
          "form": "전 운이 없는 편인데요",
          "original_form": "전 운이 없는 편인데요",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044209.1.21",
          "form": "저도요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044209.1.22",
          "form": "꽝만 골라걸리는 편인데...",
          "original_form": "꽝만 골라걸리는 편인데...",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044209.1.23",
          "form": "그래도 그 분위기가 느껴보고 싶네요",
          "original_form": "그래도 그 분위기가 느껴보고 싶네요",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044209.1.24",
          "form": "그쵸 ㅋㅋㅋ 재밌을거같아요",
          "original_form": "그쵸 ㅋㅋㅋ 재밌을거같아요",
