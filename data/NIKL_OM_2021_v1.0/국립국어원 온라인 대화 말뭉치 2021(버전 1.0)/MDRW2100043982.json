      "utterance": [
        {
          "id": "MDRW2100043982.1.1",
          "form": "오늘 비오니까",
          "original_form": "오늘 비오니까",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043982.1.2",
          "form": "배라 쿼터 할인하는데",
          "original_form": "배라 쿼터 할인하는데",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043982.1.3",
          "form": "씁",
          "original_form": "씁",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043982.1.4",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043982.1.5",
          "form": "아스크림 좋다",
          "original_form": "아스크림 좋다",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043982.1.6",
          "form": "사러갑시다",
          "original_form": "사러갑시다",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043982.1.7",
          "form": "비를 뚫고",
          "original_form": "비를 뚫고",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043982.1.8",
          "form": "비와요....",
          "original_form": "비와요....",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043982.1.9",
          "form": "비...아직 오는데,,,",
          "original_form": "비...아직 오는데,,,",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043982.1.10",
          "form": "그렇져",
          "original_form": "그렇져",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043982.1.11",
          "form": "님 요새 뭐삼???",
          "original_form": "님 요새 뭐삼???",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043982.1.12",
          "form": "비올 때 뚫고나가면 좋잖아요",
          "original_form": "비올 때 뚫고나가면 좋잖아요",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043982.1.13",
          "form": "는 제가 레인코트를 샀어오",
          "original_form": "는 제가 레인코트를 샀어오",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043982.1.14",
          "form": "와 가을장마를 위해서 산건가오",
          "original_form": "와 가을장마를 위해서 산건가오",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043982.1.15",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043982.1.16",
          "form": "나는.....산건 없고 머리를 했씀니댜",
          "original_form": "나는.....산건 없고 머리를 했씀니댜",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043982.1.17",
          "form": "여름장마를 위해 샀어요",
          "original_form": "여름장마를 위해 샀어요",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043982.1.18",
          "form": "7월인가",
          "original_form": "7월인가",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043982.1.19",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043982.1.20",
          "form": "머리가 개판이라",
          "original_form": "머리가 개판이라",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043982.1.21",
          "form": "파마???",
          "original_form": "파마???",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043982.1.22",
