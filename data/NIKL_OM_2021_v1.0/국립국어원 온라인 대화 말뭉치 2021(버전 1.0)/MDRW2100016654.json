      "utterance": [
        {
          "id": "MDRW2100016654.1.1",
          "form": "저기요 빵순이씨 밥은 안먹나요?ㅋㅋㅋ",
          "original_form": "저기요 빵순이씨 밥은 안먹나요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016654.1.2",
          "form": "몇끼째 빵인거죠?ㅋㅋ",
          "original_form": "몇끼째 빵인거죠?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016654.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ아까도 꽈배기하나먹은거야내갖",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아까도 꽈배기하나먹은거야내갖",
          "speaker_id": "1",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100016654.1.4",
          "form": "내가조그씩자주먹눈거얌",
          "original_form": "내가조그씩자주먹눈거얌",
          "speaker_id": "1",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100016654.1.5",
          "form": "",
          "original_form": "{emoji:☺}",
          "speaker_id": "1",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100016654.1.6",
          "form": "이거보내려햇는데 ㅋㅋㅋ",
          "original_form": "이거보내려햇는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100016654.1.7",
          "form": "ㅋㅋㅋ저  순진무구한 표정",
          "original_form": "ㅋㅋㅋ저  순진무구한 표정",
          "speaker_id": "2",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100016654.1.8",
          "form": "밀떡볶이 먹고싶다",
          "original_form": "밀떡볶이 먹고싶다",
          "speaker_id": "2",
          "time": "20210826 14:36"
        },
        {
          "id": "MDRW2100016654.1.9",
          "form": "면같이 긴 밀떡!",
          "original_form": "면같이 긴 밀떡!",
          "speaker_id": "2",
          "time": "20210826 14:36"
        },
        {
          "id": "MDRW2100016654.1.10",
          "form": "ㅋㅋㅋㅋㅋ난 새우볶음밥이랑",
          "original_form": "ㅋㅋㅋㅋㅋ난 새우볶음밥이랑",
          "speaker_id": "1",
          "time": "20210826 14:36"
        },
        {
          "id": "MDRW2100016654.1.11",
          "form": "라면 먹을꺼여",
          "original_form": "라면 먹을꺼여",
          "speaker_id": "1",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016654.1.12",
          "form": "좀잇다",
          "original_form": "좀잇다",
          "speaker_id": "1",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016654.1.13",
          "form": "ㅋㅋㅋ아 진짜 탕수육 먹고싶은데 엄마 어제 체해가지고 흑흑",
          "original_form": "ㅋㅋㅋ아 진짜 탕수육 먹고싶은데 엄마 어제 체해가지고 흑흑",
          "speaker_id": "2",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016654.1.14",
          "form": "헉 ㅠㅠㅠ",
          "original_form": "헉 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016654.1.15",
          "form": "게다가 아빠 있으니 중국집 실현불가능이다 ㅋㅋㅋ",
          "original_form": "게다가 아빠 있으니 중국집 실현불가능이다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016654.1.16",
          "form": "뭐드시고 잘체하시네",
          "original_form": "뭐드시고 잘체하시네",
          "speaker_id": "1",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016654.1.17",
          "form": "전에도 속안좋다하셨는데",
          "original_form": "전에도 속안좋다하셨는데",
          "speaker_id": "1",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016654.1.18",
          "form": "울엄마 진짜 위가 너무 약한거같음 ㅠㅠ",
          "original_form": "울엄마 진짜 위가 너무 약한거같음 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016654.1.19",
          "form": "그리고 다좋아도 너거기가면닫앗으",
          "original_form": "그리고 다좋아도 너거기가면닫앗으",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016654.1.20",
          "form": "ㅋㅋㅋㅋㅋ3시까지랑께잊지마",
          "original_form": "ㅋㅋㅋㅋㅋ3시까지랑께잊지마",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016654.1.21",
          "form": "알짬뽕또먹고싶다..",
          "original_form": "알짬뽕또먹고싶다..",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016654.1.22",
          "form": "여성은 위가 취약해",
          "original_form": "여성은 위가 취약해",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016654.1.23",
          "form": "현대인들이겟지만",
          "original_form": "현대인들이겟지만",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016654.1.24",
          "form": "어마나",
          "original_form": "어마나",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016654.1.25",
          "form": "ㅋㅋㅋ알짬뽕 진짜 먹고싶다",
          "original_form": "ㅋㅋㅋ알짬뽕 진짜 먹고싶다",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016654.1.26",
          "form": "잊고잇던 아이디어스초코렛이와땅 ㅋㅋㅋㅋ생초코렛",
          "original_form": "잊고잇던 아이디어스초코렛이와땅 ㅋㅋㅋㅋ생초코렛",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016654.1.27",
          "form": "곧필요할지알고 이제사보낸건가",
          "original_form": "곧필요할지알고 이제사보낸건가",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016654.1.28",
          "form": "이 자국 남는 쫀득한 생초코!!!!",
          "original_form": "이 자국 남는 쫀득한 생초코!!!!",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016654.1.29",
          "form": "ㅋㅋㅋㅋ 센스있네",
          "original_form": "ㅋㅋㅋㅋ 센스있네",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016654.1.30",
          "form": "와 나 굶고있었는데 둘이서 간식 먹었대 아이고 서러워",
          "original_form": "와 나 굶고있었는데 둘이서 간식 먹었대 아이고 서러워",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016654.1.31",
          "form": "18일날시킴",
          "original_form": "18일날시킴",
          "speaker_id": "1",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016654.1.32",
          "form": "이리와 ㅋㅋㅋ내가 간식쥬께",
          "original_form": "이리와 ㅋㅋㅋ내가 간식쥬께",
          "speaker_id": "1",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100016654.1.33",
          "form": "뭐맛난거드셧을꼬",
          "original_form": "뭐맛난거드셧을꼬",
          "speaker_id": "1",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100016654.1.34",
