      "utterance": [
        {
          "id": "MDRW2100027551.1.1",
          "form": "아 연애가 아니라 썸이군요",
          "original_form": "아 연애가 아니라 썸이군요",
          "speaker_id": "1",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100027551.1.2",
          "form": "일단은 정식으로 사귀고 있는거는 아니니깐 그럴수있겠네요",
          "original_form": "일단은 정식으로 사귀고 있는거는 아니니깐 그럴수있겠네요",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027551.1.3",
          "form": "서로 둘다좋아하는거 잘알고",
          "original_form": "서로 둘다좋아하는거 잘알고",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027551.1.4",
          "form": "러블리달달하다가도",
          "original_form": "러블리달달하다가도",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027551.1.5",
          "form": "트러블생겨서 싸우고",
          "original_form": "트러블생겨서 싸우고",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027551.1.6",
          "form": "다시 화해하고",
          "original_form": "다시 화해하고",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027551.1.7",
          "form": "헤어졌다가 다시 붙잡고",
          "original_form": "헤어졌다가 다시 붙잡고",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027551.1.8",
          "form": "....",
          "original_form": "....",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027551.1.9",
          "form": "반복중이에요",
          "original_form": "반복중이에요",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027551.1.10",
          "form": "10살차이연하남이에요",
          "original_form": "10살차이연하남이에요",
          "speaker_id": "2",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027551.1.11",
          "form": "아 사귀기도 하고요?흠ㅋㅋ",
          "original_form": "아 사귀기도 하고요?흠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027551.1.12",
          "form": "서로 좋아하고 있다고만 얘기하고있고....",
          "original_form": "서로 좋아하고 있다고만 얘기하고있고....",
          "speaker_id": "2",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027551.1.13",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027551.1.14",
          "form": "그게 참 어려운게",
          "original_form": "그게 참 어려운게",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027551.1.15",
          "form": "그 남자때문에 크리스님이 원하는 진짜 연애할 기회를 놓치는거일수도 있어서",
          "original_form": "그 남자때문에 크리스님이 원하는 진짜 연애할 기회를 놓치는거일수도 있어서",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027551.1.16",
          "form": "어제 걔가 노래방가서",
          "original_form": "어제 걔가 노래방가서",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100027551.1.17",
          "form": "저한테 노래들려줬어요",
          "original_form": "저한테 노래들려줬어요",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100027551.1.18",
          "form": "오 잘 풀리셨나요",
          "original_form": "오 잘 풀리셨나요",
          "speaker_id": "1",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100027551.1.19",
          "form": "잘풀린건지는잘모르겠어요...",
          "original_form": "잘풀린건지는잘모르겠어요...",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100027551.1.20",
          "form": "근데 걔가 너무 잘삐져서ㅜㅜ",
          "original_form": "근데 걔가 너무 잘삐져서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100027551.1.21",
          "form": "달래줘야할때 너무 힘들어요",
          "original_form": "달래줘야할때 너무 힘들어요",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100027551.1.22",
          "form": "덩치는 180 되게 큰데",
          "original_form": "덩치는 180 되게 큰데",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100027551.1.23",
          "form": "하는짓은  너무 징징거려요ㅜㅜ",
          "original_form": "하는짓은  너무 징징거려요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100027551.1.24",
          "form": "ㅋㅋ연하남의 매력이네요",
          "original_form": "ㅋㅋ연하남의 매력이네요",
          "speaker_id": "1",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100027551.1.25",
          "form": "연하남이아니라도ㅜㅜ덩치는큰데",
          "original_form": "연하남이아니라도ㅜㅜ덩치는큰데",
          "speaker_id": "2",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100027551.1.26",
          "form": "계속 눈물나온다그러구",
          "original_form": "계속 눈물나온다그러구",
          "speaker_id": "2",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100027551.1.27",
          "form": "새가슴이라서",
          "original_form": "새가슴이라서",
          "speaker_id": "2",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100027551.1.28",
          "form": "무서운거 되게 잘피해요ㅜㅜ",
          "original_form": "무서운거 되게 잘피해요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100027551.1.29",
          "form": "겁이많아요ㅜㅜ",
          "original_form": "겁이많아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100027551.1.30",
          "form": "ㅋㅋㅋ 보호본능",
          "original_form": "ㅋㅋㅋ 보호본능",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100027551.1.31",
          "form": "자극인가요",
          "original_form": "자극인가요",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100027551.1.32",
          "form": "걔가 저한테 징징거려요ㅠㅠ",
          "original_form": "걔가 저한테 징징거려요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 19:44"
        },
        {
          "id": "MDRW2100027551.1.33",
          "form": "원래그래요ㅠㅠ",
          "original_form": "원래그래요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 19:44"
        },
        {
          "id": "MDRW2100027551.1.34",
          "form": "잘삐지고",
          "original_form": "잘삐지고",
          "speaker_id": "2",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100027551.1.35",
          "form": "상처잘받고",
          "original_form": "상처잘받고",
          "speaker_id": "2",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100027551.1.36",
          "form": "ㅋㅋ어필 성공인가요 실패인가요",
          "original_form": "ㅋㅋ어필 성공인가요 실패인가요",
          "speaker_id": "1",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100027551.1.37",
          "form": "걔 그럴때마다 좀 짜증이나서ㅜㅜ",
          "original_form": "걔 그럴때마다 좀 짜증이나서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 06:08"
        },
        {
          "id": "MDRW2100027551.1.38",
          "form": "안달래주면 상처받고 삐지거든요ㅠㅠ",
          "original_form": "안달래주면 상처받고 삐지거든요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 06:08"
        },
        {
          "id": "MDRW2100027551.1.39",
          "form": "본인이기분안좋다고표현을해요",
          "original_form": "본인이기분안좋다고표현을해요",
          "speaker_id": "2",
          "time": "20210907 06:08"
        },
        {
          "id": "MDRW2100027551.1.40",
          "form": "속상하다그러고 제가 달래주면 기분풀어주겠데요",
          "original_form": "속상하다그러고 제가 달래주면 기분풀어주겠데요",
          "speaker_id": "2",
          "time": "20210907 06:09"
        },
        {
          "id": "MDRW2100027551.1.41",
          "form": "너무자주그러니깐ㅜㅜ그게 너무 싫어요",
          "original_form": "너무자주그러니깐ㅜㅜ그게 너무 싫어요",
          "speaker_id": "2",
          "time": "20210907 06:09"
        },
        {
          "id": "MDRW2100027551.1.42",
          "form": "흠 좋지 않은거 같은데요",
          "original_form": "흠 좋지 않은거 같은데요",
          "speaker_id": "1",
          "time": "20210907 06:47"
        }
      ]
