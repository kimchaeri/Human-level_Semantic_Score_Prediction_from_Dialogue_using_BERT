      "utterance": [
        {
          "id": "MDRW2100035821.1.1",
          "form": "우산 많으세용??",
          "original_form": "우산 많으세용??",
          "speaker_id": "2",
          "time": "20210830 03:33"
        },
        {
          "id": "MDRW2100035821.1.2",
          "form": "저희집은 한 8개인가 잇어요 ㅋㅋㅋ\r\n근데 버려야하는것도잇어요 ㅋㅋ",
          "original_form": "저희집은 한 8개인가 잇어요 ㅋㅋㅋ\r\n근데 버려야하는것도잇어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100035821.1.3",
          "form": "name2님은여?",
          "original_form": "&name2&님은여?",
          "speaker_id": "1",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100035821.1.4",
          "form": "일기예보를 안보고 살아서",
          "original_form": "일기예보를 안보고 살아서",
          "speaker_id": "2",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100035821.1.5",
          "form": "비 올때마다 사니까",
          "original_form": "비 올때마다 사니까",
          "speaker_id": "2",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100035821.1.6",
          "form": "엄청 많아서 오늘은 아예",
          "original_form": "엄청 많아서 오늘은 아예",
          "speaker_id": "2",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100035821.1.7",
          "form": "현관에 우산을 쌓아뒀어여",
          "original_form": "현관에 우산을 쌓아뒀어여",
          "speaker_id": "2",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100035821.1.8",
          "form": "헉... 그러시구나..애매하시겟네요 ㅠㅜ\r\n당근에 파는건 어떠세여?",
          "original_form": "헉... 그러시구나..애매하시겟네요 ㅠㅜ\r\n당근에 파는건 어떠세여?",
          "speaker_id": "1",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100035821.1.9",
          "form": "편의점에서 다 막 만원 넘게 비싸게 주고사가지고ㅋㅋㅋㅋ",
          "original_form": "편의점에서 다 막 만원 넘게 비싸게 주고사가지고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100035821.1.10",
          "form": "ㅠㅠ 아깝네여ㅋㅋㅋㅋ",
          "original_form": "ㅠㅠ 아깝네여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100035821.1.11",
          "form": "헐.. 좋은거 쓰시나봐여",
          "original_form": "헐.. 좋은거 쓰시나봐여",
          "speaker_id": "1",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100035821.1.12",
          "form": "저처럼 되지 않으시려면",
          "original_form": "저처럼 되지 않으시려면",
          "speaker_id": "2",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100035821.1.13",
          "form": "평소에 일기예보 잘 보고 다니세요!",
          "original_form": "평소에 일기예보 잘 보고 다니세요!",
          "speaker_id": "2",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100035821.1.14",
          "form": "ㅋㅋㅋㅋ 네..ㅜ 저랑 좀 비슷하신것같아요",
          "original_form": "ㅋㅋㅋㅋ 네..ㅜ 저랑 좀 비슷하신것같아요",
          "speaker_id": "1",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100035821.1.15",
          "form": "저는 카드를 참 많이 발급햇엇거든요",
          "original_form": "저는 카드를 참 많이 발급햇엇거든요",
          "speaker_id": "1",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100035821.1.16",
          "form": "앜ㅋㅋㅋ 그럴수 있죵",
          "original_form": "앜ㅋㅋㅋ 그럴수 있죵",
          "speaker_id": "2",
          "time": "20210830 03:38"
        },
        {
          "id": "MDRW2100035821.1.17",
          "form": "내일 꼭 우산 챙기시고",
          "original_form": "내일 꼭 우산 챙기시고",
          "speaker_id": "2",
          "time": "20210830 03:38"
        },
        {
          "id": "MDRW2100035821.1.18",
          "form": "아 오늘 이네요ㅋㅋㅋㅋ",
          "original_form": "아 오늘 이네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 03:38"
        },
        {
          "id": "MDRW2100035821.1.19",
          "form": "네.. ㅜ 감사해요 ㅋㅋㅋ",
          "original_form": "네.. ㅜ 감사해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 03:38"
        },
        {
          "id": "MDRW2100035821.1.20",
          "form": "따뜻하게 입으시고",
          "original_form": "따뜻하게 입으시고",
          "speaker_id": "2",
          "time": "20210830 03:38"
        },
        {
          "id": "MDRW2100035821.1.21",
          "form": "추우면 감기걸려요ㅠㅠ",
          "original_form": "추우면 감기걸려요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 03:38"
        },
        {
          "id": "MDRW2100035821.1.22",
          "form": "ㅋㅋㅋㅋ 따수우시네여",
          "original_form": "ㅋㅋㅋㅋ 따수우시네여",
          "speaker_id": "1",
          "time": "20210830 03:38"
        },
        {
          "id": "MDRW2100035821.1.23",
          "form": "저도 여름에 감기걸린적이잇엇는데..",
          "original_form": "저도 여름에 감기걸린적이잇엇는데..",
          "speaker_id": "1",
          "time": "20210830 03:38"
        },
        {
          "id": "MDRW2100035821.1.24",
          "form": "ㅎㅎㅎ 여름이라고 무시못하겠더라구요",
          "original_form": "ㅎㅎㅎ 여름이라고 무시못하겠더라구요",
          "speaker_id": "2",
          "time": "20210830 03:39"
        },
        {
          "id": "MDRW2100035821.1.25",
          "form": "여름에도 쌀쌀할땐 쌀쌀해요",
          "original_form": "여름에도 쌀쌀할땐 쌀쌀해요",
          "speaker_id": "2",
          "time": "20210830 03:39"
        }
      ]
