      "utterance": [
        {
          "id": "MDRW2100004092.1.1",
          "form": "오늘 비오다가 멈췄네",
          "original_form": "오늘 비오다가 멈췄네",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004092.1.2",
          "form": "그러게 근데 그치니까 더 꿉꿉하고 습하다",
          "original_form": "그러게 근데 그치니까 더 꿉꿉하고 습하다",
          "speaker_id": "2",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004092.1.3",
          "form": "오늘 밖에 나왓어??",
          "original_form": "오늘 밖에 나왓어??",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004092.1.4",
          "form": "아니 4단계잖아 그냥 창문 열어뒀는데도 꿉꿉한거 알겠어",
          "original_form": "아니 4단계잖아 그냥 창문 열어뒀는데도 꿉꿉한거 알겠어",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004092.1.5",
          "form": "바깥이야?",
          "original_form": "바깥이야?",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004092.1.6",
          "form": "아아 난 출근 했거든",
          "original_form": "아아 난 출근 했거든",
          "speaker_id": "1",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004092.1.7",
          "form": "퇴근길엔 비가 멈춰서 다행이야",
          "original_form": "퇴근길엔 비가 멈춰서 다행이야",
          "speaker_id": "1",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004092.1.8",
          "form": "아니 토요일 출근이라니 고생했네",
          "original_form": "아니 토요일 출근이라니 고생했네",
          "speaker_id": "2",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004092.1.9",
          "form": "아침에 비 쏟아졌는데 출근할때 힘들었겠다",
          "original_form": "아침에 비 쏟아졌는데 출근할때 힘들었겠다",
          "speaker_id": "2",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004092.1.10",
          "form": "이젠 집에 가는거?",
          "original_form": "이젠 집에 가는거?",
          "speaker_id": "2",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004092.1.11",
          "form": "응응 다음주부터 비가 계속 오나바",
          "original_form": "응응 다음주부터 비가 계속 오나바",
          "speaker_id": "1",
          "time": "20210821 15:00"
        },
        {
          "id": "MDRW2100004092.1.12",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004092.1.13",
          "form": "2차 장마라더라고",
          "original_form": "2차 장마라더라고",
          "speaker_id": "2",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004092.1.14",
          "form": "나갈때 우산 들고다녀야겠어",
          "original_form": "나갈때 우산 들고다녀야겠어",
          "speaker_id": "2",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004092.1.15",
          "form": "양말이 다 젖어서 레인부츠를 살까 생각중이야",
          "original_form": "양말이 다 젖어서 레인부츠를 살까 생각중이야",
          "speaker_id": "1",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004092.1.16",
          "form": "아 척척한 양말 최악이야",
          "original_form": "아 척척한 양말 최악이야",
          "speaker_id": "2",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004092.1.17",
          "form": "레인부츠 좋네~",
          "original_form": "레인부츠 좋네~",
          "speaker_id": "2",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004092.1.18",
          "form": "요샌 우산도 레인부츠도 디자인이 다양해져서 좋더라",
          "original_form": "요샌 우산도 레인부츠도 디자인이 다양해져서 좋더라",
          "speaker_id": "2",
          "time": "20210821 15:05"
        },
        {
          "id": "MDRW2100004092.1.19",
          "form": "응응 비오는날을 싫어해서 너는 좋아해??",
          "original_form": "응응 비오는날을 싫어해서 너는 좋아해??",
          "speaker_id": "1",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004092.1.20",
          "form": "난 실내에 있을때만 좋아해",
          "original_form": "난 실내에 있을때만 좋아해",
          "speaker_id": "2",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004092.1.21",
          "form": "비 오는 소리나 약간 가라앉은 분위기는 좋아하는데",
          "original_form": "비 오는 소리나 약간 가라앉은 분위기는 좋아하는데",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004092.1.22",
          "form": "밖에 돌아다녀야 한다면 비오는 날은 정말... 안좋지",
          "original_form": "밖에 돌아다녀야 한다면 비오는 날은 정말... 안좋지",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004092.1.23",
          "form": "넌 어떤 날씨를 좋아해?",
          "original_form": "넌 어떤 날씨를 좋아해?",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004092.1.24",
          "form": "나도 실내에 있을땐 비가오든 눈이오든 다 괜찮은거같아!! ㅋㅋ",
          "original_form": "나도 실내에 있을땐 비가오든 눈이오든 다 괜찮은거같아!! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004092.1.25",
          "form": "ㅋㅋ풍경으로 보기엔 참 좋은데",
          "original_form": "ㅋㅋ풍경으로 보기엔 참 좋은데",
          "speaker_id": "2",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004092.1.26",
          "form": "그 장면에 내가 들어가면 좀 힘들더라고 ㅋㅋ",
          "original_form": "그 장면에 내가 들어가면 좀 힘들더라고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:13"
        }
      ]
    }
