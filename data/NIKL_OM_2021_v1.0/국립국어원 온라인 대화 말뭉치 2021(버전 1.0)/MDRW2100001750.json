      "utterance": [
        {
          "id": "MDRW2100001750.1.1",
          "form": "너 배달음식 머먹고싶어???",
          "original_form": "너 배달음식 머먹고싶어???",
          "speaker_id": "2",
          "time": "20210819 18:33"
        },
        {
          "id": "MDRW2100001750.1.2",
          "form": "엄마가 짬뽕 시켜준대",
          "original_form": "엄마가 짬뽕 시켜준대",
          "speaker_id": "2",
          "time": "20210819 18:33"
        },
        {
          "id": "MDRW2100001750.1.3",
          "form": "어 나 짬뽕먹고싶다",
          "original_form": "어 나 짬뽕먹고싶다",
          "speaker_id": "1",
          "time": "20210819 18:34"
        },
        {
          "id": "MDRW2100001750.1.4",
          "form": "요즘 배달음식 잘되잇잖아",
          "original_form": "요즘 배달음식 잘되잇잖아",
          "speaker_id": "1",
          "time": "20210819 18:34"
        },
        {
          "id": "MDRW2100001750.1.5",
          "form": "맞어 배달 어플도 잘 되어있구",
          "original_form": "맞어 배달 어플도 잘 되어있구",
          "speaker_id": "2",
          "time": "20210819 18:35"
        },
        {
          "id": "MDRW2100001750.1.6",
          "form": "배민,요기요 이런거",
          "original_form": "배민,요기요 이런거",
          "speaker_id": "2",
          "time": "20210819 18:35"
        },
        {
          "id": "MDRW2100001750.1.7",
          "form": "아 마즘",
          "original_form": "아 마즘",
          "speaker_id": "1",
          "time": "20210819 18:36"
        },
        {
          "id": "MDRW2100001750.1.8",
          "form": "요즘엔 다 배민쓰더라",
          "original_form": "요즘엔 다 배민쓰더라",
          "speaker_id": "1",
          "time": "20210819 18:36"
        },
        {
          "id": "MDRW2100001750.1.9",
          "form": "우리도 배민쓰잖아 ㅋㅋㅋㅋ",
          "original_form": "우리도 배민쓰잖아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:36"
        },
        {
          "id": "MDRW2100001750.1.10",
          "form": "근데 혜택은 요기요가 좀더 많아",
          "original_form": "근데 혜택은 요기요가 좀더 많아",
          "speaker_id": "2",
          "time": "20210819 18:36"
        },
        {
          "id": "MDRW2100001750.1.11",
          "form": "쿠폰 이런거말이야",
          "original_form": "쿠폰 이런거말이야",
          "speaker_id": "2",
          "time": "20210819 18:36"
        },
        {
          "id": "MDRW2100001750.1.12",
          "form": "오 그래? 나도 요기요나 깔아봐야겟다",
          "original_form": "오 그래? 나도 요기요나 깔아봐야겟다",
          "speaker_id": "1",
          "time": "20210819 18:37"
        },
        {
          "id": "MDRW2100001750.1.13",
          "form": "언니야 엄마랑 부산에서 간 카페가",
          "original_form": "언니야 엄마랑 부산에서 간 카페가",
          "speaker_id": "1",
          "time": "20210819 18:37"
        },
        {
          "id": "MDRW2100001750.1.14",
          "form": "유명한데 아니야?",
          "original_form": "유명한데 아니야?",
          "speaker_id": "1",
          "time": "20210819 18:37"
        },
        {
          "id": "MDRW2100001750.1.15",
          "form": "맞앜ㅋㅋㅋㅋㅋ",
          "original_form": "맞앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:37"
        },
        {
          "id": "MDRW2100001750.1.16",
          "form": "그 블로그 찾아보면",
          "original_form": "그 블로그 찾아보면",
          "speaker_id": "2",
          "time": "20210819 18:37"
        },
        {
          "id": "MDRW2100001750.1.17",
          "form": "그 카페만 계속 나오더라",
          "original_form": "그 카페만 계속 나오더라",
          "speaker_id": "2",
          "time": "20210819 18:38"
        },
        {
          "id": "MDRW2100001750.1.18",
          "form": "랑데자뷰 여기야",
          "original_form": "랑데자뷰 여기야",
          "speaker_id": "2",
          "time": "20210819 18:38"
        },
        {
          "id": "MDRW2100001750.1.19",
          "form": "오호 랑데쟈뷰?",
          "original_form": "오호 랑데쟈뷰?",
          "speaker_id": "1",
          "time": "20210819 18:38"
        },
        {
          "id": "MDRW2100001750.1.20",
          "form": "나도 나중에 친구랑 가봐야겠다",
          "original_form": "나도 나중에 친구랑 가봐야겠다",
          "speaker_id": "1",
          "time": "20210819 18:38"
        },
        {
          "id": "MDRW2100001750.1.21",
          "form": "그래그래",
          "original_form": "그래그래",
          "speaker_id": "2",
          "time": "20210819 18:43"
        },
        {
          "id": "MDRW2100001750.1.22",
          "form": "아님 나중에 다같이 여행가자잉",
          "original_form": "아님 나중에 다같이 여행가자잉",
          "speaker_id": "2",
          "time": "20210819 18:43"
        },
        {
          "id": "MDRW2100001750.1.23",
          "form": "아빠도 함께",
          "original_form": "아빠도 함께",
          "speaker_id": "2",
          "time": "20210819 18:43"
        },
        {
          "id": "MDRW2100001750.1.24",
          "form": "오키오키 좋아",
          "original_form": "오키오키 좋아",
          "speaker_id": "1",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100001750.1.25",
          "form": "와 짬뽕시켰다",
          "original_form": "와 짬뽕시켰다",
          "speaker_id": "1",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100001750.1.26",
          "form": "우와 짬뽕 맛있겠다",
          "original_form": "우와 짬뽕 맛있겠다",
          "speaker_id": "2",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100001750.1.27",
          "form": "짜장면도 시켰지",
          "original_form": "짜장면도 시켰지",
          "speaker_id": "2",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100001750.1.28",
          "form": "얼른 왓음 좋겠다 그치?",
          "original_form": "얼른 왓음 좋겠다 그치?",
          "speaker_id": "2",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100001750.1.29",
          "form": "엉 배고파",
          "original_form": "엉 배고파",
          "speaker_id": "1",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100001750.1.30",
          "form": "근데 넌 요즘에 머먹고싶어???",
          "original_form": "근데 넌 요즘에 머먹고싶어???",
          "speaker_id": "2",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100001750.1.31",
          "form": "나는 뿌링클 먹고싶어",
          "original_form": "나는 뿌링클 먹고싶어",
          "speaker_id": "2",
