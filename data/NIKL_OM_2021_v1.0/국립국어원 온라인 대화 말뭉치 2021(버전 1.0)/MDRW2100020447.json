      "utterance": [
        {
          "id": "MDRW2100020447.1.1",
          "form": "안다치셨대요?",
          "original_form": "안다치셨대요?",
          "speaker_id": "1",
          "time": "20210828 06:04"
        },
        {
          "id": "MDRW2100020447.1.2",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 06:04"
        },
        {
          "id": "MDRW2100020447.1.3",
          "form": "뉴스에 나왔어요",
          "original_form": "뉴스에 나왔어요",
          "speaker_id": "2",
          "time": "20210828 06:05"
        },
        {
          "id": "MDRW2100020447.1.4",
          "form": "어머...",
          "original_form": "어머...",
          "speaker_id": "1",
          "time": "20210828 06:05"
        },
        {
          "id": "MDRW2100020447.1.5",
          "form": "좀 정신병 이력이 있으신 손님이어서",
          "original_form": "좀 정신병 이력이 있으신 손님이어서",
          "speaker_id": "2",
          "time": "20210828 06:05"
        },
        {
          "id": "MDRW2100020447.1.6",
          "form": "진짜 날벼락이었어요",
          "original_form": "진짜 날벼락이었어요",
          "speaker_id": "2",
          "time": "20210828 06:05"
        },
        {
          "id": "MDRW2100020447.1.7",
          "form": "진짜 날벼락이네요",
          "original_form": "진짜 날벼락이네요",
          "speaker_id": "1",
          "time": "20210828 06:05"
        },
        {
          "id": "MDRW2100020447.1.8",
          "form": "편의점 연지 얼마 안되서 바로 사건터져서",
          "original_form": "편의점 연지 얼마 안되서 바로 사건터져서",
          "speaker_id": "2",
          "time": "20210828 06:05"
        },
        {
          "id": "MDRW2100020447.1.9",
          "form": "다시 근무하기가 너무 무서우시겠다...",
          "original_form": "다시 근무하기가 너무 무서우시겠다...",
          "speaker_id": "1",
          "time": "20210828 06:05"
        },
        {
          "id": "MDRW2100020447.1.10",
          "form": "헐ㅠㅠㅠ",
          "original_form": "헐ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 06:05"
        },
        {
          "id": "MDRW2100020447.1.11",
          "form": "그러게요...알바생이 진짜 어린 사람이던데..",
          "original_form": "그러게요...알바생이 진짜 어린 사람이던데..",
          "speaker_id": "2",
          "time": "20210828 06:05"
        },
        {
          "id": "MDRW2100020447.1.12",
          "form": "어머 트라우마 남았을텐데 어째요ㅠㅠ",
          "original_form": "어머 트라우마 남았을텐데 어째요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 06:06"
        },
        {
          "id": "MDRW2100020447.1.13",
          "form": "진짜 새벽 편의점이 위험하네요 ㅠㅠ",
          "original_form": "진짜 새벽 편의점이 위험하네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 06:06"
        },
        {
          "id": "MDRW2100020447.1.14",
          "form": "네넹... 세상이 너무 위험하네요ㅠ",
          "original_form": "네넹... 세상이 너무 위험하네요ㅠ",
          "speaker_id": "1",
          "time": "20210828 06:06"
        },
        {
          "id": "MDRW2100020447.1.15",
          "form": "사실 남친이 경찰이라",
          "original_form": "사실 남친이 경찰이라",
          "speaker_id": "2",
          "time": "20210828 06:07"
        },
        {
          "id": "MDRW2100020447.1.16",
          "form": "오늘은 일 안나가시는 휴무이신데",
          "original_form": "오늘은 일 안나가시는 휴무이신데",
          "speaker_id": "1",
          "time": "20210828 06:07"
        },
        {
          "id": "MDRW2100020447.1.17",
          "form": "푹 쉬셔요",
          "original_form": "푹 쉬셔요",
          "speaker_id": "1",
          "time": "20210828 06:07"
        },
        {
          "id": "MDRW2100020447.1.18",
          "form": "뭔가 많이 듣는데 흉흉해요",
          "original_form": "뭔가 많이 듣는데 흉흉해요",
          "speaker_id": "2",
          "time": "20210828 06:07"
        },
        {
          "id": "MDRW2100020447.1.19",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "1",
          "time": "20210828 06:07"
        },
        {
          "id": "MDRW2100020447.1.20",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 06:07"
        },
        {
          "id": "MDRW2100020447.1.21",
          "form": "와아",
          "original_form": "와아",
          "speaker_id": "1",
          "time": "20210828 06:07"
        },
        {
          "id": "MDRW2100020447.1.22",
          "form": "든든하시겠어요",
          "original_form": "든든하시겠어요",
          "speaker_id": "1",
          "time": "20210828 06:07"
        },
        {
          "id": "MDRW2100020447.1.23",
          "form": "오늘 사실 휴무는 아닌데 암튼...눈 떠졌네요..",
          "original_form": "오늘 사실 휴무는 아닌데 암튼...눈 떠졌네요..",
          "speaker_id": "2",
          "time": "20210828 06:07"
        },
        {
          "id": "MDRW2100020447.1.24",
          "form": "헉ㅠㅠ",
          "original_form": "헉ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 06:07"
        },
        {
          "id": "MDRW2100020447.1.25",
          "form": "또 일하로 가시는구나...",
          "original_form": "또 일하로 가시는구나...",
          "speaker_id": "1",
          "time": "20210828 06:07"
        },
        {
          "id": "MDRW2100020447.1.26",
          "form": "뭐 온라인 연수같은거요",
          "original_form": "뭐 온라인 연수같은거요",
          "speaker_id": "2",
          "time": "20210828 06:07"
        },
        {
          "id": "MDRW2100020447.1.27",
          "form": "아하ㅠㅠ",
          "original_form": "아하ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 06:07"
        }
      ]
    }
