      "utterance": [
        {
          "id": "MDRW2100038144.1.1",
          "form": "공연 어떤거 가보셨어요?",
          "original_form": "공연 어떤거 가보셨어요?",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100038144.1.2",
          "form": "많이는 아니지만 뮤지컬 연극 콘서트 다 조금씩 가봤어요ㅎㅎ",
          "original_form": "많이는 아니지만 뮤지컬 연극 콘서트 다 조금씩 가봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100038144.1.3",
          "form": "저는 자이언티 콘서트 한두번 가본게 다예요ㅋㅋ",
          "original_form": "저는 자이언티 콘서트 한두번 가본게 다예요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100038144.1.4",
          "form": "어떤게 제일 좋으셨어요?",
          "original_form": "어떤게 제일 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100038144.1.5",
          "form": "저는 가봤던 공연들은 다 좋았었어요!  \r\n기회만 된다면 다양한 공연들 또 가보고 싶어요",
          "original_form": "저는 가봤던 공연들은 다 좋았었어요!  \r\n기회만 된다면 다양한 공연들 또 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100038144.1.6",
          "form": "나훈아 콘서트 끌리더라구요ㅋㅋ",
          "original_form": "나훈아 콘서트 끌리더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100038144.1.7",
          "form": "코시국이지만ㅠ",
          "original_form": "코시국이지만ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100038144.1.8",
          "form": "아ㅋㅋ 지난번에 그 나훈아콘서트 티비에 나왔던 이후로",
          "original_form": "아ㅋㅋ 지난번에 그 나훈아콘서트 티비에 나왔던 이후로",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100038144.1.9",
          "form": "팬분들과 가보고싶어하는 분들이 많이 늘어난것 같아요",
          "original_form": "팬분들과 가보고싶어하는 분들이 많이 늘어난것 같아요",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100038144.1.10",
          "form": "아 맞아요ㅋㅋㅋ아무래도",
          "original_form": "아 맞아요ㅋㅋㅋ아무래도",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100038144.1.11",
          "form": "효도목적도있고...?",
          "original_form": "효도목적도있고...?",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100038144.1.12",
          "form": "처음에 효도목적이었지만 나중에는 주객전도로 팬 되어서 나를 위해 더 열심히 티켓팅하겠다는분들도 있더라구요",
          "original_form": "처음에 효도목적이었지만 나중에는 주객전도로 팬 되어서 나를 위해 더 열심히 티켓팅하겠다는분들도 있더라구요",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100038144.1.13",
          "form": "아아 맞아요ㅋㅋㅋ부모님을 핑계로 그냥 진짜로 즐기고 와버리는 ^^",
          "original_form": "아아 맞아요ㅋㅋㅋ부모님을 핑계로 그냥 진짜로 즐기고 와버리는 ^^",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100038144.1.14",
          "form": "글케재밋다더라구요 싸이콘서트처럼",
          "original_form": "글케재밋다더라구요 싸이콘서트처럼",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100038144.1.15",
          "form": "싸이콘서트는 가면 집에 안 보내준다는 얘기가 있다고 하더라구요ㅋㅋㅋ",
          "original_form": "싸이콘서트는 가면 집에 안 보내준다는 얘기가 있다고 하더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100038144.1.16",
          "form": "지칠때까지 공연을 즐겨야한다고",
          "original_form": "지칠때까지 공연을 즐겨야한다고",
          "speaker_id": "2",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100038144.1.17",
          "form": "ㅋㅋㅋㅋ아 맞아요",
          "original_form": "ㅋㅋㅋㅋ아 맞아요",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100038144.1.18",
          "form": "영남대에 왔었는데",
          "original_form": "영남대에 왔었는데",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100038144.1.19",
          "form": "새벽에 마치더라고요...",
          "original_form": "새벽에 마치더라고요...",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100038144.1.20",
          "form": "새벽까지 공연 보셨던거에요?",
          "original_form": "새벽까지 공연 보셨던거에요?",
          "speaker_id": "2",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100038144.1.21",
          "form": "아뇨 저는 안보구",
          "original_form": "아뇨 저는 안보구",
          "speaker_id": "1",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100038144.1.22",
          "form": "친구가 전해줬었어요",
          "original_form": "친구가 전해줬었어요",
          "speaker_id": "1",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100038144.1.23",
          "form": "새벽한시까지라구",
          "original_form": "새벽한시까지라구",
          "speaker_id": "1",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100038144.1.24",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100038144.1.25",
          "form": "그런 공연가면 비싼 티켓 값이 아깝지 않을거 같아요",
          "original_form": "그런 공연가면 비싼 티켓 값이 아깝지 않을거 같아요",
          "speaker_id": "2",
          "time": "20210902 00:14"
        }
      ]
