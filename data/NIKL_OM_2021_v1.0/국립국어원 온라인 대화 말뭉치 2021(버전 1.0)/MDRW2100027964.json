      "utterance": [
        {
          "id": "MDRW2100027964.1.1",
          "form": "??팅구랑 성심당빵얘기중이라니까 엄마가 갑자기 너 성심당가본적잇대요",
          "original_form": "??팅구랑 성심당빵얘기중이라니까 엄마가 갑자기 너 성심당가본적잇대요",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027964.1.2",
          "form": "애기시절에...",
          "original_form": "애기시절에...",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027964.1.3",
          "form": "진자어이업어",
          "original_form": "진자어이업어",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027964.1.4",
          "form": "그걸..가봣다고 쳐도되는지",
          "original_form": "그걸..가봣다고 쳐도되는지",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027964.1.5",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027964.1.6",
          "form": "기억 못하면 무효예오!",
          "original_form": "기억 못하면 무효예오!",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027964.1.7",
          "form": "ㅋㅋㅋ그닉가요",
          "original_form": "ㅋㅋㅋ그닉가요",
          "speaker_id": "2",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027964.1.8",
          "form": "암튼 진짜맛있는빵...먹고싶어요...",
          "original_form": "암튼 진짜맛있는빵...먹고싶어요...",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027964.1.9",
          "form": "하레하레라는 빵집도 맛있어요",
          "original_form": "하레하레라는 빵집도 맛있어요",
          "speaker_id": "1",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027964.1.10",
          "form": "대전 동쪽의 성심당, 서쪽의 하레하레 라는 말이 있어요",
          "original_form": "대전 동쪽의 성심당, 서쪽의 하레하레 라는 말이 있어요",
          "speaker_id": "1",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027964.1.11",
          "form": "와 멋지다",
          "original_form": "와 멋지다",
          "speaker_id": "2",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100027964.1.12",
          "form": "대구는 빵집이 진짜진짜 많은데",
          "original_form": "대구는 빵집이 진짜진짜 많은데",
          "speaker_id": "2",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027964.1.13",
          "form": "그렇게 딱 손에 꼽는데는 없거든용",
          "original_form": "그렇게 딱 손에 꼽는데는 없거든용",
          "speaker_id": "2",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027964.1.14",
          "form": "되게머쉿다..",
          "original_form": "되게머쉿다..",
          "speaker_id": "2",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027964.1.15",
          "form": "그래도 대구는 맛집 많지 않아요?",
          "original_form": "그래도 대구는 맛집 많지 않아요?",
          "speaker_id": "1",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100027964.1.16",
          "form": "여기저기 많이 들어본 것 같아요",
          "original_form": "여기저기 많이 들어본 것 같아요",
          "speaker_id": "1",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100027964.1.17",
          "form": "나름 빵지순례? 도 오구",
          "original_form": "나름 빵지순례? 도 오구",
          "speaker_id": "2",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100027964.1.18",
          "form": "모랄까 전반적으로 음식이 맛없진않은데..",
          "original_form": "모랄까 전반적으로 음식이 맛없진않은데..",
          "speaker_id": "2",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100027964.1.19",
          "form": "딱 손에꼽게 맛있는데는 별루없는??",
          "original_form": "딱 손에꼽게 맛있는데는 별루없는??",
          "speaker_id": "2",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100027964.1.20",
          "form": "느낌이에요",
          "original_form": "느낌이에요",
          "speaker_id": "2",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100027964.1.21",
          "form": "이의견은지극히주관적견해이며어쩌구..",
          "original_form": "이의견은지극히주관적견해이며어쩌구..",
          "speaker_id": "2",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100027964.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100027964.1.23",
          "form": "전체적인 수준은 높지만",
          "original_form": "전체적인 수준은 높지만",
          "speaker_id": "1",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100027964.1.24",
          "form": "이름난 맛집은 없는 그런 상태군요",
          "original_form": "이름난 맛집은 없는 그런 상태군요",
          "speaker_id": "1",
          "time": "20210902 16:59"
        },
        {
          "id": "MDRW2100027964.1.25",
          "form": "ㅋㅋㅋ넹 친구들 대구놀러온다하면",
          "original_form": "ㅋㅋㅋ넹 친구들 대구놀러온다하면",
          "speaker_id": "2",
          "time": "20210902 16:59"
        },
        {
          "id": "MDRW2100027964.1.26",
          "form": "딱 두군데 추천해줘요",
          "original_form": "딱 두군데 추천해줘요",
          "speaker_id": "2",
          "time": "20210902 16:59"
        },
        {
          "id": "MDRW2100027964.1.27",
          "form": "냠님도 대구에 놀러오시면 제가...쏩니다",
          "original_form": "냠님도 대구에 놀러오시면 제가...쏩니다",
          "speaker_id": "2",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100027964.1.28",
          "form": "ㅋㅋㅋㅋ 그럼 name2님 대전 오시면 제가 쏠게요",
          "original_form": "ㅋㅋㅋㅋ 그럼 &name2&님 대전 오시면 제가 쏠게요",
          "speaker_id": "1",
          "time": "20210902 17:04"
        },
        {
          "id": "MDRW2100027964.1.29",
          "form": "벌써 5신데 저녁 뭐 드실거예요??",
          "original_form": "벌써 5신데 저녁 뭐 드실거예요??",
          "speaker_id": "1",
          "time": "20210902 17:04"
        },
        {
          "id": "MDRW2100027964.1.30",
          "form": "전 30분전에 점심을먹엇어여..",
          "original_form": "전 30분전에 점심을먹엇어여..",
          "speaker_id": "2",
          "time": "20210902 17:04"
        },
        {
          "id": "MDRW2100027964.1.31",
          "form": "ㅋㅋㅋㅋname1님은 머드세욘 저처럼 삼김줏어먹지마시구 맛난거드셔라..",
          "original_form": "ㅋㅋㅋㅋ&name1&님은 머드세욘 저처럼 삼김줏어먹지마시구 맛난거드셔라..",
          "speaker_id": "2",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100027964.1.32",
          "form": "아 아까 본 사진이 점심이었군요",
          "original_form": "아 아까 본 사진이 점심이었군요",
          "speaker_id": "1",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100027964.1.33",
          "form": "저는 지금 집 왔는데 아무도 없어서",
          "original_form": "저는 지금 집 왔는데 아무도 없어서",
          "speaker_id": "1",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100027964.1.34",
          "form": "뭐먹지 상태예요",
          "original_form": "뭐먹지 상태예요",
          "speaker_id": "1",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100027964.1.35",
          "form": "ㅠㅠ혼자드셔요?",
          "original_form": "ㅠㅠ혼자드셔요?",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100027964.1.36",
          "form": "부모님이 외출하셨어요",
          "original_form": "부모님이 외출하셨어요",
          "speaker_id": "1",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100027964.1.37",
          "form": "계룡산에 회초리 사러 가셨대요",
          "original_form": "계룡산에 회초리 사러 가셨대요",
          "speaker_id": "1",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100027964.1.38",
          "form": "아 계룡산에서 파는 호떡 맛있어요",
          "original_form": "아 계룡산에서 파는 호떡 맛있어요",
          "speaker_id": "1",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100027964.1.39",
          "form": "ㅋㅋㅋㅋ호떡 여름에두팔아요???",
          "original_form": "ㅋㅋㅋㅋ호떡 여름에두팔아요???",
          "speaker_id": "2",
          "time": "20210902 17:12"
        },
        {
          "id": "MDRW2100027964.1.40",
          "form": "계룡산좋은곳이엇네..",
          "original_form": "계룡산좋은곳이엇네..",
          "speaker_id": "2",
          "time": "20210902 17:12"
        },
        {
          "id": "MDRW2100027964.1.41",
          "form": "휴계소같은 곳에 호떡맛집이 있대요",
          "original_form": "휴계소같은 곳에 호떡맛집이 있대요",
          "speaker_id": "1",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100027964.1.42",
          "form": "저는 맨날 사다주시는 것만 먹어서 식은거 먹었는데",
          "original_form": "저는 맨날 사다주시는 것만 먹어서 식은거 먹었는데",
          "speaker_id": "1",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100027964.1.43",
          "form": "식어도 맛있더라구요",
          "original_form": "식어도 맛있더라구요",
          "speaker_id": "1",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100027964.1.44",
          "form": "회초리말구 호떡사오시길...",
          "original_form": "회초리말구 호떡사오시길...",
          "speaker_id": "2",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100027964.1.45",
          "form": "호떡 사오시길...",
          "original_form": "호떡 사오시길...",
          "speaker_id": "1",
