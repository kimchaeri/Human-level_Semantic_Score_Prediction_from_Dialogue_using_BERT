      "utterance": [
        {
          "id": "MDRW2100010353.1.1",
          "form": "와 오늘 첫째 픽업하러 왔다갔다 급 피곤하다ㅋㅋ",
          "original_form": "와 오늘 첫째 픽업하러 왔다갔다 급 피곤하다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:41"
        },
        {
          "id": "MDRW2100010353.1.2",
          "form": "ㅋㅋㄱ비가오네 그렇네 원래도픽업해주나?",
          "original_form": "ㅋㅋㄱ비가오네 그렇네 원래도픽업해주나?",
          "speaker_id": "1",
          "time": "20210823 18:41"
        },
        {
          "id": "MDRW2100010353.1.3",
          "form": "요즘 첫째가 뭐 무서운거봤다고",
          "original_form": "요즘 첫째가 뭐 무서운거봤다고",
          "speaker_id": "2",
          "time": "20210823 18:47"
        },
        {
          "id": "MDRW2100010353.1.4",
          "form": "자꾸 데려다달라고ㅡㅡ데리러오라고",
          "original_form": "자꾸 데려다달라고ㅡㅡ데리러오라고",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010353.1.5",
          "form": "아효",
          "original_form": "아효",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010353.1.6",
          "form": "안보면 되는데 왜 애들은 그런걸 자꾸 봐서는 ㅋㅋ",
          "original_form": "안보면 되는데 왜 애들은 그런걸 자꾸 봐서는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:43"
        },
        {
          "id": "MDRW2100010353.1.7",
          "form": "그러니깐ㅋ",
          "original_form": "그러니깐ㅋ",
          "speaker_id": "2",
          "time": "20210824 07:27"
        },
        {
          "id": "MDRW2100010353.1.8",
          "form": "어제 8시에 잤다. 피곤해서ㅋ",
          "original_form": "어제 8시에 잤다. 피곤해서ㅋ",
          "speaker_id": "2",
          "time": "20210824 07:27"
        },
        {
          "id": "MDRW2100010353.1.9",
          "form": "첫째는 할머니집갔고ㅋ",
          "original_form": "첫째는 할머니집갔고ㅋ",
          "speaker_id": "2",
          "time": "20210824 07:27"
        },
        {
          "id": "MDRW2100010353.1.10",
          "form": "와~ ㅎㅎ 거기서 자고?",
          "original_form": "와~ ㅎㅎ 거기서 자고?",
          "speaker_id": "1",
          "time": "20210824 10:10"
        },
        {
          "id": "MDRW2100010353.1.11",
          "form": "응응ㅋㅋ 거기서 잤어",
          "original_form": "응응ㅋㅋ 거기서 잤어",
          "speaker_id": "2",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100010353.1.12",
          "form": "말로는 엄마아프니 내가있으면 더 힘들거라고",
          "original_form": "말로는 엄마아프니 내가있으면 더 힘들거라고",
          "speaker_id": "2",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100010353.1.13",
          "form": "하고 보내달라하대",
          "original_form": "하고 보내달라하대",
          "speaker_id": "2",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100010353.1.14",
          "form": "와..효녀네",
          "original_form": "와..효녀네",
          "speaker_id": "1",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100010353.1.15",
          "form": "name3이..근데ㅋㅋㅋ서로좋은거긴 해도말을 너무 이쁘게 하잖앙",
          "original_form": "&name3&이..근데ㅋㅋㅋ서로좋은거긴 해도말을 너무 이쁘게 하잖앙",
          "speaker_id": "1",
          "time": "20210824 10:16"
        },
        {
          "id": "MDRW2100010353.1.16",
          "form": "효녀보다는 꿍꿍이가 있어보였지",
          "original_form": "효녀보다는 꿍꿍이가 있어보였지",
          "speaker_id": "2",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100010353.1.17",
          "form": "웃겼다.폰하려고 그러지? 하니깐",
          "original_form": "웃겼다.폰하려고 그러지? 하니깐",
          "speaker_id": "2",
          "time": "20210824 10:18"
        },
        {
          "id": "MDRW2100010353.1.18",
          "form": "아니라고 진짜 걱정되서 그렇다고",
          "original_form": "아니라고 진짜 걱정되서 그렇다고",
          "speaker_id": "2",
          "time": "20210824 10:18"
        },
        {
          "id": "MDRW2100010353.1.19",
          "form": "하면서 입꼬리가 올라감",
          "original_form": "하면서 입꼬리가 올라감",
          "speaker_id": "2",
          "time": "20210824 10:18"
        },
        {
          "id": "MDRW2100010353.1.20",
          "form": "ㅋㅋㅋㅋ 애들이 참 귀엽다",
          "original_form": "ㅋㅋㅋㅋ 애들이 참 귀엽다",
          "speaker_id": "1",
          "time": "20210824 10:18"
        },
        {
          "id": "MDRW2100010353.1.21",
          "form": "딱 걸렸다! 웃었다! 하니깐 엄마가 자꾸 웃긴대",
          "original_form": "딱 걸렸다! 웃었다! 하니깐 엄마가 자꾸 웃긴대",
          "speaker_id": "2",
          "time": "20210824 10:18"
        },
        {
          "id": "MDRW2100010353.1.22",
          "form": "너가 웃기거든ㅎㅎ 그럼 언제와",
          "original_form": "너가 웃기거든ㅎㅎ 그럼 언제와",
          "speaker_id": "1",
          "time": "20210824 10:19"
        },
        {
          "id": "MDRW2100010353.1.23",
          "form": "오늘이나 내일ㅋ",
          "original_form": "오늘이나 내일ㅋ",
          "speaker_id": "2",
          "time": "20210824 10:19"
        },
        {
          "id": "MDRW2100010353.1.24",
          "form": "좋구나~ ㅎㅎ 학원 그럼 보강해?",
          "original_form": "좋구나~ ㅎㅎ 학원 그럼 보강해?",
          "speaker_id": "1",
          "time": "20210824 10:19"
        },
        {
          "id": "MDRW2100010353.1.25",
          "form": "아니ㅋ안할듯ㅋ",
          "original_form": "아니ㅋ안할듯ㅋ",
          "speaker_id": "2",
          "time": "20210824 10:19"
        },
        {
          "id": "MDRW2100010353.1.26",
          "form": "내일 돌봄 마지막날인데 장기자랑 조장한다했다더라고ㅋ",
          "original_form": "내일 돌봄 마지막날인데 장기자랑 조장한다했다더라고ㅋ",
          "speaker_id": "2",
          "time": "20210824 10:20"
        },
        {
          "id": "MDRW2100010353.1.27",
          "form": "오늘 돌봄선생님께 안간다고 전화했다가 내일  장기자랑날인데 다시 한번 물어보고 전화달라하시대",
          "original_form": "오늘 돌봄선생님께 안간다고 전화했다가 내일  장기자랑날인데 다시 한번 물어보고 전화달라하시대",
