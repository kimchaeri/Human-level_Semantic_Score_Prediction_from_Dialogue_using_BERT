      "utterance": [
        {
          "id": "MDRW2100003139.1.1",
          "form": "요새 챙겨보는 프로 있으세요?",
          "original_form": "요새 챙겨보는 프로 있으세요?",
          "speaker_id": "2",
          "time": "20210820 19:32"
        },
        {
          "id": "MDRW2100003139.1.2",
          "form": "요즘 하트시그널3 보고있어요",
          "original_form": "요즘 하트시그널3 보고있어요",
          "speaker_id": "1",
          "time": "20210820 19:36"
        },
        {
          "id": "MDRW2100003139.1.3",
          "form": "앗 하시 너무 재미있게 봤었는데",
          "original_form": "앗 하시 너무 재미있게 봤었는데",
          "speaker_id": "2",
          "time": "20210820 19:37"
        },
        {
          "id": "MDRW2100003139.1.4",
          "form": "지금 보고 계시는군용",
          "original_form": "지금 보고 계시는군용",
          "speaker_id": "2",
          "time": "20210820 19:37"
        },
        {
          "id": "MDRW2100003139.1.5",
          "form": "박지현님 너무 예쁘죵?",
          "original_form": "박지현님 너무 예쁘죵?",
          "speaker_id": "2",
          "time": "20210820 19:37"
        },
        {
          "id": "MDRW2100003139.1.6",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 19:39"
        },
        {
          "id": "MDRW2100003139.1.7",
          "form": "처음 들어오실 때 깜짝 놀랬어요",
          "original_form": "처음 들어오실 때 깜짝 놀랬어요",
          "speaker_id": "1",
          "time": "20210820 19:39"
        },
        {
          "id": "MDRW2100003139.1.8",
          "form": "다른 세계 사람 같아요",
          "original_form": "다른 세계 사람 같아요",
          "speaker_id": "1",
          "time": "20210820 19:39"
        },
        {
          "id": "MDRW2100003139.1.9",
          "form": "ㅋㅋㅋㅋ그 정도였나요?",
          "original_form": "ㅋㅋㅋㅋ그 정도였나요?",
          "speaker_id": "2",
          "time": "20210820 19:39"
        },
        {
          "id": "MDRW2100003139.1.10",
          "form": "남자분들 눈에도\n예쁘긴 마찬가지인가봅니다ㅋㅋ",
          "original_form": "남자분들 눈에도\n예쁘긴 마찬가지인가봅니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 19:39"
        },
        {
          "id": "MDRW2100003139.1.11",
          "form": "맞아요 초반에 계속 몰표 받으시고ㅋㅋ",
          "original_form": "맞아요 초반에 계속 몰표 받으시고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 19:41"
        },
        {
          "id": "MDRW2100003139.1.12",
          "form": "거기서 누구랑 되든 박지현님이 너무 아까워요",
          "original_form": "거기서 누구랑 되든 박지현님이 너무 아까워요",
          "speaker_id": "1",
          "time": "20210820 19:41"
        },
        {
          "id": "MDRW2100003139.1.13",
          "form": "스포일거 같으니 말하지 않겠어여ㅋㅋㅋ",
          "original_form": "스포일거 같으니 말하지 않겠어여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 19:43"
        },
        {
          "id": "MDRW2100003139.1.14",
          "form": "김강열님만 아니면 괜찮을 것 같아요....",
          "original_form": "김강열님만 아니면 괜찮을 것 같아요....",
          "speaker_id": "1",
          "time": "20210820 19:46"
        },
        {
          "id": "MDRW2100003139.1.15",
          "form": "아.....  유감이네요....",
          "original_form": "아.....  유감이네요....",
          "speaker_id": "2",
          "time": "20210820 19:47"
        },
        {
          "id": "MDRW2100003139.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 19:47"
        },
        {
          "id": "MDRW2100003139.1.17",
          "form": "앗 안 돼요ㅠㅠ",
          "original_form": "앗 안 돼요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 19:50"
        },
        {
          "id": "MDRW2100003139.1.18",
          "form": "핫시는 여기서 하차해야겠어요",
          "original_form": "핫시는 여기서 하차해야겠어요",
          "speaker_id": "1",
          "time": "20210820 19:50"
        },
        {
          "id": "MDRW2100003139.1.19",
          "form": "ㅋㅋ그 동안 핫시 다 보셨나요?",
          "original_form": "ㅋㅋ그 동안 핫시 다 보셨나요?",
          "speaker_id": "2",
          "time": "20210820 19:51"
        },
        {
          "id": "MDRW2100003139.1.20",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210820 19:51"
        },
        {
          "id": "MDRW2100003139.1.21",
          "form": "전 2가 가장 재밌었어요",
          "original_form": "전 2가 가장 재밌었어요",
          "speaker_id": "1",
          "time": "20210820 19:52"
        },
        {
          "id": "MDRW2100003139.1.22",
          "form": "2가 오영주님 나왔을때죠?",
          "original_form": "2가 오영주님 나왔을때죠?",
          "speaker_id": "2",
          "time": "20210820 19:55"
        },
        {
          "id": "MDRW2100003139.1.23",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210820 19:57"
        },
        {
          "id": "MDRW2100003139.1.24",
          "form": "다들 감정에 솔직해서 좋더라고요",
          "original_form": "다들 감정에 솔직해서 좋더라고요",
          "speaker_id": "1",
          "time": "20210820 19:57"
        },
        {
          "id": "MDRW2100003139.1.25",
          "form": "2에서는 어느 분이\n가장 매력있게 느껴지셨어요?",
          "original_form": "2에서는 어느 분이\n가장 매력있게 느껴지셨어요?",
          "speaker_id": "2",
          "time": "20210820 19:59"
        },
        {
          "id": "MDRW2100003139.1.26",
          "form": "현주님이 현우님 매력을 본인만 알고싶다 했을 때요!",
          "original_form": "현주님이 현우님 매력을 본인만 알고싶다 했을 때요!",
          "speaker_id": "1",
          "time": "20210820 20:01"
        }
      ]
    }
  ]
