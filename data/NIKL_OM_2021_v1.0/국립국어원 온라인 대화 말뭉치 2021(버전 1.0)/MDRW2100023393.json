      "utterance": [
        {
          "id": "MDRW2100023393.1.1",
          "form": "스타벅스 당근 케이크 진짜 맛있게 먹었어요!",
          "original_form": "스타벅스 당근 케이크 진짜 맛있게 먹었어요!",
          "speaker_id": "1",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023393.1.2",
          "form": "아 저도 그거 좋아해요!!!",
          "original_form": "아 저도 그거 좋아해요!!!",
          "speaker_id": "2",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023393.1.3",
          "form": "당근케이크 최고",
          "original_form": "당근케이크 최고",
          "speaker_id": "2",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023393.1.4",
          "form": "드셔보셨군요!ㅋㅋㅋ",
          "original_form": "드셔보셨군요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023393.1.5",
          "form": "넵ㅎㅎㅎㅎ",
          "original_form": "넵ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023393.1.6",
          "form": "당근도 그래도 들어가있고 진짜 맛있게 먹었어요",
          "original_form": "당근도 그래도 들어가있고 진짜 맛있게 먹었어요",
          "speaker_id": "1",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023393.1.7",
          "form": "저는 크림 넘 많은 케이크는 안좋아해서",
          "original_form": "저는 크림 넘 많은 케이크는 안좋아해서",
          "speaker_id": "1",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023393.1.8",
          "form": "이런 케이크가 좋더라구요",
          "original_form": "이런 케이크가 좋더라구요",
          "speaker_id": "1",
          "time": "20210830 01:22"
        },
        {
          "id": "MDRW2100023393.1.9",
          "form": "위에 그 쪼그만 당근 초콜렛으루 된거 너무 귀엽지 않나요",
          "original_form": "위에 그 쪼그만 당근 초콜렛으루 된거 너무 귀엽지 않나요",
          "speaker_id": "2",
          "time": "20210830 01:22"
        },
        {
          "id": "MDRW2100023393.1.10",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210830 01:22"
        },
        {
          "id": "MDRW2100023393.1.11",
          "form": "아아 맞아요ㅠㅠㅠ 짱 귀여워요",
          "original_form": "아아 맞아요ㅠㅠㅠ 짱 귀여워요",
          "speaker_id": "1",
          "time": "20210830 01:22"
        },
        {
          "id": "MDRW2100023393.1.12",
          "form": "크림이 적어서 좋더라구요",
          "original_form": "크림이 적어서 좋더라구요",
          "speaker_id": "2",
          "time": "20210830 01:22"
        },
        {
          "id": "MDRW2100023393.1.13",
          "form": "담백한 느낌이에요! ㅎㅎ",
          "original_form": "담백한 느낌이에요! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:22"
        },
        {
          "id": "MDRW2100023393.1.14",
          "form": "맞아요 ㅎㅎㅎ",
          "original_form": "맞아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:22"
        },
        {
          "id": "MDRW2100023393.1.15",
          "form": "칼로리는 모르겠지만 살 덜찌는맛...?",
          "original_form": "칼로리는 모르겠지만 살 덜찌는맛...?",
          "speaker_id": "1",
          "time": "20210830 01:22"
        },
        {
          "id": "MDRW2100023393.1.16",
          "form": "앜ㅋㅋㅋㅋ 칼..로리는..",
          "original_form": "앜ㅋㅋㅋㅋ 칼..로리는..",
          "speaker_id": "2",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100023393.1.17",
          "form": "맛있게 먹으면..",
          "original_form": "맛있게 먹으면..",
          "speaker_id": "2",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100023393.1.18",
          "form": "0...",
          "original_form": "0...",
          "speaker_id": "2",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100023393.1.19",
          "form": "ㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100023393.1.20",
          "form": "요즘 sns에서 보니까 미국에서 한국식 레시피라고 유행하는거",
          "original_form": "요즘 sns에서 보니까 미국에서 한국식 레시피라고 유행하는거",
          "speaker_id": "1",
          "time": "20210830 01:23"
        },
        {
