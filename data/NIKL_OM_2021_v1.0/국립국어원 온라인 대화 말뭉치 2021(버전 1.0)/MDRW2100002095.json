      "utterance": [
        {
          "id": "MDRW2100002095.1.1",
          "form": "저는 최근에  다이어트하려고 식단조절 시작했어요",
          "original_form": "저는 최근에  다이어트하려고 식단조절 시작했어요",
          "speaker_id": "2",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100002095.1.2",
          "form": "머 드세여?",
          "original_form": "머 드세여?",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002095.1.3",
          "form": "샐러드 먹어요",
          "original_form": "샐러드 먹어요",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002095.1.4",
          "form": "퇴근하면 gx 꾸준히했는데",
          "original_form": "퇴근하면 gx 꾸준히했는데",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002095.1.5",
          "form": "체중변화가 없어서",
          "original_form": "체중변화가 없어서",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002095.1.6",
          "form": "잘때 배고프지 않아요?",
          "original_form": "잘때 배고프지 않아요?",
          "speaker_id": "1",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002095.1.7",
          "form": "자기전은 괜찮은데",
          "original_form": "자기전은 괜찮은데",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002095.1.8",
          "form": "회사에서 점심때 샐러드 먹으면",
          "original_form": "회사에서 점심때 샐러드 먹으면",
          "speaker_id": "2",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002095.1.9",
          "form": "오후에 배가 고파요",
          "original_form": "오후에 배가 고파요",
          "speaker_id": "2",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002095.1.10",
          "form": "저기 혹시 저희 방금반려동물로 한 대화",
          "original_form": "저기 혹시 저희 방금반려동물로 한 대화",
          "speaker_id": "2",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002095.1.11",
          "form": "통과됐다고 나오나요?",
          "original_form": "통과됐다고 나오나요?",
          "speaker_id": "2",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002095.1.12",
          "form": "저는 통과안된걸로 나오는데",
          "original_form": "저는 통과안된걸로 나오는데",
          "speaker_id": "2",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002095.1.13",
          "form": "관리하시는 분들 퇴근하셨나 본데요",
          "original_form": "관리하시는 분들 퇴근하셨나 본데요",
          "speaker_id": "1",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002095.1.14",
          "form": "검증을 안하시네요",
          "original_form": "검증을 안하시네요",
          "speaker_id": "1",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002095.1.15",
          "form": "아 그래서 그렇구나",
          "original_form": "아 그래서 그렇구나",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002095.1.16",
          "form": "저도 그래요",
          "original_form": "저도 그래요",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002095.1.17",
          "form": "늦게 주무셔도 내일 출근 무리없으세요?",
          "original_form": "늦게 주무셔도 내일 출근 무리없으세요?",
          "speaker_id": "2",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002095.1.18",
          "form": "몸이 적응 한듯요",
          "original_form": "몸이 적응 한듯요",
          "speaker_id": "1",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002095.1.19",
          "form": "저도 같은 루틴이 계속되니까 어찌어찌 버티네요",
          "original_form": "저도 같은 루틴이 계속되니까 어찌어찌 버티네요",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002095.1.20",
          "form": "매일 아침마다 밤에 일찍자야지 다짐맘 하죠",
          "original_form": "매일 아침마다 밤에 일찍자야지 다짐맘 하죠",
          "speaker_id": "1",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002095.1.21",
          "form": "그런데 일찍자려고 너무 신경쓰면 그것때문에 스트레스 받아서",
          "original_form": "그런데 일찍자려고 너무 신경쓰면 그것때문에 스트레스 받아서",
          "speaker_id": "2",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002095.1.22",
          "form": "더 잠에 못든대요",
          "original_form": "더 잠에 못든대요",
          "speaker_id": "2",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002095.1.23",
          "form": "마음을 편히가지라고 하더라고요",
          "original_form": "마음을 편히가지라고 하더라고요",
          "speaker_id": "2",
          "time": "20210819 23:47"
        }
      ]
