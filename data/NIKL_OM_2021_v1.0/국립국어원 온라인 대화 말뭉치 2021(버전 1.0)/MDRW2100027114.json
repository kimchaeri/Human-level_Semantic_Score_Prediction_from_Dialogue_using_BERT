        {
          "id": "MDRW2100027114.1.1",
          "form": "종교가 있으신가요?",
          "original_form": "종교가 있으신가요?",
          "speaker_id": "1",
          "time": "20210903 19:48"
        },
        {
          "id": "MDRW2100027114.1.2",
          "form": "기독교요",
          "original_form": "기독교요",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100027114.1.3",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100027114.1.4",
          "form": "일요일마다 예배를.드리시나요?",
          "original_form": "일요일마다 예배를.드리시나요?",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100027114.1.5",
          "form": "근데 안나간지 일년넘엇네요",
          "original_form": "근데 안나간지 일년넘엇네요",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100027114.1.6",
          "form": "코로나로 인한 건가요??",
          "original_form": "코로나로 인한 건가요??",
          "speaker_id": "1",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100027114.1.7",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100027114.1.8",
          "form": "그렇군요 ㅎㅎ",
          "original_form": "그렇군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100027114.1.9",
          "form": "종교생활에서 가장 좋은 점은 무엇인가요?",
          "original_form": "종교생활에서 가장 좋은 점은 무엇인가요?",
          "speaker_id": "1",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100027114.1.10",
          "form": "마음의 안정같아여ㅋㅋㅋ",
          "original_form": "마음의 안정같아여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100027114.1.11",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100027114.1.12",
          "form": "교회행사 같은건 ㄱ자주 가셨나요?",
          "original_form": "교회행사 같은건 ㄱ자주 가셨나요?",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100027114.1.13",
          "form": "작은 교회라 행사가 거의 없어요",
          "original_form": "작은 교회라 행사가 거의 없어요",
          "speaker_id": "2",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100027114.1.14",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210904 10:34"
        },
        {
          "id": "MDRW2100027114.1.15",
          "form": "직분이 있나요??",
          "original_form": "직분이 있나요??",
          "speaker_id": "1",
          "time": "20210904 10:34"
        },
        {
          "id": "MDRW2100027114.1.16",
          "form": "아뇨 아직이요",
          "original_form": "아뇨 아직이요",
          "speaker_id": "2",
          "time": "20210904 10:39"
        },
        {
          "id": "MDRW2100027114.1.17",
          "form": "그렇군요 ㅎㅎ",
          "original_form": "그렇군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 11:51"
        },
        {
          "id": "MDRW2100027114.1.18",
          "form": "성가대봉사는 했었어요",
          "original_form": "성가대봉사는 했었어요",
          "speaker_id": "2",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100027114.1.19",
          "form": "그랬군요ㅎㅎ",
          "original_form": "그랬군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100027114.1.20",
          "form": "그럼 사람들과 많은 교류를 하셨나요?",
          "original_form": "그럼 사람들과 많은 교류를 하셨나요?",
          "speaker_id": "1",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100027114.1.21",
          "form": "그렇게 많진 않앗네요ㅋㅋ",
          "original_form": "그렇게 많진 않앗네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100027114.1.22",
          "form": "따로 모임을 갖거나 그런적도 있으신가요??",
          "original_form": "따로 모임을 갖거나 그런적도 있으신가요??",
