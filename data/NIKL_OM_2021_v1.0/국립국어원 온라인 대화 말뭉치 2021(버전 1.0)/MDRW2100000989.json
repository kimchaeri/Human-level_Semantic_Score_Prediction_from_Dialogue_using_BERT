      "utterance": [
        {
          "id": "MDRW2100000989.1.1",
          "form": "쇼핑을 즐겨하시나요?",
          "original_form": "쇼핑을 즐겨하시나요?",
          "speaker_id": "1",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100000989.1.2",
          "form": "옷이런건 잘안사고 요즘은 주로 먹을거 사요ㅎ",
          "original_form": "옷이런건 잘안사고 요즘은 주로 먹을거 사요ㅎ",
          "speaker_id": "2",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100000989.1.3",
          "form": "먹을거? 어떤거 사시나요? 군것질?",
          "original_form": "먹을거? 어떤거 사시나요? 군것질?",
          "speaker_id": "1",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100000989.1.4",
          "form": "주로 장을 마트안가고 인터넷으로 보거든요 쿠폰뿌릴때ㅋ",
          "original_form": "주로 장을 마트안가고 인터넷으로 보거든요 쿠폰뿌릴때ㅋ",
          "speaker_id": "2",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100000989.1.5",
          "form": "쇼핑좋아하세요?",
          "original_form": "쇼핑좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100000989.1.6",
          "form": "예전에는 좋아했는데  뭔가 최근에 물욕이 없어져서",
          "original_form": "예전에는 좋아했는데  뭔가 최근에 물욕이 없어져서",
          "speaker_id": "1",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100000989.1.7",
          "form": "장볼 때 자주 사는 물품은 무엇인가요?",
          "original_form": "장볼 때 자주 사는 물품은 무엇인가요?",
          "speaker_id": "1",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100000989.1.8",
          "form": "저는 삼겹살 꼭 사는 거 같아요",
          "original_form": "저는 삼겹살 꼭 사는 거 같아요",
          "speaker_id": "1",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100000989.1.9",
          "form": "저도 고기 위주ㅋ",
          "original_form": "저도 고기 위주ㅋ",
          "speaker_id": "2",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100000989.1.10",
          "form": "아무래도 고기가 맛있으니까ㅎㅎ",
          "original_form": "아무래도 고기가 맛있으니까ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100000989.1.11",
          "form": "엊그제 정원샵에서 쿠폰줘서 불막창이랑 곱창샀어요",
          "original_form": "엊그제 정원샵에서 쿠폰줘서 불막창이랑 곱창샀어요",
          "speaker_id": "2",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100000989.1.12",
          "form": "너무좋아요",
          "original_form": "너무좋아요",
          "speaker_id": "1",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100000989.1.13",
          "form": "그 안주야 사셨어요?",
          "original_form": "그 안주야 사셨어요?",
          "speaker_id": "1",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100000989.1.14",
          "form": "그거 진짜 맛있는데",
          "original_form": "그거 진짜 맛있는데",
          "speaker_id": "1",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100000989.1.15",
          "form": "네네ㅋ 엄청싸게ㅋ 7팩사고 16700원정도썼네요",
          "original_form": "네네ㅋ 엄청싸게ㅋ 7팩사고 16700원정도썼네요",
          "speaker_id": "2",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100000989.1.16",
          "form": "와 진짜싸다",
          "original_form": "와 진짜싸다",
          "speaker_id": "1",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100000989.1.17",
          "form": "한팩당 2400원꼴",
          "original_form": "한팩당 2400원꼴",
          "speaker_id": "2",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100000989.1.18",
          "form": "그거 맨날 4천원꼴에 사먹던건데",
          "original_form": "그거 맨날 4천원꼴에 사먹던건데",
          "speaker_id": "1",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100000989.1.19",
          "form": "이것저것할인받고 샀거든요",
          "original_form": "이것저것할인받고 샀거든요",
          "speaker_id": "2",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100000989.1.20",
          "form": "2400원이라니 엄청난 할인이다",
          "original_form": "2400원이라니 엄청난 할인이다",
          "speaker_id": "1",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100000989.1.21",
          "form": "엄청나게 합리적인 쇼핑을 하셨네요",
          "original_form": "엄청나게 합리적인 쇼핑을 하셨네요",
          "speaker_id": "1",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100000989.1.22",
          "form": "3만원이상 담았더니 감자고로케도주더라구요 득템했어요",
          "original_form": "3만원이상 담았더니 감자고로케도주더라구요 득템했어요",
          "speaker_id": "2",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100000989.1.23",
          "form": "와 고로케까지 받다니",
          "original_form": "와 고로케까지 받다니",
          "speaker_id": "1",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100000989.1.24",
          "form": "진짜 대박이에요",
          "original_form": "진짜 대박이에요",
          "speaker_id": "1",
          "time": "20210820 11:26"
        },
        {
          "id": "MDRW2100000989.1.25",
          "form": "오늘도 플친40프로 쿠폰주던데 함 둘러보세요",
          "original_form": "오늘도 플친40프로 쿠폰주던데 함 둘러보세요",
          "speaker_id": "2",
          "time": "20210820 11:26"
        },
        {
          "id": "MDRW2100000989.1.26",
          "form": "정원e샵ㅎ 페이코쓰시면 10프로중곡 쿠폰있어요",
          "original_form": "정원e샵ㅎ 페이코쓰시면 10프로중곡 쿠폰있어요",
          "speaker_id": "2",
          "time": "20210820 11:26"
        },
        {
          "id": "MDRW2100000989.1.27",
          "form": "중복",
          "original_form": "중복",
          "speaker_id": "2",
          "time": "20210820 11:26"
        },
        {
          "id": "MDRW2100000989.1.28",
          "form": "꿀팁 고맙습니다. 저도 둘러봐야겠어요",
          "original_form": "꿀팁 고맙습니다. 저도 둘러봐야겠어요",
          "speaker_id": "1",
          "time": "20210820 11:26"
        },
        {
          "id": "MDRW2100000989.1.29",
          "form": "진짜 이런 할인이 있었다니",
          "original_form": "진짜 이런 할인이 있었다니",
          "speaker_id": "1",
          "time": "20210820 11:26"
        }
      ]
