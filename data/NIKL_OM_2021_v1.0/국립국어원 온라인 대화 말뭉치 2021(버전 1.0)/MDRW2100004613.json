      "utterance": [
        {
          "id": "MDRW2100004613.1.1",
          "form": "안녕하세요 name2님",
          "original_form": "안녕하세요 &name2&님",
          "speaker_id": "1",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004613.1.2",
          "form": "저도 치즈돈가스 좋아하는데 반가워요",
          "original_form": "저도 치즈돈가스 좋아하는데 반가워요",
          "speaker_id": "1",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004613.1.3",
          "form": "반가워요~",
          "original_form": "반가워요~",
          "speaker_id": "2",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004613.1.4",
          "form": "치즈돈가스 많이 사랑해주세요",
          "original_form": "치즈돈가스 많이 사랑해주세요",
          "speaker_id": "2",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004613.1.5",
          "form": "ㅋㅋ암요암요",
          "original_form": "ㅋㅋ암요암요",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004613.1.6",
          "form": "안그래도 오늘 치즈돈까스 구매하려 해요",
          "original_form": "안그래도 오늘 치즈돈까스 구매하려 해요",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004613.1.7",
          "form": "소셜에서 돈까스 할인이 많이올라오더라고요",
          "original_form": "소셜에서 돈까스 할인이 많이올라오더라고요",
          "speaker_id": "1",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004613.1.8",
          "form": "이참에 쟁여놔야겠어요",
          "original_form": "이참에 쟁여놔야겠어요",
          "speaker_id": "1",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004613.1.9",
          "form": "치즈돈가스는 냉동식품 먹어도 맛있어요",
          "original_form": "치즈돈가스는 냉동식품 먹어도 맛있어요",
          "speaker_id": "2",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004613.1.10",
          "form": "저는 집에 치즈돈가스 떨어지면 불안해서",
          "original_form": "저는 집에 치즈돈가스 떨어지면 불안해서",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004613.1.11",
          "form": "저도 10개씩 돈까스 묶음으로 쟁여놔요 ㅋㅋㅋ",
          "original_form": "저도 10개씩 돈까스 묶음으로 쟁여놔요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004613.1.12",
          "form": "반찬없을때 딱이죵",
          "original_form": "반찬없을때 딱이죵",
          "speaker_id": "1",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004613.1.13",
          "form": "에어프라이어에 돌려먹으면 되니까 간편하기도 해요",
          "original_form": "에어프라이어에 돌려먹으면 되니까 간편하기도 해요",
          "speaker_id": "2",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004613.1.14",
          "form": "위00 소셜에서 진0이 맛있더라고요",
          "original_form": "위00 소셜에서 진0이 맛있더라고요",
          "speaker_id": "1",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004613.1.15",
          "form": "치즈가 막늘어나요",
          "original_form": "치즈가 막늘어나요",
          "speaker_id": "1",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004613.1.16",
          "form": "반찬은 또 어떤걸 쟁여두시나요?",
          "original_form": "반찬은 또 어떤걸 쟁여두시나요?",
          "speaker_id": "2",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004613.1.17",
          "form": "맞아요 에어프라이기 간편해서 좋아요",
          "original_form": "맞아요 에어프라이기 간편해서 좋아요",
          "speaker_id": "1",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004613.1.18",
          "form": "냉동이 짱이라 돈까스 말고도 만두랑 떡갈비 종류도 쟁여놔요",
          "original_form": "냉동이 짱이라 돈까스 말고도 만두랑 떡갈비 종류도 쟁여놔요",
          "speaker_id": "1",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004613.1.19",
          "form": "비00만두랑 비00떡갈비 맛나요",
          "original_form": "비00만두랑 비00떡갈비 맛나요",
          "speaker_id": "1",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004613.1.20",
          "form": "최근에 비00샀어요",
          "original_form": "최근에 비00샀어요",
          "speaker_id": "1",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004613.1.21",
          "form": "만두는 비00이 최고죠",
          "original_form": "만두는 비00이 최고죠",
          "speaker_id": "1",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004613.1.22",
          "form": "만두 좋아하세요??",
          "original_form": "만두 좋아하세요??",
          "speaker_id": "1",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004613.1.23",
          "form": "만두도 없으면 못살죠",
          "original_form": "만두도 없으면 못살죠",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004613.1.24",
          "form": "김치만두 마니아입니다",
          "original_form": "김치만두 마니아입니다",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004613.1.25",
          "form": "김치만두는 얇은0 00가 최고죵",
          "original_form": "김치만두는 얇은0 00가 최고죵",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004613.1.26",
          "form": "선물받은 닭발도 있는데 저녁 드셧나용?",
          "original_form": "선물받은 닭발도 있는데 저녁 드셧나용?",
          "speaker_id": "1",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004613.1.27",
          "form": "지금 먹으려고 합니다!",
          "original_form": "지금 먹으려고 합니다!",
          "speaker_id": "2",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004613.1.28",
          "form": "맛나게드세용 ㅎ 전 냉동식품 쟁여두려 또 쇼핑하려구요",
          "original_form": "맛나게드세용 ㅎ 전 냉동식품 쟁여두려 또 쇼핑하려구요",
          "speaker_id": "1",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004613.1.29",
          "form": "요즘 소셜 할인이 많네요",
          "original_form": "요즘 소셜 할인이 많네요",
          "speaker_id": "1",
          "time": "20210821 18:19"
        }
      ]
