      "utterance": [
        {
          "id": "MDRW2100011931.1.1",
          "form": "응 좋아해",
          "original_form": "응 좋아해",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011931.1.2",
          "form": "영화 소개 해주는 유튜브 이런 것도 봐?",
          "original_form": "영화 소개 해주는 유튜브 이런 것도 봐?",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011931.1.3",
          "form": "어 가끔?",
          "original_form": "어 가끔?",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011931.1.4",
          "form": "좋은 유투 버 있으면 소개 좀",
          "original_form": "좋은 유투 버 있으면 소개 좀",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011931.1.5",
          "form": "그냥 아무거나 보는데?",
          "original_form": "그냥 아무거나 보는데?",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011931.1.6",
          "form": "아 딱히 구독하는 사람은 없고?",
          "original_form": "아 딱히 구독하는 사람은 없고?",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011931.1.7",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210824 17:33"
        },
        {
          "id": "MDRW2100011931.1.8",
          "form": "응 그렇구나",
          "original_form": "응 그렇구나",
          "speaker_id": "1",
          "time": "20210824 17:33"
        },
        {
          "id": "MDRW2100011931.1.9",
          "form": "자주보는 영상 있어?",
          "original_form": "자주보는 영상 있어?",
          "speaker_id": "2",
          "time": "20210824 17:33"
        },
        {
          "id": "MDRW2100011931.1.10",
          "form": "아니 요즘은 없는 거 같아",
          "original_form": "아니 요즘은 없는 거 같아",
          "speaker_id": "1",
          "time": "20210824 17:34"
        },
        {
          "id": "MDRW2100011931.1.11",
          "form": "그럼 예전에는?",
          "original_form": "그럼 예전에는?",
          "speaker_id": "2",
          "time": "20210824 17:34"
        },
        {
          "id": "MDRW2100011931.1.12",
          "form": "예전에는 이것저것 많이 봤던 거 같은데 잘 생각 안나",
          "original_form": "예전에는 이것저것 많이 봤던 거 같은데 잘 생각 안나",
          "speaker_id": "1",
          "time": "20210824 17:34"
