      "utterance": [
        {
          "id": "MDRW2100022828.1.1",
          "form": "홈쇼핑도 괜찮아요",
          "original_form": "홈쇼핑도 괜찮아요",
          "speaker_id": "1",
          "time": "20210829 18:13"
        },
        {
          "id": "MDRW2100022828.1.2",
          "form": "저희 엄마가 좋아하세요",
          "original_form": "저희 엄마가 좋아하세요",
          "speaker_id": "2",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100022828.1.3",
          "form": "홈쇼핑이 잘만 고르면 좋은 제품이 있어요",
          "original_form": "홈쇼핑이 잘만 고르면 좋은 제품이 있어요",
          "speaker_id": "1",
          "time": "20210829 18:15"
        },
        {
          "id": "MDRW2100022828.1.4",
          "form": "그럴 것 같아요",
          "original_form": "그럴 것 같아요",
          "speaker_id": "2",
          "time": "20210829 18:15"
        },
        {
          "id": "MDRW2100022828.1.5",
          "form": "마진만 많이 남기고 비싸면 고객이 그렇게 많지 않을테니까요",
          "original_form": "마진만 많이 남기고 비싸면 고객이 그렇게 많지 않을테니까요",
          "speaker_id": "2",
          "time": "20210829 18:16"
        },
        {
          "id": "MDRW2100022828.1.6",
          "form": "원가에 비해 매우 비싸게 팔기도 하겠지만",
          "original_form": "원가에 비해 매우 비싸게 팔기도 하겠지만",
          "speaker_id": "1",
          "time": "20210829 18:16"
        },
        {
          "id": "MDRW2100022828.1.7",
          "form": "숨겨진 알짜상품들이 있어서",
          "original_form": "숨겨진 알짜상품들이 있어서",
          "speaker_id": "1",
          "time": "20210829 18:16"
        },
        {
          "id": "MDRW2100022828.1.8",
          "form": "현명히 활용하면 좋답니다",
          "original_form": "현명히 활용하면 좋답니다",
          "speaker_id": "1",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022828.1.9",
          "form": "물건 안사고 호스트말하는거 보고 있어도 웃기던걸요",
          "original_form": "물건 안사고 호스트말하는거 보고 있어도 웃기던걸요",
          "speaker_id": "2",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022828.1.10",
          "form": "어느새 홀려서 주문전화눌러야 할듯한 기분",
          "original_form": "어느새 홀려서 주문전화눌러야 할듯한 기분",
          "speaker_id": "2",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022828.1.11",
          "form": "말솜씨들이 참 대단하죠",
          "original_form": "말솜씨들이 참 대단하죠",
          "speaker_id": "1",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022828.1.12",
          "form": "맞아요 호스트들 말고 개그맨들도 웃기구요",
          "original_form": "맞아요 호스트들 말고 개그맨들도 웃기구요",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022828.1.13",
          "form": "자기 이름 걸고 상품 기획했으니",
          "original_form": "자기 이름 걸고 상품 기획했으니",
          "speaker_id": "1",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022828.1.14",
          "form": "더 열심히 더 웃기게 홍보할거에요",
          "original_form": "더 열심히 더 웃기게 홍보할거에요",
          "speaker_id": "1",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022828.1.15",
          "form": "그쵸ㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022828.1.16",
          "form": "최근에 팽현숙부부가 나온 걸 봤는데",
          "original_form": "최근에 팽현숙부부가 나온 걸 봤는데",
          "speaker_id": "1",
          "time": "20210829 18:21"
        },
        {
          "id": "MDRW2100022828.1.17",
          "form": "최양락씨 부인이죠?",
          "original_form": "최양락씨 부인이죠?",
          "speaker_id": "2",
          "time": "20210829 18:21"
        },
        {
          "id": "MDRW2100022828.1.18",
          "form": "네 음식도 맛깔나게 만들고 홍보도 맛깔나게",
          "original_form": "네 음식도 맛깔나게 만들고 홍보도 맛깔나게",
          "speaker_id": "1",
          "time": "20210829 18:21"
        },
        {
          "id": "MDRW2100022828.1.19",
          "form": "잘 하더라구요",
          "original_form": "잘 하더라구요",
          "speaker_id": "1",
          "time": "20210829 18:22"
        }
