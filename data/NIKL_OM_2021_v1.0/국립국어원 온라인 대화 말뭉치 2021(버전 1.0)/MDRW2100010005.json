      "utterance": [
        {
          "id": "MDRW2100010005.1.1",
          "form": "글타이~혼자 바쁠예정이다ㅠ",
          "original_form": "글타이~혼자 바쁠예정이다ㅠ",
          "speaker_id": "2",
          "time": "20210823 13:07"
        },
        {
          "id": "MDRW2100010005.1.2",
          "form": "응응 ㅜㅜ이번 한주만 힘내자",
          "original_form": "응응 ㅜㅜ이번 한주만 힘내자",
          "speaker_id": "1",
          "time": "20210823 13:10"
        },
        {
          "id": "MDRW2100010005.1.3",
          "form": "다음주에 안바쁠때 커피한잔하자고ㅋ",
          "original_form": "다음주에 안바쁠때 커피한잔하자고ㅋ",
          "speaker_id": "1",
          "time": "20210823 13:10"
        },
        {
          "id": "MDRW2100010005.1.4",
          "form": "그래 그러쟈고^^",
          "original_form": "그래 그러쟈고^^",
          "speaker_id": "2",
          "time": "20210823 13:10"
        },
        {
          "id": "MDRW2100010005.1.5",
          "form": "조금만 있음 일끝나네ㅋ",
          "original_form": "조금만 있음 일끝나네ㅋ",
          "speaker_id": "1",
          "time": "20210823 13:10"
        },
        {
          "id": "MDRW2100010005.1.6",
          "form": "담주엔 학교도 가니 좋구만~",
          "original_form": "담주엔 학교도 가니 좋구만~",
          "speaker_id": "2",
          "time": "20210823 13:10"
        },
        {
          "id": "MDRW2100010005.1.7",
          "form": "50분남았다ㅋ",
          "original_form": "50분남았다ㅋ",
          "speaker_id": "1",
          "time": "20210823 13:10"
        },
        {
          "id": "MDRW2100010005.1.8",
          "form": "일 언제까지고??",
          "original_form": "일 언제까지고??",
          "speaker_id": "2",
          "time": "20210823 13:10"
        },
        {
          "id": "MDRW2100010005.1.9",
          "form": "그러니깐ㅋㅋ",
          "original_form": "그러니깐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:10"
        },
        {
          "id": "MDRW2100010005.1.10",
          "form": "아!!내 일 ㅋㅋㅋㅋ",
          "original_form": "아!!내 일 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:10"
        },
        {
          "id": "MDRW2100010005.1.11",
          "form": "12월까지",
          "original_form": "12월까지",
          "speaker_id": "1",
          "time": "20210823 13:10"
        },
        {
          "id": "MDRW2100010005.1.12",
          "form": "아빠델꼬 병원가야함 ㅋㅋㅋㅋ",
          "original_form": "아빠델꼬 병원가야함 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:11"
        },
        {
          "id": "MDRW2100010005.1.13",
          "form": "더 큰 일이 남았다 ㅋㅋㅋ",
          "original_form": "더 큰 일이 남았다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:11"
        },
        {
          "id": "MDRW2100010005.1.14",
          "form": "아하ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아하ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:11"
        },
        {
          "id": "MDRW2100010005.1.15",
          "form": "우리아버지도 백신맞으셔야되는데",
          "original_form": "우리아버지도 백신맞으셔야되는데",
          "speaker_id": "1",
          "time": "20210823 13:11"
        },
        {
          "id": "MDRW2100010005.1.16",
          "form": "ㅠㅜㅋㅋㅋ",
          "original_form": "ㅠㅜㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:11"
        },
        {
          "id": "MDRW2100010005.1.17",
          "form": "그래도 예약했네ㅋㅋ",
          "original_form": "그래도 예약했네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:11"
        },
        {
          "id": "MDRW2100010005.1.18",
          "form": "아버지 백신맞추고 오늘 내일 좀 쉬게 해드려야겠다.ㅜㅋ",
          "original_form": "아버지 백신맞추고 오늘 내일 좀 쉬게 해드려야겠다.ㅜㅋ",
          "speaker_id": "1",
          "time": "20210823 13:12"
        },
        {
          "id": "MDRW2100010005.1.19",
          "form": "서원이언니 찬스썼다 ㅋㅋㅋㅋㅋ",
          "original_form": "서원이언니 찬스썼다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:12"
        },
        {
          "id": "MDRW2100010005.1.20",
          "form": "잔여 암만 해도 안걸리더라~",
          "original_form": "잔여 암만 해도 안걸리더라~",
          "speaker_id": "2",
          "time": "20210823 13:12"
        },
        {
          "id": "MDRW2100010005.1.21",
          "form": "그러니깐 ㅜ쉽지않다",
          "original_form": "그러니깐 ㅜ쉽지않다",
          "speaker_id": "1",
          "time": "20210823 13:12"
        },
        {
          "id": "MDRW2100010005.1.22",
          "form": "오늘은 울집에 모시고 가야제~밤에 혹시나 열날까봐~",
          "original_form": "오늘은 울집에 모시고 가야제~밤에 혹시나 열날까봐~",
          "speaker_id": "2",
          "time": "20210823 13:13"
        },
        {
          "id": "MDRW2100010005.1.23",
          "form": "나 잠시 집안일좀하고 올게ㅋㅋ",
