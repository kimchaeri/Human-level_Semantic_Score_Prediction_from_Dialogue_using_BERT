      "utterance": [
        {
          "id": "MDRW2100042923.1.1",
          "form": "눈여겨보는거 있으세요?",
          "original_form": "눈여겨보는거 있으세요?",
          "speaker_id": "2",
          "time": "20210907 11:35"
        },
        {
          "id": "MDRW2100042923.1.2",
          "form": "전 원피스ㅋㅋ",
          "original_form": "전 원피스ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:35"
        },
        {
          "id": "MDRW2100042923.1.3",
          "form": "ㄷㅏ들 이쁘다고해줘서ㅎㅎ",
          "original_form": "ㄷㅏ들 이쁘다고해줘서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 11:36"
        },
        {
          "id": "MDRW2100042923.1.4",
          "form": "ㅋㅋㅋㅋ쀼듯하셨겠다",
          "original_form": "ㅋㅋㅋㅋ쀼듯하셨겠다",
          "speaker_id": "1",
          "time": "20210907 11:36"
        },
        {
          "id": "MDRW2100042923.1.5",
          "form": "저는 지금 날씨 쌀쌀해지니까",
          "original_form": "저는 지금 날씨 쌀쌀해지니까",
          "speaker_id": "1",
          "time": "20210907 11:36"
        },
        {
          "id": "MDRW2100042923.1.6",
          "form": "후리스 보고있어요",
          "original_form": "후리스 보고있어요",
          "speaker_id": "1",
          "time": "20210907 11:36"
        },
        {
          "id": "MDRW2100042923.1.7",
          "form": "파타고니아꺼",
          "original_form": "파타고니아꺼",
          "speaker_id": "1",
          "time": "20210907 11:36"
        },
        {
          "id": "MDRW2100042923.1.8",
          "form": "후리스 진짜 편해요",
          "original_form": "후리스 진짜 편해요",
          "speaker_id": "2",
          "time": "20210907 11:38"
        },
        {
          "id": "MDRW2100042923.1.9",
          "form": "맞아요 휘뚜루마뚜루 입으려구요",
          "original_form": "맞아요 휘뚜루마뚜루 입으려구요",
          "speaker_id": "1",
          "time": "20210907 11:38"
        },
        {
          "id": "MDRW2100042923.1.10",
          "form": "저도 작년에 지오다노꺼였나 뽀글이사서 뽕뺐어요",
          "original_form": "저도 작년에 지오다노꺼였나 뽀글이사서 뽕뺐어요",
          "speaker_id": "2",
          "time": "20210907 11:38"
        },
        {
          "id": "MDRW2100042923.1.11",
          "form": "오 지오다노 좋져",
          "original_form": "오 지오다노 좋져",
          "speaker_id": "1",
          "time": "20210907 11:39"
        },
        {
          "id": "MDRW2100042923.1.12",
          "form": "네 이월상품이라 가격도 되게 착했어요!",
          "original_form": "네 이월상품이라 가격도 되게 착했어요!",
          "speaker_id": "2",
          "time": "20210907 11:46"
        },
        {
          "id": "MDRW2100042923.1.13",
          "form": "맞아요 원래도  지오다노 가격도 착하잖아요",
          "original_form": "맞아요 원래도  지오다노 가격도 착하잖아요",
          "speaker_id": "1",
          "time": "20210907 12:39"
        },
        {
          "id": "MDRW2100042923.1.14",
          "form": "올햐도 뽕뽑으실건가요 ㅋㅋㅋㅋ",
          "original_form": "올햐도 뽕뽑으실건가요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:39"
        },
        {
          "id": "MDRW2100042923.1.15",
          "form": "네! ㅋㅋ 충분합니다ㅋㅋ",
          "original_form": "네! ㅋㅋ 충분합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:39"
        },
        {
          "id": "MDRW2100042923.1.16",
          "form": "무슨색 사실거에요?",
          "original_form": "무슨색 사실거에요?",
          "speaker_id": "2",
          "time": "20210907 12:40"
        },
        {
          "id": "MDRW2100042923.1.17",
          "form": "저 지금 청록색이랑 아이보리중에",
          "original_form": "저 지금 청록색이랑 아이보리중에",
          "speaker_id": "1",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100042923.1.18",
          "form": "고민입니다",
          "original_form": "고민입니다",
          "speaker_id": "1",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100042923.1.19",
          "form": "무난은 아이보리긴한데 제가 또 청록색 그린색 진짜 좋아하거든요...",
          "original_form": "무난은 아이보리긴한데 제가 또 청록색 그린색 진짜 좋아하거든요...",
          "speaker_id": "1",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100042923.1.20",
          "form": "그치만 사면 잘 입을지 고민중입니다",
          "original_form": "그치만 사면 잘 입을지 고민중입니다",
          "speaker_id": "1",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100042923.1.21",
          "form": "소매 때 낄수있으니 청록!!!",
          "original_form": "소매 때 낄수있으니 청록!!!",
          "speaker_id": "2",
          "time": "20210907 14:05"
        },
        {
          "id": "MDRW2100042923.1.22",
          "form": "추천입ㄴ다ㅋㅋ",
          "original_form": "추천입ㄴ다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:05"
        },
        {
          "id": "MDRW2100042923.1.23",
          "form": "하하 그죠?청록 잘입고 다니겠죠? ㅎㅎ",
          "original_form": "하하 그죠?청록 잘입고 다니겠죠? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100042923.1.24",
          "form": "(답정너",
          "original_form": "(답정너",
          "speaker_id": "1",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100042923.1.25",
          "form": "넵 너무 이쁠거같아요!!!",
          "original_form": "넵 너무 이쁠거같아요!!!",
          "speaker_id": "2",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100042923.1.26",
          "form": "전 핑크예요ㅋㅋ",
          "original_form": "전 핑크예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100042923.1.27",
          "form": "핑크도 예쁘죠!!",
          "original_form": "핑크도 예쁘죠!!",
