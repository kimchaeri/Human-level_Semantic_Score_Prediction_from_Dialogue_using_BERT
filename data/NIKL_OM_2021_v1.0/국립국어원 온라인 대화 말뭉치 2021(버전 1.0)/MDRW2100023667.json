        },
        {
          "id": "MDRW2100023667.1.2",
          "form": "남자들이",
          "original_form": "남자들이",
          "speaker_id": "1",
          "time": "20210830 14:09"
        },
        {
          "id": "MDRW2100023667.1.3",
          "form": "걔한테이입많이하잖아",
          "original_form": "걔한테이입많이하잖아",
          "speaker_id": "1",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023667.1.4",
          "form": "자기하고비슷해서",
          "original_form": "자기하고비슷해서",
          "speaker_id": "1",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023667.1.5",
          "form": "어휴",
          "original_form": "어휴",
          "speaker_id": "2",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023667.1.6",
          "form": "근데진심",
          "original_form": "근데진심",
          "speaker_id": "1",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023667.1.7",
          "form": "정석이긴함",
          "original_form": "정석이긴함",
          "speaker_id": "1",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023667.1.8",
          "form": "그저",
          "original_form": "그저",
          "speaker_id": "1",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023667.1.9",
          "form": "그자체",
          "original_form": "그자체",
          "speaker_id": "1",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023667.1.10",
          "form": "그니까 잘 하는 애들이 나와서 좀 바꿔라 자기 관리 잘하려고 보이는 애들로",
          "original_form": "그니까 잘 하는 애들이 나와서 좀 바꿔라 자기 관리 잘하려고 보이는 애들로",
          "speaker_id": "2",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100023667.1.11",
          "form": "그렇게 미디어가 비춰주는 걸로 바꿔야 함",
          "original_form": "그렇게 미디어가 비춰주는 걸로 바꿔야 함",
          "speaker_id": "2",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100023667.1.12",
          "form": "내말이",
          "original_form": "내말이",
          "speaker_id": "1",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100023667.1.13",
          "form": "그런애들보여주면",
          "original_form": "그런애들보여주면",
          "speaker_id": "1",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100023667.1.14",
          "form": "여자들이",
          "original_form": "여자들이",
          "speaker_id": "1",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100023667.1.15",
          "form": "하향결혼할거라고",
          "original_form": "하향결혼할거라고",
          "speaker_id": "1",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100023667.1.16",
          "form": "생각하는데",
          "original_form": "생각하는데",
          "speaker_id": "1",
          "time": "20210830 14:12"
        },
        {
          "id": "MDRW2100023667.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:12"
        },
        {
          "id": "MDRW2100023667.1.18",
          "form": "ㄱㄴㄲ",
          "original_form": "ㄱㄴㄲ",
          "speaker_id": "2",
          "time": "20210830 14:12"
        },
        {
          "id": "MDRW2100023667.1.19",
          "form": "그냥비혼만늘어날뿐",
          "original_form": "그냥비혼만늘어날뿐",
          "speaker_id": "1",
          "time": "20210830 14:12"
        },
        {
          "id": "MDRW2100023667.1.20",
          "form": "일단 그런 애들이 나와서 좀 그런 애들이 정상이구나 원래 해야할 일이구나로 바꿔야 함",
          "original_form": "일단 그런 애들이 나와서 좀 그런 애들이 정상이구나 원래 해야할 일이구나로 바꿔야 함",
          "speaker_id": "2",
          "time": "20210830 14:12"
        },
        {
          "id": "MDRW2100023667.1.21",
          "form": "그래야 자기관리 하고 집안일 반반한다고 올려치는 일이 없어지지",
          "original_form": "그래야 자기관리 하고 집안일 반반한다고 올려치는 일이 없어지지",
          "speaker_id": "2",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023667.1.22",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023667.1.23",
          "form": "솔직히",
          "original_form": "솔직히",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023667.1.24",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023667.1.25",
          "form": "1호가필요해",
          "original_form": "1호가필요해",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023667.1.26",
          "form": "이런거나오는데",
          "original_form": "이런거나오는데",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023667.1.27",
          "form": "맨날여자만집안일하고",
          "original_form": "맨날여자만집안일하고",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023667.1.28",
          "form": "장도연 없었으면 큰일났음",
          "original_form": "장도연 없었으면 큰일났음",
          "speaker_id": "2",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023667.1.29",
          "form": "그니가",
          "original_form": "그니가",
          "speaker_id": "2",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023667.1.30",
          "form": "그런거나오고",
          "original_form": "그런거나오고",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023667.1.31",
          "form": "어쩌다",
          "original_form": "어쩌다",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023667.1.32",
          "form": "최수종?",
          "original_form": "최수종?",
          "speaker_id": "1",
          "time": "20210830 14:14"
        },
        {
          "id": "MDRW2100023667.1.33",
          "form": "그사람나오면",
          "original_form": "그사람나오면",
          "speaker_id": "1",
          "time": "20210830 14:14"
        },
        {
          "id": "MDRW2100023667.1.34",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210830 14:14"
        },
        {
          "id": "MDRW2100023667.1.35",
          "form": "엄청대단하다고",
          "original_form": "엄청대단하다고",
          "speaker_id": "1",
          "time": "20210830 14:14"
        },
        {
          "id": "MDRW2100023667.1.36",
          "form": "해주고",
          "original_form": "해주고",
          "speaker_id": "1",
          "time": "20210830 14:14"
        },
        {
          "id": "MDRW2100023667.1.37",
          "form": "그게말이됨,",
          "original_form": "그게말이됨,",
          "speaker_id": "1",
          "time": "20210830 14:14"
        },
        {
          "id": "MDRW2100023667.1.38",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210830 14:14"
        },
        {
          "id": "MDRW2100023667.1.39",
          "form": "당연한거지",
          "original_form": "당연한거지",
          "speaker_id": "1",
          "time": "20210830 14:14"
        },
        {
          "id": "MDRW2100023667.1.40",
          "form": "ㄱㄴㄲ 아니 서로 사랑하려고 결혼했으면 다 같이 나누는게 당연한가지",
          "original_form": "ㄱㄴㄲ 아니 서로 사랑하려고 결혼했으면 다 같이 나누는게 당연한가지",
          "speaker_id": "2",
          "time": "20210830 14:14"
        },
        {
          "id": "MDRW2100023667.1.41",
          "form": "결혼하고 변한 남편 많다 하하하",
          "original_form": "결혼하고 변한 남편 많다 하하하",
          "speaker_id": "2",
          "time": "20210830 14:15"
        }
      ]
