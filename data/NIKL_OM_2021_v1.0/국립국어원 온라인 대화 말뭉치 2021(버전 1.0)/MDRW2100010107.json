        {
          "id": "MDRW2100010107.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210823 14:26"
        },
        {
          "id": "MDRW2100010107.1.2",
          "form": "아뇨ㅠㅠ 잘 키울 자신이 없어서요",
          "original_form": "아뇨ㅠㅠ 잘 키울 자신이 없어서요",
          "speaker_id": "2",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010107.1.3",
          "form": "name1님은 키우세요?",
          "original_form": "&name1&님은 키우세요?",
          "speaker_id": "2",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010107.1.4",
          "form": "저도 여건이.",
          "original_form": "저도 여건이.",
          "speaker_id": "1",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010107.1.5",
          "form": "키워보고 싶은 동물 있어요?",
          "original_form": "키워보고 싶은 동물 있어요?",
          "speaker_id": "2",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100010107.1.6",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100010107.1.7",
          "form": "저두요 전 대형견으로 마당에서 키우고 싶어요",
          "original_form": "저두요 전 대형견으로 마당에서 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100010107.1.8",
          "form": "저도 마당만 있으면",
          "original_form": "저도 마당만 있으면",
          "speaker_id": "1",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100010107.1.9",
          "form": "조금 색다르게 거북이도 키워보고 싶어요",
          "original_form": "조금 색다르게 거북이도 키워보고 싶어요",
          "speaker_id": "2",
          "time": "20210823 14:33"
        },
        {
          "id": "MDRW2100010107.1.10",
          "form": "어렵지 않나요?",
          "original_form": "어렵지 않나요?",
          "speaker_id": "1",
          "time": "20210823 17:05"
        },
        {
          "id": "MDRW2100010107.1.11",
          "form": "까다롭진 않은 것 같더라구요 손도 덜가구",
          "original_form": "까다롭진 않은 것 같더라구요 손도 덜가구",
          "speaker_id": "2",
          "time": "20210824 12:27"
        },
        {
          "id": "MDRW2100010107.1.12",
          "form": "물관리 힘들잖아요",
          "original_form": "물관리 힘들잖아요",
          "speaker_id": "1",
          "time": "20210824 12:44"
        },
        {
          "id": "MDRW2100010107.1.13",
          "form": "매일 포유류 동물 관리하는 것보다는 나을 것 같아서요 ㅋㅋ",
