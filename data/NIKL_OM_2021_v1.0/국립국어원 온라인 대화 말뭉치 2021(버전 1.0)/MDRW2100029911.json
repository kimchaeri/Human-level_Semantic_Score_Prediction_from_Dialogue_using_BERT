      "utterance": [
        {
          "id": "MDRW2100029911.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029911.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029911.1.3",
          "form": "독서 많이해요?",
          "original_form": "독서 많이해요?",
          "speaker_id": "2",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029911.1.4",
          "form": "1년 넘게 안하고 있습니다...ㅋㅋ",
          "original_form": "1년 넘게 안하고 있습니다...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100029911.1.5",
          "form": "문제집도 독서로 쳐주시나요",
          "original_form": "문제집도 독서로 쳐주시나요",
          "speaker_id": "1",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100029911.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ문제집은 푸은거라 독서에사 조금 거리감 있는거 아닌가요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ문제집은 푸은거라 독서에사 조금 거리감 있는거 아닌가요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100029911.1.7",
          "form": "어떤 문제집이요ㅋㅋㅋ문학 독해지문이 많으면 준독서죠? ㅋㅋㅋ",
          "original_form": "어떤 문제집이요ㅋㅋㅋ문학 독해지문이 많으면 준독서죠? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029911.1.8",
          "form": "문학도 풀었으니 준독서 성공했네요ㅋㅋ",
          "original_form": "문학도 풀었으니 준독서 성공했네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029911.1.9",
          "form": "ㅋㅋㅋㅋ저는 평소때는 읽으려고 사긴 많이 샀는데",
          "original_form": "ㅋㅋㅋㅋ저는 평소때는 읽으려고 사긴 많이 샀는데",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029911.1.10",
          "form": "못읽고 방치네요ㅜ",
          "original_form": "못읽고 방치네요ㅜ",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029911.1.11",
          "form": "많이 바쁘신가봐요",
          "original_form": "많이 바쁘신가봐요",
          "speaker_id": "1",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100029911.1.12",
          "form": "ㅋㅋㅋ음 바쁘다기 보단 놀시간은 없고 책 볼 시간은 부족한..",
          "original_form": "ㅋㅋㅋ음 바쁘다기 보단 놀시간은 없고 책 볼 시간은 부족한..",
          "speaker_id": "2",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100029911.1.13",
          "form": "그런 느낌이죠ㅋㅋㅋㅋ",
          "original_form": "그런 느낌이죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100029911.1.14",
          "form": "그래도 최근에 독서록 꾸준히 쓰기 이런거 한번 챌린지 도전해봐서",
          "original_form": "그래도 최근에 독서록 꾸준히 쓰기 이런거 한번 챌린지 도전해봐서",
          "speaker_id": "2",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100029911.1.15",
          "form": "그나마 읽었어요",
          "original_form": "그나마 읽었어요",
          "speaker_id": "2",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100029911.1.16",
          "form": "와 멋있으세요",
          "original_form": "와 멋있으세요",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100029911.1.17",
          "form": "독서록 쓰기 챌린지라니",
          "original_form": "독서록 쓰기 챌린지라니",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100029911.1.18",
          "form": "ㅋㅋㅋㅋㅋCJ에서 했던건데ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋCJ에서 했던건데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100029911.1.19",
          "form": "간단하게 한줄평 이런거 남기는거라",
          "original_form": "간단하게 한줄평 이런거 남기는거라",
          "speaker_id": "2",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100029911.1.20",
          "form": "편하게 했어요ㅋㅋㅋ명언집 같은거읽구",
          "original_form": "편하게 했어요ㅋㅋㅋ명언집 같은거읽구",
          "speaker_id": "2",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100029911.1.21",
          "form": "아하 그런게 있었군요",
          "original_form": "아하 그런게 있었군요",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100029911.1.22",
          "form": "네네ㅋㅋ",
          "original_form": "네네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100029911.1.23",
          "form": "사장님, 5시에 퇴근하겠습니다라는 책은",
          "original_form": "사장님, 5시에 퇴근하겠습니다라는 책은",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100029911.1.24",
          "form": "회사 도서지원비러 구매해서",
          "original_form": "회사 도서지원비러 구매해서",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100029911.1.25",
          "form": "열심히 읽었죠",
          "original_form": "열심히 읽었죠",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100029911.1.26",
          "form": "지원을 많이 해주나봐요",
          "original_form": "지원을 많이 해주나봐요",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100029911.1.27",
          "form": "작년까지만 해도 지원이 많았어요",
          "original_form": "작년까지만 해도 지원이 많았어요",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100029911.1.28",
          "form": "도서구입비로 한두권씩은 편하개 가능해서ㅋㅋㅋ",
          "original_form": "도서구입비로 한두권씩은 편하개 가능해서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100029911.1.29",
          "form": "대신 엑셀 워드 같은 책도 많이 읽었죠",
          "original_form": "대신 엑셀 워드 같은 책도 많이 읽었죠",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100029911.1.30",
          "form": "그런 책도 중요하죠",
          "original_form": "그런 책도 중요하죠",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100029911.1.31",
          "form": "자기 계발에 필요한 그런 책",
          "original_form": "자기 계발에 필요한 그런 책",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100029911.1.32",
          "form": "맞아요ㅋㅋㅋㅋ시집도 몇권 사서 봤어요",
          "original_form": "맞아요ㅋㅋㅋㅋ시집도 몇권 사서 봤어요",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100029911.1.33",
          "form": "그냥 짧은 글들니 부담 없고 편하게 읽히기도 하는것 같아요",
          "original_form": "그냥 짧은 글들니 부담 없고 편하게 읽히기도 하는것 같아요",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100029911.1.34",
          "form": "틈 날때 읽어도 이어지는게 없으니 좋죠",
          "original_form": "틈 날때 읽어도 이어지는게 없으니 좋죠",
          "speaker_id": "1",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100029911.1.35",
          "form": "맞아요 그래서 잘 안읽힐 땐 짧안 거라도 달고있알 버릇해야겠다 생각하고 있었어요ㅋㅋ",
          "original_form": "맞아요 그래서 잘 안읽힐 땐 짧안 거라도 달고있알 버릇해야겠다 생각하고 있었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100029911.1.36",
          "form": "ㅎㅎ좋네요",
          "original_form": "ㅎㅎ좋네요",
          "speaker_id": "1",
          "time": "20210904 00:59"
        }
      ]
    }
  ]
