      "utterance": [
        {
          "id": "MDRW2100024819.1.1",
          "form": "name2 저녁 먹었어?",
          "original_form": "&name2& 저녁 먹었어?",
          "speaker_id": "1",
          "time": "20210831 19:08"
        },
        {
          "id": "MDRW2100024819.1.2",
          "form": "넘 늦게 봤당...",
          "original_form": "넘 늦게 봤당...",
          "speaker_id": "2",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100024819.1.3",
          "form": "아까 언니가 보낼땐 안먹었고 지금은 먹었는데",
          "original_form": "아까 언니가 보낼땐 안먹었고 지금은 먹었는데",
          "speaker_id": "2",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100024819.1.4",
          "form": "밥먹었더니 영 소화가 안되는거 같다...",
          "original_form": "밥먹었더니 영 소화가 안되는거 같다...",
          "speaker_id": "2",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100024819.1.5",
          "form": "저런...",
          "original_form": "저런...",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024819.1.6",
          "form": "난 쌀국수 먹엇어...",
          "original_form": "난 쌀국수 먹엇어...",
          "speaker_id": "1",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100024819.1.7",
          "form": "속 안좋으면 국물있는거 먹지",
          "original_form": "속 안좋으면 국물있는거 먹지",
          "speaker_id": "1",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100024819.1.8",
          "form": "그럴걸 그랬나봐 국물 먹을걸...",
          "original_form": "그럴걸 그랬나봐 국물 먹을걸...",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024819.1.9",
          "form": "내일은 함 보고 굶던가.. 국물먹던가...",
          "original_form": "내일은 함 보고 굶던가.. 국물먹던가...",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024819.1.10",
          "form": "ㅇㅇ 뜨끈한게 속 진정시키는덴 좋아",
          "original_form": "ㅇㅇ 뜨끈한게 속 진정시키는덴 좋아",
          "speaker_id": "1",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024819.1.11",
          "form": "해야지...",
          "original_form": "해야지...",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024819.1.12",
          "form": "ㅇㅇ...",
          "original_form": "ㅇㅇ...",
          "speaker_id": "1",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024819.1.13",
          "form": "ㅇㅇ ㅠㅠ",
          "original_form": "ㅇㅇ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024819.1.14",
          "form": "소화가 영 안되는 그런 느낌 ㅠ",
          "original_form": "소화가 영 안되는 그런 느낌 ㅠ",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024819.1.15",
          "form": "아예 밥을 굶어야 하나 싶고 흑흑",
          "original_form": "아예 밥을 굶어야 하나 싶고 흑흑",
          "speaker_id": "2",
          "time": "20210901 08:44"
        },
        {
          "id": "MDRW2100024819.1.16",
          "form": "아침?",
          "original_form": "아침?",
          "speaker_id": "1",
          "time": "20210901 08:44"
        },
        {
          "id": "MDRW2100024819.1.17",
          "form": "왜 이거 못 봤지",
          "original_form": "왜 이거 못 봤지",
          "speaker_id": "2",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100024819.1.18",
          "form": "아침에 간단하게 요구르트 먹고 탈났었나봐 오전동안 고생했다....",
          "original_form": "아침에 간단하게 요구르트 먹고 탈났었나봐 오전동안 고생했다....",
          "speaker_id": "2",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100024819.1.19",
          "form": "헐 요구르트 먹었는데도 탈났어?",
          "original_form": "헐 요구르트 먹었는데도 탈났어?",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100024819.1.20",
          "form": "요구르트가 효과가 좋은건가..? ㅋ큐ㅠㅠ 장을 비워냈어..ㅋㅋ큐ㅠㅠ",
          "original_form": "요구르트가 효과가 좋은건가..? ㅋ큐ㅠㅠ 장을 비워냈어..ㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100024819.1.21",
          "form": "아닠ㅋㅋㅋㅋ 효과가 좋다고 해야해 말아야해...",
          "original_form": "아닠ㅋㅋㅋㅋ 효과가 좋다고 해야해 말아야해...",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100024819.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ 나도 모르겠어... \r\n더러운 이야기이지만 설사했는데 이걸 좋아해야해..말아야해......",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 나도 모르겠어... \r\n더러운 이야기이지만 설사했는데 이걸 좋아해야해..말아야해......",
          "speaker_id": "2",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100024819.1.23",
          "form": "그치만 정말 몸상태 쫌 나아진",
          "original_form": "그치만 정말 몸상태 쫌 나아진",
          "speaker_id": "2",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100024819.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100024819.1.25",
          "form": "나아졌으면 됐지",
          "original_form": "나아졌으면 됐지",
          "speaker_id": "1",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100024819.1.26",
          "form": "저녁에 죽먹거나",
          "original_form": "저녁에 죽먹거나",
          "speaker_id": "1",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100024819.1.27",
          "form": "아님 따뜻한거 먹어",
          "original_form": "아님 따뜻한거 먹어",
          "speaker_id": "1",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100024819.1.28",
          "form": "ㅋㅋㅋㅋㅋ 응응 그러려고 그게 나을듯.. 내가 이렇게꺼디 아플 일인가 싶다",
          "original_form": "ㅋㅋㅋㅋㅋ 응응 그러려고 그게 나을듯.. 내가 이렇게꺼디 아플 일인가 싶다",
          "speaker_id": "2",
          "time": "20210901 17:28"
        }
      ]
