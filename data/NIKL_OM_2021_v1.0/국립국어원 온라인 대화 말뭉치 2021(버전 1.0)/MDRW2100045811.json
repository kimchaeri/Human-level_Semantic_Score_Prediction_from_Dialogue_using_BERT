      "utterance": [
        {
          "id": "MDRW2100045811.1.1",
          "form": "지금 문득 생각나는 음식 있으세요?",
          "original_form": "지금 문득 생각나는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045811.1.2",
          "form": "새우장이요!",
          "original_form": "새우장이요!",
          "speaker_id": "2",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100045811.1.3",
          "form": "여수갔을때 게장백반집에서 딱새우장을 줘서 처음 먹어봤거든요",
          "original_form": "여수갔을때 게장백반집에서 딱새우장을 줘서 처음 먹어봤거든요",
          "speaker_id": "2",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100045811.1.4",
          "form": "헉 맛있겠다.....",
          "original_form": "헉 맛있겠다.....",
          "speaker_id": "1",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100045811.1.5",
          "form": "이게 무슨 생물인가 싶었는데 너무 맛있어서 가끔 생각나욬ㅋㅋㅋ",
          "original_form": "이게 무슨 생물인가 싶었는데 너무 맛있어서 가끔 생각나욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100045811.1.6",
          "form": "일반 새우랑 딱새우랑 달라요?",
          "original_form": "일반 새우랑 딱새우랑 달라요?",
          "speaker_id": "1",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100045811.1.7",
          "form": "모양이 좀 다르더라구요!",
          "original_form": "모양이 좀 다르더라구요!",
          "speaker_id": "2",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100045811.1.8",
          "form": "둥글둥글하게 생겼어요!",
          "original_form": "둥글둥글하게 생겼어요!",
          "speaker_id": "2",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100045811.1.9",
          "form": "근본적으로 맛이 엄청 다른건가? 싶긴한데",
          "original_form": "근본적으로 맛이 엄청 다른건가? 싶긴한데",
          "speaker_id": "2",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100045811.1.10",
          "form": "여수에서 먹어서 그런가 너무 맛있었어요...",
          "original_form": "여수에서 먹어서 그런가 너무 맛있었어요...",
          "speaker_id": "2",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100045811.1.11",
          "form": "우오.. 저도 먹어보고싶네요",
          "original_form": "우오.. 저도 먹어보고싶네요",
          "speaker_id": "1",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100045811.1.12",
          "form": "name2님 말 들으니까 급 먹고싶어진 요리가 있는데 맞춰보실래요?",
          "original_form": "&name2&님 말 들으니까 급 먹고싶어진 요리가 있는데 맞춰보실래요?",
          "speaker_id": "1",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045811.1.13",
          "form": "오 맞춰볼래요!",
          "original_form": "오 맞춰볼래요!",
          "speaker_id": "2",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100045811.1.14",
          "form": "한식인가요?",
          "original_form": "한식인가요?",
          "speaker_id": "2",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100045811.1.15",
          "form": "네 한식입니당",
          "original_form": "네 한식입니당",
          "speaker_id": "1",
          "time": "20210908 07:41"
        },
        {
          "id": "MDRW2100045811.1.16",
          "form": "국물이 있는 요리인가요?",
          "original_form": "국물이 있는 요리인가요?",
          "speaker_id": "2",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100045811.1.17",
          "form": "으음 국물이 있긴 한데 그걸 마구 퍼먹진 않아요",
          "original_form": "으음 국물이 있긴 한데 그걸 마구 퍼먹진 않아요",
          "speaker_id": "1",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045811.1.18",
          "form": "밥도 안비벼먹나요?",
          "original_form": "밥도 안비벼먹나요?",
          "speaker_id": "2",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100045811.1.19",
          "form": "음 순수하게 국물에 말아멱진 않고 들어가는 재료에 비벼먹어요!",
          "original_form": "음 순수하게 국물에 말아멱진 않고 들어가는 재료에 비벼먹어요!",
          "speaker_id": "1",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100045811.1.20",
          "form": "불고기같은걸까요",
          "original_form": "불고기같은걸까요",
          "speaker_id": "2",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100045811.1.21",
          "form": "불고기 낙지 갈비 이 정도 생각나네욬ㅋㅋ큐ㅠ",
          "original_form": "불고기 낙지 갈비 이 정도 생각나네욬ㅋㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210908 07:54"
        }
      ]
