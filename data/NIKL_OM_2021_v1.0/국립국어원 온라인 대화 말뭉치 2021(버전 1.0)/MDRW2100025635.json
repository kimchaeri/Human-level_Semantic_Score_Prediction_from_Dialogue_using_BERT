      "utterance": [
        {
          "id": "MDRW2100025635.1.1",
          "form": "안녕하세요 무슨 동물좋아해요",
          "original_form": "안녕하세요 무슨 동물좋아해요",
          "speaker_id": "1",
          "time": "20210901 14:47"
        },
        {
          "id": "MDRW2100025635.1.2",
          "form": "반려동물 뭐 좋아하세요?",
          "original_form": "반려동물 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 14:47"
        },
        {
          "id": "MDRW2100025635.1.3",
          "form": "전 고양이요!",
          "original_form": "전 고양이요!",
          "speaker_id": "2",
          "time": "20210901 14:47"
        },
        {
          "id": "MDRW2100025635.1.4",
          "form": "고양이좋아하시는구나",
          "original_form": "고양이좋아하시는구나",
          "speaker_id": "1",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100025635.1.5",
          "form": "어떤고양이좋아허세요",
          "original_form": "어떤고양이좋아허세요",
          "speaker_id": "1",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100025635.1.6",
          "form": "음 종류 구분 하지 않고 다 좋아요!",
          "original_form": "음 종류 구분 하지 않고 다 좋아요!",
          "speaker_id": "2",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100025635.1.7",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100025635.1.8",
          "form": "이름은 생각한거있어요?",
          "original_form": "이름은 생각한거있어요?",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100025635.1.9",
          "form": "ㅋㅋㅋㅋㅋ 이름..음",
          "original_form": "ㅋㅋㅋㅋㅋ 이름..음",
          "speaker_id": "2",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100025635.1.10",
          "form": "아뇨 딱히 키울생각을 안해봐서",
          "original_form": "아뇨 딱히 키울생각을 안해봐서",
          "speaker_id": "2",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100025635.1.11",
          "form": "없네요 ㅠ",
          "original_form": "없네요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100025635.1.12",
          "form": "님도 고양인가요?",
          "original_form": "님도 고양인가요?",
          "speaker_id": "2",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100025635.1.13",
          "form": "키워요?????",
          "original_form": "키워요?????",
          "speaker_id": "2",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100025635.1.14",
          "form": "아뇨 안키워요",
          "original_form": "아뇨 안키워요",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100025635.1.15",
          "form": "개도 좋아해요",
          "original_form": "개도 좋아해요",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100025635.1.16",
          "form": "오오 좋아하는 종이 있나요?",
          "original_form": "오오 좋아하는 종이 있나요?",
          "speaker_id": "2",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100025635.1.17",
          "form": "음..좀 늑대같이생긴애들이나 귀여운애들이 좋은거같아요",
          "original_form": "음..좀 늑대같이생긴애들이나 귀여운애들이 좋은거같아요",
          "speaker_id": "1",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100025635.1.18",
          "form": "퍼그나 불독은 별로..",
          "original_form": "퍼그나 불독은 별로..",
          "speaker_id": "1",
          "time": "20210901 14:52"
        },
        {
          "id": "MDRW2100025635.1.19",
          "form": "늑대같이 ㅋㅋㅋㅋㅋ 귀엽죠 귀엽죠!!",
          "original_form": "늑대같이 ㅋㅋㅋㅋㅋ 귀엽죠 귀엽죠!!",
          "speaker_id": "2",
          "time": "20210901 14:52"
        },
        {
          "id": "MDRW2100025635.1.20",
          "form": "이쁜애들을 좋아하시네요 ㅎㅎ",
          "original_form": "이쁜애들을 좋아하시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 14:52"
        },
        {
          "id": "MDRW2100025635.1.21",
          "form": "그런가요ㅎㅎ",
          "original_form": "그런가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025635.1.22",
          "form": "미래에 키울 계획은 있으신가요?",
          "original_form": "미래에 키울 계획은 있으신가요?",
          "speaker_id": "2",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025635.1.23",
          "form": "잘모르겠어요 어렵네요",
          "original_form": "잘모르겠어요 어렵네요",
          "speaker_id": "1",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025635.1.24",
          "form": "그쵸 ㅠ 키우는 결심은 어려운거같아요",
          "original_form": "그쵸 ㅠ 키우는 결심은 어려운거같아요",
          "speaker_id": "2",
          "time": "20210901 14:53"
        }
      ]
