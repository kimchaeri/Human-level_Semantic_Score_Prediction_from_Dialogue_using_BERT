      "utterance": [
        {
          "id": "MDRW2100012784.1.1",
          "form": "인천 차이나타운 유명하잖아",
          "original_form": "인천 차이나타운 유명하잖아",
          "speaker_id": "1",
          "time": "20210825 01:10"
        },
        {
          "id": "MDRW2100012784.1.2",
          "form": "차이나타운은 다 그런듯",
          "original_form": "차이나타운은 다 그런듯",
          "speaker_id": "2",
          "time": "20210825 01:10"
        },
        {
          "id": "MDRW2100012784.1.3",
          "form": "응 부산에도 차이나타운이 있는데 한번 먹으러 가봤어",
          "original_form": "응 부산에도 차이나타운이 있는데 한번 먹으러 가봤어",
          "speaker_id": "1",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100012784.1.4",
          "form": "맛있었어?",
          "original_form": "맛있었어?",
          "speaker_id": "2",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100012784.1.5",
          "form": "줄을 서서 먹었는데 그렇게 맛있는지 모르겠더라",
          "original_form": "줄을 서서 먹었는데 그렇게 맛있는지 모르겠더라",
          "speaker_id": "1",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100012784.1.6",
          "form": "아 그래",
          "original_form": "아 그래",
          "speaker_id": "2",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100012784.1.7",
          "form": "응 거기다가 너무 좁아서 그냥 여행갔다가 한번 들릴만한곳",
          "original_form": "응 거기다가 너무 좁아서 그냥 여행갔다가 한번 들릴만한곳",
          "speaker_id": "1",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100012784.1.8",
          "form": "그렇구나 난 아직 차이나타운은 안가봐서",
          "original_form": "그렇구나 난 아직 차이나타운은 안가봐서",
          "speaker_id": "2",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100012784.1.9",
          "form": "많이 기대하고 가봤는데 그냥 그렇더라",
          "original_form": "많이 기대하고 가봤는데 그냥 그렇더라",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100012784.1.10",
          "form": "기대가 너무 컸나보네",
          "original_form": "기대가 너무 컸나보네",
          "speaker_id": "2",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100012784.1.11",
          "form": "그런가 ㅋㅋ",
          "original_form": "그런가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100012784.1.12",
          "form": "그렇지 기대하고 가면 실망도 크지",
          "original_form": "그렇지 기대하고 가면 실망도 크지",
          "speaker_id": "2",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100012784.1.13",
          "form": "그럴수도 있겠다",
          "original_form": "그럴수도 있겠다",
