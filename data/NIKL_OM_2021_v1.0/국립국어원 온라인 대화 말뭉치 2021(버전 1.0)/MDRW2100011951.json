      "utterance": [
        {
          "id": "MDRW2100011951.1.1",
          "form": "아이스크림 좋아하세요?",
          "original_form": "아이스크림 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011951.1.2",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011951.1.3",
          "form": "어제도 먹었어요",
          "original_form": "어제도 먹었어요",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011951.1.4",
          "form": "어떤 아이스크림 좋아하세요",
          "original_form": "어떤 아이스크림 좋아하세요",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011951.1.5",
          "form": "요거트맛을 좋아해요",
          "original_form": "요거트맛을 좋아해요",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011951.1.6",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011951.1.7",
          "form": "짱좋아해요",
          "original_form": "짱좋아해요",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011951.1.8",
          "form": "와 정말요!!",
          "original_form": "와 정말요!!",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011951.1.9",
          "form": "베라가서도 요거트는 꼭 먹어요",
          "original_form": "베라가서도 요거트는 꼭 먹어요",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011951.1.10",
          "form": "배스킨라빈스 가면 요거트만",
          "original_form": "배스킨라빈스 가면 요거트만",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011951.1.11",
          "form": "어",
          "original_form": "어",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011951.1.12",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011951.1.13",
          "form": "앗ㅋㅋㅋㅎㅎ",
          "original_form": "앗ㅋㅋㅋㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011951.1.14",
          "form": "약간 요거트의 상큼함이",
          "original_form": "약간 요거트의 상큼함이",
          "speaker_id": "1",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011951.1.15",
          "form": "너무 좋아요",
          "original_form": "너무 좋아요",
          "speaker_id": "1",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011951.1.16",
          "form": "그쵸ㅜㅜ처음 먹었을때부터 쭉 요거트 사랑맨이에요",
          "original_form": "그쵸ㅜㅜ처음 먹었을때부터 쭉 요거트 사랑맨이에요",
          "speaker_id": "2",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011951.1.17",
          "form": "그렇다면 이쯤에서 물어보는 뻔한 질문",
          "original_form": "그렇다면 이쯤에서 물어보는 뻔한 질문",
          "speaker_id": "1",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011951.1.18",
          "form": "다른 맛은 뭐 좋아하세요?",
          "original_form": "다른 맛은 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011951.1.19",
          "form": "민트 초코 좋아하세요?",
          "original_form": "민트 초코 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011951.1.20",
          "form": "ㅋㅋㅋㅋㅋ저는 좋아해요!",
          "original_form": "ㅋㅋㅋㅋㅋ저는 좋아해요!",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011951.1.21",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011951.1.22",
          "form": "우리잠깐 멀어져요",
          "original_form": "우리잠깐 멀어져요",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011951.1.23",
          "form": "띠로리~ㅜㅜ",
          "original_form": "띠로리~ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011951.1.24",
          "form": "기껏 요거트로 가까워졌는데",
          "original_form": "기껏 요거트로 가까워졌는데",
