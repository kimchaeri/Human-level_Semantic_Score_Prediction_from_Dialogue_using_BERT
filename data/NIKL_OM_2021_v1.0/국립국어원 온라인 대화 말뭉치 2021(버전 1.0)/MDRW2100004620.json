        },
        {
          "id": "MDRW2100004620.1.10",
          "form": "청정지역이에요",
          "original_form": "청정지역이에요",
          "speaker_id": "1",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004620.1.11",
          "form": "그렇군요. 기회가 된다면 호주도 여행지로 고려해봐야겠어요.",
          "original_form": "그렇군요. 기회가 된다면 호주도 여행지로 고려해봐야겠어요.",
          "speaker_id": "2",
