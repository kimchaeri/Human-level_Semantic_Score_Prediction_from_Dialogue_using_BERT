      "utterance": [
        {
          "id": "MDRW2100030030.1.1",
          "form": "어떤 동호회 활동을 하고 계신가요?",
          "original_form": "어떤 동호회 활동을 하고 계신가요?",
          "speaker_id": "1",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100030030.1.2",
          "form": "저 우쿨렐레해요~",
          "original_form": "저 우쿨렐레해요~",
          "speaker_id": "2",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100030030.1.3",
          "form": "우왕 저는 기타 동아리에 가입한 적이 있는데 손이 작아서 힘들더라고요",
          "original_form": "우왕 저는 기타 동아리에 가입한 적이 있는데 손이 작아서 힘들더라고요",
          "speaker_id": "1",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100030030.1.4",
          "form": "우쿨렐레는 얼마나 배우셨어요?",
          "original_form": "우쿨렐레는 얼마나 배우셨어요?",
          "speaker_id": "1",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100030030.1.5",
          "form": "저도 손 작아요ㅜㅜ",
          "original_form": "저도 손 작아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100030030.1.6",
          "form": "배운진 얼마 안됐어요!!",
          "original_form": "배운진 얼마 안됐어요!!",
          "speaker_id": "2",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100030030.1.7",
          "form": "우쿨렐레는 손이 작아도 칠 만하죠???!ㅎㅎ",
          "original_form": "우쿨렐레는 손이 작아도 칠 만하죠???!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100030030.1.8",
          "form": "네네 칠만해용",
          "original_form": "네네 칠만해용",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100030030.1.9",
          "form": "추천입니당",
          "original_form": "추천입니당",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100030030.1.10",
          "form": "오오 동호회는 혹시 어떻게 구하셨어요?",
          "original_form": "오오 동호회는 혹시 어떻게 구하셨어요?",
          "speaker_id": "1",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100030030.1.11",
          "form": "학원 다니니까 학원쌤이 연결해줬어요!!",
          "original_form": "학원 다니니까 학원쌤이 연결해줬어요!!",
          "speaker_id": "2",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100030030.1.12",
          "form": "아! 코로나 끝나면 도전해 보고 싶어요",
          "original_form": "아! 코로나 끝나면 도전해 보고 싶어요",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100030030.1.13",
          "form": "동호회 활동은 어떠세요?!",
          "original_form": "동호회 활동은 어떠세요?!",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100030030.1.14",
          "form": "재밌어요! 새로운 기분이 들고 기분전환도 돼용",
          "original_form": "재밌어요! 새로운 기분이 들고 기분전환도 돼용",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100030030.1.15",
          "form": "네네! 빨리 코로나 끝나서 모임도 자유로웠으면 좋겠어요 ㅠㅠ",
          "original_form": "네네! 빨리 코로나 끝나서 모임도 자유로웠으면 좋겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100030030.1.16",
          "form": "저두요ㅠㅜ 코로나 끝나면 우쿨렐레 페스티발도 가게용",
          "original_form": "저두요ㅠㅜ 코로나 끝나면 우쿨렐레 페스티발도 가게용",
          "speaker_id": "2",
          "time": "20210903 19:12"
        },
