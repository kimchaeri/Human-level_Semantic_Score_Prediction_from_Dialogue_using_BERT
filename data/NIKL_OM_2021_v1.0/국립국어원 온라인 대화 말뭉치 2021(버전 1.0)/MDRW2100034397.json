      "utterance": [
        {
          "id": "MDRW2100034397.1.1",
          "form": "티비 프로그램 뭐 보세요??",
          "original_form": "티비 프로그램 뭐 보세요??",
          "speaker_id": "1",
          "time": "20210828 04:17"
        },
        {
          "id": "MDRW2100034397.1.2",
          "form": "티비가읍어요ㅜㅜ",
          "original_form": "티비가읍어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 04:17"
        },
        {
          "id": "MDRW2100034397.1.3",
          "form": "유툽봐요",
          "original_form": "유툽봐요",
          "speaker_id": "2",
          "time": "20210828 04:18"
        },
        {
          "id": "MDRW2100034397.1.4",
          "form": "아하ㅠㅠㅠ 요즘 티비없는집 많더라구요",
          "original_form": "아하ㅠㅠㅠ 요즘 티비없는집 많더라구요",
          "speaker_id": "1",
          "time": "20210828 04:18"
        },
        {
          "id": "MDRW2100034397.1.5",
          "form": "유튭으로 뭐 보세요?",
          "original_form": "유튭으로 뭐 보세요?",
          "speaker_id": "1",
          "time": "20210828 04:18"
        },
        {
          "id": "MDRW2100034397.1.6",
          "form": "고양이 멍멍이요",
          "original_form": "고양이 멍멍이요",
          "speaker_id": "2",
          "time": "20210828 04:18"
        },
        {
          "id": "MDRW2100034397.1.7",
          "form": "소련여자",
          "original_form": "소련여자",
          "speaker_id": "2",
          "time": "20210828 04:18"
        },
        {
          "id": "MDRW2100034397.1.8",
          "form": "즉문즉설",
          "original_form": "즉문즉설",
          "speaker_id": "2",
          "time": "20210828 04:18"
        },
        {
          "id": "MDRW2100034397.1.9",
          "form": "멍멍이",
          "original_form": "멍멍이",
          "speaker_id": "1",
          "time": "20210828 04:18"
        },
        {
          "id": "MDRW2100034397.1.10",
          "form": "이런거요 name1님은요",
          "original_form": "이런거요 &name1&님은요",
          "speaker_id": "2",
          "time": "20210828 04:19"
        },
        {
          "id": "MDRW2100034397.1.11",
          "form": "오복이 보시나요?ㅋㅋㅋ",
          "original_form": "오복이 보시나요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 04:19"
        },
        {
          "id": "MDRW2100034397.1.12",
          "form": "저도 동물 컨텐츠 많이 봐요",
          "original_form": "저도 동물 컨텐츠 많이 봐요",
          "speaker_id": "1",
          "time": "20210828 04:19"
        },
        {
          "id": "MDRW2100034397.1.13",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210828 04:19"
        },
        {
          "id": "MDRW2100034397.1.14",
          "form": "오복이 말티즈인데 귀여워요",
          "original_form": "오복이 말티즈인데 귀여워요",
          "speaker_id": "1",
          "time": "20210828 04:19"
        },
        {
          "id": "MDRW2100034397.1.15",
          "form": "고양이 멍멍이 먹방봐요",
          "original_form": "고양이 멍멍이 먹방봐요",
          "speaker_id": "2",
          "time": "20210828 04:19"
        },
        {
          "id": "MDRW2100034397.1.16",
          "form": "말티즈 귀엽겠다",
          "original_form": "말티즈 귀엽겠다",
          "speaker_id": "2",
          "time": "20210828 04:19"
        },
        {
          "id": "MDRW2100034397.1.17",
          "form": "그리고 도시오리라고 오리영상도 봐용",
          "original_form": "그리고 도시오리라고 오리영상도 봐용",
          "speaker_id": "1",
          "time": "20210828 04:19"
        },
        {
          "id": "MDRW2100034397.1.18",
          "form": "엄청 귀여워요",
          "original_form": "엄청 귀여워요",
          "speaker_id": "1",
          "time": "20210828 04:19"
        },
        {
          "id": "MDRW2100034397.1.19",
          "form": "오리도 귀엽겠네요",
          "original_form": "오리도 귀엽겠네요",
          "speaker_id": "2",
          "time": "20210828 04:20"
        },
        {
          "id": "MDRW2100034397.1.20",
          "form": "넹ㅋㅋㅋ 동물 좋아하시는구나",
