      "utterance": [
        {
          "id": "MDRW2100020165.1.1",
          "form": "아니 그 바나나과자?있잖아",
          "original_form": "아니 그 바나나과자?있잖아",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020165.1.2",
          "form": "어어어 ㅋㅋㅋ",
          "original_form": "어어어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020165.1.3",
          "form": "그게 옛날과자? 이런거잖아 그거 만드는 회사가 이제 전국에 한군데 밖에 없데 ㅜ",
          "original_form": "그게 옛날과자? 이런거잖아 그거 만드는 회사가 이제 전국에 한군데 밖에 없데 ㅜ",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020165.1.4",
          "form": "옛날 문구사 통에 든거ㅋㅋㅋㅋ",
          "original_form": "옛날 문구사 통에 든거ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020165.1.5",
          "form": "이제 온리원...",
          "original_form": "이제 온리원...",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020165.1.6",
          "form": "ㅜㅜㅜㅜㅜㅜㅜ어찌그런일이",
          "original_form": "ㅜㅜㅜㅜㅜㅜㅜ어찌그런일이",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020165.1.7",
          "form": "극한직업에 그거 만드는 아저씨나옴..",
          "original_form": "극한직업에 그거 만드는 아저씨나옴..",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020165.1.8",
          "form": "극한직업이야그거?!",
          "original_form": "극한직업이야그거?!",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020165.1.9",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020165.1.10",
          "form": "그래서 약간 주문하고싶어졌잖어...",
          "original_form": "그래서 약간 주문하고싶어졌잖어...",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020165.1.11",
          "form": "아 혹시 주문하면 같이해줄래?",
          "original_form": "아 혹시 주문하면 같이해줄래?",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020165.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ왜같이해",
          "original_form": "ㅋㅋㅋㅋㅋㅋ왜같이해",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020165.1.13",
          "form": "너가줬다고거짓말하게",
          "original_form": "너가줬다고거짓말하게",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020165.1.14",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020165.1.15",
          "form": "좋은데? ㅋㅋㅋㅋㅋㅋㅋ 근데 내일 두개나먹을거와서 지금 쿨타임 찰때까지 기다리는중임",
          "original_form": "좋은데? ㅋㅋㅋㅋㅋㅋㅋ 근데 내일 두개나먹을거와서 지금 쿨타임 찰때까지 기다리는중임",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020165.1.16",
          "form": "ㅋㅋㅋㅋ뭐오는데 신났음",
          "original_form": "ㅋㅋㅋㅋ뭐오는데 신났음",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020165.1.17",
          "form": "내일 파운드케이크랑 에이스초코~",
          "original_form": "내일 파운드케이크랑 에이스초코~",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020165.1.18",
          "form": "파운드케이크가 그냥 쬐끄만 빵이다 근데 소포장되어있는거라서",
          "original_form": "파운드케이크가 그냥 쬐끄만 빵이다 근데 소포장되어있는거라서",
          "speaker_id": "1",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020165.1.19",
          "form": "에이스초코?",
          "original_form": "에이스초코?",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020165.1.20",
          "form": "응 신제품임",
          "original_form": "응 신제품임",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020165.1.21",
          "form": "에이스 물없이 여러개 씹어서 먹는거 좋아했는데",
          "original_form": "에이스 물없이 여러개 씹어서 먹는거 좋아했는데",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020165.1.22",
          "form": "에이스 샌드알아?",
          "original_form": "에이스 샌드알아?",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020165.1.23",
          "form": "샌드도있어?",
          "original_form": "샌드도있어?",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020165.1.24",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020165.1.25",
          "form": "와 새로운 섭취법이네 ㅋㅋㅋㅋ",
          "original_form": "와 새로운 섭취법이네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
