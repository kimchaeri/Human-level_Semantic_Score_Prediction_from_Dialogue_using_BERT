      "utterance": [
        {
          "id": "MDRW2100015969.1.1",
          "form": "몇인가구세용?",
          "original_form": "몇인가구세용?",
          "speaker_id": "1",
          "time": "20210826 04:16"
        },
        {
          "id": "MDRW2100015969.1.2",
          "form": "3인가구에요!",
          "original_form": "3인가구에요!",
          "speaker_id": "2",
          "time": "20210826 04:17"
        },
        {
          "id": "MDRW2100015969.1.3",
          "form": "아항 저는 엄마랑 살고있어요",
          "original_form": "아항 저는 엄마랑 살고있어요",
          "speaker_id": "1",
          "time": "20210826 04:17"
        },
        {
          "id": "MDRW2100015969.1.4",
          "form": "원래 4인가구였는데ㅋㅋ",
          "original_form": "원래 4인가구였는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 04:17"
        },
        {
          "id": "MDRW2100015969.1.5",
          "form": "둘이 살면 좀 더 오붓할 것 같아요!",
          "original_form": "둘이 살면 좀 더 오붓할 것 같아요!",
          "speaker_id": "2",
          "time": "20210826 04:18"
        },
        {
          "id": "MDRW2100015969.1.6",
          "form": "저도 어렸을때 잠깐 엄마랑 둘이 살았던 것 같아요ㅋㅋ 가물가물하지만..",
          "original_form": "저도 어렸을때 잠깐 엄마랑 둘이 살았던 것 같아요ㅋㅋ 가물가물하지만..",
          "speaker_id": "2",
          "time": "20210826 04:18"
        },
        {
          "id": "MDRW2100015969.1.7",
          "form": "넹 집이 전체적으로 조용해요ㅎㅎ",
          "original_form": "넹 집이 전체적으로 조용해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 04:18"
        },
        {
          "id": "MDRW2100015969.1.8",
          "form": "조용한거 좋아하시나요?",
          "original_form": "조용한거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 04:18"
        },
        {
          "id": "MDRW2100015969.1.9",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210826 04:19"
        },
        {
          "id": "MDRW2100015969.1.10",
          "form": "사람마다 선호하는 분위기가 다르더라구요",
          "original_form": "사람마다 선호하는 분위기가 다르더라구요",
          "speaker_id": "2",
          "time": "20210826 04:19"
        },
        {
          "id": "MDRW2100015969.1.11",
          "form": "막 왁자지껄보다는 절간처럼조용한거",
          "original_form": "막 왁자지껄보다는 절간처럼조용한거",
          "speaker_id": "1",
          "time": "20210826 04:19"
        },
        {
          "id": "MDRW2100015969.1.12",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "1",
          "time": "20210826 04:19"
        },
        {
          "id": "MDRW2100015969.1.13",
          "form": "그쵸!!",
          "original_form": "그쵸!!",
          "speaker_id": "2",
          "time": "20210826 04:19"
        },
        {
          "id": "MDRW2100015969.1.14",
          "form": "집은 휴식하는 곳이라는 느낌이 강해서",
          "original_form": "집은 휴식하는 곳이라는 느낌이 강해서",
          "speaker_id": "2",
          "time": "20210826 04:19"
        },
        {
          "id": "MDRW2100015969.1.15",
          "form": "왁자지껄하면 좀 피곤한 것 같아요",
          "original_form": "왁자지껄하면 좀 피곤한 것 같아요",
          "speaker_id": "2",
          "time": "20210826 04:19"
        },
        {
          "id": "MDRW2100015969.1.16",
          "form": "엇 엠비티아이 i로 나오셨을것같네요ㅋㅋ",
          "original_form": "엇 엠비티아이 i로 나오셨을것같네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 04:19"
        },
        {
          "id": "MDRW2100015969.1.17",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 04:20"
        },
        {
          "id": "MDRW2100015969.1.18",
          "form": "앗 혹시 I신가요",
          "original_form": "앗 혹시 I신가요",
          "speaker_id": "2",
          "time": "20210826 04:20"
        },
        {
          "id": "MDRW2100015969.1.19",
          "form": "집은 조용하고 편안해야죵",
          "original_form": "집은 조용하고 편안해야죵",
          "speaker_id": "1",
          "time": "20210826 04:20"
        },
        {
          "id": "MDRW2100015969.1.20",
          "form": "네네! 저도 i에요",
          "original_form": "네네! 저도 i에요",
          "speaker_id": "1",
          "time": "20210826 04:20"
        }
      ]
