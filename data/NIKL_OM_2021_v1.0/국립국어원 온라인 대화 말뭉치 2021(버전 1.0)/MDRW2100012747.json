      "utterance": [
        {
          "id": "MDRW2100012747.1.1",
          "form": "이상형이 뭐야",
          "original_form": "이상형이 뭐야",
          "speaker_id": "1",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100012747.1.2",
          "form": "한참 연애에 관심 많을때지",
          "original_form": "한참 연애에 관심 많을때지",
          "speaker_id": "2",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100012747.1.3",
          "form": "여자친구 있어?",
          "original_form": "여자친구 있어?",
          "speaker_id": "2",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100012747.1.4",
          "form": "여자가 없어요",
          "original_form": "여자가 없어요",
          "speaker_id": "1",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100012747.1.5",
          "form": "남고 다니는구나",
          "original_form": "남고 다니는구나",
          "speaker_id": "2",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100012747.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100012747.1.7",
          "form": "학원같은데는 안다니고??",
          "original_form": "학원같은데는 안다니고??",
          "speaker_id": "2",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100012747.1.8",
          "form": "다닌지 한달 됐어요",
          "original_form": "다닌지 한달 됐어요",
          "speaker_id": "1",
          "time": "20210825 00:44"
        },
        {
          "id": "MDRW2100012747.1.9",
          "form": "학원은 공부하러 다니는곳이니까 뭐..그래도 마음에 드는 친구는 있고??",
          "original_form": "학원은 공부하러 다니는곳이니까 뭐..그래도 마음에 드는 친구는 있고??",
          "speaker_id": "2",
          "time": "20210825 00:44"
        },
        {
          "id": "MDRW2100012747.1.10",
          "form": "아뇨 ㅋㅋ",
          "original_form": "아뇨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:45"
        },
        {
          "id": "MDRW2100012747.1.11",
          "form": "연락하던 여자애 남친생겨서",
          "original_form": "연락하던 여자애 남친생겨서",
          "speaker_id": "1",
          "time": "20210825 00:45"
        },
        {
          "id": "MDRW2100012747.1.12",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "2",
          "time": "20210825 00:45"
        },
        {
          "id": "MDRW2100012747.1.13",
          "form": "고3??",
          "original_form": "고3??",
          "speaker_id": "2",
          "time": "20210825 00:45"
        },
        {
          "id": "MDRW2100012747.1.14",
          "form": "이상형은?",
          "original_form": "이상형은?",
          "speaker_id": "2",
          "time": "20210825 00:45"
        },
        {
          "id": "MDRW2100012747.1.15",
          "form": "이상형은",
          "original_form": "이상형은",
          "speaker_id": "1",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100012747.1.16",
          "form": "성격이 좋아야죠",
          "original_form": "성격이 좋아야죠",
          "speaker_id": "1",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100012747.1.17",
          "form": "어떤 성격을 좋아해?",
          "original_form": "어떤 성격을 좋아해?",
          "speaker_id": "2",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100012747.1.18",
          "form": "착하면 좋죠",
          "original_form": "착하면 좋죠",
          "speaker_id": "1",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100012747.1.19",
          "form": "그쪽은요",
          "original_form": "그쪽은요",
          "speaker_id": "1",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100012747.1.20",
