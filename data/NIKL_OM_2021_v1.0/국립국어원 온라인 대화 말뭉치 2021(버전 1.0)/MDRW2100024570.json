      "utterance": [
        {
          "id": "MDRW2100024570.1.1",
          "form": "학교 언제가?",
          "original_form": "학교 언제가?",
          "speaker_id": "1",
          "time": "20210831 14:27"
        },
        {
          "id": "MDRW2100024570.1.2",
          "form": "나는 9월 1일",
          "original_form": "나는 9월 1일",
          "speaker_id": "2",
          "time": "20210831 14:27"
        },
        {
          "id": "MDRW2100024570.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210831 14:27"
        },
        {
          "id": "MDRW2100024570.1.4",
          "form": "나는 비대면이야",
          "original_form": "나는 비대면이야",
          "speaker_id": "1",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100024570.1.5",
          "form": "아 정말? 나도 비대면인데",
          "original_form": "아 정말? 나도 비대면인데",
          "speaker_id": "2",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100024570.1.6",
          "form": "나도 9월 1일부터야",
          "original_form": "나도 9월 1일부터야",
          "speaker_id": "1",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100024570.1.7",
          "form": "실강인거 있어?",
          "original_form": "실강인거 있어?",
          "speaker_id": "2",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100024570.1.8",
          "form": "실강이 뭐야?",
          "original_form": "실강이 뭐야?",
          "speaker_id": "1",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100024570.1.9",
          "form": "실시간 강의!",
          "original_form": "실시간 강의!",
          "speaker_id": "2",
          "time": "20210831 14:30"
        },
        {
          "id": "MDRW2100024570.1.10",
          "form": "아하 나 2과목있어",
          "original_form": "아하 나 2과목있어",
          "speaker_id": "1",
          "time": "20210831 14:31"
        },
        {
          "id": "MDRW2100024570.1.11",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210831 14:31"
        },
        {
          "id": "MDRW2100024570.1.12",
          "form": "나도 두과목",
          "original_form": "나도 두과목",
          "speaker_id": "2",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100024570.1.13",
          "form": "너는 무슨과야,",
          "original_form": "너는 무슨과야,",
          "speaker_id": "2",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100024570.1.14",
          "form": "나는 한국어랑 영어로 콘텐츠 배우는 과야",
          "original_form": "나는 한국어랑 영어로 콘텐츠 배우는 과야",
          "speaker_id": "1",
          "time": "20210831 15:08"
        },
        {
          "id": "MDRW2100024570.1.15",
          "form": "너네는 쭉 비대면이야?",
          "original_form": "너네는 쭉 비대면이야?",
          "speaker_id": "1",
          "time": "20210831 15:08"
        },
        {
          "id": "MDRW2100024570.1.16",
          "form": "우리는 거리두기 3단계되면",
          "original_form": "우리는 거리두기 3단계되면",
          "speaker_id": "2",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100024570.1.17",
          "form": "부분적으로 대면하고",
          "original_form": "부분적으로 대면하고",
          "speaker_id": "2",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100024570.1.18",
          "form": "2단계되면 전면 비대면일껄?",
          "original_form": "2단계되면 전면 비대면일껄?",
          "speaker_id": "2",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100024570.1.19",
          "form": "너네는?",
          "original_form": "너네는?",
          "speaker_id": "2",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100024570.1.20",
          "form": "너 생각에는 내년에 대면할거같아?",
          "original_form": "너 생각에는 내년에 대면할거같아?",
          "speaker_id": "1",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100024570.1.21",
          "form": "우리는 다 비대면이야",
          "original_form": "우리는 다 비대면이야",
          "speaker_id": "1",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100024570.1.22",
          "form": "음... 아마 내년 1학기까지는 비대면 아닐까? ㅠ",
          "original_form": "음... 아마 내년 1학기까지는 비대면 아닐까? ㅠ",
          "speaker_id": "2",
          "time": "20210831 15:11"
        },
        {
          "id": "MDRW2100024570.1.23",
          "form": "2학기때는 대면할것 같아",
          "original_form": "2학기때는 대면할것 같아",
          "speaker_id": "2",
          "time": "20210831 15:11"
        },
        {
          "id": "MDRW2100024570.1.24",
          "form": "나는 지금 부분적 대면이라 내년 1학기부터 대면할수도 있을것같아",
          "original_form": "나는 지금 부분적 대면이라 내년 1학기부터 대면할수도 있을것같아",
          "speaker_id": "1",
          "time": "20210831 15:13"
        },
        {
          "id": "MDRW2100024570.1.25",
          "form": "아 그렇구낭",
          "original_form": "아 그렇구낭",
          "speaker_id": "2",
          "time": "20210831 15:14"
        },
        {
          "id": "MDRW2100024570.1.26",
          "form": "저번학기때 과제 많았어?",
          "original_form": "저번학기때 과제 많았어?",
          "speaker_id": "2",
          "time": "20210831 15:15"
        },
        {
          "id": "MDRW2100024570.1.27",
          "form": "엄청많앗어",
          "original_form": "엄청많앗어",
          "speaker_id": "1",
          "time": "20210831 15:15"
        },
        {
          "id": "MDRW2100024570.1.28",
          "form": "너네도 많아?",
          "original_form": "너네도 많아?",
          "speaker_id": "1",
          "time": "20210831 15:15"
        },
        {
          "id": "MDRW2100024570.1.29",
          "form": "응 진짜 많아...",
          "original_form": "응 진짜 많아...",
          "speaker_id": "2",
          "time": "20210831 15:17"
        },
        {
          "id": "MDRW2100024570.1.30",
          "form": "너네는 무슨 과제 내줘?",
          "original_form": "너네는 무슨 과제 내줘?",
          "speaker_id": "2",
          "time": "20210831 15:17"
        },
        {
          "id": "MDRW2100024570.1.31",
          "form": "리포트나 영상제작 많이 내줘",
          "original_form": "리포트나 영상제작 많이 내줘",
          "speaker_id": "1",
          "time": "20210831 15:17"
        },
        {
          "id": "MDRW2100024570.1.32",
          "form": "너네는?",
          "original_form": "너네는?",
          "speaker_id": "1",
          "time": "20210831 15:17"
        },
        {
          "id": "MDRW2100024570.1.33",
          "form": "우리는 리포트나 퀴즈? 매주 내줘서",
          "original_form": "우리는 리포트나 퀴즈? 매주 내줘서",
          "speaker_id": "2",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100024570.1.34",
          "form": "진짜 힘들어 죽을것 같아 ㅎ",
          "original_form": "진짜 힘들어 죽을것 같아 ㅎ",
          "speaker_id": "2",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100024570.1.35",
          "form": "퀴즈는 뭐야?",
          "original_form": "퀴즈는 뭐야?",
          "speaker_id": "1",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100024570.1.36",
          "form": "퀴즈는  경제학원론이라는 과목이 있거든?",
          "original_form": "퀴즈는  경제학원론이라는 과목이 있거든?",
          "speaker_id": "2",
          "time": "20210831 15:23"
        },
        {
          "id": "MDRW2100024570.1.37",
          "form": "그 과목에 계산하는 문제가 많은데",
          "original_form": "그 과목에 계산하는 문제가 많은데",
          "speaker_id": "2",
          "time": "20210831 15:23"
        },
        {
          "id": "MDRW2100024570.1.38",
          "form": "그거를 매주 풀어야해",
          "original_form": "그거를 매주 풀어야해",
          "speaker_id": "2",
          "time": "20210831 15:23"
        },
        {
          "id": "MDRW2100024570.1.39",
          "form": "아 정말? 복잡하겠다",
          "original_form": "아 정말? 복잡하겠다",
          "speaker_id": "1",
          "time": "20210831 15:28"
        }
      ]
    }
