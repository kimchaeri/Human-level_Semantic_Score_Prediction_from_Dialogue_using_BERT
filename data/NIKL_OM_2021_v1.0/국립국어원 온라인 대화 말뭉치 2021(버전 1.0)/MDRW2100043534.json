        {
          "id": "MDRW2100043534.1.1",
          "form": "넌 국밥 좋아해??",
          "original_form": "넌 국밥 좋아해??",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043534.1.2",
          "form": "캬 시원하지 좋아해",
          "original_form": "캬 시원하지 좋아해",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043534.1.3",
          "form": "콩나물 국밤 먹어봣남?",
          "original_form": "콩나물 국밤 먹어봣남?",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043534.1.4",
          "form": "노놉!",
          "original_form": "노놉!",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043534.1.5",
          "form": "진짜 맛잇지~",
          "original_form": "진짜 맛잇지~",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043534.1.6",
          "form": "오그래??",
          "original_form": "오그래??",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043534.1.7",
          "form": "국하고는 다르나",
          "original_form": "국하고는 다르나",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043534.1.8",
          "form": "응응 좀 달라",
          "original_form": "응응 좀 달라",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043534.1.9",
          "form": "해장국같아?",
          "original_form": "해장국같아?",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043534.1.10",
          "form": "응 조금?",
          "original_form": "응 조금?",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043534.1.11",
          "form": "따로국밥도 맛잇는데",
          "original_form": "따로국밥도 맛잇는데",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043534.1.12",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043534.1.13",
          "form": "그래도 말아먹어야해",
          "original_form": "그래도 말아먹어야해",
          "speaker_id": "2",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043534.1.14",
          "form": "그건마잡",
