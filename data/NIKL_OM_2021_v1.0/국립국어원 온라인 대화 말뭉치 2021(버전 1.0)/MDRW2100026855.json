      "utterance": [
        {
          "id": "MDRW2100026855.1.1",
          "form": "와ㅠㅠㅠㅠㅠ 너무 부러워요ㅠㅠㅠㅠ",
          "original_form": "와ㅠㅠㅠㅠㅠ 너무 부러워요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100026855.1.2",
          "form": "저 유럽여행 꼭 가보고 싶었거든요ㅠㅠ",
          "original_form": "저 유럽여행 꼭 가보고 싶었거든요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100026855.1.3",
          "form": "유럽여행 어디어디 다녀오셨어요??",
          "original_form": "유럽여행 어디어디 다녀오셨어요??",
          "speaker_id": "2",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100026855.1.4",
          "form": "처음 배낭여행간거라",
          "original_form": "처음 배낭여행간거라",
          "speaker_id": "1",
          "time": "20210902 01:21"
        },
        {
          "id": "MDRW2100026855.1.5",
          "form": "기본나라들로 갔어요",
          "original_form": "기본나라들로 갔어요",
          "speaker_id": "1",
          "time": "20210902 01:22"
        },
        {
          "id": "MDRW2100026855.1.6",
          "form": "영국 프랑스 독일 오스트리아 이탈리아",
          "original_form": "영국 프랑스 독일 오스트리아 이탈리아",
          "speaker_id": "1",
          "time": "20210902 01:23"
        },
        {
          "id": "MDRW2100026855.1.7",
          "form": "이렇게요ㅎㅎ",
          "original_form": "이렇게요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 01:23"
        },
        {
          "id": "MDRW2100026855.1.8",
          "form": "알차게 다녀오셨네요ㅠㅠ",
          "original_form": "알차게 다녀오셨네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100026855.1.9",
          "form": "배낭여행이면 혼자서 가셨나요?",
          "original_form": "배낭여행이면 혼자서 가셨나요?",
          "speaker_id": "2",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100026855.1.10",
          "form": "말이 배낭인데 친구랑요ㅋㅋ",
          "original_form": "말이 배낭인데 친구랑요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 02:11"
        },
        {
          "id": "MDRW2100026855.1.11",
          "form": "스페인이랑",
          "original_form": "스페인이랑",
          "speaker_id": "1",
          "time": "20210902 02:11"
        },
        {
          "id": "MDRW2100026855.1.12",
          "form": "스위스도 가고싶었는데",
          "original_form": "스위스도 가고싶었는데",
          "speaker_id": "1",
          "time": "20210902 02:11"
        },
        {
          "id": "MDRW2100026855.1.13",
          "form": "날짜도 애매하더라고요ㅠㅍ",
          "original_form": "날짜도 애매하더라고요ㅠㅍ",
          "speaker_id": "1",
          "time": "20210902 02:11"
        },
        {
          "id": "MDRW2100026855.1.14",
          "form": "날짜랑 시간이 참 마음대로 안되죠ㅜㅜ",
          "original_form": "날짜랑 시간이 참 마음대로 안되죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 03:10"
        },
        {
          "id": "MDRW2100026855.1.15",
          "form": "그래도 친구랑 좋은 추억 만드셨을 것 같아요!!",
          "original_form": "그래도 친구랑 좋은 추억 만드셨을 것 같아요!!",
          "speaker_id": "2",
          "time": "20210902 03:10"
        },
        {
          "id": "MDRW2100026855.1.16",
          "form": "대학다니면서 방학 때 틈내고 다녀온거라ㅜ",
          "original_form": "대학다니면서 방학 때 틈내고 다녀온거라ㅜ",
          "speaker_id": "1",
          "time": "20210902 06:19"
        },
        {
          "id": "MDRW2100026855.1.17",
          "form": "정말요! 솔직히 서로 답답한 면도 있었을거예요",
          "original_form": "정말요! 솔직히 서로 답답한 면도 있었을거예요",
          "speaker_id": "1",
          "time": "20210902 06:20"
        },
        {
          "id": "MDRW2100026855.1.18",
          "form": "맘에 안 드는 부분도 분명 있었을테고",
          "original_form": "맘에 안 드는 부분도 분명 있었을테고",
          "speaker_id": "1",
          "time": "20210902 06:20"
        },
        {
          "id": "MDRW2100026855.1.19",
          "form": "근데 친구가 많이 양보해주고 서로 조심해서 싸우지 않고 왔어요ㅋㅋㅋㅋ",
          "original_form": "근데 친구가 많이 양보해주고 서로 조심해서 싸우지 않고 왔어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 06:20"
        },
        {
          "id": "MDRW2100026855.1.20",
          "form": "와 진짜 바람직한 여행을 다녀오셨네요ㅋㅋㅋ",
          "original_form": "와 진짜 바람직한 여행을 다녀오셨네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100026855.1.21",
          "form": "저는 국내여행 다니면서 친구들이랑 정말 많이 싸웠거든요",
          "original_form": "저는 국내여행 다니면서 친구들이랑 정말 많이 싸웠거든요",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100026855.1.22",
          "form": "메뉴 정하다가 토라지고 가고싶은 곳 정하다 토라지고...ㅋㅋㅋ",
          "original_form": "메뉴 정하다가 토라지고 가고싶은 곳 정하다 토라지고...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100026855.1.23",
          "form": "저희가 MBTI J와 P의 싸움이다 보니 J는 계획에 벗어난 건 못 받아들이고 P는 즉흥적으로 어디 가고 싶은데 다 안 된다고 하니 삐지고ㅋㅋㅋㅋ",
          "original_form": "저희가 MBTI J와 P의 싸움이다 보니 J는 계획에 벗어난 건 못 받아들이고 P는 즉흥적으로 어디 가고 싶은데 다 안 된다고 하니 삐지고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100026855.1.24",
          "form": "앜ㅋㅋㅋㅋㅋㅋ국내여행에서 싸우긴했어요",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ국내여행에서 싸우긴했어요",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100026855.1.25",
          "form": "친구들 mbti를 몰라서 모르지맘",
          "original_form": "친구들 mbti를 몰라서 모르지맘",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100026855.1.26",
          "form": "입맛이 살짝 안 맞아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "입맛이 살짝 안 맞아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100026855.1.27",
          "form": "아 진짜 여행하면 입맛 맞는 친구들끼리 가야해요... 서로 가고 싶은 곳 싸우다가 밥 먹는 내내 얼음판된 적도 있어요ㅋㅋㅋㅋ",
          "original_form": "아 진짜 여행하면 입맛 맞는 친구들끼리 가야해요... 서로 가고 싶은 곳 싸우다가 밥 먹는 내내 얼음판된 적도 있어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100026855.1.28",
          "form": "저는 제가 맵찔인데",
          "original_form": "저는 제가 맵찔인데",
          "speaker_id": "1",
          "time": "20210903 07:13"
        },
        {
          "id": "MDRW2100026855.1.29",
          "form": "여행갔는데 거기가 또 뭔 매운게 맛집이라능거예요....",
          "original_form": "여행갔는데 거기가 또 뭔 매운게 맛집이라능거예요....",
          "speaker_id": "1",
          "time": "20210903 07:13"
        },
        {
          "id": "MDRW2100026855.1.30",
          "form": "친구들은 잘 먹는데 저만진짜 물 이리터",
          "original_form": "친구들은 잘 먹는데 저만진짜 물 이리터",
          "speaker_id": "1",
          "time": "20210903 07:14"
        },
        {
          "id": "MDRW2100026855.1.31",
          "form": "마시고 배고파 죽겠는데ㅜㅜ",
          "original_form": "마시고 배고파 죽겠는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 07:14"
        },
        {
