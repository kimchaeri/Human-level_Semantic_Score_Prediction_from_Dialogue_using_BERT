      "utterance": [
        {
          "id": "MDRW2100000665.1.1",
          "form": "강아지 키우고 싶당",
          "original_form": "강아지 키우고 싶당",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000665.1.2",
          "form": "난 고냥이ㅋ",
          "original_form": "난 고냥이ㅋ",
          "speaker_id": "2",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000665.1.3",
          "form": "고양이 털 엄청 빠진데!ㅋ",
          "original_form": "고양이 털 엄청 빠진데!ㅋ",
          "speaker_id": "1",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000665.1.4",
          "form": "개가 더 빠져!!!!!",
          "original_form": "개가 더 빠져!!!!!",
          "speaker_id": "2",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000665.1.5",
          "form": "개털 장난아님",
          "original_form": "개털 장난아님",
          "speaker_id": "2",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000665.1.6",
          "form": "냄새도~~~~",
          "original_form": "냄새도~~~~",
          "speaker_id": "2",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000665.1.7",
          "form": "인정\n냄새가 엄청나긴하지\n그래도 매력이 그걸 덮지 않을까?ㅋ",
          "original_form": "인정\n냄새가 엄청나긴하지\n그래도 매력이 그걸 덮지 않을까?ㅋ",
          "speaker_id": "1",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000665.1.8",
          "form": "못덮을껄??? ㅋㅋ",
          "original_form": "못덮을껄??? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000665.1.9",
          "form": "내 로망인데!\n나중에 잔디깔린 마당에 리트리버키우는거!",
          "original_form": "내 로망인데!\n나중에 잔디깔린 마당에 리트리버키우는거!",
          "speaker_id": "1",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000665.1.10",
          "form": "그 마당에 똥은??? ㅋㅋ",
          "original_form": "그 마당에 똥은??? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000665.1.11",
          "form": "이사람이..  마당관리는???  개털이랑 목욕은???",
          "original_form": "이사람이..  마당관리는???  개털이랑 목욕은???",
          "speaker_id": "2",
          "time": "20210819 17:46"
        },
        {
          "id": "MDRW2100000665.1.12",
          "form": "왜이리 구체적이죠?\n전 그냥 로망이라구요ㅋ",
          "original_form": "왜이리 구체적이죠?\n전 그냥 로망이라구요ㅋ",
          "speaker_id": "1",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000665.1.13",
          "form": "헛된 꿈을 꾸지말라고ㅋㅋㅋ",
          "original_form": "헛된 꿈을 꾸지말라고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000665.1.14",
          "form": "아~ 큰개 만지고 싶다~~~~",
          "original_form": "아~ 큰개 만지고 싶다~~~~",
          "speaker_id": "2",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000665.1.15",
          "form": "웰시코기 생각난다",
          "original_form": "웰시코기 생각난다",
          "speaker_id": "1",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000665.1.16",
          "form": "웰시코기는 궁디팡팡~~ ㅋㅋ",
          "original_form": "웰시코기는 궁디팡팡~~ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000665.1.17",
          "form": "ㅇㅇ 점심에 웰시코기봤는데\n애가 살이 없어ㅋ\n살없으니 웰시코기 안같더라",
          "original_form": "ㅇㅇ 점심에 웰시코기봤는데\n애가 살이 없어ㅋ\n살없으니 웰시코기 안같더라",
          "speaker_id": "1",
          "time": "20210819 17:54"
        },
        {
          "id": "MDRW2100000665.1.18",
          "form": "어디서  봤어???",
          "original_form": "어디서  봤어???",
          "speaker_id": "2",
          "time": "20210819 17:54"
        },
        {
          "id": "MDRW2100000665.1.19",
          "form": "점심 먹으로 가는길에~~\n회사근처서!",
          "original_form": "점심 먹으로 가는길에~~\n회사근처서!",
          "speaker_id": "1",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000665.1.20",
          "form": "길냥인 더러 있는데 개는 거의 안보이네",
          "original_form": "길냥인 더러 있는데 개는 거의 안보이네",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000665.1.21",
          "form": "맞아\n길냥이 천국이야",
          "original_form": "맞아\n길냥이 천국이야",
          "speaker_id": "1",
          "time": "20210819 17:55"
        }
      ]
