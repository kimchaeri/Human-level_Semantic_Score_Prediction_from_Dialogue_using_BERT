      "utterance": [
        {
          "id": "MDRW2100018308.1.1",
          "form": "생각보다요",
          "original_form": "생각보다요",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018308.1.2",
          "form": "기대했는데말이죠...",
          "original_form": "기대했는데말이죠...",
          "speaker_id": "1",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018308.1.3",
          "form": "생각보다요",
          "original_form": "생각보다요",
          "speaker_id": "1",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018308.1.4",
          "form": "앙버터 저도 처음엔 이게뭐지? 이런느낌이었어요 ㅋㅋ",
          "original_form": "앙버터 저도 처음엔 이게뭐지? 이런느낌이었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018308.1.5",
          "form": "ㅋㅋㅋ 생버터죠??",
          "original_form": "ㅋㅋㅋ 생버터죠??",
          "speaker_id": "1",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018308.1.6",
          "form": "가운데에!",
          "original_form": "가운데에!",
          "speaker_id": "1",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018308.1.7",
          "form": "왜 버터를 씹어먹어야 하는거야? 이런느낌이었어요 ㅋㅋㅋ",
          "original_form": "왜 버터를 씹어먹어야 하는거야? 이런느낌이었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018308.1.8",
          "form": "ㅋㅋㅋㅋ저다음에 사먹어볼게요",
          "original_form": "ㅋㅋㅋㅋ저다음에 사먹어볼게요",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018308.1.9",
          "form": "배민등급이 무엇인가요?!!!!",
          "original_form": "배민등급이 무엇인가요?!!!!",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018308.1.10",
          "form": "근데 먹다보니 또 먹을만 하더라구요 ㅋㅋㅋ팥좋아하거든요 ㅋㅋ",
          "original_form": "근데 먹다보니 또 먹을만 하더라구요 ㅋㅋㅋ팥좋아하거든요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018308.1.11",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018308.1.12",
          "form": "저는 팥을안좋아하는데",
          "original_form": "저는 팥을안좋아하는데",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018308.1.13",
          "form": "놀라실수있는데 배민을 깔지 않았답니다",
          "original_form": "놀라실수있는데 배민을 깔지 않았답니다",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018308.1.14",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018308.1.15",
          "form": "님 저와정말",
          "original_form": "님 저와정말",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018308.1.16",
          "form": "입맛이....",
          "original_form": "입맛이....",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018308.1.17",
          "form": "럴수럴수이럴수",
          "original_form": "럴수럴수이럴수",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018308.1.18",
          "form": "먹고싶은 음식이 있다면 포장은하는데 배달은 안시켜요 ㅋㅋㅋ",
          "original_form": "먹고싶은 음식이 있다면 포장은하는데 배달은 안시켜요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018308.1.19",
          "form": "님 제등급",
          "original_form": "님 제등급",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018308.1.20",
          "form": "무엇이게요?",
          "original_form": "무엇이게요?",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018308.1.21",
          "form": "높으신가요?",
          "original_form": "높으신가요?",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018308.1.22",
          "form": "천생연분이라고",
          "original_form": "천생연분이라고",
          "speaker_id": "1",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018308.1.23",
          "form": "아시나요??!",
          "original_form": "아시나요??!",
          "speaker_id": "1",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018308.1.24",
          "form": "천생연분이라고 아시나요???!",
          "original_form": "천생연분이라고 아시나요???!",
