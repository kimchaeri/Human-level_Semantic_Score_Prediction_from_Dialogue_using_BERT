      "utterance": [
        {
          "id": "MDRW2100033087.1.1",
          "form": "또 이따위 레시피라니 라고",
          "original_form": "또 이따위 레시피라니 라고",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033087.1.2",
          "form": "줄리언반스라는 사람이 나이먹고 요리에 관심을 가지면서 느낀 점을 쓴 에세이에요",
          "original_form": "줄리언반스라는 사람이 나이먹고 요리에 관심을 가지면서 느낀 점을 쓴 에세이에요",
          "speaker_id": "1",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100033087.1.3",
          "form": "아 그게 책 제목 인가요?",
          "original_form": "아 그게 책 제목 인가요?",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100033087.1.4",
          "form": "네. 흥미로운 제목이라 저도 손이 갔어요ㅋㅋ",
          "original_form": "네. 흥미로운 제목이라 저도 손이 갔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100033087.1.5",
          "form": "name2님은 최근에 뭘 읽으셨나요?",
          "original_form": "&name2&님은 최근에 뭘 읽으셨나요?",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100033087.1.6",
          "form": "저도 최근에 읽은 에세이 말씀드리면..",
          "original_form": "저도 최근에 읽은 에세이 말씀드리면..",
          "speaker_id": "2",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100033087.1.7",
          "form": "책의 말들 이라는 책 읽었어요!",
          "original_form": "책의 말들 이라는 책 읽었어요!",
          "speaker_id": "2",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100033087.1.8",
          "form": "그거 어떤 내용인가요?",
          "original_form": "그거 어떤 내용인가요?",
          "speaker_id": "1",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100033087.1.9",
          "form": "야러 책에 등장하는 \"책이나 글에 관한 문장\"들을 모아서",
          "original_form": "야러 책에 등장하는 \"책이나 글에 관한 문장\"들을 모아서",
          "speaker_id": "2",
          "time": "20210906 16:06"
        },
        {
          "id": "MDRW2100033087.1.10",
          "form": "그 문장들을 모티브로 쓴 짧은 에세이 100편을 엮은 책이에요",
          "original_form": "그 문장들을 모티브로 쓴 짧은 에세이 100편을 엮은 책이에요",
          "speaker_id": "2",
          "time": "20210906 16:06"
        },
        {
          "id": "MDRW2100033087.1.11",
          "form": "오! 흥미롭네요.",
          "original_form": "오! 흥미롭네요.",
          "speaker_id": "1",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100033087.1.12",
          "form": "제가 좋아하는 책도 들어있으면 괜히 반갑고 그럴것같아요",
          "original_form": "제가 좋아하는 책도 들어있으면 괜히 반갑고 그럴것같아요",
          "speaker_id": "1",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100033087.1.13",
          "form": "ㅎㅎ 저도 먗 권 봤어요",
          "original_form": "ㅎㅎ 저도 먗 권 봤어요",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100033087.1.14",
          "form": "에세이 내용들이 꼭 그 문장 내용에만 귀속되어 있는 게 아니라 더 재미있더라고요",
          "original_form": "에세이 내용들이 꼭 그 문장 내용에만 귀속되어 있는 게 아니라 더 재미있더라고요",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100033087.1.15",
          "form": "한번 읽어봐야겠네요.",
          "original_form": "한번 읽어봐야겠네요.",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100033087.1.16",
          "form": "재미있으실거예요!",
          "original_form": "재미있으실거예요!",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100033087.1.17",
          "form": "ㅋㅋㅋㅋ 추천 감사드려요",
          "original_form": "ㅋㅋㅋㅋ 추천 감사드려요",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100033087.1.18",
          "form": "또 이따위 레시피라니 도 꽤 재밌어요",
          "original_form": "또 이따위 레시피라니 도 꽤 재밌어요",
          "speaker_id": "1",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033087.1.19",
          "form": "부엌에서 망해본 경험 좀 있으면 더더욱ㅋㅋㅋ",
          "original_form": "부엌에서 망해본 경험 좀 있으면 더더욱ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033087.1.20",
          "form": "ㅋㅋㅋ약간 레시피에 질려버리는? 그런 내용인가뇨?",
          "original_form": "ㅋㅋㅋ약간 레시피에 질려버리는? 그런 내용인가뇨?",
          "speaker_id": "2",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033087.1.21",
          "form": "그렇다기보단",
          "original_form": "그렇다기보단",
          "speaker_id": "1",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033087.1.22",
          "form": "음... 한큰술이라는게 깎아서야, 수북히야 뭐야",
          "original_form": "음... 한큰술이라는게 깎아서야, 수북히야 뭐야",
          "speaker_id": "1",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033087.1.23",
          "form": "아 완전 공감이요",
          "original_form": "아 완전 공감이요",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033087.1.24",
          "form": "왜이렇게 대충 썼지?",
          "original_form": "왜이렇게 대충 썼지?",
          "speaker_id": "1",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033087.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033087.1.26",
          "form": "같은 감상",
          "original_form": "같은 감상",
          "speaker_id": "1",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033087.1.27",
          "form": "한번은 작가가 요리책 저자에게 전화를 해서 자기의 궁금증을 물어보기도 하는데",
          "original_form": "한번은 작가가 요리책 저자에게 전화를 해서 자기의 궁금증을 물어보기도 하는데",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033087.1.28",
          "form": "레시피 보고 따라하는 거 너무 어려워요",
          "original_form": "레시피 보고 따라하는 거 너무 어려워요",
          "speaker_id": "2",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033087.1.29",
          "form": "그 요리사분은 도대체 그게 뭐가 문젠지를 이해하지 못하고.,",
          "original_form": "그 요리사분은 도대체 그게 뭐가 문젠지를 이해하지 못하고.,",
          "speaker_id": "1",
          "time": "20210906 16:52"
        }
      ]
