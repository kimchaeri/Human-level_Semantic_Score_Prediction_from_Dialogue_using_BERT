      "utterance": [
        {
          "id": "MDRW2100013579.1.1",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013579.1.2",
          "form": "저는 종교가없ㅈ습니다",
          "original_form": "저는 종교가없ㅈ습니다",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013579.1.3",
          "form": "종교있으십니까",
          "original_form": "종교있으십니까",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013579.1.4",
          "form": "앗 저도 무교입니다",
          "original_form": "앗 저도 무교입니다",
          "speaker_id": "1",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013579.1.5",
          "form": "근데 천주교 음식은 좋아합니다. 복음자리 잼 드셔보셨나요",
          "original_form": "근데 천주교 음식은 좋아합니다. 복음자리 잼 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013579.1.6",
          "form": "추천..진심",
          "original_form": "추천..진심",
          "speaker_id": "1",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013579.1.7",
          "form": "복음자리잼..",
          "original_form": "복음자리잼..",
          "speaker_id": "2",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013579.1.8",
          "form": "그것은 유명..",
          "original_form": "그것은 유명..",
          "speaker_id": "2",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013579.1.9",
          "form": "신은 안믿어도 잼은 믿으라던..",
          "original_form": "신은 안믿어도 잼은 믿으라던..",
          "speaker_id": "2",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013579.1.10",
          "form": "그 잼 맞나요",
          "original_form": "그 잼 맞나요",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013579.1.11",
          "form": "예아ㅡ..아시는군요",
          "original_form": "예아ㅡ..아시는군요",
          "speaker_id": "1",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013579.1.12",
          "form": "신앙심 절로 생깁니다",
          "original_form": "신앙심 절로 생깁니다",
          "speaker_id": "1",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013579.1.13",
          "form": "혹시 그럼..",
          "original_form": "혹시 그럼..",
          "speaker_id": "2",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013579.1.14",
          "form": "근데 요즘 젊은층에서 무교가 늘어난대요",
          "original_form": "근데 요즘 젊은층에서 무교가 늘어난대요",
          "speaker_id": "1",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013579.1.15",
          "form": "이름은 생각안나는데 소세지아십니까",
          "original_form": "이름은 생각안나는데 소세지아십니까",
          "speaker_id": "2",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013579.1.16",
          "form": "천주교 소세지있는데",
          "original_form": "천주교 소세지있는데",
          "speaker_id": "2",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100013579.1.17",
          "form": "아무래도 종교를 믿던 기성세대들 보다는 덜 한 게 있죠",
          "original_form": "아무래도 종교를 믿던 기성세대들 보다는 덜 한 게 있죠",
          "speaker_id": "2",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100013579.1.18",
          "form": "아 그 독일분이만든다는",
          "original_form": "아 그 독일분이만든다는",
          "speaker_id": "1",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100013579.1.19",
          "form": "알죠알죠",
          "original_form": "알죠알죠",
          "speaker_id": "1",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100013579.1.20",
          "form": "그거 드셔보셨어요??",
          "original_form": "그거 드셔보셨어요??",
          "speaker_id": "1",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100013579.1.21",
          "form": "아니요 눈독만들이고 있습니다ㅎ",
          "original_form": "아니요 눈독만들이고 있습니다ㅎ",
          "speaker_id": "2",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100013579.1.22",
          "form": "종교를 믿지 않는 이유가 있으신가요",
          "original_form": "종교를 믿지 않는 이유가 있으신가요",
          "speaker_id": "2",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100013579.1.23",
          "form": "귀차니즘 때문엨ㅋㅋㅋ",
          "original_form": "귀차니즘 때문엨ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013579.1.24",
          "form": "아니 믿으려면 교회나가거나 절가야해서",
          "original_form": "아니 믿으려면 교회나가거나 절가야해서",
          "speaker_id": "1",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013579.1.25",
          "form": "안믿는거같아요(?)",
          "original_form": "안믿는거같아요(?)",
          "speaker_id": "1",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013579.1.26",
          "form": "저도 그 이유가 8할이에요",
          "original_form": "저도 그 이유가 8할이에요",
          "speaker_id": "2",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013579.1.27",
          "form": "약간 아침 일찍 일어나서 가야한다는게 영..",
          "original_form": "약간 아침 일찍 일어나서 가야한다는게 영..",
          "speaker_id": "2",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013579.1.28",
          "form": "준비과정도 너무 귀찮고",
          "original_form": "준비과정도 너무 귀찮고",
          "speaker_id": "2",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100013579.1.29",
          "form": "교회같은 경우는 헌금 내는 것도 싫고..",
          "original_form": "교회같은 경우는 헌금 내는 것도 싫고..",
          "speaker_id": "2",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100013579.1.30",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100013579.1.31",
          "form": "돈아까워서도 있죠",
          "original_form": "돈아까워서도 있죠",
          "speaker_id": "1",
          "time": "20210825 15:18"
        }
      ]
    }
  ]
