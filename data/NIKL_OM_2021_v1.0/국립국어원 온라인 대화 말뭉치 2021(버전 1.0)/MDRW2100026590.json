      "utterance": [
        {
          "id": "MDRW2100026590.1.1",
          "form": "완전가고싶었음!!!",
          "original_form": "완전가고싶었음!!!",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026590.1.2",
          "form": "엑소콘이라닠ㅋㅋㅋ",
          "original_form": "엑소콘이라닠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026590.1.3",
          "form": "나에게 이런 행운이?",
          "original_form": "나에게 이런 행운이?",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026590.1.4",
          "form": "내말이 티켓팅 한번도 성공 못해봤는데",
          "original_form": "내말이 티켓팅 한번도 성공 못해봤는데",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026590.1.5",
          "form": "운좋게 갈 수 있게 됐음ㅋㅋㅋ",
          "original_form": "운좋게 갈 수 있게 됐음ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026590.1.6",
          "form": "티켓팅 성공하는 사람이 용자 아니냐고",
          "original_form": "티켓팅 성공하는 사람이 용자 아니냐고",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026590.1.7",
          "form": "주말에 서울 올라가야되는데 괜찮아??",
          "original_form": "주말에 서울 올라가야되는데 괜찮아??",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026590.1.8",
          "form": "진짜 성공하기힘들지",
          "original_form": "진짜 성공하기힘들지",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026590.1.9",
          "form": "당근",
          "original_form": "당근",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026590.1.10",
          "form": "엑콘인데",
          "original_form": "엑콘인데",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026590.1.11",
          "form": "가야지",
          "original_form": "가야지",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026590.1.12",
          "form": "오키오키 전날 미리 도착해서",
          "original_form": "오키오키 전날 미리 도착해서",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026590.1.13",
          "form": "근처에 숙소잡자",
          "original_form": "근처에 숙소잡자",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026590.1.14",
          "form": "ㅋㅋㅋ 근처 숙소는 잡을 수 있으까",
          "original_form": "ㅋㅋㅋ 근처 숙소는 잡을 수 있으까",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026590.1.15",
          "form": "이미 엑소팬들이...",
          "original_form": "이미 엑소팬들이...",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026590.1.16",
          "form": "ㅋㅋㅋㅋ장난아니지않을까",
          "original_form": "ㅋㅋㅋㅋ장난아니지않을까",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026590.1.17",
          "form": "고시원은 있지 않을까ㅋㅋㅋ",
          "original_form": "고시원은 있지 않을까ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026590.1.18",
          "form": "우리도 체력 아껴야 제대로 즐기지",
          "original_form": "우리도 체력 아껴야 제대로 즐기지",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026590.1.19",
          "form": "엑소야 기다려 누나가 간다!!!",
          "original_form": "엑소야 기다려 누나가 간다!!!",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026590.1.20",
          "form": "ㅋㅋㅋ고시원 하루 가능하냐고",
          "original_form": "ㅋㅋㅋ고시원 하루 가능하냐고",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026590.1.21",
          "form": "완전 누나가 간다..!!",
          "original_form": "완전 누나가 간다..!!",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026590.1.22",
          "form": "ㅇㅇㅇ 나 서울에 교육받으러 갈 때 많입ㅎㅁ",
          "original_form": "ㅇㅇㅇ 나 서울에 교육받으러 갈 때 많입ㅎㅁ",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026590.1.23",
          "form": "많이 봤어",
          "original_form": "많이 봤어",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026590.1.24",
          "form": "부산에는 왜 이런 콘서트가 안열릴까?",
          "original_form": "부산에는 왜 이런 콘서트가 안열릴까?",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026590.1.25",
          "form": "와...!!",
          "original_form": "와...!!",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026590.1.26",
          "form": "새로운정보야",
          "original_form": "새로운정보야",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026590.1.27",
          "form": "멀리 안 가고 즐기고 싶은데",
          "original_form": "멀리 안 가고 즐기고 싶은데",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026590.1.28",
          "form": "부산에 인간이없어서...",
          "original_form": "부산에 인간이없어서...",
