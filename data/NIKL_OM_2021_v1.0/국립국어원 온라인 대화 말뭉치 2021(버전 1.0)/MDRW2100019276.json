      "utterance": [
        {
          "id": "MDRW2100019276.1.1",
          "form": "그렇군요! 전주는 안 가본 지 10년은 된 것 같아요..",
          "original_form": "그렇군요! 전주는 안 가본 지 10년은 된 것 같아요..",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019276.1.2",
          "form": "가봐야지 하면서 못 가봤어요ㅠ",
          "original_form": "가봐야지 하면서 못 가봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019276.1.3",
          "form": "그러시구나! 전주도 요즘 사람이 엄청많아졌어요",
          "original_form": "그러시구나! 전주도 요즘 사람이 엄청많아졌어요",
          "speaker_id": "1",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019276.1.4",
          "form": "친구말로는 예전보다 못하대요ㅠ",
          "original_form": "친구말로는 예전보다 못하대요ㅠ",
          "speaker_id": "1",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019276.1.5",
          "form": "그렇군요 지인들 보니까 한복 입고 사진 많이 찍더라구요",
          "original_form": "그렇군요 지인들 보니까 한복 입고 사진 많이 찍더라구요",
          "speaker_id": "2",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019276.1.6",
          "form": "아하 어떤 점에서요?",
          "original_form": "아하 어떤 점에서요?",
          "speaker_id": "2",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019276.1.7",
          "form": "너무 상업화되었다고 했어요",
          "original_form": "너무 상업화되었다고 했어요",
          "speaker_id": "1",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100019276.1.8",
          "form": "길거리 음식들도 다 맛이 하향되었고",
          "original_form": "길거리 음식들도 다 맛이 하향되었고",
          "speaker_id": "1",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100019276.1.9",
          "form": "가게들도 줄도 많이서야하고 가격도 올랐대요",
          "original_form": "가게들도 줄도 많이서야하고 가격도 올랐대요",
          "speaker_id": "1",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100019276.1.10",
          "form": "아...그렇군요...",
          "original_form": "아...그렇군요...",
          "speaker_id": "2",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100019276.1.11",
          "form": "상업화 되면 너무 싫은데요..",
          "original_form": "상업화 되면 너무 싫은데요..",
          "speaker_id": "2",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100019276.1.12",
          "form": "경주도 저는 사람이 많아졌다고 생각하거든요",
          "original_form": "경주도 저는 사람이 많아졌다고 생각하거든요",
          "speaker_id": "1",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100019276.1.13",
          "form": "예전에는 자전거 타고다니기 너무 좋았는데 마지막으로 갔을때는",
          "original_form": "예전에는 자전거 타고다니기 너무 좋았는데 마지막으로 갔을때는",
          "speaker_id": "1",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100019276.1.14",
          "form": "자전거타고다니면 사람들이 부딪힐까봐 제대로타질못했어요",
          "original_form": "자전거타고다니면 사람들이 부딪힐까봐 제대로타질못했어요",
          "speaker_id": "1",
          "time": "20210827 16:57"
        },
        {
          "id": "MDRW2100019276.1.15",
          "form": "요새는 확실히 인스타 맛집..? 그런게 많아지긴 하는 것 같아요",
          "original_form": "요새는 확실히 인스타 맛집..? 그런게 많아지긴 하는 것 같아요",
          "speaker_id": "2",
          "time": "20210827 16:59"
        },
        {
          "id": "MDRW2100019276.1.16",
          "form": "그죠 경주야 원래 맛집없기로 유명해서...",
          "original_form": "그죠 경주야 원래 맛집없기로 유명해서...",
          "speaker_id": "1",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019276.1.17",
          "form": "그런맛집생기는건 환영이지만",
          "original_form": "그런맛집생기는건 환영이지만",
          "speaker_id": "1",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019276.1.18",
          "form": "전주는 맛집들 많아서 안바뀌면 좋겠어요",
          "original_form": "전주는 맛집들 많아서 안바뀌면 좋겠어요",
          "speaker_id": "1",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019276.1.19",
          "form": "전주 안 가본 지 너무 오래 되어서",
          "original_form": "전주 안 가본 지 너무 오래 되어서",
          "speaker_id": "2",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019276.1.20",
          "form": "나중에 한번 가봐야겠어요 !",
          "original_form": "나중에 한번 가봐야겠어요 !",
          "speaker_id": "2",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019276.1.21",
          "form": "해외 여행도 가본 곳 있으신가요?",
          "original_form": "해외 여행도 가본 곳 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019276.1.22",
          "form": "저는 오사카 후쿠오카 대만 가봤어요",
          "original_form": "저는 오사카 후쿠오카 대만 가봤어요",
          "speaker_id": "1",
          "time": "20210827 17:51"
        },
        {
          "id": "MDRW2100019276.1.23",
          "form": "와 일본 어떠셨어요? 저도 대만은 가봤는데 일본은 못 가봤거든요",
          "original_form": "와 일본 어떠셨어요? 저도 대만은 가봤는데 일본은 못 가봤거든요",
          "speaker_id": "2",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019276.1.24",
          "form": "일본 가까워서 일정잡기도 편하구요",
          "original_form": "일본 가까워서 일정잡기도 편하구요",
          "speaker_id": "1",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019276.1.25",
          "form": "숙소가격은 조금 부답스럽긴한데 그것빼고는",
          "original_form": "숙소가격은 조금 부답스럽긴한데 그것빼고는",
          "speaker_id": "1",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019276.1.26",
          "form": "비용도 적당하고 음식도 맛있고 볼거리들도 많았어요",
          "original_form": "비용도 적당하고 음식도 맛있고 볼거리들도 많았어요",
          "speaker_id": "1",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019276.1.27",
          "form": "후쿠오카는 료칸이 좋았구요",
          "original_form": "후쿠오카는 료칸이 좋았구요",
          "speaker_id": "1",
          "time": "20210827 18:15"
        },
        {
          "id": "MDRW2100019276.1.28",
          "form": "오사카는 축제가 좋았어요",
          "original_form": "오사카는 축제가 좋았어요",
          "speaker_id": "1",
          "time": "20210827 18:15"
        },
        {
          "id": "MDRW2100019276.1.29",
          "form": "헉 료칸..축제..제 로망들 다 해보셨네요..",
          "original_form": "헉 료칸..축제..제 로망들 다 해보셨네요..",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019276.1.30",
          "form": "축제는 여름축제 가보신 건가요...?",
          "original_form": "축제는 여름축제 가보신 건가요...?",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019276.1.31",
          "form": "여름축제는 못가봤구요 할로윈축제가봤어요",
          "original_form": "여름축제는 못가봤구요 할로윈축제가봤어요",
          "speaker_id": "1",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019276.1.32",
          "form": "할로윈축제때 가봤어요",
          "original_form": "할로윈축제때 가봤어요",
          "speaker_id": "1",
          "time": "20210827 20:00"
        }
      ]
