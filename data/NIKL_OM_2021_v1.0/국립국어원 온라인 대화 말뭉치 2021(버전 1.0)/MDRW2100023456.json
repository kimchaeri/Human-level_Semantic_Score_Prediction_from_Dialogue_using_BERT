      "utterance": [
        {
          "id": "MDRW2100023456.1.1",
          "form": "나는지금테라맥주",
          "original_form": "나는지금테라맥주",
          "speaker_id": "1",
          "time": "20210830 03:33"
        },
        {
          "id": "MDRW2100023456.1.2",
          "form": "마시는중이야",
          "original_form": "마시는중이야",
          "speaker_id": "1",
          "time": "20210830 03:33"
        },
        {
          "id": "MDRW2100023456.1.3",
          "form": "살쪄",
          "original_form": "살쪄",
          "speaker_id": "2",
          "time": "20210830 03:33"
        },
        {
          "id": "MDRW2100023456.1.4",
          "form": "넌 혼술자주해?",
          "original_form": "넌 혼술자주해?",
          "speaker_id": "1",
          "time": "20210830 03:33"
        },
        {
          "id": "MDRW2100023456.1.5",
          "form": "자취하잖아",
          "original_form": "자취하잖아",
          "speaker_id": "1",
          "time": "20210830 03:33"
        },
        {
          "id": "MDRW2100023456.1.6",
          "form": "안주는?",
          "original_form": "안주는?",
          "speaker_id": "2",
          "time": "20210830 03:33"
        },
        {
          "id": "MDRW2100023456.1.7",
          "form": "응 48키로ㅋ",
          "original_form": "응 48키로ㅋ",
          "speaker_id": "1",
          "time": "20210830 03:33"
        },
        {
          "id": "MDRW2100023456.1.8",
          "form": "ㄴㄴ 나 피부 곱창나서",
          "original_form": "ㄴㄴ 나 피부 곱창나서",
          "speaker_id": "2",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.9",
          "form": "왓챠팝콘",
          "original_form": "왓챠팝콘",
          "speaker_id": "1",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.10",
          "form": "혼술 절제중",
          "original_form": "혼술 절제중",
          "speaker_id": "2",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.11",
          "form": "술이랑 피부가 먼상관",
          "original_form": "술이랑 피부가 먼상관",
          "speaker_id": "1",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.12",
          "form": "그 전엔 호가든 로제같은거 마니 사먹음",
          "original_form": "그 전엔 호가든 로제같은거 마니 사먹음",
          "speaker_id": "2",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.13",
          "form": "술이라도 덜 마셔야",
          "original_form": "술이라도 덜 마셔야",
          "speaker_id": "2",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.14",
          "form": "잇나? 아잘모르겟다",
          "original_form": "잇나? 아잘모르겟다",
          "speaker_id": "1",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.15",
          "form": "피부가 좀 좋아질거라는",
          "original_form": "피부가 좀 좋아질거라는",
          "speaker_id": "2",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.16",
          "form": "오부자네",
          "original_form": "오부자네",
          "speaker_id": "1",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.17",
          "form": "나는맨날",
          "original_form": "나는맨날",
          "speaker_id": "1",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.18",
          "form": "환상이 생기지",
          "original_form": "환상이 생기지",
          "speaker_id": "2",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.19",
          "form": "카스 테라먹는데",
          "original_form": "카스 테라먹는데",
          "speaker_id": "1",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.20",
          "form": "테라나 호가든로제나",
          "original_form": "테라나 호가든로제나",
          "speaker_id": "2",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.21",
          "form": "호가든로제먹고부자네",
          "original_form": "호가든로제먹고부자네",
          "speaker_id": "1",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.22",
          "form": "가격 거서 건데",
          "original_form": "가격 거서 건데",
          "speaker_id": "2",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.23",
          "form": "나는그냥이렇게살아와서그런가봐",
          "original_form": "나는그냥이렇게살아와서그런가봐",
          "speaker_id": "1",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.24",
          "form": "야 근데 넌 맥주 맛이",
          "original_form": "야 근데 넌 맥주 맛이",
          "speaker_id": "2",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.25",
          "form": "구분이 돼?",
          "original_form": "구분이 돼?",
          "speaker_id": "2",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.26",
          "form": "난 테라나 카스나 거기서 거긴거같아",
          "original_form": "난 테라나 카스나 거기서 거긴거같아",
          "speaker_id": "2",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.27",
          "form": "테라2000 호가든로제3500",
          "original_form": "테라2000 호가든로제3500",
          "speaker_id": "1",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.28",
          "form": "약간은",
          "original_form": "약간은",
          "speaker_id": "1",
          "time": "20210830 03:34"
        },
        {
          "id": "MDRW2100023456.1.29",
          "form": "로제처럼 획기적으로 다르지 않은 이상",
          "original_form": "로제처럼 획기적으로 다르지 않은 이상",
          "speaker_id": "2",
          "time": "20210830 03:35"
        },
        {
          "id": "MDRW2100023456.1.30",
          "form": "근데사실 외국맥주브랜드아님잘몰라",
          "original_form": "근데사실 외국맥주브랜드아님잘몰라",
          "speaker_id": "1",
          "time": "20210830 03:35"
        },
        {
          "id": "MDRW2100023456.1.31",
          "form": "로제한번도안마셔봣다",
          "original_form": "로제한번도안마셔봣다",
          "speaker_id": "1",
          "time": "20210830 03:35"
        },
        {
          "id": "MDRW2100023456.1.32",
          "form": "요즘 근데 편의점가면",
          "original_form": "요즘 근데 편의점가면",
          "speaker_id": "2",
          "time": "20210830 03:35"
        },
        {
          "id": "MDRW2100023456.1.33",
          "form": "맛잇나그거?",
          "original_form": "맛잇나그거?",
          "speaker_id": "1",
          "time": "20210830 03:35"
        },
        {
          "id": "MDRW2100023456.1.34",
          "form": "제주에일 이런거 많이 나오더라",
          "original_form": "제주에일 이런거 많이 나오더라",
          "speaker_id": "2",
          "time": "20210830 03:35"
        },
        {
          "id": "MDRW2100023456.1.35",
          "form": "로제맛잇나?",
          "original_form": "로제맛잇나?",
          "speaker_id": "1",
          "time": "20210830 03:35"
        },
        {
          "id": "MDRW2100023456.1.36",
          "form": "탄산은?",
          "original_form": "탄산은?",
          "speaker_id": "1",
          "time": "20210830 03:35"
        }
      ]
