      "utterance": [
        {
          "id": "MDRW2100023878.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023878.1.2",
          "form": "가고 싶은 여행지 있어요? 제가 소개해드릴게요",
          "original_form": "가고 싶은 여행지 있어요? 제가 소개해드릴게요",
          "speaker_id": "1",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100023878.1.3",
          "form": "훔 삼청동?",
          "original_form": "훔 삼청동?",
          "speaker_id": "2",
          "time": "20210830 19:37"
        },
        {
          "id": "MDRW2100023878.1.4",
          "form": "삼청동~ 삼청동에 대해 아는 거 있어요?",
          "original_form": "삼청동~ 삼청동에 대해 아는 거 있어요?",
          "speaker_id": "1",
          "time": "20210830 19:37"
        },
        {
          "id": "MDRW2100023878.1.5",
          "form": "맨날 한옥마을만 좀 보고 사실 삼청동은 구경만 했네여",
          "original_form": "맨날 한옥마을만 좀 보고 사실 삼청동은 구경만 했네여",
          "speaker_id": "2",
          "time": "20210830 19:37"
        },
        {
          "id": "MDRW2100023878.1.6",
          "form": "뭔가 좋은 가게 하나 알고싶어요",
          "original_form": "뭔가 좋은 가게 하나 알고싶어요",
          "speaker_id": "2",
          "time": "20210830 19:37"
        },
        {
          "id": "MDRW2100023878.1.7",
          "form": "오~ 일단 삼청동의 위치는 서울특별시 종로구에 있어요!",
          "original_form": "오~ 일단 삼청동의 위치는 서울특별시 종로구에 있어요!",
          "speaker_id": "1",
          "time": "20210830 19:37"
        },
        {
          "id": "MDRW2100023878.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023878.1.9",
          "form": "품격 있는 맛집으로 알려드릴까요?",
          "original_form": "품격 있는 맛집으로 알려드릴까요?",
          "speaker_id": "1",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023878.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023878.1.11",
          "form": "경복궁 나들이 갔다가 가기 좋은 곳이구요",
          "original_form": "경복궁 나들이 갔다가 가기 좋은 곳이구요",
          "speaker_id": "1",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023878.1.12",
          "form": "가게 이름은 부띠끄경성이에요",
          "original_form": "가게 이름은 부띠끄경성이에요",
          "speaker_id": "1",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023878.1.13",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023878.1.14",
          "form": "이름이 그럴싸",
          "original_form": "이름이 그럴싸",
          "speaker_id": "2",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023878.1.15",
          "form": "네네 메뉴도 알려드릴까요?",
          "original_form": "네네 메뉴도 알려드릴까요?",
          "speaker_id": "1",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023878.1.16",
          "form": "뭐 파는지 궁금하지 않아요?",
          "original_form": "뭐 파는지 궁금하지 않아요?",
          "speaker_id": "1",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023878.1.17",
          "form": "뭐파나요",
          "original_form": "뭐파나요",
          "speaker_id": "2",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023878.1.18",
          "form": "함박스테이크랑 콥샐러드에요ㅎㅎ",
          "original_form": "함박스테이크랑 콥샐러드에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023878.1.19",
          "form": "오 메뉴 좋네요",
          "original_form": "오 메뉴 좋네요",
          "speaker_id": "2",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023878.1.20",
          "form": "취향저격",
          "original_form": "취향저격",
          "speaker_id": "2",
          "time": "20210830 19:39"
        },
        {
          "id": "MDRW2100023878.1.21",
          "form": "가격은 함박이 1.9만, 콥샐러드 1.6만 정도 해요!",
          "original_form": "가격은 함박이 1.9만, 콥샐러드 1.6만 정도 해요!",
          "speaker_id": "1",
          "time": "20210830 19:39"
        },
        {
          "id": "MDRW2100023878.1.22",
          "form": "근데 주말만 브레이크타임 있으니까 잘 알아보고 가셔야해요",
          "original_form": "근데 주말만 브레이크타임 있으니까 잘 알아보고 가셔야해요",
          "speaker_id": "1",
          "time": "20210830 19:39"
        },
        {
          "id": "MDRW2100023878.1.23",
          "form": "괜찮네요 저장하고 옵니다",
          "original_form": "괜찮네요 저장하고 옵니다",
          "speaker_id": "2",
          "time": "20210830 19:39"
        },
        {
          "id": "MDRW2100023878.1.24",
          "form": "네네 삼청동엔 주차장시설 잘되어있어서",
          "original_form": "네네 삼청동엔 주차장시설 잘되어있어서",
          "speaker_id": "1",
          "time": "20210830 19:39"
        },
        {
          "id": "MDRW2100023878.1.25",
          "form": "여행 다녀오기 괜찮아요~",
          "original_form": "여행 다녀오기 괜찮아요~",
          "speaker_id": "1",
          "time": "20210830 19:39"
        },
        {
          "id": "MDRW2100023878.1.26",
          "form": "좋은 추천이군요",
          "original_form": "좋은 추천이군요",
          "speaker_id": "2",
          "time": "20210830 19:39"
        },
        {
          "id": "MDRW2100023878.1.27",
          "form": "서울에서 나들이 자주 다니시나요",
          "original_form": "서울에서 나들이 자주 다니시나요",
          "speaker_id": "2",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100023878.1.28",
          "form": "내친김에 삼청동 여행 갔을 때 들리기 좋은 카페도 알려드릴까요",
          "original_form": "내친김에 삼청동 여행 갔을 때 들리기 좋은 카페도 알려드릴까요",
          "speaker_id": "1",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100023878.1.29",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100023878.1.30",
          "form": "블루보틀이요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "블루보틀이요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100023878.1.31",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "2",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100023878.1.32",
          "form": "일본에서 가봤으니까 안가볼래요",
          "original_form": "일본에서 가봤으니까 안가볼래요",
          "speaker_id": "2",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100023878.1.33",
          "form": "실망하셨구나",
          "original_form": "실망하셨구나",
          "speaker_id": "1",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100023878.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100023878.1.35",
          "form": "한옥뷰가 멋있는데 그걸 포기한다구요?ㅠ",
          "original_form": "한옥뷰가 멋있는데 그걸 포기한다구요?ㅠ",
          "speaker_id": "1",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100023878.1.36",
          "form": "한옥뷰를 즐길 수 있나요?",
          "original_form": "한옥뷰를 즐길 수 있나요?",
          "speaker_id": "2",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100023878.1.37",
          "form": "사람 너무 많을거 같은데",
          "original_form": "사람 너무 많을거 같은데",
          "speaker_id": "2",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100023878.1.38",
          "form": "다들 그 생각으로 안갈 때 호다닥 여행 다녀오셔야죠^^!",
          "original_form": "다들 그 생각으로 안갈 때 호다닥 여행 다녀오셔야죠^^!",
          "speaker_id": "1",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100023878.1.39",
          "form": "호호 서울사람들은 늘 가잖아요",
          "original_form": "호호 서울사람들은 늘 가잖아요",
          "speaker_id": "2",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100023878.1.40",
          "form": "자자 가고 싶은 해외 여행지 얘기해봐요",
          "original_form": "자자 가고 싶은 해외 여행지 얘기해봐요",
          "speaker_id": "1",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100023878.1.41",
          "form": "저의 소중한 경험을 나눠 드립니다.",
          "original_form": "저의 소중한 경험을 나눠 드립니다.",
          "speaker_id": "1",
          "time": "20210830 19:41"
        }
      ]
