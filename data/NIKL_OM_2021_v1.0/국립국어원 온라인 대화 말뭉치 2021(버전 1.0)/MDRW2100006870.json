      "utterance": [
        {
          "id": "MDRW2100006870.1.1",
          "form": "내려올때 무게 안실리게 해야함",
          "original_form": "내려올때 무게 안실리게 해야함",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100006870.1.2",
          "form": "등산화필요함?",
          "original_form": "등산화필요함?",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100006870.1.3",
          "form": "울집앞은 솔직히",
          "original_form": "울집앞은 솔직히",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100006870.1.4",
          "form": "필요없는데",
          "original_form": "필요없는데",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100006870.1.5",
          "form": "근데 신고가는게 나음",
          "original_form": "근데 신고가는게 나음",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100006870.1.6",
          "form": "아 확실히 잡아주는게다른가",
          "original_form": "아 확실히 잡아주는게다른가",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100006870.1.7",
          "form": "운동화 닳음",
          "original_form": "운동화 닳음",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100006870.1.8",
          "form": "아 그럼 저렴한 등산화로하나사야겠다",
          "original_form": "아 그럼 저렴한 등산화로하나사야겠다",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100006870.1.9",
          "form": "추천감사감사",
          "original_form": "추천감사감사",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100006870.1.10",
          "form": "없으면",
          "original_form": "없으면",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006870.1.11",
          "form": "사셈",
          "original_form": "사셈",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006870.1.12",
          "form": "아울렛이나",
          "original_form": "아울렛이나",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006870.1.13",
          "form": "가면",
          "original_form": "가면",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006870.1.14",
          "form": "메이커들",
          "original_form": "메이커들",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006870.1.15",
          "form": "할인함",
          "original_form": "할인함",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006870.1.16",
          "form": "머렐 블랙야크 코오롱",
          "original_form": "머렐 블랙야크 코오롱",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006870.1.17",
          "form": "오 이제 날 좋으니까 등산 가봐야겠다",
          "original_form": "오 이제 날 좋으니까 등산 가봐야겠다",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006870.1.18",
          "form": "딱조흠",
          "original_form": "딱조흠",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100006870.1.19",
          "form": "선선하고",
          "original_form": "선선하고",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100006870.1.20",
          "form": "햇빛약하고",
          "original_form": "햇빛약하고",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100006870.1.21",
          "form": "등산하면 호흡조흠",
          "original_form": "등산하면 호흡조흠",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100006870.1.22",
          "form": "코도안골게됨",
          "original_form": "코도안골게됨",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100006870.1.23",
          "form": "오 나 코 좀 고는데 그건 처음들어봄",
          "original_form": "오 나 코 좀 고는데 그건 처음들어봄",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100006870.1.24",
          "form": "무리는 하지마셈",
          "original_form": "무리는 하지마셈",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100006870.1.25",
          "form": "나 코골다가",
          "original_form": "나 코골다가",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100006870.1.26",
          "form": "안곪",
          "original_form": "안곪",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100006870.1.27",
          "form": "산소많이먹어서그런가봄",
          "original_form": "산소많이먹어서그런가봄",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100006870.1.28",
          "form": "하체근육도 붙고",
          "original_form": "하체근육도 붙고",
          "speaker_id": "2",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100006870.1.29",
          "form": "여자들 하기 좋은거 같음",
          "original_form": "여자들 하기 좋은거 같음",
