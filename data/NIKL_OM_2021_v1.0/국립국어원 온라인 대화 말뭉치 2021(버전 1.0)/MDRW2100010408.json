      "utterance": [
        {
          "id": "MDRW2100010408.1.1",
          "form": "집안일 너무싫네",
          "original_form": "집안일 너무싫네",
          "speaker_id": "2",
          "time": "20210823 18:32"
        },
        {
          "id": "MDRW2100010408.1.2",
          "form": "화장실청소해야되는데",
          "original_form": "화장실청소해야되는데",
          "speaker_id": "2",
          "time": "20210823 18:32"
        },
        {
          "id": "MDRW2100010408.1.3",
          "form": "집안일 저도요ㅋㅋ하기싫음ㅋ",
          "original_form": "집안일 저도요ㅋㅋ하기싫음ㅋ",
          "speaker_id": "1",
          "time": "20210824 07:26"
        },
        {
          "id": "MDRW2100010408.1.4",
          "form": "어제 피곤해서 8시에잤어요",
          "original_form": "어제 피곤해서 8시에잤어요",
          "speaker_id": "1",
          "time": "20210824 07:26"
        },
        {
          "id": "MDRW2100010408.1.5",
          "form": "첫째는 할머니집보냈고요ㅋ",
          "original_form": "첫째는 할머니집보냈고요ㅋ",
          "speaker_id": "1",
          "time": "20210824 07:27"
        },
        {
          "id": "MDRW2100010408.1.6",
          "form": "오오 좋은데?",
          "original_form": "오오 좋은데?",
          "speaker_id": "2",
          "time": "20210824 08:46"
        },
        {
          "id": "MDRW2100010408.1.7",
          "form": "난 애들숙제안해줘서 name3언니한테혼남",
          "original_form": "난 애들숙제안해줘서 &name3&언니한테혼남",
          "speaker_id": "2",
          "time": "20210824 08:47"
        },
        {
          "id": "MDRW2100010408.1.8",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 08:47"
        },
        {
          "id": "MDRW2100010408.1.9",
          "form": "ㅋㅋㅋㅋ아 언니ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ아 언니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:36"
        },
        {
          "id": "MDRW2100010408.1.10",
          "form": "나도 첫째 일기를 어머님께 전달함ㅋㅋ",
          "original_form": "나도 첫째 일기를 어머님께 전달함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:36"
        },
        {
          "id": "MDRW2100010408.1.11",
          "form": "해야된다꼬요ㅋㅋ",
          "original_form": "해야된다꼬요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:36"
        },
        {
          "id": "MDRW2100010408.1.12",
          "form": "일기하나쓰는데 얼마나오래걸리는지",
          "original_form": "일기하나쓰는데 얼마나오래걸리는지",
          "speaker_id": "2",
          "time": "20210824 09:46"
        },
        {
          "id": "MDRW2100010408.1.13",
          "form": "ㅋㄱㄲ울 첫째도요ㅋㄱㅋㅋ",
          "original_form": "ㅋㄱㄲ울 첫째도요ㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:01"
        },
        {
          "id": "MDRW2100010408.1.14",
          "form": "정말 숙제봐주는거 스트레스다ㅋ",
          "original_form": "정말 숙제봐주는거 스트레스다ㅋ",
          "speaker_id": "2",
          "time": "20210824 10:04"
        },
        {
          "id": "MDRW2100010408.1.15",
          "form": "그러니깐요 저도요.ㅋㅋ",
          "original_form": "그러니깐요 저도요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:04"
        },
        {
          "id": "MDRW2100010408.1.16",
          "form": "어제 신랑한테",
          "original_form": "어제 신랑한테",
          "speaker_id": "1",
          "time": "20210824 10:04"
        },
        {
          "id": "MDRW2100010408.1.17",
          "form": "영어 그거 얘기했는데",
          "original_form": "영어 그거 얘기했는데",
          "speaker_id": "1",
          "time": "20210824 10:04"
        },
        {
          "id": "MDRW2100010408.1.18",
          "form": "돌아오는 답변은 노노",
          "original_form": "돌아오는 답변은 노노",
          "speaker_id": "1",
          "time": "20210824 10:04"
        },
        {
          "id": "MDRW2100010408.1.19",
          "form": "흠...",
          "original_form": "흠...",
          "speaker_id": "1",
          "time": "20210824 10:04"
        },
        {
          "id": "MDRW2100010408.1.20",
          "form": "진짜?  신랑은 하지마래?",
          "original_form": "진짜?  신랑은 하지마래?",
          "speaker_id": "2",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100010408.1.21",
          "form": "네 하지말래유. 대기도 하지말라는데",
          "original_form": "네 하지말래유. 대기도 하지말라는데",
          "speaker_id": "1",
          "time": "20210824 10:07"
        },
        {
          "id": "MDRW2100010408.1.22",
          "form": "어제는 내가 피곤해가 얘기 길게는못했다만",
          "original_form": "어제는 내가 피곤해가 얘기 길게는못했다만",
          "speaker_id": "1",
          "time": "20210824 10:07"
        },
        {
          "id": "MDRW2100010408.1.23",
          "form": "솔직히 저는 또 발등에 불떨어지는것보다는 꾸준히하는게 낫지않겠나싶어서ㅋㅋ",
          "original_form": "솔직히 저는 또 발등에 불떨어지는것보다는 꾸준히하는게 낫지않겠나싶어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:08"
        },
        {
          "id": "MDRW2100010408.1.24",
          "form": "다음에 또 얘기해봐야죠ㅋ",
          "original_form": "다음에 또 얘기해봐야죠ㅋ",
          "speaker_id": "1",
          "time": "20210824 10:08"
        },
        {
          "id": "MDRW2100010408.1.25",
          "form": "그래2학년되면 하게된다",
          "original_form": "그래2학년되면 하게된다",
          "speaker_id": "2",
          "time": "20210824 12:25"
        },
        {
          "id": "MDRW2100010408.1.26",
          "form": "아직1학년이라그래ㅋㅋㅋ",
          "original_form": "아직1학년이라그래ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:26"
        },
        {
          "id": "MDRW2100010408.1.27",
          "form": "ㅋㅋㄱㅋㄱ그런가요..과연ㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㄱ그런가요..과연ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100010408.1.28",
          "form": "아빠가 시키자고 할수도ㅋㅋㅋ",
          "original_form": "아빠가 시키자고 할수도ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:59"
        },
        {
          "id": "MDRW2100010408.1.29",
          "form": "맞아요 아직 1학년이라 그런가봐요ㅋㅋ",
          "original_form": "맞아요 아직 1학년이라 그런가봐요ㅋㅋ",
