      "utterance": [
        {
          "id": "MDRW2100044382.1.1",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044382.1.2",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044382.1.3",
          "form": "네 좋아하는데 카페인에 약해서 슬퍼요ㅠㅠ",
          "original_form": "네 좋아하는데 카페인에 약해서 슬퍼요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044382.1.4",
          "form": "저도!!",
          "original_form": "저도!!",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044382.1.5",
          "form": "가슴이 두근두근",
          "original_form": "가슴이 두근두근",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044382.1.6",
          "form": "잠못자요",
          "original_form": "잠못자요",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044382.1.7",
          "form": "저두요ㅠㅠㅠㅠ흐언 ㅠㅠㅠ",
          "original_form": "저두요ㅠㅠㅠㅠ흐언 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044382.1.8",
          "form": "프렌차이즈 카페중에는 어딜 좋아하시나요 ㅋㅋ",
          "original_form": "프렌차이즈 카페중에는 어딜 좋아하시나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044382.1.9",
          "form": "스벅ㅋㅋ",
          "original_form": "스벅ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044382.1.10",
          "form": "만만하죠ㅋㅋ",
          "original_form": "만만하죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044382.1.11",
          "form": "ㅋㅋㅋ저는 커피빈을 더좋아하는데 커피빈 너무 매장적어요ㅠㅠ",
          "original_form": "ㅋㅋㅋ저는 커피빈을 더좋아하는데 커피빈 너무 매장적어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044382.1.12",
          "form": "그렇더라고요",
          "original_form": "그렇더라고요",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044382.1.13",
          "form": "저도 폴바셋 좋아하는데ㅜ",
          "original_form": "저도 폴바셋 좋아하는데ㅜ",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044382.1.14",
          "form": "헐 대박!!!! 맞아요 폴바셋이야말로 ㅋㅋㅋㅋㅋ진짜 매장이너무적어요ㅠㅠㅠㅠㅠㅠ",
          "original_form": "헐 대박!!!! 맞아요 폴바셋이야말로 ㅋㅋㅋㅋㅋ진짜 매장이너무적어요ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044382.1.15",
          "form": "있던곳도 매번 문닫아요ㅜ",
          "original_form": "있던곳도 매번 문닫아요ㅜ",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044382.1.16",
          "form": "그러니까요ㅠㅠㅠ 아니 우리동네엔 왜안내주냐구요ㅠㅠㅠㅠ",
          "original_form": "그러니까요ㅠㅠㅠ 아니 우리동네엔 왜안내주냐구요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044382.1.17",
          "form": "아...ㅜㅠ 많이 어렵나봐요ㅜㅜ 속상",
          "original_form": "아...ㅜㅠ 많이 어렵나봐요ㅜㅜ 속상",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044382.1.18",
          "form": "폴바셋은 어디 대기업 소속이 아닌가봐요?!",
          "original_form": "폴바셋은 어디 대기업 소속이 아닌가봐요?!",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044382.1.19",
          "form": "매일유업에서 하는데",
          "original_form": "매일유업에서 하는데",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044382.1.20",
          "form": "아 매일유업꺼에요?!?!? 우유만 매일꺼 쓰는줄알았는데!!",
          "original_form": "아 매일유업꺼에요?!?!? 우유만 매일꺼 쓰는줄알았는데!!",
