      "utterance": [
        {
          "id": "MDRW2100041427.1.1",
          "form": "메론 수박을 보았는데",
          "original_form": "메론 수박을 보았는데",
          "speaker_id": "2",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100041427.1.2",
          "form": "과일은 비싸게 주고 사먹어야 돼요",
          "original_form": "과일은 비싸게 주고 사먹어야 돼요",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041427.1.3",
          "form": "메롱 수박",
          "original_form": "메롱 수박",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041427.1.4",
          "form": "마시멜로만큼 많이 달지 않고 실감은 멜론이랑 비슷했어요",
          "original_form": "마시멜로만큼 많이 달지 않고 실감은 멜론이랑 비슷했어요",
          "speaker_id": "2",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041427.1.5",
          "form": "마시멜로가 아니라 맛이 멜론만큼",
          "original_form": "마시멜로가 아니라 맛이 멜론만큼",
          "speaker_id": "2",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041427.1.6",
          "form": "멜로는 달아야죠",
          "original_form": "멜로는 달아야죠",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041427.1.7",
          "form": "어 아 그렇구나",
          "original_form": "어 아 그렇구나",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041427.1.8",
          "form": "멜론 하니까 멜론 빙수 먹고 싶네요",
          "original_form": "멜론 하니까 멜론 빙수 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041427.1.9",
          "form": "저는 메로나 먹고 싶어요",
          "original_form": "저는 메로나 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041427.1.10",
          "form": "올 때 메로나",
          "original_form": "올 때 메로나",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041427.1.11",
          "form": "메로나 맛있죠 메로나 소주의 타서 먹으면 맛있어 지는데",
          "original_form": "메로나 맛있죠 메로나 소주의 타서 먹으면 맛있어 지는데",
          "speaker_id": "2",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041427.1.12",
          "form": "스크루 바에 소주 타서 먹어도 맛있어요",
          "original_form": "스크루 바에 소주 타서 먹어도 맛있어요",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041427.1.13",
          "form": "웨슬리에서 올 때 메로나라는 능력 있어요",
          "original_form": "웨슬리에서 올 때 메로나라는 능력 있어요",
          "speaker_id": "2",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041427.1.14",
          "form": "한참 대학교 때 하던 거죠",
          "original_form": "한참 대학교 때 하던 거죠",
          "speaker_id": "1",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100041427.1.15",
          "form": "능력 아니라 음료수",
          "original_form": "능력 아니라 음료수",
          "speaker_id": "2",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100041427.1.16",
          "form": "인정입니다",
          "original_form": "인정입니다",
          "speaker_id": "2",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100041427.1.17",
          "form": "그 웨슬리는 진짜 꼭 가봐야 될 거 같애요",
          "original_form": "그 웨슬리는 진짜 꼭 가봐야 될 거 같애요",
          "speaker_id": "1",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100041427.1.18",
          "form": "거기서 한 10시간은 있을 수 있을 거 같아요",
          "original_form": "거기서 한 10시간은 있을 수 있을 거 같아요",
          "speaker_id": "1",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100041427.1.19",
          "form": "전에 친구 집에 갔는데 파인애플 갈아서 소주에 타서만들어주더라구요 진짜 맛있었어요",
          "original_form": "전에 친구 집에 갔는데 파인애플 갈아서 소주에 타서만들어주더라구요 진짜 맛있었어요",
          "speaker_id": "2",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100041427.1.20",
          "form": "아이네 프 소주 대박이네요",
          "original_form": "아이네 프 소주 대박이네요",
          "speaker_id": "1",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100041427.1.21",
          "form": "좁아서 테이블이 거의 없습니다 테이크 아웃 점입니다",
          "original_form": "좁아서 테이블이 거의 없습니다 테이크 아웃 점입니다",
          "speaker_id": "2",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100041427.1.22",
          "form": "테이크아웃 집이요",
          "original_form": "테이크아웃 집이요",
          "speaker_id": "1",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100041427.1.23",
          "form": "아 그건 너무 아쉽다",
          "original_form": "아 그건 너무 아쉽다",
          "speaker_id": "1",
          "time": "20210905 23:28"
        }
      ]
    }
