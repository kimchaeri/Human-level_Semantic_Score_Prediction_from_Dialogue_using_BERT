      "utterance": [
        {
          "id": "MDRW2100012375.1.1",
          "form": "최근에 시험보신적있으세요?",
          "original_form": "최근에 시험보신적있으세요?",
          "speaker_id": "1",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100012375.1.2",
          "form": "아니요~~ 시험이란말도 오랫만이네요",
          "original_form": "아니요~~ 시험이란말도 오랫만이네요",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100012375.1.3",
          "form": "마지막으로 수업 들은게 언제예요?",
          "original_form": "마지막으로 수업 들은게 언제예요?",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100012375.1.4",
          "form": "10년은된거같네요",
          "original_form": "10년은된거같네요",
          "speaker_id": "2",
          "time": "20210826 22:08"
        },
        {
          "id": "MDRW2100012375.1.5",
          "form": "전 얼마전에 자격증 시험쳤어요 ㅋㅋ",
          "original_form": "전 얼마전에 자격증 시험쳤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100012375.1.6",
          "form": "오 무슨시험인지 물어봐도되요?",
          "original_form": "오 무슨시험인지 물어봐도되요?",
          "speaker_id": "2",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100012375.1.7",
          "form": "컬러리스트 기사 시험이요!",
          "original_form": "컬러리스트 기사 시험이요!",
          "speaker_id": "1",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100012375.1.8",
          "form": "우와 !",
          "original_form": "우와 !",
          "speaker_id": "2",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100012375.1.9",
          "form": "저도 관심있는분야인데",
          "original_form": "저도 관심있는분야인데",
          "speaker_id": "2",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100012375.1.10",
          "form": "직업이 이쪽이신가요?!",
          "original_form": "직업이 이쪽이신가요?!",
          "speaker_id": "1",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100012375.1.11",
          "form": "운좋게 한번에 붙었지만 ㅠㅠ 다시하라고하면 못할 시험이었어요",
          "original_form": "운좋게 한번에 붙었지만 ㅠㅠ 다시하라고하면 못할 시험이었어요",
          "speaker_id": "1",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100012375.1.12",
          "form": "아뇨 예전에 꽃꽂이를 배웠는데",
          "original_form": "아뇨 예전에 꽃꽂이를 배웠는데",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100012375.1.13",
          "form": "컬러리스트 공부하면",
          "original_form": "컬러리스트 공부하면",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100012375.1.14",
          "form": "혹시 도움이될까해서",
          "original_form": "혹시 도움이될까해서",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100012375.1.15",
          "form": "오 도움될거예요!",
          "original_form": "오 도움될거예요!",
          "speaker_id": "1",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100012375.1.16",
          "form": "색과 관련있는 모든 분야에 확실히 도움은 될거같아요 ㅎㅎ 벼락치기 시험만 아니라면 도전해볼만합니당",
          "original_form": "색과 관련있는 모든 분야에 확실히 도움은 될거같아요 ㅎㅎ 벼락치기 시험만 아니라면 도전해볼만합니당",
          "speaker_id": "1",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100012375.1.17",
          "form": "요즘 힐링한다고 컬러링도 하고있는데",
          "original_form": "요즘 힐링한다고 컬러링도 하고있는데",
          "speaker_id": "2",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100012375.1.18",
          "form": "이게 힐링인지",
          "original_form": "이게 힐링인지",
          "speaker_id": "2",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100012375.1.19",
          "form": "눈빠질거같고 힘들어서포기포기",
          "original_form": "눈빠질거같고 힘들어서포기포기",
          "speaker_id": "2",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100012375.1.20",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ저도 그래서 컬러링 안맞더라구요ㅠㅠ 넘 칸도 작구..",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ저도 그래서 컬러링 안맞더라구요ㅠㅠ 넘 칸도 작구..",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100012375.1.21",
          "form": "맞아요 ㅋㅋㅋ 컬러공부하면 색 조합도 더",
          "original_form": "맞아요 ㅋㅋㅋ 컬러공부하면 색 조합도 더",
          "speaker_id": "2",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100012375.1.22",
          "form": "컬러리스트 시험도 그거랑 비슷한 느낌이 있어요..ㅋㅋ",
          "original_form": "컬러리스트 시험도 그거랑 비슷한 느낌이 있어요..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100012375.1.23",
          "form": "예쁘게 할텐데",
          "original_form": "예쁘게 할텐데",
          "speaker_id": "2",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100012375.1.24",
          "form": "고개숙이고 앉아가지고..이게 시험공분지..머하잔건지..ㅠㅋㅋㅋ",
          "original_form": "고개숙이고 앉아가지고..이게 시험공분지..머하잔건지..ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100012375.1.25",
          "form": "재미라도 있으면 하겠는데",
          "original_form": "재미라도 있으면 하겠는데",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100012375.1.26",
          "form": "저는 재미도없고 눈아프더라구요",
          "original_form": "저는 재미도없고 눈아프더라구요",
          "speaker_id": "2",
          "time": "20210826 22:24"
        }
      ]
    }
