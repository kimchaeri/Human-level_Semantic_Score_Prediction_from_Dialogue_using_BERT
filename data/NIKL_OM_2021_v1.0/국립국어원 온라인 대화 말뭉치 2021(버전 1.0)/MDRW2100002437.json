      "utterance": [
        {
          "id": "MDRW2100002437.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 12:15"
        },
        {
          "id": "MDRW2100002437.1.2",
          "form": "안녕하세요 반가워요",
          "original_form": "안녕하세요 반가워요",
          "speaker_id": "2",
          "time": "20210820 12:15"
        },
        {
          "id": "MDRW2100002437.1.3",
          "form": "올림픽 보셨나요~?",
          "original_form": "올림픽 보셨나요~?",
          "speaker_id": "2",
          "time": "20210820 12:15"
        },
        {
          "id": "MDRW2100002437.1.4",
          "form": "다 챙겨보진 않았지만 몇몇 경기는 봤어요",
          "original_form": "다 챙겨보진 않았지만 몇몇 경기는 봤어요",
          "speaker_id": "1",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100002437.1.5",
          "form": "저는 양궁이 너무재밌었어요",
          "original_form": "저는 양궁이 너무재밌었어요",
          "speaker_id": "2",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100002437.1.6",
          "form": "어떤스포츠 좋아하세요?",
          "original_form": "어떤스포츠 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100002437.1.7",
          "form": "아 저도 양궁은 봤어요",
          "original_form": "아 저도 양궁은 봤어요",
          "speaker_id": "1",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100002437.1.8",
          "form": "그래도 챙겨보는 건 야구에요",
          "original_form": "그래도 챙겨보는 건 야구에요",
          "speaker_id": "1",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100002437.1.9",
          "form": "야구 어플깔고 저도보고있어요",
          "original_form": "야구 어플깔고 저도보고있어요",
          "speaker_id": "2",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002437.1.10",
          "form": "요즘은 띄엄띄엄 하다보니 좀 시들해졌는데",
          "original_form": "요즘은 띄엄띄엄 하다보니 좀 시들해졌는데",
          "speaker_id": "1",
          "time": "20210820 12:21"
        },
        {
          "id": "MDRW2100002437.1.11",
          "form": "전에는 퇴근하고 매일챙겨봤어요",
          "original_form": "전에는 퇴근하고 매일챙겨봤어요",
          "speaker_id": "1",
          "time": "20210820 12:22"
        },
        {
          "id": "MDRW2100002437.1.12",
          "form": "야구 직접 보러도 다니시나요~?",
          "original_form": "야구 직접 보러도 다니시나요~?",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002437.1.13",
          "form": "예전에는 친구들이랑 몇번 보러갔었어요",
          "original_form": "예전에는 친구들이랑 몇번 보러갔었어요",
          "speaker_id": "1",
          "time": "20210820 12:29"
        },
        {
          "id": "MDRW2100002437.1.14",
          "form": "직관하는 재미는 티비랑은 다를거같아요",
          "original_form": "직관하는 재미는 티비랑은 다를거같아요",
          "speaker_id": "2",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002437.1.15",
          "form": "친구들이 야구를 좋아해서 연간권까지 끊어가면서 쫒아다녔거든요",
          "original_form": "친구들이 야구를 좋아해서 연간권까지 끊어가면서 쫒아다녔거든요",
          "speaker_id": "1",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002437.1.16",
          "form": "확실히 가서 보면 재미있긴 하더라고요",
          "original_form": "확실히 가서 보면 재미있긴 하더라고요",
          "speaker_id": "1",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002437.1.17",
          "form": "직접  가서 보신 경기 있으세여?",
          "original_form": "직접  가서 보신 경기 있으세여?",
          "speaker_id": "1",
          "time": "20210825 09:22"
        },
        {
          "id": "MDRW2100002437.1.18",
          "form": "네 축구보러 가봤어요",
          "original_form": "네 축구보러 가봤어요",
          "speaker_id": "2",
