      "utterance": [
        {
          "id": "MDRW2100032501.1.1",
          "form": "이건 몰랐네",
          "original_form": "이건 몰랐네",
          "speaker_id": "1",
          "time": "20210905 19:41"
        },
        {
          "id": "MDRW2100032501.1.2",
          "form": "아 근데 난 커피 안마심",
          "original_form": "아 근데 난 커피 안마심",
          "speaker_id": "2",
          "time": "20210905 19:41"
        },
        {
          "id": "MDRW2100032501.1.3",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "2",
          "time": "20210905 19:41"
        },
        {
          "id": "MDRW2100032501.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:41"
        },
        {
          "id": "MDRW2100032501.1.5",
          "form": "난 커피 마시지만 굳이 여긴 안가긴 하지...",
          "original_form": "난 커피 마시지만 굳이 여긴 안가긴 하지...",
          "speaker_id": "1",
          "time": "20210905 19:42"
        },
        {
          "id": "MDRW2100032501.1.6",
          "form": "커피 많이 마시는 분한테는",
          "original_form": "커피 많이 마시는 분한테는",
          "speaker_id": "2",
          "time": "20210905 19:42"
        },
        {
          "id": "MDRW2100032501.1.7",
          "form": "중요한 사안인걸까",
          "original_form": "중요한 사안인걸까",
          "speaker_id": "2",
          "time": "20210905 19:43"
        },
        {
          "id": "MDRW2100032501.1.8",
          "form": "프랜차이즈 다된다고 했는데",
          "original_form": "프랜차이즈 다된다고 했는데",
          "speaker_id": "1",
          "time": "20210905 19:43"
        },
        {
          "id": "MDRW2100032501.1.9",
          "form": "요두개만 안된다니 좀 신기한거 같기두 하고",
          "original_form": "요두개만 안된다니 좀 신기한거 같기두 하고",
          "speaker_id": "1",
          "time": "20210905 19:43"
        },
        {
          "id": "MDRW2100032501.1.10",
          "form": "걔들이 거부한건가",
          "original_form": "걔들이 거부한건가",
          "speaker_id": "2",
          "time": "20210905 19:44"
        },
        {
          "id": "MDRW2100032501.1.11",
          "form": "뭔가 다른 이유가 있겠지...?",
          "original_form": "뭔가 다른 이유가 있겠지...?",
          "speaker_id": "1",
          "time": "20210905 19:44"
        },
        {
          "id": "MDRW2100032501.1.12",
          "form": "콕 찝은게 이상해 ㅋㅋㅋㅋ",
          "original_form": "콕 찝은게 이상해 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032501.1.13",
          "form": "스벅은 직영점이라",
          "original_form": "스벅은 직영점이라",
          "speaker_id": "1",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032501.1.14",
          "form": "안될거같긴함",
          "original_form": "안될거같긴함",
          "speaker_id": "1",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032501.1.15",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210905 19:46"
        },
        {
          "id": "MDRW2100032501.1.16",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "2",
          "time": "20210905 19:46"
        },
        {
          "id": "MDRW2100032501.1.17",
          "form": "이디야는 왜 안되지 했더니 이디야가 아니라 이케아잖아",
          "original_form": "이디야는 왜 안되지 했더니 이디야가 아니라 이케아잖아",
          "speaker_id": "1",
          "time": "20210905 19:46"
        },
        {
          "id": "MDRW2100032501.1.18",
          "form": "ㅋㅋㅋ ㅋ ㅋ ㅋ ㅋ ㅋㅋ ㅋ ㅋ ㅋ ㅋ ㅋ ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ ㅋ ㅋ ㅋ ㅋ ㅋㅋ ㅋ ㅋ ㅋ ㅋ ㅋ ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:46"
        },
        {
          "id": "MDRW2100032501.1.19",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100032501.1.20",
          "form": "나도 이디야로 봄",
          "original_form": "나도 이디야로 봄",
          "speaker_id": "2",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032501.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032501.1.22",
          "form": "이디얔ㅋㅋㅋㅋ",
          "original_form": "이디얔ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032501.1.23",
          "form": "이캐. 이케야 ..",
          "original_form": "이캐. 이케야 ..",
          "speaker_id": "2",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032501.1.24",
          "form": "방금 바뀐 거 아니야?",
          "original_form": "방금 바뀐 거 아니야?",
          "speaker_id": "2",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032501.1.25",
          "form": "아까 분명 이디야 아니었어?",
          "original_form": "아까 분명 이디야 아니었어?",
          "speaker_id": "2",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032501.1.26",
          "form": "ㅋㅋ ㅋㅋ ㅋ ㅋ ㅋ ㅋㅋ ㅋ ㅋ ㅋ ㅋ ㅋ 난... 이디야로 봤는데...",
          "original_form": "ㅋㅋ ㅋㅋ ㅋ ㅋ ㅋ ㅋㅋ ㅋ ㅋ ㅋ ㅋ ㅋ 난... 이디야로 봤는데...",
          "speaker_id": "1",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032501.1.27",
          "form": "모지...",
          "original_form": "모지...",
          "speaker_id": "1",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032501.1.28",
          "form": "ㅋㅋ ㅋ ㅋ ㅋㅋ ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ ㅋ ㅋ ㅋㅋ ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032501.1.29",
          "form": "글구 이케아는 안될 만도 하지",
          "original_form": "글구 이케아는 안될 만도 하지",
          "speaker_id": "1",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032501.1.30",
          "form": "얘가 안되는게 이상한 건가",
          "original_form": "얘가 안되는게 이상한 건가",
          "speaker_id": "1",
          "time": "20210905 19:51"
        },
        {
          "id": "MDRW2100032501.1.31",
          "form": "별로 안 이상해!",
          "original_form": "별로 안 이상해!",
          "speaker_id": "2",
          "time": "20210905 19:52"
        },
        {
          "id": "MDRW2100032501.1.32",
          "form": "이케아가 안되는 게 요케 특이 표시되는 게 좀 신기하네",
          "original_form": "이케아가 안되는 게 요케 특이 표시되는 게 좀 신기하네",
          "speaker_id": "1",
          "time": "20210905 19:52"
        },
        {
          "id": "MDRW2100032501.1.33",
          "form": "문의한 사람이 많아거 콕집어서 안ㄷㄴ다고 했나보다",
          "original_form": "문의한 사람이 많아거 콕집어서 안ㄷㄴ다고 했나보다",
          "speaker_id": "1",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100032501.1.34",
          "form": "아하아하",
          "original_form": "아하아하",
          "speaker_id": "2",
          "time": "20210905 19:54"
        }
      ]
    }
