      "utterance": [
        {
          "id": "MDRW2100043977.1.1",
          "form": "가족과 더불어 살기가 쉽지 않아~",
          "original_form": "가족과 더불어 살기가 쉽지 않아~",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043977.1.2",
          "form": "어 빨리 독립해야지",
          "original_form": "어 빨리 독립해야지",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043977.1.3",
          "form": "엄빠와 더불어살기",
          "original_form": "엄빠와 더불어살기",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043977.1.4",
          "form": "가족끼리도 각자 영역이 필요해",
          "original_form": "가족끼리도 각자 영역이 필요해",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043977.1.5",
          "form": "지혜가 필요합니다",
          "original_form": "지혜가 필요합니다",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043977.1.6",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043977.1.7",
          "form": "엄마 얼른 주무시지",
          "original_form": "엄마 얼른 주무시지",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043977.1.8",
          "form": "안자려고 용쓰시네ㅜ",
          "original_form": "안자려고 용쓰시네ㅜ",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043977.1.9",
          "form": "근데 엄마는 걍 생활비 안받고 계속 참견하고 영역침해하고 싶어할걸",
          "original_form": "근데 엄마는 걍 생활비 안받고 계속 참견하고 영역침해하고 싶어할걸",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043977.1.10",
          "form": "그럼그럼",
          "original_form": "그럼그럼",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043977.1.11",
          "form": "걍 생활비 받으시지",
          "original_form": "걍 생활비 받으시지",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043977.1.12",
          "form": "어제도 결국 그냥 난 참견하고 싶고",
          "original_form": "어제도 결국 그냥 난 참견하고 싶고",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043977.1.13",
          "form": "너의 프라이버시에 침해할 것이다",
          "original_form": "너의 프라이버시에 침해할 것이다",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043977.1.14",
          "form": "내 권력을 누릴것이다",
          "original_form": "내 권력을 누릴것이다",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043977.1.15",
          "form": "무슨얘기를 해도 안바뀔듯...",
          "original_form": "무슨얘기를 해도 안바뀔듯...",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043977.1.16",
          "form": "응 글치...",
          "original_form": "응 글치...",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043977.1.17",
          "form": "나이 드시면서 섭섭한 건 더 많아지고",
          "original_form": "나이 드시면서 섭섭한 건 더 많아지고",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043977.1.18",
          "form": "유세부리지말라면서",
          "original_form": "유세부리지말라면서",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043977.1.19",
          "form": "유세부리시기",
          "original_form": "유세부리시기",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043977.1.20",
          "form": "에고ㅠ",
          "original_form": "에고ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043977.1.21",
          "form": "힘들구만",
          "original_form": "힘들구만",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043977.1.22",
          "form": "돈 열심히 모으자",
          "original_form": "돈 열심히 모으자",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043977.1.23",
          "form": "내말이",
          "original_form": "내말이",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043977.1.24",
          "form": "결국 돈모아서 적당히 거리두는게 제일이야",
          "original_form": "결국 돈모아서 적당히 거리두는게 제일이야",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043977.1.25",
          "form": "내집 내냉장고 내방 생겨야",
          "original_form": "내집 내냉장고 내방 생겨야",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043977.1.26",
          "form": "서로 온화해짐",
          "original_form": "서로 온화해짐",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043977.1.27",
          "form": "응 요즘 실감해",
          "original_form": "응 요즘 실감해",
          "speaker_id": "1",
          "time": "20210907 21:16"
        }
      ]
    }
  ]
