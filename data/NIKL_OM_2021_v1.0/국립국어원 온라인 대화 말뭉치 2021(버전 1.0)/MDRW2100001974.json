      "utterance": [
        {
          "id": "MDRW2100001974.1.1",
          "form": "기숙사 사시면 밥은 어떻게 해드세요?",
          "original_form": "기숙사 사시면 밥은 어떻게 해드세요?",
          "speaker_id": "1",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001974.1.2",
          "form": "기숙사 식사 나와요!!",
          "original_form": "기숙사 식사 나와요!!",
          "speaker_id": "2",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100001974.1.3",
          "form": "아침이랑 주말은 안나오지만",
          "original_form": "아침이랑 주말은 안나오지만",
          "speaker_id": "2",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100001974.1.4",
          "form": "그래서 주말에는 주로 시켜 먹습니다!!",
          "original_form": "그래서 주말에는 주로 시켜 먹습니다!!",
          "speaker_id": "2",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001974.1.5",
          "form": "식사가 나와서 다행이네요",
          "original_form": "식사가 나와서 다행이네요",
          "speaker_id": "1",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001974.1.6",
          "form": "주말에 뭐 시켜드세요?",
          "original_form": "주말에 뭐 시켜드세요?",
          "speaker_id": "1",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001974.1.7",
          "form": "주말에는 주로 초밥 혹은 치킨 혹은 돈까스",
          "original_form": "주말에는 주로 초밥 혹은 치킨 혹은 돈까스",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001974.1.8",
          "form": "혹은 대학교 앞에 있는 도시락 가게에서 먹어요",
          "original_form": "혹은 대학교 앞에 있는 도시락 가게에서 먹어요",
          "speaker_id": "2",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001974.1.9",
          "form": "한솥",
          "original_form": "한솥",
          "speaker_id": "2",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001974.1.10",
          "form": "오모 돈까스 맛있겠어요!!!!!!",
          "original_form": "오모 돈까스 맛있겠어요!!!!!!",
          "speaker_id": "1",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001974.1.11",
          "form": "혹시 학교 근처에 알촌도 있나요?",
          "original_form": "혹시 학교 근처에 알촌도 있나요?",
          "speaker_id": "1",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001974.1.12",
          "form": "알촌..?",
          "original_form": "알촌..?",
          "speaker_id": "2",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001974.1.13",
          "form": "그게 뭐죠..?",
          "original_form": "그게 뭐죠..?",
          "speaker_id": "2",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001974.1.14",
          "form": "알촌이라는 프렌차이즈 밥 가게인데",
          "original_form": "알촌이라는 프렌차이즈 밥 가게인데",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001974.1.15",
          "form": "여기 되게 저렴하면서 맛있어요 하하 ㅎㅎ",
          "original_form": "여기 되게 저렴하면서 맛있어요 하하 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001974.1.16",
          "form": "우와..",
          "original_form": "우와..",
          "speaker_id": "2",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001974.1.17",
          "form": "배달되요?",
          "original_form": "배달되요?",
          "speaker_id": "2",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001974.1.18",
          "form": "나중에 가면 꼭 찾아봐야겠어요!!",
          "original_form": "나중에 가면 꼭 찾아봐야겠어요!!",
          "speaker_id": "2",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001974.1.19",
          "form": "보통 학교 근처에 있어서 배달은 안시켜봤어요 ㅎ ㅎ",
          "original_form": "보통 학교 근처에 있어서 배달은 안시켜봤어요 ㅎ ㅎ",
          "speaker_id": "1",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001974.1.20",
          "form": "왜 저희 학교는... ㅠㅠ",
          "original_form": "왜 저희 학교는... ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001974.1.21",
          "form": "저희 학교는 외각이라..",
          "original_form": "저희 학교는 외각이라..",
          "speaker_id": "2",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001974.1.22",
          "form": "좀 나가야 해요.. ㅠㅠ",
          "original_form": "좀 나가야 해요.. ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100001974.1.23",
          "form": "앞애 몇몇 가게 빼고는..",
          "original_form": "앞애 몇몇 가게 빼고는..",
          "speaker_id": "2",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100001974.1.24",
          "form": "그래서 배달 시켜먹어요..",
          "original_form": "그래서 배달 시켜먹어요..",
          "speaker_id": "2",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100001974.1.25",
          "form": "그럼 배달비가 꽤 나오겠어요",
          "original_form": "그럼 배달비가 꽤 나오겠어요",
          "speaker_id": "1",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100001974.1.26",
          "form": "요새 배달비 너무 비싸서 흑",
          "original_form": "요새 배달비 너무 비싸서 흑",
          "speaker_id": "1",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100001974.1.27",
          "form": "... 맞아요..",
          "original_form": "... 맞아요..",
          "speaker_id": "2",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001974.1.28",
          "form": "배달료.. 3000원..",
          "original_form": "배달료.. 3000원..",
          "speaker_id": "2",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001974.1.29",
          "form": "너무해에...",
          "original_form": "너무해에...",
          "speaker_id": "2",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001974.1.30",
          "form": "기본 요금까지..",
          "original_form": "기본 요금까지..",
          "speaker_id": "2",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001974.1.31",
          "form": "그래서 룸메랑 먹어요..",
          "original_form": "그래서 룸메랑 먹어요..",
          "speaker_id": "2",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001974.1.32",
          "form": "그렇게라도 배달비를 줄이는 거 좋은 거 같아요,,,",
          "original_form": "그렇게라도 배달비를 줄이는 거 좋은 거 같아요,,,",
          "speaker_id": "1",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100001974.1.33",
          "form": "예전에는 배달비 다 공짜였는데... 지금은..",
          "original_form": "예전에는 배달비 다 공짜였는데... 지금은..",
          "speaker_id": "1",
          "time": "20210819 22:49"
        }
      ]
