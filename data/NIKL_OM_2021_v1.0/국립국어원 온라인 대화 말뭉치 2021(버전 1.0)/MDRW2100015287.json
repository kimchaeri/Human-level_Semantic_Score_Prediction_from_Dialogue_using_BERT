        },
        {
          "id": "MDRW2100015287.1.2",
          "form": "말랑말랑하네요",
          "original_form": "말랑말랑하네요",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015287.1.3",
          "form": "주제 뭔가요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "주제 뭔가요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015287.1.4",
          "form": "상대방 맞춤형 주제입니다",
          "original_form": "상대방 맞춤형 주제입니다",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015287.1.5",
          "form": "연애를 오랜만에 했더니 적응 안되네요",
          "original_form": "연애를 오랜만에 했더니 적응 안되네요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015287.1.6",
          "form": "고수님 많은 지도편달 부탁드립니다",
          "original_form": "고수님 많은 지도편달 부탁드립니다",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015287.1.7",
          "form": "예? 누가 고수죠? 저는 말하는 감자입니다ㅠ",
          "original_form": "예? 누가 고수죠? 저는 말하는 감자입니다ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015287.1.8",
          "form": "이상형이 어떻게 되나요?",
          "original_form": "이상형이 어떻게 되나요?",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015287.1.9",
          "form": "ㅋㅋㅋㅋ이상형은 착한 사람입니다",
          "original_form": "ㅋㅋㅋㅋ이상형은 착한 사람입니다",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015287.1.10",
          "form": "착한 사람들 좋아요!",
          "original_form": "착한 사람들 좋아요!",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015287.1.11",
          "form": "이상형은 어떻게 되나여",
          "original_form": "이상형은 어떻게 되나여",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015287.1.12",
          "form": "너무 답이 정해져있네요",
          "original_form": "너무 답이 정해져있네요",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015287.1.13",
          "form": "저도 착한사람인데",
          "original_form": "저도 착한사람인데",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015287.1.14",
          "form": "착한데 안질리는 사람이요 ㅋㅋㅋㅋ",
          "original_form": "착한데 안질리는 사람이요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015287.1.15",
          "form": "똑똑하고 잘생기고 모든 주제의 대화 쌉가능하고",
          "original_form": "똑똑하고 잘생기고 모든 주제의 대화 쌉가능하고",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015287.1.16",
          "form": "키는 별로 안봐요 다행이죠?^^",
          "original_form": "키는 별로 안봐요 다행이죠?^^",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015287.1.17",
          "form": "name1님께 정말 잘어울리는 사람일거 같네요",
          "original_form": "&name1&님께 정말 잘어울리는 사람일거 같네요",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015287.1.18",
          "form": "그대는 완벽한 여성",
          "original_form": "그대는 완벽한 여성",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015287.1.19",
          "form": "흐앙 그런 사람이 있을까요?",
          "original_form": "흐앙 그런 사람이 있을까요?",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015287.1.20",
          "form": "새로 시작한 그 분은 착한 분입니까?",
          "original_form": "새로 시작한 그 분은 착한 분입니까?",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015287.1.21",
          "form": "이미 만나고 있지 않나요",
          "original_form": "이미 만나고 있지 않나요",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015287.1.22",
          "form": "네 너무 착해서 이런 사람이 있구나 놀라고 있습니다",
          "original_form": "네 너무 착해서 이런 사람이 있구나 놀라고 있습니다",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015287.1.23",
          "form": "우와우와 다행입니다",
          "original_form": "우와우와 다행입니다",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015287.1.24",
          "form": "차카니와 차카니가 만났네요",
          "original_form": "차카니와 차카니가 만났네요",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015287.1.25",
          "form": "그래서 착한 대화를 하고 있는 중이에요",
          "original_form": "그래서 착한 대화를 하고 있는 중이에요",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015287.1.26",
          "form": "약간 교과서 실릴 대화에요",
          "original_form": "약간 교과서 실릴 대화에요",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015287.1.27",
          "form": "EBS재질이네요!!!",
          "original_form": "EBS재질이네요!!!",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015287.1.28",
          "form": "아직 첫 시작이라 그럴것같아요",
          "original_form": "아직 첫 시작이라 그럴것같아요",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015287.1.29",
          "form": "ㅋㅋㅋㅋ교육적입니다",
          "original_form": "ㅋㅋㅋㅋ교육적입니다",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015287.1.30",
          "form": "좀 더 친해지면 더욱 재밌을 것 같아요",
          "original_form": "좀 더 친해지면 더욱 재밌을 것 같아요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015287.1.31",
          "form": "맞아요 첫 시작을 소개팅으로 해본게 처음이라 낯설어요",
          "original_form": "맞아요 첫 시작을 소개팅으로 해본게 처음이라 낯설어요",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015287.1.32",
          "form": "서른세살의 그분이 이분일까요!!",
          "original_form": "서른세살의 그분이 이분일까요!!",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015287.1.33",
          "form": "헐 그러네요?!?!",
          "original_form": "헐 그러네요?!?!",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015287.1.34",
          "form": "그럴수도 있을 거 같기도 하고",
          "original_form": "그럴수도 있을 거 같기도 하고",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015287.1.35",
          "form": "소개팅도 처음아닌가요 ??",
          "original_form": "소개팅도 처음아닌가요 ??",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
