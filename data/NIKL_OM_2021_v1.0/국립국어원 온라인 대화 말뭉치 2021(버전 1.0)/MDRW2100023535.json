      "utterance": [
        {
          "id": "MDRW2100023535.1.1",
          "form": "거기 날씨는 어떠세요?",
          "original_form": "거기 날씨는 어떠세요?",
          "speaker_id": "1",
          "time": "20210830 11:10"
        },
        {
          "id": "MDRW2100023535.1.2",
          "form": "여긴 아침부터 엄청 더워요",
          "original_form": "여긴 아침부터 엄청 더워요",
          "speaker_id": "2",
          "time": "20210830 11:12"
        },
        {
          "id": "MDRW2100023535.1.3",
          "form": "여기 우중충해요",
          "original_form": "여기 우중충해요",
          "speaker_id": "1",
          "time": "20210830 11:12"
        },
        {
          "id": "MDRW2100023535.1.4",
          "form": "저흰 내일 비소식있어용",
          "original_form": "저흰 내일 비소식있어용",
          "speaker_id": "2",
          "time": "20210830 11:12"
        },
        {
          "id": "MDRW2100023535.1.5",
          "form": "저희도요\n내일 이사인데 걱정이네요",
          "original_form": "저희도요\n내일 이사인데 걱정이네요",
          "speaker_id": "1",
          "time": "20210830 11:27"
        },
        {
          "id": "MDRW2100023535.1.6",
          "form": "헛 내일 이사신건가요?!",
          "original_form": "헛 내일 이사신건가요?!",
          "speaker_id": "2",
          "time": "20210830 11:28"
        },
        {
          "id": "MDRW2100023535.1.7",
          "form": "비오면 많이 불편하시겠어요ㅜ",
          "original_form": "비오면 많이 불편하시겠어요ㅜ",
          "speaker_id": "2",
          "time": "20210830 11:28"
        },
        {
          "id": "MDRW2100023535.1.8",
          "form": "그래도 비오는날 이사하면  잘산다는 말이있으니",
          "original_form": "그래도 비오는날 이사하면  잘산다는 말이있으니",
          "speaker_id": "2",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100023535.1.9",
          "form": "새로이사가는 집에서 잘사실꺼에요",
          "original_form": "새로이사가는 집에서 잘사실꺼에요",
          "speaker_id": "2",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100023535.1.10",
          "form": "여기 이사올때도 겨울이었는데 비왔었거든요\n이사때마다 비오네요ㅋㅋ",
          "original_form": "여기 이사올때도 겨울이었는데 비왔었거든요\n이사때마다 비오네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100023535.1.11",
          "form": "잘살면 좋죠",
          "original_form": "잘살면 좋죠",
          "speaker_id": "1",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100023535.1.12",
          "form": "ㅋㅋㅋ 좋은징조라 여겨요!ㅎㅎ",
          "original_form": "ㅋㅋㅋ 좋은징조라 여겨요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100023535.1.13",
          "form": "가을장마라더니 쭈욱 비소식이있네요",
          "original_form": "가을장마라더니 쭈욱 비소식이있네요",
          "speaker_id": "2",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100023535.1.14",
          "form": "그쵸ㅋㅋ 그래서 저런 속설이 나온 것 같아요",
          "original_form": "그쵸ㅋㅋ 그래서 저런 속설이 나온 것 같아요",
          "speaker_id": "1",
          "time": "20210830 11:34"
        },
        {
          "id": "MDRW2100023535.1.15",
          "form": "그니까요 가을장마..  넘기네요",
          "original_form": "그니까요 가을장마..  넘기네요",
          "speaker_id": "1",
          "time": "20210830 11:35"
        },
        {
          "id": "MDRW2100023535.1.16",
          "form": "근데 오늘은 30도까지올라서",
          "original_form": "근데 오늘은 30도까지올라서",
          "speaker_id": "2",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100023535.1.17",
          "form": "엄청 덥고 습해요",
          "original_form": "엄청 덥고 습해요",
          "speaker_id": "2",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100023535.1.18",
          "form": "불쾌지수 상승중입니다ㅜㅋ",
          "original_form": "불쾌지수 상승중입니다ㅜㅋ",
          "speaker_id": "2",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100023535.1.19",
          "form": "그니까요 차라리 덥기만하면 괜찮은데 습하니 진짜 끈적이고 짜증나요",
          "original_form": "그니까요 차라리 덥기만하면 괜찮은데 습하니 진짜 끈적이고 짜증나요",
          "speaker_id": "1",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023535.1.20",
          "form": "마스크쓰면 더 답답해요",
          "original_form": "마스크쓰면 더 답답해요",
          "speaker_id": "1",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023535.1.21",
          "form": "맞죠ㅠ 마스크 부분에 뾰루지가~~",
          "original_form": "맞죠ㅠ 마스크 부분에 뾰루지가~~",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023535.1.22",
          "form": "ㅜㅜ 너무더워서 땀차서",
          "original_form": "ㅜㅜ 너무더워서 땀차서",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023535.1.23",
          "form": "그니까요 얼굴에 뾰루지 천지예요",
          "original_form": "그니까요 얼굴에 뾰루지 천지예요",
          "speaker_id": "1",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100023535.1.24",
          "form": "덥기만하면 괜찮을텐데",
          "original_form": "덥기만하면 괜찮을텐데",
          "speaker_id": "2",
          "time": "20210830 11:44"
        },
        {
          "id": "MDRW2100023535.1.25",
          "form": "습한게 큰 문제인듯요ㅜㅜ",
          "original_form": "습한게 큰 문제인듯요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 11:44"
        },
        {
          "id": "MDRW2100023535.1.26",
          "form": "그니까요 습해서 숨쉬는게 더 어려워요",
          "original_form": "그니까요 습해서 숨쉬는게 더 어려워요",
          "speaker_id": "1",
          "time": "20210830 11:44"
        }
      ]
    }
