      "utterance": [
        {
          "id": "MDRW2100041964.1.1",
          "form": "엄청 좋아해요",
          "original_form": "엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041964.1.2",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041964.1.3",
          "form": "여름엔 늘 설빙가는거같아요",
          "original_form": "여름엔 늘 설빙가는거같아요",
          "speaker_id": "2",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041964.1.4",
          "form": "아 저도 설빙 좋아해요 인절미 빙수랑 메론빙수 너무 맛있어요",
          "original_form": "아 저도 설빙 좋아해요 인절미 빙수랑 메론빙수 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041964.1.5",
          "form": "저는 인절미 빙수가 제일 좋아요 클래식 ㅎㅎ",
          "original_form": "저는 인절미 빙수가 제일 좋아요 클래식 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041964.1.6",
          "form": "콩가루가 맛있죠!",
          "original_form": "콩가루가 맛있죠!",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041964.1.7",
          "form": "팥도요 ㅎㅎ",
          "original_form": "팥도요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041964.1.8",
          "form": "저는 편의점 빙수도 좋아해요!",
          "original_form": "저는 편의점 빙수도 좋아해요!",
          "speaker_id": "1",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041964.1.9",
          "form": "편의점에도 빙수 팔아요?",
          "original_form": "편의점에도 빙수 팔아요?",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041964.1.10",
          "form": "네 아이스크림처럼 떠먹는컵에 담아서요",
          "original_form": "네 아이스크림처럼 떠먹는컵에 담아서요",
          "speaker_id": "1",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041964.1.11",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041964.1.12",
          "form": "우유부어서 먹으면 가성비가 괜찮아요!",
          "original_form": "우유부어서 먹으면 가성비가 괜찮아요!",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041964.1.13",
          "form": "가격은 얼마나해요?",
          "original_form": "가격은 얼마나해요?",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041964.1.14",
          "form": "2천원정도요 ㅎㅎ",
          "original_form": "2천원정도요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 18:01"
