      "utterance": [
        {
          "id": "MDRW2100038825.1.1",
          "form": "계시는 곳에 비가 오나요?",
          "original_form": "계시는 곳에 비가 오나요?",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100038825.1.2",
          "form": "아니요 여긴 그냥 흐려요",
          "original_form": "아니요 여긴 그냥 흐려요",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100038825.1.3",
          "form": "저희 계속 비가 오다가 그쳤다 비가 오다그쳤다 그러네요",
          "original_form": "저희 계속 비가 오다가 그쳤다 비가 오다그쳤다 그러네요",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100038825.1.4",
          "form": "오락가락하는군요",
          "original_form": "오락가락하는군요",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100038825.1.5",
          "form": "큰 피해없어야할텐데",
          "original_form": "큰 피해없어야할텐데",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100038825.1.6",
          "form": "밤 되니 바람도 많이 불고",
          "original_form": "밤 되니 바람도 많이 불고",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100038825.1.7",
          "form": "감기 걸리기 딱 좋은 날씨네요ㅠ",
          "original_form": "감기 걸리기 딱 좋은 날씨네요ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100038825.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100038825.1.9",
          "form": "일교차크고",
          "original_form": "일교차크고",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100038825.1.10",
          "form": "바람많이 불어서",
          "original_form": "바람많이 불어서",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100038825.1.11",
          "form": "아프기 딱 좋은 날씨",
          "original_form": "아프기 딱 좋은 날씨",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038825.1.12",
          "form": "쨍 한햇볕이 보고 싶어요ㅋㅋ",
          "original_form": "쨍 한햇볕이 보고 싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038825.1.13",
          "form": "코로나 땜에 감기도  걸리면 안 될 거 같고",
          "original_form": "코로나 땜에 감기도  걸리면 안 될 거 같고",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038825.1.14",
          "form": "눈부신 햇살",
          "original_form": "눈부신 햇살",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038825.1.15",
          "form": "상상만해도 좋네요",
          "original_form": "상상만해도 좋네요",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038825.1.16",
          "form": "여름은 너무 더웠는데",
          "original_form": "여름은 너무 더웠는데",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038825.1.17",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038825.1.18",
          "form": "또 그 햇볕이 그리워지네요;",
          "original_form": "또 그 햇볕이 그리워지네요;",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038825.1.19",
          "form": "지나는거보니 그립기도",
          "original_form": "지나는거보니 그립기도",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038825.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100038825.1.21",
          "form": "정말 시간이 너무 빨리 지나가는 것 같아요",
          "original_form": "정말 시간이 너무 빨리 지나가는 것 같아요",
          "speaker_id": "1",
          "time": "20210902 21:52"
        }
