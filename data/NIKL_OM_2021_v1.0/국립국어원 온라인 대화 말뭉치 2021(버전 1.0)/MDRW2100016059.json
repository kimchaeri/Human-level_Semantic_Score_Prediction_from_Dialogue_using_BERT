      "utterance": [
        {
          "id": "MDRW2100016059.1.1",
          "form": "진짜 존경스럽습니다ㅜㄴ",
          "original_form": "진짜 존경스럽습니다ㅜㄴ",
          "speaker_id": "1",
          "time": "20210826 09:00"
        },
        {
          "id": "MDRW2100016059.1.2",
          "form": "ㅎㅎ요새 다시 전면등교해요",
          "original_form": "ㅎㅎ요새 다시 전면등교해요",
          "speaker_id": "2",
          "time": "20210826 09:01"
        },
        {
          "id": "MDRW2100016059.1.3",
          "form": "아 요새 조용하니 좋네요",
          "original_form": "아 요새 조용하니 좋네요",
          "speaker_id": "2",
          "time": "20210826 09:01"
        },
        {
          "id": "MDRW2100016059.1.4",
          "form": "아이들도 좋아하고요",
          "original_form": "아이들도 좋아하고요",
          "speaker_id": "2",
          "time": "20210826 09:01"
        },
        {
          "id": "MDRW2100016059.1.5",
          "form": "학교는 역시 가야 제맛이죠",
          "original_form": "학교는 역시 가야 제맛이죠",
          "speaker_id": "2",
          "time": "20210826 09:01"
        },
        {
          "id": "MDRW2100016059.1.6",
          "form": "아무리 코로나여도 집보단 학교가 좋죠",
          "original_form": "아무리 코로나여도 집보단 학교가 좋죠",
          "speaker_id": "2",
          "time": "20210826 09:02"
        },
        {
          "id": "MDRW2100016059.1.7",
          "form": "뭔가 행복해보이십니다.",
          "original_form": "뭔가 행복해보이십니다.",
          "speaker_id": "1",
          "time": "20210826 09:05"
        },
        {
          "id": "MDRW2100016059.1.8",
          "form": "ㅋㅋㅋ 학교보낸 9시가 최고인가봅니다",
          "original_form": "ㅋㅋㅋ 학교보낸 9시가 최고인가봅니다",
          "speaker_id": "1",
          "time": "20210826 09:05"
        },
        {
          "id": "MDRW2100016059.1.9",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:05"
        },
        {
          "id": "MDRW2100016059.1.10",
          "form": "근데 아이들이 온라인하니 학력이 너무 떨어져서",
          "original_form": "근데 아이들이 온라인하니 학력이 너무 떨어져서",
          "speaker_id": "2",
          "time": "20210826 09:06"
        },
        {
          "id": "MDRW2100016059.1.11",
          "form": "진짜 걱정이였어요",
          "original_form": "진짜 걱정이였어요",
          "speaker_id": "2",
          "time": "20210826 09:06"
        },
        {
          "id": "MDRW2100016059.1.12",
          "form": "온라인 할때 딴짓하고",
          "original_form": "온라인 할때 딴짓하고",
          "speaker_id": "2",
          "time": "20210826 09:06"
        },
        {
          "id": "MDRW2100016059.1.13",
          "form": "직접 가서 들으니 안심돼요",
          "original_form": "직접 가서 들으니 안심돼요",
          "speaker_id": "2",
          "time": "20210826 09:06"
        },
        {
          "id": "MDRW2100016059.1.14",
          "form": "교육이 어쩌고 말은 많지만",
          "original_form": "교육이 어쩌고 말은 많지만",
          "speaker_id": "1",
          "time": "20210826 09:07"
        },
        {
          "id": "MDRW2100016059.1.15",
          "form": "코로나 이제 어쩔수 없으니 살짝 포기 하고 열심히 방역수칙 지켜요 ㅎㅎ",
          "original_form": "코로나 이제 어쩔수 없으니 살짝 포기 하고 열심히 방역수칙 지켜요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 09:07"
        },
        {
          "id": "MDRW2100016059.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 09:07"
        },
        {
          "id": "MDRW2100016059.1.17",
          "form": "그래도 학교가 소중한 듯해요",
          "original_form": "그래도 학교가 소중한 듯해요",
          "speaker_id": "1",
          "time": "20210826 09:07"
        },
        {
          "id": "MDRW2100016059.1.18",
          "form": "그럼요 추억도 있고",
          "original_form": "그럼요 추억도 있고",
          "speaker_id": "2",
          "time": "20210826 09:07"
        },
        {
          "id": "MDRW2100016059.1.19",
          "form": "그걸 뺏을수 없죠 ㅜㅜ",
          "original_form": "그걸 뺏을수 없죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 09:07"
        },
        {
          "id": "MDRW2100016059.1.20",
          "form": "온라인은",
          "original_form": "온라인은",
          "speaker_id": "1",
          "time": "20210826 09:07"
        },
        {
          "id": "MDRW2100016059.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 09:07"
        },
        {
          "id": "MDRW2100016059.1.22",
          "form": "부모가 성질 돋는다던데",
          "original_form": "부모가 성질 돋는다던데",
          "speaker_id": "1",
          "time": "20210826 09:07"
        },
        {
          "id": "MDRW2100016059.1.23",
          "form": "근데 안챙겨주면 아예 안하고ㅋㅋ",
          "original_form": "근데 안챙겨주면 아예 안하고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:07"
        },
        {
          "id": "MDRW2100016059.1.24",
          "form": "ㅋㅋ어린애들은 그런데요",
          "original_form": "ㅋㅋ어린애들은 그런데요",
          "speaker_id": "2",
          "time": "20210826 09:08"
        },
        {
          "id": "MDRW2100016059.1.25",
          "form": "저희는 고학년이라",
          "original_form": "저희는 고학년이라",
          "speaker_id": "2",
          "time": "20210826 09:08"
        },
        {
          "id": "MDRW2100016059.1.26",
          "form": "알아서 잘들어요",
          "original_form": "알아서 잘들어요",
          "speaker_id": "2",
          "time": "20210826 09:08"
        },
        {
          "id": "MDRW2100016059.1.27",
          "form": "근데 딴짓하고 기억을 못해요 ㅡㅡ",
          "original_form": "근데 딴짓하고 기억을 못해요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210826 09:08"
        },
        {
          "id": "MDRW2100016059.1.28",
          "form": "어서 코로나 사라지고 아이들 마스크 벗고 다님좋겠어요",
          "original_form": "어서 코로나 사라지고 아이들 마스크 벗고 다님좋겠어요",
          "speaker_id": "2",
          "time": "20210826 09:10"
        },
        {
          "id": "MDRW2100016059.1.29",
          "form": "학교에 추억 있으세요?",
          "original_form": "학교에 추억 있으세요?",
          "speaker_id": "2",
          "time": "20210826 09:11"
        },
        {
          "id": "MDRW2100016059.1.30",
          "form": "그러게요 역시 학교는 사람이 기억남는거 같아요",
          "original_form": "그러게요 역시 학교는 사람이 기억남는거 같아요",
          "speaker_id": "1",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100016059.1.31",
          "form": "선생님이나 친구들",
          "original_form": "선생님이나 친구들",
          "speaker_id": "1",
          "time": "20210826 09:25"
        },
        {
          "id": "MDRW2100016059.1.32",
          "form": "학교 친구들 아직 만나세요?",
          "original_form": "학교 친구들 아직 만나세요?",
          "speaker_id": "1",
          "time": "20210826 09:25"
        }
      ]
    }
