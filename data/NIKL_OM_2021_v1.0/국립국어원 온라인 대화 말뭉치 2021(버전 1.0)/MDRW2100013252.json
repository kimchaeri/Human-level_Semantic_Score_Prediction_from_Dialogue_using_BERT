      "utterance": [
        {
          "id": "MDRW2100013252.1.1",
          "form": "요즘 뭔겜재밌어",
          "original_form": "요즘 뭔겜재밌어",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013252.1.2",
          "form": "ㅃㄹ 답장",
          "original_form": "ㅃㄹ 답장",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013252.1.3",
          "form": "원신",
          "original_form": "원신",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013252.1.4",
          "form": "그거짭겜이잖아",
          "original_form": "그거짭겜이잖아",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013252.1.5",
          "form": "우",
          "original_form": "우",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013252.1.6",
          "form": "그래도 잘만든건 맞는걸",
          "original_form": "그래도 잘만든건 맞는걸",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013252.1.7",
          "form": "그렇게 따지면 모든 로그라이크 게임은 로그짭이야",
          "original_form": "그렇게 따지면 모든 로그라이크 게임은 로그짭이야",
          "speaker_id": "1",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013252.1.8",
          "form": "아니근데",
          "original_form": "아니근데",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013252.1.9",
          "form": "아니뭐가",
          "original_form": "아니뭐가",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013252.1.10",
          "form": "이건짭겜이라고",
          "original_form": "이건짭겜이라고",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013252.1.11",
          "form": "젤다랑 비슷한 형식도 많다고?",
          "original_form": "젤다랑 비슷한 형식도 많다고?",
          "speaker_id": "1",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013252.1.12",
          "form": "아니그래서",
          "original_form": "아니그래서",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013252.1.13",
          "form": "짭이긷하지만",
          "original_form": "짭이긷하지만",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.14",
          "form": "짭겜이라는거잖악",
          "original_form": "짭겜이라는거잖악",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.15",
          "form": "원신때문에",
          "original_form": "원신때문에",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.16",
          "form": "게임업계",
          "original_form": "게임업계",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.17",
          "form": "ㄴㄴ",
          "original_form": "ㄴㄴ",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.18",
          "form": "요즘파쿠리개늘어났어",
          "original_form": "요즘파쿠리개늘어났어",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.19",
          "form": "그전부더",
          "original_form": "그전부더",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.20",
          "form": "많음",
          "original_form": "많음",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.21",
          "form": "아니더",
          "original_form": "아니더",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.22",
          "form": "아님",
          "original_form": "아님",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.23",
          "form": "늘어났다고",
          "original_form": "늘어났다고",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.24",
          "form": "맞거든",
          "original_form": "맞거든",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.25",
          "form": "늘어난건 없어",
          "original_form": "늘어난건 없어",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.26",
          "form": "닌텐도가",
          "original_form": "닌텐도가",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.27",
          "form": "그대로야",
          "original_form": "그대로야",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.28",
          "form": "고소했음좋겠다",
          "original_form": "고소했음좋겠다",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.29",
          "form": "개많거든??",
          "original_form": "개많거든??",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.30",
          "form": "고소 안한데",
          "original_form": "고소 안한데",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.31",
          "form": "님이모를뿐이지",
          "original_form": "님이모를뿐이지",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.32",
          "form": "그리고보통",
          "original_form": "그리고보통",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.33",
          "form": "뭔 모를뿐이지야",
          "original_form": "뭔 모를뿐이지야",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.34",
          "form": "이런게선례로남으ㅡ면",
          "original_form": "이런게선례로남으ㅡ면",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.35",
          "form": "전부터 많고",
          "original_form": "전부터 많고",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.36",
          "form": "업계에",
          "original_form": "업계에",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013252.1.37",
          "form": "좋은영향을",
          "original_form": "좋은영향을",
          "speaker_id": "2",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013252.1.38",
          "form": "지금도 많고만",
          "original_form": "지금도 많고만",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013252.1.39",
          "form": "안끼친다고바보야",
          "original_form": "안끼친다고바보야",
          "speaker_id": "2",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013252.1.40",
          "form": "아짱나",
          "original_form": "아짱나",
          "speaker_id": "2",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013252.1.41",
          "form": "실제로 해보면 젤다랑",
          "original_form": "실제로 해보면 젤다랑",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013252.1.42",
          "form": "느낌도 많이다르고",
          "original_form": "느낌도 많이다르고",
          "speaker_id": "1",
          "time": "20210825 13:45"
        }
      ]
    }
