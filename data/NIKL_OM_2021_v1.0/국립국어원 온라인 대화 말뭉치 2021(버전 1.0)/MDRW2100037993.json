      "utterance": [
        {
          "id": "MDRW2100037993.1.1",
          "form": "잘먹고 튼튼해야죠 갈비 장어",
          "original_form": "잘먹고 튼튼해야죠 갈비 장어",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100037993.1.2",
          "form": "오늘 머리자르고왔어요",
          "original_form": "오늘 머리자르고왔어요",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100037993.1.3",
          "form": "오늘 머리잘랐어요",
          "original_form": "오늘 머리잘랐어요",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037993.1.4",
          "form": "오오 마음에드세요",
          "original_form": "오오 마음에드세요",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037993.1.5",
          "form": "저도 자르러가야하는데ㅜㅜ",
          "original_form": "저도 자르러가야하는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037993.1.6",
          "form": "너무짧게돼서 ㅠㅠ 걱정이에요",
          "original_form": "너무짧게돼서 ㅠㅠ 걱정이에요",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037993.1.7",
          "form": "얼만큼?",
          "original_form": "얼만큼?",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037993.1.8",
          "form": "저는 음 좀 확치고싶어요",
          "original_form": "저는 음 좀 확치고싶어요",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037993.1.9",
          "form": "귀밑 삼센티?",
          "original_form": "귀밑 삼센티?",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037993.1.10",
          "form": "숯도 좀 치고",
          "original_form": "숯도 좀 치고",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037993.1.11",
          "form": "제가 그정도에요",
          "original_form": "제가 그정도에요",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037993.1.12",
          "form": "커트만 하셨어요?",
          "original_form": "커트만 하셨어요?",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037993.1.13",
          "form": "너무짧아져서 지금 후회중 ㅠ",
          "original_form": "너무짧아져서 지금 후회중 ㅠ",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037993.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037993.1.15",
          "form": "에고ㅜ 원래 기장이 어느정도셨어요?",
          "original_form": "에고ㅜ 원래 기장이 어느정도셨어요?",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037993.1.16",
          "form": "어깨에서 뒤집어지다가 이젠 안닿아요 ㅋㅋ",
          "original_form": "어깨에서 뒤집어지다가 이젠 안닿아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100037993.1.17",
          "form": "길겠죠 뭐 ㅋㅋ",
          "original_form": "길겠죠 뭐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100037993.1.18",
          "form": "저도 그정도되는거같은데",
          "original_form": "저도 그정도되는거같은데",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100037993.1.19",
          "form": "넘 더울땐 차라리 확 묶어올리는게 낫더라고요",
          "original_form": "넘 더울땐 차라리 확 묶어올리는게 낫더라고요",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100037993.1.20",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100037993.1.21",
          "form": "맞아요 한여름 단발 노노",
          "original_form": "맞아요 한여름 단발 노노",
          "speaker_id": "1",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100037993.1.22",
          "form": "그래서 계속 미뤘는데 요며칠 가을장마다보니",
          "original_form": "그래서 계속 미뤘는데 요며칠 가을장마다보니",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100037993.1.23",
          "form": "그냥잘라버릴까 싶어요 ㅋㅋ",
          "original_form": "그냥잘라버릴까 싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100037993.1.24",
          "form": "오호 도전도 응원합니다!",
          "original_form": "오호 도전도 응원합니다!",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100037993.1.25",
          "form": "요즘 어떤 패션이세용",
          "original_form": "요즘 어떤 패션이세용",
