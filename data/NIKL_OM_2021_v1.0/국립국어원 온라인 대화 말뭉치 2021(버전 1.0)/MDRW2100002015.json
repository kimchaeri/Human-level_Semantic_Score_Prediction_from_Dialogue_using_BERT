      "utterance": [
        {
          "id": "MDRW2100002015.1.1",
          "form": "이번 도쿄 올림픽 봤어?",
          "original_form": "이번 도쿄 올림픽 봤어?",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100002015.1.2",
          "form": "응 봤어!",
          "original_form": "응 봤어!",
          "speaker_id": "1",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100002015.1.3",
          "form": "무슨 종목 봤어?",
          "original_form": "무슨 종목 봤어?",
          "speaker_id": "2",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100002015.1.4",
          "form": "다는 아니고...양궁이랑 배구랑 수영?이정도?",
          "original_form": "다는 아니고...양궁이랑 배구랑 수영?이정도?",
          "speaker_id": "1",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100002015.1.5",
          "form": "너는 뭐 봤어?",
          "original_form": "너는 뭐 봤어?",
          "speaker_id": "1",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100002015.1.6",
          "form": "오 역시 양궁이랑 배구가 핫하긴 했어",
          "original_form": "오 역시 양궁이랑 배구가 핫하긴 했어",
          "speaker_id": "2",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100002015.1.7",
          "form": "난 내가 보면 질것 같아서",
          "original_form": "난 내가 보면 질것 같아서",
          "speaker_id": "2",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100002015.1.8",
          "form": "뉴스 기사나 재방송으로 봤어 거의 다 본 건 아니고 결과들은 알아",
          "original_form": "뉴스 기사나 재방송으로 봤어 거의 다 본 건 아니고 결과들은 알아",
          "speaker_id": "2",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100002015.1.9",
          "form": "아.....ㅠㅠㅠ슬프네",
          "original_form": "아.....ㅠㅠㅠ슬프네",
          "speaker_id": "1",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100002015.1.10",
          "form": "재방송으로 본 건 양궁이랑 체조",
          "original_form": "재방송으로 본 건 양궁이랑 체조",
          "speaker_id": "2",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100002015.1.11",
          "form": "인터넷에서 클라이밍이랑 요트같은 비인기 종목들 짤막한 영상들은 봤어",
          "original_form": "인터넷에서 클라이밍이랑 요트같은 비인기 종목들 짤막한 영상들은 봤어",
          "speaker_id": "2",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100002015.1.12",
          "form": "아 나두나두 그정도 봤다.",
          "original_form": "아 나두나두 그정도 봤다.",
          "speaker_id": "1",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100002015.1.13",
          "form": "운동 경기같은 거 내가 보면 진짜 질것 같아서 생방송은 못 봐",
          "original_form": "운동 경기같은 거 내가 보면 진짜 질것 같아서 생방송은 못 봐",
          "speaker_id": "2",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100002015.1.14",
          "form": "방에서 아예 신경 꺼서 뒷북으로 봐",
          "original_form": "방에서 아예 신경 꺼서 뒷북으로 봐",
          "speaker_id": "2",
          "time": "20210819 23:18"
        },
        {
          "id": "MDRW2100002015.1.15",
          "form": "아니야ㅠㅠ",
          "original_form": "아니야ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 23:18"
        },
        {
          "id": "MDRW2100002015.1.16",
          "form": "그래도 스포츠도 그 실시간으로 가슴졸이며 보는 맛이 있는데...",
          "original_form": "그래도 스포츠도 그 실시간으로 가슴졸이며 보는 맛이 있는데...",
          "speaker_id": "1",
          "time": "20210819 23:18"
        },
        {
          "id": "MDRW2100002015.1.17",
          "form": "그래도 축구랑 야구는 진짜 편하게 보게 되는거 같아.",
          "original_form": "그래도 축구랑 야구는 진짜 편하게 보게 되는거 같아.",
          "speaker_id": "1",
          "time": "20210819 23:18"
        },
        {
          "id": "MDRW2100002015.1.18",
          "form": "k리그들의 팬이 아니어서 그런가.",
          "original_form": "k리그들의 팬이 아니어서 그런가.",
          "speaker_id": "1",
          "time": "20210819 23:19"
        },
        {
          "id": "MDRW2100002015.1.19",
          "form": "아니 진짜 미신이긴한데 잘 안봐",
          "original_form": "아니 진짜 미신이긴한데 잘 안봐",
          "speaker_id": "2",
          "time": "20210819 23:19"
        },
        {
          "id": "MDRW2100002015.1.20",
          "form": "야구는 아빠가 티비 소리 크게 틀었는데",
          "original_form": "야구는 아빠가 티비 소리 크게 틀었는데",
          "speaker_id": "2",
          "time": "20210819 23:19"
        },
        {
          "id": "MDRW2100002015.1.21",
          "form": "그날 졌던가",
          "original_form": "그날 졌던가",
          "speaker_id": "2",
          "time": "20210819 23:19"
        },
        {
          "id": "MDRW2100002015.1.22",
          "form": "근데 야구는 원래...잘 못하는거 같더라고.",
          "original_form": "근데 야구는 원래...잘 못하는거 같더라고.",
          "speaker_id": "1",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002015.1.23",
          "form": "이스라엘이랑 해서 이겼을때가 진짜 대박이었잖아.",
          "original_form": "이스라엘이랑 해서 이겼을때가 진짜 대박이었잖아.",
          "speaker_id": "1",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002015.1.24",
          "form": "데드볼 밀어내기로 이기기...ㅠㅠ",
          "original_form": "데드볼 밀어내기로 이기기...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002015.1.25",
          "form": "응 맞아 잘... 못한대",
          "original_form": "응 맞아 잘... 못한대",
          "speaker_id": "2",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002015.1.26",
          "form": "역시 9회말인가",
          "original_form": "역시 9회말인가",
          "speaker_id": "2",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002015.1.27",
          "form": "근데 출전팀이 되게 적었다며",
          "original_form": "근데 출전팀이 되게 적었다며",
          "speaker_id": "2",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002015.1.28",
          "form": "아 그래서 엄청 빨리 예선이 끝났구나",
          "original_form": "아 그래서 엄청 빨리 예선이 끝났구나",
          "speaker_id": "1",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002015.1.29",
          "form": "몇개더라 6팀?",
          "original_form": "몇개더라 6팀?",
          "speaker_id": "2",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002015.1.30",
          "form": "근데 원래 올림픽 야구는 미국메이저리그 사람들은 안나온다더라고",
          "original_form": "근데 원래 올림픽 야구는 미국메이저리그 사람들은 안나온다더라고",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002015.1.31",
          "form": "되게 적었던걸로 기억나",
          "original_form": "되게 적었던걸로 기억나",
          "speaker_id": "2",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002015.1.32",
          "form": "그래서 몇팀 밖에 없는데 뭐하냐고 ㅋㅋㅋ",
          "original_form": "그래서 몇팀 밖에 없는데 뭐하냐고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002015.1.33",
          "form": "어쩐지ㅋㅋㅋㅋㅋ",
          "original_form": "어쩐지ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002015.1.34",
          "form": "어쩐지 못한거치곤 4강갔더라!",
          "original_form": "어쩐지 못한거치곤 4강갔더라!",
          "speaker_id": "1",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002015.1.35",
          "form": "우천시 동메달 될뻔 했다던데",
          "original_form": "우천시 동메달 될뻔 했다던데",
          "speaker_id": "2",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002015.1.36",
          "form": "맞아 비 많이 오면 경기 취소되서 둘다 그냥 동메달 준다며",
          "original_form": "맞아 비 많이 오면 경기 취소되서 둘다 그냥 동메달 준다며",
          "speaker_id": "1",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002015.1.37",
          "form": "야구 팬들이 다 제발 비오길 원하던데....",
          "original_form": "야구 팬들이 다 제발 비오길 원하던데....",
          "speaker_id": "1",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002015.1.38",
          "form": "그거 웃기던데 지면 바다 수영해서 오라고 ㅋㅋㅋ",
          "original_form": "그거 웃기던데 지면 바다 수영해서 오라고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002015.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋ나는 비행기 들고 걸어오라는거 봤는데ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ나는 비행기 들고 걸어오라는거 봤는데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100002015.1.40",
          "form": "맞아 사절단처럼 조상님께서 미리 개척한 길로 걸어오라는거",
          "original_form": "맞아 사절단처럼 조상님께서 미리 개척한 길로 걸어오라는거",
          "speaker_id": "2",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100002015.1.41",
          "form": "와 진짜 멋있네....",
          "original_form": "와 진짜 멋있네....",
          "speaker_id": "1",
          "time": "20210819 23:38"
        }
      ]
    }
