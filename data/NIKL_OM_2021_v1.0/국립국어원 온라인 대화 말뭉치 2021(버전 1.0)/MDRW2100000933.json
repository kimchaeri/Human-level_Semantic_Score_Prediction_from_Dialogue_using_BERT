      "utterance": [
        {
          "id": "MDRW2100000933.1.1",
          "form": "오늘도 어김없이 모델링팩 한다ㅋㅋ",
          "original_form": "오늘도 어김없이 모델링팩 한다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:56"
        },
        {
          "id": "MDRW2100000933.1.2",
          "form": "부지런해 나도 오늘은 필링젤 쓰고 팩해야겠다",
          "original_form": "부지런해 나도 오늘은 필링젤 쓰고 팩해야겠다",
          "speaker_id": "1",
          "time": "20210819 21:01"
        },
        {
          "id": "MDRW2100000933.1.3",
          "form": "그래 이제 우리 주기적인 관리가 필요한 나이다ㅋㅋ",
          "original_form": "그래 이제 우리 주기적인 관리가 필요한 나이다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000933.1.4",
          "form": "진짜 지금 피부 나아질까 싶다 갈수록 나빠지 ㅋㅋㅋ",
          "original_form": "진짜 지금 피부 나아질까 싶다 갈수록 나빠지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000933.1.5",
          "form": "근데 난 진짜 화장안하면 아픈사람인게 딱 티가 남ㅋㅋㅋ",
          "original_form": "근데 난 진짜 화장안하면 아픈사람인게 딱 티가 남ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000933.1.6",
          "form": "이번 트러블 언제 가라앉을런지ㅜㅜ",
          "original_form": "이번 트러블 언제 가라앉을런지ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000933.1.7",
          "form": "그릏게 심하면 피부과 함 가보지ㅠ",
          "original_form": "그릏게 심하면 피부과 함 가보지ㅠ",
          "speaker_id": "2",
          "time": "20210819 21:12"
        },
        {
          "id": "MDRW2100000933.1.8",
          "form": "이 시국에 어디도 가고싶지 않드아",
          "original_form": "이 시국에 어디도 가고싶지 않드아",
          "speaker_id": "1",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100000933.1.9",
          "form": "환절기라 피부 안좋아진 사람들이 쩜 있긴 하더라",
          "original_form": "환절기라 피부 안좋아진 사람들이 쩜 있긴 하더라",
          "speaker_id": "1",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100000933.1.10",
          "form": "하긴 피부과도 시내 나가야하니...",
          "original_form": "하긴 피부과도 시내 나가야하니...",
          "speaker_id": "2",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100000933.1.11",
          "form": "마스크팩이라도 자주 해봐야겠어",
          "original_form": "마스크팩이라도 자주 해봐야겠어",
          "speaker_id": "1",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100000933.1.12",
          "form": "썩어가는 바나브도 돌리고 ㅋㅋㅋ",
          "original_form": "썩어가는 바나브도 돌리고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100000933.1.13",
          "form": "바나브가 뭐여",
          "original_form": "바나브가 뭐여",
          "speaker_id": "2",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100000933.1.14",
          "form": "전에도 그렇게 물은거 같은데 ㅋㅋㅋ 갈바닉?",
          "original_form": "전에도 그렇게 물은거 같은데 ㅋㅋㅋ 갈바닉?",
          "speaker_id": "1",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100000933.1.15",
          "form": "아아 들어본듯ㅋㅋ",
          "original_form": "아아 들어본듯ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100000933.1.16",
          "form": "쳐박혀있는거 꺼내야겠다 ㅋㅋㅋ",
          "original_form": "쳐박혀있는거 꺼내야겠다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100000933.1.17",
          "form": "그런것도 집에 있다니...",
          "original_form": "그런것도 집에 있다니...",
          "speaker_id": "2",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100000933.1.18",
          "form": "부내가 난다ㅋㅋㅋ",
          "original_form": "부내가 난다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100000933.1.19",
          "form": "나도 저런거 사는 스타일이 아니었는데 뭐에 홀린듯 전에 면세서 샀지",
          "original_form": "나도 저런거 사는 스타일이 아니었는데 뭐에 홀린듯 전에 면세서 샀지",
          "speaker_id": "1",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100000933.1.20",
          "form": "난 그거 사고싶더라 리파? 얼굴 마사지하는 거",
          "original_form": "난 그거 사고싶더라 리파? 얼굴 마사지하는 거",
          "speaker_id": "2",
          "time": "20210819 21:43"
        },
        {
          "id": "MDRW2100000933.1.21",
          "form": "넌 부지런하니 그런거 사도 될듯 홈 관리기는 부지런이 기본이다",
          "original_form": "넌 부지런하니 그런거 사도 될듯 홈 관리기는 부지런이 기본이다",
          "speaker_id": "1",
          "time": "20210819 21:44"
        },
        {
          "id": "MDRW2100000933.1.22",
          "form": "응 리파는 티비보면서 그냥 생각없이 해도 될것같아서ㅋㅋ",
          "original_form": "응 리파는 티비보면서 그냥 생각없이 해도 될것같아서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:46"
        },
        {
          "id": "MDRW2100000933.1.23",
          "form": "다들 그렇게 생각하고 사지만 그마저도 못하니 ㅋㅋ",
          "original_form": "다들 그렇게 생각하고 사지만 그마저도 못하니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100000933.1.24",
          "form": "ㅋㅋㅋㅋ 그래도 모델링팩 보다는 간편하니까ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 그래도 모델링팩 보다는 간편하니까ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100000933.1.25",
          "form": "그건 확실하다 모델링팩 한달에 한번도 못해",
          "original_form": "그건 확실하다 모델링팩 한달에 한번도 못해",
          "speaker_id": "1",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100000933.1.26",
          "form": "난 이번주 하루 빼놓고 다 함ㅋㅋ",
          "original_form": "난 이번주 하루 빼놓고 다 함ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:59"
        },
        {
          "id": "MDRW2100000933.1.27",
          "form": "출장 모델링팩 부탁합니다",
          "original_form": "출장 모델링팩 부탁합니다",
          "speaker_id": "1",
          "time": "20210819 21:59"
        },
        {
          "id": "MDRW2100000933.1.28",
          "form": "거절합니다",
          "original_form": "거절합니다",
          "speaker_id": "2",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100000933.1.29",
          "form": "마스크팩보다 더 편리한 팩 간절",
          "original_form": "마스크팩보다 더 편리한 팩 간절",
          "speaker_id": "1",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100000933.1.30",
          "form": "오늘은 머드 마스크 같은것도 하고 티트리 마스크팩도 하는 중",
          "original_form": "오늘은 머드 마스크 같은것도 하고 티트리 마스크팩도 하는 중",
