        {
          "id": "MDRW2100020079.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020079.1.2",
          "form": "나는 요즘 고양이 유튜브를 많이 봐",
          "original_form": "나는 요즘 고양이 유튜브를 많이 봐",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020079.1.3",
          "form": "오 어떤 채널인데",
          "original_form": "오 어떤 채널인데",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020079.1.4",
          "form": "이름은 메탈 남이야",
          "original_form": "이름은 메탈 남이야",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020079.1.5",
          "form": "어 들어 본 적 있어",
          "original_form": "어 들어 본 적 있어",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020079.1.6",
          "form": "길고양이를 우연히 키우게 되신 분이야",
          "original_form": "길고양이를 우연히 키우게 되신 분이야",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020079.1.7",
          "form": "유명한 영상 몇 개 봤는데 감동이 더라",
          "original_form": "유명한 영상 몇 개 봤는데 감동이 더라",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020079.1.8",
          "form": "엄마 인 누리가 정말 똑똑해",
          "original_form": "엄마 인 누리가 정말 똑똑해",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020079.1.9",
          "form": "나는 하하하란 유투 버 좋아해",
          "original_form": "나는 하하하란 유투 버 좋아해",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020079.1.10",
          "form": "거기 나오는 고양이들도 귀엽니",
          "original_form": "거기 나오는 고양이들도 귀엽니",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020079.1.11",
          "form": "노란 뚱땅 이란 고양이가 제일 귀여움",
          "original_form": "노란 뚱땅 이란 고양이가 제일 귀여움",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020079.1.12",
          "form": "나도 고양이는 치즈 같은 노란 귀 노란 아이가 귀여워",
          "original_form": "나도 고양이는 치즈 같은 노란 귀 노란 아이가 귀여워",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020079.1.13",
          "form": "뚱땅 이가 대장 고양이야",
          "original_form": "뚱땅 이가 대장 고양이야",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020079.1.14",
          "form": "이름이 왜 뚱땅이야",
          "original_form": "이름이 왜 뚱땅이야",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020079.1.15",
          "form": "뚱땅 똥땅 걸어",
