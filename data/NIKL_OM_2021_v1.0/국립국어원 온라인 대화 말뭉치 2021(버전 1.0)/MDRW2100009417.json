      "utterance": [
        {
          "id": "MDRW2100009417.1.1",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "1",
          "time": "20210822 23:18"
        },
        {
          "id": "MDRW2100009417.1.2",
          "form": "반려동물 있으세요?",
          "original_form": "반려동물 있으세요?",
          "speaker_id": "1",
          "time": "20210822 23:18"
        },
        {
          "id": "MDRW2100009417.1.3",
          "form": "안녕하세요~ 저는 물고기 키우고 있어요~",
          "original_form": "안녕하세요~ 저는 물고기 키우고 있어요~",
          "speaker_id": "2",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009417.1.4",
          "form": "반려동물있으세요?",
          "original_form": "반려동물있으세요?",
          "speaker_id": "2",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009417.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009417.1.6",
          "form": "물고기!!",
          "original_form": "물고기!!",
          "speaker_id": "1",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009417.1.7",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009417.1.8",
          "form": "근데 있었으면 해요ㅜㅜ",
          "original_form": "근데 있었으면 해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009417.1.9",
          "form": "새도 좋아하고 강아지 고양이 물고기 다 좋아해요:)",
          "original_form": "새도 좋아하고 강아지 고양이 물고기 다 좋아해요:)",
          "speaker_id": "1",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009417.1.10",
          "form": "물고기 이름 있나요?",
          "original_form": "물고기 이름 있나요?",
          "speaker_id": "1",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009417.1.11",
          "form": "몇마리없었을땐 있었는디",
          "original_form": "몇마리없었을땐 있었는디",
          "speaker_id": "2",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009417.1.12",
          "form": "지금은 너무 많아져서..ㅎㅎ",
          "original_form": "지금은 너무 많아져서..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009417.1.13",
          "form": "이름은 없어요. ㅎㅎ",
          "original_form": "이름은 없어요. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009417.1.14",
          "form": "동물 좋아하시나봐요~",
          "original_form": "동물 좋아하시나봐요~",
          "speaker_id": "2",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009417.1.15",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009417.1.16",
          "form": "종을 가리지 않아요",
          "original_form": "종을 가리지 않아요",
          "speaker_id": "1",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009417.1.17",
          "form": "예전에 파충류 관련해서 알바도 한 적 있어요",
          "original_form": "예전에 파충류 관련해서 알바도 한 적 있어요",
          "speaker_id": "1",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009417.1.18",
          "form": "물고기 잘 기르시나 봐요 대단해요",
          "original_form": "물고기 잘 기르시나 봐요 대단해요",
          "speaker_id": "1",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009417.1.19",
          "form": "파충류요!! 우와~~~",
          "original_form": "파충류요!! 우와~~~",
          "speaker_id": "2",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009417.1.20",
          "form": "잘기른다기보다는 애들이 잘 살아주어서ㅎㅎ",
          "original_form": "잘기른다기보다는 애들이 잘 살아주어서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009417.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009417.1.22",
          "form": "훌륭한 양육방식이라고 생각해요",
          "original_form": "훌륭한 양육방식이라고 생각해요",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009417.1.23",
          "form": "자식은 그렇게 키우는 것",
          "original_form": "자식은 그렇게 키우는 것",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009417.1.24",
          "form": "종이 뭔가요?",
          "original_form": "종이 뭔가요?",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009417.1.25",
          "form": "구피요~",
          "original_form": "구피요~",
          "speaker_id": "2",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009417.1.26",
          "form": "이 아이들이 생명력도 좋고 번신력도 좋고 키우기도 어렵지.않더라고요~",
          "original_form": "이 아이들이 생명력도 좋고 번신력도 좋고 키우기도 어렵지.않더라고요~",
          "speaker_id": "2",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009417.1.27",
          "form": "오 구피",
          "original_form": "오 구피",
          "speaker_id": "1",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009417.1.28",
          "form": "구피 저 어릴 때 키워봤어요",
          "original_form": "구피 저 어릴 때 키워봤어요",
          "speaker_id": "1",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009417.1.29",
          "form": "엄청 예쁘더라고요",
          "original_form": "엄청 예쁘더라고요",
          "speaker_id": "1",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009417.1.30",
          "form": "구피 한 마리씩 밖에 못 키우지 않나요?",
          "original_form": "구피 한 마리씩 밖에 못 키우지 않나요?",
          "speaker_id": "1",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100009417.1.31",
          "form": "맞아요?",
          "original_form": "맞아요?",
          "speaker_id": "1",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100009417.1.32",
          "form": "제가 알기로는 여러마리 키워도 괜찮다고 해서~",
          "original_form": "제가 알기로는 여러마리 키워도 괜찮다고 해서~",
          "speaker_id": "2",
          "time": "20210823 00:15"
        },
        {
          "id": "MDRW2100009417.1.33",
          "form": "여러마리 같이 키우고 있어서",
          "original_form": "여러마리 같이 키우고 있어서",
          "speaker_id": "2",
          "time": "20210823 00:15"
        },
        {
          "id": "MDRW2100009417.1.34",
          "form": "새끼만 따로 분류해놔요~",
          "original_form": "새끼만 따로 분류해놔요~",
          "speaker_id": "2",
          "time": "20210823 00:15"
        },
        {
          "id": "MDRW2100009417.1.35",
          "form": "아하 이해했어요",
          "original_form": "아하 이해했어요",
          "speaker_id": "1",
          "time": "20210823 00:18"
        },
        {
          "id": "MDRW2100009417.1.36",
          "form": "진짜 잘 키우시네요",
          "original_form": "진짜 잘 키우시네요",
          "speaker_id": "1",
          "time": "20210823 00:18"
        },
        {
          "id": "MDRW2100009417.1.37",
          "form": "다른 키우고 싶으신 동물 또 있으세요?",
          "original_form": "다른 키우고 싶으신 동물 또 있으세요?",
          "speaker_id": "1",
          "time": "20210823 00:18"
        },
        {
          "id": "MDRW2100009417.1.38",
          "form": "저는 강아지나 고양이를 키우고 싶은데..",
          "original_form": "저는 강아지나 고양이를 키우고 싶은데..",
          "speaker_id": "2",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009417.1.39",
          "form": "제스스로 아직 자신이 없어서 열심히 고민중에만 있어요~ㅎㅎ",
          "original_form": "제스스로 아직 자신이 없어서 열심히 고민중에만 있어요~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009417.1.40",
          "form": "파충류 알바하실 때 어떠셨어요??",
          "original_form": "파충류 알바하실 때 어떠셨어요??",
          "speaker_id": "2",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009417.1.41",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "1",
          "time": "20210823 00:43"
        },
        {
          "id": "MDRW2100009417.1.42",
          "form": "학교도 있고",
          "original_form": "학교도 있고",
          "speaker_id": "1",
          "time": "20210823 00:43"
        },
        {
          "id": "MDRW2100009417.1.43",
          "form": "곧 직장도 있을 거고",
          "original_form": "곧 직장도 있을 거고",
          "speaker_id": "1",
          "time": "20210823 00:43"
        },
        {
          "id": "MDRW2100009417.1.44",
          "form": "그래서요",
          "original_form": "그래서요",
          "speaker_id": "1",
          "time": "20210823 00:44"
        },
        {
          "id": "MDRW2100009417.1.45",
          "form": "파충류 카페에서는",
          "original_form": "파충류 카페에서는",
          "speaker_id": "1",
          "time": "20210823 00:44"
        },
        {
          "id": "MDRW2100009417.1.46",
          "form": "손님들이 오면",
          "original_form": "손님들이 오면",
          "speaker_id": "1",
          "time": "20210823 00:44"
        },
        {
          "id": "MDRW2100009417.1.47",
          "form": "설명해주고 보여줬어요",
          "original_form": "설명해주고 보여줬어요",
          "speaker_id": "1",
          "time": "20210823 00:44"
        },
        {
          "id": "MDRW2100009417.1.48",
          "form": "뱀이나 도마뱀이 괜찮아서 갔는데",
          "original_form": "뱀이나 도마뱀이 괜찮아서 갔는데",
          "speaker_id": "1",
          "time": "20210823 00:44"
        },
        {
          "id": "MDRW2100009417.1.49",
          "form": "타란튤라랑 두꺼비 맡았고",
          "original_form": "타란튤라랑 두꺼비 맡았고",
          "speaker_id": "1",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009417.1.50",
          "form": "밀웜과 귀뚜라미도 키웠어쇼",
          "original_form": "밀웜과 귀뚜라미도 키웠어쇼",
          "speaker_id": "1",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009417.1.51",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009417.1.52",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009417.1.53",
          "form": "와.... 타란튤라랑 두꺼비..",
          "original_form": "와.... 타란튤라랑 두꺼비..",
          "speaker_id": "2",
          "time": "20210823 09:15"
        },
        {
          "id": "MDRW2100009417.1.54",
          "form": "대단하세요~~",
          "original_form": "대단하세요~~",
          "speaker_id": "2",
          "time": "20210823 09:15"
        },
        {
          "id": "MDRW2100009417.1.55",
          "form": "저는 절대 못 만졌을 거에요..ㅎ",
          "original_form": "저는 절대 못 만졌을 거에요..ㅎ",
          "speaker_id": "2",
