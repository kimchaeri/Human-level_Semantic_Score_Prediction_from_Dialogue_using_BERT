      "utterance": [
        {
          "id": "MDRW2100005360.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005360.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005360.1.3",
          "form": "혹시 책 자주 읽으세요?",
          "original_form": "혹시 책 자주 읽으세요?",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005360.1.4",
          "form": "자주는 안읽는데 베스트셀러정도는 알고있어요!",
          "original_form": "자주는 안읽는데 베스트셀러정도는 알고있어요!",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005360.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005360.1.6",
          "form": "저는 철학책 정말 조금읽고 만화책은 되게좋아해요",
          "original_form": "저는 철학책 정말 조금읽고 만화책은 되게좋아해요",
          "speaker_id": "1",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005360.1.7",
          "form": "만화책 좋아하세요?",
          "original_form": "만화책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005360.1.8",
          "form": "앗 네네! 요즘은 잘 안보는데",
          "original_form": "앗 네네! 요즘은 잘 안보는데",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005360.1.9",
          "form": "예전에 나루토 완결권까지 다 봤었어요!ㅋㅋ",
          "original_form": "예전에 나루토 완결권까지 다 봤었어요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005360.1.10",
          "form": "name1님은 어떤만화책 좋아하세요?",
          "original_form": "&name1&님은 어떤만화책 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005360.1.11",
          "form": "저는 드래곤볼이랑 귀멸의칼날 올해초부터 사서 보고있어요",
          "original_form": "저는 드래곤볼이랑 귀멸의칼날 올해초부터 사서 보고있어요",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005360.1.12",
          "form": "귀멸의칼날 보셨어요?",
          "original_form": "귀멸의칼날 보셨어요?",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005360.1.13",
          "form": "앗 저는 귀멸의 칼날은 본적 없어요ㅠㅠ",
          "original_form": "앗 저는 귀멸의 칼날은 본적 없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005360.1.14",
          "form": "대충 여주인공이",
          "original_form": "대충 여주인공이",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005360.1.15",
          "form": "귀엽게 생겼는ㄷㅔ",
          "original_form": "귀엽게 생겼는ㄷㅔ",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005360.1.16",
          "form": "입에 대나무 물고있는 그런것만 알아요!ㅋㅋ",
          "original_form": "입에 대나무 물고있는 그런것만 알아요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005360.1.17",
          "form": "아 안보셨구나",
          "original_form": "아 안보셨구나",
          "speaker_id": "1",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005360.1.18",
          "form": "베스트셀러는 어떤거 읽으셨어요?",
          "original_form": "베스트셀러는 어떤거 읽으셨어요?",
          "speaker_id": "1",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005360.1.19",
          "form": "저는 달러구트 꿈 백화점 지금 읽다가 말았구",
          "original_form": "저는 달러구트 꿈 백화점 지금 읽다가 말았구",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005360.1.20",
          "form": "히가시노 게이고책 위주로 좀 읽은거같아요!",
          "original_form": "히가시노 게이고책 위주로 좀 읽은거같아요!",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005360.1.21",
          "form": "name1님은 좋아하는 작가 있으세요?",
          "original_form": "&name1&님은 좋아하는 작가 있으세요?",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005360.1.22",
          "form": "저는 책을 많이 안읽어봐서 아직 좋아하는 작가는 없고",
          "original_form": "저는 책을 많이 안읽어봐서 아직 좋아하는 작가는 없고",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005360.1.23",
          "form": "그냥 주제별로 읽는 편이에요",
          "original_form": "그냥 주제별로 읽는 편이에요",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005360.1.24",
          "form": "아하ㅎㅎ",
          "original_form": "아하ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005360.1.25",
          "form": "주로 어떤 주제로 읽으세요?",
          "original_form": "주로 어떤 주제로 읽으세요?",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005360.1.26",
          "form": "전 추리소설 좋아해요",
          "original_form": "전 추리소설 좋아해요",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005360.1.27",
          "form": "철학이나 뭐 그런게 되게 좋더라구요",
          "original_form": "철학이나 뭐 그런게 되게 좋더라구요",
