      "utterance": [
        {
          "id": "MDRW2100013188.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100013188.1.2",
          "form": "네~ 좋아하는데 요새 코로나라 못가고있어요ㅠㅠ",
          "original_form": "네~ 좋아하는데 요새 코로나라 못가고있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100013188.1.3",
          "form": "ㅜㅜ 맞아요 코로나라서 어디 나가기가 뭐해요",
          "original_form": "ㅜㅜ 맞아요 코로나라서 어디 나가기가 뭐해요",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100013188.1.4",
          "form": "그동안 어디가보셨어요?",
          "original_form": "그동안 어디가보셨어요?",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100013188.1.5",
          "form": "코로나전에는 주로 서울 많이 갔어요!",
          "original_form": "코로나전에는 주로 서울 많이 갔어요!",
          "speaker_id": "2",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100013188.1.6",
          "form": "지방사람이라ㅎㅎ",
          "original_form": "지방사람이라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100013188.1.7",
          "form": "아하!! 서울가면 모해요??",
          "original_form": "아하!! 서울가면 모해요??",
          "speaker_id": "1",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100013188.1.8",
          "form": "서울가면 뮤지컬이나 전시회도 가고 연극도 보고 그랬어용",
          "original_form": "서울가면 뮤지컬이나 전시회도 가고 연극도 보고 그랬어용",
          "speaker_id": "2",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100013188.1.9",
          "form": "햐 진짜 잘 놀으셨네요!!",
          "original_form": "햐 진짜 잘 놀으셨네요!!",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013188.1.10",
          "form": "막 맛집같은 곳은 안가보셧어요???",
          "original_form": "막 맛집같은 곳은 안가보셧어요???",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013188.1.11",
          "form": "ㅋㅋㅋㅋ이영자 맛집가거나 닭한마리 진짜 자주 먹었어여ㅎㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ이영자 맛집가거나 닭한마리 진짜 자주 먹었어여ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100013188.1.12",
          "form": "name2은요??",
          "original_form": "&name2&은요??",
          "speaker_id": "2",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100013188.1.13",
          "form": "앜ㅋㅋㅋㅋ 닭한마리어때요?? 전 안가봐서리..",
          "original_form": "앜ㅋㅋㅋㅋ 닭한마리어때요?? 전 안가봐서리..",
          "speaker_id": "1",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100013188.1.14",
          "form": "전 많이 안놀러가봣어요 ㅜㅜ",
          "original_form": "전 많이 안놀러가봣어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100013188.1.15",
          "form": "닭한마리.. 물에 빠진 닭맛이긴한데 미묘한 매력이 있어요..ㅋㅋㅋ",
          "original_form": "닭한마리.. 물에 빠진 닭맛이긴한데 미묘한 매력이 있어요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100013188.1.16",
          "form": "ㅋㅋㅋㅋ 삼계탕이랑 뭐가 다름가요??",
          "original_form": "ㅋㅋㅋㅋ 삼계탕이랑 뭐가 다름가요??",
          "speaker_id": "1",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100013188.1.17",
          "form": "저도 처음 여행가보기 전에는 엄두가 안나더라구요..!",
          "original_form": "저도 처음 여행가보기 전에는 엄두가 안나더라구요..!",
          "speaker_id": "2",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100013188.1.18",
          "form": "음.. 좀더 맑은 국물에 소스찍어먹는 맛으로 먹었어요! 거기에 칼국수까지",
          "original_form": "음.. 좀더 맑은 국물에 소스찍어먹는 맛으로 먹었어요! 거기에 칼국수까지",
          "speaker_id": "2",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013188.1.19",
          "form": "전 약간 게스트하우스에 대한 로망이 있긴한데... 코로나땜시 ㅜㅜ",
          "original_form": "전 약간 게스트하우스에 대한 로망이 있긴한데... 코로나땜시 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013188.1.20",
          "form": "아 약간 닭칼국수같은 느낌인가??",
          "original_form": "아 약간 닭칼국수같은 느낌인가??",
          "speaker_id": "1",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013188.1.21",
          "form": "음 비슷해요!",
          "original_form": "음 비슷해요!",
          "speaker_id": "2",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013188.1.22",
          "form": "서울 혼자 놀러갈때는 게하 자주 이용하는데\r\n사람들이랑 월드컵 같이 보고 이러니까 재밌더라구요ㅎㅎ",
          "original_form": "서울 혼자 놀러갈때는 게하 자주 이용하는데\r\n사람들이랑 월드컵 같이 보고 이러니까 재밌더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013188.1.23",
          "form": "게하에서 친구도 사겼어요ㅎㅎ",
          "original_form": "게하에서 친구도 사겼어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013188.1.24",
          "form": "와!!!! 완전 신기해요",
          "original_form": "와!!!! 완전 신기해요",
          "speaker_id": "1",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100013188.1.25",
          "form": "게하는 분위기가 어때요??",
          "original_form": "게하는 분위기가 어때요??",
          "speaker_id": "1",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100013188.1.26",
          "form": "게하 1인실도 있고 다인실도 있고 다양한데 외국인들도 만날 수 있고",
          "original_form": "게하 1인실도 있고 다인실도 있고 다양한데 외국인들도 만날 수 있고",
          "speaker_id": "2",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013188.1.27",
          "form": "재밌었어요ㅋㅋㅋ 근데 게하마다 분위기가 다 좀 다르더라구용",
          "original_form": "재밌었어요ㅋㅋㅋ 근데 게하마다 분위기가 다 좀 다르더라구용",
          "speaker_id": "2",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013188.1.28",
          "form": "아하 게하가면 막 파티하고 그러나요?",
          "original_form": "아하 게하가면 막 파티하고 그러나요?",
          "speaker_id": "1",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013188.1.29",
          "form": "제가 갔을 땐 파티까진 아니지만 월드컵 예선전 같이 보면서 치킨 먹었어요ㅎㅎ",
          "original_form": "제가 갔을 땐 파티까진 아니지만 월드컵 예선전 같이 보면서 치킨 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013188.1.30",
          "form": "헐헐 완전 신기해요 처음엔 다들 어떻게 친해지는 거예요??",
          "original_form": "헐헐 완전 신기해요 처음엔 다들 어떻게 친해지는 거예요??",
          "speaker_id": "1",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013188.1.31",
          "form": "같은 방 쓰고 그러면 자기소개 조금하고 나는 여행땜에 왔다 이런 얘기 나누면서 친해지는 것 같아요ㅋㅋ",
          "original_form": "같은 방 쓰고 그러면 자기소개 조금하고 나는 여행땜에 왔다 이런 얘기 나누면서 친해지는 것 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013188.1.32",
          "form": "성남갔을 때는 같은 숙소 다른 분이랑 알고보니 똑같은 뮤지컬보러 온거라 친해지고 그랬어요ㅋㅋ",
          "original_form": "성남갔을 때는 같은 숙소 다른 분이랑 알고보니 똑같은 뮤지컬보러 온거라 친해지고 그랬어요ㅋㅋ",
          "speaker_id": "2",
