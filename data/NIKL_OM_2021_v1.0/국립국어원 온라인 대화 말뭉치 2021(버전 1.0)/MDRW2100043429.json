      "utterance": [
        {
          "id": "MDRW2100043429.1.1",
          "form": "일하는 건 힘들지 않수?",
          "original_form": "일하는 건 힘들지 않수?",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043429.1.2",
          "form": "실시간 수업 일단 끝나서 만족",
          "original_form": "실시간 수업 일단 끝나서 만족",
          "speaker_id": "2",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043429.1.3",
          "form": "너야말로 고생이지",
          "original_form": "너야말로 고생이지",
          "speaker_id": "2",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043429.1.4",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043429.1.5",
          "form": "나는 고생하는 직장인ㅋㅋ",
          "original_form": "나는 고생하는 직장인ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043429.1.6",
          "form": "맞소",
          "original_form": "맞소",
          "speaker_id": "2",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100043429.1.7",
          "form": "고생이 많습니다 name1님",
          "original_form": "고생이 많습니다 &name1&님",
          "speaker_id": "2",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100043429.1.8",
          "form": "낼은 몇시간이야 근무",
          "original_form": "낼은 몇시간이야 근무",
          "speaker_id": "2",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100043429.1.9",
          "form": "5시간",
          "original_form": "5시간",
          "speaker_id": "1",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100043429.1.10",
          "form": "그중 3시간은 오전 야외행사",
          "original_form": "그중 3시간은 오전 야외행사",
          "speaker_id": "1",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100043429.1.11",
          "form": "아 그럼 좀 쉽게 지나가려나",
          "original_form": "아 그럼 좀 쉽게 지나가려나",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043429.1.12",
          "form": "야외라 통솔하기 더 어렵나?",
          "original_form": "야외라 통솔하기 더 어렵나?",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043429.1.13",
          "form": "아니지 인솔하려면",
          "original_form": "아니지 인솔하려면",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043429.1.14",
          "form": "정신이 없겠지...",
          "original_form": "정신이 없겠지...",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043429.1.15",
          "form": "고생이 많아요",
          "original_form": "고생이 많아요",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043429.1.16",
          "form": "또르르",
          "original_form": "또르르",
          "speaker_id": "2",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100043429.1.17",
          "form": "그래도 월급이 나오니까",
          "original_form": "그래도 월급이 나오니까",
          "speaker_id": "1",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100043429.1.18",
          "form": "그래 월급아니면 이렇게까지 고생하지 않을텐데",
          "original_form": "그래 월급아니면 이렇게까지 고생하지 않을텐데",
          "speaker_id": "2",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100043429.1.19",
          "form": "그려",
          "original_form": "그려",
          "speaker_id": "1",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100043429.1.20",
          "form": "심상정씨가 주4일 공약 내걸었더라고",
          "original_form": "심상정씨가 주4일 공약 내걸었더라고",
          "speaker_id": "2",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100043429.1.21",
          "form": "월급과 연금만 생각한다",
          "original_form": "월급과 연금만 생각한다",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043429.1.22",
          "form": "주4 하고싶다",
          "original_form": "주4 하고싶다",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043429.1.23",
          "form": "주4일 좋지",
          "original_form": "주4일 좋지",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043429.1.24",
          "form": "개인적으로 수요일 쉬게 해줬으면",
          "original_form": "개인적으로 수요일 쉬게 해줬으면",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043429.1.25",
          "form": "맞아 수요일 쉬고",
          "original_form": "맞아 수요일 쉬고",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043429.1.26",
          "form": "그만큼 선생님을 더 고용해서",
          "original_form": "그만큼 선생님을 더 고용해서",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043429.1.27",
          "form": "다들 돌아가면서 주4하게 해줘라",
          "original_form": "다들 돌아가면서 주4하게 해줘라",
