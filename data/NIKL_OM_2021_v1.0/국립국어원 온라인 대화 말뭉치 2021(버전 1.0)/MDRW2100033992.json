      "utterance": [
        {
          "id": "MDRW2100033992.1.1",
          "form": "네네 먹어봤는데",
          "original_form": "네네 먹어봤는데",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100033992.1.2",
          "form": "맛은 잘 기억이안나네요ㅋㅋㅋㅋ",
          "original_form": "맛은 잘 기억이안나네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100033992.1.3",
          "form": "오 맛있었는지는 기억하시나요?",
          "original_form": "오 맛있었는지는 기억하시나요?",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100033992.1.4",
          "form": "왜 이건 맛있었다 메모리가 있잖아요 맛은 기억안나도",
          "original_form": "왜 이건 맛있었다 메모리가 있잖아요 맛은 기억안나도",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100033992.1.5",
          "form": "ㅋㅋㅋㅋㅋ 사실제가요",
          "original_form": "ㅋㅋㅋㅋㅋ 사실제가요",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100033992.1.6",
          "form": "음식엔 진심인편인데",
          "original_form": "음식엔 진심인편인데",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100033992.1.7",
          "form": "술은 아니랍니다",
          "original_form": "술은 아니랍니다",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100033992.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100033992.1.9",
          "form": "대답이 짐작이 가시죠?",
          "original_form": "대답이 짐작이 가시죠?",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100033992.1.10",
          "form": "그럴수도 있죠",
          "original_form": "그럴수도 있죠",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100033992.1.11",
          "form": "왜 고독한 미식가  주인공도 술못하잖아요",
          "original_form": "왜 고독한 미식가  주인공도 술못하잖아요",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100033992.1.12",
          "form": "ㅋㅋㅋ 그래요?",
          "original_form": "ㅋㅋㅋ 그래요?",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100033992.1.13",
          "form": "그사람 근데",
          "original_form": "그사람 근데",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100033992.1.14",
          "form": "술.집은 안가지않나요?",
          "original_form": "술.집은 안가지않나요?",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100033992.1.15",
          "form": "네 주인공이 술을 못해요 그래서 밥에 더 집착한대요ㅋㅋ",
          "original_form": "네 주인공이 술을 못해요 그래서 밥에 더 집착한대요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100033992.1.16",
          "form": "ㅋㅋㅋㅋ 비슷하군요",
          "original_form": "ㅋㅋㅋㅋ 비슷하군요",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100033992.1.17",
          "form": "저도술은",
          "original_form": "저도술은",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100033992.1.18",
          "form": "하이볼이 제일나은것같아요",
          "original_form": "하이볼이 제일나은것같아요",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100033992.1.19",
          "form": "아 시원깔끔하죠",
          "original_form": "아 시원깔끔하죠",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100033992.1.20",
          "form": "밀러도 가벼운 맥주던데요",
          "original_form": "밀러도 가벼운 맥주던데요",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100033992.1.21",
          "form": "전 맥주중엔",
          "original_form": "전 맥주중엔",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100033992.1.22",
          "form": "써머스비를 제일좋아한답니다ㅋㅋㅋ",
          "original_form": "써머스비를 제일좋아한답니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100033992.1.23",
          "form": "아 좋죠",
          "original_form": "아 좋죠",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100033992.1.24",
          "form": "맛있다구생각합니다",
          "original_form": "맛있다구생각합니다",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100033992.1.25",
          "form": "요새 민트초코과자 많이나오던데 드셔보셨나요",
          "original_form": "요새 민트초코과자 많이나오던데 드셔보셨나요",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100033992.1.26",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100033992.1.27",
          "form": "저는요",
          "original_form": "저는요",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100033992.1.28",
          "form": "그거예요",
          "original_form": "그거예요",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100033992.1.29",
          "form": "반민초파",
          "original_form": "반민초파",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100033992.1.30",
          "form": "아하ㅋㅋ",
          "original_form": "아하ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100033992.1.31",
          "form": "치약으로 보시는구나",
          "original_form": "치약으로 보시는구나",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100033992.1.32",
