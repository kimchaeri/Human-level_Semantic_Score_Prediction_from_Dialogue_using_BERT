      "utterance": [
        {
          "id": "MDRW2100014562.1.1",
          "form": "좋아하는 연예인 있나요",
          "original_form": "좋아하는 연예인 있나요",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014562.1.2",
          "form": "저 래퍼 래원 좋아해요 ㅋㅋㅋ",
          "original_form": "저 래퍼 래원 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014562.1.3",
          "form": "배우는요?",
          "original_form": "배우는요?",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014562.1.4",
          "form": "배우는 잘 몰라서요 ㅋㅋ",
          "original_form": "배우는 잘 몰라서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014562.1.5",
          "form": "전 최근에 드라마보고 김지석 좋아졌어요",
          "original_form": "전 최근에 드라마보고 김지석 좋아졌어요",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014562.1.6",
          "form": "오 무슨 드라마 보셧어요?",
          "original_form": "오 무슨 드라마 보셧어요?",
          "speaker_id": "2",
          "time": "20210826 00:48"
        },
        {
          "id": "MDRW2100014562.1.7",
          "form": "월간집이요!",
          "original_form": "월간집이요!",
          "speaker_id": "1",
          "time": "20210826 08:24"
        },
        {
          "id": "MDRW2100014562.1.8",
          "form": "무슨내용이에요?",
          "original_form": "무슨내용이에요?",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100014562.1.9",
          "form": "결론은 로맨슨데",
          "original_form": "결론은 로맨슨데",
          "speaker_id": "1",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100014562.1.10",
          "form": "집에관한 내용이에요 집을사고싶은사람 집에투자하는사람 ㅎㅎ",
          "original_form": "집에관한 내용이에요 집을사고싶은사람 집에투자하는사람 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100014562.1.11",
          "form": "오오 재밋겟어요!!",
          "original_form": "오오 재밋겟어요!!",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100014562.1.12",
          "form": "네네 ㅎㅎ 즐겨보시는 티비프로있나요",
          "original_form": "네네 ㅎㅎ 즐겨보시는 티비프로있나요",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100014562.1.13",
          "form": "티비 자체를 잘 안보긴 하지만 그래도 런닝맨은 봐요!",
          "original_form": "티비 자체를 잘 안보긴 하지만 그래도 런닝맨은 봐요!",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100014562.1.14",
          "form": "런닝맨 잼나요!",
          "original_form": "런닝맨 잼나요!",
