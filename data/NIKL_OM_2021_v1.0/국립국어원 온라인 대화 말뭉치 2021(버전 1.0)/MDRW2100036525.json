      "utterance": [
        {
          "id": "MDRW2100036525.1.1",
          "form": "나는 요즘 밤 늦게까지 잠이 안와",
          "original_form": "나는 요즘 밤 늦게까지 잠이 안와",
          "speaker_id": "2",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036525.1.2",
          "form": "잠을 잘 때 몸이 회복을 한다잖아.",
          "original_form": "잠을 잘 때 몸이 회복을 한다잖아.",
          "speaker_id": "1",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036525.1.3",
          "form": "사실 잠이와도 버텨",
          "original_form": "사실 잠이와도 버텨",
          "speaker_id": "2",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036525.1.4",
          "form": "너 올빼미가 되었구나",
          "original_form": "너 올빼미가 되었구나",
          "speaker_id": "1",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036525.1.5",
          "form": "너는 몇시쯤 자??",
          "original_form": "너는 몇시쯤 자??",
          "speaker_id": "2",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036525.1.6",
          "form": "뭐 재밌는 게 있니?",
          "original_form": "뭐 재밌는 게 있니?",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036525.1.7",
          "form": "나는 1시 전에 자려고 노력해",
          "original_form": "나는 1시 전에 자려고 노력해",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036525.1.8",
          "form": "아침에 일찍 일어나야하고든",
          "original_form": "아침에 일찍 일어나야하고든",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036525.1.9",
          "form": "요새 건강을 위햐 각종 노력을 한단다",
          "original_form": "요새 건강을 위햐 각종 노력을 한단다",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036525.1.10",
          "form": "근데 자도자도 잠이와",
          "original_form": "근데 자도자도 잠이와",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036525.1.11",
          "form": "건강에 이상이 생긴걸까?",
          "original_form": "건강에 이상이 생긴걸까?",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036525.1.12",
          "form": "아침에 일찍 일어나는거 너무 힘들지?",
          "original_form": "아침에 일찍 일어나는거 너무 힘들지?",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036525.1.13",
          "form": "너도 그래?",
          "original_form": "너도 그래?",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036525.1.14",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036525.1.15",
          "form": "잠은 너무 소중해",
          "original_form": "잠은 너무 소중해",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036525.1.16",
          "form": "아니 자도자도 잠이오는건",
          "original_form": "아니 자도자도 잠이오는건",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036525.1.17",
          "form": "낮잠도 좋아해 나는",
          "original_form": "낮잠도 좋아해 나는",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036525.1.18",
          "form": "나는 중학교때부터 그랬어",
          "original_form": "나는 중학교때부터 그랬어",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036525.1.19",
          "form": "낮잠 사랑해",
          "original_form": "낮잠 사랑해",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036525.1.20",
          "form": "한결같구나",
          "original_form": "한결같구나",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036525.1.21",
          "form": "낮잠은 정말 삶의 활력소야",
          "original_form": "낮잠은 정말 삶의 활력소야",
          "speaker_id": "2",
          "time": "20210831 00:07"
        }
      ]
