      "utterance": [
        {
          "id": "MDRW2100038099.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038099.1.2",
          "form": "홀맨 이벤트 아세요?",
          "original_form": "홀맨 이벤트 아세요?",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038099.1.3",
          "form": "네 알아요!",
          "original_form": "네 알아요!",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038099.1.4",
          "form": "아까 뜬건 아세요??",
          "original_form": "아까 뜬건 아세요??",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100038099.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038099.1.6",
          "form": "친구가 바로 톡을 줬어요",
          "original_form": "친구가 바로 톡을 줬어요",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038099.1.7",
          "form": "오 빠른 친구네요ㅋㅋ",
          "original_form": "오 빠른 친구네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100038099.1.8",
          "form": "그쵸 소식도 빠르고 정답도 알더라구요",
          "original_form": "그쵸 소식도 빠르고 정답도 알더라구요",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038099.1.9",
          "form": "저도 방에 있는데 저희 언니가 방문 부시는줄 알았어요",
          "original_form": "저도 방에 있는데 저희 언니가 방문 부시는줄 알았어요",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100038099.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038099.1.11",
          "form": "소식을 알려주는데 마음이 많이 급했나봐요",
          "original_form": "소식을 알려주는데 마음이 많이 급했나봐요",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038099.1.12",
          "form": "그쵸 원래 성격이 많이 급해요ㅋㅋㅋ",
          "original_form": "그쵸 원래 성격이 많이 급해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038099.1.13",
          "form": "행동은 엄청 느리면서ㅋ",
          "original_form": "행동은 엄청 느리면서ㅋ",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038099.1.14",
          "form": "ㅋㅋㅋㅋㅋ 반전이네요",
          "original_form": "ㅋㅋㅋㅋㅋ 반전이네요",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038099.1.15",
          "form": "그쵸 노답이라는 뜻이죠",
          "original_form": "그쵸 노답이라는 뜻이죠",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038099.1.16",
          "form": "어머나ㅋㅋㅋㅋ 언니가 알면 노하겠어요",
          "original_form": "어머나ㅋㅋㅋㅋ 언니가 알면 노하겠어요",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038099.1.17",
          "form": "덕분에 고생많아요",
          "original_form": "덕분에 고생많아요",
          "speaker_id": "1",
          "time": "20210901 23:27"
