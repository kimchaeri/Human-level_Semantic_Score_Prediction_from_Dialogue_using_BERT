      "utterance": [
        {
          "id": "MDRW2100029256.1.1",
          "form": "저 동호회 하는거 없는뎅...ㅋㅋㅋ",
          "original_form": "저 동호회 하는거 없는뎅...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029256.1.2",
          "form": "동호회 하신적 있으세요?!",
          "original_form": "동호회 하신적 있으세요?!",
          "speaker_id": "1",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029256.1.3",
          "form": "name1님 있으세요??",
          "original_form": "&name1&님 있으세요??",
          "speaker_id": "2",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029256.1.4",
          "form": "대학때 동아리 들어간적있어요",
          "original_form": "대학때 동아리 들어간적있어요",
          "speaker_id": "1",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029256.1.5",
          "form": "무슨 동아리 하셨어요?",
          "original_form": "무슨 동아리 하셨어요?",
          "speaker_id": "2",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029256.1.6",
          "form": "과 학생회도 하고,,",
          "original_form": "과 학생회도 하고,,",
          "speaker_id": "1",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029256.1.7",
          "form": "동아리는 선배가 밥사준다고해서 들어갔다가",
          "original_form": "동아리는 선배가 밥사준다고해서 들어갔다가",
          "speaker_id": "1",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029256.1.8",
          "form": "한번도 안나가고 탈퇴했습니다",
          "original_form": "한번도 안나가고 탈퇴했습니다",
          "speaker_id": "1",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029256.1.9",
          "form": "토론동아리였어요",
          "original_form": "토론동아리였어요",
          "speaker_id": "1",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029256.1.10",
          "form": "오우 열심히 하셨네요",
          "original_form": "오우 열심히 하셨네요",
          "speaker_id": "2",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029256.1.11",
          "form": "라고 하려 했어요",
          "original_form": "라고 하려 했어요",
          "speaker_id": "2",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029256.1.12",
          "form": "ㅋㅋㅋ 근데 학생회 힘드셨겠어요",
          "original_form": "ㅋㅋㅋ 근데 학생회 힘드셨겠어요",
          "speaker_id": "2",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029256.1.13",
          "form": "어 네 다음년도에 휴학했어요",
          "original_form": "어 네 다음년도에 휴학했어요",
          "speaker_id": "1",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029256.1.14",
          "form": "학생회 성격에 맞으면 정말 괜찮은데...",
          "original_form": "학생회 성격에 맞으면 정말 괜찮은데...",
          "speaker_id": "2",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100029256.1.15",
          "form": "몇몇은 홧병 날거 같던데요",
          "original_form": "몇몇은 홧병 날거 같던데요",
          "speaker_id": "2",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100029256.1.16",
          "form": "인류애상실,..",
          "original_form": "인류애상실,..",
          "speaker_id": "1",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100029256.1.17",
          "form": "네 이해해요",
          "original_form": "네 이해해요",
          "speaker_id": "2",
          "time": "20210903 00:48"
        },
        {
          "id": "MDRW2100029256.1.18",
          "form": "봉사활동 하셨나요?!",
          "original_form": "봉사활동 하셨나요?!",
