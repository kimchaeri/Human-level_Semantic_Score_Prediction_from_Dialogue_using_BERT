      "utterance": [
        {
          "id": "MDRW2100012017.1.1",
          "form": "안녕하세요 식사하셨나요",
          "original_form": "안녕하세요 식사하셨나요",
          "speaker_id": "1",
          "time": "20210824 18:28"
        },
        {
          "id": "MDRW2100012017.1.2",
          "form": "지금 식사중이예요",
          "original_form": "지금 식사중이예요",
          "speaker_id": "2",
          "time": "20210824 18:30"
        },
        {
          "id": "MDRW2100012017.1.3",
          "form": "뭐드시고계세요?",
          "original_form": "뭐드시고계세요?",
          "speaker_id": "1",
          "time": "20210824 18:36"
        },
        {
          "id": "MDRW2100012017.1.4",
          "form": "샌드위치 사먹었어요. 밥하기 넘 귀찮아서요. 식사 하셨나요?",
          "original_form": "샌드위치 사먹었어요. 밥하기 넘 귀찮아서요. 식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100012017.1.5",
          "form": "오 어떤 샌드위치드셨나요",
          "original_form": "오 어떤 샌드위치드셨나요",
          "speaker_id": "1",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100012017.1.6",
          "form": "전 아직안먹었어요ㅜ",
          "original_form": "전 아직안먹었어요ㅜ",
          "speaker_id": "1",
          "time": "20210824 18:51"
        },
        {
          "id": "MDRW2100012017.1.7",
          "form": "파바에서 사먹었어요. 미니 햄버거 3개 들은거 좋아하거든요 ㅎ",
          "original_form": "파바에서 사먹었어요. 미니 햄버거 3개 들은거 좋아하거든요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012017.1.8",
          "form": "아 뭔지알것같아여 맛있죠",
          "original_form": "아 뭔지알것같아여 맛있죠",
          "speaker_id": "1",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012017.1.9",
          "form": "파바 샐러드도 맛있자나요",
          "original_form": "파바 샐러드도 맛있자나요",
          "speaker_id": "1",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012017.1.10",
          "form": "가격이 별로긴한데ㅜ",
          "original_form": "가격이 별로긴한데ㅜ",
          "speaker_id": "1",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012017.1.11",
          "form": "맞아요 좀 비싼감이 있죠~ 오늘은 무료 기프티콘이 있어서 먹었습니다 ㅎ",
          "original_form": "맞아요 좀 비싼감이 있죠~ 오늘은 무료 기프티콘이 있어서 먹었습니다 ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:57"
        },
        {
          "id": "MDRW2100012017.1.12",
          "form": "저녁 뭐 드실껀가요?",
          "original_form": "저녁 뭐 드실껀가요?",
          "speaker_id": "2",
          "time": "20210824 18:57"
        },
        {
          "id": "MDRW2100012017.1.13",
          "form": "ㅎㅎ저도 파바는 어플할인할때 즐겨먹어요",
          "original_form": "ㅎㅎ저도 파바는 어플할인할때 즐겨먹어요",
          "speaker_id": "1",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100012017.1.14",
          "form": "저녁지금고민이에요 떡볶이먹을지...그냥밥먹을지",
          "original_form": "저녁지금고민이에요 떡볶이먹을지...그냥밥먹을지",
          "speaker_id": "1",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100012017.1.15",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100012017.1.16",
          "form": "저 결국 해먹기귀찮아서..피자데워먹고 포도한송이먹었어요 ㅋㅋㅋ",
          "original_form": "저 결국 해먹기귀찮아서..피자데워먹고 포도한송이먹었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:51"
        },
        {
          "id": "MDRW2100012017.1.17",
          "form": "ㅋㅋㅋㅋㅋ 요즘 뭐 해먹기 참 귀찮죠? 그래도 피자가 있었나봐요. 저도 피자 있었으면 샌드위치 사러 안나갔을거예요 ㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 요즘 뭐 해먹기 참 귀찮죠? 그래도 피자가 있었나봐요. 저도 피자 있었으면 샌드위치 사러 안나갔을거예요 ㅋ",
          "speaker_id": "2",
          "time": "20210824 21:52"
        },
        {
          "id": "MDRW2100012017.1.18",
          "form": "ㅋㅋㅋ네 왜이렇게 하기가 싫은지",
          "original_form": "ㅋㅋㅋ네 왜이렇게 하기가 싫은지",
          "speaker_id": "1",
          "time": "20210824 21:54"
        },
        {
          "id": "MDRW2100012017.1.19",
          "form": "피자는 냉동실에 항상 있어요 ㅋㅋㅋ",
          "original_form": "피자는 냉동실에 항상 있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:54"
        },
        {
          "id": "MDRW2100012017.1.20",
          "form": "전 이미 냉동실 피자는 다 털어먹은지 오래예요 ㅋㅋㅋ",
          "original_form": "전 이미 냉동실 피자는 다 털어먹은지 오래예요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012017.1.21",
          "form": "ㅋㅋㅋㅋㅋ전 동생이 피자집 직원이어서요 매번채워두고가요",
          "original_form": "ㅋㅋㅋㅋㅋ전 동생이 피자집 직원이어서요 매번채워두고가요",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012017.1.22",
          "form": "우와!! 어느 피자요??",
          "original_form": "우와!! 어느 피자요??",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012017.1.23",
          "form": "유명한곳은 아니구요 밑에지방있는 피자브랜드에요 유로코라고 ㅎㅎ",
          "original_form": "유명한곳은 아니구요 밑에지방있는 피자브랜드에요 유로코라고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 21:57"
        }
      ]
    }
