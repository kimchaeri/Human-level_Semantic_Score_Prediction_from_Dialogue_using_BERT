      "utterance": [
        {
          "id": "MDRW2100037822.1.1",
          "form": "전 빵에든 건포도는 좀 먹어요",
          "original_form": "전 빵에든 건포도는 좀 먹어요",
          "speaker_id": "2",
          "time": "20210901 18:58"
        },
        {
          "id": "MDRW2100037822.1.2",
          "form": "아~ 그러시군여 ㅋㅋㅋㅋ",
          "original_form": "아~ 그러시군여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100037822.1.3",
          "form": "막 좋아하지는 않는데",
          "original_form": "막 좋아하지는 않는데",
          "speaker_id": "2",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100037822.1.4",
          "form": "거르진 않는 정도...?",
          "original_form": "거르진 않는 정도...?",
          "speaker_id": "2",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100037822.1.5",
          "form": "저는 고구마식빵 이런건 좋은데 진짜 어릴때 엄마가 건포도 든 빵만 사와서 너무 힘들었어요 ㅋㅋㅋㅋ",
          "original_form": "저는 고구마식빵 이런건 좋은데 진짜 어릴때 엄마가 건포도 든 빵만 사와서 너무 힘들었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100037822.1.6",
          "form": "ㅋㅋㅋㅋ  그런 분들 많더라구요",
          "original_form": "ㅋㅋㅋㅋ  그런 분들 많더라구요",
          "speaker_id": "2",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100037822.1.7",
          "form": "빵은 뭐 좋아해요?",
          "original_form": "빵은 뭐 좋아해요?",
          "speaker_id": "2",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100037822.1.8",
          "form": "저는 이성당빵 좋아해요!!!",
          "original_form": "저는 이성당빵 좋아해요!!!",
          "speaker_id": "1",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100037822.1.9",
          "form": "거기 슈크림빵 제 최애",
          "original_form": "거기 슈크림빵 제 최애{emoji:♡}",
          "speaker_id": "1",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100037822.1.10",
          "form": "오호, 전 명란바게트 좋아해요.",
          "original_form": "오호, 전 명란바게트 좋아해요.",
          "speaker_id": "2",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100037822.1.11",
          "form": "그리고 카스테라!",
          "original_form": "그리고 카스테라!",
          "speaker_id": "2",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100037822.1.12",
          "form": "옹! 저는 명란바게트 맛있다는 이야기만 듣고 아직 못먿어봤어요ㅠㅠ",
          "original_form": "옹! 저는 명란바게트 맛있다는 이야기만 듣고 아직 못먿어봤어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100037822.1.13",
          "form": "명란파스타는 맛있쥬.. ㅋㅋㅋㅋㅋ",
          "original_form": "명란파스타는 맛있쥬.. ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100037822.1.14",
          "form": "그럼 그 빵도 맛있겠네요 ㅋㅋㅋ!!!",
          "original_form": "그럼 그 빵도 맛있겠네요 ㅋㅋㅋ!!!",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100037822.1.15",
          "form": "네, 근데 좀 조심해야해요",
          "original_form": "네, 근데 좀 조심해야해요",
          "speaker_id": "2",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100037822.1.16",
          "form": "딱딱해서 잘못하면 입천장같은데 까짐ㅠ",
          "original_form": "딱딱해서 잘못하면 입천장같은데 까짐ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100037822.1.17",
          "form": "아앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100037822.1.18",
          "form": "눈앞에서 그려지는 낭패...ㅋㅋㅋㅋ",
          "original_form": "눈앞에서 그려지는 낭패...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100037822.1.19",
          "form": "많이 까지셨을것 같은데요??",
          "original_form": "많이 까지셨을것 같은데요??",
          "speaker_id": "1",
          "time": "20210901 19:05"
        },
        {
          "id": "MDRW2100037822.1.20",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 19:05"
        },
        {
          "id": "MDRW2100037822.1.21",
          "form": "네....",
          "original_form": "네....",
          "speaker_id": "2",
          "time": "20210901 19:06"
        },
        {
          "id": "MDRW2100037822.1.22",
          "form": "갓 구워 나온거 운좋게 겟하면 괜찮은데",
          "original_form": "갓 구워 나온거 운좋게 겟하면 괜찮은데",
          "speaker_id": "2",
          "time": "20210901 19:06"
        },
        {
          "id": "MDRW2100037822.1.23",
          "form": "좀 지난거, 혹은 내가 킵해뒀다 먹으면",
          "original_form": "좀 지난거, 혹은 내가 킵해뒀다 먹으면",
          "speaker_id": "2",
          "time": "20210901 19:06"
        },
        {
          "id": "MDRW2100037822.1.24",
          "form": "까집니다...",
          "original_form": "까집니다...",
          "speaker_id": "2",
          "time": "20210901 19:07"
        },
        {
          "id": "MDRW2100037822.1.25",
          "form": "또르르.. 바게트종류가 좀 그래요 ㅋㅋㅋㅋㅋ",
          "original_form": "또르르.. 바게트종류가 좀 그래요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100037822.1.26",
          "form": "저는 한참 그냥 바게트 크림찍어서 먹는거 중독되서 사먹었는뎈ㅋㅋㅌ",
          "original_form": "저는 한참 그냥 바게트 크림찍어서 먹는거 중독되서 사먹었는뎈ㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100037822.1.27",
          "form": "아아.. 그때 잘못씹으면 그랬던것 같아요",
          "original_form": "아아.. 그때 잘못씹으면 그랬던것 같아요",
          "speaker_id": "1",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100037822.1.28",
          "form": "아, 그것도 맛있죠",
          "original_form": "아, 그것도 맛있죠",
          "speaker_id": "2",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100037822.1.29",
          "form": "바게트 은근 맛있는데 딱딱해요..ㅠ",
          "original_form": "바게트 은근 맛있는데 딱딱해요..ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:09"
        }
      ]
