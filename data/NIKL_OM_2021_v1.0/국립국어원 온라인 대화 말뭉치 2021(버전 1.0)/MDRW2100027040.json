        {
          "id": "MDRW2100027040.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210902 07:55"
        },
        {
          "id": "MDRW2100027040.1.2",
          "form": "동물 키우고 싶어요 마음같아선.....",
          "original_form": "동물 키우고 싶어요 마음같아선.....",
          "speaker_id": "2",
          "time": "20210902 07:56"
        },
        {
          "id": "MDRW2100027040.1.3",
          "form": "저도 여건이",
          "original_form": "저도 여건이",
          "speaker_id": "1",
          "time": "20210902 08:00"
        },
        {
          "id": "MDRW2100027040.1.4",
          "form": "그니깐요...여건이 중요한거죠",
          "original_form": "그니깐요...여건이 중요한거죠",
          "speaker_id": "2",
          "time": "20210902 08:01"
        },
        {
          "id": "MDRW2100027040.1.5",
          "form": "시간도 없잖아요",
          "original_form": "시간도 없잖아요",
          "speaker_id": "1",
          "time": "20210902 08:01"
        },
        {
          "id": "MDRW2100027040.1.6",
          "form": "양육 키워줄사람도없고.....",
          "original_form": "양육 키워줄사람도없고.....",
          "speaker_id": "2",
          "time": "20210902 08:02"
        },
        {
          "id": "MDRW2100027040.1.7",
          "form": "케어가어렵죠",
          "original_form": "케어가어렵죠",
          "speaker_id": "1",
          "time": "20210902 08:04"
        },
        {
          "id": "MDRW2100027040.1.8",
          "form": "케어가 중요해서 아직 못키우고 있어요",
          "original_form": "케어가 중요해서 아직 못키우고 있어요",
          "speaker_id": "2",
          "time": "20210902 08:05"
        },
        {
          "id": "MDRW2100027040.1.9",
          "form": "사람보다 더 어려운 들",
          "original_form": "사람보다 더 어려운 들",
          "speaker_id": "1",
          "time": "20210902 08:06"
        },
        {
          "id": "MDRW2100027040.1.10",
          "form": "사람은 말을 하지만 동물은 말을 못하니깐.....",
          "original_form": "사람은 말을 하지만 동물은 말을 못하니깐.....",
          "speaker_id": "2",
          "time": "20210902 08:07"
        },
        {
          "id": "MDRW2100027040.1.11",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 08:07"
        },
        {
          "id": "MDRW2100027040.1.12",
          "form": "맞아요 맞추려고노력많이해야죠",
          "original_form": "맞아요 맞추려고노력많이해야죠",
          "speaker_id": "1",
          "time": "20210902 08:08"
        },
        {
          "id": "MDRW2100027040.1.13",
          "form": "그래서 동물키우는걸 눈팅하고있어요",
          "original_form": "그래서 동물키우는걸 눈팅하고있어요",
