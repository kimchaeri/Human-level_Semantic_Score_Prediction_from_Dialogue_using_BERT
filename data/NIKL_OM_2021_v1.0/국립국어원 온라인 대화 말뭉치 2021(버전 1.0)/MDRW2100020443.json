      "utterance": [
        {
          "id": "MDRW2100020443.1.1",
          "form": "물론 돈 잃은 사람들이 흥분을 과하게 ㅋㅋ",
          "original_form": "물론 돈 잃은 사람들이 흥분을 과하게 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:58"
        },
        {
          "id": "MDRW2100020443.1.2",
          "form": "뽑히기 힘들다고 들었는데",
          "original_form": "뽑히기 힘들다고 들었는데",
          "speaker_id": "1",
          "time": "20210828 05:58"
        },
        {
          "id": "MDRW2100020443.1.3",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:58"
        },
        {
          "id": "MDRW2100020443.1.4",
          "form": "그러네요",
          "original_form": "그러네요",
          "speaker_id": "1",
          "time": "20210828 05:58"
        },
        {
          "id": "MDRW2100020443.1.5",
          "form": "맞아요 인기 많았어요 알바자리",
          "original_form": "맞아요 인기 많았어요 알바자리",
          "speaker_id": "2",
          "time": "20210828 05:59"
        },
        {
          "id": "MDRW2100020443.1.6",
          "form": "돈 딴 사람들이 돈 주면",
          "original_form": "돈 딴 사람들이 돈 주면",
          "speaker_id": "1",
          "time": "20210828 05:59"
        },
        {
          "id": "MDRW2100020443.1.7",
          "form": "받아도 돼요?",
          "original_form": "받아도 돼요?",
          "speaker_id": "1",
          "time": "20210828 05:59"
        },
        {
          "id": "MDRW2100020443.1.8",
          "form": "아니요ㅋㅋㅋㅋㅋ안줘요ㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋㅋ안줘요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:59"
        },
        {
          "id": "MDRW2100020443.1.9",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210828 05:59"
        },
        {
          "id": "MDRW2100020443.1.10",
          "form": "다시 새로운 도박을 해야하기 때문에",
          "original_form": "다시 새로운 도박을 해야하기 때문에",
          "speaker_id": "2",
          "time": "20210828 05:59"
        },
        {
          "id": "MDRW2100020443.1.11",
          "form": "그래도 막 줄줄 알았는데",
          "original_form": "그래도 막 줄줄 알았는데",
          "speaker_id": "1",
          "time": "20210828 05:59"
        },
        {
          "id": "MDRW2100020443.1.12",
          "form": "아 따고 끝이 아니군요",
          "original_form": "아 따고 끝이 아니군요",
          "speaker_id": "1",
          "time": "20210828 05:59"
        },
        {
          "id": "MDRW2100020443.1.13",
          "form": "다시 고스란히 재투자",
          "original_form": "다시 고스란히 재투자",
          "speaker_id": "2",
          "time": "20210828 05:59"
        },
        {
          "id": "MDRW2100020443.1.14",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:59"
        },
        {
          "id": "MDRW2100020443.1.15",
          "form": "그걸로 또 하는...",
          "original_form": "그걸로 또 하는...",
          "speaker_id": "1",
          "time": "20210828 05:59"
        },
        {
          "id": "MDRW2100020443.1.16",
          "form": "이야 대단하네요",
          "original_form": "이야 대단하네요",
          "speaker_id": "1",
          "time": "20210828 05:59"
        },
        {
          "id": "MDRW2100020443.1.17",
          "form": "손님이 주는 거는 욕밖에 없는",
          "original_form": "손님이 주는 거는 욕밖에 없는",
          "speaker_id": "2",
          "time": "20210828 05:59"
        },
        {
          "id": "MDRW2100020443.1.18",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 06:00"
        },
        {
          "id": "MDRW2100020443.1.19",
          "form": "저는 편의점 아르바이트 옷공장 아르바이트 호텔 뭐 이것저것 엄청 많이 했어용",
          "original_form": "저는 편의점 아르바이트 옷공장 아르바이트 호텔 뭐 이것저것 엄청 많이 했어용",
          "speaker_id": "1",
          "time": "20210828 06:00"
        },
        {
          "id": "MDRW2100020443.1.20",
          "form": "우와 엄청 열심히셨군요",
          "original_form": "우와 엄청 열심히셨군요",
          "speaker_id": "2",
          "time": "20210828 06:00"
        },
        {
          "id": "MDRW2100020443.1.21",
          "form": "뭐가 젤 힘들었어요",
          "original_form": "뭐가 젤 힘들었어요",
          "speaker_id": "2",
          "time": "20210828 06:00"
        },
        {
          "id": "MDRW2100020443.1.22",
          "form": "네네ㅋㅋㅋㅋ음....",
          "original_form": "네네ㅋㅋㅋㅋ음....",
          "speaker_id": "1",
          "time": "20210828 06:00"
        },
        {
          "id": "MDRW2100020443.1.23",
          "form": "호텔이요!",
          "original_form": "호텔이요!",
          "speaker_id": "1",
          "time": "20210828 06:00"
        },
        {
          "id": "MDRW2100020443.1.24",
          "form": "여기 진짜 힘들었어요ㅡㅡ",
          "original_form": "여기 진짜 힘들었어요ㅡㅡ",
          "speaker_id": "1",
          "time": "20210828 06:00"
        }
      ]
