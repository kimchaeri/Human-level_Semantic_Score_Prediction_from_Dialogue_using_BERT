      "utterance": [
        {
          "id": "MDRW2100019846.1.1",
          "form": "새로운 가족 만들기가 그래서 상상이 안가는데",
          "original_form": "새로운 가족 만들기가 그래서 상상이 안가는데",
          "speaker_id": "1",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019846.1.2",
          "form": "비혼 여성들끼리 같이 동거하면서 가족처럼 지내는 것도 많이 봤어요",
          "original_form": "비혼 여성들끼리 같이 동거하면서 가족처럼 지내는 것도 많이 봤어요",
          "speaker_id": "1",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019846.1.3",
          "form": "오오 비혼타운",
          "original_form": "오오 비혼타운",
          "speaker_id": "2",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019846.1.4",
          "form": "흔히 말하는 ㅋㅋㅋ가",
          "original_form": "흔히 말하는 {censored}ㅋㅋㅋ가",
          "speaker_id": "2",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019846.1.5",
          "form": "궁금하긴 해요 경험해보고싶고",
          "original_form": "궁금하긴 해요 경험해보고싶고",
          "speaker_id": "2",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019846.1.6",
          "form": "저도 새 가족을 제 손으로 선택한다는 게",
          "original_form": "저도 새 가족을 제 손으로 선택한다는 게",
          "speaker_id": "2",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019846.1.7",
          "form": "아직 상상이 잘 안 가는데",
          "original_form": "아직 상상이 잘 안 가는데",
          "speaker_id": "2",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019846.1.8",
          "form": "적어도 비혼 모임이면 결혼이나 남자 등으로 골치아플 일은",
          "original_form": "적어도 비혼 모임이면 결혼이나 남자 등으로 골치아플 일은",
          "speaker_id": "2",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019846.1.9",
          "form": "없지 않을까 하는 기대가 있어요",
          "original_form": "없지 않을까 하는 기대가 있어요",
          "speaker_id": "2",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019846.1.10",
          "form": "지금은 정상인이라면 결혼하고 짝이 있어야 한다는 인식이 있잖아요?",
          "original_form": "지금은 정상인이라면 결혼하고 짝이 있어야 한다는 인식이 있잖아요?",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019846.1.11",
          "form": "가부장에 반대하는 입장에서는 전혀 안 그래보이지만",
          "original_form": "가부장에 반대하는 입장에서는 전혀 안 그래보이지만",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019846.1.12",
          "form": "ㅎ..",
          "original_form": "ㅎ..",
          "speaker_id": "2",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019846.1.13",
          "form": "맞앙요 ㅋㅋㅋㅋ",
          "original_form": "맞앙요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019846.1.14",
          "form": "저도 좀 걱정을 하는 성향이라",
          "original_form": "저도 좀 걱정을 하는 성향이라",
          "speaker_id": "2",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019846.1.15",
          "form": "지금 당장 비혼동거 하고싶다! 이런 건 아니지만",
          "original_form": "지금 당장 비혼동거 하고싶다! 이런 건 아니지만",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019846.1.16",
          "form": "먼 미래라도 한 번쯤은 해보고싶어요",
          "original_form": "먼 미래라도 한 번쯤은 해보고싶어요",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019846.1.17",
          "form": "싸우는 게 두렵지만",
          "original_form": "싸우는 게 두렵지만",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019846.1.18",
          "form": "청첩장 없는 마을",
          "original_form": "청첩장 없는 마을",
          "speaker_id": "1",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019846.1.19",
          "form": "한편으로는 뭐 여자가 좀 싸우고 크는거지 뭐..싶고",
          "original_form": "한편으로는 뭐 여자가 좀 싸우고 크는거지 뭐..싶고",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019846.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아 진짜 좋네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아 진짜 좋네요",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019846.1.21",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019846.1.22",
          "form": "노청첩장마을",
          "original_form": "노청첩장마을",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019846.1.23",
          "form": "근데 여자들끼리 연애 엄청 할거같긴해서",
          "original_form": "근데 여자들끼리 연애 엄청 할거같긴해서",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019846.1.24",
          "form": "그거도 맞는 마을로 들어가야할거같아요 ㅋㅋㅋㅋ",
          "original_form": "그거도 맞는 마을로 들어가야할거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019846.1.25",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019846.1.26",
          "form": "동성 포함 비연애를",
          "original_form": "동성 포함 비연애를",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019846.1.27",
          "form": "추구하시는 분들도 있고",
          "original_form": "추구하시는 분들도 있고",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019846.1.28",
          "form": "맞아요 맞아요 이거가 갈리더라구요",
          "original_form": "맞아요 맞아요 이거가 갈리더라구요",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019846.1.29",
          "form": "동성은 포용하시는 분도 있더라구요",
          "original_form": "동성은 포용하시는 분도 있더라구요",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019846.1.30",
          "form": "이걸로도 싸우고.. 치정극 벌일수있기때문에",
          "original_form": "이걸로도 싸우고.. 치정극 벌일수있기때문에",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019846.1.31",
          "form": "이게 맞는 마을로 가고싶습니다 ㅋㅋㅋㅋ",
          "original_form": "이게 맞는 마을로 가고싶습니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019846.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
