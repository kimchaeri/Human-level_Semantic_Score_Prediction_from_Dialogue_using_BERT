      "utterance": [
        {
          "id": "MDRW2100022252.1.1",
          "form": "그래서 제로음료 4개회사가 지금",
          "original_form": "그래서 제로음료 4개회사가 지금",
          "speaker_id": "2",
          "time": "20210829 11:38"
        },
        {
          "id": "MDRW2100022252.1.2",
          "form": "할인 하느라 박터지게 싸워요",
          "original_form": "할인 하느라 박터지게 싸워요",
          "speaker_id": "2",
          "time": "20210829 11:38"
        },
        {
          "id": "MDRW2100022252.1.3",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210829 11:38"
        },
        {
          "id": "MDRW2100022252.1.4",
          "form": "본거같아요",
          "original_form": "본거같아요",
          "speaker_id": "1",
          "time": "20210829 11:38"
        },
        {
          "id": "MDRW2100022252.1.5",
          "form": "소비자 입장에선 좋은듯해요",
          "original_form": "소비자 입장에선 좋은듯해요",
          "speaker_id": "2",
          "time": "20210829 11:38"
        },
        {
          "id": "MDRW2100022252.1.6",
          "form": "코카콜라가 할인한다고",
          "original_form": "코카콜라가 할인한다고",
          "speaker_id": "1",
          "time": "20210829 11:38"
        },
        {
          "id": "MDRW2100022252.1.7",
          "form": "원래 코카콜라가 콧대 높아서 할인안하잖아요",
          "original_form": "원래 코카콜라가 콧대 높아서 할인안하잖아요",
          "speaker_id": "2",
          "time": "20210829 11:38"
        },
        {
          "id": "MDRW2100022252.1.8",
          "form": "고래싸움에 새우등 터지는거죠 뭐",
          "original_form": "고래싸움에 새우등 터지는거죠 뭐",
          "speaker_id": "1",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100022252.1.9",
          "form": "그런 코카콜라가 할인하다니 ㄷㄷ 너무 좋아요",
          "original_form": "그런 코카콜라가 할인하다니 ㄷㄷ 너무 좋아요",
          "speaker_id": "2",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100022252.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100022252.1.11",
          "form": "저 그래서 코카 제로 쟁여놨어요 ㅋㅋㅋ",
          "original_form": "저 그래서 코카 제로 쟁여놨어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100022252.1.12",
          "form": "할인진짜 안하던데",
          "original_form": "할인진짜 안하던데",
          "speaker_id": "1",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100022252.1.13",
          "form": "오 쟁여놓고 드시는구나",
          "original_form": "오 쟁여놓고 드시는구나",
          "speaker_id": "1",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100022252.1.14",
          "form": "코카제로가 더 저렴하던데요 심지어?",
          "original_form": "코카제로가 더 저렴하던데요 심지어?",
          "speaker_id": "2",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100022252.1.15",
          "form": "전 그때그때 사먹어요",
          "original_form": "전 그때그때 사먹어요",
          "speaker_id": "1",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100022252.1.16",
          "form": "네 대신 캔으로 사야돼요 ㅋㅋ 오래보관하려면",
          "original_form": "네 대신 캔으로 사야돼요 ㅋㅋ 오래보관하려면",
          "speaker_id": "2",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100022252.1.17",
          "form": "페트병 쟁여놨더니 나중에 탄산 빠진거있죠",
          "original_form": "페트병 쟁여놨더니 나중에 탄산 빠진거있죠",
          "speaker_id": "2",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100022252.1.18",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210829 11:40"
        },
        {
          "id": "MDRW2100022252.1.19",
          "form": "탄산에 탄산이 빠지다니",
          "original_form": "탄산에 탄산이 빠지다니",
          "speaker_id": "1",
