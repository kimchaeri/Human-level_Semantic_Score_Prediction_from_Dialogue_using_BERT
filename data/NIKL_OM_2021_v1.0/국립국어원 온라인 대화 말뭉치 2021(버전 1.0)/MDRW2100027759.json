      "utterance": [
        {
          "id": "MDRW2100027759.1.1",
          "form": "쇼핑 많이 하시나요?",
          "original_form": "쇼핑 많이 하시나요?",
          "speaker_id": "1",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100027759.1.2",
          "form": "많이 하는 편은 아니고",
          "original_form": "많이 하는 편은 아니고",
          "speaker_id": "2",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100027759.1.3",
          "form": "필요한거 생기면 사요",
          "original_form": "필요한거 생기면 사요",
          "speaker_id": "2",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100027759.1.4",
          "form": "저는 소비는 극도로 자제하고 있어요ㅋ",
          "original_form": "저는 소비는 극도로 자제하고 있어요ㅋ",
          "speaker_id": "1",
          "time": "20210902 14:49"
        },
        {
          "id": "MDRW2100027759.1.5",
          "form": "무슨 일 있으신가요",
          "original_form": "무슨 일 있으신가요",
          "speaker_id": "2",
          "time": "20210902 14:50"
        },
        {
          "id": "MDRW2100027759.1.6",
          "form": "그냥 부자되고파서 돈모은다고요ㅋ 금융쇼핑만 합니다",
          "original_form": "그냥 부자되고파서 돈모은다고요ㅋ 금융쇼핑만 합니다",
          "speaker_id": "1",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100027759.1.7",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210902 14:52"
        },
        {
          "id": "MDRW2100027759.1.8",
          "form": "저 금융쇼핑은 해본적이 없어요",
          "original_form": "저 금융쇼핑은 해본적이 없어요",
          "speaker_id": "2",
          "time": "20210902 14:52"
        },
        {
          "id": "MDRW2100027759.1.9",
          "form": "헛 금융도 쇼핑이다ㅋ 캐치프레이즈에요ㅋ",
          "original_form": "헛 금융도 쇼핑이다ㅋ 캐치프레이즈에요ㅋ",
          "speaker_id": "1",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100027759.1.10",
          "form": "ㅋㅋ 재밌네요",
          "original_form": "ㅋㅋ 재밌네요",
          "speaker_id": "2",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027759.1.11",
          "form": "금융 쇼핑은 어떻게 하나요",
          "original_form": "금융 쇼핑은 어떻게 하나요",
          "speaker_id": "2",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027759.1.12",
          "form": "적금 예금 펀드 주식 이런거요ㅋ",
          "original_form": "적금 예금 펀드 주식 이런거요ㅋ",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027759.1.13",
          "form": "그러면 전부 인터넷에서 하시나요?",
          "original_form": "그러면 전부 인터넷에서 하시나요?",
          "speaker_id": "2",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027759.1.14",
          "form": "요즘은 전부 비대면 온라인이죠ㅋ",
          "original_form": "요즘은 전부 비대면 온라인이죠ㅋ",
          "speaker_id": "1",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027759.1.15",
          "form": "ㅋㅋ 금융도 그렇군요",
          "original_form": "ㅋㅋ 금융도 그렇군요",
          "speaker_id": "2",
          "time": "20210902 15:31"
        },
        {
          "id": "MDRW2100027759.1.16",
          "form": "그렇죠ㅋ 그나저나 저는 빔프로젝트 사고싶어요ㅋ 중고로 알아보는중",
          "original_form": "그렇죠ㅋ 그나저나 저는 빔프로젝트 사고싶어요ㅋ 중고로 알아보는중",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027759.1.17",
          "form": "시계가..",
          "original_form": "시계가..",
