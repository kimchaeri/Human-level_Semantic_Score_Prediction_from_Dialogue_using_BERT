      "utterance": [
        {
          "id": "MDRW2100004306.1.1",
          "form": "오늘 저녁메뉴 정하셨나요?ㅎ",
          "original_form": "오늘 저녁메뉴 정하셨나요?ㅎ",
          "speaker_id": "2",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004306.1.2",
          "form": "오늘 수제 햄버거요!",
          "original_form": "오늘 수제 햄버거요!",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004306.1.3",
          "form": "감튀랑 빨리 먹고싶네요",
          "original_form": "감튀랑 빨리 먹고싶네요",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004306.1.4",
          "form": "감튀를 안팔아서...ㅠㅠㅠ",
          "original_form": "감튀를 안팔아서...ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004306.1.5",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004306.1.6",
          "form": "맥도날드 마늘버거랬는데",
          "original_form": "맥도날드 마늘버거랬는데",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004306.1.7",
          "form": "왜 나쁜말이죠",
          "original_form": "왜 나쁜말이죠",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004306.1.8",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004306.1.9",
          "form": "엥..",
          "original_form": "엥..",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004306.1.10",
          "form": "이거 나쁜말을 쓰면 나쁜말이라고 알려주나봐요??",
          "original_form": "이거 나쁜말을 쓰면 나쁜말이라고 알려주나봐요??",
          "speaker_id": "2",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004306.1.11",
          "form": "네 나쁜말은 보낼 수 없어요 다시 입력해주셍ㅅ",
          "original_form": "네 나쁜말은 보낼 수 없어요 다시 입력해주셍ㅅ",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004306.1.12",
          "form": "마늘버거 맛있겠다ㅠㅠ",
          "original_form": "마늘버거 맛있겠다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004306.1.13",
          "form": "이러네요",
          "original_form": "이러네요",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004306.1.14",
          "form": "이상하군요..",
          "original_form": "이상하군요..",
          "speaker_id": "2",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004306.1.15",
          "form": "차ㅇ녕갈릭버거",
          "original_form": "차ㅇ녕갈릭버거",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004306.1.16",
          "form": "인데",
          "original_form": "인데",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004306.1.17",
          "form": "어감이 이상하다고 생각 못했는데",
          "original_form": "어감이 이상하다고 생각 못했는데",
          "speaker_id": "1",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004306.1.18",
          "form": "나쁜말이라하니",
          "original_form": "나쁜말이라하니",
          "speaker_id": "1",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004306.1.19",
          "form": "괜히 인식하게되는",
          "original_form": "괜히 인식하게되는",
          "speaker_id": "1",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004306.1.20",
          "form": "아~ 헐 ㄷㄷ 그러게요",
          "original_form": "아~ 헐 ㄷㄷ 그러게요",
          "speaker_id": "2",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004306.1.21",
          "form": "그런말이 데이터가 좀 쌓였나봐요..",
          "original_form": "그런말이 데이터가 좀 쌓였나봐요..",
          "speaker_id": "2",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004306.1.22",
          "form": "그쳨ㅋㅋㅋㅋ",
          "original_form": "그쳨ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004306.1.23",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004306.1.24",
          "form": "아무튼 갈릭버거 맛있을것같아요",
          "original_form": "아무튼 갈릭버거 맛있을것같아요",
          "speaker_id": "1",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004306.1.25",
          "form": "마늘 러버인 한국인들에기",
          "original_form": "마늘 러버인 한국인들에기",
          "speaker_id": "1",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004306.1.26",
          "form": "한국인은 마늘이죠",
          "original_form": "한국인은 마늘이죠",
          "speaker_id": "2",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004306.1.27",
          "form": "소울푸드죠",
          "original_form": "소울푸드죠",
          "speaker_id": "1",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004306.1.28",
          "form": "앜ㅋㅋㅋㅋㅋㅋ맞죠맞죠",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ맞죠맞죠",
          "speaker_id": "2",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004306.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004306.1.30",
          "form": "저녁으로 뭐드실건가요",
          "original_form": "저녁으로 뭐드실건가요",
          "speaker_id": "1",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004306.1.31",
          "form": "음...라면으로 때울거 같아요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "음...라면으로 때울거 같아요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004306.1.32",
          "form": "점심에 호기롭게 탕수육을 만들었는데 망해서요",
          "original_form": "점심에 호기롭게 탕수육을 만들었는데 망해서요",
          "speaker_id": "2",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004306.1.33",
          "form": "뭘 할 기운이없네요 ㅎ",
          "original_form": "뭘 할 기운이없네요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004306.1.34",
          "form": "ㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004306.1.35",
          "form": "탕수육 어렵죠....",
          "original_form": "탕수육 어렵죠....",
          "speaker_id": "1",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004306.1.36",
          "form": "저도 요리는 젬병이에요",
          "original_form": "저도 요리는 젬병이에요",
          "speaker_id": "1",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004306.1.37",
          "form": "요리 어렵죠ㅠㅠ 분명히 유튜브 보고 고대로 따라했는데 말이죸ㅋㅋㅋ",
          "original_form": "요리 어렵죠ㅠㅠ 분명히 유튜브 보고 고대로 따라했는데 말이죸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004306.1.38",
          "form": "ㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004306.1.39",
          "form": "유튜브 따라하면 더 이상해지는것같아요",
          "original_form": "유튜브 따라하면 더 이상해지는것같아요",
          "speaker_id": "1",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004306.1.40",
          "form": "저도 그래서 따라하면 백종원만 따라하지",
          "original_form": "저도 그래서 따라하면 백종원만 따라하지",
          "speaker_id": "1",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004306.1.41",
          "form": "다른분들은 그냥 패스해요...ㅠㅠ",
          "original_form": "다른분들은 그냥 패스해요...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004306.1.42",
          "form": "어머니가 원테이크 아니면 요리 유튜버들 못믿겠다구",
          "original_form": "어머니가 원테이크 아니면 요리 유튜버들 못믿겠다구",
          "speaker_id": "1",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004306.1.43",
          "form": "중간에 교체할수도 있지 않냐고 하시는데 맞더라구요",
          "original_form": "중간에 교체할수도 있지 않냐고 하시는데 맞더라구요",
          "speaker_id": "1",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004306.1.44",
          "form": "헙 ㄷㄷ그러네요 완성작만 따란~",
          "original_form": "헙 ㄷㄷ그러네요 완성작만 따란~",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004306.1.45",
          "form": "하 비와서 그런가 기름진게 땡기네요..",
          "original_form": "하 비와서 그런가 기름진게 땡기네요..",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004306.1.46",
          "form": "치킨!",
          "original_form": "치킨!",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004306.1.47",
          "form": "감튀!",
          "original_form": "감튀!",
          "speaker_id": "1",
          "time": "20210821 16:24"
        }
      ]
    }
