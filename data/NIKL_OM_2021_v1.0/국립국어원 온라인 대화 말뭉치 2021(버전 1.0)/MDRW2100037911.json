      "utterance": [
        {
          "id": "MDRW2100037911.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100037911.1.2",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100037911.1.3",
          "form": "안녕하세요  네네",
          "original_form": "안녕하세요  네네",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037911.1.4",
          "form": "어떤 사이트 주로 이용하세요?",
          "original_form": "어떤 사이트 주로 이용하세요?",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037911.1.5",
          "form": "전  옷은 h패션몰",
          "original_form": "전  옷은 h패션몰",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037911.1.6",
          "form": "나머진 홈쇼핑 두루두루",
          "original_form": "나머진 홈쇼핑 두루두루",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100037911.1.7",
          "form": "전 기다리는 게 싫어",
          "original_form": "전 기다리는 게 싫어",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100037911.1.8",
          "form": "거의 쿠팡이에요~",
          "original_form": "거의 쿠팡이에요~",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100037911.1.9",
          "form": "요즘 불매운동도 많이 하고 있어서",
          "original_form": "요즘 불매운동도 많이 하고 있어서",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100037911.1.10",
          "form": "저더 쿠팡 예전엔 자주썼는데 언젠가부터 급한일이 없어서 안쓰길래  지금은 안써요",
          "original_form": "저더 쿠팡 예전엔 자주썼는데 언젠가부터 급한일이 없어서 안쓰길래  지금은 안써요",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100037911.1.11",
          "form": "전택배 기다리는 그 시간이 왜 그렇게 길게 느껴질까요;;",
          "original_form": "전택배 기다리는 그 시간이 왜 그렇게 길게 느껴질까요;;",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100037911.1.12",
          "form": "안 써야 하는데,,",
          "original_form": "안 써야 하는데,,",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100037911.1.13",
          "form": "ㅎㅎㅎ 사람마다 다르니 그건 이해해요 ~",
          "original_form": "ㅎㅎㅎ 사람마다 다르니 그건 이해해요 ~",
          "speaker_id": "2",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100037911.1.14",
          "form": "최근 구매한 물건이 어떤 거에요?",
          "original_form": "최근 구매한 물건이 어떤 거에요?",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100037911.1.15",
          "form": "전 가방이요 ㅎ",
          "original_form": "전 가방이요 ㅎ",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100037911.1.16",
          "form": "전 가방 안 사본 지가;;",
          "original_form": "전 가방 안 사본 지가;;",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100037911.1.17",
          "form": "다 먹을 거였던 거 같애요ㅋㅋ",
          "original_form": "다 먹을 거였던 거 같애요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100037911.1.18",
          "form": "저는 막가방으로 샀는데 맘에 들어요",
          "original_form": "저는 막가방으로 샀는데 맘에 들어요",
          "speaker_id": "2",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100037911.1.19",
          "form": "모든새것은  다 신납니다ㅋㅋ",
          "original_form": "모든새것은  다 신납니다ㅋㅋ",
