      "utterance": [
        {
          "id": "MDRW2100018468.1.1",
          "form": "내일 아침은 뭐 드실 거예요?",
          "original_form": "내일 아침은 뭐 드실 거예요?",
          "speaker_id": "1",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018468.1.2",
          "form": "내일은 간단하게 빵이요!",
          "original_form": "내일은 간단하게 빵이요!",
          "speaker_id": "2",
          "time": "20210827 02:26"
        },
        {
          "id": "MDRW2100018468.1.3",
          "form": "아침에 보통 뭐 드세요?",
          "original_form": "아침에 보통 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210827 02:26"
        },
        {
          "id": "MDRW2100018468.1.4",
          "form": "저는 오늘 국밥 시켰는데 내일 아침에 또 먹으려구요!",
          "original_form": "저는 오늘 국밥 시켰는데 내일 아침에 또 먹으려구요!",
          "speaker_id": "1",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018468.1.5",
          "form": "아침이 아니라 점심이 되겠지만...",
          "original_form": "아침이 아니라 점심이 되겠지만...",
          "speaker_id": "1",
          "time": "20210827 02:29"
        },
        {
          "id": "MDRW2100018468.1.6",
          "form": "ㅋㅋㅋ사실 저도 아점!",
          "original_form": "ㅋㅋㅋ사실 저도 아점!",
          "speaker_id": "2",
          "time": "20210827 02:29"
        },
        {
          "id": "MDRW2100018468.1.7",
          "form": "두 번에 나눠 드세요?",
          "original_form": "두 번에 나눠 드세요?",
          "speaker_id": "2",
          "time": "20210827 02:29"
        },
        {
          "id": "MDRW2100018468.1.8",
          "form": "하긴 국밥이 양이 넉넉하죠",
          "original_form": "하긴 국밥이 양이 넉넉하죠",
          "speaker_id": "2",
          "time": "20210827 02:29"
        },
        {
          "id": "MDRW2100018468.1.9",
          "form": "넵ㅋㅋㅋ 두개 시켰어요! 순대국밥 돼지국밥!",
          "original_form": "넵ㅋㅋㅋ 두개 시켰어요! 순대국밥 돼지국밥!",
          "speaker_id": "1",
          "time": "20210827 02:30"
        },
        {
          "id": "MDRW2100018468.1.10",
          "form": "엇 맛잘알...!",
          "original_form": "엇 맛잘알...!",
          "speaker_id": "2",
          "time": "20210827 02:32"
        },
        {
          "id": "MDRW2100018468.1.11",
          "form": "순대국밥에 따악 그 양념장 넉넉하게 먹으면 너무 맛있는데",
          "original_form": "순대국밥에 따악 그 양념장 넉넉하게 먹으면 너무 맛있는데",
          "speaker_id": "2",
          "time": "20210827 02:33"
        },
        {
          "id": "MDRW2100018468.1.12",
          "form": "든든한 점심 되시겠네요ㅎㅎ",
          "original_form": "든든한 점심 되시겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 02:33"
        },
        {
          "id": "MDRW2100018468.1.13",
          "form": "맞아요 다대기 많이 넣어야 맛있어요 ㅠㅠ",
          "original_form": "맞아요 다대기 많이 넣어야 맛있어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018468.1.14",
          "form": "밤에 음식얘기하니 힘드네요",
          "original_form": "밤에 음식얘기하니 힘드네요",
          "speaker_id": "1",
          "time": "20210827 02:42"
        },
        {
          "id": "MDRW2100018468.1.15",
          "form": "야식이 끌리는 시간...ㅠㅠ",
          "original_form": "야식이 끌리는 시간...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:42"
        },
        {
          "id": "MDRW2100018468.1.16",
          "form": "그러니까요ㅋㅋㅋㅋ",
          "original_form": "그러니까요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:45"
        },
        {
          "id": "MDRW2100018468.1.17",
          "form": "국밥 말아먹고 싶다",
          "original_form": "국밥 말아먹고 싶다",
          "speaker_id": "2",
          "time": "20210827 02:45"
        },
        {
          "id": "MDRW2100018468.1.18",
          "form": "국밥집 체인점이에요?",
          "original_form": "국밥집 체인점이에요?",
          "speaker_id": "2",
          "time": "20210827 02:45"
        },
        {
          "id": "MDRW2100018468.1.19",
          "form": "아뇨 그냥 지역 맛집이에요!",
          "original_form": "아뇨 그냥 지역 맛집이에요!",
          "speaker_id": "1",
          "time": "20210827 02:46"
        },
        {
          "id": "MDRW2100018468.1.20",
          "form": "아까 친구가 햄버거 야식으로 먹는다고 했는데",
          "original_form": "아까 친구가 햄버거 야식으로 먹는다고 했는데",
          "speaker_id": "1",
          "time": "20210827 02:46"
        },
        {
          "id": "MDRW2100018468.1.21",
          "form": "갑자기 저도 햄버거가 머리에 맴도네요...",
          "original_form": "갑자기 저도 햄버거가 머리에 맴도네요...",
          "speaker_id": "1",
          "time": "20210827 02:47"
        },
        {
          "id": "MDRW2100018468.1.22",
          "form": "저녁을 너무 일찍 먹음 ㅠㅠ",
          "original_form": "저녁을 너무 일찍 먹음 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:47"
        },
        {
          "id": "MDRW2100018468.1.23",
          "form": "저녁은 절대 7시에 먹어야 버틸 만합니다ㅋㅋㅋㅋ",
          "original_form": "저녁은 절대 7시에 먹어야 버틸 만합니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:48"
        },
        {
          "id": "MDRW2100018468.1.24",
          "form": "저도 버거킹이 당기네요...책임지세요...",
          "original_form": "저도 버거킹이 당기네요...책임지세요...",
          "speaker_id": "2",
          "time": "20210827 02:48"
        },
        {
          "id": "MDRW2100018468.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ친구도 버거킹을 먹었댔는데...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ친구도 버거킹을 먹었댔는데...",
          "speaker_id": "1",
          "time": "20210827 02:49"
        },
        {
          "id": "MDRW2100018468.1.26",
          "form": "그거 아세요?",
          "original_form": "그거 아세요?",
          "speaker_id": "1",
          "time": "20210827 02:49"
        },
        {
          "id": "MDRW2100018468.1.27",
          "form": "트리플머쉬룸버거가 단종됐대요 ㅠㅠㅠ",
          "original_form": "트리플머쉬룸버거가 단종됐대요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:49"
        },
        {
          "id": "MDRW2100018468.1.28",
          "form": "헐 머쉬룸 맛있었는데 왜죠?!",
          "original_form": "헐 머쉬룸 맛있었는데 왜죠?!",
          "speaker_id": "2",
          "time": "20210827 02:50"
        },
        {
          "id": "MDRW2100018468.1.29",
          "form": "그니까요!",
          "original_form": "그니까요!",
          "speaker_id": "1",
          "time": "20210827 02:50"
        },
        {
          "id": "MDRW2100018468.1.30",
          "form": "전 이제껏 그거 먹으려고 버거킹 갔거든요 ㅠㅠ",
          "original_form": "전 이제껏 그거 먹으려고 버거킹 갔거든요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:50"
        },
        {
          "id": "MDRW2100018468.1.31",
          "form": "버거킹 이렇게 민심을 읽지 못하다니...",
          "original_form": "버거킹 이렇게 민심을 읽지 못하다니...",
          "speaker_id": "2",
          "time": "20210827 02:50"
        },
        {
          "id": "MDRW2100018468.1.32",
          "form": "헉 그럼 진짜 아쉬우시겠다",
          "original_form": "헉 그럼 진짜 아쉬우시겠다",
          "speaker_id": "2",
          "time": "20210827 02:51"
        },
        {
          "id": "MDRW2100018468.1.33",
          "form": "엄청 아쉬워요 ㅠㅠㅠ 이제 뭘 먹어야할런지",
          "original_form": "엄청 아쉬워요 ㅠㅠㅠ 이제 뭘 먹어야할런지",
          "speaker_id": "1",
          "time": "20210827 02:51"
        }
      ]
    }
