      "utterance": [
        {
          "id": "MDRW2100010815.1.1",
          "form": "오늘 뭐 드셨어요?",
          "original_form": "오늘 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210823 22:05"
        },
        {
          "id": "MDRW2100010815.1.2",
          "form": "전 쭈꾸미요!",
          "original_form": "전 쭈꾸미요!",
          "speaker_id": "2",
          "time": "20210823 22:05"
        },
        {
          "id": "MDRW2100010815.1.3",
          "form": "와 매운거 잘 드세요?",
          "original_form": "와 매운거 잘 드세요?",
          "speaker_id": "1",
          "time": "20210823 22:05"
        },
        {
          "id": "MDRW2100010815.1.4",
          "form": "넵 좋아해요",
          "original_form": "넵 좋아해요",
          "speaker_id": "2",
          "time": "20210823 22:05"
        },
        {
          "id": "MDRW2100010815.1.5",
          "form": "맛잇는거 드셧나요?",
          "original_form": "맛잇는거 드셧나요?",
          "speaker_id": "2",
          "time": "20210823 22:06"
        },
        {
          "id": "MDRW2100010815.1.6",
          "form": "그냥 그롷죠 뭐... 요즘은 뭐가 제철일라나..",
          "original_form": "그냥 그롷죠 뭐... 요즘은 뭐가 제철일라나..",
          "speaker_id": "1",
          "time": "20210823 22:08"
        },
        {
          "id": "MDRW2100010815.1.7",
          "form": "과일은 좋아하세요?",
          "original_form": "과일은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 22:08"
        },
        {
          "id": "MDRW2100010815.1.8",
          "form": "과일 좋아하는데 직접 손질해먹기가..",
          "original_form": "과일 좋아하는데 직접 손질해먹기가..",
          "speaker_id": "2",
          "time": "20210823 22:10"
        },
        {
          "id": "MDRW2100010815.1.9",
          "form": "아 그죠그죠",
          "original_form": "아 그죠그죠",
          "speaker_id": "1",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100010815.1.10",
          "form": "복숭아는 좋아하세요?",
          "original_form": "복숭아는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100010815.1.11",
          "form": "엄마가 씻어주는 과일은 좋더라구요ㅋㅋ",
          "original_form": "엄마가 씻어주는 과일은 좋더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100010815.1.12",
          "form": "넵ㅎ 전 천도복숭아ㅇᆞ",
          "original_form": "넵ㅎ 전 천도복숭아ㅇᆞ",
          "speaker_id": "2",
          "time": "20210823 22:13"
        },
        {
          "id": "MDRW2100010815.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ 복숭아에 마스카포네 치즈넣어서 먹어보셧어요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 복숭아에 마스카포네 치즈넣어서 먹어보셧어요?",
          "speaker_id": "1",
          "time": "20210823 22:13"
        },
        {
          "id": "MDRW2100010815.1.14",
          "form": "엄청엄청 맛잇대요!!",
          "original_form": "엄청엄청 맛잇대요!!",
          "speaker_id": "1",
          "time": "20210823 22:13"
        },
        {
          "id": "MDRW2100010815.1.15",
          "form": "아 박나래먹는거봣어요ㅎ",
          "original_form": "아 박나래먹는거봣어요ㅎ",
          "speaker_id": "2",
          "time": "20210823 22:13"
        },
        {
          "id": "MDRW2100010815.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저두 먹어보고싶은디 손이 한 번 더가는 그게 귀찮네요..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저두 먹어보고싶은디 손이 한 번 더가는 그게 귀찮네요..",
          "speaker_id": "1",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010815.1.17",
          "form": "ㅋㅋ저두요",
          "original_form": "ㅋㅋ저두요",
          "speaker_id": "2",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010815.1.18",
          "form": "다른거 좋아하시는거잇으세요?",
          "original_form": "다른거 좋아하시는거잇으세요?",
          "speaker_id": "1",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100010815.1.19",
          "form": "전원래 과일은 사과요",
          "original_form": "전원래 과일은 사과요",
          "speaker_id": "2",
          "time": "20210823 22:24"
        }
      ]
