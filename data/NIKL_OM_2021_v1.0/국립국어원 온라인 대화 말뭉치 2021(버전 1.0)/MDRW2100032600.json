      "utterance": [
        {
          "id": "MDRW2100032600.1.1",
          "form": "한식분식 이렁거 중에 머조아해",
          "original_form": "한식분식 이렁거 중에 머조아해",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100032600.1.2",
          "form": "아 어려운데",
          "original_form": "아 어려운데",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100032600.1.3",
          "form": "넌 뭐좋아해",
          "original_form": "넌 뭐좋아해",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100032600.1.4",
          "form": "흠 사실 다좋아하는데ㅋㅋㅋㅋㅋㅋ",
          "original_form": "흠 사실 다좋아하는데ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100032600.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100032600.1.6",
          "form": "먹을건 한식이 더 많으니까...",
          "original_form": "먹을건 한식이 더 많으니까...",
          "speaker_id": "2",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100032600.1.7",
          "form": "근데 또 떡볶이 못버리지ㅠ",
          "original_form": "근데 또 떡볶이 못버리지ㅠ",
          "speaker_id": "2",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100032600.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ나는 굳이굳이고르면 양식 흠..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ나는 굳이굳이고르면 양식 흠..",
          "speaker_id": "1",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100032600.1.9",
          "form": "고기도좋은데 두루치기 이렁거",
          "original_form": "고기도좋은데 두루치기 이렁거",
          "speaker_id": "1",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100032600.1.10",
          "form": "ㅋㅋㅋㅋ흠 나는 아무래도 한식인듯...",
          "original_form": "ㅋㅋㅋㅋ흠 나는 아무래도 한식인듯...",
          "speaker_id": "2",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100032600.1.11",
          "form": "고기 된장찌개 이런거...넘 좋은거 같음ㅋㅋㅋ",
          "original_form": "고기 된장찌개 이런거...넘 좋은거 같음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100032600.1.12",
          "form": "ㅋㅋㅋㅋㅋ맛잇는거 너무 많다...",
          "original_form": "ㅋㅋㅋㅋㅋ맛잇는거 너무 많다...",
          "speaker_id": "1",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100032600.1.13",
          "form": "아 헐 추천떡볶이잇음",
          "original_form": "아 헐 추천떡볶이잇음",
          "speaker_id": "1",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100032600.1.14",
          "form": "뭔데머야",
          "original_form": "뭔데머야",
          "speaker_id": "2",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100032600.1.15",
          "form": "마켓컬리에 금미옥떡볶이",
          "original_form": "마켓컬리에 금미옥떡볶이",
          "speaker_id": "1",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100032600.1.16",
          "form": "양념 진짜 좀맛탱",
          "original_form": "양념 진짜 좀맛탱",
          "speaker_id": "1",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100032600.1.17",
          "form": "밀키트 같은거야??",
          "original_form": "밀키트 같은거야??",
          "speaker_id": "2",
          "time": "20210905 21:19"
        },
        {
          "id": "MDRW2100032600.1.18",
          "form": "헐 비주얼 엣날떡볶이 느낌이네 ㅋㅋㅋ",
          "original_form": "헐 비주얼 엣날떡볶이 느낌이네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:19"
        },
        {
          "id": "MDRW2100032600.1.19",
          "form": "쌀떡 밀떡 둘다잇어! 양념같은지는 몰겟다",
          "original_form": "쌀떡 밀떡 둘다잇어! 양념같은지는 몰겟다",
          "speaker_id": "1",
          "time": "20210905 21:20"
        },
        {
          "id": "MDRW2100032600.1.20",
          "form": "진짜 파는떡봌깅 양념 그대로인거야",
          "original_form": "진짜 파는떡봌깅 양념 그대로인거야",
          "speaker_id": "1",
          "time": "20210905 21:20"
        },
        {
          "id": "MDRW2100032600.1.21",
          "form": "와 매콤달콤 장난아님",
          "original_form": "와 매콤달콤 장난아님",
          "speaker_id": "1",
          "time": "20210905 21:21"
        },
        {
          "id": "MDRW2100032600.1.22",
          "form": "원래 본점 이런게 있는겨?",
          "original_form": "원래 본점 이런게 있는겨?",
          "speaker_id": "2",
          "time": "20210905 21:21"
        },
        {
          "id": "MDRW2100032600.1.23",
          "form": "음 몬가 그런느낌이야 그래서 밀키트만든느낌??",
          "original_form": "음 몬가 그런느낌이야 그래서 밀키트만든느낌??",
          "speaker_id": "1",
          "time": "20210905 21:21"
        },
        {
          "id": "MDRW2100032600.1.24",
          "form": "ㅇㅎ...마켓컬리 결국 깔아야하는 것인가",
          "original_form": "ㅇㅎ...마켓컬리 결국 깔아야하는 것인가",
          "speaker_id": "2",
          "time": "20210905 21:22"
        },
        {
          "id": "MDRW2100032600.1.25",
          "form": "아양념 꾸떡한느낌인데 오뎅 더 사서 넣으면 양념오뎅에딱임",
          "original_form": "아양념 꾸떡한느낌인데 오뎅 더 사서 넣으면 양념오뎅에딱임",
          "speaker_id": "1",
          "time": "20210905 21:22"
        },
        {
          "id": "MDRW2100032600.1.26",
          "form": "ㅋㅋㅋㅋ큐ㅠ 미춋네 라면 먹는데 배고픈 느낌 우야노 이거",
          "original_form": "ㅋㅋㅋㅋ큐ㅠ 미춋네 라면 먹는데 배고픈 느낌 우야노 이거",
          "speaker_id": "2",
          "time": "20210905 21:22"
        }
      ]
    }
  ]
