      "utterance": [
        {
          "id": "MDRW2100017388.1.1",
          "form": "요리라는거나",
          "original_form": "요리라는거나",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017388.1.2",
          "form": "먹방?",
          "original_form": "먹방?",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017388.1.3",
          "form": "아 나도 요즘 먹방 많이 봐",
          "original_form": "아 나도 요즘 먹방 많이 봐",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017388.1.4",
          "form": "대리 만족ㅠㅜ",
          "original_form": "대리 만족ㅠㅜ",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017388.1.5",
          "form": "어떤 채널 봐?",
          "original_form": "어떤 채널 봐?",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017388.1.6",
          "form": "나 상윤쓰?",
          "original_form": "나 상윤쓰?",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017388.1.7",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017388.1.8",
          "form": "나는 입짧은햇님 많이 보는 듯",
          "original_form": "나는 입짧은햇님 많이 보는 듯",
          "speaker_id": "1",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017388.1.9",
          "form": "나도 그분꺼도 봄",
          "original_form": "나도 그분꺼도 봄",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017388.1.10",
          "form": "요즘 넷플릭스나 왓챠 봐?",
          "original_form": "요즘 넷플릭스나 왓챠 봐?",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017388.1.11",
          "form": "잘 안봐..",
          "original_form": "잘 안봐..",
          "speaker_id": "2",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017388.1.12",
          "form": "나도 다 끊었어ㅋㅋㅋ",
          "original_form": "나도 다 끊었어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017388.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017388.1.14",
          "form": "요즘엔 라프텔로 애니만 좀 봤어",
          "original_form": "요즘엔 라프텔로 애니만 좀 봤어",
          "speaker_id": "1",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017388.1.15",
          "form": "애니",
          "original_form": "애니",
          "speaker_id": "2",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017388.1.16",
          "form": "전생슬 봄",
          "original_form": "전생슬 봄",
          "speaker_id": "2",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017388.1.17",
          "form": "전생슬이 뭐야?",
          "original_form": "전생슬이 뭐야?",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017388.1.18",
          "form": "아 전슬생",
          "original_form": "아 전슬생",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017388.1.19",
          "form": "이건가",
          "original_form": "이건가",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017388.1.20",
          "form": "전생했더니 슬라임이었던것에 대하여",
          "original_form": "전생했더니 슬라임이었던것에 대하여",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017388.1.21",
          "form": "재밌어?",
          "original_form": "재밌어?",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
