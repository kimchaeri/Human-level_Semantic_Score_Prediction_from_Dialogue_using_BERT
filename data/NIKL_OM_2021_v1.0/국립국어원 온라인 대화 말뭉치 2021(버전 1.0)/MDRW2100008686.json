      "utterance": [
        {
          "id": "MDRW2100008686.1.1",
          "form": "요즘 캬라멜마키야또에 빠짐",
          "original_form": "요즘 캬라멜마키야또에 빠짐",
          "speaker_id": "1",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008686.1.2",
          "form": "나는 요새 명동 카페 도장깨기중",
          "original_form": "나는 요새 명동 카페 도장깨기중",
          "speaker_id": "2",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008686.1.3",
          "form": "오 명동에 카페 많나?",
          "original_form": "오 명동에 카페 많나?",
          "speaker_id": "1",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008686.1.4",
          "form": "빽다방 스벅 배라 팔공티 개인카페2곳",
          "original_form": "빽다방 스벅 배라 팔공티 개인카페2곳",
          "speaker_id": "2",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008686.1.5",
          "form": "점심시간마다 돌아가며 먹는중ㅋㅋㅋ",
          "original_form": "점심시간마다 돌아가며 먹는중ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008686.1.6",
          "form": "와씨 팔공티 짱맛",
          "original_form": "와씨 팔공티 짱맛",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008686.1.7",
          "form": "근데 그냥 거의 아아를 많이먹지..",
          "original_form": "근데 그냥 거의 아아를 많이먹지..",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008686.1.8",
          "form": "아이스아메리카노가 무난",
          "original_form": "아이스아메리카노가 무난",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008686.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008686.1.10",
          "form": "거기 자몽블랙티 스벅이랑 비슷해",
          "original_form": "거기 자몽블랙티 스벅이랑 비슷해",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008686.1.11",
          "form": "내가 자허블 먹어봤는데",
          "original_form": "내가 자허블 먹어봤는데",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008686.1.12",
          "form": "처음에 엥 스럽가가",
          "original_form": "처음에 엥 스럽가가",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008686.1.13",
          "form": "맛있는거같응데",
          "original_form": "맛있는거같응데",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008686.1.14",
          "form": "내돈내산은 아닌..",
          "original_form": "내돈내산은 아닌..",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008686.1.15",
          "form": "하 그 쌉쌀하고 향긋한",
          "original_form": "하 그 쌉쌀하고 향긋한",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008686.1.16",
          "form": "중독이야",
          "original_form": "중독이야",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008686.1.17",
          "form": "ㅌㅋㅋㅋ그게 제일 인기많더라",
          "original_form": "ㅌㅋㅋㅋ그게 제일 인기많더라",
          "speaker_id": "2",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008686.1.18",
          "form": "서울집근처에는 팔공티없어서 춘천가면 꼭 사먹는중",
          "original_form": "서울집근처에는 팔공티없어서 춘천가면 꼭 사먹는중",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008686.1.19",
          "form": "너 캬라멜마키야또 먹어봤니",
          "original_form": "너 캬라멜마키야또 먹어봤니",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008686.1.20",
          "form": "ㅋㅋㅋ나는 집에서는 크리스탈라이트 먹어",
          "original_form": "ㅋㅋㅋ나는 집에서는 크리스탈라이트 먹어",
          "speaker_id": "2",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008686.1.21",
          "form": "맛있어 이것도ㅋㅋㅋ",
          "original_form": "맛있어 이것도ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008686.1.22",
          "form": "아아 그거 유명하잖아",
          "original_form": "아아 그거 유명하잖아",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008686.1.23",
          "form": "무슨맛 먹어?",
          "original_form": "무슨맛 먹어?",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008686.1.24",
          "form": "ㅇㅇ당 없어가지구",
          "original_form": "ㅇㅇ당 없어가지구",
          "speaker_id": "2",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008686.1.25",
          "form": "나 한 10가지만 먹어",
          "original_form": "나 한 10가지만 먹어",
          "speaker_id": "2",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008686.1.26",
          "form": "맛이 그렇게 많아?",
          "original_form": "맛이 그렇게 많아?",
          "speaker_id": "1",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008686.1.27",
          "form": "맛종류 다양해서 포도 딸기 망고 레몬 핑크레몬 체리",
          "original_form": "맛종류 다양해서 포도 딸기 망고 레몬 핑크레몬 체리",
          "speaker_id": "2",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008686.1.28",
          "form": "웅웅 엄청많음ㅋㅋ",
          "original_form": "웅웅 엄청많음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008686.1.29",
          "form": "와 엄청많네",
          "original_form": "와 엄청많네",
          "speaker_id": "1",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008686.1.30",
          "form": "근데 맛있어",
          "original_form": "근데 맛있어",
          "speaker_id": "2",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008686.1.31",
          "form": "다이어트하는 사람들 쟁여두더라",
          "original_form": "다이어트하는 사람들 쟁여두더라",
          "speaker_id": "1",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008686.1.32",
          "form": "나도 당분 없는거라 잘 먹는중ㅋㅋ",
          "original_form": "나도 당분 없는거라 잘 먹는중ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008686.1.33",
          "form": "그거 한번 먹으면 중독될것같아서 참는중",
          "original_form": "그거 한번 먹으면 중독될것같아서 참는중",
          "speaker_id": "1",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008686.1.34",
          "form": "당 없어도 건강에 좋은건 역시 맹물이죠",
          "original_form": "당 없어도 건강에 좋은건 역시 맹물이죠",
          "speaker_id": "1",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008686.1.35",
          "form": "ㅋㅋㅋㅋ그래도 당분들어간 음료 대체로 괜춘",
          "original_form": "ㅋㅋㅋㅋ그래도 당분들어간 음료 대체로 괜춘",
          "speaker_id": "2",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008686.1.36",
          "form": "인정 난 보리차좋아",
          "original_form": "인정 난 보리차좋아",
