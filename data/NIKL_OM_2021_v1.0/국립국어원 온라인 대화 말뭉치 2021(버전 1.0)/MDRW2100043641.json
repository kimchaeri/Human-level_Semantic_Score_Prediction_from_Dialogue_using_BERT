      "utterance": [
        {
          "id": "MDRW2100043641.1.1",
          "form": "그러게 재택 근무가 좋은데",
          "original_form": "그러게 재택 근무가 좋은데",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043641.1.2",
          "form": "웅 ㅜㅜ이제 하던 프로젝트 끝나서 출근해야하는듯",
          "original_form": "웅 ㅜㅜ이제 하던 프로젝트 끝나서 출근해야하는듯",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043641.1.3",
          "form": "집에서 하면 안 되나",
          "original_form": "집에서 하면 안 되나",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043641.1.4",
          "form": "회사 컴터가 필요하다",
          "original_form": "회사 컴터가 필요하다",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043641.1.5",
          "form": "그러면 어쩔 수 없네",
          "original_form": "그러면 어쩔 수 없네",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043641.1.6",
          "form": "웅웅 ㅠ출근해야함 ㅋ",
          "original_form": "웅웅 ㅠ출근해야함 ㅋ",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043641.1.7",
          "form": "출근 퇴근 힘들겠다",
          "original_form": "출근 퇴근 힘들겠다",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043641.1.8",
          "form": "웅웅..오고가는시간 넘 아까워",
          "original_form": "웅웅..오고가는시간 넘 아까워",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043641.1.9",
          "form": "앞으로 계속 집에서 일 하면 너무 좋을 텐데",
          "original_form": "앞으로 계속 집에서 일 하면 너무 좋을 텐데",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043641.1.10",
          "form": "웅..그냥 때려치우고 프리랜서를 하던지 ㅋ",
          "original_form": "웅..그냥 때려치우고 프리랜서를 하던지 ㅋ",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043641.1.11",
          "form": "잘 알아보고 해야지",
          "original_form": "잘 알아보고 해야지",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043641.1.12",
          "form": "웅웅..ㅋㅋ일하면서 알아봐야짘",
          "original_form": "웅웅..ㅋㅋ일하면서 알아봐야짘",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043641.1.13",
          "form": "재택근무 해 달라고 말해 봐라",
          "original_form": "재택근무 해 달라고 말해 봐라",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043641.1.14",
          "form": "여기 회사 그만두면 또 이직준비해야할까",
