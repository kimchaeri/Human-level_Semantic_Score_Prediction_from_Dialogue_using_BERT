      "utterance": [
        {
          "id": "MDRW2100043352.1.1",
          "form": "엘에이치는",
          "original_form": "엘에이치는",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043352.1.2",
          "form": "엘에이치도 안됨",
          "original_form": "엘에이치도 안됨",
          "speaker_id": "1",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043352.1.3",
          "form": "진짜 전체 조사를 다 해봐야 됨",
          "original_form": "진짜 전체 조사를 다 해봐야 됨",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043352.1.4",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043352.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043352.1.6",
          "form": "그렇게 세대수가 많은데",
          "original_form": "그렇게 세대수가 많은데",
          "speaker_id": "2",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043352.1.7",
          "form": "관리비를 다 그만큼 떼면",
          "original_form": "관리비를 다 그만큼 떼면",
          "speaker_id": "2",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043352.1.8",
          "form": "그것도 나갈 때도 돈 다 받아먹잖아요",
          "original_form": "그것도 나갈 때도 돈 다 받아먹잖아요",
          "speaker_id": "1",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043352.1.9",
          "form": "얼마나 많이 가져가겠다는 거야",
          "original_form": "얼마나 많이 가져가겠다는 거야",
          "speaker_id": "2",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043352.1.10",
          "form": "원상 복구하라고",
          "original_form": "원상 복구하라고",
          "speaker_id": "1",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043352.1.11",
          "form": "나갈 때도 돈 내나??",
          "original_form": "나갈 때도 돈 내나??",
          "speaker_id": "2",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043352.1.12",
          "form": "아 진짜??",
          "original_form": "아 진짜??",
          "speaker_id": "2",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043352.1.13",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043352.1.14",
          "form": "엘에이치는",
          "original_form": "엘에이치는",
          "speaker_id": "2",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043352.1.15",
          "form": "진짜 문제가 많아 보이는",
          "original_form": "진짜 문제가 많아 보이는",
          "speaker_id": "2",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043352.1.16",
          "form": "맞아요 언니 자기들끼리 다 해먹어요",
          "original_form": "맞아요 언니 자기들끼리 다 해먹어요",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100043352.1.17",
          "form": "뭐 쪼끔 고장나고 뭐 이런 것도 다 봤는데요",
          "original_form": "뭐 쪼끔 고장나고 뭐 이런 것도 다 봤는데요",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100043352.1.18",
          "form": "하다 보면 은 하자 조금씩은 다 있는데",
          "original_form": "하다 보면 은 하자 조금씩은 다 있는데",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100043352.1.19",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100043352.1.20",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "2",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100043352.1.21",
          "form": "나라서 하는 거는",
          "original_form": "나라서 하는 거는",
          "speaker_id": "1",
          "time": "20210907 18:09"
        },
        {
          "id": "MDRW2100043352.1.22",
          "form": "다 별나요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "다 별나요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:09"
        },
        {
          "id": "MDRW2100043352.1.23",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210907 18:09"
        },
        {
          "id": "MDRW2100043352.1.24",
