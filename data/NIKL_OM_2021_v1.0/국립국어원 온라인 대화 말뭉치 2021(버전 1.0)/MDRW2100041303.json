      "utterance": [
        {
          "id": "MDRW2100041303.1.1",
          "form": "간장이요? 아님 빨갛게요?",
          "original_form": "간장이요? 아님 빨갛게요?",
          "speaker_id": "2",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100041303.1.2",
          "form": "전 빨간게 좋더라구요ㅋㅋㅋ",
          "original_form": "전 빨간게 좋더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100041303.1.3",
          "form": "저두 빨간거요!ㅋㅋㅋㅋ",
          "original_form": "저두 빨간거요!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100041303.1.4",
          "form": "비법이 있나요?",
          "original_form": "비법이 있나요?",
          "speaker_id": "2",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100041303.1.5",
          "form": "제가 하면 약간 텁텁한 맛이 나요",
          "original_form": "제가 하면 약간 텁텁한 맛이 나요",
          "speaker_id": "2",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100041303.1.6",
          "form": "글쎄요? 요리 잘 하는 편은 아니라서ㅎ",
          "original_form": "글쎄요? 요리 잘 하는 편은 아니라서ㅎ",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041303.1.7",
          "form": "양파 좋아해서 많이 넣었더니 좀 달달하게 되긴 해요",
          "original_form": "양파 좋아해서 많이 넣었더니 좀 달달하게 되긴 해요",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041303.1.8",
          "form": "오 양파!",
          "original_form": "오 양파!",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100041303.1.9",
          "form": "양파 넣을 생각을 왜 못했죠",
          "original_form": "양파 넣을 생각을 왜 못했죠",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100041303.1.10",
          "form": "국물 자작하게 해서 밥에 쓱쓱 비벼먹어도 맛있어요",
          "original_form": "국물 자작하게 해서 밥에 쓱쓱 비벼먹어도 맛있어요",
          "speaker_id": "2",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100041303.1.11",
          "form": "맞아요ㅋㅋㅋ 두부 부셔서 밥이랑 같이 비비는거 너무 좋아요",
          "original_form": "맞아요ㅋㅋㅋ 두부 부셔서 밥이랑 같이 비비는거 너무 좋아요",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100041303.1.12",
          "form": "맞아요 특별히 밥도둑이 있는게 아니라 두부조림이 밥도둑이죠ㅋㅋ",
          "original_form": "맞아요 특별히 밥도둑이 있는게 아니라 두부조림이 밥도둑이죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100041303.1.13",
          "form": "아! 근데 양념이 잘 안배요",
          "original_form": "아! 근데 양념이 잘 안배요",
          "speaker_id": "2",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100041303.1.14",
          "form": "아주아주아주 오래 긇여야할까요?",
          "original_form": "아주아주아주 오래 긇여야할까요?",
          "speaker_id": "2",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100041303.1.15",
          "form": "끓",
          "original_form": "끓",
          "speaker_id": "2",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100041303.1.16",
          "form": "물기가 남아있어서 그런걸까요?",
          "original_form": "물기가 남아있어서 그런걸까요?",
          "speaker_id": "1",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100041303.1.17",
          "form": "좀 자박하면 오래 끓여도 좋을거 같아요",
          "original_form": "좀 자박하면 오래 끓여도 좋을거 같아요",
          "speaker_id": "1",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100041303.1.18",
          "form": "흠 도전해봐야겠어요!",
          "original_form": "흠 도전해봐야겠어요!",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041303.1.19",
          "form": "요리 너무 어려운거 같아요",
          "original_form": "요리 너무 어려운거 같아요",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041303.1.20",
          "form": "라면이 제일 쉬운ㅋㅋㅋㅋ",
          "original_form": "라면이 제일 쉬운ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041303.1.21",
          "form": "그니까요! 요리 잘하는 사람들은 천재에요",
          "original_form": "그니까요! 요리 잘하는 사람들은 천재에요",
          "speaker_id": "2",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100041303.1.22",
          "form": "전 라면 물도 잘 못맞춰서 한강이거나 쫄면이거나 이래요ㅠㅋㅋㅋㅋ",
          "original_form": "전 라면 물도 잘 못맞춰서 한강이거나 쫄면이거나 이래요ㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100041303.1.23",
          "form": "맞아요ㅠㅠ 요리초보에게 백종원 선생님 너무 감사하고요ㅋㅋㅋ",
          "original_form": "맞아요ㅠㅠ 요리초보에게 백종원 선생님 너무 감사하고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100041303.1.24",
          "form": "아 진짜 백쌤..ㅠㅠㅠ 제 은인 ㅠㅠㅠ ㅋㅋㅋㅋㅋ 뭐 요리 흉내라도 내게 해주셔서 감사할따름이죠",
          "original_form": "아 진짜 백쌤..ㅠㅠㅠ 제 은인 ㅠㅠㅠ ㅋㅋㅋㅋㅋ 뭐 요리 흉내라도 내게 해주셔서 감사할따름이죠",
          "speaker_id": "2",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100041303.1.25",
          "form": "그러면 여행하시면서 젤 맛있었던거 얘기해주세요ㅋㅋㅋㅋ",
          "original_form": "그러면 여행하시면서 젤 맛있었던거 얘기해주세요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:50"
        }
      ]
    }
