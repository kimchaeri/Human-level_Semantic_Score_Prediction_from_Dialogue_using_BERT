      "utterance": [
        {
          "id": "MDRW2100045372.1.1",
          "form": "여튼 첫기억이",
          "original_form": "여튼 첫기억이",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045372.1.2",
          "form": "너무 맛없게 박혀서",
          "original_form": "너무 맛없게 박혀서",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045372.1.3",
          "form": "지금도 더 잘 안들어가더라",
          "original_form": "지금도 더 잘 안들어가더라",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045372.1.4",
          "form": "그때는 진짜",
          "original_form": "그때는 진짜",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045372.1.5",
          "form": "탄산이면 다 맛있는건줄 알았어",
          "original_form": "탄산이면 다 맛있는건줄 알았어",
          "speaker_id": "2",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100045372.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 어릴때 추억이 너무 강렬했구나...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 어릴때 추억이 너무 강렬했구나...",
          "speaker_id": "1",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100045372.1.7",
          "form": "글치....",
          "original_form": "글치....",
          "speaker_id": "2",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100045372.1.8",
          "form": "지금도 북엇국 못먹는거랑 비슷해",
          "original_form": "지금도 북엇국 못먹는거랑 비슷해",
          "speaker_id": "2",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100045372.1.9",
          "form": "맛의 정수는",
          "original_form": "맛의 정수는",
          "speaker_id": "1",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045372.1.10",
          "form": "액상과당이었음을 알기엔",
          "original_form": "액상과당이었음을 알기엔",
          "speaker_id": "1",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045372.1.11",
          "form": "너무 어린 나이긴 하지 ㅋㅋㅋㅋ",
          "original_form": "너무 어린 나이긴 하지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045372.1.12",
          "form": "사실 북어국은",
          "original_form": "사실 북어국은",
          "speaker_id": "2",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045372.1.13",
          "form": "처음 먹었을때 진짜",
          "original_form": "처음 먹었을때 진짜",
          "speaker_id": "2",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045372.1.14",
          "form": "너무 맛있는거야",
          "original_form": "너무 맛있는거야",
          "speaker_id": "2",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045372.1.15",
          "form": "말린 북어를 하루종일 뜯어먹고",
          "original_form": "말린 북어를 하루종일 뜯어먹고",
          "speaker_id": "2",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045372.1.16",
          "form": "오 ㅋㅋㅋㅋ 반전인뎈",
          "original_form": "오 ㅋㅋㅋㅋ 반전인뎈",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045372.1.17",
          "form": "저녁으로 북엇국까지 먹었더니",
          "original_form": "저녁으로 북엇국까지 먹었더니",
          "speaker_id": "2",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045372.1.18",
          "form": "ㄹㅇ 지독하게 체했지.....",
          "original_form": "ㄹㅇ 지독하게 체했지.....",
          "speaker_id": "2",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045372.1.19",
          "form": "그때 엄청 토했는데",
          "original_form": "그때 엄청 토했는데",
          "speaker_id": "2",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045372.1.20",
          "form": "ㄹㅇ 북어만 나오는거야",
          "original_form": "ㄹㅇ 북어만 나오는거야",
          "speaker_id": "2",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045372.1.21",
          "form": "저런....",
          "original_form": "저런....",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045372.1.22",
          "form": "그게 문제엿군",
          "original_form": "그게 문제엿군",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045372.1.23",
          "form": "그 토사물+북어 냄새가",
          "original_form": "그 토사물+북어 냄새가",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045372.1.24",
          "form": "내가 북어국 냄새만 맡아도",
          "original_form": "내가 북어국 냄새만 맡아도",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045372.1.25",
          "form": "자리를 피하게 만들엇지.....",
          "original_form": "자리를 피하게 만들엇지.....",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045372.1.26",
          "form": "이건 초 3때야",
          "original_form": "이건 초 3때야",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045372.1.27",
          "form": "ㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045372.1.28",
          "form": "어쩐지",
          "original_form": "어쩐지",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045372.1.29",
          "form": "근데 비린거 잘 못먹지 않아?",
          "original_form": "근데 비린거 잘 못먹지 않아?",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045372.1.30",
          "form": "어릴땐 그나마 좀 나았어",
          "original_form": "어릴땐 그나마 좀 나았어",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045372.1.31",
          "form": "어릴땐 파래무침같은것도 먹고 그랬는데",
          "original_form": "어릴땐 파래무침같은것도 먹고 그랬는데",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045372.1.32",
          "form": "점점 못먹겠더라구",
          "original_form": "점점 못먹겠더라구",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045372.1.33",
          "form": "한의원 어디 가보니까",
          "original_form": "한의원 어디 가보니까",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045372.1.34",
          "form": "신장이 안좋으면 비위가 약해진다는데",
          "original_form": "신장이 안좋으면 비위가 약해진다는데",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045372.1.35",
          "form": "딱히 신장이 안좋진 않음....",
          "original_form": "딱히 신장이 안좋진 않음....",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045372.1.36",
          "form": "헐 그런것도 관련있대?",
          "original_form": "헐 그런것도 관련있대?",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045372.1.37",
          "form": "그런다고 하긴 하던데",
          "original_form": "그런다고 하긴 하던데",
          "speaker_id": "2",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045372.1.38",
          "form": "난 건강검진에서 신장이상은 한번도 나온적 없어서",
          "original_form": "난 건강검진에서 신장이상은 한번도 나온적 없어서",
          "speaker_id": "2",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045372.1.39",
          "form": "내 케이스는 아닌것같아",
          "original_form": "내 케이스는 아닌것같아",
          "speaker_id": "2",
          "time": "20210908 02:13"
        }
      ]
    }
  ]
