      "utterance": [
        {
          "id": "MDRW2100016259.1.1",
          "form": "저는 해산물 요리 좋아해서 일식 좋아해요",
          "original_form": "저는 해산물 요리 좋아해서 일식 좋아해요",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016259.1.2",
          "form": "오 간장게장도 좋아하시나요?",
          "original_form": "오 간장게장도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016259.1.3",
          "form": "네 없어서 못 먹어요",
          "original_form": "네 없어서 못 먹어요",
          "speaker_id": "2",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100016259.1.4",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100016259.1.5",
          "form": "네! 오늘 점심엔 뭐 드실 예정이세요?",
          "original_form": "네! 오늘 점심엔 뭐 드실 예정이세요?",
          "speaker_id": "1",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100016259.1.6",
          "form": "아직 안정했어요 추천해주세요",
          "original_form": "아직 안정했어요 추천해주세요",
          "speaker_id": "2",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100016259.1.7",
          "form": "짬뽕 좋아하시나요?",
          "original_form": "짬뽕 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100016259.1.8",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016259.1.9",
          "form": "맛있겠네요!",
          "original_form": "맛있겠네요!",
          "speaker_id": "2",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016259.1.10",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016259.1.11",
          "form": "곧 먹을것같아요!",
          "original_form": "곧 먹을것같아요!",
          "speaker_id": "1",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100016259.1.12",
          "form": "뭐드세요?",
          "original_form": "뭐드세요?",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016259.1.13",
          "form": "전 닭ㄱㅏ슴살이요ㅎㅎ... 드셔보셨나요?",
          "original_form": "전 닭ㄱㅏ슴살이요ㅎㅎ... 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016259.1.14",
          "form": "그럼요 식이할때 자주먹었죠",
          "original_form": "그럼요 식이할때 자주먹었죠",
          "speaker_id": "2",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016259.1.15",
          "form": "오 어디게 맛있나요?",
          "original_form": "오 어디게 맛있나요?",
          "speaker_id": "1",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016259.1.16",
          "form": "저는 노브랜드 냉동 가슴살 사두고 먹었었어요",
          "original_form": "저는 노브랜드 냉동 가슴살 사두고 먹었었어요",
          "speaker_id": "2",
          "time": "20210826 12:12"
        }
      ]
