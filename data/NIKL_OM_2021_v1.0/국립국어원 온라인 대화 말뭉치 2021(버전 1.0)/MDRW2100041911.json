      "utterance": [
        {
          "id": "MDRW2100041911.1.1",
          "form": "최근에 젤 탐나는 물건은 뭐예요?",
          "original_form": "최근에 젤 탐나는 물건은 뭐예요?",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041911.1.2",
          "form": "저는... 저는",
          "original_form": "저는... 저는",
          "speaker_id": "1",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041911.1.3",
          "form": "일단 운동화랑,,,",
          "original_form": "일단 운동화랑,,,",
          "speaker_id": "1",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041911.1.4",
          "form": "갤럭시워치랑......",
          "original_form": "갤럭시워치랑......",
          "speaker_id": "1",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041911.1.5",
          "form": "볼캡이랑.........",
          "original_form": "볼캡이랑.........",
          "speaker_id": "1",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041911.1.6",
          "form": "그리고 덕질하는 캐릭터 굿즈랑.......",
          "original_form": "그리고 덕질하는 캐릭터 굿즈랑.......",
          "speaker_id": "1",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041911.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041911.1.8",
          "form": "그만....그만...!",
          "original_form": "그만....그만...!",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041911.1.9",
          "form": "물욕을 멈춰주세요 ㅠ",
          "original_form": "물욕을 멈춰주세요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041911.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041911.1.11",
          "form": "아.....",
          "original_form": "아.....",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041911.1.12",
          "form": "괜찮아요",
          "original_form": "괜찮아요",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041911.1.13",
          "form": "돈이 없어서 못사요!",
          "original_form": "돈이 없어서 못사요!",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041911.1.14",
          "form": "껄껄껄,,,",
          "original_form": "껄껄껄,,,",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041911.1.15",
          "form": "껄.",
          "original_form": "껄.",
          "speaker_id": "2",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041911.1.16",
          "form": "그래서 name2님 생일선물은",
          "original_form": "그래서 &name2&님 생일선물은",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041911.1.17",
          "form": "무엇으로 생각하셨나요?",
          "original_form": "무엇으로 생각하셨나요?",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041911.1.18",
          "form": "....저도 사실 돈있으면 뭐 없던 물욕도 생길걸요^^",
          "original_form": "....저도 사실 돈있으면 뭐 없던 물욕도 생길걸요^^",
          "speaker_id": "2",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041911.1.19",
          "form": "이건 타의적 노물욕인듯",
          "original_form": "이건 타의적 노물욕인듯",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041911.1.20",
          "form": "저 아직 고민중입니다..",
          "original_form": "저 아직 고민중입니다..",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041911.1.21",
          "form": "궁금합니다 name2님의",
          "original_form": "궁금합니다 &name2&님의",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041911.1.22",
          "form": "물욕~!~!!",
          "original_form": "물욕~!~!!",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041911.1.23",
          "form": "물욕 없는 분들 부러워요ㅜ",
          "original_form": "물욕 없는 분들 부러워요ㅜ",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041911.1.24",
          "form": "근데 진짜 물욕 많은 사람에 비하면",
          "original_form": "근데 진짜 물욕 많은 사람에 비하면",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041911.1.25",
          "form": "ㅋㅋㅋㅋ아냐요 진짜 저도 물욕 쩔었어요",
          "original_form": "ㅋㅋㅋㅋ아냐요 진짜 저도 물욕 쩔었어요",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041911.1.26",
          "form": "타의적 검소함이에요 저는,,,,",
          "original_form": "타의적 검소함이에요 저는,,,,",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041911.1.27",
          "form": "맥시멀리스트라...",
          "original_form": "맥시멀리스트라...",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041911.1.28",
          "form": "그럼 계기가 있으신가요?!",
          "original_form": "그럼 계기가 있으신가요?!",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041911.1.29",
          "form": "근데 돈이 아깝기도 하고 저는 진짜 옷 화장품 많이 샀거든요",
          "original_form": "근데 돈이 아깝기도 하고 저는 진짜 옷 화장품 많이 샀거든요",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041911.1.30",
          "form": "아뮤래도 탈코가 큰 영향을 끼치기도 했고",
          "original_form": "아뮤래도 탈코가 큰 영향을 끼치기도 했고",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041911.1.31",
          "form": "호주워홀할 때 짐많으니까 미쳐버리겠더라구료",
          "original_form": "호주워홀할 때 짐많으니까 미쳐버리겠더라구료",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041911.1.32",
          "form": "그 뒤로 짐을 늘리는걸 별로 안좋아하게됐어요",
          "original_form": "그 뒤로 짐을 늘리는걸 별로 안좋아하게됐어요",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041911.1.33",
          "form": "아 그러면 진짜",
          "original_form": "아 그러면 진짜",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041911.1.34",
          "form": "그러시겠어요",
          "original_form": "그러시겠어요",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041911.1.35",
          "form": "맞아요 저도 기숙사 이사할 때 너무 힘들어서",
          "original_form": "맞아요 저도 기숙사 이사할 때 너무 힘들어서",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041911.1.36",
          "form": "다 버리고 싶었던 기억이... ^~^",
          "original_form": "다 버리고 싶었던 기억이... ^~^",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041911.1.37",
          "form": "ㅋㅋㅋㅋㅋ그니까요 짐짜 뭐 쫌쫌짜리 삿는데",
          "original_form": "ㅋㅋㅋㅋㅋ그니까요 짐짜 뭐 쫌쫌짜리 삿는데",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041911.1.38",
          "form": "무게 1톤",
          "original_form": "무게 1톤",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041911.1.39",
          "form": "거의 이지경..",
          "original_form": "거의 이지경..",
          "speaker_id": "2",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041911.1.40",
          "form": "맞아요,,,, (숙연",
          "original_form": "맞아요,,,, (숙연",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041911.1.41",
