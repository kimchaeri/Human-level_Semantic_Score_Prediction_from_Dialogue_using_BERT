      "utterance": [
        {
          "id": "MDRW2100028893.1.1",
          "form": "라멘 맛집 꼭 찾고 싶어요..",
          "original_form": "라멘 맛집 꼭 찾고 싶어요..",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028893.1.2",
          "form": "예전에 승리....일터지기전에 ㅎㅎ",
          "original_form": "예전에 승리....일터지기전에 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028893.1.3",
          "form": "라멘 본점 가봤거든요",
          "original_form": "라멘 본점 가봤거든요",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028893.1.4",
          "form": "맛은있는데",
          "original_form": "맛은있는데",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028893.1.5",
          "form": "역시나 기름기가 많은 음식이라그런가",
          "original_form": "역시나 기름기가 많은 음식이라그런가",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028893.1.6",
          "form": "나중에 느끼해서 못먹게ㅛ더라구요 ㅋㅋ",
          "original_form": "나중에 느끼해서 못먹게ㅛ더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028893.1.7",
          "form": "으악 거기 너무 가고 싶었는데 바로 터진..ㅋㅋㅋ",
          "original_form": "으악 거기 너무 가고 싶었는데 바로 터진..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100028893.1.8",
          "form": "헉 진짜요? 하...면 정말 좋아하는데... 맛있는거 먹고 싶어요ㅠ",
          "original_form": "헉 진짜요? 하...면 정말 좋아하는데... 맛있는거 먹고 싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100028893.1.9",
          "form": "정말 맛은있었어요 ㅋㅋㅋ",
          "original_form": "정말 맛은있었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100028893.1.10",
          "form": "라멘자체가 기름때문에그렇징 ...ㅎㅎ",
          "original_form": "라멘자체가 기름때문에그렇징 ...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100028893.1.11",
          "form": "하... 먹어보고 싶었어요... 눈물... 정말 맛집이라 소문났었는데ㅜㅜ 이제 갈데가없네요",
          "original_form": "하... 먹어보고 싶었어요... 눈물... 정말 맛집이라 소문났었는데ㅜㅜ 이제 갈데가없네요",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028893.1.12",
          "form": "요줌엔",
          "original_form": "요줌엔",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100028893.1.13",
          "form": "맛집이라고 소개해도..",
          "original_form": "맛집이라고 소개해도..",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100028893.1.14",
          "form": "못믿겠어염 ㅠ",
          "original_form": "못믿겠어염 ㅠ",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100028893.1.15",
          "form": "진짜요... 이게 광고인지 뭔지를 모르겠어서ㅠㅜ",
          "original_form": "진짜요... 이게 광고인지 뭔지를 모르겠어서ㅠㅜ",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100028893.1.16",
          "form": "긍께여 ㅠㅠ맛집이라고 찾아가면",
          "original_form": "긍께여 ㅠㅠ맛집이라고 찾아가면",
          "speaker_id": "2",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100028893.1.17",
          "form": "맛이...그냥.",
          "original_form": "맛이...그냥.",
          "speaker_id": "2",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100028893.1.18",
          "form": "그래가지고;;",
          "original_form": "그래가지고;;",
          "speaker_id": "2",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100028893.1.19",
          "form": "맞아요,, 진짜 저만의 맛집을 찾고 싶은..",
          "original_form": "맞아요,, 진짜 저만의 맛집을 찾고 싶은..",
          "speaker_id": "1",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100028893.1.20",
          "form": "저번에 도쿄스테이크라고 체인...점인가??",
          "original_form": "저번에 도쿄스테이크라고 체인...점인가??",
          "speaker_id": "2",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100028893.1.21",
          "form": "아닐수도있는데",
          "original_form": "아닐수도있는데",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100028893.1.22",
          "form": "거기도 조금 괜찮드라구용",
          "original_form": "거기도 조금 괜찮드라구용",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100028893.1.23",
          "form": "라멘집.ㅡ",
          "original_form": "라멘집.ㅡ",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100028893.1.24",
          "form": "도쿄스테이크 라멘집이에요? 헉 가봐야겟어요",
          "original_form": "도쿄스테이크 라멘집이에요? 헉 가봐야겟어요",
          "speaker_id": "1",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100028893.1.25",
          "form": "뭐...지점마다 맛이.다들수도 있쥐만 ..ㅎㅎ",
          "original_form": "뭐...지점마다 맛이.다들수도 있쥐만 ..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 17:05"
        },
        {
          "id": "MDRW2100028893.1.26",
          "form": "그래도 비슷하지 않을까요 넘 맛있을 것 같아요",
          "original_form": "그래도 비슷하지 않을까요 넘 맛있을 것 같아요",
          "speaker_id": "1",
          "time": "20210903 17:06"
        },
        {
          "id": "MDRW2100028893.1.27",
          "form": "라멘 진짜 먹으러 가고 싶어요",
          "original_form": "라멘 진짜 먹으러 가고 싶어요",
