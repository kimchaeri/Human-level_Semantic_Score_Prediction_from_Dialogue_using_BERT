      "utterance": [
        {
          "id": "MDRW2100003724.1.1",
          "form": "치과 비용이 만만치 않네요",
          "original_form": "치과 비용이 만만치 않네요",
          "speaker_id": "1",
          "time": "20210821 11:18"
        },
        {
          "id": "MDRW2100003724.1.2",
          "form": "치아건강을 위해 하고 계신거 있으세요?",
          "original_form": "치아건강을 위해 하고 계신거 있으세요?",
          "speaker_id": "1",
          "time": "20210821 11:22"
        },
        {
          "id": "MDRW2100003724.1.3",
          "form": "비용이 너무 많이 들어요",
          "original_form": "비용이 너무 많이 들어요",
          "speaker_id": "2",
          "time": "20210821 11:26"
        },
        {
          "id": "MDRW2100003724.1.4",
          "form": "잇몸치약 구강청결제",
          "original_form": "잇몸치약 구강청결제",
          "speaker_id": "2",
          "time": "20210821 11:26"
        },
        {
          "id": "MDRW2100003724.1.5",
          "form": "영양제 먹어요",
          "original_form": "영양제 먹어요",
          "speaker_id": "2",
          "time": "20210821 11:26"
        },
        {
          "id": "MDRW2100003724.1.6",
          "form": "아~영양제는 뭐드세요?",
          "original_form": "아~영양제는 뭐드세요?",
          "speaker_id": "1",
          "time": "20210821 11:26"
        },
        {
          "id": "MDRW2100003724.1.7",
          "form": "치아에 좋은게 따로 있나요?",
          "original_form": "치아에 좋은게 따로 있나요?",
          "speaker_id": "1",
          "time": "20210821 11:26"
        },
        {
          "id": "MDRW2100003724.1.8",
          "form": "딱히 치아에 좋은거는 아니고 몸에 좋은거요",
          "original_form": "딱히 치아에 좋은거는 아니고 몸에 좋은거요",
          "speaker_id": "2",
          "time": "20210821 11:27"
        },
        {
          "id": "MDRW2100003724.1.9",
          "form": "치아 제품은 효과 없데요",
          "original_form": "치아 제품은 효과 없데요",
          "speaker_id": "2",
          "time": "20210821 11:27"
        },
        {
          "id": "MDRW2100003724.1.10",
          "form": "아~ㅠㅠ 아쉽네요ㅠㅠ 저도 잇몸이 너무 안좋아서ㅠㅠ",
          "original_form": "아~ㅠㅠ 아쉽네요ㅠㅠ 저도 잇몸이 너무 안좋아서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 11:27"
        },
        {
          "id": "MDRW2100003724.1.11",
          "form": "고기 먹으면 잇몸이 너무 아파요",
          "original_form": "고기 먹으면 잇몸이 너무 아파요",
          "speaker_id": "1",
          "time": "20210821 11:27"
        },
        {
          "id": "MDRW2100003724.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 11:28"
        },
        {
          "id": "MDRW2100003724.1.13",
          "form": "임플란트 쪽은 잘안써서 반대쪽만 먹으니 그쪽이 부어요",
          "original_form": "임플란트 쪽은 잘안써서 반대쪽만 먹으니 그쪽이 부어요",
          "speaker_id": "2",
          "time": "20210821 11:28"
        },
        {
          "id": "MDRW2100003724.1.14",
          "form": "아~임플란트 쪽은 왜 안 쓰세요?ㅠㅠ 저도 곧 해야할수 있어서 관심이 많아요",
          "original_form": "아~임플란트 쪽은 왜 안 쓰세요?ㅠㅠ 저도 곧 해야할수 있어서 관심이 많아요",
          "speaker_id": "1",
          "time": "20210821 11:29"
        },
        {
          "id": "MDRW2100003724.1.15",
          "form": "좀 겁나요 쓰기가",
          "original_form": "좀 겁나요 쓰기가",
          "speaker_id": "2",
          "time": "20210821 11:29"
        },
        {
          "id": "MDRW2100003724.1.16",
          "form": "어색하고요",
          "original_form": "어색하고요",
          "speaker_id": "2",
          "time": "20210821 11:29"
        },
        {
          "id": "MDRW2100003724.1.17",
          "form": "사이에 뭐 낄까봐도 안써요",
          "original_form": "사이에 뭐 낄까봐도 안써요",
          "speaker_id": "2",
          "time": "20210821 11:29"
        },
        {
          "id": "MDRW2100003724.1.18",
          "form": "아~~저도 임플란트 하려고 했던 2개가 치아는 없고 뿌리만 있거든요",
          "original_form": "아~~저도 임플란트 하려고 했던 2개가 치아는 없고 뿌리만 있거든요",
          "speaker_id": "1",
          "time": "20210821 11:30"
        },
        {
          "id": "MDRW2100003724.1.19",
          "form": "아 빨리 하셔야 겠네요",
          "original_form": "아 빨리 하셔야 겠네요",
          "speaker_id": "2",
          "time": "20210821 11:30"
        },
        {
          "id": "MDRW2100003724.1.20",
          "form": "거기에 가짜 이빨로 몸통만 입힌거라 약해요",
          "original_form": "거기에 가짜 이빨로 몸통만 입힌거라 약해요",
          "speaker_id": "1",
          "time": "20210821 11:30"
        },
        {
          "id": "MDRW2100003724.1.21",
          "form": "전 뿌리도 없어져서 이식했어요",
          "original_form": "전 뿌리도 없어져서 이식했어요",
          "speaker_id": "2",
          "time": "20210821 11:31"
        },
        {
          "id": "MDRW2100003724.1.22",
          "form": "헉 그거 빨리 해야해요",
          "original_form": "헉 그거 빨리 해야해요",
          "speaker_id": "2",
          "time": "20210821 11:31"
        }
      ]
    }
