      "utterance": [
        {
          "id": "MDRW2100038582.1.1",
          "form": "굿모닝입니다",
          "original_form": "굿모닝입니다",
          "speaker_id": "1",
          "time": "20210903 08:44"
        },
        {
          "id": "MDRW2100038582.1.2",
          "form": "네 안녕하세요 ㅎㅎ",
          "original_form": "네 안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 08:44"
        },
        {
          "id": "MDRW2100038582.1.3",
          "form": "최근에 사신거 있나요",
          "original_form": "최근에 사신거 있나요",
          "speaker_id": "1",
          "time": "20210903 08:45"
        },
        {
          "id": "MDRW2100038582.1.4",
          "form": "전 어젯밤에 하나 질렀어요",
          "original_form": "전 어젯밤에 하나 질렀어요",
          "speaker_id": "1",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100038582.1.5",
          "form": "오 어떤거 사셨나요",
          "original_form": "오 어떤거 사셨나요",
          "speaker_id": "2",
          "time": "20210903 08:47"
        },
        {
          "id": "MDRW2100038582.1.6",
          "form": "겨울에 잘 때 안고잘 알파카 인형이요",
          "original_form": "겨울에 잘 때 안고잘 알파카 인형이요",
          "speaker_id": "1",
          "time": "20210903 08:48"
        },
        {
          "id": "MDRW2100038582.1.7",
          "form": "여름에는 더워서 엄두 못냈다가 가을 와서 샀어요",
          "original_form": "여름에는 더워서 엄두 못냈다가 가을 와서 샀어요",
          "speaker_id": "1",
          "time": "20210903 08:49"
        },
        {
          "id": "MDRW2100038582.1.8",
          "form": "오오 좋겠다 저도 인형여러개있어욬ㅋㅋ 안고자면좋아요",
          "original_form": "오오 좋겠다 저도 인형여러개있어욬ㅋㅋ 안고자면좋아요",
          "speaker_id": "2",
          "time": "20210903 09:09"
        },
        {
          "id": "MDRW2100038582.1.9",
          "form": "저는 최근에 오렌지산답니다",
          "original_form": "저는 최근에 오렌지산답니다",
          "speaker_id": "2",
          "time": "20210903 09:09"
        },
        {
          "id": "MDRW2100038582.1.10",
          "form": "오렌지요?",
          "original_form": "오렌지요?",
          "speaker_id": "1",
          "time": "20210903 09:10"
        },
        {
          "id": "MDRW2100038582.1.11",
          "form": "넵ㅋㅋ 어제 샀어요",
          "original_form": "넵ㅋㅋ 어제 샀어요",
          "speaker_id": "2",
          "time": "20210903 09:42"
        },
        {
          "id": "MDRW2100038582.1.12",
          "form": "친구가 맛있다 추천해서...",
          "original_form": "친구가 맛있다 추천해서...",
          "speaker_id": "2",
          "time": "20210903 09:42"
        },
        {
          "id": "MDRW2100038582.1.13",
          "form": "ㅋㅋ 친구분이 그런 추천도 해주셨군요",
          "original_form": "ㅋㅋ 친구분이 그런 추천도 해주셨군요",
          "speaker_id": "1",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100038582.1.14",
          "form": "ㅋㅋㅋㅋ아침마다 먹는다고 피부 좋아졌대서",
          "original_form": "ㅋㅋㅋㅋ아침마다 먹는다고 피부 좋아졌대서",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100038582.1.15",
          "form": "냉큼 샀어요",
          "original_form": "냉큼 샀어요",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100038582.1.16",
          "form": "피부 좋아지는건 못참죠",
          "original_form": "피부 좋아지는건 못참죠",
          "speaker_id": "1",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100038582.1.17",
          "form": "못참져ㅋㅋㅋ요즘 포도도 땡기는데 살말고민하고있어여ㅋㅋㅋ",
          "original_form": "못참져ㅋㅋㅋ요즘 포도도 땡기는데 살말고민하고있어여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100038582.1.18",
          "form": "씨없는 포도라면 사시고",
          "original_form": "씨없는 포도라면 사시고",
          "speaker_id": "1",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100038582.1.19",
          "form": "씨 있으면 좀더 고민하시죠",
          "original_form": "씨 있으면 좀더 고민하시죠",
          "speaker_id": "1",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100038582.1.20",
          "form": "좋은생각입니다ㅎㅎ 발라먹기 귀찮아요",
          "original_form": "좋은생각입니다ㅎㅎ 발라먹기 귀찮아요",
          "speaker_id": "2",
          "time": "20210903 10:52"
        }
      ]
