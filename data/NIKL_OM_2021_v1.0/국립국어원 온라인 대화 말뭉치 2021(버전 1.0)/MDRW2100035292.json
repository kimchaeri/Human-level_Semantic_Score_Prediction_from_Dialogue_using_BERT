      "utterance": [
        {
          "id": "MDRW2100035292.1.1",
          "form": "아 생각해보니 그 가게도",
          "original_form": "아 생각해보니 그 가게도",
          "speaker_id": "2",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100035292.1.2",
          "form": "코로나 이후에 사라졌네요",
          "original_form": "코로나 이후에 사라졌네요",
          "speaker_id": "2",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100035292.1.3",
          "form": "진짜 코로나ㅜ",
          "original_form": "진짜 코로나ㅜ",
          "speaker_id": "2",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100035292.1.4",
          "form": "10년 정도 있던 가게인데 정말 아쉬워요",
          "original_form": "10년 정도 있던 가게인데 정말 아쉬워요",
          "speaker_id": "2",
          "time": "20210829 13:56"
        },
        {
          "id": "MDRW2100035292.1.5",
          "form": "제 떡볶이집도 코로나때문에 몇개 날아갓ㄱ어요",
          "original_form": "제 떡볶이집도 코로나때문에 몇개 날아갓ㄱ어요",
          "speaker_id": "1",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100035292.1.6",
          "form": "1인분씩 파는 학교앞 스타일은",
          "original_form": "1인분씩 파는 학교앞 스타일은",
          "speaker_id": "1",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100035292.1.7",
          "form": "배달을 잘 안하잖아요",
          "original_form": "배달을 잘 안하잖아요",
          "speaker_id": "1",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100035292.1.8",
          "form": "그래서 못버티더라고요",
          "original_form": "그래서 못버티더라고요",
          "speaker_id": "1",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100035292.1.9",
          "form": "아ㅜ",
          "original_form": "아ㅜ",
          "speaker_id": "2",
          "time": "20210829 14:01"
        },
        {
          "id": "MDRW2100035292.1.10",
          "form": "진짜 그런가봐요",
          "original_form": "진짜 그런가봐요",
          "speaker_id": "2",
          "time": "20210829 14:01"
        },
        {
          "id": "MDRW2100035292.1.11",
          "form": "소상인들 타격이 컸죠",
          "original_form": "소상인들 타격이 컸죠",
          "speaker_id": "2",
          "time": "20210829 14:01"
        },
        {
          "id": "MDRW2100035292.1.12",
          "form": "네 전 꼬마김밥도 좋아하는데",
          "original_form": "네 전 꼬마김밥도 좋아하는데",
          "speaker_id": "1",
          "time": "20210829 14:01"
        },
        {
          "id": "MDRW2100035292.1.13",
          "form": "주로 길에서 먹잖아요",
          "original_form": "주로 길에서 먹잖아요",
          "speaker_id": "1",
          "time": "20210829 14:01"
        },
        {
          "id": "MDRW2100035292.1.14",
          "form": "ㅁ네",
          "original_form": "ㅁ네",
          "speaker_id": "2",
          "time": "20210829 14:01"
        },
        {
          "id": "MDRW2100035292.1.15",
          "form": "마스크벗고 길에서 밥먹기 어려워지니 다 망했어요",
          "original_form": "마스크벗고 길에서 밥먹기 어려워지니 다 망했어요",
          "speaker_id": "1",
          "time": "20210829 14:01"
        },
        {
          "id": "MDRW2100035292.1.16",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210829 14:01"
        },
        {
          "id": "MDRW2100035292.1.17",
          "form": "진짜 안타까워요",
          "original_form": "진짜 안타까워요",
          "speaker_id": "2",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100035292.1.18",
          "form": "빨리 코로나가 끝나거나",
          "original_form": "빨리 코로나가 끝나거나",
          "speaker_id": "2",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100035292.1.19",
          "form": "ㅠㅜ코로나가 여럿잡네요..",
          "original_form": "ㅠㅜ코로나가 여럿잡네요..",
          "speaker_id": "1",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100035292.1.20",
          "form": "좀 나아지면 괜찮을텐데",
          "original_form": "좀 나아지면 괜찮을텐데",
          "speaker_id": "2",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100035292.1.21",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100035292.1.22",
          "form": "그래도 백신 맞으면 많이 나아젤것 같아서 다행이에요",
          "original_form": "그래도 백신 맞으면 많이 나아젤것 같아서 다행이에요",
          "speaker_id": "2",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100035292.1.23",
          "form": "델타변이래서 그것두 잘..ㅠㅋㅋㅋ",
          "original_form": "델타변이래서 그것두 잘..ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100035292.1.24",
          "form": "그건 그렇긴하죠",
          "original_form": "그건 그렇긴하죠",
          "speaker_id": "2",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100035292.1.25",
          "form": "그래도 걸려도 안 죽겠거니ㅜ하는 기대는 있어요",
          "original_form": "그래도 걸려도 안 죽겠거니ㅜ하는 기대는 있어요",
          "speaker_id": "1",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100035292.1.26",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100035292.1.27",
          "form": "쌀떡파시면 그래도 소량으로 떡사긴 편하시겠네요",
          "original_form": "쌀떡파시면 그래도 소량으로 떡사긴 편하시겠네요",
          "speaker_id": "1",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100035292.1.28",
          "form": "밀떡은 좀 큰 마트가야 소량으로 팔구. 아님 분식집 용 떡 한판으로 큰걸루 사야해요",
          "original_form": "밀떡은 좀 큰 마트가야 소량으로 팔구. 아님 분식집 용 떡 한판으로 큰걸루 사야해요",
          "speaker_id": "1",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100035292.1.29",
          "form": "헐 그렇군요",
          "original_form": "헐 그렇군요",
          "speaker_id": "2",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100035292.1.30",
          "form": "그건 또 몰랐네요",
          "original_form": "그건 또 몰랐네요",
          "speaker_id": "2",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100035292.1.31",
          "form": "그래서 전 집에서 떡볶이를 해먹을땐",
          "original_form": "그래서 전 집에서 떡볶이를 해먹을땐",
          "speaker_id": "1",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100035292.1.32",
          "form": "박진영이 돼요",
          "original_form": "박진영이 돼요",
          "speaker_id": "1",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100035292.1.33",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100035292.1.34",
          "form": "박진영이요???",
          "original_form": "박진영이요???",
          "speaker_id": "2",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100035292.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100035292.1.36",
          "form": "니가 사는 그 집\r\n그 집이 내 집이었어야 해\r\n니가 타는 그 차\r\n그 차가 내 차였어야 해",
          "original_form": "니가 사는 그 집\r\n그 집이 내 집이었어야 해\r\n니가 타는 그 차\r\n그 차가 내 차였어야 해",
          "speaker_id": "1",
          "time": "20210829 14:05"
        },
        {
          "id": "MDRW2100035292.1.37",
          "form": "이 가사대로",
          "original_form": "이 가사대로",
          "speaker_id": "1",
          "time": "20210829 14:05"
        },
        {
          "id": "MDRW2100035292.1.38",
          "form": "이 쌀떡..니가 밀떡이었어야해",
          "original_form": "이 쌀떡..니가 밀떡이었어야해",
          "speaker_id": "1",
          "time": "20210829 14:05"
        },
        {
          "id": "MDRW2100035292.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:05"
        },
        {
          "id": "MDRW2100035292.1.40",
          "form": "쌀떡볶이가 그래도",
          "original_form": "쌀떡볶이가 그래도",
          "speaker_id": "2",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100035292.1.41",
          "form": "구하기 쉽다는 장점이 있었네요",
          "original_form": "구하기 쉽다는 장점이 있었네요",
          "speaker_id": "2",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100035292.1.42",
          "form": "전 영 안되면 떡국떡으로 먹기도해요",
          "original_form": "전 영 안되면 떡국떡으로 먹기도해요",
          "speaker_id": "2",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100035292.1.43",
          "form": "네 집에서 해먹기에 좋구 그리고 정안되면",
          "original_form": "네 집에서 해먹기에 좋구 그리고 정안되면",
          "speaker_id": "1",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100035292.1.44",
          "form": "네 떡국떡이 있어요",
          "original_form": "네 떡국떡이 있어요",
          "speaker_id": "1",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100035292.1.45",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100035292.1.46",
          "form": "제가 초등학생때 먹은",
          "original_form": "제가 초등학생때 먹은",
          "speaker_id": "1",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100035292.1.47",
          "form": "사는 거 다 비슷하네요",
          "original_form": "사는 거 다 비슷하네요",
          "speaker_id": "2",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100035292.1.48",
          "form": "밀떡맛에 길이 들어서 그래요 ㅋㅋㅋ",
          "original_form": "밀떡맛에 길이 들어서 그래요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:07"
        },
