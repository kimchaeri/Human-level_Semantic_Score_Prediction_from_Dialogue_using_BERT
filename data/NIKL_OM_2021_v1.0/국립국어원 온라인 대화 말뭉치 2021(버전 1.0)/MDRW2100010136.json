      "utterance": [
        {
          "id": "MDRW2100010136.1.1",
          "form": "즐겨 보는 프로 있니",
          "original_form": "즐겨 보는 프로 있니",
          "speaker_id": "1",
          "time": "20210823 14:45"
        },
        {
          "id": "MDRW2100010136.1.2",
          "form": "나 티비를 잘 안 봐",
          "original_form": "나 티비를 잘 안 봐",
          "speaker_id": "2",
          "time": "20210823 14:45"
        },
        {
          "id": "MDRW2100010136.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210823 14:45"
        },
        {
          "id": "MDRW2100010136.1.4",
          "form": "나는 할 거 없을 때 봐",
          "original_form": "나는 할 거 없을 때 봐",
          "speaker_id": "1",
          "time": "20210823 14:46"
        },
        {
          "id": "MDRW2100010136.1.5",
          "form": "그럼 너는 주로 뭘 봐",
          "original_form": "그럼 너는 주로 뭘 봐",
          "speaker_id": "1",
          "time": "20210823 14:46"
        },
        {
          "id": "MDRW2100010136.1.6",
          "form": "유튜브에서 클립된 영상들 봐",
          "original_form": "유튜브에서 클립된 영상들 봐",
          "speaker_id": "2",
          "time": "20210823 14:47"
        },
        {
          "id": "MDRW2100010136.1.7",
          "form": "웃긴 부분만 골라서 편집된 거",
          "original_form": "웃긴 부분만 골라서 편집된 거",
          "speaker_id": "2",
          "time": "20210823 14:47"
        },
        {
          "id": "MDRW2100010136.1.8",
          "form": "예를 들면 어떤 거",
          "original_form": "예를 들면 어떤 거",
          "speaker_id": "1",
          "time": "20210823 14:47"
        },
        {
          "id": "MDRW2100010136.1.9",
          "form": "나혼자산다나 아는형님이나 특정 개그맨을 찾아본다든지 그렇게",
          "original_form": "나혼자산다나 아는형님이나 특정 개그맨을 찾아본다든지 그렇게",
          "speaker_id": "2",
          "time": "20210823 14:48"
        },
        {
          "id": "MDRW2100010136.1.10",
          "form": "짧은 영상 보는 걸 좋아하는구나",
          "original_form": "짧은 영상 보는 걸 좋아하는구나",
          "speaker_id": "1",
          "time": "20210823 14:49"
        },
        {
          "id": "MDRW2100010136.1.11",
          "form": "구독하는 유튜버는 있어?",
          "original_form": "구독하는 유튜버는 있어?",
          "speaker_id": "1",
          "time": "20210823 14:49"
        },
        {
          "id": "MDRW2100010136.1.12",
          "form": "구독하는 유튜버는 너무 많아서",
          "original_form": "구독하는 유튜버는 너무 많아서",
          "speaker_id": "2",
          "time": "20210823 14:49"
        },
        {
          "id": "MDRW2100010136.1.13",
          "form": "그중 제일 좋아하는 건 누구야",
          "original_form": "그중 제일 좋아하는 건 누구야",
          "speaker_id": "1",
          "time": "20210823 14:49"
        },
        {
          "id": "MDRW2100010136.1.14",
          "form": "누굴 말해야될지 모르겠어",
          "original_form": "누굴 말해야될지 모르겠어",
          "speaker_id": "2",
          "time": "20210823 14:49"
        },
        {
          "id": "MDRW2100010136.1.15",
          "form": "한명만 뽑아봐",
          "original_form": "한명만 뽑아봐",
          "speaker_id": "1",
          "time": "20210823 14:49"
        },
        {
          "id": "MDRW2100010136.1.16",
          "form": "너무 어려운 질문인가",
          "original_form": "너무 어려운 질문인가",
          "speaker_id": "1",
          "time": "20210823 14:49"
        },
        {
          "id": "MDRW2100010136.1.17",
          "form": "음.. 생각중",
          "original_form": "음.. 생각중",
          "speaker_id": "2",
          "time": "20210823 14:50"
        },
        {
          "id": "MDRW2100010136.1.18",
          "form": "정리되면 말해줘",
          "original_form": "정리되면 말해줘",
          "speaker_id": "1",
          "time": "20210823 14:50"
        },
        {
          "id": "MDRW2100010136.1.19",
          "form": "약국님으로 할래",
          "original_form": "약국님으로 할래",
          "speaker_id": "2",
          "time": "20210823 14:51"
        },
        {
          "id": "MDRW2100010136.1.20",
          "form": "그림 그리시는 분이야",
          "original_form": "그림 그리시는 분이야",
          "speaker_id": "2",
          "time": "20210823 14:51"
        }
      ]
