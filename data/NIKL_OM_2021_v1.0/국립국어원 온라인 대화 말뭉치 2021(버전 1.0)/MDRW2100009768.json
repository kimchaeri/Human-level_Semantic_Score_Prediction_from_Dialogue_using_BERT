        {
          "id": "MDRW2100009768.1.1",
          "form": "아직 택배가ㅜ안와서 초조허구만요;;",
          "original_form": "아직 택배가ㅜ안와서 초조허구만요;;",
          "speaker_id": "2",
          "time": "20210823 15:14"
        },
        {
          "id": "MDRW2100009768.1.2",
          "form": "햄스터 집 언능와야는데;;",
          "original_form": "햄스터 집 언능와야는데;;",
          "speaker_id": "2",
          "time": "20210823 15:14"
        },
        {
          "id": "MDRW2100009768.1.3",
          "form": "난 쌀왔다",
          "original_form": "난 쌀왔다",
          "speaker_id": "1",
          "time": "20210823 15:40"
        },
        {
          "id": "MDRW2100009768.1.4",
          "form": "로션도왔다",
          "original_form": "로션도왔다",
          "speaker_id": "1",
          "time": "20210823 15:40"
        },
        {
          "id": "MDRW2100009768.1.5",
          "form": "어제 쇼핑의결과",
          "original_form": "어제 쇼핑의결과",
          "speaker_id": "1",
          "time": "20210823 15:41"
        },
        {
          "id": "MDRW2100009768.1.6",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:41"
        },
        {
          "id": "MDRW2100009768.1.7",
          "form": "오~ 언니 성공 하셨군요^^",
          "original_form": "오~ 언니 성공 하셨군요^^",
          "speaker_id": "2",
          "time": "20210823 15:41"
        },
        {
          "id": "MDRW2100009768.1.8",
          "form": "밥 안먹어도 배가 부를것 같아요~",
          "original_form": "밥 안먹어도 배가 부를것 같아요~",
          "speaker_id": "2",
          "time": "20210823 15:41"
        },
        {
          "id": "MDRW2100009768.1.9",
          "form": "배는 먹어야부르지 푸하히하",
          "original_form": "배는 먹어야부르지 푸하히하",
          "speaker_id": "1",
          "time": "20210823 15:44"
        },
        {
          "id": "MDRW2100009768.1.10",
          "form": "그렇죠 언니~역시 언니는 배운사람 ㅋㅋㅋㅋ",
          "original_form": "그렇죠 언니~역시 언니는 배운사람 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100009768.1.11",
          "form": "핫딜은 못찾았다",
          "original_form": "핫딜은 못찾았다",
          "speaker_id": "1",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100009768.1.12",
          "form": "정가주고 사는 슬픈 느낌",
          "original_form": "정가주고 사는 슬픈 느낌",
          "speaker_id": "1",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100009768.1.13",
          "form": "핫딜은 요즘 잘 없다네요~",
          "original_form": "핫딜은 요즘 잘 없다네요~",
          "speaker_id": "2",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100009768.1.14",
          "form": "핫딜박사 친구님이 ㅋㅋㅋㅋ",
          "original_form": "핫딜박사 친구님이 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100009768.1.15",
          "form": "뭐든 내손에 들어오믄 기분이가 좋죠~~",
          "original_form": "뭐든 내손에 들어오믄 기분이가 좋죠~~",
          "speaker_id": "2",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100009768.1.16",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210823 16:37"
        },
        {
          "id": "MDRW2100009768.1.17",
          "form": "다른 쇼핑으로 즐거워보자",
          "original_form": "다른 쇼핑으로 즐거워보자",
          "speaker_id": "1",
          "time": "20210823 16:37"
        },
        {
          "id": "MDRW2100009768.1.18",
          "form": "내일은 책상오겠다",
          "original_form": "내일은 책상오겠다",
          "speaker_id": "1",
          "time": "20210823 16:37"
        },
        {
          "id": "MDRW2100009768.1.19",
          "form": "책상 결정 했어요??",
          "original_form": "책상 결정 했어요??",
          "speaker_id": "2",
          "time": "20210823 16:39"
        },
        {
          "id": "MDRW2100009768.1.20",
          "form": "학교책상",
          "original_form": "학교책상",
          "speaker_id": "1",
          "time": "20210823 16:39"
        },
        {
          "id": "MDRW2100009768.1.21",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:39"
        },
        {
          "id": "MDRW2100009768.1.22",
          "form": "책상 필요는 한데 딱히 필요 없는것도 같고 ㅋㅋㅋㅋ",
          "original_form": "책상 필요는 한데 딱히 필요 없는것도 같고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:39"
        },
        {
          "id": "MDRW2100009768.1.23",
          "form": "아~!!",
          "original_form": "아~!!",
          "speaker_id": "2",
          "time": "20210823 16:39"
        },
        {
          "id": "MDRW2100009768.1.24",
          "form": "학교책상 멋지구만요^^",
          "original_form": "학교책상 멋지구만요^^",
          "speaker_id": "2",
          "time": "20210823 16:39"
        },
        {
          "id": "MDRW2100009768.1.25",
          "form": "싸고 편하고",
          "original_form": "싸고 편하고",
          "speaker_id": "1",
          "time": "20210823 16:40"
        },
        {
          "id": "MDRW2100009768.1.26",
          "form": "집에있는거 중고로 팔아야되는데",
          "original_form": "집에있는거 중고로 팔아야되는데",
          "speaker_id": "1",
          "time": "20210823 16:40"
        },
        {
          "id": "MDRW2100009768.1.27",
          "form": "ㅋㅋㅋㅋㅋ그거 파는것도 일이여요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ그거 파는것도 일이여요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:40"
