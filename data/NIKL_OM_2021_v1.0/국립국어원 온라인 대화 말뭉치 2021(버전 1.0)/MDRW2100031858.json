      "utterance": [
        {
          "id": "MDRW2100031858.1.1",
          "form": "교무실들어갈때마다 선생님들께인사드려서",
          "original_form": "교무실들어갈때마다 선생님들께인사드려서",
          "speaker_id": "2",
          "time": "20210905 03:11"
        },
        {
          "id": "MDRW2100031858.1.2",
          "form": "암튼 맛난것도자주얻어목고",
          "original_form": "암튼 맛난것도자주얻어목고",
          "speaker_id": "2",
          "time": "20210905 03:11"
        },
        {
          "id": "MDRW2100031858.1.3",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210905 03:11"
        },
        {
          "id": "MDRW2100031858.1.4",
          "form": "그랬었던...",
          "original_form": "그랬었던...",
          "speaker_id": "2",
          "time": "20210905 03:11"
        },
        {
          "id": "MDRW2100031858.1.5",
          "form": "다들 예뻐하셨겠어요",
          "original_form": "다들 예뻐하셨겠어요",
          "speaker_id": "1",
          "time": "20210905 03:11"
        },
        {
          "id": "MDRW2100031858.1.6",
          "form": "대박....",
          "original_form": "대박....",
          "speaker_id": "1",
          "time": "20210905 03:11"
        },
        {
          "id": "MDRW2100031858.1.7",
          "form": "모범상도받았었어요!",
          "original_form": "모범상도받았었어요!",
          "speaker_id": "2",
          "time": "20210905 03:11"
        },
        {
          "id": "MDRW2100031858.1.8",
          "form": "아니 누가 보면",
          "original_form": "아니 누가 보면",
          "speaker_id": "1",
          "time": "20210905 03:11"
        },
        {
          "id": "MDRW2100031858.1.9",
          "form": "esfj",
          "original_form": "esfj",
          "speaker_id": "1",
          "time": "20210905 03:11"
        },
        {
          "id": "MDRW2100031858.1.10",
          "form": "같은데요",
          "original_form": "같은데요",
          "speaker_id": "1",
          "time": "20210905 03:11"
        },
        {
          "id": "MDRW2100031858.1.11",
          "form": "와중에 도서관도맨날가서 다독상도받았어요",
          "original_form": "와중에 도서관도맨날가서 다독상도받았어요",
          "speaker_id": "2",
          "time": "20210905 03:11"
        },
        {
          "id": "MDRW2100031858.1.12",
          "form": "대박인데요?!?!?!?!",
          "original_form": "대박인데요?!?!?!?!",
          "speaker_id": "1",
          "time": "20210905 03:12"
        },
        {
          "id": "MDRW2100031858.1.13",
          "form": "아니 왜이렇게 성실하셨어요?",
          "original_form": "아니 왜이렇게 성실하셨어요?",
          "speaker_id": "1",
          "time": "20210905 03:12"
        },
        {
          "id": "MDRW2100031858.1.14",
          "form": "헉뭔가 그때는 학교가 나와바리 그런인식이있어서",
          "original_form": "헉뭔가 그때는 학교가 나와바리 그런인식이있어서",
          "speaker_id": "2",
          "time": "20210905 03:12"
        },
        {
          "id": "MDRW2100031858.1.15",
          "form": "네네네네",
          "original_form": "네네네네",
          "speaker_id": "1",
          "time": "20210905 03:12"
        },
        {
          "id": "MDRW2100031858.1.16",
          "form": "근데공부는안했네요",
          "original_form": "근데공부는안했네요",
          "speaker_id": "2",
          "time": "20210905 03:12"
        },
        {
          "id": "MDRW2100031858.1.17",
          "form": "아낰ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아낰ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 03:12"
        },
        {
          "id": "MDRW2100031858.1.18",
          "form": "아니그래도",
          "original_form": "아니그래도",
          "speaker_id": "1",
          "time": "20210905 03:12"
        },
        {
          "id": "MDRW2100031858.1.19",
          "form": "그러면 수시 같은걸로",
          "original_form": "그러면 수시 같은걸로",
          "speaker_id": "1",
          "time": "20210905 03:12"
        },
        {
          "id": "MDRW2100031858.1.20",
          "form": "국어...를 제일못해버렸어요...",
          "original_form": "국어...를 제일못해버렸어요...",
          "speaker_id": "2",
          "time": "20210905 03:12"
        },
        {
          "id": "MDRW2100031858.1.21",
          "form": "선생님들이 잘 해주시지 않았을까...",
          "original_form": "선생님들이 잘 해주시지 않았을까...",
          "speaker_id": "1",
          "time": "20210905 03:12"
        },
        {
          "id": "MDRW2100031858.1.22",
          "form": "아하!!!!",
          "original_form": "아하!!!!",
          "speaker_id": "1",
          "time": "20210905 03:12"
        },
        {
          "id": "MDRW2100031858.1.23",
          "form": "그래도 이과인이니까",
          "original_form": "그래도 이과인이니까",
          "speaker_id": "1",
          "time": "20210905 03:12"
        },
        {
          "id": "MDRW2100031858.1.24",
          "form": "괜찮지 않았나요 (?)",
          "original_form": "괜찮지 않았나요 (?)",
          "speaker_id": "1",
          "time": "20210905 03:12"
        },
        {
          "id": "MDRW2100031858.1.25",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 03:13"
        },
        {
          "id": "MDRW2100031858.1.26",
          "form": "저는 수리가형을 못하니까 진짜...",
          "original_form": "저는 수리가형을 못하니까 진짜...",
          "speaker_id": "1",
          "time": "20210905 03:13"
        },
        {
          "id": "MDRW2100031858.1.27",
          "form": "다른 과목들이 멱살끌고",
          "original_form": "다른 과목들이 멱살끌고",
          "speaker_id": "1",
          "time": "20210905 03:13"
        },
        {
          "id": "MDRW2100031858.1.28",
          "form": "대학보냈어욬ㅋㅋㅋㅋ",
          "original_form": "대학보냈어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 03:13"
        },
        {
          "id": "MDRW2100031858.1.29",
          "form": "ㅠㅠㅠ 저도 과학만잘했었던..",
          "original_form": "ㅠㅠㅠ 저도 과학만잘했었던..",
          "speaker_id": "2",
          "time": "20210905 03:13"
        },
        {
          "id": "MDRW2100031858.1.30",
          "form": "근데진짜 하나만 잘해도 되는데ㅠ 하",
          "original_form": "근데진짜 하나만 잘해도 되는데ㅠ 하",
