      "utterance": [
        {
          "id": "MDRW2100022596.1.1",
          "form": "배달음식 최애가뭐에요?",
          "original_form": "배달음식 최애가뭐에요?",
          "speaker_id": "2",
          "time": "20210829 16:09"
        },
        {
          "id": "MDRW2100022596.1.2",
          "form": "전자레인지로 하는 요리 중에",
          "original_form": "전자레인지로 하는 요리 중에",
          "speaker_id": "1",
          "time": "20210829 16:09"
        },
        {
          "id": "MDRW2100022596.1.3",
          "form": "할 줄 아는 거 있어요?",
          "original_form": "할 줄 아는 거 있어요?",
          "speaker_id": "1",
          "time": "20210829 16:09"
        },
        {
          "id": "MDRW2100022596.1.4",
          "form": "저 그거요 뭐더라 맵피탕?",
          "original_form": "저 그거요 뭐더라 맵피탕?",
          "speaker_id": "1",
          "time": "20210829 16:09"
        },
        {
          "id": "MDRW2100022596.1.5",
          "form": "김피탕!!",
          "original_form": "김피탕!!",
          "speaker_id": "1",
          "time": "20210829 16:09"
        },
        {
          "id": "MDRW2100022596.1.6",
          "form": "아 김피탕!! 그거 진짜 궁근해요",
          "original_form": "아 김피탕!! 그거 진짜 궁근해요",
          "speaker_id": "2",
          "time": "20210829 16:09"
        },
        {
          "id": "MDRW2100022596.1.7",
          "form": "김피탕 안먹어봤어요?",
          "original_form": "김피탕 안먹어봤어요?",
          "speaker_id": "1",
          "time": "20210829 16:10"
        },
        {
          "id": "MDRW2100022596.1.8",
          "form": "생김새는 그냥 음식물쓰레기에요 ㅋㅋㅋ",
          "original_form": "생김새는 그냥 음식물쓰레기에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:10"
        },
        {
          "id": "MDRW2100022596.1.9",
          "form": "네네 ㅠㅠ잘 안팔더라구요",
          "original_form": "네네 ㅠㅠ잘 안팔더라구요",
          "speaker_id": "2",
          "time": "20210829 16:10"
        },
        {
          "id": "MDRW2100022596.1.10",
          "form": "근데 맛은 진짜 맛있어요",
          "original_form": "근데 맛은 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210829 16:10"
        },
        {
          "id": "MDRW2100022596.1.11",
          "form": "앜ㅋㅋㅋㅋㅋ비쥬얼은별로군요",
          "original_form": "앜ㅋㅋㅋㅋㅋ비쥬얼은별로군요",
          "speaker_id": "2",
          "time": "20210829 16:10"
        },
        {
          "id": "MDRW2100022596.1.12",
          "form": "저 인생에서 그런 맛 처음 먹어봐요",
          "original_form": "저 인생에서 그런 맛 처음 먹어봐요",
          "speaker_id": "1",
          "time": "20210829 16:10"
        },
        {
          "id": "MDRW2100022596.1.13",
          "form": "그게 수원인가? 거기서 첨나온거아닌가요?",
          "original_form": "그게 수원인가? 거기서 첨나온거아닌가요?",
          "speaker_id": "2",
          "time": "20210829 16:10"
        },
        {
          "id": "MDRW2100022596.1.14",
          "form": "잘 모르겠어요 ㅜㅜ 그래요?",
          "original_form": "잘 모르겠어요 ㅜㅜ 그래요?",
          "speaker_id": "1",
          "time": "20210829 16:10"
        },
        {
          "id": "MDRW2100022596.1.15",
          "form": "누가 생각해낸 조합인지 모르는데 진짜 맛있어서..감동",
          "original_form": "누가 생각해낸 조합인지 모르는데 진짜 맛있어서..감동",
          "speaker_id": "1",
          "time": "20210829 16:10"
        },
        {
          "id": "MDRW2100022596.1.16",
          "form": "단점은 두고두고 못먹어요 ^.^ 보관 불가 ㅋㅋㅋ",
          "original_form": "단점은 두고두고 못먹어요 ^.^ 보관 불가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:11"
        },
        {
          "id": "MDRW2100022596.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아그래요 왜요??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아그래요 왜요??",
          "speaker_id": "2",
          "time": "20210829 16:11"
        },
        {
          "id": "MDRW2100022596.1.18",
          "form": "하루만 지나도 탕수육이 딱딱해져요",
          "original_form": "하루만 지나도 탕수육이 딱딱해져요",
          "speaker_id": "1",
          "time": "20210829 16:11"
        },
        {
          "id": "MDRW2100022596.1.19",
          "form": "치즈도 막 굳고 그래서 데워도 딱딱하더라구요",
          "original_form": "치즈도 막 굳고 그래서 데워도 딱딱하더라구요",
          "speaker_id": "1",
          "time": "20210829 16:11"
        },
        {
          "id": "MDRW2100022596.1.20",
          "form": "아 그럼 난감하네요 ㅠㅠ",
          "original_form": "아 그럼 난감하네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100022596.1.21",
          "form": "그래서 혼자 살땐 그거 시켜먹어본 적 없어요",
          "original_form": "그래서 혼자 살땐 그거 시켜먹어본 적 없어요",
          "speaker_id": "1",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100022596.1.22",
          "form": "그낭시켜서바로먹어야겟다",
          "original_form": "그낭시켜서바로먹어야겟다",
          "speaker_id": "2",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100022596.1.23",
          "form": "친구들의 도움이 필요해요 ㅋㅋㅋㅋ",
          "original_form": "친구들의 도움이 필요해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100022596.1.24",
          "form": "아 그렇겟네요 ㅠㅠ",
          "original_form": "아 그렇겟네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100022596.1.25",
          "form": "김피탕 하나 시키고 술 먹으면 딱이에요",
          "original_form": "김피탕 하나 시키고 술 먹으면 딱이에요",
          "speaker_id": "1",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100022596.1.26",
          "form": "ㅋㅋㅋㅋㅋ술은 잘드세요?",
          "original_form": "ㅋㅋㅋㅋㅋ술은 잘드세요?",
