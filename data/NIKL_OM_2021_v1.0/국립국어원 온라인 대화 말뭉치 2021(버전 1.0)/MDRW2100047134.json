      "utterance": [
        {
          "id": "MDRW2100047134.1.1",
          "form": "짜장짬뽕중엔 짬뽕...",
          "original_form": "짜장짬뽕중엔 짬뽕...",
          "speaker_id": "2",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100047134.1.2",
          "form": "와우 찢었다",
          "original_form": "와우 찢었다",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100047134.1.3",
          "form": "하지만 탕수육이 제일 좋습니다",
          "original_form": "하지만 탕수육이 제일 좋습니다",
          "speaker_id": "2",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100047134.1.4",
          "form": "중식은 탕수육이죠",
          "original_form": "중식은 탕수육이죠",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100047134.1.5",
          "form": "그럼 또 뻔한질문 해보죠",
          "original_form": "그럼 또 뻔한질문 해보죠",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100047134.1.6",
          "form": "예상이 가죠?",
          "original_form": "예상이 가죠?",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100047134.1.7",
          "form": "나는 찍먹 부먹",
          "original_form": "나는 찍먹 부먹",
          "speaker_id": "1",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100047134.1.8",
          "form": "찍먹파입니다",
          "original_form": "찍먹파입니다",
          "speaker_id": "2",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100047134.1.9",
          "form": "근데 부먹도 뭐 가리진 않아요",
          "original_form": "근데 부먹도 뭐 가리진 않아요",
          "speaker_id": "2",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100047134.1.10",
          "form": "막먹파...",
          "original_form": "막먹파...",
          "speaker_id": "2",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100047134.1.11",
          "form": "와우 저도막먹입니다 우리 멀어질 일이 없어요",
          "original_form": "와우 저도막먹입니다 우리 멀어질 일이 없어요",
          "speaker_id": "1",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100047134.1.12",
          "form": "다시 가까워집시다",
          "original_form": "다시 가까워집시다",
          "speaker_id": "1",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100047134.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100047134.1.14",
          "form": "맛잘알은 가리지않아요",
          "original_form": "맛잘알은 가리지않아요",
          "speaker_id": "1",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100047134.1.15",
          "form": "소스따위 그냥 막 먹는다구요",
          "original_form": "소스따위 그냥 막 먹는다구요",
          "speaker_id": "1",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100047134.1.16",
          "form": "맞아요 어차피 듬뿍 묻혀먹을 거...",
          "original_form": "맞아요 어차피 듬뿍 묻혀먹을 거...",
          "speaker_id": "2",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100047134.1.17",
          "form": "김피탕 드셔보셨어요??",
          "original_form": "김피탕 드셔보셨어요??",
          "speaker_id": "2",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100047134.1.18",
