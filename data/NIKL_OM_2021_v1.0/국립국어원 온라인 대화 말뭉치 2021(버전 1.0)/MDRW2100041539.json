      "utterance": [
        {
          "id": "MDRW2100041539.1.1",
          "form": "배고파요 ㅠㅠ",
          "original_form": "배고파요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 09:54"
        },
        {
          "id": "MDRW2100041539.1.2",
          "form": "저두 진짜 배고파요ㅠㅠ 꼬르륵",
          "original_form": "저두 진짜 배고파요ㅠㅠ 꼬르륵",
          "speaker_id": "1",
          "time": "20210906 10:17"
        },
        {
          "id": "MDRW2100041539.1.3",
          "form": "점심머드시나요",
          "original_form": "점심머드시나요",
          "speaker_id": "2",
          "time": "20210906 10:22"
        },
        {
          "id": "MDRW2100041539.1.4",
          "form": "저는 도시락 싸와서 오므라이스요~",
          "original_form": "저는 도시락 싸와서 오므라이스요~",
          "speaker_id": "1",
          "time": "20210906 10:31"
        },
        {
          "id": "MDRW2100041539.1.5",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210906 10:31"
        },
        {
          "id": "MDRW2100041539.1.6",
          "form": "저도 도시락김치볶음밥",
          "original_form": "저도 도시락김치볶음밥",
          "speaker_id": "2",
          "time": "20210906 10:49"
        },
        {
          "id": "MDRW2100041539.1.7",
          "form": "옼ㅋㅋ 저도 내일은 김볶밥 싸려고요",
          "original_form": "옼ㅋㅋ 저도 내일은 김볶밥 싸려고요",
          "speaker_id": "1",
          "time": "20210906 11:03"
        },
        {
          "id": "MDRW2100041539.1.8",
          "form": "배뷰르게 먹었어용",
          "original_form": "배뷰르게 먹었어용",
          "speaker_id": "2",
          "time": "20210906 11:24"
        },
        {
          "id": "MDRW2100041539.1.9",
          "form": "벌써드셨어요?",
          "original_form": "벌써드셨어요?",
          "speaker_id": "1",
          "time": "20210906 11:26"
        },
        {
          "id": "MDRW2100041539.1.10",
          "form": "저희는 점심시감이 안정해져서",
          "original_form": "저희는 점심시감이 안정해져서",
          "speaker_id": "2",
          "time": "20210906 11:27"
        },
        {
          "id": "MDRW2100041539.1.11",
          "form": "손님없을때 먹어야해욬ㅋㅋ",
          "original_form": "손님없을때 먹어야해욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:27"
        },
        {
          "id": "MDRW2100041539.1.12",
          "form": "앜ㅋㅋㅋ 하긴 점심먹고나면 바쁠시간이니..",
          "original_form": "앜ㅋㅋㅋ 하긴 점심먹고나면 바쁠시간이니..",
          "speaker_id": "1",
          "time": "20210906 11:47"
        },
        {
          "id": "MDRW2100041539.1.13",
          "form": "너무 바빠요 ㅠㅠ",
          "original_form": "너무 바빠요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100041539.1.14",
          "form": "헉 맞아요 진짜 한번에몰리죠 ㅠㅠ",
          "original_form": "헉 맞아요 진짜 한번에몰리죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 13:01"
        },
        {
          "id": "MDRW2100041539.1.15",
          "form": "저도 한잔 부탁드립니더",
          "original_form": "저도 한잔 부탁드립니더",
          "speaker_id": "1",
          "time": "20210906 13:02"
        },
        {
          "id": "MDRW2100041539.1.16",
          "form": "아아로 드릴까요?",
          "original_form": "아아로 드릴까요?",
          "speaker_id": "2",
          "time": "20210906 13:02"
        },
        {
          "id": "MDRW2100041539.1.17",
          "form": "아아에 헤이즐넛 시럽 추가 되나요?",
          "original_form": "아아에 헤이즐넛 시럽 추가 되나요?",
          "speaker_id": "1",
          "time": "20210906 13:03"
        },
        {
          "id": "MDRW2100041539.1.18",
          "form": "어 당연하죠",
          "original_form": "어 당연하죠",
          "speaker_id": "2",
          "time": "20210906 13:19"
        },
        {
          "id": "MDRW2100041539.1.19",
          "form": "헤이즐넛아메리카노 좋아하시는군요",
          "original_form": "헤이즐넛아메리카노 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210906 13:19"
        },
        {
          "id": "MDRW2100041539.1.20",
          "form": "근데 헤이즐넛은 라떼랑먹우면 엄청달더라구요 ㅠㅠ",
          "original_form": "근데 헤이즐넛은 라떼랑먹우면 엄청달더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 13:19"
        },
        {
          "id": "MDRW2100041539.1.21",
          "form": "헉 그래요?? 그게 단거였어요..?",
          "original_form": "헉 그래요?? 그게 단거였어요..?",
          "speaker_id": "1",
          "time": "20210906 13:36"
        },
        {
          "id": "MDRW2100041539.1.22",
          "form": "시럽3번이면달더라구오..",
          "original_form": "시럽3번이면달더라구오..",
          "speaker_id": "2",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100041539.1.23",
          "form": "단지도몰랐어욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "단지도몰랐어욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100041539.1.24",
          "form": "향만첨가인줄ㅋㅋㅋㅋㅋ",
          "original_form": "향만첨가인줄ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100041539.1.25",
          "form": "아진짜요??",
          "original_form": "아진짜요??",
          "speaker_id": "2",
          "time": "20210906 14:14"
        },
        {
          "id": "MDRW2100041539.1.26",
          "form": "시럽인지라 좀달더라구오..",
          "original_form": "시럽인지라 좀달더라구오..",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100041539.1.27",
          "form": "향첨가는 아마",
          "original_form": "향첨가는 아마",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100041539.1.28",
          "form": "헤이즐넛맛나는 원두를 써서 그런걸수도있어요!!",
          "original_form": "헤이즐넛맛나는 원두를 써서 그런걸수도있어요!!",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100041539.1.29",
          "form": "오 완전 전문가~~ 저는 그냥 겉보기로 한거라ㅋㅋ 전문적이지는 못한데",
          "original_form": "오 완전 전문가~~ 저는 그냥 겉보기로 한거라ㅋㅋ 전문적이지는 못한데",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041539.1.30",
          "form": "에잉 전문적이고 아니고 어디있나요오!",
          "original_form": "에잉 전문적이고 아니고 어디있나요오!",
          "speaker_id": "2",
          "time": "20210906 15:18"
        },
        {
          "id": "MDRW2100041539.1.31",
          "form": "배우려는 열정이없고 그냥 시간 떼우기라서 그런가봐욬ㅋㅋㅋㅋ",
