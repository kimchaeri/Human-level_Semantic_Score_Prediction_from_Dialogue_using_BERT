        {
          "id": "MDRW2100040245.1.1",
          "form": "네 아주 많아요",
          "original_form": "네 아주 많아요",
          "speaker_id": "2",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100040245.1.2",
          "form": "가디건 자켓 점퍼",
          "original_form": "가디건 자켓 점퍼",
          "speaker_id": "2",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100040245.1.3",
          "form": "가을옷이요",
          "original_form": "가을옷이요",
          "speaker_id": "2",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100040245.1.4",
          "form": "아 사셔야죠??",
          "original_form": "아 사셔야죠??",
          "speaker_id": "1",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100040245.1.5",
          "form": "네 살거에요 그래서 지금 포인트도 꾸준히 모으고있어요",
          "original_form": "네 살거에요 그래서 지금 포인트도 꾸준히 모으고있어요",
          "speaker_id": "2",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100040245.1.6",
          "form": "오...얼마필요하세요",
          "original_form": "오...얼마필요하세요",
          "speaker_id": "1",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100040245.1.7",
          "form": "사고싶은거 다 사려면요??",
          "original_form": "사고싶은거 다 사려면요??",
          "speaker_id": "2",
          "time": "20210904 15:03"
        },
        {
          "id": "MDRW2100040245.1.8",
          "form": "필수템들요",
          "original_form": "필수템들요",
          "speaker_id": "1",
          "time": "20210904 15:03"
        },
        {
          "id": "MDRW2100040245.1.9",
          "form": "무한대로요",
          "original_form": "무한대로요",
          "speaker_id": "2",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040245.1.10",
          "form": "많을수록 좋죠",
          "original_form": "많을수록 좋죠",
          "speaker_id": "2",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040245.1.11",
          "form": "백마넌 어때요",
          "original_form": "백마넌 어때요",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040245.1.12",
          "form": "백만원 저 주시게요??",
          "original_form": "백만원 저 주시게요??",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040245.1.13",
          "form": "아니요 상플이요",
          "original_form": "아니요 상플이요",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040245.1.14",
          "form": "상플이면 통 좀 더 크게 쏘시죠",
          "original_form": "상플이면 통 좀 더 크게 쏘시죠",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040245.1.15",
          "form": "500? ㅎ",
          "original_form": "500? ㅎ",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040245.1.16",
          "form": "리스트업 해보세요",
          "original_form": "리스트업 해보세요",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040245.1.17",
          "form": "아 상플이면 억대로 좀 써주세요",
          "original_form": "아 상플이면 억대로 좀 써주세요",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
