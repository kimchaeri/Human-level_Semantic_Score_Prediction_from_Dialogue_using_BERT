      "utterance": [
        {
          "id": "MDRW2100014822.1.1",
          "form": "혹시남친있어요?",
          "original_form": "혹시남친있어요?",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014822.1.2",
          "form": "저는 남편이있어요ㅋㅋㅋ",
          "original_form": "저는 남편이있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014822.1.3",
          "form": "참고로 전여잡니다",
          "original_form": "참고로 전여잡니다",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014822.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014822.1.5",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014822.1.6",
          "form": "부럽",
          "original_form": "부럽",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014822.1.7",
          "form": "앜ㅋㅋㄱ처음에 name1님이셔서",
          "original_form": "앜ㅋㅋㄱ처음에 &name1&님이셔서",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014822.1.8",
          "form": "남자분인가했는데",
          "original_form": "남자분인가했는데",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014822.1.9",
          "form": "전 결혼 적령기라서",
          "original_form": "전 결혼 적령기라서",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014822.1.10",
          "form": "티파니에서 여자분이실거같았어요!!",
          "original_form": "티파니에서 여자분이실거같았어요!!",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014822.1.11",
          "form": "이제 결혼해야되는데",
          "original_form": "이제 결혼해야되는데",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014822.1.12",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014822.1.13",
          "form": "고민이에요",
          "original_form": "고민이에요",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014822.1.14",
          "form": "이남자가 맞을지",
          "original_form": "이남자가 맞을지",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014822.1.15",
          "form": "아아아ㅜㅜ",
          "original_form": "아아아ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014822.1.16",
          "form": "고민되는부분이",
          "original_form": "고민되는부분이",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014822.1.17",
          "form": "남자분에 초점이 맞춰져있나요",
          "original_form": "남자분에 초점이 맞춰져있나요",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014822.1.18",
          "form": "남자가 착하고 무난한데 그냥 그게다에요",
          "original_form": "남자가 착하고 무난한데 그냥 그게다에요",
          "speaker_id": "1",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014822.1.19",
          "form": "뭔가 심심이같아",
          "original_form": "뭔가 심심이같아",
          "speaker_id": "1",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014822.1.20",
          "form": "지금 내가 결혼을 해야할까에 맞춰져있나요",
          "original_form": "지금 내가 결혼을 해야할까에 맞춰져있나요",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014822.1.21",
          "form": "아아ㅜㅜ그래도일단ㅇ",
          "original_form": "아아ㅜㅜ그래도일단ㅇ",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014822.1.22",
          "form": "음 지금결혼이 맞는거같아요",
          "original_form": "음 지금결혼이 맞는거같아요",
          "speaker_id": "1",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014822.1.23",
          "form": "남들다가니까",
          "original_form": "남들다가니까",
          "speaker_id": "1",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014822.1.24",
          "form": "여자문제 술문제이런거 없어서 다행이에요",
          "original_form": "여자문제 술문제이런거 없어서 다행이에요",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014822.1.25",
          "form": "나두 가야되구",
          "original_form": "나두 가야되구",
          "speaker_id": "1",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014822.1.26",
          "form": "그런건없는데.. 몬가 버라이어티하고 운명적인사랑이런거ㄹ원하나봐요",
          "original_form": "그런건없는데.. 몬가 버라이어티하고 운명적인사랑이런거ㄹ원하나봐요",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014822.1.27",
          "form": "욕심이죠",
          "original_form": "욕심이죠",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014822.1.28",
          "form": "자금 남편분과 결혼할때이사람이다! 라는 확신이있었나요",
          "original_form": "자금 남편분과 결혼할때이사람이다! 라는 확신이있었나요",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014822.1.29",
          "form": "ㅜㅜㅜㅜ길게썼는데 나쁜말은 보낼수가 없다고 뜨네요ㅋㅋㅋ",
          "original_form": "ㅜㅜㅜㅜ길게썼는데 나쁜말은 보낼수가 없다고 뜨네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014822.1.30",
          "form": "어휴심심이..",
          "original_form": "어휴심심이..",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014822.1.31",
          "form": "아니 어떤말을썼길래 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니 어떤말을썼길래 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014822.1.32",
          "form": "ㅋㅋㅋㅋㅋ어디서 걸린건지도 모르겠어요ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ어디서 걸린건지도 모르겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014822.1.33",
          "form": "저는 일단 저를 좋아해줬고,여자문제 술문제가 없고,당장 결혼후 주거할공간이 해결됐고",
          "original_form": "저는 일단 저를 좋아해줬고,여자문제 술문제가 없고,당장 결혼후 주거할공간이 해결됐고",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014822.1.34",
          "form": "오 완전 1등신랑감인데요",
          "original_form": "오 완전 1등신랑감인데요",
          "speaker_id": "1",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014822.1.35",
          "form": "나도 이쯤 결혼해도 되겠다싶을때 남편이니까 괜찮겠다 생각이들어서 했는데",
          "original_form": "나도 이쯤 결혼해도 되겠다싶을때 남편이니까 괜찮겠다 생각이들어서 했는데",
          "speaker_id": "2",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014822.1.36",
          "form": "앗 감사합니닼ㅋㅋㅋ",
          "original_form": "앗 감사합니닼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014822.1.37",
          "form": "제가 이렇게 하나씩 다쓴이유가",
          "original_form": "제가 이렇게 하나씩 다쓴이유가",
          "speaker_id": "2",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014822.1.38",
          "form": "사실 부자거나 엄청로맨틱한 이벤트를 해줬거나이런게 없이",
          "original_form": "사실 부자거나 엄청로맨틱한 이벤트를 해줬거나이런게 없이",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014822.1.39",
          "form": "어떻게보면 평범한것들이지만 내가 앞으로 살면서 나랑 문제없이 잘 지낼수있겠다싶고",
          "original_form": "어떻게보면 평범한것들이지만 내가 앞으로 살면서 나랑 문제없이 잘 지낼수있겠다싶고",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014822.1.40",
          "form": "원래 평범한게 젤 어려운법이거든요ㅠ",
          "original_form": "원래 평범한게 젤 어려운법이거든요ㅠ",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014822.1.41",
          "form": "남편이나 저한테 살면서 문제가생겨도 서로붙잡아줄수있겠다 싶어서 결심했어요",
          "original_form": "남편이나 저한테 살면서 문제가생겨도 서로붙잡아줄수있겠다 싶어서 결심했어요",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014822.1.42",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014822.1.43",
          "form": "굉장히 서로에게 신뢰가있었네요",
          "original_form": "굉장히 서로에게 신뢰가있었네요",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014822.1.44",
          "form": "저는 6년정도 사귀다결혼해서 그런거같은데",
          "original_form": "저는 6년정도 사귀다결혼해서 그런거같은데",
          "speaker_id": "2",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014822.1.45",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014822.1.46",
          "form": "남친분도",
          "original_form": "남친분도",
          "speaker_id": "2",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014822.1.47",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014822.1.48",
          "form": "전 장수커플을 항상 부러워했어요",
          "original_form": "전 장수커플을 항상 부러워했어요",
          "speaker_id": "1",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014822.1.49",
          "form": "화려한 이벤트나 프로포즈 해주시기전이더라도 평소모습은 name1님이 젤 잘아실거같아요",
          "original_form": "화려한 이벤트나 프로포즈 해주시기전이더라도 평소모습은 &name1&님이 젤 잘아실거같아요",
          "speaker_id": "2",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014822.1.50",
          "form": "긴 연애를 하면 서로질릴수도있는데 결혼까지한다는건 진짜 사랑한다고생각해서",
          "original_form": "긴 연애를 하면 서로질릴수도있는데 결혼까지한다는건 진짜 사랑한다고생각해서",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014822.1.51",
          "form": "자상한데 표현이 서툴다거나",
          "original_form": "자상한데 표현이 서툴다거나",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014822.1.52",
          "form": "ㅜㅜㅜㅜ반대로 긴연애를 하다가 헤어지면",
          "original_form": "ㅜㅜㅜㅜ반대로 긴연애를 하다가 헤어지면",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014822.1.53",
          "form": "그 후 몇달만난 사람하고 결혼하는일도 흔하니까",
          "original_form": "그 후 몇달만난 사람하고 결혼하는일도 흔하니까",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014822.1.54",
          "form": "하긴 그렇죠ㅠㅠ",
          "original_form": "하긴 그렇죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014822.1.55",
          "form": "장수커플이 아니더라도",
          "original_form": "장수커플이 아니더라도",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014822.1.56",
          "form": "결혼 후 장수부부 하실수있을거에요!",
          "original_form": "결혼 후 장수부부 하실수있을거에요!",
