      "utterance": [
        {
          "id": "MDRW2100027953.1.1",
          "form": "저는 저리 다녀요",
          "original_form": "저는 저리 다녀요",
          "speaker_id": "2",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100027953.1.2",
          "form": "저는 불교예요",
          "original_form": "저는 불교예요",
          "speaker_id": "2",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100027953.1.3",
          "form": "저는 불교예요",
          "original_form": "저는 불교예요",
          "speaker_id": "1",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100027953.1.4",
          "form": "그렇군요 저는 기독교에요",
          "original_form": "그렇군요 저는 기독교에요",
          "speaker_id": "1",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100027953.1.5",
          "form": "매주 교회를 가시나요",
          "original_form": "매주 교회를 가시나요",
          "speaker_id": "2",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100027953.1.6",
          "form": "아니요 안 간지 제법 되었어요",
          "original_form": "아니요 안 간지 제법 되었어요",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100027953.1.7",
          "form": "왜 안 가세요",
          "original_form": "왜 안 가세요",
          "speaker_id": "2",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100027953.1.8",
          "form": "바쁘게 살다 보니 일요일은 쉬고 싶었어요",
          "original_form": "바쁘게 살다 보니 일요일은 쉬고 싶었어요",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100027953.1.9",
          "form": "교회는 일요일뿐만 아니라 수요일도 가더라고요",
          "original_form": "교회는 일요일뿐만 아니라 수요일도 가더라고요",
          "speaker_id": "2",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100027953.1.10",
          "form": "교회는 행사가 좀 많아요",
          "original_form": "교회는 행사가 좀 많아요",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100027953.1.11",
          "form": "헌금도 많이 내야 되고요",
          "original_form": "헌금도 많이 내야 되고요",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100027953.1.12",
          "form": "불교는 좀 편하게 다녀요",
          "original_form": "불교는 좀 편하게 다녀요",
          "speaker_id": "2",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100027953.1.13",
          "form": "석가탄신일은 절에 가나요",
          "original_form": "석가탄신일은 절에 가나요",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100027953.1.14",
          "form": "그 정도에만 가거나 기도드리고 싶을 때 가요",
          "original_form": "그 정도에만 가거나 기도드리고 싶을 때 가요",
          "speaker_id": "2",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100027953.1.15",
          "form": "교회도 조금 더 자유로워지면 좋겠어요",
          "original_form": "교회도 조금 더 자유로워지면 좋겠어요",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100027953.1.16",
          "form": "아무래도 행사 같은게 많으니까 어쩔 수 없죠",
          "original_form": "아무래도 행사 같은게 많으니까 어쩔 수 없죠",
