      "utterance": [
        {
          "id": "MDRW2100026510.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026510.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026510.1.3",
          "form": "날이 선선하고 좋네요",
          "original_form": "날이 선선하고 좋네요",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026510.1.4",
          "form": "그러게요! 특히 밤에 진짜 선선해서 좋은 거 같아요",
          "original_form": "그러게요! 특히 밤에 진짜 선선해서 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026510.1.5",
          "form": "딱 가을 느낌!!",
          "original_form": "딱 가을 느낌!!",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026510.1.6",
          "form": "맞아요 방금 나갔다 왔는데 다리도 안 시리더라구요",
          "original_form": "맞아요 방금 나갔다 왔는데 다리도 안 시리더라구요",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026510.1.7",
          "form": "겉옷도 입고 ㅋㅋ",
          "original_form": "겉옷도 입고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026510.1.8",
          "form": "앗 나갔다 오셨구나!",
          "original_form": "앗 나갔다 오셨구나!",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026510.1.9",
          "form": "이제 슬슬 긴팔 입어야겠더라구용!",
          "original_form": "이제 슬슬 긴팔 입어야겠더라구용!",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026510.1.10",
          "form": "가을 옷도 사야겠어요...",
          "original_form": "가을 옷도 사야겠어요...",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026510.1.11",
          "form": "네 담배 조금 피우기 딱 좋아요 날씨가",
          "original_form": "네 담배 조금 피우기 딱 좋아요 날씨가",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026510.1.12",
          "form": "얼마 전까진 손가락이 타들어가는 줄 알았어요...",
          "original_form": "얼마 전까진 손가락이 타들어가는 줄 알았어요...",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026510.1.13",
          "form": "으아ㅋㅋㅋㅋㅋ진짜 그랬겠네요 더운데 불까지ㅠㅠ",
          "original_form": "으아ㅋㅋㅋㅋㅋ진짜 그랬겠네요 더운데 불까지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026510.1.14",
          "form": "여름...정말 싫죠",
          "original_form": "여름...정말 싫죠",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026510.1.15",
          "form": "저는 여름에 밖을 안 나가요",
          "original_form": "저는 여름에 밖을 안 나가요",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026510.1.16",
          "form": "이제 드디어 밖을 나갈 수 있게 되어 기쁘네요",
          "original_form": "이제 드디어 밖을 나갈 수 있게 되어 기쁘네요",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026510.1.17",
          "form": "전 날이 시원해서인지 더 많이 피우게 되는 것 같아요...ㅋㅋㅋㅋ 나가고 싶은데 나가면 숨쉬듯이 피우게 돼갖고",
          "original_form": "전 날이 시원해서인지 더 많이 피우게 되는 것 같아요...ㅋㅋㅋㅋ 나가고 싶은데 나가면 숨쉬듯이 피우게 돼갖고",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026510.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ아 진짜 그렇겠어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 진짜 그렇겠어요",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026510.1.19",
          "form": "원래 바람쐬러 간다고 하시잖아요 흡연하시는 분들이ㅋㅋㅋㅋㅋ",
          "original_form": "원래 바람쐬러 간다고 하시잖아요 흡연하시는 분들이ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026510.1.20",
          "form": "근데 그래두 낮엔 역시 아직 덥네용...",
          "original_form": "근데 그래두 낮엔 역시 아직 덥네용...",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026510.1.21",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 담배 태우러 간다는 말보다 바람 쐬러 간다는 말을 해요ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 담배 태우러 간다는 말보다 바람 쐬러 간다는 말을 해요ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026510.1.22",
          "form": "맞아요... 딱 이 정도면 좋을 텐데",
          "original_form": "맞아요... 딱 이 정도면 좋을 텐데",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026510.1.23",
          "form": "이제 금방 시원해지겎죠?ㅠㅠ",
          "original_form": "이제 금방 시원해지겎죠?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026510.1.24",
          "form": "사실 전 1년 내내 이정도 온도였으면 좋겠어요ㅋㅋㅋㅋㅋ",
          "original_form": "사실 전 1년 내내 이정도 온도였으면 좋겠어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026510.1.25",
          "form": "사계절의 붕괴...",
          "original_form": "사계절의 붕괴...",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026510.1.26",
          "form": "저도.....................",
          "original_form": "저도.....................",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026510.1.27",
          "form": "4계절 솔직히 힘들어요...ㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "4계절 솔직히 힘들어요...ㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026510.1.28",
          "form": "ㅋㅋㅋㅋ큐ㅠㅠㅠ그쵸.... 계절마다 옷 꺼내는 것도 지치고...",
          "original_form": "ㅋㅋㅋㅋ큐ㅠㅠㅠ그쵸.... 계절마다 옷 꺼내는 것도 지치고...",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026510.1.29",
          "form": "이제 가을 와서 좋긴 한데...",
          "original_form": "이제 가을 와서 좋긴 한데...",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026510.1.30",
          "form": "옷정리 할 생각에",
          "original_form": "옷정리 할 생각에",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026510.1.31",
          "form": "19도까지는 반팔로 버틸 수 있지 않을까...",
          "original_form": "19도까지는 반팔로 버틸 수 있지 않을까...",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026510.1.32",
          "form": "같은 생각 했지 뭐예요...",
          "original_form": "같은 생각 했지 뭐예요...",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026510.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026510.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026510.1.35",
          "form": "아ㅜㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저는 사실 버틸 수 있었는데",
          "original_form": "아ㅜㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저는 사실 버틸 수 있었는데",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026510.1.36",
          "form": "몇 년 전부터 몸이 노쇄했나... 춥더라구요...",
          "original_form": "몇 년 전부터 몸이 노쇄했나... 춥더라구요...",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026510.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 저도",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 저도",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026510.1.38",
          "form": "무릎뼈가 시리긴 해요",
          "original_form": "무릎뼈가 시리긴 해요",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026510.1.39",
          "form": "그래서 여름에도 바지는 긴바지를 입어요...",
          "original_form": "그래서 여름에도 바지는 긴바지를 입어요...",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026510.1.40",
          "form": "나이...무섭죠",
          "original_form": "나이...무섭죠",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026510.1.41",
          "form": "세월 너무 빨라요",
          "original_form": "세월 너무 빨라요",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026510.1.42",
          "form": "무릎뼈ㅠㅠㅋㅋㅋㅋ 저는 발목이랑 어깨랑 척추랑 무릎이랑",
          "original_form": "무릎뼈ㅠㅠㅋㅋㅋㅋ 저는 발목이랑 어깨랑 척추랑 무릎이랑",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026510.1.43",
          "form": "하... 다 아프네요",
          "original_form": "하... 다 아프네요",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026510.1.44",
          "form": "살 빠지면서 더 그런 것 같아요 어릴 때 감사할걸...",
          "original_form": "살 빠지면서 더 그런 것 같아요 어릴 때 감사할걸...",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026510.1.45",
          "form": "아이고ㅠㅠㅠ관절이랑 디스크 진짜 문제예요",
          "original_form": "아이고ㅠㅠㅠ관절이랑 디스크 진짜 문제예요",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026510.1.46",
          "form": "어깨 너무 공감가네요ㅠㅠㅠ",
          "original_form": "어깨 너무 공감가네요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026510.1.47",
          "form": "헉 살 빠지셨구낭...ㅜㅜㅜㅜ",
          "original_form": "헉 살 빠지셨구낭...ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026510.1.48",
          "form": "다이어트 좋긴 해두 건강엔....ㅠㅠ 안 좋은가봐요",
          "original_form": "다이어트 좋긴 해두 건강엔....ㅠㅠ 안 좋은가봐요",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026510.1.49",
          "form": "요새 안 움직이고 그러니까 근육이 빠져서 척추가 비명을 질러요",
          "original_form": "요새 안 움직이고 그러니까 근육이 빠져서 척추가 비명을 질러요",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026510.1.50",
          "form": "그럼요 엄청 안 좋아요 다이어트 절대 굶어서 하지 마시길,,",
          "original_form": "그럼요 엄청 안 좋아요 다이어트 절대 굶어서 하지 마시길,,",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026510.1.51",
          "form": "저는 살다 보니 빠졌는데도 이래요ㅠㅠ",
          "original_form": "저는 살다 보니 빠졌는데도 이래요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026510.1.52",
          "form": "아 근육 빠지는 거 너무 공감이에요ㅋ큐ㅠㅠㅠㅠㅠ",
          "original_form": "아 근육 빠지는 거 너무 공감이에요ㅋ큐ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026510.1.53",
          "form": "진짜 근육빠지는 거 젤 끔찍해요",
          "original_form": "진짜 근육빠지는 거 젤 끔찍해요",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
