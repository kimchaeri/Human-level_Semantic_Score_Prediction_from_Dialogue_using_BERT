      "utterance": [
        {
          "id": "MDRW2100025302.1.1",
          "form": "오늘 너무 춥네여ㅠㅠㅠ",
          "original_form": "오늘 너무 춥네여ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 08:43"
        },
        {
          "id": "MDRW2100025302.1.2",
          "form": "이제 진짜 가을인가 봐요",
          "original_form": "이제 진짜 가을인가 봐요",
          "speaker_id": "2",
          "time": "20210901 08:44"
        },
        {
          "id": "MDRW2100025302.1.3",
          "form": "그러게요ㅠㅜㅜ",
          "original_form": "그러게요ㅠㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 08:47"
        },
        {
          "id": "MDRW2100025302.1.4",
          "form": "여긴 이제 좀 비가 그쳤어요ㅠ",
          "original_form": "여긴 이제 좀 비가 그쳤어요ㅠ",
          "speaker_id": "1",
          "time": "20210901 08:47"
        },
        {
          "id": "MDRW2100025302.1.5",
          "form": "여기두여ㅎㅎ",
          "original_form": "여기두여ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 09:42"
        },
        {
          "id": "MDRW2100025302.1.6",
          "form": "어젠 진짜 비가 폭포처럼 쏟아졌는데",
          "original_form": "어젠 진짜 비가 폭포처럼 쏟아졌는데",
          "speaker_id": "2",
          "time": "20210901 09:43"
        },
        {
          "id": "MDRW2100025302.1.7",
          "form": "오늘은 맑네요~",
          "original_form": "오늘은 맑네요~",
          "speaker_id": "2",
          "time": "20210901 09:43"
        },
        {
          "id": "MDRW2100025302.1.8",
          "form": "오~ 다행이에요!",
          "original_form": "오~ 다행이에요!",
          "speaker_id": "1",
          "time": "20210901 09:49"
        },
        {
          "id": "MDRW2100025302.1.9",
          "form": "여긴 아직 하늘이 흐려서 불안불안해요",
          "original_form": "여긴 아직 하늘이 흐려서 불안불안해요",
          "speaker_id": "1",
          "time": "20210901 09:49"
        },
        {
          "id": "MDRW2100025302.1.10",
          "form": "오늘 또 오면 더 추워지겟네요ㅠㅠㅠㅠ",
          "original_form": "오늘 또 오면 더 추워지겟네요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 14:37"
        },
        {
          "id": "MDRW2100025302.1.11",
          "form": "어후ㅠ 겉옷은 챙기셨어요?",
          "original_form": "어후ㅠ 겉옷은 챙기셨어요?",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100025302.1.12",
          "form": "그럼용~ㅋㅋㅋㅋ",
          "original_form": "그럼용~ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100025302.1.13",
          "form": "밖에나갈때",
          "original_form": "밖에나갈때",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100025302.1.14",
          "form": "겉옷필수예요ㅎㅎ",
          "original_form": "겉옷필수예요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100025302.1.15",
          "form": "맞아용ㅋㅋ",
          "original_form": "맞아용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 07:09"
        },
        {
          "id": "MDRW2100025302.1.16",
          "form": "안챙기면 안되요ㅠ",
          "original_form": "안챙기면 안되요ㅠ",
          "speaker_id": "1",
          "time": "20210902 07:09"
        },
        {
          "id": "MDRW2100025302.1.17",
          "form": "이제 잠옷도 긴팔 입어야 해요ㅎㅎ",
          "original_form": "이제 잠옷도 긴팔 입어야 해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100025302.1.18",
          "form": "맞아요 밤에 특히 춥더라구요",
          "original_form": "맞아요 밤에 특히 춥더라구요",
          "speaker_id": "1",
          "time": "20210902 12:08"
        },
        {
          "id": "MDRW2100025302.1.19",
          "form": "그러니깐요ㅠㅠㅠ",
          "original_form": "그러니깐요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100025302.1.20",
          "form": "특히 저희 집은 바람이 잘통하는 집이라",
          "original_form": "특히 저희 집은 바람이 잘통하는 집이라",
          "speaker_id": "2",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100025302.1.21",
          "form": "더 추워요ㅠㅠㅠㅠ",
          "original_form": "더 추워요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100025302.1.22",
          "form": "헉ㅜㅜ 문 꼭 잘 닫고 주무셔야겠어요ㅠㅜ",
          "original_form": "헉ㅜㅜ 문 꼭 잘 닫고 주무셔야겠어요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100025302.1.23",
          "form": "네넹ㅠㅠ",
          "original_form": "네넹ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100025302.1.24",
          "form": "이런 시국에 감기 걸리면 위험하니깐",
          "original_form": "이런 시국에 감기 걸리면 위험하니깐",
          "speaker_id": "2",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100025302.1.25",
