      "utterance": [
        {
          "id": "MDRW2100031407.1.1",
          "form": "천도복숭아 맛있어?",
          "original_form": "천도복숭아 맛있어?",
          "speaker_id": "2",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031407.1.2",
          "form": "천도복숭아 맛있는거 찾기힘들더라",
          "original_form": "천도복숭아 맛있는거 찾기힘들더라",
          "speaker_id": "2",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031407.1.3",
          "form": "산거는 괜춘?",
          "original_form": "산거는 괜춘?",
          "speaker_id": "2",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031407.1.4",
          "form": "맛있는건 맛나",
          "original_form": "맛있는건 맛나",
          "speaker_id": "1",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031407.1.5",
          "form": "이건 마트꺼라 모르겠어\r\n먹어봐야지ㅎ",
          "original_form": "이건 마트꺼라 모르겠어\r\n먹어봐야지ㅎ",
          "speaker_id": "1",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100031407.1.6",
          "form": "나 슬리퍼 샀다했던가",
          "original_form": "나 슬리퍼 샀다했던가",
          "speaker_id": "1",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100031407.1.7",
          "form": "새신 신고 다녀오는길ㅎ",
          "original_form": "새신 신고 다녀오는길ㅎ",
          "speaker_id": "1",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100031407.1.8",
          "form": "오 슬리퍼 ㅎㅎ",
          "original_form": "오 슬리퍼 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031407.1.9",
          "form": "이마트 가까이에 있나보네",
          "original_form": "이마트 가까이에 있나보네",
          "speaker_id": "2",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031407.1.10",
          "form": "걸어서 갔다온거보니",
          "original_form": "걸어서 갔다온거보니",
          "speaker_id": "2",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031407.1.11",
          "form": "걸어서 편도 25분정도ㅎ",
          "original_form": "걸어서 편도 25분정도ㅎ",
          "speaker_id": "1",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100031407.1.12",
          "form": "딱 다닐만한 거리",
          "original_form": "딱 다닐만한 거리",
          "speaker_id": "1",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031407.1.13",
          "form": "근데 짐 무거움 택시ㅋㅋ",
          "original_form": "근데 짐 무거움 택시ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031407.1.14",
          "form": "왕복 50분이네! 합성동 거리정도 되나보다",
          "original_form": "왕복 50분이네! 합성동 거리정도 되나보다",
          "speaker_id": "2",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031407.1.15",
          "form": "응ㅎㅎ",
          "original_form": "응ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100031407.1.16",
          "form": "편한듯 안편하네ㅋ",
          "original_form": "편한듯 안편하네ㅋ",
          "speaker_id": "1",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100031407.1.17",
          "form": "새신이라 그런가ㅎ",
          "original_form": "새신이라 그런가ㅎ",
          "speaker_id": "1",
          "time": "20210904 21:33"
        },
        {
          "id": "MDRW2100031407.1.18",
          "form": "어묵은 전자렌지 데워먹어?",
          "original_form": "어묵은 전자렌지 데워먹어?",
          "speaker_id": "2",
          "time": "20210904 21:38"
        },
        {
          "id": "MDRW2100031407.1.19",
          "form": "아님 떡볶이에 넣어 먹을라고?",
          "original_form": "아님 떡볶이에 넣어 먹을라고?",
          "speaker_id": "2",
          "time": "20210904 21:38"
        },
        {
          "id": "MDRW2100031407.1.20",
          "form": "떡뽁이",
          "original_form": "떡뽁이",
          "speaker_id": "1",
          "time": "20210904 21:40"
        },
        {
          "id": "MDRW2100031407.1.21",
          "form": "얇게 썰어 얼려둘려고ㅎ",
          "original_form": "얇게 썰어 얼려둘려고ㅎ",
          "speaker_id": "1",
          "time": "20210904 21:40"
        },
        {
          "id": "MDRW2100031407.1.22",
          "form": "인스턴트 죠스 떡볶이에 넣어 먹어ㅎ",
          "original_form": "인스턴트 죠스 떡볶이에 넣어 먹어ㅎ",
          "speaker_id": "1",
          "time": "20210904 21:41"
        },
        {
          "id": "MDRW2100031407.1.23",
          "form": "오  좋다 ㅋㅋㅋ 맛있겠옹",
          "original_form": "오  좋다 ㅋㅋㅋ 맛있겠옹",
          "speaker_id": "2",
          "time": "20210904 21:42"
        },
        {
          "id": "MDRW2100031407.1.24",
          "form": "ㅋㅋㅋ이마트 댕겨와서 땀나\r\n운동한듯ㅋㅋ",
          "original_form": "ㅋㅋㅋ이마트 댕겨와서 땀나\r\n운동한듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:43"
        },
        {
          "id": "MDRW2100031407.1.25",
          "form": "ㅇㅇ 좋은듯 걸어다니는거",
          "original_form": "ㅇㅇ 좋은듯 걸어다니는거",
          "speaker_id": "2",
          "time": "20210904 21:43"
        },
        {
          "id": "MDRW2100031407.1.26",
          "form": "오늘 지금까지 만천보 찍었네ㅋ",
          "original_form": "오늘 지금까지 만천보 찍었네ㅋ",
          "speaker_id": "1",
          "time": "20210904 21:44"
        },
        {
          "id": "MDRW2100031407.1.27",
          "form": "나 여름 겉에 걸치는거 인터넷으로 삿는데",
          "original_form": "나 여름 겉에 걸치는거 인터넷으로 삿는데",
          "speaker_id": "1",
          "time": "20210904 21:44"
        },
        {
          "id": "MDRW2100031407.1.28",
          "form": ".ㅇㅇ",
          "original_form": ".ㅇㅇ",
          "speaker_id": "2",
          "time": "20210904 21:44"
        },
        {
          "id": "MDRW2100031407.1.29",
          "form": "가디건??",
          "original_form": "가디건??",
          "speaker_id": "2",
          "time": "20210904 21:44"
        },
        {
          "id": "MDRW2100031407.1.30",
          "form": "사이즈 미스로 입을순있는데 핏이 아쉽\r\n좀 크더라",
          "original_form": "사이즈 미스로 입을순있는데 핏이 아쉽\r\n좀 크더라",
          "speaker_id": "1",
          "time": "20210904 21:44"
        },
        {
          "id": "MDRW2100031407.1.31",
          "form": "외투??",
          "original_form": "외투??",
          "speaker_id": "2",
          "time": "20210904 21:44"
        },
        {
          "id": "MDRW2100031407.1.32",
          "form": "메쉬인듯 아닌듯한 겉옷",
          "original_form": "메쉬인듯 아닌듯한 겉옷",
          "speaker_id": "1",
          "time": "20210904 21:45"
        },
        {
          "id": "MDRW2100031407.1.33",
          "form": "여름에 걸치는거",
          "original_form": "여름에 걸치는거",
          "speaker_id": "1",
          "time": "20210904 21:45"
        },
        {
          "id": "MDRW2100031407.1.34",
          "form": "교환은 하는건?",
          "original_form": "교환은 하는건?",
          "speaker_id": "2",
          "time": "20210904 21:45"
        },
        {
          "id": "MDRW2100031407.1.35",
          "form": "사이즈",
          "original_form": "사이즈",
          "speaker_id": "2",
          "time": "20210904 21:45"
        },
        {
          "id": "MDRW2100031407.1.36",
          "form": "왕복택배비 아깝ㅋ",
          "original_form": "왕복택배비 아깝ㅋ",
          "speaker_id": "1",
          "time": "20210904 21:45"
        },
        {
          "id": "MDRW2100031407.1.37",
          "form": "입을만은해\r\n나 살쪄서 안맞을지알았더니 \r\n옷이 더 크너ㅋㅋ",
          "original_form": "입을만은해\r\n나 살쪄서 안맞을지알았더니 \r\n옷이 더 크너ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:45"
        },
        {
          "id": "MDRW2100031407.1.38",
          "form": "내심 기쁨ㅋㅋㅋㅋ",
          "original_form": "내심 기쁨ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:46"
        },
        {
          "id": "MDRW2100031407.1.39",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:46"
        }
      ]
    }
