      "utterance": [
        {
          "id": "MDRW2100034684.1.1",
          "form": "넷플릭스 보시나요?",
          "original_form": "넷플릭스 보시나요?",
          "speaker_id": "1",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100034684.1.2",
          "form": "지금은 구독안하는데 예전에 한참 빠질때가있었어요",
          "original_form": "지금은 구독안하는데 예전에 한참 빠질때가있었어요",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100034684.1.3",
          "form": "name1님은 보시나요",
          "original_form": "&name1&님은 보시나요",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100034684.1.4",
          "form": "저는 dp 보려구요!",
          "original_form": "저는 dp 보려구요!",
          "speaker_id": "1",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100034684.1.5",
          "form": "드라마 좋아하시나요??",
          "original_form": "드라마 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100034684.1.6",
          "form": "네 드라마좋아해요",
          "original_form": "네 드라마좋아해요",
          "speaker_id": "2",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100034684.1.7",
          "form": "dp가 혹시 웹툰dp개의날 이게원작인건가요? 웹툰은본적있는데",
          "original_form": "dp가 혹시 웹툰dp개의날 이게원작인건가요? 웹툰은본적있는데",
          "speaker_id": "2",
          "time": "20210828 18:42"
        },
        {
          "id": "MDRW2100034684.1.8",
          "form": "그런건가봐요",
          "original_form": "그런건가봐요",
          "speaker_id": "1",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100034684.1.9",
          "form": "탈영병 잡는 얘기에요",
          "original_form": "탈영병 잡는 얘기에요",
          "speaker_id": "1",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100034684.1.10",
          "form": "요즘 보는 드라마가뭔가요",
          "original_form": "요즘 보는 드라마가뭔가요",
          "speaker_id": "1",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100034684.1.11",
          "form": "어 요즘은 드라마 하나도 안보구요",
          "original_form": "어 요즘은 드라마 하나도 안보구요",
          "speaker_id": "2",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100034684.1.12",
          "form": "예능은 스우파아세요?",
          "original_form": "예능은 스우파아세요?",
          "speaker_id": "2",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100034684.1.13",
          "form": "스트리트우먼파이트",
          "original_form": "스트리트우먼파이트",
          "speaker_id": "2",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100034684.1.14",
          "form": "스우파",
          "original_form": "스우파",
          "speaker_id": "1",
          "time": "20210828 18:46"
        },
        {
          "id": "MDRW2100034684.1.15",
          "form": "그거엄청 재밋다던데",
          "original_form": "그거엄청 재밋다던데",
          "speaker_id": "1",
          "time": "20210828 18:46"
        },
        {
          "id": "MDRW2100034684.1.16",
          "form": "네 이제1화밖에안했는데 진짜 넘재밌어요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 이제1화밖에안했는데 진짜 넘재밌어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100034684.1.17",
          "form": "보고나면 기 쪽 빨리는데 넘멋있고 재밌고 다해요",
          "original_form": "보고나면 기 쪽 빨리는데 넘멋있고 재밌고 다해요",
          "speaker_id": "2",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100034684.1.18",
          "form": "다들 기가 정말 장난아닐것같아욬ㅋㅋ",
          "original_form": "다들 기가 정말 장난아닐것같아욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100034684.1.19",
          "form": "네 근데 또 귀여운모습도있구여",
          "original_form": "네 근데 또 귀여운모습도있구여",
          "speaker_id": "2",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100034684.1.20",
          "form": "지금은 놀뭐보고있어요ㅎㅎ",
          "original_form": "지금은 놀뭐보고있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100034684.1.21",
          "form": "예능을 좋아하시나봐요!",
          "original_form": "예능을 좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100034684.1.22",
          "form": "넵 그 전 회차안봐도되니까 가볍게보기좋은것같아요",
          "original_form": "넵 그 전 회차안봐도되니까 가볍게보기좋은것같아요",
          "speaker_id": "2",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100034684.1.23",
          "form": "다시 넷플 하시게되면 디피 봐보세요!!",
