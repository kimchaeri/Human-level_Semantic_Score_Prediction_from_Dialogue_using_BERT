      "utterance": [
        {
          "id": "MDRW2100043368.1.1",
          "form": "좋아하시는 음료 있으세요??",
          "original_form": "좋아하시는 음료 있으세요??",
          "speaker_id": "1",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043368.1.2",
          "form": "저는탄산수요",
          "original_form": "저는탄산수요",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043368.1.3",
          "form": "그냥 탄산음료는 안 좋아하세요?",
          "original_form": "그냥 탄산음료는 안 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043368.1.4",
          "form": "콜라 이런거말씀하시는거죵",
          "original_form": "콜라 이런거말씀하시는거죵",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043368.1.5",
          "form": "네 ㅋㅋ 콜라 제일 좋아해요",
          "original_form": "네 ㅋㅋ 콜라 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043368.1.6",
          "form": "건강에 안 좋지만...",
          "original_form": "건강에 안 좋지만...",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043368.1.7",
          "form": "ㅎㅎㅎ저도사이다좋아해요",
          "original_form": "ㅎㅎㅎ저도사이다좋아해요",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043368.1.8",
          "form": "혹시 그 청귤 사이다 복숭아 사이다 드셔보셖어요?",
          "original_form": "혹시 그 청귤 사이다 복숭아 사이다 드셔보셖어요?",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043368.1.9",
          "form": "저번에 원쁠원 하길래 사봤는데 맛있더라구요",
          "original_form": "저번에 원쁠원 하길래 사봤는데 맛있더라구요",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043368.1.10",
          "form": "오안먹어봤는데보이면도전할게요",
          "original_form": "오안먹어봤는데보이면도전할게요",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043368.1.11",
          "form": "ㅋㅋㅋ 이프로 복숭아 탄산맛인데 전 맛있더라구요",
          "original_form": "ㅋㅋㅋ 이프로 복숭아 탄산맛인데 전 맛있더라구요",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043368.1.12",
          "form": "청귤은 약간 환타 오렌지맛 같기도 하구....",
          "original_form": "청귤은 약간 환타 오렌지맛 같기도 하구....",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043368.1.13",
          "form": "오 이프로 저도 좋아해요",
          "original_form": "오 이프로 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043368.1.14",
          "form": "커피는좋아하세요?",
          "original_form": "커피는좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043368.1.15",
          "form": "네 하루에 두 잔씯 마셔요",
          "original_form": "네 하루에 두 잔씯 마셔요",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043368.1.16",
          "form": "저는한잔으로줄였어요",
          "original_form": "저는한잔으로줄였어요",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043368.1.17",
          "form": "ㅋㅋㅋㅋ 좋아하는 거 반 강제 반이지만요",
          "original_form": "ㅋㅋㅋㅋ 좋아하는 거 반 강제 반이지만요",
          "speaker_id": "1",
          "time": "20210907 19:46"
