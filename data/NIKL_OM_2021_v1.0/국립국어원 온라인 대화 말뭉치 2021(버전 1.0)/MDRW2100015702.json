      "utterance": [
        {
          "id": "MDRW2100015702.1.1",
          "form": "너무싸서 알아보기 귀찮고 해서",
          "original_form": "너무싸서 알아보기 귀찮고 해서",
          "speaker_id": "2",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100015702.1.2",
          "form": "걍후딱날잡고 갔다왔어요",
          "original_form": "걍후딱날잡고 갔다왔어요",
          "speaker_id": "2",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100015702.1.3",
          "form": "저는 클립에서 중간중간 일정 추가해서 반 패키지 했는데 괜찮더라구요 ㅎㅎ",
          "original_form": "저는 클립에서 중간중간 일정 추가해서 반 패키지 했는데 괜찮더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015702.1.4",
          "form": "그것도 괜찮죠 보기힘든건 패키지로",
          "original_form": "그것도 괜찮죠 보기힘든건 패키지로",
          "speaker_id": "2",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015702.1.5",
          "form": "맞아요 ㅋㅋ 교통 편한곳은 구글맵으로 다니고",
          "original_form": "맞아요 ㅋㅋ 교통 편한곳은 구글맵으로 다니고",
          "speaker_id": "1",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015702.1.6",
          "form": "막 버스타고 고속도로 가는곳은 패키지로ㅎㅎ",
          "original_form": "막 버스타고 고속도로 가는곳은 패키지로ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015702.1.7",
          "form": "호주는 가서 일일투어했어요",
          "original_form": "호주는 가서 일일투어했어요",
          "speaker_id": "2",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015702.1.8",
          "form": "오 호주는 진짜 어릴때 가서",
          "original_form": "오 호주는 진짜 어릴때 가서",
          "speaker_id": "1",
          "time": "20210826 01:32"
        },
        {
          "id": "MDRW2100015702.1.9",
          "form": "기억이 가물가물해요",
          "original_form": "기억이 가물가물해요",
          "speaker_id": "1",
          "time": "20210826 01:32"
        },
        {
          "id": "MDRW2100015702.1.10",
          "form": "오 부유하신거 맞네요ㅋ",
          "original_form": "오 부유하신거 맞네요ㅋ",
          "speaker_id": "2",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015702.1.11",
          "form": "어릴때 해외여행도댕기고",
          "original_form": "어릴때 해외여행도댕기고",
          "speaker_id": "2",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015702.1.12",
          "form": "ㅋㅋ부모님이 데려가줘서 ㅋㅋ",
          "original_form": "ㅋㅋ부모님이 데려가줘서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015702.1.13",
          "form": "싸게 싸게 돌아댕겼습니다ㅋㅋㅋ",
          "original_form": "싸게 싸게 돌아댕겼습니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015702.1.14",
          "form": "또 어디가보셨어요?",
          "original_form": "또 어디가보셨어요?",
          "speaker_id": "2",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100015702.1.15",
          "form": "1년에 4번나갈정도면",
          "original_form": "1년에 4번나갈정도면",
          "speaker_id": "2",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100015702.1.16",
          "form": "웬만한곳은 다다녀왔을듯",
          "original_form": "웬만한곳은 다다녀왔을듯",
          "speaker_id": "2",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015702.1.17",
          "form": "저 아시아는 꽤 많이 돌아댕겨서ㅋㅋ",
          "original_form": "저 아시아는 꽤 많이 돌아댕겨서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015702.1.18",
          "form": "일본 중국 대만 홍콩 태국 캄보디아 말레이시아 인도네시아 싱가포르 스리랑카 호주",
          "original_form": "일본 중국 대만 홍콩 태국 캄보디아 말레이시아 인도네시아 싱가포르 스리랑카 호주",
          "speaker_id": "1",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015702.1.19",
          "form": "이정도 같은데요 ㅎㅎ",
          "original_form": "이정도 같은데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015702.1.20",
          "form": "많이 다니셨네요ㅎ",
          "original_form": "많이 다니셨네요ㅎ",
          "speaker_id": "2",
          "time": "20210826 01:38"
        },
        {
          "id": "MDRW2100015702.1.21",
          "form": "인도네시아 스리랑카빼곤",
          "original_form": "인도네시아 스리랑카빼곤",
          "speaker_id": "2",
          "time": "20210826 01:38"
        },
        {
          "id": "MDRW2100015702.1.22",
          "form": "저도 가긴했군요ㅎ",
          "original_form": "저도 가긴했군요ㅎ",
          "speaker_id": "2",
          "time": "20210826 01:38"
        },
        {
          "id": "MDRW2100015702.1.23",
          "form": "스리랑카는 전직장에서 출장 보내서 ㅎㅎ",
          "original_form": "스리랑카는 전직장에서 출장 보내서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:39"
        },
        {
          "id": "MDRW2100015702.1.24",
          "form": "뭐 그럼 name2도 부유하시네요!!ㅋㅋ",
          "original_form": "뭐 그럼 &name2&도 부유하시네요!!ㅋㅋ",
