      "utterance": [
        {
          "id": "MDRW2100004497.1.1",
          "form": "지금 비 겁나오잖아요",
          "original_form": "지금 비 겁나오잖아요",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004497.1.2",
          "form": "거기도 겁나오죠?",
          "original_form": "거기도 겁나오죠?",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004497.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004497.1.4",
          "form": "지금 방충망 열려있는데",
          "original_form": "지금 방충망 열려있는데",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004497.1.5",
          "form": "비와서 못 닫고 있어요",
          "original_form": "비와서 못 닫고 있어요",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004497.1.6",
          "form": "아니...?",
          "original_form": "아니...?",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004497.1.7",
          "form": "왜 못닫아여",
          "original_form": "왜 못닫아여",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004497.1.8",
          "form": "비맞을까봐?",
          "original_form": "비맞을까봐?",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004497.1.9",
          "form": "언젠간 닫아야지...",
          "original_form": "언젠간 닫아야지...",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004497.1.10",
          "form": "비가 안으로 들쳐서요",
          "original_form": "비가 안으로 들쳐서요",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004497.1.11",
          "form": "...?",
          "original_form": "...?",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004497.1.12",
          "form": "ㅜㅠㅠㅠㅠ",
          "original_form": "ㅜㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004497.1.13",
          "form": "어디 방충망임데녀",
          "original_form": "어디 방충망임데녀",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004497.1.14",
          "form": "창문은 닫혀있고 방충망이 열려있나?",
          "original_form": "창문은 닫혀있고 방충망이 열려있나?",
          "speaker_id": "1",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004497.1.15",
          "form": "제 방 창문에 달려있는거에요",
          "original_form": "제 방 창문에 달려있는거에요",
          "speaker_id": "2",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004497.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004497.1.17",
          "form": "어쩐지 비오는 소리가 좀 크더라니",
          "original_form": "어쩐지 비오는 소리가 좀 크더라니",
          "speaker_id": "2",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004497.1.18",
          "form": "방충망이 열려있었어요",
          "original_form": "방충망이 열려있었어요",
          "speaker_id": "2",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004497.1.19",
          "form": "빗소리",
          "original_form": "빗소리",
          "speaker_id": "1",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004497.1.20",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004497.1.21",
          "form": "대체 뭐가?? 안 좋은 말이죠??",
          "original_form": "대체 뭐가?? 안 좋은 말이죠??",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004497.1.22",
          "form": "아무튼 방에만 있으니 좋네요",
          "original_form": "아무튼 방에만 있으니 좋네요",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004497.1.23",
          "form": "빗소리를 asmr삼으며...",
          "original_form": "빗소리를 asmr삼으며...",
          "speaker_id": "2",
          "time": "20210821 16:54"
        }
      ]
