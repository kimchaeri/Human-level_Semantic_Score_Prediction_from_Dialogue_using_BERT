      "utterance": [
        {
          "id": "MDRW2100031105.1.1",
          "form": "난 삼겹살에 껍데기 붙은 게 좋아",
          "original_form": "난 삼겹살에 껍데기 붙은 게 좋아",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031105.1.2",
          "form": "넌 어때?",
          "original_form": "넌 어때?",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031105.1.3",
          "form": "나도 지방층이 두꺼운게 좋아",
          "original_form": "나도 지방층이 두꺼운게 좋아",
          "speaker_id": "1",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031105.1.4",
          "form": "콜라겐 좋지",
          "original_form": "콜라겐 좋지",
          "speaker_id": "1",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031105.1.5",
          "form": "쫀득쫀득한 게 좋아",
          "original_form": "쫀득쫀득한 게 좋아",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031105.1.6",
          "form": "난 바싹 구운 게 좋아",
          "original_form": "난 바싹 구운 게 좋아",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031105.1.7",
          "form": "너는 어때?",
          "original_form": "너는 어때?",
          "speaker_id": "2",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100031105.1.8",
          "form": "둘다 각자 매력이 있어서",
          "original_form": "둘다 각자 매력이 있어서",
          "speaker_id": "1",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100031105.1.9",
          "form": "처음먹을땐 쫀득한게 좋고",
          "original_form": "처음먹을땐 쫀득한게 좋고",
          "speaker_id": "1",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100031105.1.10",
          "form": "나중에 먹을땐 바싹익힌게 좋더라",
          "original_form": "나중에 먹을땐 바싹익힌게 좋더라",
          "speaker_id": "1",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100031105.1.11",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100031105.1.12",
          "form": "ㅋㅋ맞아 둘 다 사실 맛있어",
          "original_form": "ㅋㅋ맞아 둘 다 사실 맛있어",
          "speaker_id": "2",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100031105.1.13",
          "form": "넌 두꺼운 고기가 좋니 얇은 고기가 좋니",
          "original_form": "넌 두꺼운 고기가 좋니 얇은 고기가 좋니",
          "speaker_id": "2",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100031105.1.14",
          "form": "삼겹살말야",
          "original_form": "삼겹살말야",
          "speaker_id": "2",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100031105.1.15",
          "form": "두꺼운게 좋아",
          "original_form": "두꺼운게 좋아",
          "speaker_id": "1",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100031105.1.16",
          "form": "얇은건 금방타서 먹어야돼",
          "original_form": "얇은건 금방타서 {censored}먹어야돼",
          "speaker_id": "1",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100031105.1.17",
          "form": "난 두껍되 가늘게 썰어서",
          "original_form": "난 두껍되 가늘게 썰어서",
          "speaker_id": "2",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100031105.1.18",
          "form": "정사각형 모양으로 굴려가며 구운 게 좋아",
          "original_form": "정사각형 모양으로 굴려가며 구운 게 좋아",
          "speaker_id": "2",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100031105.1.19",
          "form": "맞어 그것도 맛있어",
          "original_form": "맞어 그것도 맛있어",
          "speaker_id": "1",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100031105.1.20",
          "form": "두께와 너비가 1대1인게 좋아",
          "original_form": "두께와 너비가 1대1인게 좋아",
          "speaker_id": "2",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100031105.1.21",
          "form": "넌 라면 먹을 때",
          "original_form": "넌 라면 먹을 때",
          "speaker_id": "2",
          "time": "20210904 16:53"
        },
        {
          "id": "MDRW2100031105.1.22",
          "form": "꼬들면이 좋니 부드러운 게 좋니",
          "original_form": "꼬들면이 좋니 부드러운 게 좋니",
          "speaker_id": "2",
          "time": "20210904 16:53"
        },
        {
          "id": "MDRW2100031105.1.23",
          "form": "난 꼬들면파야",
          "original_form": "난 꼬들면파야",
          "speaker_id": "1",
          "time": "20210904 16:53"
        },
        {
          "id": "MDRW2100031105.1.24",
          "form": "옛날엔 푹익어서 퍼진게 좋았는데",
          "original_form": "옛날엔 푹익어서 퍼진게 좋았는데",
          "speaker_id": "1",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100031105.1.25",
          "form": "난 그때그때 다른데 요즘은 꼬들파야",
          "original_form": "난 그때그때 다른데 요즘은 꼬들파야",
          "speaker_id": "2",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100031105.1.26",
          "form": "근데 너 너구리 싫어한다며 라죽은 좋아하니",
          "original_form": "근데 너 너구리 싫어한다며 라죽은 좋아하니",
          "speaker_id": "2",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100031105.1.27",
          "form": "식성도 계속 변하는가봐",
          "original_form": "식성도 계속 변하는가봐",
          "speaker_id": "1",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100031105.1.28",
          "form": "둘이 무슨 차이야",
          "original_form": "둘이 무슨 차이야",
          "speaker_id": "2",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100031105.1.29",
          "form": "너구리는 국물도 맛없어",
          "original_form": "너구리는 국물도 맛없어",
          "speaker_id": "1",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100031105.1.30",
          "form": "라면이름이 너구리인것도 이해가안가",
          "original_form": "라면이름이 너구리인것도 이해가안가",
          "speaker_id": "1",
          "time": "20210904 16:55"
        }
      ]
