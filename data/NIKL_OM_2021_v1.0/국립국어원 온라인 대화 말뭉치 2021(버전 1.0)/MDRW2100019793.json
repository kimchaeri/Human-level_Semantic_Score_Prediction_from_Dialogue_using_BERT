      "utterance": [
        {
          "id": "MDRW2100019793.1.1",
          "form": "날이 추워지니 긴팔이 생각나네요",
          "original_form": "날이 추워지니 긴팔이 생각나네요",
          "speaker_id": "2",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019793.1.2",
          "form": "네 그래서 전 가디건 챙겨 다녀요. 감기 예방하려고요",
          "original_form": "네 그래서 전 가디건 챙겨 다녀요. 감기 예방하려고요",
          "speaker_id": "1",
          "time": "20210828 09:06"
        },
        {
          "id": "MDRW2100019793.1.3",
          "form": "이때 감기 걸리기 쉽죠",
          "original_form": "이때 감기 걸리기 쉽죠",
          "speaker_id": "2",
          "time": "20210828 09:20"
        },
        {
          "id": "MDRW2100019793.1.4",
          "form": "전 허약체질이 되서요ㅠㅠ 건강 잘 챙기시나요?",
          "original_form": "전 허약체질이 되서요ㅠㅠ 건강 잘 챙기시나요?",
          "speaker_id": "1",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100019793.1.5",
          "form": "저는 비염이 있어서 환절기는 힘들어요ㅠ",
          "original_form": "저는 비염이 있어서 환절기는 힘들어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100019793.1.6",
          "form": "요즘엔 운동을 하고 있어요!",
          "original_form": "요즘엔 운동을 하고 있어요!",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100019793.1.7",
          "form": "운동 뭐 하세요?",
          "original_form": "운동 뭐 하세요?",
          "speaker_id": "1",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100019793.1.8",
          "form": "걷기랑 스쿼트하고 있어요",
          "original_form": "걷기랑 스쿼트하고 있어요",
          "speaker_id": "2",
          "time": "20210828 13:01"
        },
        {
          "id": "MDRW2100019793.1.9",
          "form": "그 어려운 스쿼트를... 넘 멋져요",
          "original_form": "그 어려운 스쿼트를... 넘 멋져요",
          "speaker_id": "1",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100019793.1.10",
          "form": "앗 아녜요",
          "original_form": "앗 아녜요",
          "speaker_id": "2",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100019793.1.11",
          "form": "후들후들하면서 하고있어요ㅋㅋ",
          "original_form": "후들후들하면서 하고있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100019793.1.12",
          "form": "전 부끄럽지만 도전도 못했어요 무릎이 약해져서 걷기랑 간단한 운동만 하거든요",
          "original_form": "전 부끄럽지만 도전도 못했어요 무릎이 약해져서 걷기랑 간단한 운동만 하거든요",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100019793.1.13",
          "form": "걷기만으로도 대단하죠",
          "original_form": "걷기만으로도 대단하죠",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100019793.1.14",
          "form": "저도 이제 도전해보느라 30개정도만 하는걸요",
          "original_form": "저도 이제 도전해보느라 30개정도만 하는걸요",
          "speaker_id": "2",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100019793.1.15",
          "form": "10개도 힘들다고 하던데요",
          "original_form": "10개도 힘들다고 하던데요",
          "speaker_id": "1",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100019793.1.16",
          "form": "와 30개면 정도면 익숙해지면 더 잘 하실 것 같네요",
          "original_form": "와 30개면 정도면 익숙해지면 더 잘 하실 것 같네요",
          "speaker_id": "1",
          "time": "20210828 17:13"
        },
        {
          "id": "MDRW2100019793.1.17",
          "form": "어휴 감사합니다",
          "original_form": "어휴 감사합니다",
          "speaker_id": "2",
          "time": "20210828 18:23"
        },
        {
          "id": "MDRW2100019793.1.18",
          "form": "몸둘바를 모르겠네요ㅎㅎ",
          "original_form": "몸둘바를 모르겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 18:23"
        },
        {
          "id": "MDRW2100019793.1.19",
          "form": "저번에 30개씩 10세트로 14키론가 뺐다고해서 도전하려 시작했거든요ㅋㅋ",
          "original_form": "저번에 30개씩 10세트로 14키론가 뺐다고해서 도전하려 시작했거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:24"
        }
