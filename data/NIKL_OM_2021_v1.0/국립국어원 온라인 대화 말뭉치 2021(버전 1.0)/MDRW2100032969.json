      "utterance": [
        {
          "id": "MDRW2100032969.1.1",
          "form": "요즘도 그 제품이 나오나요",
          "original_form": "요즘도 그 제품이 나오나요",
          "speaker_id": "2",
          "time": "20210906 11:05"
        },
        {
          "id": "MDRW2100032969.1.2",
          "form": "저도 너무 반가워서 샀어요 어릴적에 먹던건데",
          "original_form": "저도 너무 반가워서 샀어요 어릴적에 먹던건데",
          "speaker_id": "1",
          "time": "20210906 11:05"
        },
        {
          "id": "MDRW2100032969.1.3",
          "form": "그건 가격이 얼마예요",
          "original_form": "그건 가격이 얼마예요",
          "speaker_id": "2",
          "time": "20210906 11:05"
        },
        {
          "id": "MDRW2100032969.1.4",
          "form": "1.5리터에 단돈 천원이더라구요 괜찮죠",
          "original_form": "1.5리터에 단돈 천원이더라구요 괜찮죠",
          "speaker_id": "1",
          "time": "20210906 11:05"
        },
        {
          "id": "MDRW2100032969.1.5",
          "form": "진짜 저렴한데 뭐가가 얼마 안 한대요",
          "original_form": "진짜 저렴한데 뭐가가 얼마 안 한대요",
          "speaker_id": "2",
          "time": "20210906 11:06"
        },
        {
          "id": "MDRW2100032969.1.6",
          "form": "어쩌면 내용물보다 케이스값이 더 클거에요",
          "original_form": "어쩌면 내용물보다 케이스값이 더 클거에요",
          "speaker_id": "1",
          "time": "20210906 11:06"
        },
        {
          "id": "MDRW2100032969.1.7",
          "form": "그리고 또 그게 전부인가요",
          "original_form": "그리고 또 그게 전부인가요",
          "speaker_id": "2",
          "time": "20210906 11:06"
        },
        {
          "id": "MDRW2100032969.1.8",
          "form": "크래미도 구입했어요 3개사면 만원이에요",
          "original_form": "크래미도 구입했어요 3개사면 만원이에요",
          "speaker_id": "1",
          "time": "20210906 11:06"
        },
        {
          "id": "MDRW2100032969.1.9",
          "form": "그래미는 맥주 안주로 딱 좋아요",
          "original_form": "그래미는 맥주 안주로 딱 좋아요",
          "speaker_id": "2",
          "time": "20210906 11:06"
        },
        {
          "id": "MDRW2100032969.1.10",
          "form": "감자샐러드에 넣어 먹으려고 샀어요",
          "original_form": "감자샐러드에 넣어 먹으려고 샀어요",
          "speaker_id": "1",
          "time": "20210906 11:07"
        },
        {
          "id": "MDRW2100032969.1.11",
          "form": "고기는 안 샀나요",
          "original_form": "고기는 안 샀나요",
          "speaker_id": "2",
          "time": "20210906 11:07"
        },
        {
          "id": "MDRW2100032969.1.12",
          "form": "고기는 집에 있어서 오징어 샀어요",
          "original_form": "고기는 집에 있어서 오징어 샀어요",
          "speaker_id": "1",
          "time": "20210906 11:07"
        },
        {
          "id": "MDRW2100032969.1.13",
          "form": "오징어는 얼마예요",
          "original_form": "오징어는 얼마예요",
          "speaker_id": "2",
          "time": "20210906 11:07"
        }
