      "utterance": [
        {
          "id": "MDRW2100046908.2.1",
          "form": "저는 그래서 지금부터 절약해서",
          "original_form": "저는 그래서 지금부터 절약해서",
          "speaker_id": "1",
          "time": "20211005 19:20"
        },
        {
          "id": "MDRW2100046908.2.2",
          "form": "연봉을 다 저축할 생각이에요",
          "original_form": "연봉을 다 저축할 생각이에요",
          "speaker_id": "1",
          "time": "20211005 19:20"
        },
        {
          "id": "MDRW2100046908.2.3",
          "form": "전이제 저혼자 살잖아요ㅎ",
          "original_form": "전이제 저혼자 살잖아요ㅎ",
          "speaker_id": "1",
          "time": "20211005 19:21"
        },
        {
          "id": "MDRW2100046908.2.4",
          "form": "오ㅏ 생활비는요?",
          "original_form": "오ㅏ 생활비는요?",
          "speaker_id": "2",
          "time": "20211005 19:21"
        },
        {
          "id": "MDRW2100046908.2.5",
          "form": "그러면 기본비용빼고 모두 저금하려구요ㅎ",
          "original_form": "그러면 기본비용빼고 모두 저금하려구요ㅎ",
          "speaker_id": "1",
          "time": "20211005 19:21"
        },
        {
          "id": "MDRW2100046908.2.6",
          "form": "아ㅎㅎ저 회사에서 년간 200만원씩 복지카드가 주어져요ㅎ",
          "original_form": "아ㅎㅎ저 회사에서 년간 200만원씩 복지카드가 주어져요ㅎ",
          "speaker_id": "1",
          "time": "20211005 19:21"
        },
        {
          "id": "MDRW2100046908.2.7",
          "form": "고정비 ㅈㅔ외해도 여기저기 들어가실건ㄷㅔ요",
          "original_form": "고정비 ㅈㅔ외해도 여기저기 들어가실건ㄷㅔ요",
          "speaker_id": "2",
          "time": "20211005 19:21"
        },
        {
          "id": "MDRW2100046908.2.8",
          "form": "그걸로 생활하고ㅎ",
          "original_form": "그걸로 생활하고ㅎ",
          "speaker_id": "1",
          "time": "20211005 19:21"
        },
        {
          "id": "MDRW2100046908.2.9",
          "form": "고정비는 제외하면",
          "original_form": "고정비는 제외하면",
          "speaker_id": "1",
          "time": "20211005 19:21"
        },
        {
          "id": "MDRW2100046908.2.10",
          "form": "200! 와.엄청마니주네요",
          "original_form": "200! 와.엄청마니주네요",
          "speaker_id": "2",
          "time": "20211005 19:21"
        },
        {
          "id": "MDRW2100046908.2.11",
          "form": "그리고 부수입으로 생활한다면",
          "original_form": "그리고 부수입으로 생활한다면",
          "speaker_id": "1",
          "time": "20211005 19:21"
        },
        {
          "id": "MDRW2100046908.2.12",
          "form": "되지않을까요?",
          "original_form": "되지않을까요?",
          "speaker_id": "1",
          "time": "20211005 19:21"
        },
        {
          "id": "MDRW2100046908.2.13",
          "form": "한달에 16정도로 가능하나요?",
          "original_form": "한달에 16정도로 가능하나요?",
          "speaker_id": "2",
          "time": "20211005 19:22"
        },
        {
          "id": "MDRW2100046908.2.14",
          "form": "글죠ㅎ 아무래도?",
          "original_form": "글죠ㅎ 아무래도?",
          "speaker_id": "1",
          "time": "20211005 19:22"
        },
        {
          "id": "MDRW2100046908.2.15",
          "form": "아ㅡ부수입은 뭐잇나요",
          "original_form": "아ㅡ부수입은 뭐잇나요",
          "speaker_id": "2",
          "time": "20211005 19:22"
        },
        {
          "id": "MDRW2100046908.2.16",
          "form": "그리고 만약에 제가 부수입이 100만원이라묜",
          "original_form": "그리고 만약에 제가 부수입이 100만원이라묜",
          "speaker_id": "1",
          "time": "20211005 19:22"
        },
        {
          "id": "MDRW2100046908.2.17",
          "form": "그럼 충분하죠",
          "original_form": "그럼 충분하죠",
          "speaker_id": "2",
          "time": "20211005 19:22"
        },
        {
          "id": "MDRW2100046908.2.18",
          "form": "되지않을까요?",
          "original_form": "되지않을까요?",
          "speaker_id": "1",
          "time": "20211005 19:22"
        },
        {
          "id": "MDRW2100046908.2.19",
          "form": "100부수입이라 ㅎ",
          "original_form": "100부수입이라 ㅎ",
          "speaker_id": "2",
          "time": "20211005 19:22"
        },
        {
          "id": "MDRW2100046908.2.20",
          "form": "부수입은 저 앱테크하고있어요",
          "original_form": "부수입은 저 앱테크하고있어요",
          "speaker_id": "1",
          "time": "20211005 19:22"
        },
        {
          "id": "MDRW2100046908.2.21",
          "form": "그리고 이벤트도 하고 있구요",
          "original_form": "그리고 이벤트도 하고 있구요",
          "speaker_id": "1",
          "time": "20211005 19:22"
        },
        {
          "id": "MDRW2100046908.2.22",
          "form": "아 앱테크",
          "original_form": "아 앱테크",
          "speaker_id": "2",
          "time": "20211005 19:23"
        },
        {
          "id": "MDRW2100046908.2.23",
          "form": "이제 블로그랑 유튜브도 할 생각이에요ㅎ",
          "original_form": "이제 블로그랑 유튜브도 할 생각이에요ㅎ",
          "speaker_id": "1",
          "time": "20211005 19:23"
        },
        {
          "id": "MDRW2100046908.2.24",
          "form": "대단하시네요 ㅎ",
          "original_form": "대단하시네요 ㅎ",
          "speaker_id": "2",
          "time": "20211005 19:23"
        },
        {
          "id": "MDRW2100046908.2.25",
          "form": "뭐든 하면 안되더라도 경험치는 쌓일것같애서ㅎ 해보려구요ㅎ",
