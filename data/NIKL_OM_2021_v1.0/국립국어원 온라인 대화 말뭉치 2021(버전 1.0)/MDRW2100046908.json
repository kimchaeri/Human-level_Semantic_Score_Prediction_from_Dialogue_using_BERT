      "utterance": [
        {
          "id": "MDRW2100046908.1.1",
          "form": "안녕하세요 name2님",
          "original_form": "안녕하세요 &name2&님",
          "speaker_id": "1",
          "time": "20211005 18:27"
        },
        {
          "id": "MDRW2100046908.1.2",
          "form": "혹시 파이어족이라고 아세요?",
          "original_form": "혹시 파이어족이라고 아세요?",
          "speaker_id": "1",
          "time": "20211005 18:28"
        },
        {
          "id": "MDRW2100046908.1.3",
          "form": "뉴스에서 본적있어요",
          "original_form": "뉴스에서 본적있어요",
          "speaker_id": "2",
          "time": "20211005 19:16"
        },
        {
          "id": "MDRW2100046908.1.4",
          "form": "혹시 뭐 준비하고 있으신거있으세요?",
          "original_form": "혹시 뭐 준비하고 있으신거있으세요?",
          "speaker_id": "1",
          "time": "20211005 19:16"
        },
        {
          "id": "MDRW2100046908.1.5",
          "form": "주식?코인?",
          "original_form": "주식?코인?",
          "speaker_id": "1",
          "time": "20211005 19:17"
        },
        {
          "id": "MDRW2100046908.1.6",
          "form": "경제적 독립을 통해 조기 은퇴라.. 정말하고싶네요",
          "original_form": "경제적 독립을 통해 조기 은퇴라.. 정말하고싶네요",
          "speaker_id": "2",
          "time": "20211005 19:17"
        },
        {
          "id": "MDRW2100046908.1.7",
          "form": "경제적독립은 왜요?",
          "original_form": "경제적독립은 왜요?",
          "speaker_id": "1",
          "time": "20211005 19:17"
        },
        {
          "id": "MDRW2100046908.1.8",
          "form": "뭔 문제라도?",
          "original_form": "뭔 문제라도?",
          "speaker_id": "1",
          "time": "20211005 19:17"
        },
        {
          "id": "MDRW2100046908.1.9",
          "form": "파이어족이면 경제적 독립인데ㅠ",
          "original_form": "파이어족이면 경제적 독립인데ㅠ",
          "speaker_id": "1",
          "time": "20211005 19:17"
        },
        {
          "id": "MDRW2100046908.1.10",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20211005 19:17"
        },
        {
          "id": "MDRW2100046908.1.11",
          "form": "그게 쉽지않네요",
          "original_form": "그게 쉽지않네요",
          "speaker_id": "2",
          "time": "20211005 19:17"
        },
        {
          "id": "MDRW2100046908.1.12",
          "form": "지금 그러면 혹시 누가 경제관리하세뇨?",
          "original_form": "지금 그러면 혹시 누가 경제관리하세뇨?",
          "speaker_id": "1",
          "time": "20211005 19:18"
        },
        {
          "id": "MDRW2100046908.1.13",
          "form": "아... 미혼이신가요?",
          "original_form": "아... 미혼이신가요?",
          "speaker_id": "1",
          "time": "20211005 19:18"
        },
        {
          "id": "MDRW2100046908.1.14",
          "form": "돈을 모아야하는데 그게어렵네요. 지금은 제가요",
          "original_form": "돈을 모아야하는데 그게어렵네요. 지금은 제가요",
          "speaker_id": "2",
          "time": "20211005 19:18"
        },
        {
          "id": "MDRW2100046908.1.15",
          "form": "네 미혼입니다~",
          "original_form": "네 미혼입니다~",
          "speaker_id": "2",
          "time": "20211005 19:18"
        },
        {
          "id": "MDRW2100046908.1.16",
          "form": "아아ㅎㅎ 그러시구나ㅎㅎ",
          "original_form": "아아ㅎㅎ 그러시구나ㅎㅎ",
          "speaker_id": "1",
          "time": "20211005 19:18"
        },
        {
          "id": "MDRW2100046908.1.17",
          "form": "결혼도 선택이니 ㅎ",
          "original_form": "결혼도 선택이니 ㅎ",
          "speaker_id": "2",
          "time": "20211005 19:18"
        },
        {
          "id": "MDRW2100046908.1.18",
          "form": "저도 똑갘애요ㅎ 돈을 모아야하는데",
          "original_form": "저도 똑갘애요ㅎ 돈을 모아야하는데",
          "speaker_id": "1",
          "time": "20211005 19:18"
        },
        {
          "id": "MDRW2100046908.1.19",
          "form": "전 이혼했거든요ㅎ",
          "original_form": "전 이혼했거든요ㅎ",
          "speaker_id": "1",
          "time": "20211005 19:18"
        },
        {
          "id": "MDRW2100046908.1.20",
          "form": "name1님은 뭐하세요?",
          "original_form": "&name1&님은 뭐하세요?",
          "speaker_id": "2",
          "time": "20211005 19:18"
        },
        {
          "id": "MDRW2100046908.1.21",
          "form": "그래서 지금부터 계속 모으려구욤ㅎ",
          "original_form": "그래서 지금부터 계속 모으려구욤ㅎ",
          "speaker_id": "1",
          "time": "20211005 19:18"
        },
        {
          "id": "MDRW2100046908.1.22",
          "form": "헛..\r\n그러시구나",
          "original_form": "헛..\r\n그러시구나",
          "speaker_id": "2",
          "time": "20211005 19:18"
        },
        {
          "id": "MDRW2100046908.1.23",
          "form": "전 맨날 돈모아요ㅎ",
          "original_form": "전 맨날 돈모아요ㅎ",
          "speaker_id": "1",
          "time": "20211005 19:19"
        },
        {
          "id": "MDRW2100046908.1.24",
          "form": "어떻게요?",
          "original_form": "어떻게요?",
          "speaker_id": "2",
          "time": "20211005 19:19"
        },
        {
          "id": "MDRW2100046908.1.25",
          "form": "솔직히 투자보다 절약이 더 낫더라구여",
          "original_form": "솔직히 투자보다 절약이 더 낫더라구여",
          "speaker_id": "1",
          "time": "20211005 19:19"
        },
        {
          "id": "MDRW2100046908.1.26",
          "form": "아 종잣돈이모여야죠",
          "original_form": "아 종잣돈이모여야죠",
          "speaker_id": "2",
          "time": "20211005 19:19"
        },
        {
          "id": "MDRW2100046908.1.27",
          "form": "투자해서 10프로 올리는게 낫겠어요? 아니면 절약 10프로가 낫겠아요?",
          "original_form": "투자해서 10프로 올리는게 낫겠어요? 아니면 절약 10프로가 낫겠아요?",
          "speaker_id": "1",
          "time": "20211005 19:19"
        },
        {
          "id": "MDRW2100046908.1.28",
          "form": "투자는 손실이 발생하는데",
          "original_form": "투자는 손실이 발생하는데",
          "speaker_id": "1",
          "time": "20211005 19:19"
        },
        {
          "id": "MDRW2100046908.1.29",
          "form": "아 맞네요",
          "original_form": "아 맞네요",
          "speaker_id": "2",
          "time": "20211005 19:20"
        },
        {
          "id": "MDRW2100046908.1.30",
          "form": "절약은 손실이 있을까요?",
          "original_form": "절약은 손실이 있을까요?",
          "speaker_id": "1",
          "time": "20211005 19:20"
        },
        {
          "id": "MDRW2100046908.1.31",
          "form": "그말들으니 확실허네요",
          "original_form": "그말들으니 확실허네요",
          "speaker_id": "2",
          "time": "20211005 19:20"
        },
        {
          "id": "MDRW2100046908.1.32",
          "form": "파이어족을 준비중이시라면",
          "original_form": "파이어족을 준비중이시라면",
          "speaker_id": "1",
          "time": "20211005 19:20"
        },
        {
          "id": "MDRW2100046908.1.33",
          "form": "없죠 절약!!",
          "original_form": "없죠 절약!!",
          "speaker_id": "2",
          "time": "20211005 19:20"
        },
        {
          "id": "MDRW2100046908.1.34",
          "form": "그게 맞지않을까요m",
          "original_form": "그게 맞지않을까요m",
          "speaker_id": "1",
          "time": "20211005 19:20"
        },
        {
          "id": "MDRW2100046908.1.35",
          "form": "네, 저도 늦기전에 시작하려고요",
          "original_form": "네, 저도 늦기전에 시작하려고요",
          "speaker_id": "2",
          "time": "20211005 19:20"
        }
