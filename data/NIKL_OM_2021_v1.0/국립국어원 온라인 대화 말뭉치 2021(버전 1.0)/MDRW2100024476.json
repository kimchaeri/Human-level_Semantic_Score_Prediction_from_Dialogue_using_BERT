      "utterance": [
        {
          "id": "MDRW2100024476.1.1",
          "form": "싸이월드 해봤나요?",
          "original_form": "싸이월드 해봤나요?",
          "speaker_id": "1",
          "time": "20210831 11:06"
        },
        {
          "id": "MDRW2100024476.1.2",
          "form": "네! 예전에 잠깐했었어요~ name1은요?",
          "original_form": "네! 예전에 잠깐했었어요~ &name1&은요?",
          "speaker_id": "2",
          "time": "20210831 11:16"
        },
        {
          "id": "MDRW2100024476.1.3",
          "form": "전 몇년 열심히 했었죠~",
          "original_form": "전 몇년 열심히 했었죠~",
          "speaker_id": "1",
          "time": "20210831 11:18"
        },
        {
          "id": "MDRW2100024476.1.4",
          "form": "그래서 아마 그곳에 제 흑역사가 잔뜩 있을거에요..ㅋㅋ",
          "original_form": "그래서 아마 그곳에 제 흑역사가 잔뜩 있을거에요..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:19"
        },
        {
          "id": "MDRW2100024476.1.5",
          "form": "앗ㅋㅋㅋ 저두..ㅋㅋ 그래서 저는 삭제했어욬ㅋㄲ",
          "original_form": "앗ㅋㅋㅋ 저두..ㅋㅋ 그래서 저는 삭제했어욬ㅋㄲ",
          "speaker_id": "2",
          "time": "20210831 11:28"
        },
        {
          "id": "MDRW2100024476.1.6",
          "form": "근데 부활하면 그때 인기가 돌아올까요,?!",
          "original_form": "근데 부활하면 그때 인기가 돌아올까요,?!",
          "speaker_id": "2",
          "time": "20210831 11:28"
        },
        {
          "id": "MDRW2100024476.1.7",
          "form": "어.. 어미 삭제했어요?",
          "original_form": "어.. 어미 삭제했어요?",
          "speaker_id": "1",
          "time": "20210831 11:30"
        },
        {
          "id": "MDRW2100024476.1.8",
          "form": "넹ㅋㅋ 뭔가 그때 남기면 안돼! 이렇게 생각했던거 같아요ㅋㅋㄲ",
          "original_form": "넹ㅋㅋ 뭔가 그때 남기면 안돼! 이렇게 생각했던거 같아요ㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100024476.1.9",
          "form": "대단해요 난 왜 그생각을 못했을까",
          "original_form": "대단해요 난 왜 그생각을 못했을까",
          "speaker_id": "1",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100024476.1.10",
          "form": "그때 어떤글  보고 혹했던거 같아요ㅋㄱㅋ",
          "original_form": "그때 어떤글  보고 혹했던거 같아요ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024476.1.11",
          "form": "미래암시글같은거요ㄱㅋㅋ",
          "original_form": "미래암시글같은거요ㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024476.1.12",
          "form": "ㅎㅎㅎㅎㅎㅎ 선경지몀이 있네요..",
          "original_form": "ㅎㅎㅎㅎㅎㅎ 선경지몀이 있네요..",
          "speaker_id": "1",
          "time": "20210901 06:29"
        },
        {
          "id": "MDRW2100024476.1.13",
          "form": "그럼 복원되도 볼게 없는건가요?",
          "original_form": "그럼 복원되도 볼게 없는건가요?",
          "speaker_id": "1",
          "time": "20210901 06:29"
        },
        {
          "id": "MDRW2100024476.1.14",
          "form": "네.. 그건 슬퍼요ㅜ",
          "original_form": "네.. 그건 슬퍼요ㅜ",
          "speaker_id": "2",
          "time": "20210901 07:29"
        },
        {
          "id": "MDRW2100024476.1.15",
          "form": "그렇군요.. 전 사진도 사진이지만 방명록과 일촌평 등도 궁굼하거든요",
          "original_form": "그렇군요.. 전 사진도 사진이지만 방명록과 일촌평 등도 궁굼하거든요",
          "speaker_id": "1",
          "time": "20210901 09:33"
        },
        {
          "id": "MDRW2100024476.1.16",
          "form": "오오 진짜요",
          "original_form": "오오 진짜요",
          "speaker_id": "2",
          "time": "20210901 09:48"
        },
        {
          "id": "MDRW2100024476.1.17",
          "form": "그런거 보는 재미는 있을거 같아요 오글거려도",
          "original_form": "그런거 보는 재미는 있을거 같아요 오글거려도",
          "speaker_id": "2",
          "time": "20210901 09:48"
        },
        {
          "id": "MDRW2100024476.1.18",
          "form": "그니까요.. 간만에 보면 추억도 되고 오글거리기도하고..ㅎㅎㅎ",
          "original_form": "그니까요.. 간만에 보면 추억도 되고 오글거리기도하고..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100024476.1.19",
