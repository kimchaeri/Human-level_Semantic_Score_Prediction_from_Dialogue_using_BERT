      "utterance": [
        {
          "id": "MDRW2100018131.1.1",
          "form": "아..집안일은 해도해도 끝이없어요",
          "original_form": "아..집안일은 해도해도 끝이없어요",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100018131.1.2",
          "form": "맞아요.. 저도 평일엔 집에 저만 있어서",
          "original_form": "맞아요.. 저도 평일엔 집에 저만 있어서",
          "speaker_id": "1",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018131.1.3",
          "form": "할 수 있는 건 하려고 하는데",
          "original_form": "할 수 있는 건 하려고 하는데",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018131.1.4",
          "form": "정말 이거 끝나면 저거하고 저거 끝나면 또 다른 거 하고",
          "original_form": "정말 이거 끝나면 저거하고 저거 끝나면 또 다른 거 하고",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018131.1.5",
          "form": "무한 반복이에여",
          "original_form": "무한 반복이에여",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018131.1.6",
          "form": "맞아요 저는 그래서",
          "original_form": "맞아요 저는 그래서",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018131.1.7",
          "form": "안하다가 1주나 2주에 한번",
          "original_form": "안하다가 1주나 2주에 한번",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018131.1.8",
          "form": "청소연구소 이용해서 대청소해요",
          "original_form": "청소연구소 이용해서 대청소해요",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018131.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018131.1.10",
          "form": "그다음날 다시 도로묵되지만...",
          "original_form": "그다음날 다시 도로묵되지만...",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018131.1.11",
          "form": "헐 그런건 한 번 하면",
          "original_form": "헐 그런건 한 번 하면",
          "speaker_id": "1",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018131.1.12",
          "form": "얼마나 나가요?",
          "original_form": "얼마나 나가요?",
          "speaker_id": "1",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018131.1.13",
          "form": "5만2천원정도",
          "original_form": "5만2천원정도",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018131.1.14",
          "form": "4시간 청소해주시는데",
          "original_form": "4시간 청소해주시는데",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018131.1.15",
          "form": "화장실이랑 베란다 싹더",
          "original_form": "화장실이랑 베란다 싹더",
          "speaker_id": "2",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018131.1.16",
          "form": "해주셔요",
          "original_form": "해주셔요",
          "speaker_id": "2",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018131.1.17",
          "form": "어지러운거 정리도해주시고",
          "original_form": "어지러운거 정리도해주시고",
          "speaker_id": "2",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018131.1.18",
          "form": "아 그런거 한번 씩 하면 진짜 좋은데",
          "original_form": "아 그런거 한번 씩 하면 진짜 좋은데",
          "speaker_id": "1",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018131.1.19",
          "form": "그쵸 완전 추천!",
          "original_form": "그쵸 완전 추천!",
          "speaker_id": "2",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018131.1.20",
          "form": "저희 부모님은 남한테 뭘 못맡기시는 성격이라.. ㅎㅎ",
          "original_form": "저희 부모님은 남한테 뭘 못맡기시는 성격이라.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018131.1.21",
          "form": "나중에 독립하시거나 결혼하시면 이용해보세요. ㅋ",
          "original_form": "나중에 독립하시거나 결혼하시면 이용해보세요. ㅋ",
          "speaker_id": "2",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018131.1.22",
          "form": "그래야뎄어요",
          "original_form": "그래야뎄어요",
          "speaker_id": "1",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018131.1.23",
          "form": "저도 지금 엄마랑 같이 살고있어요ㅜ",
          "original_form": "저도 지금 엄마랑 같이 살고있어요ㅜ",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018131.1.24",
          "form": "남편이랑 잠깐 엄마집에 들어와서",
          "original_form": "남편이랑 잠깐 엄마집에 들어와서",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018131.1.25",
          "form": "지내는데 쉽지가 않네요 ㅎ",
          "original_form": "지내는데 쉽지가 않네요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018131.1.26",
          "form": "그래도 엄마집이 더 편하지 않아요? 전 임신 중일 때 엄마집에거 지내고 싶은데",
          "original_form": "그래도 엄마집이 더 편하지 않아요? 전 임신 중일 때 엄마집에거 지내고 싶은데",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018131.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018131.1.28",
          "form": "ㅜㅜㅜㅜ엄마가 작년에",
          "original_form": "ㅜㅜㅜㅜ엄마가 작년에",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018131.1.29",
          "form": "항암치료하고 회복중이시라",
          "original_form": "항암치료하고 회복중이시라",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018131.1.30",
          "form": "아ㅜㅜ 그러면 좀 마음이 불편하겠네요",
          "original_form": "아ㅜㅜ 그러면 좀 마음이 불편하겠네요",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018131.1.31",
          "form": "에혀 그래도따로사는게.나아요ㅠㅠ \r\n남편이 엄마 걱정된다고 잠깐 돌봐드리자고 들어왔는데",
          "original_form": "에혀 그래도따로사는게.나아요ㅠㅠ \r\n남편이 엄마 걱정된다고 잠깐 돌봐드리자고 들어왔는데",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018131.1.32",
          "form": "임신이되서...ㅋㅋㅋㅋ",
          "original_form": "임신이되서...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018131.1.33",
          "form": "동생이랑 엄마가 애기태어나면 좀도와주지않을까 하는 기대도있어요",
          "original_form": "동생이랑 엄마가 애기태어나면 좀도와주지않을까 하는 기대도있어요",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018131.1.34",
          "form": "ㅋㅋㅋ 지금은 힘들지만 내년이면 회복도 잘 되시겠지 이러면서",
          "original_form": "ㅋㅋㅋ 지금은 힘들지만 내년이면 회복도 잘 되시겠지 이러면서",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018131.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 남편분은.집안일 잘 하시나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 남편분은.집안일 잘 하시나요?",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018131.1.36",
          "form": "ㅋㅋㅋ요즘 제가 상전모드라",
          "original_form": "ㅋㅋㅋ요즘 제가 상전모드라",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018131.1.37",
          "form": "남편이 거의다해요",
          "original_form": "남편이 거의다해요",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018131.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋ 와 정말 좋네요!! 부디 출산후에도 이어지시길... ㅎㅎ",
