          "id": "MDRW2100005397.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005397.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005397.1.3",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210822 08:33"
        },
        {
          "id": "MDRW2100005397.1.4",
          "form": "고양이이요",
          "original_form": "고양이이요",
          "speaker_id": "2",
          "time": "20210822 12:23"
        },
        {
          "id": "MDRW2100005397.1.5",
          "form": "털관리는요?",
          "original_form": "털관리는요?",
          "speaker_id": "1",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100005397.1.6",
          "form": "주기적으로 털을밀어줍니다",
          "original_form": "주기적으로 털을밀어줍니다",
          "speaker_id": "2",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100005397.1.7",
          "form": "돌돌이 필수죠?",
          "original_form": "돌돌이 필수죠?",
          "speaker_id": "1",
          "time": "20210822 12:46"
        },
        {
          "id": "MDRW2100005397.1.8",
          "form": "당연하죠 집에2개있어요",
          "original_form": "당연하죠 집에2개있어요",
          "speaker_id": "2",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100005397.1.9",
          "form": "집사님 대단해요",
          "original_form": "집사님 대단해요",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100005397.1.10",
          "form": "그러니 절 좀좋아해줬으멸해요",
          "original_form": "그러니 절 좀좋아해줬으멸해요",
          "speaker_id": "2",
          "time": "20210822 13:47"
        },
        {
          "id": "MDRW2100005397.1.11",
          "form": "새초롬하져?",
          "original_form": "새초롬하져?",
          "speaker_id": "1",
          "time": "20210822 13:47"
        },
        {
          "id": "MDRW2100005397.1.12",
          "form": "네ㅠㅠ",
          "original_form": "네ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100005397.1.13",
          "form": "주인만이라더 알아봐주면",
          "original_form": "주인만이라더 알아봐주면",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100005397.1.14",
          "form": "알아보긴하는데 별로안좋아해줘요",
