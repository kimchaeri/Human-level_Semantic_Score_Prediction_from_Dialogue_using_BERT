      "utterance": [
        {
          "id": "MDRW2100021958.1.1",
          "form": "저 매운갈비찜이요!!",
          "original_form": "저 매운갈비찜이요!!",
          "speaker_id": "2",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021958.1.2",
          "form": "매운거 잘 드세요,",
          "original_form": "매운거 잘 드세요,",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100021958.1.3",
          "form": "아뇨 그 정도는 아닌데",
          "original_form": "아뇨 그 정도는 아닌데",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100021958.1.4",
          "form": "매콤한 거 좋아해요",
          "original_form": "매콤한 거 좋아해요",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100021958.1.5",
          "form": "매운갈비찜 맛있을것같아요",
          "original_form": "매운갈비찜 맛있을것같아요",
          "speaker_id": "1",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100021958.1.6",
          "form": "맞아요ㅎㅎ 제가 좋아하는 음식점이라서",
          "original_form": "맞아요ㅎㅎ 제가 좋아하는 음식점이라서",
          "speaker_id": "2",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100021958.1.7",
          "form": "자주 시켜먹어요",
          "original_form": "자주 시켜먹어요",
          "speaker_id": "2",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100021958.1.8",
          "form": "아구찜도 갑자기 먹고싶어요",
          "original_form": "아구찜도 갑자기 먹고싶어요",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100021958.1.9",
          "form": "헉 아구찜도 너무 맛있죠...",
          "original_form": "헉 아구찜도 너무 맛있죠...",
          "speaker_id": "2",
          "time": "20210906 20:01"
        },
        {
          "id": "MDRW2100021958.1.10",
          "form": "저도 먹고 싶네요",
          "original_form": "저도 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210906 20:01"
        },
        {
          "id": "MDRW2100021958.1.11",
          "form": "맛있겠죠ㅠㅠ요즘 매운게 땡겨요",
          "original_form": "맛있겠죠ㅠㅠ요즘 매운게 땡겨요",
          "speaker_id": "1",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100021958.1.12",
          "form": "저두요ㅠㅠ 요즘 진짜 매운맛에 미쳐가지고..",
          "original_form": "저두요ㅠㅠ 요즘 진짜 매운맛에 미쳐가지고..",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100021958.1.13",
          "form": "오늘은 매콤한 김치찜 먹었어요",
          "original_form": "오늘은 매콤한 김치찜 먹었어요",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100021958.1.14",
          "form": "매운음식 또 뭐졸아하세요?",
          "original_form": "매운음식 또 뭐졸아하세요?",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100021958.1.15",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100021958.1.16",
          "form": "오 김치찜도 맛있겠다",
          "original_form": "오 김치찜도 맛있겠다",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100021958.1.17",
          "form": "저 마라탕이랑 매운쌀국수도 좋아해요!",
          "original_form": "저 마라탕이랑 매운쌀국수도 좋아해요!",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100021958.1.18",
          "form": "오늘 김치찜 새로운 음식점 도전했는데 성공했어요ㅜㅜ",
          "original_form": "오늘 김치찜 새로운 음식점 도전했는데 성공했어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100021958.1.19",
          "form": "존맛...!",
          "original_form": "존맛...!",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100021958.1.20",
          "form": "축하해요!!!!",
          "original_form": "축하해요!!!!",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100021958.1.21",
          "form": "음식점 새로 찾을때 엄청 고민되던뎅ㅋㅋ",
          "original_form": "음식점 새로 찾을때 엄청 고민되던뎅ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100021958.1.22",
          "form": "잘 찾으셨나봐요",
          "original_form": "잘 찾으셨나봐요",
          "speaker_id": "1",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100021958.1.23",
          "form": "배달 자주 시키는데",
          "original_form": "배달 자주 시키는데",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100021958.1.24",
          "form": "주문 많은 순서대로 해서 후기 참고하니까",
          "original_form": "주문 많은 순서대로 해서 후기 참고하니까",
          "speaker_id": "2",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100021958.1.25",
          "form": "많이 성공하는 거 같아요ㅎㅎ",
          "original_form": "많이 성공하는 거 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100021958.1.26",
          "form": "역시 배달 최고",
          "original_form": "역시 배달 최고",
          "speaker_id": "1",
          "time": "20210907 19:09"
        }
      ]
    }
  ]
