      "utterance": [
        {
          "id": "MDRW2100014050.1.1",
          "form": "여긴 평화로워요",
          "original_form": "여긴 평화로워요",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014050.1.2",
          "form": "비가 많이 오나봐요?",
          "original_form": "비가 많이 오나봐요?",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014050.1.3",
          "form": "아 미쳤죠",
          "original_form": "아 미쳤죠",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014050.1.4",
          "form": "창문 뿌사질 것 같애요",
          "original_form": "창문 뿌사질 것 같애요",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014050.1.5",
          "form": "헐 신문지 붙여야하는거아니에요??",
          "original_form": "헐 신문지 붙여야하는거아니에요??",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100014050.1.6",
          "form": "신문지 죅죅붙여요",
          "original_form": "신문지 죅죅붙여요",
          "speaker_id": "2",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014050.1.7",
          "form": "아 전에 붙였다가",
          "original_form": "아 전에 붙였다가",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014050.1.8",
          "form": "찌꺼기 남는 거 치우는 게 귀찮아서",
          "original_form": "찌꺼기 남는 거 치우는 게 귀찮아서",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014050.1.9",
          "form": "그냥 깨지면 교체하자고",
          "original_form": "그냥 깨지면 교체하자고",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014050.1.10",
          "form": "쇼부봤어요",
          "original_form": "쇼부봤어요",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014050.1.11",
          "form": "헐 그런문제가...",
          "original_form": "헐 그런문제가...",
          "speaker_id": "2",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014050.1.12",
          "form": "껄껄",
          "original_form": "껄껄",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014050.1.13",
          "form": "그..깨지면 인간도깨질것같은데요",
          "original_form": "그..깨지면 인간도깨질것같은데요",
          "speaker_id": "2",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014050.1.14",
          "form": "그전에 테이프 붙여봤는데",
          "original_form": "그전에 테이프 붙여봤는데",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014050.1.15",
          "form": "테이프 떼는데 1년 걸렸어요",
          "original_form": "테이프 떼는데 1년 걸렸어요",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014050.1.16",
          "form": "정신과 팔이 깨졌어요",
          "original_form": "정신과 팔이 깨졌어요",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014050.1.17",
          "form": "그렇구나....",
          "original_form": "그렇구나....",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014050.1.18",
          "form": "짱이다...",
          "original_form": "짱이다...",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014050.1.19",
          "form": "접착력짱이네요",
          "original_form": "접착력짱이네요",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014050.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014050.1.21",
          "form": "요즘 비때문에",
          "original_form": "요즘 비때문에",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014050.1.22",
          "form": "좀 쌀쌀해요",
          "original_form": "좀 쌀쌀해요",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014050.1.23",
          "form": "근데 진짜 깨진 거 같아요",
          "original_form": "근데 진짜 깨진 거 같아요",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014050.1.24",
          "form": "아까 빨래걷는데 저한테 물이 튀더라고요",
          "original_form": "아까 빨래걷는데 저한테 물이 튀더라고요",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014050.1.25",
          "form": "슬슬 가을옷 꺼내야해요",
          "original_form": "슬슬 가을옷 꺼내야해요",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014050.1.26",
          "form": "가을옷...? 선넘네...",
          "original_form": "가을옷...? 선넘네...",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014050.1.27",
          "form": "예...?",
          "original_form": "예...?",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014050.1.28",
          "form": "저 지금 에어컨 틀고있는데",
          "original_form": "저 지금 에어컨 틀고있는데",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014050.1.29",
          "form": "창문이나 고치세요",
          "original_form": "창문이나 고치세요",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014050.1.30",
          "form": "참나",
          "original_form": "참나",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014050.1.31",
          "form": "말돌리지마세요",
          "original_form": "말돌리지마세요",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014050.1.32",
          "form": "저도 에어컨켰어요",
          "original_form": "저도 에어컨켰어요",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014050.1.33",
          "form": "킼킼",
          "original_form": "킼킼",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014050.1.34",
          "form": "보일러도켰어요",
          "original_form": "보일러도켰어요",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014050.1.35",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014050.1.36",
          "form": "너무습해...",
          "original_form": "너무습해...",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014050.1.37",
          "form": "지구파괴자",
          "original_form": "지구파괴자",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014050.1.38",
          "form": "아 그건 킹정이지..",
          "original_form": "아 그건 킹정이지..",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014050.1.39",
          "form": "그렇게됐습니다...",
          "original_form": "그렇게됐습니다...",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014050.1.40",
          "form": "전 습하니까 좋아요",
          "original_form": "전 습하니까 좋아요",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014050.1.41",
          "form": "피부가 안갈라져",
          "original_form": "피부가 안갈라져",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014050.1.42",
          "form": "짱!",
          "original_form": "짱!",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014050.1.43",
          "form": "??개구리이신지",
          "original_form": "??개구리이신지",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014050.1.44",
          "form": "개구리도 물밖에서 하루는 버틸듯",
          "original_form": "개구리도 물밖에서 하루는 버틸듯",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014050.1.45",
          "form": "크툴루",
          "original_form": "크툴루",
          "speaker_id": "2",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014050.1.46",
          "form": "왜 저는 하마처럼",
          "original_form": "왜 저는 하마처럼",
          "speaker_id": "1",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014050.1.47",
          "form": "피부보호빨간즙",
          "original_form": "피부보호빨간즙",
          "speaker_id": "1",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014050.1.48",
          "form": "안나올까요",
          "original_form": "안나올까요",
          "speaker_id": "1",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014050.1.49",
          "form": "당신도",
          "original_form": "당신도",
          "speaker_id": "2",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014050.1.50",
          "form": "나오잖아요",
          "original_form": "나오잖아요",
          "speaker_id": "2",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014050.1.51",
          "form": "아 피가 나긴 해요~?~?",
          "original_form": "아 피가 나긴 해요~?~?",
          "speaker_id": "1",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014050.1.52",
          "form": "그게 피부보호즙인가?",
          "original_form": "그게 피부보호즙인가?",
          "speaker_id": "1",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014050.1.53",
          "form": "비슷하긴 해~",
          "original_form": "비슷하긴 해~",
          "speaker_id": "1",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014050.1.54",
          "form": "비슷하죠...",
          "original_form": "비슷하죠...",
          "speaker_id": "2",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014050.1.55",
          "form": "아무래도...",
          "original_form": "아무래도...",
          "speaker_id": "2",
          "time": "20210825 17:09"
        }
      ]
