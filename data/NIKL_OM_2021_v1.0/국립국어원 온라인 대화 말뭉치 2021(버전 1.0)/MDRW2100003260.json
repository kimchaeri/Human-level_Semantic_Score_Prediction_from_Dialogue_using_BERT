      "utterance": [
        {
          "id": "MDRW2100003260.1.1",
          "form": "혹시 반려동물 키우세요?",
          "original_form": "혹시 반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210820 21:05"
        },
        {
          "id": "MDRW2100003260.1.2",
          "form": "저 앵무새키워요!!",
          "original_form": "저 앵무새키워요!!",
          "speaker_id": "2",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100003260.1.3",
          "form": "name1님은 반려동물이랑 함께 사시나요??",
          "original_form": "&name1&님은 반려동물이랑 함께 사시나요??",
          "speaker_id": "2",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100003260.1.4",
          "form": "우와 신기해요!",
          "original_form": "우와 신기해요!",
          "speaker_id": "1",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100003260.1.5",
          "form": "전 키우고 싶은데 가족이 알레르기가 있어요ㅠㅠ",
          "original_form": "전 키우고 싶은데 가족이 알레르기가 있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100003260.1.6",
          "form": "아아ㅠ",
          "original_form": "아아ㅠ",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100003260.1.7",
          "form": "어떤 동물 입양하고싶으세요?",
          "original_form": "어떤 동물 입양하고싶으세요?",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100003260.1.8",
          "form": "전 강아지나 고양이요!",
          "original_form": "전 강아지나 고양이요!",
          "speaker_id": "1",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100003260.1.9",
          "form": "친구네 강아지 대신 예뻐해줘요..",
          "original_form": "친구네 강아지 대신 예뻐해줘요..",
          "speaker_id": "1",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100003260.1.10",
          "form": "앜ㅋㅋㅋㅋ 저도 동물 다 좋아해서 강아지나 고양이도 키우고싶어요!!",
          "original_form": "앜ㅋㅋㅋㅋ 저도 동물 다 좋아해서 강아지나 고양이도 키우고싶어요!!",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100003260.1.11",
          "form": "길냥이들도 좋아요ㅠㅠ",
          "original_form": "길냥이들도 좋아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100003260.1.12",
          "form": "그쵸 저 어제 집에 오다가 길냥이 봤는데",
          "original_form": "그쵸 저 어제 집에 오다가 길냥이 봤는데",
          "speaker_id": "1",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100003260.1.13",
          "form": "단지 안에 맨날 돌아다니거든요 진짜 귀여워요",
          "original_form": "단지 안에 맨날 돌아다니거든요 진짜 귀여워요",
          "speaker_id": "1",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100003260.1.14",
          "form": "허얼",
          "original_form": "허얼",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100003260.1.15",
          "form": "저희아파트에도 두마리가 잔디에서 매일 누워있는데 너무 귀여워요",
          "original_form": "저희아파트에도 두마리가 잔디에서 매일 누워있는데 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100003260.1.16",
          "form": "진짜 귀여울 거 같아요ㅠㅠ",
          "original_form": "진짜 귀여울 거 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100003260.1.17",
          "form": "뤼님 그럼 앵무새 말도 잘 해요?",
          "original_form": "뤼님 그럼 앵무새 말도 잘 해요?",
          "speaker_id": "1",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100003260.1.18",
          "form": "맨날 듣는 질문이실 거 같긴 한데ㅎㅎ",
          "original_form": "맨날 듣는 질문이실 거 같긴 한데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100003260.1.19",
          "form": "저희 name3는 중소형앵무라 말은 해도 좀 어눌하게 해요 ㅋㅋㅋㅋㅋ",
          "original_form": "저희 &name3&는 중소형앵무라 말은 해도 좀 어눌하게 해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100003260.1.20",
          "form": "어 맞아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어 맞아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100003260.1.21",
          "form": "요즘은 안녕 가르치고있어요",
          "original_form": "요즘은 안녕 가르치고있어요",
          "speaker_id": "2",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100003260.1.22",
          "form": "헉 너무 신기해요",
          "original_form": "헉 너무 신기해요",
          "speaker_id": "1",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100003260.1.23",
          "form": "저 앵무새 실제로 본 적도 없어요ㅠㅠ",
          "original_form": "저 앵무새 실제로 본 적도 없어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100003260.1.24",
          "form": "허어어얼 저희집 name3 보여드리고싶다....",
          "original_form": "허어어얼 저희집 &name3& 보여드리고싶다....",
          "speaker_id": "2",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100003260.1.25",
          "form": "완전 껌딱지예요..",
          "original_form": "완전 껌딱지예요..",
          "speaker_id": "2",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100003260.1.26",
          "form": "저두 너무 보고싶어요",
          "original_form": "저두 너무 보고싶어요",
          "speaker_id": "1",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100003260.1.27",
          "form": "세상에 진짜 귀엽겠다",
          "original_form": "세상에 진짜 귀엽겠다",
          "speaker_id": "1",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100003260.1.28",
          "form": "사진이 안가서 넘 슬프다 흑흑",
          "original_form": "사진이 안가서 넘 슬프다 흑흑",
          "speaker_id": "2",
          "time": "20210821 17:59"
        }
      ]
