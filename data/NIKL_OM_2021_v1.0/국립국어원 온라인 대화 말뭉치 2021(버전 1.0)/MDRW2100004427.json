      "utterance": [
        {
          "id": "MDRW2100004427.1.1",
          "form": "티비봐요?",
          "original_form": "티비봐요?",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004427.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004427.1.3",
          "form": "집에 있을 때는 텔레비 많이 보는 거 같아요",
          "original_form": "집에 있을 때는 텔레비 많이 보는 거 같아요",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004427.1.4",
          "form": "무슨 프로그램 좋아하세요",
          "original_form": "무슨 프로그램 좋아하세요",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004427.1.5",
          "form": "런닝맨이요",
          "original_form": "런닝맨이요",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004427.1.6",
          "form": "런닝맨 재밌죠",
          "original_form": "런닝맨 재밌죠",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004427.1.7",
          "form": "넷플릭스같은건안보시나요",
          "original_form": "넷플릭스같은건안보시나요",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004427.1.8",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004427.1.9",
          "form": "재밌는 프로 아시면 추천해주세요!",
          "original_form": "재밌는 프로 아시면 추천해주세요!",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004427.1.10",
          "form": "모가디슈 재밌다고 들었어요",
          "original_form": "모가디슈 재밌다고 들었어요",
          "speaker_id": "1",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004427.1.11",
          "form": "뭔지 모르는데 찾아봐야겠어요",
          "original_form": "뭔지 모르는데 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004427.1.12",
          "form": "저는 요즘 환승연애 보고있어요",
          "original_form": "저는 요즘 환승연애 보고있어요",
          "speaker_id": "2",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004427.1.13",
          "form": "넷플이요?",
          "original_form": "넷플이요?",
          "speaker_id": "1",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004427.1.14",
          "form": "이거는 티빙에서 볼 수 있어요 넷플릭스는 없구요",
          "original_form": "이거는 티빙에서 볼 수 있어요 넷플릭스는 없구요",
          "speaker_id": "2",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004427.1.15",
          "form": "하트시그널 같은 거 보신 적 있으신가요",
          "original_form": "하트시그널 같은 거 보신 적 있으신가요",
          "speaker_id": "2",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004427.1.16",
          "form": "짤만요",
          "original_form": "짤만요",
          "speaker_id": "1",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004427.1.17",
          "form": "재밌게 보셨으면 환승연애도 한 번 봐보세요",
          "original_form": "재밌게 보셨으면 환승연애도 한 번 봐보세요",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004427.1.18",
          "form": "저는 이게 더 재밌더라구요!",
          "original_form": "저는 이게 더 재밌더라구요!",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004427.1.19",
          "form": "봐야겠어요",
          "original_form": "봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 16:55"
        }
      ]
