      "utterance": [
        {
          "id": "MDRW2100011537.1.1",
          "form": "중고거래 자주하시나요",
          "original_form": "중고거래 자주하시나요",
          "speaker_id": "1",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011537.1.2",
          "form": "가끔하죠!",
          "original_form": "가끔하죠!",
          "speaker_id": "2",
          "time": "20210824 14:01"
        },
        {
          "id": "MDRW2100011537.1.3",
          "form": "당근이나 번개 이용해요",
          "original_form": "당근이나 번개 이용해요",
          "speaker_id": "2",
          "time": "20210824 14:01"
        },
        {
          "id": "MDRW2100011537.1.4",
          "form": "오 어떤물건 구매해보셨나요",
          "original_form": "오 어떤물건 구매해보셨나요",
          "speaker_id": "1",
          "time": "20210824 14:01"
        },
        {
          "id": "MDRW2100011537.1.5",
          "form": "중고 노트북이랑",
          "original_form": "중고 노트북이랑",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011537.1.6",
          "form": "아이돌 포카 정도 해봤네요!",
          "original_form": "아이돌 포카 정도 해봤네요!",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011537.1.7",
          "form": "근데 요즘은 직거래 하기 조금 꺼려지네요ㅠ",
          "original_form": "근데 요즘은 직거래 하기 조금 꺼려지네요ㅠ",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011537.1.8",
          "form": "맞아요 저도 그래서 직거래는 좀 피하는 편이긴합니다 ㅠㅠ",
          "original_form": "맞아요 저도 그래서 직거래는 좀 피하는 편이긴합니다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011537.1.9",
          "form": "중고거래는 사기도 조금 조심해야될것같아요",
          "original_form": "중고거래는 사기도 조금 조심해야될것같아요",
          "speaker_id": "1",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100011537.1.10",
          "form": "맞아요 사기도 조심",
          "original_form": "맞아요 사기도 조심",
          "speaker_id": "2",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011537.1.11",
          "form": "사기가 극성이라고 하는데 전 아직까지 겪진 않았어요 다행히도",
          "original_form": "사기가 극성이라고 하는데 전 아직까지 겪진 않았어요 다행히도",
          "speaker_id": "2",
          "time": "20210824 14:29"
        },
        {
          "id": "MDRW2100011537.1.12",
          "form": "네 저도요 계좌랑 폰번호 확인 필수더라구요",
          "original_form": "네 저도요 계좌랑 폰번호 확인 필수더라구요",
          "speaker_id": "1",
          "time": "20210824 14:29"
        },
        {
          "id": "MDRW2100011537.1.13",
          "form": "어떻게 보면 직거래를 선호하게 된게 사기 걱정되소",
          "original_form": "어떻게 보면 직거래를 선호하게 된게 사기 걱정되소",
          "speaker_id": "2",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011537.1.14",
          "form": "그랬었죠",
          "original_form": "그랬었죠",
          "speaker_id": "2",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011537.1.15",
          "form": "근데 지금은 둘다 잘 안하구있으니",
          "original_form": "근데 지금은 둘다 잘 안하구있으니",
          "speaker_id": "2",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011537.1.16",
          "form": "당근에 쓸만한게 가끔 뜨기도 하더라구요",
          "original_form": "당근에 쓸만한게 가끔 뜨기도 하더라구요",
          "speaker_id": "1",
          "time": "20210824 14:34"
        },
        {
          "id": "MDRW2100011537.1.17",
          "form": "당근이 직거래하기엔 좋긴하더라고요",
          "original_form": "당근이 직거래하기엔 좋긴하더라고요",
          "speaker_id": "2",
          "time": "20210824 14:41"
        },
        {
          "id": "MDRW2100011537.1.18",
          "form": "쓸만한것도 잘나오고용",
          "original_form": "쓸만한것도 잘나오고용",
          "speaker_id": "2",
          "time": "20210824 14:41"
        }
      ]
