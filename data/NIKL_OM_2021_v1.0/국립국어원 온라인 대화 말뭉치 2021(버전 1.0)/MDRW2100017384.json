      "utterance": [
        {
          "id": "MDRW2100017384.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017384.1.2",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "1",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017384.1.3",
          "form": "무교예요",
          "original_form": "무교예요",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017384.1.4",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017384.1.5",
          "form": "게을러서 종교활동을 할 수가 없어요",
          "original_form": "게을러서 종교활동을 할 수가 없어요",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017384.1.6",
          "form": "절가본적있으세요?",
          "original_form": "절가본적있으세요?",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017384.1.7",
          "form": "불국사정도?",
          "original_form": "불국사정도?",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017384.1.8",
          "form": "산에 가면 간혹가요 막 찾아가지는 않구요",
          "original_form": "산에 가면 간혹가요 막 찾아가지는 않구요",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017384.1.9",
          "form": "저는 봉은사 가봤어요",
          "original_form": "저는 봉은사 가봤어요",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017384.1.10",
          "form": "봉은사면 서울?인가요?",
          "original_form": "봉은사면 서울?인가요?",
          "speaker_id": "1",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100017384.1.11",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017384.1.12",
          "form": "지나가기만했지 들어가 본 적은 없어요",
          "original_form": "지나가기만했지 들어가 본 적은 없어요",
          "speaker_id": "1",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017384.1.13",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "1",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017384.1.14",
          "form": "종소리 들으면 평온해져요",
          "original_form": "종소리 들으면 평온해져요",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017384.1.15",
          "form": "한번 가봐야겠어요 주변에서 맛난것도 먹고",
          "original_form": "한번 가봐야겠어요 주변에서 맛난것도 먹고",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017384.1.16",
          "form": "한번 가보세요",
          "original_form": "한번 가보세요",
          "speaker_id": "2",
          "time": "20210826 19:44"
        }
