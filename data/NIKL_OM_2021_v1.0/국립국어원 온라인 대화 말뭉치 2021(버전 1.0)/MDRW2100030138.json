      "utterance": [
        {
          "id": "MDRW2100030138.1.1",
          "form": "ㅋㅋ결혼준비는",
          "original_form": "ㅋㅋ결혼준비는",
          "speaker_id": "1",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100030138.1.2",
          "form": "잘하고 계신가요",
          "original_form": "잘하고 계신가요",
          "speaker_id": "1",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100030138.1.3",
          "form": "남친이 해외유학중이라",
          "original_form": "남친이 해외유학중이라",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030138.1.4",
          "form": "아직 1년 4개월 남았어요",
          "original_form": "아직 1년 4개월 남았어요",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030138.1.5",
          "form": "아 그 뒤 결혼 예정인가요!",
          "original_form": "아 그 뒤 결혼 예정인가요!",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030138.1.6",
          "form": "네 예정은 그런데 혹시나 모르죠 뭐ㅠㅠ",
          "original_form": "네 예정은 그런데 혹시나 모르죠 뭐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100030138.1.7",
          "form": "그래서 그게 너무 걱정이에요",
          "original_form": "그래서 그게 너무 걱정이에요",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100030138.1.8",
          "form": "타이밍 안맞아서 결혼적령기 놓칠까봐요ㅠㅠ",
          "original_form": "타이밍 안맞아서 결혼적령기 놓칠까봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030138.1.9",
          "form": "아 잘 풀릴거에요",
          "original_form": "아 잘 풀릴거에요",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030138.1.10",
          "form": "걱정하지마세오",
          "original_form": "걱정하지마세오",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030138.1.11",
          "form": "그럴까요 감사합니다",
          "original_form": "그럴까요 감사합니다",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030138.1.12",
          "form": "인연이면 할거고 아니면 더 좋은 인연 오겠죠!!!!!",
          "original_form": "인연이면 할거고 아니면 더 좋은 인연 오겠죠!!!!!",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030138.1.13",
          "form": "그럼요 항상 긍정적인 사고",
          "original_form": "그럼요 항상 긍정적인 사고",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030138.1.14",
          "form": "와오 감사합니다 긍정파워",
          "original_form": "와오 감사합니다 긍정파워",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030138.1.15",
          "form": "결혼하면 맞벌이하시나요",
          "original_form": "결혼하면 맞벌이하시나요",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030138.1.16",
          "form": "네 그럴것같아요",
          "original_form": "네 그럴것같아요",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030138.1.17",
          "form": "전 전업주부 싫어요ㅠ",
          "original_form": "전 전업주부 싫어요ㅠ",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030138.1.18",
          "form": "설거지시러요ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "설거지시러요ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030138.1.19",
          "form": "식기세척기가 있습니다",
          "original_form": "식기세척기가 있습니다",
          "speaker_id": "1",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100030138.1.20",
          "form": "앜ㅋㅋ그러네요",
          "original_form": "앜ㅋㅋ그러네요",
          "speaker_id": "2",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030138.1.21",
          "form": "식기세척기 무조건 살거에요",
          "original_form": "식기세척기 무조건 살거에요",
          "speaker_id": "2",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030138.1.22",
          "form": "필수입니다",
          "original_form": "필수입니다",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030138.1.23",
          "form": "로봇청소기랑",
          "original_form": "로봇청소기랑",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030138.1.24",
          "form": "그러네요 건조기도요",
