      "utterance": [
        {
          "id": "MDRW2100021666.1.1",
          "form": "결혼하셧나요?",
          "original_form": "결혼하셧나요?",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021666.1.2",
          "form": "아녀",
          "original_form": "아녀",
          "speaker_id": "2",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021666.1.3",
          "form": "비혼주의신가요? 요즘 많던데..",
          "original_form": "비혼주의신가요? 요즘 많던데..",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021666.1.4",
          "form": "딱히 비혼주의는 아닌데요 그냥 마땅한 상대가 없어서요",
          "original_form": "딱히 비혼주의는 아닌데요 그냥 마땅한 상대가 없어서요",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021666.1.5",
          "form": "결혼 하셨나요?",
          "original_form": "결혼 하셨나요?",
          "speaker_id": "2",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100021666.1.6",
          "form": "저도 싱글이에요.. \r\n요즘은 비혼주의들이 많아서.. 결혼 물어보기 겁나더라구요..",
          "original_form": "저도 싱글이에요.. \r\n요즘은 비혼주의들이 많아서.. 결혼 물어보기 겁나더라구요..",
          "speaker_id": "1",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100021666.1.7",
          "form": "아 그렇죠 대부분은 ㅋㅋ 저도 뭐 딱히 꼭 해야겠단 생각은 없어서요",
          "original_form": "아 그렇죠 대부분은 ㅋㅋ 저도 뭐 딱히 꼭 해야겠단 생각은 없어서요",
          "speaker_id": "2",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100021666.1.8",
          "form": "하면 하고 못하면 말고",
          "original_form": "하면 하고 못하면 말고",
          "speaker_id": "2",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100021666.1.9",
          "form": "30대도 혼인률 50프로도 안되니까..",
          "original_form": "30대도 혼인률 50프로도 안되니까..",
          "speaker_id": "1",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100021666.1.10",
          "form": "많이 변했죠...",
          "original_form": "많이 변했죠...",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100021666.1.11",
          "form": "이상형은 어케되요?",
          "original_form": "이상형은 어케되요?",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100021666.1.12",
          "form": "잘생기고 키큰남자요 ㅋㅋ",
          "original_form": "잘생기고 키큰남자요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100021666.1.13",
          "form": "못생기고 키크면 ng인가여? ㅋㅋㅋ",
          "original_form": "못생기고 키크면 ng인가여? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021666.1.14",
          "form": "얼마나 못생긴지에 따라서...?",
          "original_form": "얼마나 못생긴지에 따라서...?",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021666.1.15",
          "form": "근데 저는 키 엄청 커야되는데 ㅋㅋ 제가 키가 커서요",
          "original_form": "근데 저는 키 엄청 커야되는데 ㅋㅋ 제가 키가 커서요",
          "speaker_id": "2",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100021666.1.16",
          "form": "아.. 그럼 커야겠네요...\r\n아무래도..",
          "original_form": "아.. 그럼 커야겠네요...\r\n아무래도..",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021666.1.17",
          "form": "남자 깔창 10cm까진 용서가능? ㅋㅋㅋ",
          "original_form": "남자 깔창 10cm까진 용서가능? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021666.1.18",
          "form": "네 ㅋㅋㅋ 저희 집 사람들도 다 키가 커서",
          "original_form": "네 ㅋㅋㅋ 저희 집 사람들도 다 키가 커서",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021666.1.19",
          "form": "아뇨... 깔창 왜 끼죵",
          "original_form": "아뇨... 깔창 왜 끼죵",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021666.1.20",
          "form": "키 작아도 자격지심 없는 사람이 좋은데 ㅎㅎ",
          "original_form": "키 작아도 자격지심 없는 사람이 좋은데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021666.1.21",
          "form": "깔창끼는 거 자신감없어보이잖아요",
          "original_form": "깔창끼는 거 자신감없어보이잖아요",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021666.1.22",
          "form": "눈썹흐리면 그리는 거랑 비슷한거 아닌가여? ㅋㅋㅋㅋㅋ",
          "original_form": "눈썹흐리면 그리는 거랑 비슷한거 아닌가여? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100021666.1.23",
          "form": "그냥 메이크업 같은...",
          "original_form": "그냥 메이크업 같은...",
          "speaker_id": "1",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100021666.1.24",
          "form": "실예지만 신장 물어봐도 되나여?",
          "original_form": "실예지만 신장 물어봐도 되나여?",
          "speaker_id": "1",
          "time": "20210828 21:56"
        },
        {
