      "utterance": [
        {
          "id": "MDRW2100024506.1.1",
          "form": "응 막",
          "original_form": "응 막",
          "speaker_id": "2",
          "time": "20210831 12:07"
        },
        {
          "id": "MDRW2100024506.1.2",
          "form": "오븐돈가스?",
          "original_form": "오븐돈가스?",
          "speaker_id": "2",
          "time": "20210831 12:07"
        },
        {
          "id": "MDRW2100024506.1.3",
          "form": "느글? 느끼해서반쯤먹음ㅎ",
          "original_form": "느글? 느끼해서반쯤먹음ㅎ",
          "speaker_id": "2",
          "time": "20210831 12:07"
        },
        {
          "id": "MDRW2100024506.1.4",
          "form": "아침 김밥을 늦께먹어서 배는 터질거같아ㅋ",
          "original_form": "아침 김밥을 늦께먹어서 배는 터질거같아ㅋ",
          "speaker_id": "2",
          "time": "20210831 12:08"
        },
        {
          "id": "MDRW2100024506.1.5",
          "form": "점심 준비하나?",
          "original_form": "점심 준비하나?",
          "speaker_id": "2",
          "time": "20210831 12:08"
        },
        {
          "id": "MDRW2100024506.1.6",
          "form": "온븐돈까스 맛있을거같은데",
          "original_form": "온븐돈까스 맛있을거같은데",
          "speaker_id": "1",
          "time": "20210831 12:08"
        },
        {
          "id": "MDRW2100024506.1.7",
          "form": "느끼하다니 ㅜㅜ",
          "original_form": "느끼하다니 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 12:08"
        },
        {
          "id": "MDRW2100024506.1.8",
          "form": "느끼하더라\r\n기름 덜뺏나",
          "original_form": "느끼하더라\r\n기름 덜뺏나",
          "speaker_id": "2",
          "time": "20210831 12:09"
        },
        {
          "id": "MDRW2100024506.1.9",
          "form": "소스 끼얹어서 바삭함도없고\r\n소스는 역시 찍먹ㅋㅋㅋ",
          "original_form": "소스 끼얹어서 바삭함도없고\r\n소스는 역시 찍먹ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:09"
        },
        {
          "id": "MDRW2100024506.1.10",
          "form": "바삭하게 튀기면 소스부어도 맛나는디",
          "original_form": "바삭하게 튀기면 소스부어도 맛나는디",
          "speaker_id": "1",
          "time": "20210831 12:09"
        },
        {
          "id": "MDRW2100024506.1.11",
          "form": "거기서 조리를 잘못한듯",
          "original_form": "거기서 조리를 잘못한듯",
          "speaker_id": "1",
          "time": "20210831 12:10"
        },
        {
          "id": "MDRW2100024506.1.12",
          "form": "ㅎㅎ단체식이라ㅎㅎ",
          "original_form": "ㅎㅎ단체식이라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 12:10"
        },
        {
          "id": "MDRW2100024506.1.13",
          "form": "오늘은 느끼해서 샐러드 같이나와 좋았어ㅎㅎ",
          "original_form": "오늘은 느끼해서 샐러드 같이나와 좋았어ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 12:10"
        },
        {
          "id": "MDRW2100024506.1.14",
          "form": "돈까스는 샐러드지! 그래도 맛있었겠다 누가 차려주는 밥이 젤 맛있어",
          "original_form": "돈까스는 샐러드지! 그래도 맛있었겠다 누가 차려주는 밥이 젤 맛있어",
          "speaker_id": "1",
          "time": "20210831 12:11"
        },
        {
          "id": "MDRW2100024506.1.15",
          "form": "맞어 챙겨먹기 귀찮",
          "original_form": "맞어 챙겨먹기 귀찮",
          "speaker_id": "2",
          "time": "20210831 12:14"
        },
        {
          "id": "MDRW2100024506.1.16",
          "form": "넌 점심 먹은겨?",
          "original_form": "넌 점심 먹은겨?",
          "speaker_id": "2",
          "time": "20210831 12:14"
        },
        {
          "id": "MDRW2100024506.1.17",
          "form": "아니 아직 ㅋㅋ 아침 늦게 먹어서 배가 덜 고픔",
          "original_form": "아니 아직 ㅋㅋ 아침 늦게 먹어서 배가 덜 고픔",
          "speaker_id": "1",
          "time": "20210831 12:14"
        },
        {
          "id": "MDRW2100024506.1.18",
          "form": "맞어 미뤄져ㅎ 시간 딱딱 안먹어져ㅎ",
          "original_form": "맞어 미뤄져ㅎ 시간 딱딱 안먹어져ㅎ",
          "speaker_id": "2",
          "time": "20210831 12:21"
        },
        {
          "id": "MDRW2100024506.1.19",
          "form": "맛있는거 먹고싶네 저녁때 뭐먹지",
          "original_form": "맛있는거 먹고싶네 저녁때 뭐먹지",
          "speaker_id": "1",
          "time": "20210831 12:22"
        },
        {
          "id": "MDRW2100024506.1.20",
          "form": "김치볶음밥?",
          "original_form": "김치볶음밥?",
          "speaker_id": "2",
          "time": "20210831 12:24"
        },
        {
          "id": "MDRW2100024506.1.21",
          "form": "돼지고기 넣고ㅋ",
          "original_form": "돼지고기 넣고ㅋ",
          "speaker_id": "2",
          "time": "20210831 12:24"
        },
        {
          "id": "MDRW2100024506.1.22",
          "form": "집에 돼지고기없어서 참치넣고 만들면되겠다",
          "original_form": "집에 돼지고기없어서 참치넣고 만들면되겠다",
          "speaker_id": "1",
          "time": "20210831 12:24"
        },
        {
          "id": "MDRW2100024506.1.23",
          "form": "참치김치볶음밥",
          "original_form": "참치김치볶음밥",
          "speaker_id": "1",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100024506.1.24",
          "form": "응ㅋ",
          "original_form": "응ㅋ",
          "speaker_id": "2",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100024506.1.25",
          "form": "것도 맛있지~~",
          "original_form": "것도 맛있지~~",
          "speaker_id": "2",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100024506.1.26",
          "form": "집에 있는 냉동시킨 옥수수알이랑 파 양파 김치 참치 넣고 볶아서 먹고싶네",
          "original_form": "집에 있는 냉동시킨 옥수수알이랑 파 양파 김치 참치 넣고 볶아서 먹고싶네",
          "speaker_id": "1",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100024506.1.27",
          "form": "벌써 침나옴",
          "original_form": "벌써 침나옴",
