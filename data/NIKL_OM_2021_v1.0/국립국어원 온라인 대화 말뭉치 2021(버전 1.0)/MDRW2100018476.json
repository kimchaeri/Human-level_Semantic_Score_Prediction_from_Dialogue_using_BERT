      "utterance": [
        {
          "id": "MDRW2100018476.1.1",
          "form": "밤이 되니까 야식 먹고 싶네요...",
          "original_form": "밤이 되니까 야식 먹고 싶네요...",
          "speaker_id": "2",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018476.1.2",
          "form": "맞아요! 야식은 꿀맛이죠ㅠㅠ",
          "original_form": "맞아요! 야식은 꿀맛이죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018476.1.3",
          "form": "야식하면 치킨이나 피바 아니겠습니까 ㅎㅎ",
          "original_form": "야식하면 치킨이나 피바 아니겠습니까 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018476.1.4",
          "form": "인정합니다ㅋㅋ 저 치킨 되게 좋아해요",
          "original_form": "인정합니다ㅋㅋ 저 치킨 되게 좋아해요",
          "speaker_id": "1",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018476.1.5",
          "form": "양념치킨 시켜먹는게 삶의 낙입니다",
          "original_form": "양념치킨 시켜먹는게 삶의 낙입니다",
          "speaker_id": "1",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018476.1.6",
          "form": "맞아요 요즘 야식은 시켜먹는게 트렌드죠",
          "original_form": "맞아요 요즘 야식은 시켜먹는게 트렌드죠",
          "speaker_id": "2",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018476.1.7",
          "form": "배달음식은 아무래도 편해서 좋더라구요",
          "original_form": "배달음식은 아무래도 편해서 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018476.1.8",
          "form": "맞아요 야식은 간편하게 먹어야 제맛이니까요",
          "original_form": "맞아요 야식은 간편하게 먹어야 제맛이니까요",
          "speaker_id": "1",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018476.1.9",
          "form": "내일 점심엔 뭐먹을지 벌써부터 생각해놔야겠어요 ㅎ",
          "original_form": "내일 점심엔 뭐먹을지 벌써부터 생각해놔야겠어요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 02:38"
        },
        {
          "id": "MDRW2100018476.1.10",
          "form": "앗 빠르시네요ㅋㅋㅋㅋ 좋네요 저도 생각 해놓을래요",
          "original_form": "앗 빠르시네요ㅋㅋㅋㅋ 좋네요 저도 생각 해놓을래요",
          "speaker_id": "2",
          "time": "20210827 02:38"
        },
        {
          "id": "MDRW2100018476.1.11",
          "form": "저는... 음 점심으로 카레 먹으려구요",
          "original_form": "저는... 음 점심으로 카레 먹으려구요",
          "speaker_id": "2",
          "time": "20210827 02:38"
        },
        {
          "id": "MDRW2100018476.1.12",
          "form": "카레 좋아하세요?",
          "original_form": "카레 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 02:39"
        },
        {
          "id": "MDRW2100018476.1.13",
          "form": "네! 카레도 맛있죠",
          "original_form": "네! 카레도 맛있죠",
          "speaker_id": "1",
          "time": "20210827 02:39"
        },
        {
          "id": "MDRW2100018476.1.14",
          "form": "카레라면 갑자기 땡기네요 ㅋㅋ",
          "original_form": "카레라면 갑자기 땡기네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:39"
        },
        {
          "id": "MDRW2100018476.1.15",
          "form": "와 그러네요 야밤에 먹고싶은거 천지입니다ㅜㅜ",
          "original_form": "와 그러네요 야밤에 먹고싶은거 천지입니다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 02:39"
        },
        {
          "id": "MDRW2100018476.1.16",
          "form": "카레랑 짜장 중 뭐가 좋으세요?",
          "original_form": "카레랑 짜장 중 뭐가 좋으세요?",
          "speaker_id": "2",
          "time": "20210827 02:39"
        },
        {
          "id": "MDRW2100018476.1.17",
          "form": "당연히 카레 아니겠습니까?!",
          "original_form": "당연히 카레 아니겠습니까?!",
          "speaker_id": "1",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018476.1.18",
          "form": "전 카레가 더 좋아용",
          "original_form": "전 카레가 더 좋아용",
          "speaker_id": "1",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018476.1.19",
          "form": "역시 뭘 먹을 줄 아시네요 ㅎㅎ",
          "original_form": "역시 뭘 먹을 줄 아시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018476.1.20",
          "form": "저도 카레가 도 좋아요",
          "original_form": "저도 카레가 도 좋아요",
          "speaker_id": "2",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018476.1.21",
          "form": "카레에 또띠아 조합은 최고입니다",
          "original_form": "카레에 또띠아 조합은 최고입니다",
          "speaker_id": "2",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018476.1.22",
          "form": "전 점심에 샐러드 먹으려고요",
          "original_form": "전 점심에 샐러드 먹으려고요",
          "speaker_id": "1",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018476.1.23",
          "form": "샐러드에 치킨텐더 추가요",
          "original_form": "샐러드에 치킨텐더 추가요",
          "speaker_id": "1",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018476.1.24",
          "form": "샐러드 좋아하시나봐요",
          "original_form": "샐러드 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018476.1.25",
          "form": "전 야채는 양보하겠습니다 ㅎㅎ",
          "original_form": "전 야채는 양보하겠습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 02:41"
        }
      ]
    }
