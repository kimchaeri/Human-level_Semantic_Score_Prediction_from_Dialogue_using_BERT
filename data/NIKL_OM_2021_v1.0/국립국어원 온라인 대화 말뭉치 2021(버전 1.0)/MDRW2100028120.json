        },
        {
          "id": "MDRW2100028120.1.2",
          "form": "안녕하세요~~~",
          "original_form": "안녕하세요~~~",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100028120.1.3",
          "form": "오늘 날씨가 오랜만에 좋아요~~",
          "original_form": "오늘 날씨가 오랜만에 좋아요~~",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100028120.1.4",
          "form": "빨래하셨나요?",
          "original_form": "빨래하셨나요?",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100028120.1.5",
          "form": "오 네~~",
          "original_form": "오 네~~",
          "speaker_id": "1",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100028120.1.6",
          "form": "오늘 이불빨래 했어요 ㅎㅎ",
          "original_form": "오늘 이불빨래 했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100028120.1.7",
          "form": "헐 마침 딱이네요",
          "original_form": "헐 마침 딱이네요",
          "speaker_id": "2",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100028120.1.8",
          "form": "저도 집에 두고온 이불이 생각나는데요",
          "original_form": "저도 집에 두고온 이불이 생각나는데요",
          "speaker_id": "2",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100028120.1.9",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028120.1.10",
          "form": "물론 저는 빨래를 하지 못했습니다",
          "original_form": "물론 저는 빨래를 하지 못했습니다",
          "speaker_id": "2",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028120.1.11",
          "form": "조만간 날 잡아서 해야겠어요",
          "original_form": "조만간 날 잡아서 해야겠어요",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028120.1.12",
          "form": "더 추워지기 전에 해두는게 좋죠!!",
          "original_form": "더 추워지기 전에 해두는게 좋죠!!",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028120.1.13",
          "form": "코인 빨래방 가보신적 잏으세용?",
          "original_form": "코인 빨래방 가보신적 잏으세용?",
          "speaker_id": "2",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028120.1.14",
          "form": "전 코인빨래방 가는거 좋아해요",
          "original_form": "전 코인빨래방 가는거 좋아해요",
          "speaker_id": "2",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028120.1.15",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028120.1.16",
          "form": "오 한번 가봤어요!",
          "original_form": "오 한번 가봤어요!",
          "speaker_id": "1",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028120.1.17",
          "form": "기다리는 거 지루하지 않으세요?",
          "original_form": "기다리는 거 지루하지 않으세요?",
          "speaker_id": "1",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028120.1.18",
          "form": "앗 근데 저는 그 시간이 뭔가 젛은거같아요",
          "original_form": "앗 근데 저는 그 시간이 뭔가 젛은거같아요",
          "speaker_id": "2",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028120.1.19",
          "form": "아무것도 안하구 가만히 있능 시간",
          "original_form": "아무것도 안하구 가만히 있능 시간",
          "speaker_id": "2",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028120.1.20",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028120.1.21",
          "form": "오 약간의 여유 같은 건가요?",
          "original_form": "오 약간의 여유 같은 건가요?",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028120.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028120.1.23",
          "form": "거기서 그냥 노래들으면서",
          "original_form": "거기서 그냥 노래들으면서",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028120.1.24",
          "form": "시간 가길 기다리면 여유롭고 조은거같아용",
          "original_form": "시간 가길 기다리면 여유롭고 조은거같아용",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028120.1.25",
          "form": "뭔가 아무것도 안해도 되는 시간이라",
          "original_form": "뭔가 아무것도 안해도 되는 시간이라",
