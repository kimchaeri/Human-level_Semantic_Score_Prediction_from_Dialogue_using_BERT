      "utterance": [
        {
          "id": "MDRW2100015564.1.1",
          "form": "제일 좋아하는 음식!!",
          "original_form": "제일 좋아하는 음식!!",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015564.1.2",
          "form": "음 어려워요",
          "original_form": "음 어려워요",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015564.1.3",
          "form": "고기",
          "original_form": "고기",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015564.1.4",
          "form": "종류 다요 ㅋㅋ",
          "original_form": "종류 다요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015564.1.5",
          "form": "달달이 디저트",
          "original_form": "달달이 디저트",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015564.1.6",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015564.1.7",
          "form": "다 너무 맛잇죠",
          "original_form": "다 너무 맛잇죠",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015564.1.8",
          "form": "무슨고기 좋아하세요?",
          "original_form": "무슨고기 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015564.1.9",
          "form": "돼지고기요",
          "original_form": "돼지고기요",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015564.1.10",
          "form": "무슨고기 좋아해요?",
          "original_form": "무슨고기 좋아해요?",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015564.1.11",
          "form": "저는 돌아가며 꽂히는 것 같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 돌아가며 꽂히는 것 같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015564.1.12",
          "form": "한동안 양꼬치에 미쳤었는데 요즘은 오리고기가 그렇게 좋아요 ㅋㅋㅋㅋㅋ",
          "original_form": "한동안 양꼬치에 미쳤었는데 요즘은 오리고기가 그렇게 좋아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015564.1.13",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015564.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015564.1.15",
          "form": "저 막 꼬치를 몇십꼬치나 ㅋㅋ",
          "original_form": "저 막 꼬치를 몇십꼬치나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015564.1.16",
          "form": "앙 조그매서 막먹었죠",
          "original_form": "앙 조그매서 막먹었죠",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015564.1.17",
          "form": "양꼬치 그 쯔란에 딱 ㅋㅋ",
          "original_form": "양꼬치 그 쯔란에 딱 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015564.1.18",
          "form": "맞아요 양꼬치 진짜 한도끝도없이 들어가죠 ㅠㅠ",
          "original_form": "맞아요 양꼬치 진짜 한도끝도없이 들어가죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015564.1.19",
          "form": "마늘도 꽂아서 같이 구워먹고 ㅠㅠ",
          "original_form": "마늘도 꽂아서 같이 구워먹고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015564.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015564.1.21",
          "form": "뭐좀 아시네요 ㅋㅋ",
          "original_form": "뭐좀 아시네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015564.1.22",
          "form": "그래서 먹다보면 돈이 ㅡㅡ",
          "original_form": "그래서 먹다보면 돈이 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015564.1.23",
          "form": "무제한 가야해요",
          "original_form": "무제한 가야해요",
          "speaker_id": "1",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015564.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 무제한 안가면 감당할수없는금액나옴....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 무제한 안가면 감당할수없는금액나옴....",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015564.1.25",
          "form": "클나요",
          "original_form": "클나요",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015564.1.26",
          "form": "양꼬치는",
          "original_form": "양꼬치는",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015564.1.27",
          "form": "문제가 집에서 못해요",
          "original_form": "문제가 집에서 못해요",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015564.1.28",
          "form": "냄새가 ㅋㅌ했다가 집이 중국집 됐어요 ㅜㅜ",
          "original_form": "냄새가 ㅋㅌ했다가 집이 중국집 됐어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015564.1.29",
          "form": "양고기는 냄새가 냄새가....",
          "original_form": "양고기는 냄새가 냄새가....",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015564.1.30",
          "form": "가게에서 먹고오기만해도 옷에 밴 냄새 장난아니잖아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "가게에서 먹고오기만해도 옷에 밴 냄새 장난아니잖아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015564.1.31",
          "form": "네 그걸모르고 집에서 하다 ㅋㅋ",
          "original_form": "네 그걸모르고 집에서 하다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015564.1.32",
          "form": "완전 난리였어요",
          "original_form": "완전 난리였어요",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015564.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ상상이 갑니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ상상이 갑니다",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015564.1.34",
          "form": "양고기는 역시 밖에서 사먹는걸로...!!",
          "original_form": "양고기는 역시 밖에서 사먹는걸로...!!",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015564.1.35",
          "form": "그럼요 ㅎㅎ",
          "original_form": "그럼요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015564.1.36",
          "form": "집은 지지 해요",
          "original_form": "집은 지지 해요",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015564.1.37",
          "form": "그런데 코로나라 외식도 쉽지 않아서ㅠㅠ",
          "original_form": "그런데 코로나라 외식도 쉽지 않아서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015564.1.38",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015564.1.39",
          "form": "못가고 있어요",
          "original_form": "못가고 있어요",
          "speaker_id": "1",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015564.1.40",
          "form": "양꼬치 다 구워서 배달해주시면 그걸 후라이팬에 살짝만 구워서먹고 그랫어요",
          "original_form": "양꼬치 다 구워서 배달해주시면 그걸 후라이팬에 살짝만 구워서먹고 그랫어요",
          "speaker_id": "2",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015564.1.41",
          "form": "낮에 애매하게 가볼까봐요 ㅋㅋ",
          "original_form": "낮에 애매하게 가볼까봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015564.1.42",
          "form": "아 좋은데요",
          "original_form": "아 좋은데요",
          "speaker_id": "1",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015564.1.43",
          "form": "배달되는 양꼬치집을 찾아보시는것도 방법일듯!!",
          "original_form": "배달되는 양꼬치집을 찾아보시는것도 방법일듯!!",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015564.1.44",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015564.1.45",
          "form": "가게에서 바로먹는 그맛은 아니지만 그래도 괜찮앗엉ㅅ",
          "original_form": "가게에서 바로먹는 그맛은 아니지만 그래도 괜찮앗엉ㅅ",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015564.1.46",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015564.1.47",
          "form": "네 ㅎㅎ 그럼요 양꼬치는 맛나죠",
          "original_form": "네 ㅎㅎ 그럼요 양꼬치는 맛나죠",
          "speaker_id": "1",
