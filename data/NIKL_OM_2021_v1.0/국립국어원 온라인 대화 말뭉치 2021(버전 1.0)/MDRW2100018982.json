      "utterance": [
        {
          "id": "MDRW2100018982.1.1",
          "form": "맞아요ㅜㅜ 돈 더주더라도 정가구매하는게",
          "original_form": "맞아요ㅜㅜ 돈 더주더라도 정가구매하는게",
          "speaker_id": "1",
          "time": "20210827 14:16"
        },
        {
          "id": "MDRW2100018982.1.2",
          "form": "속 편하더라구요ㅜㅜ",
          "original_form": "속 편하더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 14:17"
        },
        {
          "id": "MDRW2100018982.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100018982.1.4",
          "form": "그리고 명품같은건 인터넷에서 사지말라고 하더라고요",
          "original_form": "그리고 명품같은건 인터넷에서 사지말라고 하더라고요",
          "speaker_id": "2",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100018982.1.5",
          "form": "병행수입 거진 다 짭이라고",
          "original_form": "병행수입 거진 다 짭이라고",
          "speaker_id": "2",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100018982.1.6",
          "form": "맞아요 홍콩꺼는 홍콩백화점도 거르래요",
          "original_form": "맞아요 홍콩꺼는 홍콩백화점도 거르래요",
          "speaker_id": "1",
          "time": "20210828 15:56"
        },
        {
          "id": "MDRW2100018982.1.7",
          "form": "헉 홍콩백화점도요?",
          "original_form": "헉 홍콩백화점도요?",
          "speaker_id": "2",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100018982.1.8",
          "form": "납품되는게 짭인가봐요 백화점인데",
          "original_form": "납품되는게 짭인가봐요 백화점인데",
          "speaker_id": "2",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100018982.1.9",
          "form": "그거까지는 모르겠어요ㅜㅜ 아마 그 백화점 영수증같이 위조하는게 아닐까싶어요",
          "original_form": "그거까지는 모르겠어요ㅜㅜ 아마 그 백화점 영수증같이 위조하는게 아닐까싶어요",
          "speaker_id": "1",
          "time": "20210829 14:47"
        },
        {
          "id": "MDRW2100018982.1.10",
          "form": "어우.. 마음놓고 못사겠네요",
          "original_form": "어우.. 마음놓고 못사겠네요",
          "speaker_id": "2",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100018982.1.11",
          "form": "예전에 아울렛에도 한번 짝퉁판다고 난리였잖아요",
          "original_form": "예전에 아울렛에도 한번 짝퉁판다고 난리였잖아요",
          "speaker_id": "2",
          "time": "20210829 20:59"
        },
        {
          "id": "MDRW2100018982.1.12",
          "form": "헐 그런일도 있었어요? 어느브랜드에요?",
          "original_form": "헐 그런일도 있었어요? 어느브랜드에요?",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100018982.1.13",
          "form": "지금 찾는데 가방브랜드라 프라다로 기억하는데 기사가 없네용ㅠ 기사는 몽클레르 뿐.. 커뮤에서도 당시 핫했는데 14쯤",
          "original_form": "지금 찾는데 가방브랜드라 프라다로 기억하는데 기사가 없네용ㅠ 기사는 몽클레르 뿐.. 커뮤에서도 당시 핫했는데 14쯤",
          "speaker_id": "2",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100018982.1.14",
          "form": "암튼 그래서 백화점만 가는게..",
          "original_form": "암튼 그래서 백화점만 가는게..",
          "speaker_id": "2",
          "time": "20210830 00:32"
        },
        {
          "id": "MDRW2100018982.1.15",
          "form": "가격도 비싸면서ㅠㅠ",
          "original_form": "가격도 비싸면서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 00:32"
        },
        {
          "id": "MDRW2100018982.1.16",
          "form": "헐ㅜㅜㅜ 프라다라니...\r\n그러니까요 인터넷 병행도 믿을수없으니까 백화점이 마음편해요ㅜㅜ",
          "original_form": "헐ㅜㅜㅜ 프라다라니...\r\n그러니까요 인터넷 병행도 믿을수없으니까 백화점이 마음편해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 10:54"
        },
        {
          "id": "MDRW2100018982.1.17",
          "form": "가격도 계속올라서 샤넬은 이제 못사겠어요ㅋㅋㅜㅜ",
          "original_form": "가격도 계속올라서 샤넬은 이제 못사겠어요ㅋㅋㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 10:55"
        },
        {
          "id": "MDRW2100018982.1.18",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210830 17:35"
        },
        {
          "id": "MDRW2100018982.1.19",
          "form": "샤넬은 거의 중고모닝값이던데요,,,?!",
          "original_form": "샤넬은 거의 중고모닝값이던데요,,,?!",
          "speaker_id": "2",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100018982.1.20",
          "form": "그럼에도 줄서는거 보면 와..",
          "original_form": "그럼에도 줄서는거 보면 와..",
          "speaker_id": "2",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100018982.1.21",
          "form": "맞아요....ㅋㅋ 진짜 에르메스값이더라구요",
          "original_form": "맞아요....ㅋㅋ 진짜 에르메스값이더라구요",
          "speaker_id": "1",
          "time": "20210830 18:23"
        },
        {
          "id": "MDRW2100018982.1.22",
          "form": "진짜 뭐 특별하다고...ㅠ",
          "original_form": "진짜 뭐 특별하다고...ㅠ",
          "speaker_id": "2",
          "time": "20210831 13:42"
        },
