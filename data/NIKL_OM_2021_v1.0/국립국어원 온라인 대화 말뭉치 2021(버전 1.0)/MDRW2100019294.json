      "utterance": [
        {
          "id": "MDRW2100019294.1.1",
          "form": "오늘 저녁엔 뭐 먹어?",
          "original_form": "오늘 저녁엔 뭐 먹어?",
          "speaker_id": "2",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019294.1.2",
          "form": "애들 1학기 마무리했고, 한 학기동안 고생했으니까 피자 파티하자 ㅎㅎ",
          "original_form": "애들 1학기 마무리했고, 한 학기동안 고생했으니까 피자 파티하자 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019294.1.3",
          "form": "그랴요~ 애들 먹고 싶은거로 먹자",
          "original_form": "그랴요~ 애들 먹고 싶은거로 먹자",
          "speaker_id": "2",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019294.1.4",
          "form": "코로나라 밖에 나가지도 못하고 안타까워서 ㅎㅎ",
          "original_form": "코로나라 밖에 나가지도 못하고 안타까워서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019294.1.5",
          "form": "맛있는 걸로 사줘",
          "original_form": "맛있는 걸로 사줘",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019294.1.6",
          "form": "애들이 좋아하는 피자 도미노지 뭐 ㅎㅎ",
          "original_form": "애들이 좋아하는 피자 도미노지 뭐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100019294.1.7",
          "form": "두 판 시켜서 실컷먹자",
          "original_form": "두 판 시켜서 실컷먹자",
          "speaker_id": "2",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100019294.1.8",
          "form": "이제 한판으로는 좀 모자르지? ㅋ",
          "original_form": "이제 한판으로는 좀 모자르지? ㅋ",
          "speaker_id": "1",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100019294.1.9",
          "form": "많이 모자라~ 애들 너무 잘먹어서",
          "original_form": "많이 모자라~ 애들 너무 잘먹어서",
          "speaker_id": "2",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100019294.1.10",
          "form": "알았어~ 신랑 올 때쯤 맞춰서 시켜야겠다",
          "original_form": "알았어~ 신랑 올 때쯤 맞춰서 시켜야겠다",
          "speaker_id": "1",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100019294.1.11",
          "form": "오랜만에 사이다도 시켜줘 ㅎㅎ",
          "original_form": "오랜만에 사이다도 시켜줘 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100019294.1.12",
          "form": "ㅎㅎ 알았어 엄청 좋아하겠네",
          "original_form": "ㅎㅎ 알았어 엄청 좋아하겠네",
          "speaker_id": "1",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100019294.1.13",
          "form": "피자+콜라 생각만 해도 군침도네",
          "original_form": "피자+콜라 생각만 해도 군침도네",
          "speaker_id": "2",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100019294.1.14",
          "form": "나도 ㅎㅎ 갑자기 배고프다",
          "original_form": "나도 ㅎㅎ 갑자기 배고프다",
