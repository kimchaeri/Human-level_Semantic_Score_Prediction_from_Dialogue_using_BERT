      "utterance": [
        {
          "id": "MDRW2100025782.1.1",
          "form": "요즘 비 너무 자주 오지 않나요? ㅠ",
          "original_form": "요즘 비 너무 자주 오지 않나요? ㅠ",
          "speaker_id": "2",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100025782.1.2",
          "form": "그죠 ㅠㅠ 한동안 빨래 말리기 힘들었어요",
          "original_form": "그죠 ㅠㅠ 한동안 빨래 말리기 힘들었어요",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025782.1.3",
          "form": "맞아요 저도 작년에 빨래로 고생했어요",
          "original_form": "맞아요 저도 작년에 빨래로 고생했어요",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025782.1.4",
          "form": "이번엔 제습기로 빨래 말렸어요!",
          "original_form": "이번엔 제습기로 빨래 말렸어요!",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025782.1.5",
          "form": "헉 제습기 좋으시겠다",
          "original_form": "헉 제습기 좋으시겠다",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100025782.1.6",
          "form": "저도 사고싶네요ㅠㅠ",
          "original_form": "저도 사고싶네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100025782.1.7",
          "form": "겨울에도 뜨끈 뽀송하게 잘사용한다던데",
          "original_form": "겨울에도 뜨끈 뽀송하게 잘사용한다던데",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025782.1.8",
          "form": "마련하고싶네요",
          "original_form": "마련하고싶네요",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025782.1.9",
          "form": "올해 산 것 중에 최고예요 ㅋㅋ",
          "original_form": "올해 산 것 중에 최고예요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025782.1.10",
          "form": "겨울은 아직 못써봤는데 진짜 추천해요",
          "original_form": "겨울은 아직 못써봤는데 진짜 추천해요",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025782.1.11",
          "form": "장마엔 필수...",
          "original_form": "장마엔 필수...",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025782.1.12",
          "form": "부러워요 곧 가을이라 전 그때까지 버텨야겠어요",
          "original_form": "부러워요 곧 가을이라 전 그때까지 버텨야겠어요",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025782.1.13",
          "form": "요즘 날씨가 선선해져서 금방 가을 올 것 같아요",
          "original_form": "요즘 날씨가 선선해져서 금방 가을 올 것 같아요",
          "speaker_id": "2",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025782.1.14",
          "form": "그죠! 가을 좋아하시나요?",
          "original_form": "그죠! 가을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025782.1.15",
          "form": "좋죠! ㅎㅎ 여기저기 다니기 좋아서요",
          "original_form": "좋죠! ㅎㅎ 여기저기 다니기 좋아서요",
          "speaker_id": "2",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025782.1.16",
          "form": "저두요! 여행가기 좋은날씨 너무 좋아요",
          "original_form": "저두요! 여행가기 좋은날씨 너무 좋아요",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025782.1.17",
          "form": "낙엽때문에 풍경도 좋구요",
          "original_form": "낙엽때문에 풍경도 좋구요",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025782.1.18",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025782.1.19",
          "form": "이번 가을엔 글램핑 가려구여!",
          "original_form": "이번 가을엔 글램핑 가려구여!",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025782.1.20",
          "form": "헉 너무 부러워요",
          "original_form": "헉 너무 부러워요",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025782.1.21",
          "form": "누구랑 가시나요?",
          "original_form": "누구랑 가시나요?",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025782.1.22",
          "form": "동네친구들이랑요 ㅎㅎ",
          "original_form": "동네친구들이랑요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:30"
        }
      ]
