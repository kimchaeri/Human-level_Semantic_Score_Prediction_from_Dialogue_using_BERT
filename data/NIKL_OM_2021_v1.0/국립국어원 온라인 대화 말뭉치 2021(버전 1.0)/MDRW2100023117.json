      "utterance": [
        {
          "id": "MDRW2100023117.1.1",
          "form": "엄청 고생하셨네요ㅠ",
          "original_form": "엄청 고생하셨네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023117.1.2",
          "form": "저도 공감하는게 대학교때 아버지가 다치셔서",
          "original_form": "저도 공감하는게 대학교때 아버지가 다치셔서",
          "speaker_id": "2",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023117.1.3",
          "form": "ㅎㅎ 다 그렇게 사는건줄요 ㅎㅎ",
          "original_form": "ㅎㅎ 다 그렇게 사는건줄요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023117.1.4",
          "form": "네넵",
          "original_form": "네넵",
          "speaker_id": "1",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023117.1.5",
          "form": "그때 보직을 제대로 못받으셨어요",
          "original_form": "그때 보직을 제대로 못받으셨어요",
          "speaker_id": "2",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023117.1.6",
          "form": "회사가 그래도 좋아서 다시복직했지만",
          "original_form": "회사가 그래도 좋아서 다시복직했지만",
          "speaker_id": "2",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023117.1.7",
          "form": "어머님이 다 하셨어요ㅠ 그때 2개 식당일하시면서 ㅠ",
          "original_form": "어머님이 다 하셨어요ㅠ 그때 2개 식당일하시면서 ㅠ",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100023117.1.8",
          "form": "철없는 저는 알바도안하고 그냥 용돈달라고하고ㅠ",
          "original_form": "철없는 저는 알바도안하고 그냥 용돈달라고하고ㅠ",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100023117.1.9",
          "form": "어렸잖아요 ㅎㅎㅎ",
          "original_form": "어렸잖아요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100023117.1.10",
          "form": "name2은 기억남는 알바 잇어요?",
          "original_form": "&name2&은 기억남는 알바 잇어요?",
          "speaker_id": "1",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100023117.1.11",
          "form": "ㅎㅎ 기억나는알바는 음...",
          "original_form": "ㅎㅎ 기억나는알바는 음...",
          "speaker_id": "2",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100023117.1.12",
          "form": "노가다요?",
          "original_form": "노가다요?",
          "speaker_id": "2",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100023117.1.13",
          "form": "그때도 대학때였는데ㅎ",
          "original_form": "그때도 대학때였는데ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023117.1.14",
          "form": "여자친구 선물산다고 그랬을꺼에요ㅎ",
          "original_form": "여자친구 선물산다고 그랬을꺼에요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023117.1.15",
          "form": "참 철없었네요ㅎㅎ",
          "original_form": "참 철없었네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023117.1.16",
          "form": "ㅎㅎㅎ 추억이죠",
          "original_form": "ㅎㅎㅎ 추억이죠",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023117.1.17",
          "form": "전 편의점 알바 제일 좋앗어요 ㅎㅎ",
          "original_form": "전 편의점 알바 제일 좋앗어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023117.1.18",
          "form": "전  편의점 밤알바 했엇거든요",
          "original_form": "전  편의점 밤알바 했엇거든요",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023117.1.19",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023117.1.20",
          "form": "편의점알바ㅎㅎ",
          "original_form": "편의점알바ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023117.1.21",
          "form": "택시  버스  기사님들ㅇ ㅣ 교대나 출근 하면서",
          "original_form": "택시  버스  기사님들ㅇ ㅣ 교대나 출근 하면서",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023117.1.22",
          "form": "어린데  고생한다고",
          "original_form": "어린데  고생한다고",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023117.1.23",
          "form": "우와ㅎ",
          "original_form": "우와ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023117.1.24",
          "form": "맛있는것도 사주시고",
          "original_form": "맛있는것도 사주시고",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023117.1.25",
          "form": "뭔지알겠어요ㅎ",
          "original_form": "뭔지알겠어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023117.1.26",
          "form": "수다도 떨다  가시구여 ㅎㅎ",
          "original_form": "수다도 떨다  가시구여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023117.1.27",
          "form": "딸. 처럼 생각하셨나봐요ㅎ",
          "original_form": "딸. 처럼 생각하셨나봐요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023117.1.28",
          "form": "자기집에  맛있는거  사가시면서 알바하다 먹으라고 제꺼도 포장해다 주시고 했어여ㅠㅎㅎ",
          "original_form": "자기집에  맛있는거  사가시면서 알바하다 먹으라고 제꺼도 포장해다 주시고 했어여ㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023117.1.29",
          "form": "우와 정이다 정말",
          "original_form": "우와 정이다 정말",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023117.1.30",
          "form": "그런사람들이 많아야해요ㅠ",
          "original_form": "그런사람들이 많아야해요ㅠ",
          "speaker_id": "2",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100023117.1.31",
          "form": "ㅎㅎㅎ  그러게요",
          "original_form": "ㅎㅎㅎ  그러게요",
          "speaker_id": "1",
          "time": "20210829 21:58"
        }
      ]
