      "utterance": [
        {
          "id": "MDRW2100037530.1.1",
          "form": "마자요 반숙 후라이 짱",
          "original_form": "마자요 반숙 후라이 짱",
          "speaker_id": "2",
          "time": "20210901 06:41"
        },
        {
          "id": "MDRW2100037530.1.2",
          "form": "점심엔 주로 뭐드세요?",
          "original_form": "점심엔 주로 뭐드세요?",
          "speaker_id": "1",
          "time": "20210901 06:42"
        },
        {
          "id": "MDRW2100037530.1.3",
          "form": "요즘 집에만 있으니까",
          "original_form": "요즘 집에만 있으니까",
          "speaker_id": "1",
          "time": "20210901 06:42"
        },
        {
          "id": "MDRW2100037530.1.4",
          "form": "메뉴고민도 지겹도",
          "original_form": "메뉴고민도 지겹도",
          "speaker_id": "1",
          "time": "20210901 06:42"
        },
        {
          "id": "MDRW2100037530.1.5",
          "form": "배달도 맨날 비슷한거만 먹게되고",
          "original_form": "배달도 맨날 비슷한거만 먹게되고",
          "speaker_id": "1",
          "time": "20210901 06:42"
        },
        {
          "id": "MDRW2100037530.1.6",
          "form": "저는 구내식당 있어서",
          "original_form": "저는 구내식당 있어서",
          "speaker_id": "2",
          "time": "20210901 06:42"
        },
        {
          "id": "MDRW2100037530.1.7",
          "form": "주는 대로",
          "original_form": "주는 대로",
          "speaker_id": "2",
          "time": "20210901 06:42"
        },
        {
          "id": "MDRW2100037530.1.8",
          "form": "우와ㅜ 그게 최고져....",
          "original_form": "우와ㅜ 그게 최고져....",
          "speaker_id": "1",
          "time": "20210901 06:42"
        },
        {
          "id": "MDRW2100037530.1.9",
          "form": "돈도 덜들고",
          "original_form": "돈도 덜들고",
          "speaker_id": "1",
          "time": "20210901 06:43"
        },
        {
          "id": "MDRW2100037530.1.10",
          "form": "영양도 챙기고",
          "original_form": "영양도 챙기고",
          "speaker_id": "1",
          "time": "20210901 06:43"
        },
        {
          "id": "MDRW2100037530.1.11",
          "form": "네 영양사님을 믿고있어요ㅋㅋㅋㅋ",
          "original_form": "네 영양사님을 믿고있어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 07:06"
        },
        {
          "id": "MDRW2100037530.1.12",
          "form": "구내식당 메뉴는 하나예요?!",
          "original_form": "구내식당 메뉴는 하나예요?!",
          "speaker_id": "1",
          "time": "20210901 07:11"
        },
        {
          "id": "MDRW2100037530.1.13",
          "form": "친구네 회사는 라면코너가 따로 있다고 하더라구요ㅎ",
          "original_form": "친구네 회사는 라면코너가 따로 있다고 하더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210901 07:11"
        },
        {
          "id": "MDRW2100037530.1.14",
          "form": "와 대박대박  네...급식이라... ㅎㅎ..",
          "original_form": "와 대박대박  네...급식이라... ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210901 08:33"
        },
        {
          "id": "MDRW2100037530.1.15",
          "form": "아?!아하~~~~ㅋㅋㅋㅋ",
          "original_form": "아?!아하~~~~ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100037530.1.16",
          "form": "아침에 계란두개가지고 에너지가 돼요?",
          "original_form": "아침에 계란두개가지고 에너지가 돼요?",
          "speaker_id": "1",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100037530.1.17",
          "form": "음 또 이런저런 간식도 주워먹고",
          "original_form": "음 또 이런저런 간식도 주워먹고",
          "speaker_id": "2",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100037530.1.18",
          "form": "적응되니 좀 적응되더라구요",
          "original_form": "적응되니 좀 적응되더라구요",
          "speaker_id": "2",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100037530.1.19",
          "form": "말이 뭐이래",
          "original_form": "말이 뭐이래",
          "speaker_id": "2",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100037530.1.20",
          "form": "ㅋㅋㅋㅋ두알에 적응했어요",
          "original_form": "ㅋㅋㅋㅋ두알에 적응했어요",
          "speaker_id": "2",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100037530.1.21",
          "form": "그럼 저녁도 학식먹어요?",
          "original_form": "그럼 저녁도 학식먹어요?",
          "speaker_id": "1",
          "time": "20210901 10:55"
        },
        {
          "id": "MDRW2100037530.1.22",
          "form": "저녁은 집밥 먹어요ㅋㅋㅋ",
          "original_form": "저녁은 집밥 먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:55"
        },
        {
          "id": "MDRW2100037530.1.23",
          "form": "다행이예요ㅋㅋㅋ저녁도",
          "original_form": "다행이예요ㅋㅋㅋ저녁도",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100037530.1.24",
          "form": "계란 먹으면 어쩌나",
          "original_form": "계란 먹으면 어쩌나",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100037530.1.25",
          "form": "근데 요즘 막 연애를 시작해서",
          "original_form": "근데 요즘 막 연애를 시작해서",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100037530.1.26",
          "form": "집밥말고",
          "original_form": "집밥말고",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100037530.1.27",
          "form": "어마마",
          "original_form": "어마마",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100037530.1.28",
          "form": "다른 거 먹으러 돌아다니니가",
          "original_form": "다른 거 먹으러 돌아다니니가",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100037530.1.29",
          "form": "좀 찌려고하네요",
          "original_form": "좀 찌려고하네요",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100037530.1.30",
          "form": "역시 외식은 살이 잘 찌나봐요",
          "original_form": "역시 외식은 살이 잘 찌나봐요",
          "speaker_id": "2",
          "time": "20210901 10:56"
        }
      ]
    }
  ]
