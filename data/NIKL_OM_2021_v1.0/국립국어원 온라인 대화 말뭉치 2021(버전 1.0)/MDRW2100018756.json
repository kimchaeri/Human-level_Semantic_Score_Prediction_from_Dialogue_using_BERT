      "utterance": [
        {
          "id": "MDRW2100018756.1.1",
          "form": "ㅋㅋㅋ가고싶은데는 딱히 없는데 예쁜 바다가서 스노쿨링해보고 싶다",
          "original_form": "ㅋㅋㅋ가고싶은데는 딱히 없는데 예쁜 바다가서 스노쿨링해보고 싶다",
          "speaker_id": "2",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100018756.1.2",
          "form": "아니면 스킨스쿠버?",
          "original_form": "아니면 스킨스쿠버?",
          "speaker_id": "2",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100018756.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100018756.1.4",
          "form": "재밌겠다 나도 해보고싶어ㅋㅋ\n난 해외! 이젠 그냥 아무데나 가고싶어ㅠㅠㅋㅋ",
          "original_form": "재밌겠다 나도 해보고싶어ㅋㅋ\n난 해외! 이젠 그냥 아무데나 가고싶어ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:46"
        },
        {
          "id": "MDRW2100018756.1.5",
          "form": "마자 이제 진짜 아무데나 가도 감지덕지지ㅋㅋㄱ",
          "original_form": "마자 이제 진짜 아무데나 가도 감지덕지지ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100018756.1.6",
          "form": "그래서 우리 11월에 놀긴하는거지?",
          "original_form": "그래서 우리 11월에 놀긴하는거지?",
          "speaker_id": "2",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100018756.1.7",
          "form": "11월에 경주든 다른거는 놀긴하지않을까?",
          "original_form": "11월에 경주든 다른거는 놀긴하지않을까?",
          "speaker_id": "1",
          "time": "20210827 13:01"
        },
        {
          "id": "MDRW2100018756.1.8",
          "form": "근데 풀빌라가기엔 금액이 부족하지않으려나?",
          "original_form": "근데 풀빌라가기엔 금액이 부족하지않으려나?",
          "speaker_id": "1",
          "time": "20210827 13:01"
        },
        {
          "id": "MDRW2100018756.1.9",
          "form": "일단 그럼 11월은 확정인거지?",
          "original_form": "일단 그럼 11월은 확정인거지?",
          "speaker_id": "2",
          "time": "20210827 13:03"
        },
        {
          "id": "MDRW2100018756.1.10",
          "form": "모자르면 더 모아야지뭐ㅠ",
          "original_form": "모자르면 더 모아야지뭐ㅠ",
          "speaker_id": "2",
          "time": "20210827 13:03"
        },
        {
          "id": "MDRW2100018756.1.11",
          "form": "112만원이네",
          "original_form": "112만원이네",
          "speaker_id": "1",
          "time": "20210827 13:03"
        },
        {
          "id": "MDRW2100018756.1.12",
          "form": "꽤되는군",
          "original_form": "꽤되는군",
          "speaker_id": "1",
          "time": "20210827 13:03"
        },
        {
          "id": "MDRW2100018756.1.13",
          "form": "와 짱많네",
          "original_form": "와 짱많네",
          "speaker_id": "2",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018756.1.14",
          "form": "돈은 넉넉하군ㅋㅋ",
          "original_form": "돈은 넉넉하군ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018756.1.15",
          "form": "경주말고 가고싶은데 있어?",
          "original_form": "경주말고 가고싶은데 있어?",
          "speaker_id": "1",
          "time": "20210827 13:06"
        },
        {
          "id": "MDRW2100018756.1.16",
          "form": "음..딱히없어ㅋㅋㅋ",
          "original_form": "음..딱히없어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100018756.1.17",
          "form": "ㅋㅋㅋ 그럼 경주가자",
          "original_form": "ㅋㅋㅋ 그럼 경주가자",
          "speaker_id": "1",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018756.1.18",
          "form": "너 차 갖고온다했나?",
          "original_form": "너 차 갖고온다했나?",
          "speaker_id": "1",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018756.1.19",
          "form": "ㅋㅋㅋ그래ㅋㅋ\n놀이공원만 안가면 돼ㅋㅋ",
          "original_form": "ㅋㅋㅋ그래ㅋㅋ\n놀이공원만 안가면 돼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018756.1.20",
          "form": "응 차 갖고갈까 생각중인데",
          "original_form": "응 차 갖고갈까 생각중인데",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018756.1.21",
          "form": "ㅋㅋㅋ 아맞다! 경주월드!",
          "original_form": "ㅋㅋㅋ 아맞다! 경주월드!",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018756.1.22",
          "form": "자꾸 너가 상기시켜주는데?ㅋㅋㅋ",
          "original_form": "자꾸 너가 상기시켜주는데?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018756.1.23",
          "form": "아..조용히해야지.",
          "original_form": "아..조용히해야지.",
