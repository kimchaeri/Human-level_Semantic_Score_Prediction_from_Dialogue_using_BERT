      "utterance": [
        {
          "id": "MDRW2100001476.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211016 00:25"
        },
        {
          "id": "MDRW2100001476.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211016 00:26"
        },
        {
          "id": "MDRW2100001476.1.3",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20211016 00:26"
        },
        {
          "id": "MDRW2100001476.1.4",
          "form": "먹고있어요",
          "original_form": "먹고있어요",
          "speaker_id": "1",
          "time": "20211016 00:28"
        },
        {
          "id": "MDRW2100001476.1.5",
          "form": "오늘의 점심메뉴는 무엇인가요?",
          "original_form": "오늘의 점심메뉴는 무엇인가요?",
          "speaker_id": "2",
          "time": "20211016 00:32"
        },
        {
          "id": "MDRW2100001476.1.6",
          "form": "한솥도시락 가서 도시락 포장해왔어요",
          "original_form": "한솥도시락 가서 도시락 포장해왔어요",
          "speaker_id": "1",
          "time": "20211016 00:32"
        },
        {
          "id": "MDRW2100001476.1.7",
          "form": "돈가스카레",
          "original_form": "돈가스카레",
          "speaker_id": "1",
          "time": "20211016 00:32"
        },
        {
          "id": "MDRW2100001476.1.8",
          "form": "한솥은 근본이죠..",
          "original_form": "한솥은 근본이죠..",
          "speaker_id": "2",
          "time": "20211016 00:33"
        },
        {
          "id": "MDRW2100001476.1.9",
          "form": "한솥은 뭔가 확실히 퀄리티 있는 맛은 아닌데",
          "original_form": "한솥은 뭔가 확실히 퀄리티 있는 맛은 아닌데",
          "speaker_id": "2",
          "time": "20211016 00:33"
        },
        {
          "id": "MDRW2100001476.1.10",
          "form": "엄청 맛있어요 ㅋㅋㅋㅋㅋ 희한하게",
          "original_form": "엄청 맛있어요 ㅋㅋㅋㅋㅋ 희한하게",
          "speaker_id": "2",
          "time": "20211016 00:33"
        },
        {
          "id": "MDRW2100001476.1.11",
          "form": "맨날 먹어도 안질려...!",
          "original_form": "맨날 먹어도 안질려...!",
          "speaker_id": "2",
          "time": "20211016 00:33"
        },
        {
          "id": "MDRW2100001476.1.12",
          "form": "한끼 가볍게 먹기는 딱이에요",
          "original_form": "한끼 가볍게 먹기는 딱이에요",
          "speaker_id": "1",
          "time": "20211016 00:33"
        },
        {
          "id": "MDRW2100001476.1.13",
          "form": "혼자서",
          "original_form": "혼자서",
          "speaker_id": "1",
          "time": "20211016 00:33"
        },
        {
          "id": "MDRW2100001476.1.14",
          "form": "아 혼밥중이시구나!",
          "original_form": "아 혼밥중이시구나!",
          "speaker_id": "2",
          "time": "20211016 00:34"
        },
        {
          "id": "MDRW2100001476.1.15",
          "form": "저는 오늘 뭐먹을지 고민이 되네요 하하",
          "original_form": "저는 오늘 뭐먹을지 고민이 되네요 하하",
          "speaker_id": "2",
          "time": "20211016 00:34"
        },
        {
          "id": "MDRW2100001476.1.16",
          "form": "저도 혼밥해야되는데",
          "original_form": "저도 혼밥해야되는데",
          "speaker_id": "2",
          "time": "20211016 00:34"
        },
        {
          "id": "MDRW2100001476.1.17",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211016 00:34"
        },
        {
          "id": "MDRW2100001476.1.18",
          "form": "굶지말고 일하세요 ㅎㅎ 먹고살자고 하는건데",
          "original_form": "굶지말고 일하세요 ㅎㅎ 먹고살자고 하는건데",
          "speaker_id": "1",
          "time": "20211016 00:35"
        },
        {
          "id": "MDRW2100001476.1.19",
          "form": "절대 굶지않죠",
          "original_form": "절대 굶지않죠",
          "speaker_id": "2",
          "time": "20211016 00:35"
        },
        {
          "id": "MDRW2100001476.1.20",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20211016 00:35"
        },
        {
          "id": "MDRW2100001476.1.21",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20211016 00:36"
        },
        {
          "id": "MDRW2100001476.1.22",
          "form": "다 먹고살자고 일하는건데..",
          "original_form": "다 먹고살자고 일하는건데..",
          "speaker_id": "2",
          "time": "20211016 00:36"
        },
        {
          "id": "MDRW2100001476.1.23",
          "form": "아 혹시 서브웨이 좋아하세요?",
          "original_form": "아 혹시 서브웨이 좋아하세요?",
          "speaker_id": "2",
          "time": "20211016 00:36"
        },
        {
          "id": "MDRW2100001476.1.24",
          "form": "간혹 가는데 메뉴 고르기가 까다로워서",
          "original_form": "간혹 가는데 메뉴 고르기가 까다로워서",
          "speaker_id": "1",
          "time": "20211016 00:37"
        },
        {
          "id": "MDRW2100001476.1.25",
          "form": "아하 맞아요",
          "original_form": "아하 맞아요",
          "speaker_id": "2",
          "time": "20211016 00:37"
        },
        {
          "id": "MDRW2100001476.1.26",
          "form": "가끔 특가로 소스까지 싹 정해주는 메뉴 뜨면 그건 사먹어요",
          "original_form": "가끔 특가로 소스까지 싹 정해주는 메뉴 뜨면 그건 사먹어요",
          "speaker_id": "1",
          "time": "20211016 00:37"
        },
        {
          "id": "MDRW2100001476.1.27",
          "form": "오 현명하시네요 ㅎㅎㅎ",
          "original_form": "오 현명하시네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211016 00:37"
        },
        {
          "id": "MDRW2100001476.1.28",
          "form": "쿠키도 드셔보셨어요?!",
          "original_form": "쿠키도 드셔보셨어요?!",
          "speaker_id": "2",
          "time": "20211016 00:37"
        },
        {
          "id": "MDRW2100001476.1.29",
          "form": "정말 맛있는데 ㅠㅠ 모르시는 분들이 많더라구요",
          "original_form": "정말 맛있는데 ㅠㅠ 모르시는 분들이 많더라구요",
          "speaker_id": "2",
          "time": "20211016 00:38"
        },
        {
          "id": "MDRW2100001476.1.30",
          "form": "쿠키는 5개 세트로 사먹어요 ㅎㅎ",
          "original_form": "쿠키는 5개 세트로 사먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211016 00:38"
        },
        {
          "id": "MDRW2100001476.1.31",
          "form": "6개..? 5개..?",
          "original_form": "6개..? 5개..?",
          "speaker_id": "1",
          "time": "20211016 00:38"
        },
        {
          "id": "MDRW2100001476.1.32",
          "form": "오 서브웨이 쿠키도 세트가 있나요?!",
          "original_form": "오 서브웨이 쿠키도 세트가 있나요?!",
          "speaker_id": "2",
          "time": "20211016 00:38"
        },
        {
          "id": "MDRW2100001476.1.33",
          "form": "왜 몰랐지!",
          "original_form": "왜 몰랐지!",
          "speaker_id": "2",
          "time": "20211016 00:38"
        },
        {
          "id": "MDRW2100001476.1.34",
          "form": "4갠가 5개 값에 하나 더 줘요",
          "original_form": "4갠가 5개 값에 하나 더 줘요",
          "speaker_id": "1",
          "time": "20211016 00:38"
        },
        {
          "id": "MDRW2100001476.1.35",
          "form": "쿠키가 부드러워서 좋더라구요",
          "original_form": "쿠키가 부드러워서 좋더라구요",
          "speaker_id": "1",
          "time": "20211016 00:38"
        },
        {
          "id": "MDRW2100001476.1.36",
          "form": "오 저보다 고수신데요?!",
          "original_form": "오 저보다 고수신데요?!",
          "speaker_id": "2",
          "time": "20211016 00:39"
        },
        {
          "id": "MDRW2100001476.1.37",
          "form": "다음에 그렇게 사먹어볼게요!",
          "original_form": "다음에 그렇게 사먹어볼게요!",
          "speaker_id": "2",
          "time": "20211016 00:39"
        }
      ]
    }
