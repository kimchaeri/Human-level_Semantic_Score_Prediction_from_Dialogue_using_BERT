      "utterance": [
        {
          "id": "MDRW2100045779.1.1",
          "form": "영화 자주보세요?",
          "original_form": "영화 자주보세요?",
          "speaker_id": "2",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045779.1.2",
          "form": "최근엔 자주 못봤는데 코로나 전엔 극장가서 자주봤어요",
          "original_form": "최근엔 자주 못봤는데 코로나 전엔 극장가서 자주봤어요",
          "speaker_id": "1",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045779.1.3",
          "form": "저도 혼영파라 주로 혼자봅니다",
          "original_form": "저도 혼영파라 주로 혼자봅니다",
          "speaker_id": "1",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045779.1.4",
          "form": "극장가는 게 확실히 재미있는 거 같아요",
          "original_form": "극장가는 게 확실히 재미있는 거 같아요",
          "speaker_id": "2",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100045779.1.5",
          "form": "집중도 잘되고",
          "original_form": "집중도 잘되고",
          "speaker_id": "2",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100045779.1.6",
          "form": "맞아요 집에서 보면 자꾸 딴짓하게돼서요",
          "original_form": "맞아요 집에서 보면 자꾸 딴짓하게돼서요",
          "speaker_id": "1",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045779.1.7",
          "form": "화면이나 음향도 그렇고",
          "original_form": "화면이나 음향도 그렇고",
          "speaker_id": "1",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045779.1.8",
          "form": "집에서는 영화 자주보시는 편이신가요?",
          "original_form": "집에서는 영화 자주보시는 편이신가요?",
          "speaker_id": "1",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045779.1.9",
          "form": "음 집에서는 봤던 거 봐요",
          "original_form": "음 집에서는 봤던 거 봐요",
          "speaker_id": "2",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100045779.1.10",
          "form": "그러시구나. 저는 일부러 집에서 영화볼땐 핸드폰 차단설정해놓고 봐요",
          "original_form": "그러시구나. 저는 일부러 집에서 영화볼땐 핸드폰 차단설정해놓고 봐요",
          "speaker_id": "1",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100045779.1.11",
          "form": "오 좋은데요?",
          "original_form": "오 좋은데요?",
          "speaker_id": "2",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100045779.1.12",
          "form": "그렇게 안하면 뭘보는지도 모르겠더라구요",
          "original_form": "그렇게 안하면 뭘보는지도 모르겠더라구요",
          "speaker_id": "1",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100045779.1.13",
          "form": "맞아요...휴대폰이 자꾸 번쩍거려서ㅋㅋㅋ",
          "original_form": "맞아요...휴대폰이 자꾸 번쩍거려서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100045779.1.14",
          "form": "집중력이 떨어져서 큰일이에요",
          "original_form": "집중력이 떨어져서 큰일이에요",
          "speaker_id": "1",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100045779.1.15",
          "form": "저도요...뇌가 늙나봐요ㅜㅜ",
          "original_form": "저도요...뇌가 늙나봐요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 07:12"
        },
        {
          "id": "MDRW2100045779.1.16",
          "form": "네 눈도 안좋아지고 뇌도 안좋아지고",
          "original_form": "네 눈도 안좋아지고 뇌도 안좋아지고",
          "speaker_id": "1",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100045779.1.17",
          "form": "그래도 없는 시절은 생각할 수 없네요",
          "original_form": "그래도 없는 시절은 생각할 수 없네요",
          "speaker_id": "1",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100045779.1.18",
          "form": "맞아요ㅜㅜㅜ",
          "original_form": "맞아요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 07:14"
        },
        {
          "id": "MDRW2100045779.1.19",
          "form": "그래도 눈 생각해서 줄이긴 해야겠어요",
          "original_form": "그래도 눈 생각해서 줄이긴 해야겠어요",
          "speaker_id": "2",
          "time": "20210908 07:14"
        },
        {
          "id": "MDRW2100045779.1.20",
          "form": "네 블루라이트 필터 해놨는데 안과에서 물어보니 아무소용없다고 ㅋㅋ",
          "original_form": "네 블루라이트 필터 해놨는데 안과에서 물어보니 아무소용없다고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:14"
        },
        {
          "id": "MDRW2100045779.1.21",
          "form": "맞아요ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100045779.1.22",
          "form": "플라시보ㅠㅠㅠ 열심히 했는데",
          "original_form": "플라시보ㅠㅠㅠ 열심히 했는데",
