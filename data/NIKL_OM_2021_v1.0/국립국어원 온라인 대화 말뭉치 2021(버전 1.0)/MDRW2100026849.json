      "utterance": [
        {
          "id": "MDRW2100026849.1.1",
          "form": "종교도 신박하네요",
          "original_form": "종교도 신박하네요",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026849.1.2",
          "form": "저는...사대천왕 목소리를 믿어요",
          "original_form": "저는...사대천왕 목소리를 믿어요",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026849.1.3",
          "form": "신박하죠 도믿맨같이",
          "original_form": "신박하죠 도믿맨같이",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026849.1.4",
          "form": "아  진짜 그러실건가요",
          "original_form": "아  진짜 그러실건가요",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026849.1.5",
          "form": "그분들의 목소리로 더빙된 계시를 들으면",
          "original_form": "그분들의 목소리로 더빙된 계시를 들으면",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026849.1.6",
          "form": "아 그럼요그럼요",
          "original_form": "아 그럼요그럼요",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026849.1.7",
          "form": "사대천왕교가 있나요?예?",
          "original_form": "사대천왕교가 있나요?예?",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026849.1.8",
          "form": "저만 믿어요 저만 주교에요",
          "original_form": "저만 믿어요 저만 주교에요",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026849.1.9",
          "form": "그리고 신도들은 받지않아요",
          "original_form": "그리고 신도들은 받지않아요",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026849.1.10",
          "form": "네....알겠어요 신도가 한명뿐인 종교",
          "original_form": "네....알겠어요 신도가 한명뿐인 종교",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026849.1.11",
          "form": "동담불가",
          "original_form": "동담불가",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026849.1.12",
          "form": "의미 있는지",
          "original_form": "의미 있는지",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026849.1.13",
          "form": "님은 종교가 있나요??",
          "original_form": "님은 종교가 있나요??",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026849.1.14",
          "form": "저는 무신론자예요",
          "original_form": "저는 무신론자예요",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026849.1.15",
          "form": "무신코우",
          "original_form": "무신코우",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026849.1.16",
          "form": "아무래도 제가 신인 편",
          "original_form": "아무래도 제가 신인 편",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026849.1.17",
          "form": "걔는 깔이라서 카운트 안 해요",
          "original_form": "걔는 깔이라서 카운트 안 해요",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026849.1.18",
          "form": "신을 깔로 두시는군요",
          "original_form": "신을 깔로 두시는군요",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026849.1.19",
          "form": "역시 본인이 신인 편",
          "original_form": "역시 본인이 신인 편",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026849.1.20",
          "form": "저희 둘다 기독교 집안 아닌가요",
          "original_form": "저희 둘다 기독교 집안 아닌가요",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026849.1.21",
          "form": "끝내주네요",
          "original_form": "끝내주네요",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026849.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 신성모독 오진다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 신성모독 오진다",
          "speaker_id": "1",
          "time": "20210902 00:40"
        }
