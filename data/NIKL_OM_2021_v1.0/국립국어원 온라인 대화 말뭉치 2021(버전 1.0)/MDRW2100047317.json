      "utterance": [
        {
          "id": "MDRW2100047317.1.1",
          "form": "안녕하세요. 지금 대화가능 하신가요?",
          "original_form": "안녕하세요. 지금 대화가능 하신가요?",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100047317.1.2",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100047317.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100047317.1.4",
          "form": "뭐 키우시나요 ?",
          "original_form": "뭐 키우시나요 ?",
          "speaker_id": "2",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100047317.1.5",
          "form": "전 도마뱀 두마리 키워요",
          "original_form": "전 도마뱀 두마리 키워요",
          "speaker_id": "2",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100047317.1.6",
          "form": "전 옛날에 개을 키우고 지금은 안키워요 ㅠ",
          "original_form": "전 옛날에 개을 키우고 지금은 안키워요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100047317.1.7",
          "form": "개가 하늘나라로 간 이후엔 안키우게 되네요",
          "original_form": "개가 하늘나라로 간 이후엔 안키우게 되네요",
          "speaker_id": "1",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100047317.1.8",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100047317.1.9",
          "form": "도마뱀의 종류는 뭔가요?",
          "original_form": "도마뱀의 종류는 뭔가요?",
          "speaker_id": "1",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100047317.1.10",
          "form": "저도 그게 항상 걱정이예요",
          "original_form": "저도 그게 항상 걱정이예요",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100047317.1.11",
          "form": "키우면서도 그날이 올까봐ㅜ겁나요",
          "original_form": "키우면서도 그날이 올까봐ㅜ겁나요",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100047317.1.12",
          "form": "비어디드래곤이요",
          "original_form": "비어디드래곤이요",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100047317.1.13",
          "form": "아주 용맹하게 ㅋㅋ생겼어요",
          "original_form": "아주 용맹하게 ㅋㅋ생겼어요",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100047317.1.14",
          "form": "도마뱀은 수명이 긴가요? 개는 제가 키울 무렵엔 보통 십년정도였거든요.",
          "original_form": "도마뱀은 수명이 긴가요? 개는 제가 키울 무렵엔 보통 십년정도였거든요.",
          "speaker_id": "1",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100047317.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100047317.1.16",
          "form": "10년정도요",
          "original_form": "10년정도요",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100047317.1.17",
          "form": "뭔지 모르는 종인데 검색해보고 올께요 !",
          "original_form": "뭔지 모르는 종인데 검색해보고 올께요 !",
          "speaker_id": "1",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100047317.1.18",
          "form": "그래서 책임감이 무거워요",
          "original_form": "그래서 책임감이 무거워요",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100047317.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100047317.1.20",
          "form": "그래도 오래 사네요.",
          "original_form": "그래도 오래 사네요.",
          "speaker_id": "1",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100047317.1.21",
          "form": "검색해봤는데 이구아나 보다 작네요",
          "original_form": "검색해봤는데 이구아나 보다 작네요",
          "speaker_id": "1",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100047317.1.22",
          "form": "네 그러니 그때까지 잘키울지 걱정돼요",
          "original_form": "네 그러니 그때까지 잘키울지 걱정돼요",
          "speaker_id": "2",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100047317.1.23",
          "form": "네 음작다고 하기도 애매해요 꼬리랑따지면 꽤 커요",
          "original_form": "네 음작다고 하기도 애매해요 꼬리랑따지면 꽤 커요",
          "speaker_id": "2",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100047317.1.24",
          "form": "게코도마뱀보단 크더라구요",
          "original_form": "게코도마뱀보단 크더라구요",
          "speaker_id": "1",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100047317.1.25",
          "form": "여샌 발톱이 길어서 아파요 ㅋㅋ",
          "original_form": "여샌 발톱이 길어서 아파요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100047317.1.26",
          "form": "네 크죠",
          "original_form": "네 크죠",
          "speaker_id": "2",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100047317.1.27",
          "form": "잘 움직일 사육장을 한마리씩 따로 하면 방 한면은 다 차지하겠는데요?",
          "original_form": "잘 움직일 사육장을 한마리씩 따로 하면 방 한면은 다 차지하겠는데요?",
          "speaker_id": "1",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100047317.1.28",
          "form": "아주조그마한 아기때 데려왔는데 벌써 다컸어요",
          "original_form": "아주조그마한 아기때 데려왔는데 벌써 다컸어요",
          "speaker_id": "2",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100047317.1.29",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100047317.1.30",
          "form": "4자예요",
          "original_form": "4자예요",
          "speaker_id": "2",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100047317.1.31",
          "form": "두개",
          "original_form": "두개",
          "speaker_id": "2",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100047317.1.32",
          "form": "엄청커요",
          "original_form": "엄청커요",
          "speaker_id": "2",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100047317.1.33",
          "form": "근데 크고나니 안돌아다녀요",
          "original_form": "근데 크고나니 안돌아다녀요",
          "speaker_id": "2",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100047317.1.34",
          "form": "역시 잘 키우려면 공간과 시간과 돈이드네요",
          "original_form": "역시 잘 키우려면 공간과 시간과 돈이드네요",
          "speaker_id": "1",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100047317.1.35",
          "form": "파충류는 원래 좀 장식품처럼 안 움직이는 맛이 있는 것 같아요.ㅋㅋ",
          "original_form": "파충류는 원래 좀 장식품처럼 안 움직이는 맛이 있는 것 같아요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100047317.1.36",
          "form": "ㅠㅠ움직이때 귀여웠는데",
          "original_form": "ㅠㅠ움직이때 귀여웠는데",
          "speaker_id": "2",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100047317.1.37",
          "form": "아쉬워요",
          "original_form": "아쉬워요",
          "speaker_id": "2",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100047317.1.38",
          "form": "먹이도 안먹고",
          "original_form": "먹이도 안먹고",
          "speaker_id": "2",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100047317.1.39",
          "form": "자제를 못하고 먹었는데",
          "original_form": "자제를 못하고 먹었는데",
          "speaker_id": "2",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100047317.1.40",
          "form": "이게 포유류랑 다른 점 같아요",
          "original_form": "이게 포유류랑 다른 점 같아요",
          "speaker_id": "1",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100047317.1.41",
          "form": "이젠 먹이려고 아주 제가 엄청 노력해요",
          "original_form": "이젠 먹이려고 아주 제가 엄청 노력해요",
          "speaker_id": "2",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100047317.1.42",
          "form": "햄스터는 늙어도 계속 볼이 터져라 먹는데 파충류는",
          "original_form": "햄스터는 늙어도 계속 볼이 터져라 먹는데 파충류는",
          "speaker_id": "1",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100047317.1.43",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100047317.1.44",
          "form": "자기 몸에 필요한 정도만 딱 먹는?",
          "original_form": "자기 몸에 필요한 정도만 딱 먹는?",
          "speaker_id": "1",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100047317.1.45",
          "form": "진짜 안먹어요",
          "original_form": "진짜 안먹어요",
          "speaker_id": "2",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100047317.1.46",
          "form": "요새 그 걱정이 커요",
          "original_form": "요새 그 걱정이 커요",
          "speaker_id": "2",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100047317.1.47",
          "form": "것도 줘야 겨우 먹어요",
          "original_form": "것도 줘야 겨우 먹어요",
          "speaker_id": "2",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100047317.1.48",
          "form": "도마뱀이 안 먹으면 건강니 나빠지나요? 안필요해서 안먹는게 아닌가요?",
          "original_form": "도마뱀이 안 먹으면 건강니 나빠지나요? 안필요해서 안먹는게 아닌가요?",
          "speaker_id": "1",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100047317.1.49",
          "form": "그량이 맞는지도 모르겠어요",
          "original_form": "그량이 맞는지도 모르겠어요",
          "speaker_id": "2",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100047317.1.50",
          "form": "그런것 같아요",
          "original_form": "그런것 같아요",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100047317.1.51",
          "form": "그래도 이게 자꾸 먹이려고 노력해요",
          "original_form": "그래도 이게 자꾸 먹이려고 노력해요",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100047317.1.52",
          "form": "아프면 안되니까",
          "original_form": "아프면 안되니까",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100047317.1.53",
          "form": "이녀석들",
          "original_form": "이녀석들",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100047317.1.54",
          "form": "식성도 달라요",
          "original_form": "식성도 달라요",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100047317.1.55",
          "form": "하나는 밀웜먹고",
          "original_form": "하나는 밀웜먹고",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100047317.1.56",
          "form": "하나는 사료만 먹어요",
          "original_form": "하나는 사료만 먹어요",
          "speaker_id": "2",
          "time": "20210826 11:33"
        }
      ]
