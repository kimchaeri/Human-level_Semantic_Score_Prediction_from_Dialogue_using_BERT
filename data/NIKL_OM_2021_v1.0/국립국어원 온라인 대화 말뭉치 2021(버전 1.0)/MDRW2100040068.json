      "utterance": [
        {
          "id": "MDRW2100040068.1.1",
          "form": "부대장이 임명되어 왔는데",
          "original_form": "부대장이 임명되어 왔는데",
          "speaker_id": "2",
          "time": "20210904 02:49"
        },
        {
          "id": "MDRW2100040068.1.2",
          "form": "그럼 니 실력이나 증명해보실까?",
          "original_form": "그럼 니 실력이나 증명해보실까?",
          "speaker_id": "2",
          "time": "20210904 02:49"
        },
        {
          "id": "MDRW2100040068.1.3",
          "form": "이러고있어",
          "original_form": "이러고있어",
          "speaker_id": "2",
          "time": "20210904 02:49"
        },
        {
          "id": "MDRW2100040068.1.4",
          "form": "ㄱㄴㄲ ㅋㅋㅋㅋㅋ",
          "original_form": "ㄱㄴㄲ ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:49"
        },
        {
          "id": "MDRW2100040068.1.5",
          "form": "군대는 상명하복 아닌가요",
          "original_form": "군대는 상명하복 아닌가요",
          "speaker_id": "1",
          "time": "20210904 02:49"
        },
        {
          "id": "MDRW2100040068.1.6",
          "form": "널 후드려패서 증명하면 될까",
          "original_form": "널 후드려패서 증명하면 될까",
          "speaker_id": "2",
          "time": "20210904 02:49"
        },
        {
          "id": "MDRW2100040068.1.7",
          "form": "심지어 전시인데....?",
          "original_form": "심지어 전시인데....?",
          "speaker_id": "1",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.8",
          "form": "하고싶었음",
          "original_form": "하고싶었음",
          "speaker_id": "2",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.9",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.10",
          "form": "ㄹㅇㅋㅋㅋㅋㅋ 비무라도 하고싶럿음 ㅋㅋㅋ",
          "original_form": "ㄹㅇㅋㅋㅋㅋㅋ 비무라도 하고싶럿음 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.11",
          "form": "ㅇ ㅏ",
          "original_form": "ㅇ ㅏ",
          "speaker_id": "1",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.12",
          "form": "나 졸려서",
          "original_form": "나 졸려서",
          "speaker_id": "1",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.13",
          "form": "이만 잘건데",
          "original_form": "이만 잘건데",
          "speaker_id": "1",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.14",
          "form": "저항군에게 붙일수 있는 형용사는",
          "original_form": "저항군에게 붙일수 있는 형용사는",
          "speaker_id": "2",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.15",
          "form": "빠져가지고",
          "original_form": "빠져가지고",
          "speaker_id": "2",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.16",
          "form": "밖에 없었음",
          "original_form": "밖에 없었음",
          "speaker_id": "2",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.17",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.18",
          "form": "그럼 내일 더 밀게",
          "original_form": "그럼 내일 더 밀게",
          "speaker_id": "2",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.19",
          "form": "최대한 이거",
          "original_form": "최대한 이거",
          "speaker_id": "2",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.20",
          "form": "구랭",
          "original_form": "구랭",
          "speaker_id": "1",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.21",
          "form": "뽕 뽑아야지....",
          "original_form": "뽕 뽑아야지....",
          "speaker_id": "2",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.22",
          "form": "이담에",
          "original_form": "이담에",
          "speaker_id": "1",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.23",
          "form": "시뇨라 전투 잇는대",
          "original_form": "시뇨라 전투 잇는대",
          "speaker_id": "1",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.24",
          "form": "피가 물처럼 깎이니까",
          "original_form": "피가 물처럼 깎이니까",
          "speaker_id": "1",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100040068.1.25",
          "form": "ㅇㅏ 만나기 싫다",
          "original_form": "ㅇㅏ 만나기 싫다",
          "speaker_id": "2",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100040068.1.26",
          "form": "그것만 대비하면 도ㅐ",
          "original_form": "그것만 대비하면 도ㅐ",
          "speaker_id": "1",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100040068.1.27",
          "form": "그냥 차라리",
          "original_form": "그냥 차라리",
          "speaker_id": "2",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100040068.1.28",
          "form": "여기서 멈추고",
          "original_form": "여기서 멈추고",
          "speaker_id": "2",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100040068.1.29",
          "form": "생각보다 딜 그렇게 안세고",
          "original_form": "생각보다 딜 그렇게 안세고",
          "speaker_id": "1",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100040068.1.30",
          "form": "전기박쥐사냥이나 해서",
          "original_form": "전기박쥐사냥이나 해서",
          "speaker_id": "2",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100040068.1.31",
          "form": "라이덴을 키워가는게",
          "original_form": "라이덴을 키워가는게",
          "speaker_id": "2",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100040068.1.32",
          "form": "낫지 않을까",
          "original_form": "낫지 않을까",
          "speaker_id": "2",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100040068.1.33",
          "form": "패턴도 단순함",
          "original_form": "패턴도 단순함",
          "speaker_id": "1",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100040068.1.34",
          "form": "그래.......",
          "original_form": "그래.......",
          "speaker_id": "2",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100040068.1.35",
          "form": "걍... 피가 계속 까여...",
          "original_form": "걍... 피가 계속 까여...",
          "speaker_id": "1",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100040068.1.36",
          "form": "그뿐이야....",
          "original_form": "그뿐이야....",
          "speaker_id": "1",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100040068.1.37",
          "form": "ㅇㅎ.....",
          "original_form": "ㅇㅎ.....",
          "speaker_id": "2",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100040068.1.38",
          "form": "새우감자카나페를",
          "original_form": "새우감자카나페를",
          "speaker_id": "2",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100040068.1.39",
          "form": "글고 설산 혹한도처럼",
          "original_form": "글고 설산 혹한도처럼",
          "speaker_id": "1",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100040068.1.40",
          "form": "더 만들어야겠군",
          "original_form": "더 만들어야겠군",
          "speaker_id": "2",
          "time": "20210904 02:52"
        },
        {
          "id": "MDRW2100040068.1.41",
          "form": "얜 화염도랑 혹한도가 번갈아가면서 오르는데",
          "original_form": "얜 화염도랑 혹한도가 번갈아가면서 오르는데",
          "speaker_id": "1",
          "time": "20210904 02:52"
        },
        {
          "id": "MDRW2100040068.1.42",
          "form": "스테이지 네곳 끝에 있는 토템 근처로 가면 좀 낮춰줌",
          "original_form": "스테이지 네곳 끝에 있는 토템 근처로 가면 좀 낮춰줌",
          "speaker_id": "1",
          "time": "20210904 02:52"
        },
        {
          "id": "MDRW2100040068.1.43",
          "form": "뭔데 진짜",
          "original_form": "뭔데 진짜",
          "speaker_id": "2",
          "time": "20210904 02:52"
        },
        {
          "id": "MDRW2100040068.1.44",
          "form": "날것의 여행자를",
          "original_form": "날것의 여행자를",
          "speaker_id": "2",
          "time": "20210904 02:52"
        },
        {
          "id": "MDRW2100040068.1.45",
          "form": "날것의 여행자 ㅋㅋㅋㅋㅋ",
          "original_form": "날것의 여행자 ㅋㅋㅋㅋㅋ",
