        },
        {
          "id": "MDRW2100003230.1.12",
          "form": "근데 그런 거 어색하긴하죠",
          "original_form": "근데 그런 거 어색하긴하죠",
          "speaker_id": "2",
          "time": "20210820 21:41"
        },
        {
