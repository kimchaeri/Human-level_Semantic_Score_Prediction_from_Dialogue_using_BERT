      "utterance": [
        {
          "id": "MDRW2100035680.1.1",
          "form": "근데 그친구 고음도 안되잖아요 ..",
          "original_form": "근데 그친구 고음도 안되잖아요 ..",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035680.1.2",
          "form": "팝송은 잘부르지만 ..",
          "original_form": "팝송은 잘부르지만 ..",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035680.1.3",
          "form": "^_^.. 할말하않이네요",
          "original_form": "^_^.. 할말하않이네요",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035680.1.4",
          "form": "저같으면 논란생겼을때 기회다! 하고 내보냈을거같은데",
          "original_form": "저같으면 논란생겼을때 기회다! 하고 내보냈을거같은데",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035680.1.5",
          "form": "아 맞아요 ㅋㅋㅋ",
          "original_form": "아 맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035680.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ넘못됐나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ넘못됐나요",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035680.1.7",
          "form": "아니 저라두 .. 내보냈을거같아오 ㅋㅋㅋ",
          "original_form": "아니 저라두 .. 내보냈을거같아오 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035680.1.8",
          "form": "솔직히 실력구멍으로 항상 언급되는데 ㅠ",
          "original_form": "솔직히 실력구멍으로 항상 언급되는데 ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035680.1.9",
          "form": "그그룹 인기멤도 아니구요",
          "original_form": "그그룹 인기멤도 아니구요",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035680.1.10",
          "form": "맞아요 춤 비교영상 이런거 맨날 올라오는거 안쓰러울정도...",
          "original_form": "맞아요 춤 비교영상 이런거 맨날 올라오는거 안쓰러울정도...",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035680.1.11",
          "form": "아 진짜 맞아요",
          "original_form": "아 진짜 맞아요",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035680.1.12",
          "form": "그 막 주간아에서 춤춘거",
          "original_form": "그 막 주간아에서 춤춘거",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035680.1.13",
          "form": "맨날 비교짤로 올라오고 ..",
          "original_form": "맨날 비교짤로 올라오고 ..",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035680.1.14",
          "form": "인스타에도 올라와요ㅋㅋㅋㅋㅜㅜ",
          "original_form": "인스타에도 올라와요ㅋㅋㅋㅋㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035680.1.15",
          "form": "아 진짜욬ㅋㅋ ???",
          "original_form": "아 진짜욬ㅋㅋ ???",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035680.1.16",
          "form": "ㅇㅖ. .. 그렇더라고요 저는 트위터에서만 욕먹는줄...",
          "original_form": "ㅇㅖ. .. 그렇더라고요 저는 트위터에서만 욕먹는줄...",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035680.1.17",
          "form": "그 막 외국인들이 올리는거죠??",
          "original_form": "그 막 외국인들이 올리는거죠??",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035680.1.18",
          "form": "아 유튭댓글로도",
          "original_form": "아 유튭댓글로도",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035680.1.19",
          "form": "욕 마이 먹죠 .. 하하",
          "original_form": "욕 마이 먹죠 .. 하하",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035680.1.20",
          "form": "ㅋㅋㅋ춤은 그 아이들에 나간 친구가",
          "original_form": "ㅋㅋㅋ춤은 그 아이들에 나간 친구가",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035680.1.21",
          "form": "잘추긴 했는데요....",
          "original_form": "잘추긴 했는데요....",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035680.1.22",
          "form": "아 인정이요 .. 그 친구 춤 제스탈 ...",
          "original_form": "아 인정이요 .. 그 친구 춤 제스탈 ...",
