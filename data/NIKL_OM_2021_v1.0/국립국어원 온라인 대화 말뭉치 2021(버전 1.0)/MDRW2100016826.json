      "utterance": [
        {
          "id": "MDRW2100016826.1.1",
          "form": "유튜브에서 동물 많이보시나봐요?",
          "original_form": "유튜브에서 동물 많이보시나봐요?",
          "speaker_id": "1",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016826.1.2",
          "form": "많이는 아니고 유툽 알고리즘에 뜨면 종종 봅니다",
          "original_form": "많이는 아니고 유툽 알고리즘에 뜨면 종종 봅니다",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016826.1.3",
          "form": "에버랜드 채널보면..",
          "original_form": "에버랜드 채널보면..",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016826.1.4",
          "form": "사자랑 이런거 완전 애기같아요...",
          "original_form": "사자랑 이런거 완전 애기같아요...",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016826.1.5",
          "form": "아 그 채널 아시는구나 ㅎㅎ",
          "original_form": "아 그 채널 아시는구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016826.1.6",
          "form": "아ㅡ. 같은거 본건가보네여?",
          "original_form": "아ㅡ. 같은거 본건가보네여?",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016826.1.7",
          "form": "네ㅋㅋ저는 팬더들 봣어요",
          "original_form": "네ㅋㅋ저는 팬더들 봣어요",
          "speaker_id": "2",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016826.1.8",
          "form": "팬더는 곰탱이 같은게.솜뭉치 같고...",
          "original_form": "팬더는 곰탱이 같은게.솜뭉치 같고...",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016826.1.9",
          "form": "엉덩이 때.려.보고 싶어요",
          "original_form": "엉덩이 때.려.보고 싶어요",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016826.1.10",
          "form": "우찌 그렇게 귀엽게생겻는지 ㅋㅋ 신비로운 귀여움",
          "original_form": "우찌 그렇게 귀엽게생겻는지 ㅋㅋ 신비로운 귀여움",
          "speaker_id": "2",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100016826.1.11",
          "form": "게으른것마져 귀엽",
          "original_form": "게으른것마져 귀엽",
          "speaker_id": "2",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100016826.1.12",
          "form": "유튭으로 보는 다른채널은 모있어요?",
          "original_form": "유튭으로 보는 다른채널은 모있어요?",
          "speaker_id": "1",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100016826.1.13",
          "form": "유툽 별로 안봐요",
          "original_form": "유툽 별로 안봐요",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016826.1.14",
          "form": "주로 영화많이봐서 넷플왓챠 써요",
          "original_form": "주로 영화많이봐서 넷플왓챠 써요",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016826.1.15",
          "form": "넷플릭스나 이런건여?",
          "original_form": "넷플릭스나 이런건여?",
          "speaker_id": "1",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016826.1.16",
          "form": "기묘한이야기",
          "original_form": "기묘한이야기",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016826.1.17",
          "form": "킹덤 이런거",
          "original_form": "킹덤 이런거",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016826.1.18",
          "form": "재밋어요",
          "original_form": "재밋어요",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016826.1.19",
          "form": "스트레인지띵?",
          "original_form": "스트레인지띵?",
          "speaker_id": "1",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016826.1.20",
          "form": "hbo제작한 미드 좋아해서 거의다봣어요",
          "original_form": "hbo제작한 미드 좋아해서 거의다봣어요",
          "speaker_id": "2",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016826.1.21",
          "form": "거기 나오는 여자꼬마 벌써 어른됏더라구요\n.",
          "original_form": "거기 나오는 여자꼬마 벌써 어른됏더라구요\n.",
          "speaker_id": "1",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016826.1.22",
          "form": "hbo 야한데.. ㅋㅋㅋ 부끄부끄",
          "original_form": "hbo 야한데.. ㅋㅋㅋ 부끄부끄",
          "speaker_id": "1",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016826.1.23",
          "form": "그중 체르노빌이 원픽",
          "original_form": "그중 체르노빌이 원픽",
          "speaker_id": "2",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016826.1.24",
          "form": "그 방사능 측정할때 따다다다.. 소리가 소음 돋아요",
          "original_form": "그 방사능 측정할때 따다다다.. 소리가 소음 돋아요",
          "speaker_id": "1",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016826.1.25",
          "form": "공포영화에요",
          "original_form": "공포영화에요",
          "speaker_id": "1",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016826.1.26",
          "form": "재밋게본거 추천해주세요 ㅎ",
          "original_form": "재밋게본거 추천해주세요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016826.1.27",
          "form": "최근 넷플에서 본건... \n종이의집, 오스만제국, 라스트킹덤 .위쳐 정도 생각나네요..",
          "original_form": "최근 넷플에서 본건... \n종이의집, 오스만제국, 라스트킹덤 .위쳐 정도 생각나네요..",
          "speaker_id": "1",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016826.1.28",
          "form": "어떤 장르 좋아하시는데용?",
          "original_form": "어떤 장르 좋아하시는데용?",
          "speaker_id": "1",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016826.1.29",
          "form": "저는 장르라기보다 hbo께 너무 다 재밋엇어요",
          "original_form": "저는 장르라기보다 hbo께 너무 다 재밋엇어요",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016826.1.30",
          "form": "종이의집 2까지 봣어요",
          "original_form": "종이의집 2까지 봣어요",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016826.1.31",
          "form": "hbo면 왕겜, 브레이킹베드?",
          "original_form": "hbo면 왕겜, 브레이킹베드?",
          "speaker_id": "1",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016826.1.32",
          "form": "다밧져ㅋㅋ",
          "original_form": "다밧져ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016826.1.33",
          "form": "이어즈앤이어즈도 재밋어요",
          "original_form": "이어즈앤이어즈도 재밋어요",
          "speaker_id": "2",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016826.1.34",
          "form": "이어즈 정치코미디에요? 재밋겠다..",
          "original_form": "이어즈 정치코미디에요? 재밋겠다..",
