        },
        {
          "id": "MDRW2100007256.1.2",
          "form": "저는 없어요",
          "original_form": "저는 없어요",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007256.1.3",
          "form": "저도 없어요 ㅠㅠ",
          "original_form": "저도 없어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007256.1.4",
          "form": "저는 진짜 한번도 없어요...ㅋㅋㅋ",
          "original_form": "저는 진짜 한번도 없어요...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007256.1.5",
          "form": "체감상 정말 우리집만 없는 느낌",
          "original_form": "체감상 정말 우리집만 없는 느낌",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007256.1.6",
          "form": "티비 틀어도 반려동물 얘기 엄청 나오고 ㅋㅋㅋ",
          "original_form": "티비 틀어도 반려동물 얘기 엄청 나오고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007256.1.7",
          "form": "저도!!! 주변친구들 sns보면 정말 저만 없는기분...",
          "original_form": "저도!!! 주변친구들 sns보면 정말 저만 없는기분...",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007256.1.8",
          "form": "안 키우세요 못 키우세요?",
          "original_form": "안 키우세요 못 키우세요?",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007256.1.9",
          "form": "저는 전자.....?",
          "original_form": "저는 전자.....?",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007256.1.10",
          "form": "금전적으로나 시간으로나 책임이 무서워요",
          "original_form": "금전적으로나 시간으로나 책임이 무서워요",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007256.1.11",
          "form": "귀여움만 누리고 싶지만 그럴 순 없으니까 ㅠㅠ",
          "original_form": "귀여움만 누리고 싶지만 그럴 순 없으니까 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007256.1.12",
          "form": "저는 후자요... 부모님이 강경하게 반대하셔서요ㅠ",
          "original_form": "저는 후자요... 부모님이 강경하게 반대하셔서요ㅠ",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007256.1.13",
          "form": "근데 허락해주셔도 자신이 있는건 아니라... 앞으로도 바빠질수 있어서 안키울거 같긴해요 ㅠㅠ",
          "original_form": "근데 허락해주셔도 자신이 있는건 아니라... 앞으로도 바빠질수 있어서 안키울거 같긴해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007256.1.14",
          "form": "그러게요 집에 하루종일 있는 사람이 없으니까",
          "original_form": "그러게요 집에 하루종일 있는 사람이 없으니까",
          "speaker_id": "1",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007256.1.15",
          "form": "그것도 저도 주저하는 이유 중 하나였어요",
          "original_form": "그것도 저도 주저하는 이유 중 하나였어요",
          "speaker_id": "1",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007256.1.16",
          "form": "혹시 가능하면 뭐 키우고 싶으세요?",
          "original_form": "혹시 가능하면 뭐 키우고 싶으세요?",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007256.1.17",
          "form": "저는 강아지요~",
          "original_form": "저는 강아지요~",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007256.1.18",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007256.1.19",
          "form": "name1가",
          "original_form": "&name1&가",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007256.1.20",
          "form": "제가 좋아하는 연예인 반려견 이름인데",
          "original_form": "제가 좋아하는 연예인 반려견 이름인데",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007256.1.21",
          "form": "걔가 관리 엄청 잘 받은 실버푸들이거든요",
          "original_form": "걔가 관리 엄청 잘 받은 실버푸들이거든요",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007256.1.22",
          "form": "헉 혹시 태연님 반려견??",
          "original_form": "헉 혹시 태연님 반려견??",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007256.1.23",
          "form": "넘 예뻐요 ㅋㅋ",
          "original_form": "넘 예뻐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007256.1.24",
          "form": "어 네 맞아요 아시네요",
          "original_form": "어 네 맞아요 아시네요",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007256.1.25",
          "form": "제로 진짜 인형 같이 생겼어요",
          "original_form": "제로 진짜 인형 같이 생겼어요",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007256.1.26",
          "form": "라방에 나온걸 봤어요 ㅎㅎ",
          "original_form": "라방에 나온걸 봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007256.1.27",
          "form": "걔는 어지간한 인간보다 나은 삶을 사는 느낌......",
          "original_form": "걔는 어지간한 인간보다 나은 삶을 사는 느낌......",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007256.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 다음생에는 꼭 제로로..!!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 다음생에는 꼭 제로로..!!!",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007256.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007256.1.30",
          "form": "저는 산책 시킬 자신이 없어서 고양이요",
          "original_form": "저는 산책 시킬 자신이 없어서 고양이요",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007256.1.31",
          "form": "전 원래 완전 강아지파였는데",
          "original_form": "전 원래 완전 강아지파였는데",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007256.1.32",
          "form": "유튜브 하하하님 영상보면서",
          "original_form": "유튜브 하하하님 영상보면서",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007256.1.33",
          "form": "고양이들이 넘좋아졌어요ㅎㅎ",
          "original_form": "고양이들이 넘좋아졌어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007256.1.34",
          "form": "저도 영상 가끔 보며 대리만족 ㅎㅎㅎ",
          "original_form": "저도 영상 가끔 보며 대리만족 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007256.1.35",
          "form": "내일은 일요일 아침이니 동물농장도 하겠네요",
          "original_form": "내일은 일요일 아침이니 동물농장도 하겠네요",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007256.1.36",
          "form": "그거 보면 사람들 정말 온갖 걸 다 키우는 것 같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "그거 보면 사람들 정말 온갖 걸 다 키우는 것 같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007256.1.37",
          "form": "맞아요 ㅋㅋ 요즘은 동물농장 유튜브로 가끔 보는데",
          "original_form": "맞아요 ㅋㅋ 요즘은 동물농장 유튜브로 가끔 보는데",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007256.1.38",
          "form": "에버랜드 사파리 동물들 나온게 재밌었어요 ... 사육사분들 대단해보이고 ㅎㅎ",
          "original_form": "에버랜드 사파리 동물들 나온게 재밌었어요 ... 사육사분들 대단해보이고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007256.1.39",
          "form": "맞아요 에버랜드 보면서 반려 아기호랑이 키우고 싶단 생각 하고 ㅋㅋ",
          "original_form": "맞아요 에버랜드 보면서 반려 아기호랑이 키우고 싶단 생각 하고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007256.1.40",
          "form": "요즘 하고 있는 보스베이비 영화가",
          "original_form": "요즘 하고 있는 보스베이비 영화가",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007256.1.41",
          "form": "사람들이 점점 애 대신 개를 키우는 게 문제(?)라는 세계관 바탕으로 만들어진 영화잖아요",
          "original_form": "사람들이 점점 애 대신 개를 키우는 게 문제(?)라는 세계관 바탕으로 만들어진 영화잖아요",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007256.1.42",
          "form": "앞으로도 계속 반려동물 인구만 늘까요?",
          "original_form": "앞으로도 계속 반려동물 인구만 늘까요?",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007256.1.43",
          "form": "저는 그럴것같아요 지금도 늘고 있고...",
          "original_form": "저는 그럴것같아요 지금도 늘고 있고...",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007256.1.44",
          "form": "저도 나이들어서 돈 많이 벌고 시간생기면 반려동물이랑 둘이 살거 같아요 ㅎㅎ",
          "original_form": "저도 나이들어서 돈 많이 벌고 시간생기면 반려동물이랑 둘이 살거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007256.1.45",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007256.1.46",
          "form": "그럼 강아지와 함께 늙어가는 거죠",
          "original_form": "그럼 강아지와 함께 늙어가는 거죠",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007256.1.47",
          "form": "지금은 기르면 강아지별로 먼저 가는 것도 싫어요",
