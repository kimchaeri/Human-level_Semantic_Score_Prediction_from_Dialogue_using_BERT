      "utterance": [
        {
          "id": "MDRW2100045657.1.1",
          "form": "이번엔 제 차례예요!",
          "original_form": "이번엔 제 차례예요!",
          "speaker_id": "1",
          "time": "20210908 04:51"
        },
        {
          "id": "MDRW2100045657.1.2",
          "form": "제 머릿속 야채를 맞춰보세요",
          "original_form": "제 머릿속 야채를 맞춰보세요",
          "speaker_id": "1",
          "time": "20210908 04:51"
        },
        {
          "id": "MDRW2100045657.1.3",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "2",
          "time": "20210908 04:51"
        },
        {
          "id": "MDRW2100045657.1.4",
          "form": "붉은 색인가요?",
          "original_form": "붉은 색인가요?",
          "speaker_id": "2",
          "time": "20210908 04:51"
        },
        {
          "id": "MDRW2100045657.1.5",
          "form": "아니요! 빨갛지 않아요",
          "original_form": "아니요! 빨갛지 않아요",
          "speaker_id": "1",
          "time": "20210908 04:51"
        },
        {
          "id": "MDRW2100045657.1.6",
          "form": "빨간 야채가 아닙니다 후후",
          "original_form": "빨간 야채가 아닙니다 후후",
          "speaker_id": "1",
          "time": "20210908 04:52"
        },
        {
          "id": "MDRW2100045657.1.7",
          "form": "초록색인가요?",
          "original_form": "초록색인가요?",
          "speaker_id": "2",
          "time": "20210908 04:52"
        },
        {
          "id": "MDRW2100045657.1.8",
          "form": "빨간고 아니면 초록색 아니면?노란거아 생각합니다.ㅋㅋㅋㅋ",
          "original_form": "빨간고 아니면 초록색 아니면?노란거아 생각합니다.ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:52"
        },
        {
          "id": "MDRW2100045657.1.9",
          "form": "뒝~~~ 초록색도 노랑색도 아닙니다",
          "original_form": "뒝~~~ 초록색도 노랑색도 아닙니다",
          "speaker_id": "1",
          "time": "20210908 04:53"
        },
        {
          "id": "MDRW2100045657.1.10",
          "form": "그럼 주황색?",
          "original_form": "그럼 주황색?",
          "speaker_id": "2",
          "time": "20210908 04:54"
        },
        {
          "id": "MDRW2100045657.1.11",
          "form": "컬러 방울토마토인가요?",
          "original_form": "컬러 방울토마토인가요?",
          "speaker_id": "2",
          "time": "20210908 04:55"
        },
        {
          "id": "MDRW2100045657.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄴㅋㅌㅌㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄴㅋㅌㅌㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:55"
        },
        {
          "id": "MDRW2100045657.1.13",
          "form": "이분 자꾸 희귀한 거 가져오신다!!!",
          "original_form": "이분 자꾸 희귀한 거 가져오신다!!!",
          "speaker_id": "1",
          "time": "20210908 04:55"
        },
        {
          "id": "MDRW2100045657.1.14",
          "form": "컬방토 아닙니다!",
          "original_form": "컬방토 아닙니다!",
          "speaker_id": "1",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100045657.1.15",
          "form": "컬러 방울토마토다 희귀한거였나요?ㅋㅋㅋㅋ",
          "original_form": "컬러 방울토마토다 희귀한거였나요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100045657.1.16",
          "form": "음... 무?",
          "original_form": "음... 무?",
          "speaker_id": "2",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100045657.1.17",
          "form": "무도 아닙니다",
          "original_form": "무도 아닙니다",
          "speaker_id": "1",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100045657.1.18",
          "form": "흰색은 맞아요!",
          "original_form": "흰색은 맞아요!",
          "speaker_id": "1",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100045657.1.19",
