      "utterance": [
        {
          "id": "MDRW2100017187.1.1",
          "form": "최근에 영화 보셨나요??",
          "original_form": "최근에 영화 보셨나요??",
          "speaker_id": "1",
          "time": "20210826 17:54"
        },
        {
          "id": "MDRW2100017187.1.2",
          "form": "저는 왓챠로 캐치미이프유캔 봤어요 ㅋㅋㅋ",
          "original_form": "저는 왓챠로 캐치미이프유캔 봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017187.1.3",
          "form": "디카프리오?",
          "original_form": "디카프리오?",
          "speaker_id": "1",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017187.1.4",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017187.1.5",
          "form": "리즈시절 너무 잘생겨서 작품 찾다가 봤어요 ㅋㅋㅋ",
          "original_form": "리즈시절 너무 잘생겨서 작품 찾다가 봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017187.1.6",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "1",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017187.1.7",
          "form": "디카프리오 좋아하시나봐요!",
          "original_form": "디카프리오 좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017187.1.8",
          "form": "인생영화됐어요 ㅋㅋㅋ 실화바탕이라 더 몰입되구요",
          "original_form": "인생영화됐어요 ㅋㅋㅋ 실화바탕이라 더 몰입되구요",
          "speaker_id": "2",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017187.1.9",
          "form": "원래는 관심없었는데 우연히 리즈사진보고 반해서요 ㅋㅋㅋ",
          "original_form": "원래는 관심없었는데 우연히 리즈사진보고 반해서요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017187.1.10",
          "form": "오 저도 기회되면 봐봐야겠어요",
          "original_form": "오 저도 기회되면 봐봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017187.1.11",
          "form": "저는 어제 인질 봤어요",
          "original_form": "저는 어제 인질 봤어요",
          "speaker_id": "1",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017187.1.12",
          "form": "인질은 누구나오나요??",
          "original_form": "인질은 누구나오나요??",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017187.1.13",
          "form": "황정민이요!",
          "original_form": "황정민이요!",
          "speaker_id": "1",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100017187.1.14",
          "form": "재밌긴 한데 뭔가 잔인했어요 사실적인 느낌이랄까",
          "original_form": "재밌긴 한데 뭔가 잔인했어요 사실적인 느낌이랄까",
          "speaker_id": "1",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100017187.1.15",
          "form": "오 궁금하네요 황정민은 믿고보죠",
          "original_form": "오 궁금하네요 황정민은 믿고보죠",
          "speaker_id": "2",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100017187.1.16",
          "form": "맞아요 연기 대박",
          "original_form": "맞아요 연기 대박",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017187.1.17",
          "form": "모가디슈도 재밌어요!",
          "original_form": "모가디슈도 재밌어요!",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017187.1.18",
          "form": "모가디슈 인기많더라구여",
          "original_form": "모가디슈 인기많더라구여",
          "speaker_id": "2",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017187.1.19",
          "form": "조인성 맞나여??",
          "original_form": "조인성 맞나여??",
          "speaker_id": "2",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017187.1.20",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017187.1.21",
          "form": "너무잘생겼어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무잘생겼어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:03"
        }
      ]
