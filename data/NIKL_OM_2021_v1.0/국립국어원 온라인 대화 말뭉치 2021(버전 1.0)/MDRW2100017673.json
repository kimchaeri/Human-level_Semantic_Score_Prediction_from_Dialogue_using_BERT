      "utterance": [
        {
          "id": "MDRW2100017673.1.1",
          "form": "연유라떼 좋아하는 사람",
          "original_form": "연유라떼 좋아하는 사람",
          "speaker_id": "2",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017673.1.2",
          "form": "저요ㅋ",
          "original_form": "저요ㅋ",
          "speaker_id": "1",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017673.1.3",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017673.1.4",
          "form": "바닐라라떼좋아하세요?",
          "original_form": "바닐라라떼좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017673.1.5",
          "form": "네 달달한 거 좋아해요",
          "original_form": "네 달달한 거 좋아해요",
          "speaker_id": "2",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017673.1.6",
          "form": "민트초코는요?",
          "original_form": "민트초코는요?",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017673.1.7",
          "form": "민트초코 사랑하죠",
          "original_form": "민트초코 사랑하죠",
          "speaker_id": "2",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017673.1.8",
          "form": "저도 오늘 민초 먹었어요",
          "original_form": "저도 오늘 민초 먹었어요",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017673.1.9",
          "form": "맛있었겠어요",
          "original_form": "맛있었겠어요",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017673.1.10",
          "form": "전 지금 미숫가루 마시고있어요",
          "original_form": "전 지금 미숫가루 마시고있어요",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017673.1.11",
          "form": "집에서 만드셧어요?",
          "original_form": "집에서 만드셧어요?",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017673.1.12",
          "form": "네ㅎㅎ 꿀이랑 넣으면 맛있어요",
          "original_form": "네ㅎㅎ 꿀이랑 넣으면 맛있어요",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017673.1.13",
          "form": "저도 마시고싶네요",
          "original_form": "저도 마시고싶네요",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017673.1.14",
          "form": "집에 미술가루나 선식 있으세요?",
          "original_form": "집에 미술가루나 선식 있으세요?",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017673.1.15",
          "form": "아니요ㅜㅜ",
          "original_form": "아니요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017673.1.16",
          "form": "ㅜㅜ 아쉽네요",
          "original_form": "ㅜㅜ 아쉽네요",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017673.1.17",
          "form": "치킨 먹고싶어요",
          "original_form": "치킨 먹고싶어요",
