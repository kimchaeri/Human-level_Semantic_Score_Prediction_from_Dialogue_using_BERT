      "utterance": [
        {
          "id": "MDRW2100008394.1.1",
          "form": "책 좋아하시나요?",
          "original_form": "책 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008394.1.2",
          "form": "실용서 위주로 읽게되더라고요",
          "original_form": "실용서 위주로 읽게되더라고요",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008394.1.3",
          "form": "저는 경제서직 봐요",
          "original_form": "저는 경제서직 봐요",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008394.1.4",
          "form": "얼마전에 수면 관련 책 빌렸었어요",
          "original_form": "얼마전에 수면 관련 책 빌렸었어요",
          "speaker_id": "2",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008394.1.5",
          "form": "엇 수면요? 불면증 있으시나요?",
          "original_form": "엇 수면요? 불면증 있으시나요?",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008394.1.6",
          "form": "그건아니고요 책제목에ㅋㅋ혹해서요",
          "original_form": "그건아니고요 책제목에ㅋㅋ혹해서요",
          "speaker_id": "2",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008394.1.7",
          "form": "오 재미있는 내용 있나요?",
          "original_form": "오 재미있는 내용 있나요?",
          "speaker_id": "1",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008394.1.8",
          "form": "3시간 수면법 이었어요",
          "original_form": "3시간 수면법 이었어요",
          "speaker_id": "2",
          "time": "20210822 16:50"
        },
        {
          "id": "MDRW2100008394.1.9",
          "form": "뭔가 3시간만 잘 수 있다니 혹해서요 ㅋㅋ",
          "original_form": "뭔가 3시간만 잘 수 있다니 혹해서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:50"
        },
        {
          "id": "MDRW2100008394.1.10",
          "form": "헉.....6시간도 부족합니다...잠은 넉넉히...",
          "original_form": "헉.....6시간도 부족합니다...잠은 넉넉히...",
          "speaker_id": "1",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008394.1.11",
          "form": "읽고나니 좀 허무하더라고요",
          "original_form": "읽고나니 좀 허무하더라고요",
          "speaker_id": "2",
          "time": "20210822 16:54"
        },
        {
          "id": "MDRW2100008394.1.12",
          "form": "과연저걸 따라할수 있을까 싶은 ㅋㅋ",
          "original_form": "과연저걸 따라할수 있을까 싶은 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:54"
        },
        {
          "id": "MDRW2100008394.1.13",
          "form": "헉...수면 시간은 개인마다 필요 시간이 있는듯요",
          "original_form": "헉...수면 시간은 개인마다 필요 시간이 있는듯요",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008394.1.14",
          "form": "그것말고 스트레칭 책도 사놓기만했네요 ㅋㅋ",
          "original_form": "그것말고 스트레칭 책도 사놓기만했네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008394.1.15",
          "form": "음 건강 관련 도서를 많이 보시네요",
          "original_form": "음 건강 관련 도서를 많이 보시네요",
          "speaker_id": "1",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008394.1.16",
          "form": "말하고 보니 그렇네요 ㅎㅎ",
          "original_form": "말하고 보니 그렇네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008394.1.17",
          "form": "저는 건강책은 본 적이 없네요",
          "original_form": "저는 건강책은 본 적이 없네요",
          "speaker_id": "1",
