      "utterance": [
        {
          "id": "MDRW2100040264.1.1",
          "form": "점심은 잘 챙겨드셨어요?",
          "original_form": "점심은 잘 챙겨드셨어요?",
          "speaker_id": "1",
          "time": "20210904 15:15"
        },
        {
          "id": "MDRW2100040264.1.2",
          "form": "네네 된장찌개 먹었어요~",
          "original_form": "네네 된장찌개 먹었어요~",
          "speaker_id": "2",
          "time": "20210904 15:15"
        },
        {
          "id": "MDRW2100040264.1.3",
          "form": "뭐 드셨나요",
          "original_form": "뭐 드셨나요",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040264.1.4",
          "form": "그래도 잘 챙겨드셨다니 다행이네요",
          "original_form": "그래도 잘 챙겨드셨다니 다행이네요",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040264.1.5",
          "form": "저는 무국이요",
          "original_form": "저는 무국이요",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040264.1.6",
          "form": "직접 해드시나요",
          "original_form": "직접 해드시나요",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040264.1.7",
          "form": "아뇨 엄마가 해논거예요 ㅎㅎ",
          "original_form": "아뇨 엄마가 해논거예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040264.1.8",
          "form": "똥손이라",
          "original_form": "똥손이라",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040264.1.9",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040264.1.10",
          "form": "아니요ㅋㅋㅋ저두 엄마가 해주신편ㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋ저두 엄마가 해주신편ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040264.1.11",
          "form": "그쵸 엄마 손맛을 못따라가요",
          "original_form": "그쵸 엄마 손맛을 못따라가요",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040264.1.12",
          "form": "저녁에는 죽드세요?",
          "original_form": "저녁에는 죽드세요?",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040264.1.13",
          "form": "네 아직 저는 조미료맛이라 ㅋㅋㅋ",
          "original_form": "네 아직 저는 조미료맛이라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040264.1.14",
          "form": "음 아직 죽은 안 먹어도 될거같아요",
          "original_form": "음 아직 죽은 안 먹어도 될거같아요",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040264.1.15",
          "form": "아 다행이네요",
          "original_form": "아 다행이네요",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100040264.1.16",
          "form": "백신 부작용 중에 식욕이 늘어난다는 것도 잇는거 같은데ㅋㅋㅋ",
          "original_form": "백신 부작용 중에 식욕이 늘어난다는 것도 잇는거 같은데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100040264.1.17",
          "form": "식욕이 왕성해져서 오히려ㅋㅋㅋ",
          "original_form": "식욕이 왕성해져서 오히려ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100040264.1.18",
          "form": "맞아요 저도 일차맞고",
          "original_form": "맞아요 저도 일차맞고",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100040264.1.19",
          "form": "배고파서 엄청 먹었어요",
          "original_form": "배고파서 엄청 먹었어요",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100040264.1.20",
          "form": "몸에서 잘먹으라고 신호주나봐요ㅎㅎ",
          "original_form": "몸에서 잘먹으라고 신호주나봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040264.1.21",
          "form": "그러게요 ㅋㅋ 심심이가 치킨줬으니",
          "original_form": "그러게요 ㅋㅋ 심심이가 치킨줬으니",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040264.1.22",
          "form": "오늘 시켜먹을까 고민중이에요",
          "original_form": "오늘 시켜먹을까 고민중이에요",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040264.1.23",
          "form": "와 벌써 거기까지하셨구나",
          "original_form": "와 벌써 거기까지하셨구나",
          "speaker_id": "1",
          "time": "20210904 15:19"
        }
      ]
