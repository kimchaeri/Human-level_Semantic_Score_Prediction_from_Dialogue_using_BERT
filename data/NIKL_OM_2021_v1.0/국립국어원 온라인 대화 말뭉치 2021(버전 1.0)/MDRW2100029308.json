      "utterance": [
        {
          "id": "MDRW2100029308.1.1",
          "form": "음 둘다 그렇게 느껴요",
          "original_form": "음 둘다 그렇게 느껴요",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029308.1.2",
          "form": "와이프도",
          "original_form": "와이프도",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029308.1.3",
          "form": "맨날 하는말이",
          "original_form": "맨날 하는말이",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029308.1.4",
          "form": "혼자 있고 싶다",
          "original_form": "혼자 있고 싶다",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029308.1.5",
          "form": "나 1박2일로 좀 다녀오면 안돼?",
          "original_form": "나 1박2일로 좀 다녀오면 안돼?",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029308.1.6",
          "form": "라고 합니다",
          "original_form": "라고 합니다",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029308.1.7",
          "form": "아ㅋㅋ",
          "original_form": "아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029308.1.8",
          "form": "1년반동안",
          "original_form": "1년반동안",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029308.1.9",
          "form": "육아하니까",
          "original_form": "육아하니까",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029308.1.10",
          "form": "피로?",
          "original_form": "피로?",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029308.1.11",
          "form": "아이고ㅜㅜ;; 장난 아니죠;;",
          "original_form": "아이고ㅜㅜ;; 장난 아니죠;;",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029308.1.12",
          "form": "정신적 육체적으로",
          "original_form": "정신적 육체적으로",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029308.1.13",
          "form": "온거같더라구요",
          "original_form": "온거같더라구요",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029308.1.14",
          "form": "그래서!",
          "original_form": "그래서!",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029308.1.15",
          "form": "이번주 일요일에",
          "original_form": "이번주 일요일에",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029308.1.16",
          "form": "나가라고 했습니다",
          "original_form": "나가라고 했습니다",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029308.1.17",
          "form": "아ㅎㅎㅎ 어디로요!",
          "original_form": "아ㅎㅎㅎ 어디로요!",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029308.1.18",
          "form": "안가르쳐주더라구요 ㅋㅋ",
          "original_form": "안가르쳐주더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029308.1.19",
          "form": "그거 name2도 해방일 아닌가요ㅎㅎ",
          "original_form": "그거 &name2&도 해방일 아닌가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029308.1.20",
          "form": "멀리 바람쐬러 가라고했서요",
          "original_form": "멀리 바람쐬러 가라고했서요",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029308.1.21",
          "form": "놉",
          "original_form": "놉",
          "speaker_id": "2",
          "time": "20210903 01:21"
        },
        {
          "id": "MDRW2100029308.1.22",
          "form": "독박육아입니다",
          "original_form": "독박육아입니다",
          "speaker_id": "2",
          "time": "20210903 01:21"
        },
        {
          "id": "MDRW2100029308.1.23",
          "form": "아기 봐야죠 ㅎㅎ",
          "original_form": "아기 봐야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:21"
        },
        {
          "id": "MDRW2100029308.1.24",
          "form": "빡셉니다 ㅋㅋ",
          "original_form": "빡셉니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:21"
        },
        {
          "id": "MDRW2100029308.1.25",
          "form": "차라리 일하는게 나아용",
          "original_form": "차라리 일하는게 나아용",
          "speaker_id": "2",
          "time": "20210903 01:21"
        },
        {
          "id": "MDRW2100029308.1.26",
          "form": "아 뿔 싸 가 왜 나쁜 말이죠ㅜ",
          "original_form": "아 뿔 싸 가 왜 나쁜 말이죠ㅜ",
          "speaker_id": "1",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100029308.1.27",
          "form": "아뿔싸",
          "original_form": "아뿔싸",
          "speaker_id": "2",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100029308.1.28",
          "form": "육아휴직한 선배들이",
          "original_form": "육아휴직한 선배들이",
          "speaker_id": "1",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100029308.1.29",
          "form": "예정보다",
          "original_form": "예정보다",
          "speaker_id": "1",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100029308.1.30",
          "form": "빨리?",
          "original_form": "빨리?",
          "speaker_id": "2",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100029308.1.31",
          "form": "어 왜 님은 되죠!",
          "original_form": "어 왜 님은 되죠!",
          "speaker_id": "1",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100029308.1.32",
          "form": "아뿔싸",
          "original_form": "아뿔싸",
          "speaker_id": "2",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100029308.1.33",
          "form": "네 빨리 복귀해서",
          "original_form": "네 빨리 복귀해서",
          "speaker_id": "1",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100029308.1.34",
          "form": "일이 낫다고 ㅎㅎ",
          "original_form": "일이 낫다고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100029308.1.35",
          "form": "육아 쉽지않습니다 ㅋㅋㅋ",
          "original_form": "육아 쉽지않습니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100029308.1.36",
          "form": "일이 낫습니다 진짜",
          "original_form": "일이 낫습니다 진짜",
          "speaker_id": "2",
          "time": "20210903 01:23"
        },
        {
          "id": "MDRW2100029308.1.37",
          "form": "맞아요ㅜ 아기 귀여운것과 힘든건 별개라",
          "original_form": "맞아요ㅜ 아기 귀여운것과 힘든건 별개라",
          "speaker_id": "1",
          "time": "20210903 01:23"
        },
        {
          "id": "MDRW2100029308.1.38",
          "form": "맞아여 ㅋㅋ",
          "original_form": "맞아여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:23"
        }
      ]
