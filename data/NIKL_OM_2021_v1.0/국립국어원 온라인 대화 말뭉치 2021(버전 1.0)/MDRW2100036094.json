      "utterance": [
        {
          "id": "MDRW2100036094.1.1",
          "form": "유니 집에왔네",
          "original_form": "유니 집에왔네",
          "speaker_id": "2",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100036094.1.2",
          "form": "혼자?? 데리고 온거아니고??",
          "original_form": "혼자?? 데리고 온거아니고??",
          "speaker_id": "1",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100036094.1.3",
          "form": "웅 점프윙스에서",
          "original_form": "웅 점프윙스에서",
          "speaker_id": "2",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100036094.1.4",
          "form": "아 매일안가지??",
          "original_form": "아 매일안가지??",
          "speaker_id": "1",
          "time": "20210830 16:13"
        },
        {
          "id": "MDRW2100036094.1.5",
          "form": "응 미니만 매일가고ㅋㅋ유닌 3일",
          "original_form": "응 미니만 매일가고ㅋㅋ유닌 3일",
          "speaker_id": "2",
          "time": "20210830 16:13"
        },
        {
          "id": "MDRW2100036094.1.6",
          "form": "내꺼 우삼겹3개가?",
          "original_form": "내꺼 우삼겹3개가?",
          "speaker_id": "2",
          "time": "20210830 16:14"
        },
        {
          "id": "MDRW2100036094.1.7",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210830 16:14"
        },
        {
          "id": "MDRW2100036094.1.8",
          "form": "부대3스파1",
          "original_form": "부대3스파1",
          "speaker_id": "1",
          "time": "20210830 16:14"
        },
        {
          "id": "MDRW2100036094.1.9",
          "form": "부대찌개가 안 낮나?",
          "original_form": "부대찌개가 안 낮나?",
          "speaker_id": "1",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100036094.1.10",
          "form": "좋으다ㅋㅋ나도 냉동해놔야지",
          "original_form": "좋으다ㅋㅋ나도 냉동해놔야지",
          "speaker_id": "2",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100036094.1.11",
          "form": "스파게티 많자나",
          "original_form": "스파게티 많자나",
          "speaker_id": "1",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100036094.1.12",
          "form": "냉동실자리많나",
          "original_form": "냉동실자리많나",
          "speaker_id": "1",
          "time": "20210830 16:18"
        },
        {
          "id": "MDRW2100036094.1.13",
          "form": "냉동실은 자리 별로 없고 냉장실엔 자리 많지 ㅋㅋ",
          "original_form": "냉동실은 자리 별로 없고 냉장실엔 자리 많지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:19"
        },
        {
          "id": "MDRW2100036094.1.14",
          "form": "오우ㅋㅋㅋ 점심 뭐먹을거야?",
          "original_form": "오우ㅋㅋㅋ 점심 뭐먹을거야?",
          "speaker_id": "1",
          "time": "20210831 11:19"
        },
        {
          "id": "MDRW2100036094.1.15",
          "form": "다 좋다 ㅋㅋㅋ 니 먹고 싶은걸로 먹자 ㅋㅋㅋ",
          "original_form": "다 좋다 ㅋㅋㅋ 니 먹고 싶은걸로 먹자 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:19"
        },
        {
          "id": "MDRW2100036094.1.16",
          "form": "머먹지",
          "original_form": "머먹지",
          "speaker_id": "1",
