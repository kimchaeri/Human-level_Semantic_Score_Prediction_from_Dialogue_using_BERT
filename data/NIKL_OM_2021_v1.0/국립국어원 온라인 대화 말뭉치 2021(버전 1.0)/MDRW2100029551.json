      "utterance": [
        {
          "id": "MDRW2100029551.1.1",
          "form": "전 운동은 별로요",
          "original_form": "전 운동은 별로요",
          "speaker_id": "2",
          "time": "20210903 11:49"
        },
        {
          "id": "MDRW2100029551.1.2",
          "form": "전 홈트요",
          "original_form": "전 홈트요",
          "speaker_id": "1",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100029551.1.3",
          "form": "오~ 운동을 하긴해야하는데. 귀찮네요",
          "original_form": "오~ 운동을 하긴해야하는데. 귀찮네요",
          "speaker_id": "2",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100029551.1.4",
          "form": "예전에는 걷기운동이라도 했는데",
          "original_form": "예전에는 걷기운동이라도 했는데",
          "speaker_id": "2",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100029551.1.5",
          "form": "플랭크해요",
          "original_form": "플랭크해요",
          "speaker_id": "1",
          "time": "20210903 14:25"
        },
        {
          "id": "MDRW2100029551.1.6",
          "form": "그거 하면 담날 근육통이",
          "original_form": "그거 하면 담날 근육통이",
          "speaker_id": "2",
          "time": "20210903 14:35"
        },
        {
          "id": "MDRW2100029551.1.7",
          "form": "그래도 해야죠",
          "original_form": "그래도 해야죠",
          "speaker_id": "1",
          "time": "20210903 14:36"
        },
        {
          "id": "MDRW2100029551.1.8",
          "form": "네 해야줘",
          "original_form": "네 해야줘",
          "speaker_id": "2",
          "time": "20210903 14:43"
        },
        {
          "id": "MDRW2100029551.1.9",
          "form": "운동 해야하는데 ....",
          "original_form": "운동 해야하는데 ....",
          "speaker_id": "2",
          "time": "20210903 14:43"
        },
        {
          "id": "MDRW2100029551.1.10",
          "form": "효과있어요",
          "original_form": "효과있어요",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100029551.1.11",
          "form": "이번주말부터 해야겠어요 30초라도",
          "original_form": "이번주말부터 해야겠어요 30초라도",
          "speaker_id": "2",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100029551.1.12",
          "form": "지금당장요",
          "original_form": "지금당장요",
          "speaker_id": "1",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100029551.1.13",
          "form": "호옥시 트레이너세요?",
          "original_form": "호옥시 트레이너세요?",
          "speaker_id": "2",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100029551.1.14",
          "form": "아니요 ㅋㅋ",
          "original_form": "아니요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100029551.1.15",
          "form": "너무 단호하셔서. ㅋㅋ",
          "original_form": "너무 단호하셔서. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:58"
        },
        {
          "id": "MDRW2100029551.1.16",
          "form": "네! 오늘 저녁 티비보면서 30초씩 히ㅓ야겠어요",
          "original_form": "네! 오늘 저녁 티비보면서 30초씩 히ㅓ야겠어요",
          "speaker_id": "2",
