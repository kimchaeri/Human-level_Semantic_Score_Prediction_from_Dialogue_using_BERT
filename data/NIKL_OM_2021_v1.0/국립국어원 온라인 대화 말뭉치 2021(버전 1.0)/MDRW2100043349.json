      "utterance": [
        {
          "id": "MDRW2100043349.1.1",
          "form": "좀 늦게 보긴 했는데 저라면 김찌용",
          "original_form": "좀 늦게 보긴 했는데 저라면 김찌용",
          "speaker_id": "2",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043349.1.2",
          "form": "왜냐면",
          "original_form": "왜냐면",
          "speaker_id": "2",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043349.1.3",
          "form": "고기 구워먹으면 저도 먹고싶기 때문 ㅋㅋ",
          "original_form": "고기 구워먹으면 저도 먹고싶기 때문 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043349.1.4",
          "form": "김치찌개..!",
          "original_form": "김치찌개..!",
          "speaker_id": "1",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043349.1.5",
          "form": "저기요",
          "original_form": "저기요",
          "speaker_id": "1",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043349.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043349.1.7",
          "form": "저녁 뭐 드실 계획이신지",
          "original_form": "저녁 뭐 드실 계획이신지",
          "speaker_id": "1",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043349.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저는 방금 육개장에 물 부었습니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저는 방금 육개장에 물 부었습니다",
          "speaker_id": "2",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100043349.1.9",
          "form": "작고 소중한 저녁이네요",
          "original_form": "작고 소중한 저녁이네요",
          "speaker_id": "2",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100043349.1.10",
          "form": "라면 드시냐고요",
          "original_form": "라면 드시냐고요",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100043349.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100043349.1.12",
          "form": "그래도 이따 구슬아이스크림 먹을 거예요",
          "original_form": "그래도 이따 구슬아이스크림 먹을 거예요",
          "speaker_id": "2",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100043349.1.13",
          "form": "진짜 작고 소중하다",
          "original_form": "진짜 작고 소중하다",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100043349.1.14",
          "form": "ㅋㅋ 하... 밥이없어서 ㅋㅋ",
          "original_form": "ㅋㅋ 하... 밥이없어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100043349.1.15",
          "form": "저녁보다 비싸다 후식!",
          "original_form": "저녁보다 비싸다 후식!",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100043349.1.16",
          "form": "ㅋㅋ 삼김 먹고 아메리카노 먹는 이치죠",
          "original_form": "ㅋㅋ 삼김 먹고 아메리카노 먹는 이치죠",
          "speaker_id": "2",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100043349.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100043349.1.18",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100043349.1.19",
          "form": "완벽한 예시네요",
          "original_form": "완벽한 예시네요",
          "speaker_id": "1",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100043349.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ 그쵸 ^-^",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그쵸 ^-^",
          "speaker_id": "2",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100043349.1.21",
          "form": "그래서 둘중에 머 픽하셨나요",
          "original_form": "그래서 둘중에 머 픽하셨나요",
          "speaker_id": "2",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100043349.1.22",
          "form": "아무것도... 안하고있는중",
          "original_form": "아무것도... 안하고있는중",
          "speaker_id": "1",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100043349.1.23",
          "form": "김치찌개는 끓여야해요",
          "original_form": "김치찌개는 끓여야해요",
          "speaker_id": "1",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100043349.1.24",
          "form": "뭐야....",
          "original_form": "뭐야....",
          "speaker_id": "2",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100043349.1.25",
          "form": "하하!",
          "original_form": "하하!",
          "speaker_id": "1",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100043349.1.26",
          "form": "많이 해둬서 낼까지 우려먹죠 ㅋ",
          "original_form": "많이 해둬서 낼까지 우려먹죠 ㅋ",
          "speaker_id": "2",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100043349.1.27",
          "form": "그럴려구요 한 이틀치 끓여놔야겠어요",
          "original_form": "그럴려구요 한 이틀치 끓여놔야겠어요",
          "speaker_id": "1",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100043349.1.28",
