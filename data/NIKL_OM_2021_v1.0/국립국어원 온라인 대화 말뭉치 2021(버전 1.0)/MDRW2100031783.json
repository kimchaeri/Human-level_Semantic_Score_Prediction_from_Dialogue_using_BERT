      "utterance": [
        {
          "id": "MDRW2100031783.1.1",
          "form": "자전거여행!",
          "original_form": "자전거여행!",
          "speaker_id": "2",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100031783.1.2",
          "form": "자전거면 멀리는 못가지",
          "original_form": "자전거면 멀리는 못가지",
          "speaker_id": "1",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100031783.1.3",
          "form": "엉덩이 힘들걸 ?",
          "original_form": "엉덩이 힘들걸 ?",
          "speaker_id": "1",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100031783.1.4",
          "form": "기차는 어때 ?",
          "original_form": "기차는 어때 ?",
          "speaker_id": "1",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031783.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ아니 여행지에서 타는거",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아니 여행지에서 타는거",
          "speaker_id": "2",
          "time": "20210905 10:45"
        },
        {
          "id": "MDRW2100031783.1.6",
          "form": "기차타고 가서",
          "original_form": "기차타고 가서",
          "speaker_id": "2",
          "time": "20210905 10:45"
        },
        {
          "id": "MDRW2100031783.1.7",
          "form": "타면 되겠네",
          "original_form": "타면 되겠네",
          "speaker_id": "2",
          "time": "20210905 10:45"
        },
        {
          "id": "MDRW2100031783.1.8",
          "form": "아 그거",
          "original_form": "아 그거",
          "speaker_id": "1",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100031783.1.9",
          "form": "아닌면 어디가면 레일바이크 타는곳 있던데",
          "original_form": "아닌면 어디가면 레일바이크 타는곳 있던데",
          "speaker_id": "1",
          "time": "20210905 10:47"
        },
        {
          "id": "MDRW2100031783.1.10",
          "form": "그것두있구",
          "original_form": "그것두있구",
          "speaker_id": "2",
          "time": "20210905 10:58"
        },
        {
          "id": "MDRW2100031783.1.11",
          "form": "세종에서 그때 못탄거ㅠ",
          "original_form": "세종에서 그때 못탄거ㅠ",
          "speaker_id": "2",
          "time": "20210905 10:58"
        },
        {
          "id": "MDRW2100031783.1.12",
          "form": "세종시 또 가자구",
          "original_form": "세종시 또 가자구",
          "speaker_id": "1",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100031783.1.13",
          "form": "그것도 좋구!!!",
          "original_form": "그것도 좋구!!!",
          "speaker_id": "2",
          "time": "20210905 11:13"
        },
        {
          "id": "MDRW2100031783.1.14",
          "form": "이번엔 좀 다른곳으로 가자",
          "original_form": "이번엔 좀 다른곳으로 가자",
          "speaker_id": "1",
          "time": "20210905 11:50"
        },
        {
          "id": "MDRW2100031783.1.15",
          "form": "어디~",
          "original_form": "어디~",
          "speaker_id": "2",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100031783.1.16",
          "form": "글쎄 좀 나이지면 강원도 쪽으로 갈까 ?",
          "original_form": "글쎄 좀 나이지면 강원도 쪽으로 갈까 ?",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100031783.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ난 강원도 갛다왔ㅇㄷ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ난 강원도 갛다왔ㅇㄷ",
          "speaker_id": "2",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100031783.1.18",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100031783.1.19",
          "form": "동생 군대 데려다줬자나",
          "original_form": "동생 군대 데려다줬자나",
          "speaker_id": "2",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100031783.1.20",
          "form": "엄마빼규",
          "original_form": "엄마빼규",
          "speaker_id": "2",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100031783.1.21",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100031783.1.22",
          "form": "그럼 남쪽으로 가야하나 ?",
          "original_form": "그럼 남쪽으로 가야하나 ?",
          "speaker_id": "1",
          "time": "20210905 12:08"
