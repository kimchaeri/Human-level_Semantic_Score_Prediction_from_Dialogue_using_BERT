      "utterance": [
        {
          "id": "MDRW2100027397.1.1",
          "form": "일이 힘드네요",
          "original_form": "일이 힘드네요",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100027397.1.2",
          "form": "맞아요....ㅠㅠ",
          "original_form": "맞아요....ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100027397.1.3",
          "form": "실업자보다는 나은데",
          "original_form": "실업자보다는 나은데",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100027397.1.4",
          "form": "일을 하면 너무 힘들어요",
          "original_form": "일을 하면 너무 힘들어요",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100027397.1.5",
          "form": "전 일을쉬어본적이없어요",
          "original_form": "전 일을쉬어본적이없어요",
          "speaker_id": "1",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100027397.1.6",
          "form": "부지런하시네요",
          "original_form": "부지런하시네요",
          "speaker_id": "2",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100027397.1.7",
          "form": "건 아니고요ㅜ 대학졸업후바로취직. 전역후 바로 복직ㅜ",
          "original_form": "건 아니고요ㅜ 대학졸업후바로취직. 전역후 바로 복직ㅜ",
          "speaker_id": "1",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100027397.1.8",
          "form": "우와~~!!!좋네요",
          "original_form": "우와~~!!!좋네요",
          "speaker_id": "2",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100027397.1.9",
          "form": "ㅜㅜ 돈을 헤프게 썼어요 20대에ㅋ",
          "original_form": "ㅜㅜ 돈을 헤프게 썼어요 20대에ㅋ",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027397.1.10",
          "form": "앜ㅋㅋ어떡해",
          "original_form": "앜ㅋㅋ어떡해",
          "speaker_id": "2",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100027397.1.11",
          "form": "일을해서 돈 많이 벌어야겠네요",
          "original_form": "일을해서 돈 많이 벌어야겠네요",
          "speaker_id": "2",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100027397.1.12",
          "form": "어떻게 돈을 썼길래...",
          "original_form": "어떻게 돈을 썼길래...",
          "speaker_id": "2",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100027397.1.13",
          "form": "게임에 쓰고..여자 만나고ㅋㅜㅜ",
          "original_form": "게임에 쓰고..여자 만나고ㅋㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100027397.1.14",
          "form": "앜ㅋㅋ어떡해",
          "original_form": "앜ㅋㅋ어떡해",
          "speaker_id": "2",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100027397.1.15",
          "form": "게임은 멀 했어요?",
          "original_form": "게임은 멀 했어요?",
          "speaker_id": "2",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100027397.1.16",
          "form": "직업이 안정성이 있어서 방심한듯요",
          "original_form": "직업이 안정성이 있어서 방심한듯요",
          "speaker_id": "1",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100027397.1.17",
          "form": "듣보잡게임요ㅋ 텐비라고",
          "original_form": "듣보잡게임요ㅋ 텐비라고",
          "speaker_id": "1",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100027397.1.18",
          "form": "앜ㅋㅋ몰라요",
          "original_form": "앜ㅋㅋ몰라요",
          "speaker_id": "2",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100027397.1.19",
          "form": "저는 카트",
          "original_form": "저는 카트",
          "speaker_id": "2",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100027397.1.20",
          "form": "연애는 지금 하고 있어요?",
          "original_form": "연애는 지금 하고 있어요?",
          "speaker_id": "2",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100027397.1.21",
          "form": "일만 하고 있어요",
          "original_form": "일만 하고 있어요",
          "speaker_id": "1",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100027397.1.22",
          "form": "악ㅋㅋㅋㄱ일에만 집중하는군용",
