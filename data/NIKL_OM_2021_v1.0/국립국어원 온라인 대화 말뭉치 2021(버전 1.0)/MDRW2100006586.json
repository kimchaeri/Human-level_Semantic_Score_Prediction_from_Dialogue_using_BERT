      "utterance": [
        {
          "id": "MDRW2100006586.1.1",
          "form": "책 좋아해요?",
          "original_form": "책 좋아해요?",
          "speaker_id": "1",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100006586.1.2",
          "form": "책 읽으세요?",
          "original_form": "책 읽으세요?",
          "speaker_id": "2",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100006586.1.3",
          "form": "네 전 좋아해요",
          "original_form": "네 전 좋아해요",
          "speaker_id": "1",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100006586.1.4",
          "form": "좋아하긴 해요",
          "original_form": "좋아하긴 해요",
          "speaker_id": "2",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100006586.1.5",
          "form": "지금은 트라우마 읽어요",
          "original_form": "지금은 트라우마 읽어요",
          "speaker_id": "1",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100006586.1.6",
          "form": "전 읽냐고 물엌ㅅ는데",
          "original_form": "전 읽냐고 물엌ㅅ는데",
          "speaker_id": "2",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100006586.1.7",
          "form": "네.",
          "original_form": "네.",
          "speaker_id": "2",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100006586.1.8",
          "form": "옛날에 밀리에서제에서 읽다",
          "original_form": "옛날에 밀리에서제에서 읽다",
          "speaker_id": "1",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100006586.1.9",
          "form": "뭔 내용이예요?",
          "original_form": "뭔 내용이예요?",
          "speaker_id": "2",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100006586.1.10",
          "form": "기간끝나서 도서관에서 빌렸어요",
          "original_form": "기간끝나서 도서관에서 빌렸어요",
          "speaker_id": "1",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100006586.1.11",
          "form": "pstd의 학명이 정해지기까지 각종 사고피해자의",
          "original_form": "pstd의 학명이 정해지기까지 각종 사고피해자의",
          "speaker_id": "1",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100006586.1.12",
          "form": "와.. 좀 암울하고",
          "original_form": "와.. 좀 암울하고",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100006586.1.13",
          "form": "기록이요",
          "original_form": "기록이요",
          "speaker_id": "1",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100006586.1.14",
          "form": "많이 어려울 것 같네요",
          "original_form": "많이 어려울 것 같네요",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100006586.1.15",
          "form": "가정폭력 전쟁포로 정치범",
          "original_form": "가정폭력 전쟁포로 정치범",
          "speaker_id": "1",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100006586.1.16",
          "form": "정치범까지요?",
          "original_form": "정치범까지요?",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100006586.1.17",
          "form": "그냥 911도요",
          "original_form": "그냥 911도요",
          "speaker_id": "1",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100006586.1.18",
          "form": "아 하긴 그럴 수 있네요",
          "original_form": "아 하긴 그럴 수 있네요",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100006586.1.19",
          "form": "ㅋㅋㅋ 외국쪽인가봐요",
          "original_form": "ㅋㅋㅋ 외국쪽인가봐요",
          "speaker_id": "2",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100006586.1.20",
          "form": "내 정치범도 보통 수용소에 넣어요",
          "original_form": "내 정치범도 보통 수용소에 넣어요",
          "speaker_id": "1",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100006586.1.21",
          "form": "나인 원 원. ~",
          "original_form": "나인 원 원. ~",
          "speaker_id": "2",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100006586.1.22",
          "form": "외국책이라서요 ㅋㅋ",
          "original_form": "외국책이라서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100006586.1.23",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "2",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100006586.1.24",
          "form": "재밌어요 하나의 사회가 공유한 트라우마라고 하내요",
          "original_form": "재밌어요 하나의 사회가 공유한 트라우마라고 하내요",
          "speaker_id": "1",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100006586.1.25",
          "form": "이책이 유명한게",
          "original_form": "이책이 유명한게",
          "speaker_id": "1",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100006586.1.26",
          "form": "저희는 교통사고당하면 이제 pstd가 올껄 알잖아요",
          "original_form": "저희는 교통사고당하면 이제 pstd가 올껄 알잖아요",
          "speaker_id": "1",
          "time": "20210908 02:56"
        }
