      "utterance": [
        {
          "id": "MDRW2100003633.1.1",
          "form": "반려동물 기르시나요",
          "original_form": "반려동물 기르시나요",
          "speaker_id": "1",
          "time": "20210821 09:23"
        },
        {
          "id": "MDRW2100003633.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210821 09:24"
        },
        {
          "id": "MDRW2100003633.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210821 09:24"
        },
        {
          "id": "MDRW2100003633.1.4",
          "form": "강아지 좋아하는데",
          "original_form": "강아지 좋아하는데",
          "speaker_id": "2",
          "time": "20210821 09:24"
        },
        {
          "id": "MDRW2100003633.1.5",
          "form": "반려동물길러요 고양이랑 진돗개",
          "original_form": "반려동물길러요 고양이랑 진돗개",
          "speaker_id": "1",
          "time": "20210821 09:24"
        },
        {
          "id": "MDRW2100003633.1.6",
          "form": "부모님이 시러하셔서",
          "original_form": "부모님이 시러하셔서",
          "speaker_id": "2",
          "time": "20210821 09:24"
        },
        {
          "id": "MDRW2100003633.1.7",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210821 09:24"
        },
        {
          "id": "MDRW2100003633.1.8",
          "form": "진돗개요?",
          "original_form": "진돗개요?",
          "speaker_id": "2",
          "time": "20210821 09:24"
        },
        {
          "id": "MDRW2100003633.1.9",
          "form": "네 진돗개요",
          "original_form": "네 진돗개요",
          "speaker_id": "1",
          "time": "20210821 09:24"
        },
        {
          "id": "MDRW2100003633.1.10",
          "form": "집안에서?",
          "original_form": "집안에서?",
          "speaker_id": "2",
          "time": "20210821 09:24"
        },
        {
          "id": "MDRW2100003633.1.11",
          "form": "마당에서요",
          "original_form": "마당에서요",
          "speaker_id": "1",
          "time": "20210821 09:24"
        },
        {
          "id": "MDRW2100003633.1.12",
          "form": "아빠가 사슴농장에 있던 애 데려왔어요ㅋㅋ",
          "original_form": "아빠가 사슴농장에 있던 애 데려왔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 09:24"
        },
        {
          "id": "MDRW2100003633.1.13",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210821 09:24"
        },
        {
          "id": "MDRW2100003633.1.14",
          "form": "주택사시는구나",
          "original_form": "주택사시는구나",
          "speaker_id": "2",
          "time": "20210821 09:24"
        },
        {
          "id": "MDRW2100003633.1.15",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "2",
          "time": "20210821 09:24"
        },
        {
          "id": "MDRW2100003633.1.16",
          "form": "ㅎ.. 장단점이 있죠",
          "original_form": "ㅎ.. 장단점이 있죠",
          "speaker_id": "1",
          "time": "20210821 09:25"
        },
        {
          "id": "MDRW2100003633.1.17",
          "form": "고양이는 한마리?",
          "original_form": "고양이는 한마리?",
          "speaker_id": "2",
          "time": "20210821 09:25"
        },
        {
          "id": "MDRW2100003633.1.18",
          "form": "개냥이 인가요?",
          "original_form": "개냥이 인가요?",
          "speaker_id": "2",
          "time": "20210821 09:25"
        },
        {
          "id": "MDRW2100003633.1.19",
          "form": "네 고양이도 한마리 길에서 주워왔는데 성질 겁나더러워요",
          "original_form": "네 고양이도 한마리 길에서 주워왔는데 성질 겁나더러워요",
          "speaker_id": "1",
          "time": "20210821 09:25"
        },
        {
          "id": "MDRW2100003633.1.20",
          "form": "ㅎ.. 진짜 더러워요",
          "original_form": "ㅎ.. 진짜 더러워요",
          "speaker_id": "1",
          "time": "20210821 09:26"
        },
