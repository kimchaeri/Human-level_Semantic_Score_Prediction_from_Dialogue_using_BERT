      "utterance": [
        {
          "id": "MDRW2100007736.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007736.1.2",
          "form": "배달먹어요?",
          "original_form": "배달먹어요?",
          "speaker_id": "1",
          "time": "20210822 10:03"
        },
        {
          "id": "MDRW2100007736.1.3",
          "form": "아뇨 ㅋㅋ안먹었어요",
          "original_form": "아뇨 ㅋㅋ안먹었어요",
          "speaker_id": "2",
          "time": "20210822 10:03"
        },
        {
          "id": "MDRW2100007736.1.4",
          "form": "배달 넘 많이 먹어서",
          "original_form": "배달 넘 많이 먹어서",
          "speaker_id": "2",
          "time": "20210822 10:03"
        },
        {
          "id": "MDRW2100007736.1.5",
          "form": "어제 소고기 사서",
          "original_form": "어제 소고기 사서",
          "speaker_id": "2",
          "time": "20210822 10:03"
        },
        {
          "id": "MDRW2100007736.1.6",
          "form": "이따 늦게 스테이크 하려고요",
          "original_form": "이따 늦게 스테이크 하려고요",
          "speaker_id": "2",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007736.1.7",
          "form": "아점으로요",
          "original_form": "아점으로요",
          "speaker_id": "2",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007736.1.8",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007736.1.9",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007736.1.10",
          "form": "생각중이에요",
          "original_form": "생각중이에요",
          "speaker_id": "1",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007736.1.11",
          "form": "다들 주말은 늦게먹죠",
          "original_form": "다들 주말은 늦게먹죠",
          "speaker_id": "2",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100007736.1.12",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100007736.1.13",
          "form": "배달 하시게요?",
          "original_form": "배달 하시게요?",
          "speaker_id": "2",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100007736.1.14",
          "form": "뭐 주로 배달하세요?",
          "original_form": "뭐 주로 배달하세요?",
          "speaker_id": "2",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100007736.1.15",
          "form": "치킨뇨",
          "original_form": "치킨뇨",
          "speaker_id": "1",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007736.1.16",
          "form": "ㅋㅋ다 치킨이네요",
          "original_form": "ㅋㅋ다 치킨이네요",
          "speaker_id": "2",
          "time": "20210822 10:07"
        },
        {
          "id": "MDRW2100007736.1.17",
          "form": "어제 저 치킨 초밥",
          "original_form": "어제 저 치킨 초밥",
          "speaker_id": "2",
          "time": "20210822 10:07"
        },
        {
          "id": "MDRW2100007736.1.18",
          "form": "아 다 좋아하는데",
          "original_form": "아 다 좋아하는데",
          "speaker_id": "1",
          "time": "20210822 10:07"
        },
        {
          "id": "MDRW2100007736.1.19",
          "form": "네 그래서 둘다 먹었죠",
          "original_form": "네 그래서 둘다 먹었죠",
          "speaker_id": "2",
          "time": "20210822 10:08"
        },
        {
          "id": "MDRW2100007736.1.20",
          "form": "둘다 드세요",
          "original_form": "둘다 드세요",
          "speaker_id": "2",
          "time": "20210822 10:08"
        },
        {
          "id": "MDRW2100007736.1.21",
          "form": "결정 힘들때 둘다 먹어야죠 ㅎㅎ",
          "original_form": "결정 힘들때 둘다 먹어야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100007736.1.22",
          "form": "정답",
          "original_form": "정답",
          "speaker_id": "1",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007736.1.23",
          "form": "어서 초밥 사오고 치킨 시키세요 ㅎㅎ",
          "original_form": "어서 초밥 사오고 치킨 시키세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 10:28"
        },
        {
          "id": "MDRW2100007736.1.24",
          "form": "아침부터눈 좀 ㅎㅎ",
          "original_form": "아침부터눈 좀 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 11:35"
        }
