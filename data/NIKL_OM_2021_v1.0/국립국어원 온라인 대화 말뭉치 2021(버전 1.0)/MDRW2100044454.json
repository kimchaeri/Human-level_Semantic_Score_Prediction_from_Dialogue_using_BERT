      "utterance": [
        {
          "id": "MDRW2100044454.1.1",
          "form": "왜요? 미끄러워서 그런가요?",
          "original_form": "왜요? 미끄러워서 그런가요?",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044454.1.2",
          "form": "네네 ㅋㅋ저원래 학생때 눈오면 학교안갈정도였어요",
          "original_form": "네네 ㅋㅋ저원래 학생때 눈오면 학교안갈정도였어요",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044454.1.3",
          "form": "심하게 넘어진적이있어서",
          "original_form": "심하게 넘어진적이있어서",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044454.1.4",
          "form": "지금도 눈길 잘못걸어요 ..",
          "original_form": "지금도 눈길 잘못걸어요 ..",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044454.1.5",
          "form": "오 맞아요.. 저도 진짜 잘 미끄러져서 눈오는 거 싫어했어요",
          "original_form": "오 맞아요.. 저도 진짜 잘 미끄러져서 눈오는 거 싫어했어요",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044454.1.6",
          "form": "특히 학겨가는길에 내리막길 규간이 있는데.. 진짜 너무 싫었어요",
          "original_form": "특히 학겨가는길에 내리막길 규간이 있는데.. 진짜 너무 싫었어요",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044454.1.7",
          "form": "으.. 맞아요",
          "original_form": "으.. 맞아요",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044454.1.8",
          "form": "저도 고등학교가 오르막이었는데",
          "original_form": "저도 고등학교가 오르막이었는데",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044454.1.9",
          "form": "그래서 잘안갔어욬ㅋㅋㅋㅋ",
          "original_form": "그래서 잘안갔어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044454.1.10",
          "form": "핑계일수있겠지만 ;",
          "original_form": "핑계일수있겠지만 ;",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044454.1.11",
          "form": "ㅋㅋㅋㅋㅋ 그럴 수 있죠",
          "original_form": "ㅋㅋㅋㅋㅋ 그럴 수 있죠",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044454.1.12",
          "form": "이번 겨울에는 과연 눈이 많이 올까요..,?",
          "original_form": "이번 겨울에는 과연 눈이 많이 올까요..,?",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044454.1.13",
          "form": "저 정말ㅠㅠ 너무 무서워서",
          "original_form": "저 정말ㅠㅠ 너무 무서워서",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044454.1.14",
          "form": "항상 애들이 부축?가까이해서 학교갈정도였어요",
          "original_form": "항상 애들이 부축?가까이해서 학교갈정도였어요",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044454.1.15",
          "form": "트라우마가 너무심해가지구 ㅠㅠ",
          "original_form": "트라우마가 너무심해가지구 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044454.1.16",
          "form": "그쵸 ㅠㅠ 한번 그런일있으면 진짜 걷기 무섭더라구요..",
          "original_form": "그쵸 ㅠㅠ 한번 그런일있으면 진짜 걷기 무섭더라구요..",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044454.1.17",
          "form": "근데 나이드니깐 혼자",
          "original_form": "근데 나이드니깐 혼자",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044454.1.18",
          "form": "가야될때가 많아서",
          "original_form": "가야될때가 많아서",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044454.1.19",
          "form": "조심히 잘다니고",
          "original_form": "조심히 잘다니고",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044454.1.20",
          "form": "운전도해서 많이 나아졌어용 ㅎㅎ",
          "original_form": "운전도해서 많이 나아졌어용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044454.1.21",
          "form": "조금 극복하셔서 다행이에요 ㅎㅎ",
          "original_form": "조금 극복하셔서 다행이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044454.1.22",
          "form": "이번 겨울에는 추울까요 ㅠㅠ?",
          "original_form": "이번 겨울에는 추울까요 ㅠㅠ?",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044454.1.23",
          "form": "추울거같지 않아요 ㅠㅠ?",
          "original_form": "추울거같지 않아요 ㅠㅠ?",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044454.1.24",
          "form": "그쵸 ..이번 여름도 심상치않아서..",
          "original_form": "그쵸 ..이번 여름도 심상치않아서..",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044454.1.25",
          "form": "겨울도 걱정되네요",
          "original_form": "겨울도 걱정되네요",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044454.1.26",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044454.1.27",
          "form": "혹시 몇시 주무세요?",
          "original_form": "혹시 몇시 주무세요?",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044454.1.28",
