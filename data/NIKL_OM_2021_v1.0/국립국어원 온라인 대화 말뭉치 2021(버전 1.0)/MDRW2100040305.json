      "utterance": [
        {
          "id": "MDRW2100040305.1.1",
          "form": "아 아쉽습니다~~~~아니예요",
          "original_form": "아 아쉽습니다~~~~아니예요",
          "speaker_id": "2",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100040305.1.2",
          "form": "앗 우유 종류가 아니라구요?",
          "original_form": "앗 우유 종류가 아니라구요?",
          "speaker_id": "1",
          "time": "20210904 15:56"
        },
        {
          "id": "MDRW2100040305.1.3",
          "form": "후후후후 우유랑 비슷해요",
          "original_form": "후후후후 우유랑 비슷해요",
          "speaker_id": "2",
          "time": "20210904 15:56"
        },
        {
          "id": "MDRW2100040305.1.4",
          "form": "뭐지 달콤한가요?",
          "original_form": "뭐지 달콤한가요?",
          "speaker_id": "1",
          "time": "20210904 15:57"
        },
        {
          "id": "MDRW2100040305.1.5",
          "form": "네 달달하답니다",
          "original_form": "네 달달하답니다",
          "speaker_id": "2",
          "time": "20210904 15:58"
        },
        {
          "id": "MDRW2100040305.1.6",
          "form": "탁하고 얼음은 없고 시원한거...뭐지",
          "original_form": "탁하고 얼음은 없고 시원한거...뭐지",
          "speaker_id": "1",
          "time": "20210904 15:59"
        },
        {
          "id": "MDRW2100040305.1.7",
          "form": "두유... 인가요?",
          "original_form": "두유... 인가요?",
          "speaker_id": "1",
          "time": "20210904 15:59"
        },
        {
          "id": "MDRW2100040305.1.8",
          "form": "딩동댕 맞아요 두유!",
          "original_form": "딩동댕 맞아요 두유!",
          "speaker_id": "2",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100040305.1.9",
          "form": "아니 되게 빨리 맞추셨네요?",
          "original_form": "아니 되게 빨리 맞추셨네요?",
          "speaker_id": "2",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100040305.1.10",
          "form": "추상적이라 오래 걸릴 줄 알았는데 ㅋㅋㅋ",
          "original_form": "추상적이라 오래 걸릴 줄 알았는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100040305.1.11",
          "form": "찬 음료인데 얼음을 안 넣는다는 힌트가 주요했어요ㅋㅋㅋ",
          "original_form": "찬 음료인데 얼음을 안 넣는다는 힌트가 주요했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100040305.1.12",
          "form": "ㅋㅋㅋㅋ앗 그랬군요 그게 핵심이었구나",
          "original_form": "ㅋㅋㅋㅋ앗 그랬군요 그게 핵심이었구나",
          "speaker_id": "2",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100040305.1.13",
          "form": "두유에 얼음 넣으면 밍밍해지니께ㅎ",
          "original_form": "두유에 얼음 넣으면 밍밍해지니께ㅎ",
          "speaker_id": "2",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100040305.1.14",
          "form": "그쵸그쵸ㅋㅋㅋㅋ",
          "original_form": "그쵸그쵸ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100040305.1.15",
          "form": "간식으로 드시는 건가요",
          "original_form": "간식으로 드시는 건가요",
          "speaker_id": "1",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100040305.1.16",
          "form": "네 점심 먹고 입이 심심해서 먹고 있어요",
          "original_form": "네 점심 먹고 입이 심심해서 먹고 있어요",
          "speaker_id": "2",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100040305.1.17",
          "form": "병두유게요 팩두유게요?",
          "original_form": "병두유게요 팩두유게요?",
          "speaker_id": "2",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100040305.1.18",
          "form": "ㄴㅔ...? 팩두유...!! 일단찍기",
          "original_form": "ㄴㅔ...? 팩두유...!! 일단찍기",
          "speaker_id": "1",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100040305.1.19",
          "form": "오매 정답이예요",
          "original_form": "오매 정답이예요",
          "speaker_id": "2",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100040305.1.20",
          "form": "오늘따라 엄청 스피디하게 맞추셔요",
          "original_form": "오늘따라 엄청 스피디하게 맞추셔요",
          "speaker_id": "2",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100040305.1.21",
          "form": "스고이",
          "original_form": "스고이",
          "speaker_id": "2",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100040305.1.22",
          "form": "아 오늘 좀 음식감이 좋은가 봐요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 오늘 좀 음식감이 좋은가 봐요 ㅋㅋㅋㅋㅋㅋㅋ",
