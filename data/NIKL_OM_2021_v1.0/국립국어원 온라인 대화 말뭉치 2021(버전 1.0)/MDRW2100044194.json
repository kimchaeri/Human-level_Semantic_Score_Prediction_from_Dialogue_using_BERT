      "utterance": [
        {
          "id": "MDRW2100044194.1.1",
          "form": "저번주꺼?",
          "original_form": "저번주꺼?",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044194.1.2",
          "form": "봤지~",
          "original_form": "봤지~",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044194.1.3",
          "form": "진짜 마지막에 누군지 너무 궁금!!",
          "original_form": "진짜 마지막에 누군지 너무 궁금!!",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044194.1.4",
          "form": "저번주는 결방하지 않았어??",
          "original_form": "저번주는 결방하지 않았어??",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044194.1.5",
          "form": "저저번주꺼봤어",
          "original_form": "저저번주꺼봤어",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044194.1.6",
          "form": "아 맞다!!",
          "original_form": "아 맞다!!",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044194.1.7",
          "form": "축구함다고 결방했던 듯~",
          "original_form": "축구함다고 결방했던 듯~",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044194.1.8",
          "form": "이번주는 하는 거지??",
          "original_form": "이번주는 하는 거지??",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044194.1.9",
          "form": "그럴걸요~",
          "original_form": "그럴걸요~",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044194.1.10",
          "form": "2주연속 결방하면 고건 너무함!!",
          "original_form": "2주연속 결방하면 고건 너무함!!",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044194.1.11",
          "form": "ㅎㅎ이번에도 본방봐야겠네",
          "original_form": "ㅎㅎ이번에도 본방봐야겠네",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044194.1.12",
          "form": "나는 기다렸다가 넷플로",
          "original_form": "나는 기다렸다가 넷플로",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044194.1.13",
          "form": "왜~ 본방같이 보지~",
          "original_form": "왜~ 본방같이 보지~",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044194.1.14",
          "form": "난 혼자 봐야 집중잘되고 좋더라규요#",
          "original_form": "난 혼자 봐야 집중잘되고 좋더라규요#",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044194.1.15",
          "form": "ㅎㅎ엄마는 아빠랑 같이 봐야겠다~",
          "original_form": "ㅎㅎ엄마는 아빠랑 같이 봐야겠다~",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044194.1.16",
          "form": "웅~ 두분이서 오붓하게 봐요~",
          "original_form": "웅~ 두분이서 오붓하게 봐요~",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044194.1.17",
          "form": "몇시에하지??",
          "original_form": "몇시에하지??",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044194.1.18",
          "form": "밤.9시 반일걸~",
          "original_form": "밤.9시 반일걸~",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044194.1.19",
          "form": "엄마 어제 오늘 넷플릭스 잘 보네",
