      "utterance": [
        {
          "id": "MDRW2100012769.1.1",
          "form": "하얀나무 같은거 말이지",
          "original_form": "하얀나무 같은거 말이지",
          "speaker_id": "2",
          "time": "20210825 00:56"
        },
        {
          "id": "MDRW2100012769.1.2",
          "form": "엉 노르웨이 가면 자작나무숲이 많은데",
          "original_form": "엉 노르웨이 가면 자작나무숲이 많은데",
          "speaker_id": "1",
          "time": "20210825 00:57"
        },
        {
          "id": "MDRW2100012769.1.3",
          "form": "노르웨이에 많구나",
          "original_form": "노르웨이에 많구나",
          "speaker_id": "2",
          "time": "20210825 00:57"
        },
        {
          "id": "MDRW2100012769.1.4",
          "form": "거기처럼 우리나라에도 비슷하게 꾸며놓은데가 있데",
          "original_form": "거기처럼 우리나라에도 비슷하게 꾸며놓은데가 있데",
          "speaker_id": "1",
          "time": "20210825 00:57"
        },
        {
          "id": "MDRW2100012769.1.5",
          "form": "아 정말",
          "original_form": "아 정말",
          "speaker_id": "2",
          "time": "20210825 00:57"
        },
        {
          "id": "MDRW2100012769.1.6",
          "form": "응 평창에 있어",
          "original_form": "응 평창에 있어",
          "speaker_id": "1",
          "time": "20210825 00:58"
        },
        {
          "id": "MDRW2100012769.1.7",
          "form": "평창이면 너무 멀다",
          "original_form": "평창이면 너무 멀다",
          "speaker_id": "2",
          "time": "20210825 00:59"
        },
        {
          "id": "MDRW2100012769.1.8",
          "form": "새하얀 수피로 둘렀아니 자작나무가 엄청 많데",
          "original_form": "새하얀 수피로 둘렀아니 자작나무가 엄청 많데",
          "speaker_id": "1",
          "time": "20210825 01:00"
        },
        {
          "id": "MDRW2100012769.1.9",
          "form": "아~사진찍으면 인생사진이겠다",
          "original_form": "아~사진찍으면 인생사진이겠다",
          "speaker_id": "2",
          "time": "20210825 01:00"
        },
        {
          "id": "MDRW2100012769.1.10",
          "form": "그렇지 쭉 뻗은 나무를 배경으로 하면",
          "original_form": "그렇지 쭉 뻗은 나무를 배경으로 하면",
          "speaker_id": "1",
          "time": "20210825 01:01"
        },
        {
          "id": "MDRW2100012769.1.11",
          "form": "상상이 된다",
          "original_form": "상상이 된다",
          "speaker_id": "2",
          "time": "20210825 01:01"
        },
        {
          "id": "MDRW2100012769.1.12",
          "form": "근처에 5일장도 있어서 식도락도 있을듯",
          "original_form": "근처에 5일장도 있어서 식도락도 있을듯",
          "speaker_id": "1",
          "time": "20210825 01:01"
        },
        {
          "id": "MDRW2100012769.1.13",
          "form": "5일장이면 날짜를 알아가야겠다",
          "original_form": "5일장이면 날짜를 알아가야겠다",
          "speaker_id": "2",
