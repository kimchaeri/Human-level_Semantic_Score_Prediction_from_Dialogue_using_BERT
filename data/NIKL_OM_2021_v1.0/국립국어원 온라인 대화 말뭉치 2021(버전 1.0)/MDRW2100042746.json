      "utterance": [
        {
          "id": "MDRW2100042746.1.1",
          "form": "아침드시나요,?ㅎ",
          "original_form": "아침드시나요,?ㅎ",
          "speaker_id": "1",
          "time": "20210907 08:16"
        },
        {
          "id": "MDRW2100042746.1.2",
          "form": "아침은 그냥 넘겨요ㅠㅜ",
          "original_form": "아침은 그냥 넘겨요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100042746.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100042746.1.4",
          "form": "전 빵있음 빵먹고ㅠ",
          "original_form": "전 빵있음 빵먹고ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100042746.1.5",
          "form": "없으면 과자..좀 줏어먹고 ㅎㅎ",
          "original_form": "없으면 과자..좀 줏어먹고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100042746.1.6",
          "form": "ㅋㅋㅋ앗 그런것도 밥이라고 한다면 저도 먹는거네요...ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ앗 그런것도 밥이라고 한다면 저도 먹는거네요...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100042746.1.7",
          "form": "밥은 아니지만 끼니는..떼우니..ㅋㅋㅋㅋㅋ",
          "original_form": "밥은 아니지만 끼니는..떼우니..ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100042746.1.8",
          "form": "오늘 저녁 뭐드셨어요?",
          "original_form": "오늘 저녁 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100042746.1.9",
          "form": "저녁에 오겹살!!!!먹었어용",
          "original_form": "저녁에 오겹살!!!!먹었어용",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100042746.1.10",
          "form": "헉 맛있었겠어요!",
          "original_form": "헉 맛있었겠어요!",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100042746.1.11",
          "form": "삼겹살도 아닌 오겹살!",
          "original_form": "삼겹살도 아닌 오겹살!",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100042746.1.12",
          "form": "맛있엇어여ㅠㅠㅠ 비계좀 많긴해도 쫀득하니",
          "original_form": "맛있엇어여ㅠㅠㅠ 비계좀 많긴해도 쫀득하니",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100042746.1.13",
          "form": "ㅜㅜ알죠알죠 같이사는 사람이 오겹살을 안좋아해서 안먹은지 오래됐어요ㅜㅜㅜ",
          "original_form": "ㅜㅜ알죠알죠 같이사는 사람이 오겹살을 안좋아해서 안먹은지 오래됐어요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100042746.1.14",
          "form": "집에서드셨어요?",
          "original_form": "집에서드셨어요?",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100042746.1.15",
          "form": "네 ㅎㅎ ㅎㅎ 후라이팬에!",
          "original_form": "네 ㅎㅎ ㅎㅎ 후라이팬에!",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100042746.1.16",
          "form": "맛있었겠다... 뒷정리가 불편하긴 하지만 집에서 먹어야 편하고 맛있어요ㅜㅜㅜ",
          "original_form": "맛있었겠다... 뒷정리가 불편하긴 하지만 집에서 먹어야 편하고 맛있어요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100042746.1.17",
          "form": "제가 오늘 삼겹살이 땡겻는데 못먹어섴ㅋㅋㅋㅜㅜ",
          "original_form": "제가 오늘 삼겹살이 땡겻는데 못먹어섴ㅋㅋㅋㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100042746.1.18",
          "form": "저내일은 파스타먹어용",
          "original_form": "저내일은 파스타먹어용",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100042746.1.19",
          "form": "자랑아니에욬ㅋㅋㅋㅋ",
          "original_form": "자랑아니에욬ㅋㅋㅋㅋ",
