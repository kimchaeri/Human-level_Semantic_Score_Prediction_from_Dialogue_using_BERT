      "utterance": [
        {
          "id": "MDRW2100042672.1.1",
          "form": "스타벅스 텀블러를 같이 골라주라",
          "original_form": "스타벅스 텀블러를 같이 골라주라",
          "speaker_id": "2",
          "time": "20210907 00:55"
        },
        {
          "id": "MDRW2100042672.1.2",
          "form": "전에 산 거 아직 안 썼잖아",
          "original_form": "전에 산 거 아직 안 썼잖아",
          "speaker_id": "1",
          "time": "20210907 00:55"
        },
        {
          "id": "MDRW2100042672.1.3",
          "form": "스타벅스 텀블러는 많을수록 좋아",
          "original_form": "스타벅스 텀블러는 많을수록 좋아",
          "speaker_id": "2",
          "time": "20210907 00:56"
        },
        {
          "id": "MDRW2100042672.1.4",
          "form": "무슨 재질 살 건데",
          "original_form": "무슨 재질 살 건데",
          "speaker_id": "1",
          "time": "20210907 00:56"
        },
        {
          "id": "MDRW2100042672.1.5",
          "form": "플라스틱",
          "original_form": "플라스틱",
          "speaker_id": "2",
          "time": "20210907 00:56"
        },
        {
          "id": "MDRW2100042672.1.6",
          "form": "나는 스테이크 스가 좋던데 스테인레스",
          "original_form": "나는 스테이크 스가 좋던데 스테인레스",
          "speaker_id": "1",
          "time": "20210907 00:56"
        },
        {
          "id": "MDRW2100042672.1.7",
          "form": "플라스틱이 가벼워서 좋아",
          "original_form": "플라스틱이 가벼워서 좋아",
          "speaker_id": "2",
          "time": "20210907 00:56"
        },
        {
          "id": "MDRW2100042672.1.8",
          "form": "가을 시즌 텀블러 살거야?",
          "original_form": "가을 시즌 텀블러 살거야?",
          "speaker_id": "1",
          "time": "20210907 00:57"
        },
        {
          "id": "MDRW2100042672.1.9",
          "form": "가을 거 귀여워 보이더라",
          "original_form": "가을 거 귀여워 보이더라",
          "speaker_id": "2",
          "time": "20210907 00:57"
        },
        {
          "id": "MDRW2100042672.1.10",
          "form": "양은 얼마나 돼 용량",
          "original_form": "양은 얼마나 돼 용량",
          "speaker_id": "1",
          "time": "20210907 00:58"
        },
        {
          "id": "MDRW2100042672.1.11",
          "form": "나는 너무 큰 건 싫어해",
          "original_form": "나는 너무 큰 건 싫어해",
          "speaker_id": "2",
          "time": "20210907 00:58"
        },
        {
          "id": "MDRW2100042672.1.12",
          "form": "톨 사이즈 살 거니",
          "original_form": "톨 사이즈 살 거니",
          "speaker_id": "1",
          "time": "20210907 00:58"
        },
        {
          "id": "MDRW2100042672.1.13",
          "form": "맞아 너는 어떤 사이즈 좋아하니",
          "original_form": "맞아 너는 어떤 사이즈 좋아하니",
          "speaker_id": "2",
          "time": "20210907 00:58"
        },
        {
          "id": "MDRW2100042672.1.14",
          "form": "나는 큰 게 좋더라",
          "original_form": "나는 큰 게 좋더라",
          "speaker_id": "1",
          "time": "20210907 00:59"
        },
        {
          "id": "MDRW2100042672.1.15",
          "form": "큰 게 편할 때 있긴 해",
          "original_form": "큰 게 편할 때 있긴 해",
          "speaker_id": "2",
          "time": "20210907 00:59"
        }
      ]
