        },
        {
          "id": "MDRW2100018294.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018294.1.3",
          "form": "해외여행 다녀온지 정말 오래네요",
          "original_form": "해외여행 다녀온지 정말 오래네요",
          "speaker_id": "2",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018294.1.4",
          "form": "결혼하셨군요ㅋㅋㅋㅋ",
          "original_form": "결혼하셨군요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018294.1.5",
          "form": "코로나 때문에 잘 돌아다니지도 못하고",
          "original_form": "코로나 때문에 잘 돌아다니지도 못하고",
          "speaker_id": "2",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018294.1.6",
          "form": "네 작년에 결혼했어요",
          "original_form": "네 작년에 결혼했어요",
          "speaker_id": "2",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018294.1.7",
          "form": "좋나요?",
          "original_form": "좋나요?",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018294.1.8",
          "form": "결혼하멱",
          "original_form": "결혼하멱",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018294.1.9",
          "form": "음 장점도 있고 단점도 있지만",
          "original_form": "음 장점도 있고 단점도 있지만",
          "speaker_id": "2",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018294.1.10",
          "form": "장점이 더 많은 것 같아요",
          "original_form": "장점이 더 많은 것 같아요",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018294.1.11",
          "form": "아직 결혼안하셨나보네요",
          "original_form": "아직 결혼안하셨나보네요",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018294.1.12",
          "form": "ㅋㅋㅋ 네ㅋㅋㄱ",
          "original_form": "ㅋㅋㅋ 네ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018294.1.13",
          "form": "결혼추천하나요?",
          "original_form": "결혼추천하나요?",
          "speaker_id": "1",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018294.1.14",
          "form": "그런데 저는 아이를 계획하지 않는다면 꼭 결혼할 필요가 있을지는 의문이에요",
          "original_form": "그런데 저는 아이를 계획하지 않는다면 꼭 결혼할 필요가 있을지는 의문이에요",
          "speaker_id": "2",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018294.1.15",
          "form": "어려운 질문이라 대답하기 힘든데",
          "original_form": "어려운 질문이라 대답하기 힘든데",
          "speaker_id": "2",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018294.1.16",
          "form": "요즘",
          "original_form": "요즘",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018294.1.17",
          "form": "딩크족도많지않나요ㅋㅋㅋ",
          "original_form": "딩크족도많지않나요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018294.1.18",
          "form": "그쵸 많죠",
          "original_form": "그쵸 많죠",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018294.1.19",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018294.1.20",
          "form": "결혼은어려운것같아요",
          "original_form": "결혼은어려운것같아요",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018294.1.21",
          "form": "그런경우에 제도적으로 구속하는게 꼭 필요한가",
          "original_form": "그런경우에 제도적으로 구속하는게 꼭 필요한가",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018294.1.22",
          "form": "그렇게 생각해요",
          "original_form": "그렇게 생각해요",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018294.1.23",
          "form": "결혼 예정이세요?",
          "original_form": "결혼 예정이세요?",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018294.1.24",
          "form": "아니요ㅋㅋㄱ",
          "original_form": "아니요ㅋㅋㄱ",
