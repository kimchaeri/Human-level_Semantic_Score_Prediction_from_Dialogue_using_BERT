      "utterance": [
        {
          "id": "MDRW2100025093.1.1",
          "form": "아뇨 지금 엄청 선선해요!",
          "original_form": "아뇨 지금 엄청 선선해요!",
          "speaker_id": "2",
          "time": "20210831 23:23"
        },
        {
          "id": "MDRW2100025093.1.2",
          "form": "여긴 습해여",
          "original_form": "여긴 습해여",
          "speaker_id": "1",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100025093.1.3",
          "form": "아하 비땜에 그런가봐요",
          "original_form": "아하 비땜에 그런가봐요",
          "speaker_id": "2",
          "time": "20210831 23:38"
        },
        {
          "id": "MDRW2100025093.1.4",
          "form": "네 맞아오",
          "original_form": "네 맞아오",
          "speaker_id": "1",
          "time": "20210901 00:00"
        },
        {
          "id": "MDRW2100025093.1.5",
          "form": "여기도 비와서 좀 습하긴한데",
          "original_form": "여기도 비와서 좀 습하긴한데",
          "speaker_id": "2",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100025093.1.6",
          "form": "많이 안와서 그런지",
          "original_form": "많이 안와서 그런지",
          "speaker_id": "2",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100025093.1.7",
          "form": "막 숨막힐정돈 아닌거같아요 ㅎㅎ",
          "original_form": "막 숨막힐정돈 아닌거같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100025093.1.8",
          "form": "여름보다는훨씬",
          "original_form": "여름보다는훨씬",
          "speaker_id": "1",
          "time": "20210901 07:13"
        },
        {
          "id": "MDRW2100025093.1.9",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025093.1.10",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025093.1.11",
          "form": "이번주도 장마 시작이네요 에이구",
          "original_form": "이번주도 장마 시작이네요 에이구",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025093.1.12",
          "form": "금요일까지 온다던데",
          "original_form": "금요일까지 온다던데",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025093.1.13",
          "form": "지겹네여",
          "original_form": "지겹네여",
          "speaker_id": "1",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025093.1.14",
          "form": "그리고 다음주에도 온대요;;",
          "original_form": "그리고 다음주에도 온대요;;",
          "speaker_id": "2",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100025093.1.15",
          "form": "뒤늦게 왜이리 비가 오는지..",
          "original_form": "뒤늦게 왜이리 비가 오는지..",
          "speaker_id": "2",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100025093.1.16",
          "form": "늦장마네요",
          "original_form": "늦장마네요",
          "speaker_id": "1",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025093.1.17",
          "form": "그러게여",
          "original_form": "그러게여",
          "speaker_id": "2",
          "time": "20210901 12:04"
        },
        {
          "id": "MDRW2100025093.1.18",
          "form": "늦어도 너무 늦은 장마네여 ㅠㅠ",
          "original_form": "늦어도 너무 늦은 장마네여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 12:04"
        },
        {
          "id": "MDRW2100025093.1.19",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210901 12:05"
        },
        {
          "id": "MDRW2100025093.1.20",
          "form": "덥죠?",
          "original_form": "덥죠?",
          "speaker_id": "1",
