      "utterance": [
        {
          "id": "MDRW2100008094.1.1",
          "form": "그게 뭔데?",
          "original_form": "그게 뭔데?",
          "speaker_id": "2",
          "time": "20210822 13:18"
        },
        {
          "id": "MDRW2100008094.1.2",
          "form": "단어책인데 맘카페에서 많이 추천하더라고",
          "original_form": "단어책인데 맘카페에서 많이 추천하더라고",
          "speaker_id": "1",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100008094.1.3",
          "form": "ㅇㅇ 중학교 필수단어?",
          "original_form": "ㅇㅇ 중학교 필수단어?",
          "speaker_id": "2",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100008094.1.4",
          "form": "고등학교까지 있던데",
          "original_form": "고등학교까지 있던데",
          "speaker_id": "1",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100008094.1.5",
          "form": "ㅇㅇ 좋네 ㅎㅎ",
          "original_form": "ㅇㅇ 좋네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100008094.1.6",
          "form": "그것만 외워도 문제없을 듯",
          "original_form": "그것만 외워도 문제없을 듯",
          "speaker_id": "1",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100008094.1.7",
          "form": "하루에 20~30개씩 꾸준히 하게 해",
          "original_form": "하루에 20~30개씩 꾸준히 하게 해",
          "speaker_id": "2",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008094.1.8",
          "form": "워드마스터 사면 하루 분량으로 잘 나눠져 있더라고",
          "original_form": "워드마스터 사면 하루 분량으로 잘 나눠져 있더라고",
          "speaker_id": "1",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008094.1.9",
          "form": "오 보기 편하겠네",
          "original_form": "오 보기 편하겠네",
          "speaker_id": "2",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008094.1.10",
          "form": "하루치 분량 꼬박꼬박 외우면 40일이면 끝나는 거 같아",
          "original_form": "하루치 분량 꼬박꼬박 외우면 40일이면 끝나는 거 같아",
          "speaker_id": "1",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008094.1.11",
          "form": "계속 반복해야지",
          "original_form": "계속 반복해야지",
          "speaker_id": "2",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008094.1.12",
          "form": "단계도 나누어져 있어서 중학교 3권인 것 같아",
          "original_form": "단계도 나누어져 있어서 중학교 3권인 것 같아",
          "speaker_id": "1",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008094.1.13",
          "form": "외울 양이 꽤 되겠네 ㅎㅎ",
          "original_form": "외울 양이 꽤 되겠네 ㅎㅎ",
