      "utterance": [
        {
          "id": "MDRW2100012785.1.1",
          "form": "머리이쁘게하고있나?",
          "original_form": "머리이쁘게하고있나?",
          "speaker_id": "2",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100012785.1.2",
          "form": "난 집에오니 집 장난아니네ㅡㅡ",
          "original_form": "난 집에오니 집 장난아니네ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100012785.1.3",
          "form": "ㅋㅋ 언니 나도 집 장난아녀요ㅋ",
          "original_form": "ㅋㅋ 언니 나도 집 장난아녀요ㅋ",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100012785.1.4",
          "form": "집가서 청소해야됨",
          "original_form": "집가서 청소해야됨",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100012785.1.5",
          "form": "머리는 괜찮게나온듯요ㅋ",
          "original_form": "머리는 괜찮게나온듯요ㅋ",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100012785.1.6",
          "form": "상한게 많아서ㅜㅜ",
          "original_form": "상한게 많아서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100012785.1.7",
          "form": "나도 짤라야되는데ㅋ",
          "original_form": "나도 짤라야되는데ㅋ",
          "speaker_id": "2",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100012785.1.8",
          "form": "ㅋㅋㅋ 나는 짧은데 더 잘랐어요ㅋ",
          "original_form": "ㅋㅋㅋ 나는 짧은데 더 잘랐어요ㅋ",
          "speaker_id": "1",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100012785.1.9",
          "form": "언니 보니깐",
          "original_form": "언니 보니깐",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100012785.1.10",
          "form": "주식거래해야지 만원들어온대요.ㅋ",
          "original_form": "주식거래해야지 만원들어온대요.ㅋ",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100012785.1.11",
          "form": "아 정말 빨래도 , 청소도해야되는데 ㄷㄷ",
          "original_form": "아 정말 빨래도 , 청소도해야되는데 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100012785.1.12",
          "form": "거래방금하나했다",
          "original_form": "거래방금하나했다",
          "speaker_id": "2",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100012785.1.13",
          "form": "내일들어올란강?ㅋㅋ",
          "original_form": "내일들어올란강?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100012785.1.14",
          "form": "난 빨래ㅜㅜ",
          "original_form": "난 빨래ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100012785.1.15",
          "form": "오늘이나 내일이나 들어오겠죠. ㅋㅋ 언니 잘했어유. ㅋㅋ",
          "original_form": "오늘이나 내일이나 들어오겠죠. ㅋㅋ 언니 잘했어유. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100012785.1.16",
          "form": "ㅋㅋ 그래 이제장보러ㄱㄱ",
          "original_form": "ㅋㅋ 그래 이제장보러ㄱㄱ",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100012785.1.17",
          "form": "뭐먹노ㅋㅋ",
          "original_form": "뭐먹노ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100012785.1.18",
          "form": "김치볶음밥이요ㅋ",
          "original_form": "김치볶음밥이요ㅋ",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100012785.1.19",
          "form": "언니는 먹었죠?ㅋ",
          "original_form": "언니는 먹었죠?ㅋ",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100012785.1.20",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100012785.1.21",
          "form": "ㅋㅋ아하 안먹었어요?",
          "original_form": "ㅋㅋ아하 안먹었어요?",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100012785.1.22",
          "form": "못먹음",
          "original_form": "못먹음",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100012785.1.23",
          "form": "배 이제좀고파지네",
          "original_form": "배 이제좀고파지네",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100012785.1.24",
          "form": "전 이제 집에 왔어요. 첫째 데리고왔어요.",
          "original_form": "전 이제 집에 왔어요. 첫째 데리고왔어요.",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100012785.1.25",
          "form": "그래 내일개학이니일많재?",
          "original_form": "그래 내일개학이니일많재?",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100012785.1.26",
          "form": "우리는 자러갔다",
          "original_form": "우리는 자러갔다",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100012785.1.27",
          "form": "ㅋㅋㅋㅋㅋ언니 ㅋㅋ 뭘 챙겨야되죠??ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ언니 ㅋㅋ 뭘 챙겨야되죠??ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100012785.1.28",
          "form": "ㅋㅋ 물통?",
          "original_form": "ㅋㅋ 물통?",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100012785.1.29",
          "form": "필통?",
          "original_form": "필통?",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100012785.1.30",
          "form": "몰라ㅋㅋ 숙제도 몰라ㅋㅋ",
          "original_form": "몰라ㅋㅋ 숙제도 몰라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100012785.1.31",
          "form": "힘이든다",
          "original_form": "힘이든다",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100012785.1.32",
          "form": "수도권은 초등숙제없다던데",
          "original_form": "수도권은 초등숙제없다던데",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100012785.1.33",
          "form": "여긴 뭐;;",
          "original_form": "여긴 뭐;;",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
