      "utterance": [
        {
          "id": "MDRW2100008563.1.1",
          "form": "저 대학생이에요 3학년",
          "original_form": "저 대학생이에요 3학년",
          "speaker_id": "1",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008563.1.2",
          "form": "헉 저는 방금 졸업한 백수에요",
          "original_form": "헉 저는 방금 졸업한 백수에요",
          "speaker_id": "2",
          "time": "20210822 16:49"
        },
        {
          "id": "MDRW2100008563.1.3",
          "form": "과는 어떤거세요?!",
          "original_form": "과는 어떤거세요?!",
          "speaker_id": "2",
          "time": "20210822 16:49"
        },
        {
          "id": "MDRW2100008563.1.4",
          "form": "저는 문콘입니다",
          "original_form": "저는 문콘입니다",
          "speaker_id": "2",
          "time": "20210822 16:49"
        },
        {
          "id": "MDRW2100008563.1.5",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008563.1.6",
          "form": "문화콘텐츠.. 이런 건가요?",
          "original_form": "문화콘텐츠.. 이런 건가요?",
          "speaker_id": "1",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008563.1.7",
          "form": "저도 비슷해요 영상 미디어 문화... 이런거 배우는 학과요",
          "original_form": "저도 비슷해요 영상 미디어 문화... 이런거 배우는 학과요",
          "speaker_id": "1",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008563.1.8",
          "form": "신기 ㅋㅋㅋㅋ",
          "original_form": "신기 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008563.1.9",
          "form": "헝헐",
          "original_form": "헝헐",
          "speaker_id": "2",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008563.1.10",
          "form": "헐 진짜 비슷하네요?!!!!",
          "original_form": "헐 진짜 비슷하네요?!!!!",
          "speaker_id": "2",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008563.1.11",
          "form": "과제 주로 어떤건가요 거기는",
          "original_form": "과제 주로 어떤건가요 거기는",
          "speaker_id": "2",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008563.1.12",
          "form": "저희는 스토리텔링 분석이랑 콘텐츠 분석하는 거였어요",
          "original_form": "저희는 스토리텔링 분석이랑 콘텐츠 분석하는 거였어요",
          "speaker_id": "2",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008563.1.13",
          "form": "와 저도 그런거 많이 배우고 싶네요",
          "original_form": "와 저도 그런거 많이 배우고 싶네요",
          "speaker_id": "1",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008563.1.14",
          "form": "저흰 주로 영상쪽 배워요",
          "original_form": "저흰 주로 영상쪽 배워요",
          "speaker_id": "1",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008563.1.15",
          "form": "스토리텔링에 대해서 배운 적도 있는데 보통은 다큐멘터리의 역사, 영화의 이해 이런거요..",
          "original_form": "스토리텔링에 대해서 배운 적도 있는데 보통은 다큐멘터리의 역사, 영화의 이해 이런거요..",
          "speaker_id": "1",
          "time": "20210822 16:54"
        },
        {
          "id": "MDRW2100008563.1.16",
          "form": "아아 아예 영상영화쪽이시군요",
          "original_form": "아아 아예 영상영화쪽이시군요",
          "speaker_id": "2",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008563.1.17",
          "form": "영화 애니 이런쪽",
          "original_form": "영화 애니 이런쪽",
          "speaker_id": "2",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008563.1.18",
          "form": "그게 더 좋죠",
          "original_form": "그게 더 좋죠",
          "speaker_id": "2",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008563.1.19",
          "form": "그러면 모션그래픽을 위주로 배우시나요 아니면 편집쪽 위주로 하시나요?",
          "original_form": "그러면 모션그래픽을 위주로 배우시나요 아니면 편집쪽 위주로 하시나요?",
          "speaker_id": "2",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008563.1.20",
          "form": "요즘 영상학과들이 다른 학과들이랑 합쳐져서ㅠㅠㅠ 다 커리큘럼이 다르더라고요",
          "original_form": "요즘 영상학과들이 다른 학과들이랑 합쳐져서ㅠㅠㅠ 다 커리큘럼이 다르더라고요",
          "speaker_id": "2",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008563.1.21",
          "form": "저희학과는 그런거 안가르쳐줘요 ㅠㅠ",
          "original_form": "저희학과는 그런거 안가르쳐줘요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008563.1.22",
          "form": "따로 프로그램을 가끔 운영하긴 하지만요",
          "original_form": "따로 프로그램을 가끔 운영하긴 하지만요",
          "speaker_id": "1",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008563.1.23",
          "form": "아까 말한 역사, 이론... 이런 것만 배워요",
          "original_form": "아까 말한 역사, 이론... 이런 것만 배워요",
          "speaker_id": "1",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008563.1.24",
          "form": "아아아아ㅜㅠ 이론적인거 하고 실습은 스스로 해야하는군요ㅠㅠ",
          "original_form": "아아아아ㅜㅠ 이론적인거 하고 실습은 스스로 해야하는군요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008563.1.25",
          "form": "혹시 영화쪽으로 나가시나요?!",
          "original_form": "혹시 영화쪽으로 나가시나요?!",
          "speaker_id": "2",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008563.1.26",
          "form": "아뇨 저 웹툰쪽으로 가고 싶어요",
          "original_form": "아뇨 저 웹툰쪽으로 가고 싶어요",
          "speaker_id": "1",
          "time": "20210822 17:01"
        },
        {
          "id": "MDRW2100008563.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:01"
        },
        {
          "id": "MDRW2100008563.1.28",
          "form": "근데 이 학과를 와버렸죠",
          "original_form": "근데 이 학과를 와버렸죠",
          "speaker_id": "1",
          "time": "20210822 17:01"
        },
        {
          "id": "MDRW2100008563.1.29",
          "form": "졸업을 위해 일단 다닙니다 ㅠ",
          "original_form": "졸업을 위해 일단 다닙니다 ㅠ",
          "speaker_id": "1",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008563.1.30",
          "form": "헐헐",
          "original_form": "헐헐",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008563.1.31",
          "form": "그럼 혹시 동아리같은거 웹툰쪽으로 들고 계신가요?!",
          "original_form": "그럼 혹시 동아리같은거 웹툰쪽으로 들고 계신가요?!",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008563.1.32",
          "form": "아쉽게도 학교에 그런 동아리가 없어요",
          "original_form": "아쉽게도 학교에 그런 동아리가 없어요",
          "speaker_id": "1",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008563.1.33",
          "form": "그림동아리는 있는데 작년에 나왔네요 ㅋㅋ",
          "original_form": "그림동아리는 있는데 작년에 나왔네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008563.1.34",
          "form": "헉 그림 잘그리시는 군요",
          "original_form": "헉 그림 잘그리시는 군요",
          "speaker_id": "2",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008563.1.35",
          "form": "쓰읍",
          "original_form": "쓰읍",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008563.1.36",
          "form": "칭긔중에 웹툰 같이 꿈꾸는 사람이 있나요?",
          "original_form": "칭긔중에 웹툰 같이 꿈꾸는 사람이 있나요?",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008563.1.37",
          "form": "네! ㅋㅋ 웹툰작가가 꿈이라고 우리학과에 전과온 친구가 있어요 ㅋㅋㅋㅋ",
          "original_form": "네! ㅋㅋ 웹툰작가가 꿈이라고 우리학과에 전과온 친구가 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
