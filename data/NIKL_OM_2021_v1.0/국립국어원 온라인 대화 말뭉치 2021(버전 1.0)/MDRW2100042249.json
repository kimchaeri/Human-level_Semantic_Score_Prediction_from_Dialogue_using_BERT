      "utterance": [
        {
          "id": "MDRW2100042249.1.1",
          "form": "저 센스맘 매트리스 당근 예약중이여",
          "original_form": "저 센스맘 매트리스 당근 예약중이여",
          "speaker_id": "2",
          "time": "20210906 21:01"
        },
        {
          "id": "MDRW2100042249.1.2",
          "form": "오 센스맘 좋더라구요",
          "original_form": "오 센스맘 좋더라구요",
          "speaker_id": "1",
          "time": "20210906 21:02"
        },
        {
          "id": "MDRW2100042249.1.3",
          "form": "폭신하니",
          "original_form": "폭신하니",
          "speaker_id": "1",
          "time": "20210906 21:02"
        },
        {
          "id": "MDRW2100042249.1.4",
          "form": "오 써보셨나요",
          "original_form": "오 써보셨나요",
          "speaker_id": "2",
          "time": "20210906 21:02"
        },
        {
          "id": "MDRW2100042249.1.5",
          "form": "아주버님이 쓰시더라구요 높은거",
          "original_form": "아주버님이 쓰시더라구요 높은거",
          "speaker_id": "1",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100042249.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100042249.1.7",
          "form": "전 내일 오후에 픽업해요",
          "original_form": "전 내일 오후에 픽업해요",
          "speaker_id": "2",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100042249.1.8",
          "form": "얼마에용,",
          "original_form": "얼마에용,",
          "speaker_id": "1",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100042249.1.9",
          "form": "슈퍼싱글 며칠쓴거 3만원이여",
          "original_form": "슈퍼싱글 며칠쓴거 3만원이여",
          "speaker_id": "2",
          "time": "20210906 21:04"
        },
        {
          "id": "MDRW2100042249.1.10",
          "form": "세상에나 넘싸네요",
          "original_form": "세상에나 넘싸네요",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042249.1.11",
          "form": "정가가얼마져?",
          "original_form": "정가가얼마져?",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042249.1.12",
          "form": "아 글쎄여",
          "original_form": "아 글쎄여",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042249.1.13",
          "form": "당근가격이 대충 5만원선이라",
          "original_form": "당근가격이 대충 5만원선이라",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042249.1.14",
          "form": "와득템이셔요",
          "original_form": "와득템이셔요",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042249.1.15",
          "form": "중고가격보고괜찮아서",
          "original_form": "중고가격보고괜찮아서",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042249.1.16",
          "form": "네네맞아여",
          "original_form": "네네맞아여",
          "speaker_id": "2",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042249.1.17",
          "form": "요즘 보고있는 아이템 있으세여",
          "original_form": "요즘 보고있는 아이템 있으세여",
          "speaker_id": "2",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042249.1.18",
          "form": "핸드폰을 사고싶은데ㅠㅠ",
          "original_form": "핸드폰을 사고싶은데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100042249.1.19",
          "form": "비싸서ㅠ",
          "original_form": "비싸서ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100042249.1.20",
          "form": "z플립3 최신형이요?",
          "original_form": "z플립3 최신형이요?",
          "speaker_id": "2",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100042249.1.21",
          "form": "넘좋죠ㅠㅠ근데비싸서 못살것같아여ㅜㅠ",
          "original_form": "넘좋죠ㅠㅠ근데비싸서 못살것같아여ㅜㅠ",
