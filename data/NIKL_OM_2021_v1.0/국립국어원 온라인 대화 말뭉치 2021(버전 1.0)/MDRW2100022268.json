      "utterance": [
        {
          "id": "MDRW2100022268.1.1",
          "form": "시판소스 많이 먹어봤어요?",
          "original_form": "시판소스 많이 먹어봤어요?",
          "speaker_id": "2",
          "time": "20210829 11:51"
        },
        {
          "id": "MDRW2100022268.1.2",
          "form": "2천원 정도하는데 적정선에 물 붓고 졸여보세요",
          "original_form": "2천원 정도하는데 적정선에 물 붓고 졸여보세요",
          "speaker_id": "1",
          "time": "20210829 11:51"
        },
        {
          "id": "MDRW2100022268.1.3",
          "form": "전 많이 먹는건 아닌데 다담소스는.. 맛있어서",
          "original_form": "전 많이 먹는건 아닌데 다담소스는.. 맛있어서",
          "speaker_id": "1",
          "time": "20210829 11:51"
        },
        {
          "id": "MDRW2100022268.1.4",
          "form": "켁 2천원",
          "original_form": "켁 2천원",
          "speaker_id": "2",
          "time": "20210829 11:51"
        },
        {
          "id": "MDRW2100022268.1.5",
          "form": "비싼 건 아닌데 싼 것도 아닌 것 같다",
          "original_form": "비싼 건 아닌데 싼 것도 아닌 것 같다",
          "speaker_id": "2",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100022268.1.6",
          "form": "닭 한마리에 2천원 투자해서",
          "original_form": "닭 한마리에 2천원 투자해서",
          "speaker_id": "1",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100022268.1.7",
          "form": "전 다담소스 중에 순두부찌개 그거 좋아해요",
          "original_form": "전 다담소스 중에 순두부찌개 그거 좋아해요",
          "speaker_id": "2",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100022268.1.8",
          "form": "2만원짜리 찜닭만들면 이득인거죠",
          "original_form": "2만원짜리 찜닭만들면 이득인거죠",
          "speaker_id": "1",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100022268.1.9",
          "form": "ㅋㅋㅋㅋ말씀 들어보니 그러네요",
          "original_form": "ㅋㅋㅋㅋ말씀 들어보니 그러네요",
          "speaker_id": "2",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100022268.1.10",
          "form": "순두부 맛있더라구요",
          "original_form": "순두부 맛있더라구요",
          "speaker_id": "1",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100022268.1.11",
          "form": "순두부찌개도 해물순두부찌개 나오는거 같던데",
          "original_form": "순두부찌개도 해물순두부찌개 나오는거 같던데",
          "speaker_id": "2",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100022268.1.12",
          "form": "이것도 1인분용이죠?",
          "original_form": "이것도 1인분용이죠?",
          "speaker_id": "2",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100022268.1.13",
          "form": "그건 2인분정도?",
          "original_form": "그건 2인분정도?",
          "speaker_id": "1",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100022268.1.14",
          "form": "아하 근데 순두부만 계속 리필해서 여러번 먹었네요",
          "original_form": "아하 근데 순두부만 계속 리필해서 여러번 먹었네요",
          "speaker_id": "2",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022268.1.15",
          "form": "사실 혼자먹으면 1인분이고 둘이먹음 2번 인분인거라ㅋㅋ",
          "original_form": "사실 혼자먹으면 1인분이고 둘이먹음 2번 인분인거라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022268.1.16",
          "form": "그렇게 하니까 한 3~4번은 먹더라구요",
          "original_form": "그렇게 하니까 한 3~4번은 먹더라구요",
          "speaker_id": "2",
          "time": "20210829 11:53"
        }
