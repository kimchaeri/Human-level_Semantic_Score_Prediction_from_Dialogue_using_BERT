      "utterance": [
        {
          "id": "MDRW2100010231.1.1",
          "form": "마실게 없으면 먹어",
          "original_form": "마실게 없으면 먹어",
          "speaker_id": "2",
          "time": "20210823 15:52"
        },
        {
          "id": "MDRW2100010231.1.2",
          "form": "나는 중학생때는 카페모카만 주구장창 마시다가 이제는 아이스아메리카노만 마셔",
          "original_form": "나는 중학생때는 카페모카만 주구장창 마시다가 이제는 아이스아메리카노만 마셔",
          "speaker_id": "1",
          "time": "20210823 15:52"
        },
        {
          "id": "MDRW2100010231.1.3",
          "form": "그리고 시험기간에 애용해",
          "original_form": "그리고 시험기간에 애용해",
          "speaker_id": "2",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010231.1.4",
          "form": "마실 게 없으면 먹는다니",
          "original_form": "마실 게 없으면 먹는다니",
          "speaker_id": "1",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010231.1.5",
          "form": "마시면 잠이 안오니?",
          "original_form": "마시면 잠이 안오니?",
          "speaker_id": "1",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010231.1.6",
          "form": "나는 어릴땐 카라멜마끼야또를 먹었지",
          "original_form": "나는 어릴땐 카라멜마끼야또를 먹었지",
          "speaker_id": "2",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010231.1.7",
          "form": "나는 한 3잔까지는 그저그런거같아. 그냥 입에 뭘 물고있으면",
          "original_form": "나는 한 3잔까지는 그저그런거같아. 그냥 입에 뭘 물고있으면",
          "speaker_id": "1",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010231.1.8",
          "form": "응 나는 별로 안예민해",
          "original_form": "응 나는 별로 안예민해",
          "speaker_id": "2",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010231.1.9",
          "form": "잠이안오는거같아",
          "original_form": "잠이안오는거같아",
          "speaker_id": "1",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010231.1.10",
          "form": "계속 먹어야 잠이 깨.",
          "original_form": "계속 먹어야 잠이 깨.",
          "speaker_id": "1",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010231.1.11",
          "form": "시험기간에는 기분내려고 마시는거야.",
          "original_form": "시험기간에는 기분내려고 마시는거야.",
          "speaker_id": "2",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010231.1.12",
          "form": "카라멜마끼아또 한때 유행이었지",
          "original_form": "카라멜마끼아또 한때 유행이었지",
          "speaker_id": "1",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010231.1.13",
          "form": "아. 그렇구나. 어떤 기분이니",
          "original_form": "아. 그렇구나. 어떤 기분이니",
          "speaker_id": "1",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010231.1.14",
          "form": "열심히 공부하는기분",
          "original_form": "열심히 공부하는기분",
          "speaker_id": "2",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010231.1.15",
          "form": "어디 드라마에 나왔던 거 같아. 카라멜마끼아또.",
          "original_form": "어디 드라마에 나왔던 거 같아. 카라멜마끼아또.",
          "speaker_id": "1",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010231.1.16",
          "form": "음료로 얄심히 공부하는 기분을 느낄 수 있다니 정말 가성비가 대박이구나.",
          "original_form": "음료로 얄심히 공부하는 기분을 느낄 수 있다니 정말 가성비가 대박이구나.",
          "speaker_id": "1",
          "time": "20210823 15:54"
        },
        {
          "id": "MDRW2100010231.1.17",
          "form": "맞아 웹툰제목도 있었어 카라멜 마끼야토",
          "original_form": "맞아 웹툰제목도 있었어 카라멜 마끼야토",
          "speaker_id": "2",
          "time": "20210823 15:55"
        },
        {
          "id": "MDRW2100010231.1.18",
          "form": "그리고 아메리카노는 가성비가 좋아",
          "original_form": "그리고 아메리카노는 가성비가 좋아",
          "speaker_id": "2",
          "time": "20210823 15:55"
        }
      ]
