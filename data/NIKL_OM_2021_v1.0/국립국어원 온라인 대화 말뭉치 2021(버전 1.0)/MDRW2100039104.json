      "utterance": [
        {
          "id": "MDRW2100039104.1.1",
          "form": "하고 있는 일은 할만 한가요?",
          "original_form": "하고 있는 일은 할만 한가요?",
          "speaker_id": "1",
          "time": "20210903 10:05"
        },
        {
          "id": "MDRW2100039104.1.2",
          "form": "공부가 일이라고 한다면... 아직은 할만 해요",
          "original_form": "공부가 일이라고 한다면... 아직은 할만 해요",
          "speaker_id": "2",
          "time": "20210903 10:06"
        },
        {
          "id": "MDRW2100039104.1.3",
          "form": "공부하는 입장이군요",
          "original_form": "공부하는 입장이군요",
          "speaker_id": "1",
          "time": "20210903 10:12"
        },
        {
          "id": "MDRW2100039104.1.4",
          "form": "원하는 직업을 갖기 위한 공부인가요?",
          "original_form": "원하는 직업을 갖기 위한 공부인가요?",
          "speaker_id": "1",
          "time": "20210903 10:12"
        },
        {
          "id": "MDRW2100039104.1.5",
          "form": "그쵸ㅎㅎ 지금 공부해야 갈 수 있을테니까요",
          "original_form": "그쵸ㅎㅎ 지금 공부해야 갈 수 있을테니까요",
          "speaker_id": "2",
          "time": "20210903 10:14"
        },
        {
          "id": "MDRW2100039104.1.6",
          "form": "어떤 종류의 직업을 갖고 싶나요?",
          "original_form": "어떤 종류의 직업을 갖고 싶나요?",
          "speaker_id": "1",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100039104.1.7",
          "form": "웹디자인 계열 생각하고 있어요",
          "original_form": "웹디자인 계열 생각하고 있어요",
          "speaker_id": "2",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100039104.1.8",
          "form": "디자인쪽이군요",
          "original_form": "디자인쪽이군요",
          "speaker_id": "1",
          "time": "20210903 10:25"
        },
        {
          "id": "MDRW2100039104.1.9",
          "form": "창의력이 많이 필요할텐데요",
          "original_form": "창의력이 많이 필요할텐데요",
          "speaker_id": "1",
          "time": "20210903 10:25"
        },
        {
          "id": "MDRW2100039104.1.10",
          "form": "그래서 여러 작품 둘러보곤 해요",
          "original_form": "그래서 여러 작품 둘러보곤 해요",
          "speaker_id": "2",
          "time": "20210903 10:26"
        },
        {
          "id": "MDRW2100039104.1.11",
          "form": "요즘 인터넷으로도 자료 잘 찾아볼 수 있죠?",
          "original_form": "요즘 인터넷으로도 자료 잘 찾아볼 수 있죠?",
          "speaker_id": "1",
          "time": "20210903 10:27"
        },
        {
          "id": "MDRW2100039104.1.12",
          "form": "맞아요 핀터레스트만 들어가도 쌓여있죠",
          "original_form": "맞아요 핀터레스트만 들어가도 쌓여있죠",
          "speaker_id": "2",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100039104.1.13",
          "form": "제가 그런쪽으로 재주가 없다보니 신기하네요",
          "original_form": "제가 그런쪽으로 재주가 없다보니 신기하네요",
          "speaker_id": "1",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100039104.1.14",
          "form": "웹쪽도 그렇고 디자인도 그렇고 먼 이야기에요",
          "original_form": "웹쪽도 그렇고 디자인도 그렇고 먼 이야기에요",
          "speaker_id": "1",
          "time": "20210903 10:30"
        },
        {
          "id": "MDRW2100039104.1.15",
          "form": "저도 재주가 있는건 아니고",
          "original_form": "저도 재주가 있는건 아니고",
          "speaker_id": "2",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100039104.1.16",
          "form": "공부해서 메꾸고 있어요...ㅋㅋ",
          "original_form": "공부해서 메꾸고 있어요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100039104.1.17",
          "form": "공부로 메꿀수 있으면 타고난게 있다는 뜻입니다",
          "original_form": "공부로 메꿀수 있으면 타고난게 있다는 뜻입니다",
          "speaker_id": "1",
          "time": "20210903 10:33"
        },
        {
          "id": "MDRW2100039104.1.18",
          "form": "해도 안되는 사람 많아요..ㅎㅎ",
          "original_form": "해도 안되는 사람 많아요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:33"
        },
        {
          "id": "MDRW2100039104.1.19",
          "form": "결국은 디자인이 뛰어난건 아니고 기술로 메꾸고 있죠",
          "original_form": "결국은 디자인이 뛰어난건 아니고 기술로 메꾸고 있죠",
          "speaker_id": "2",
          "time": "20210903 10:34"
        },
        {
          "id": "MDRW2100039104.1.20",
          "form": "웹디자인이라서 가능한거 같아요",
          "original_form": "웹디자인이라서 가능한거 같아요",
          "speaker_id": "2",
          "time": "20210903 10:34"
        },
