        },
        {
          "id": "MDRW2100012166.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210824 20:04"
        },
        {
          "id": "MDRW2100012166.1.3",
          "form": "집안일 자주하시나요?",
          "original_form": "집안일 자주하시나요?",
          "speaker_id": "1",
          "time": "20210824 20:05"
        },
        {
          "id": "MDRW2100012166.1.4",
          "form": "네ㅋㅋ집안일이야 항상 있죠ㅋㅋ",
          "original_form": "네ㅋㅋ집안일이야 항상 있죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:06"
        },
        {
          "id": "MDRW2100012166.1.5",
          "form": "설거지도 하고 분리수거도 하고 바닥도 닦고",
          "original_form": "설거지도 하고 분리수거도 하고 바닥도 닦고",
          "speaker_id": "2",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100012166.1.6",
          "form": "금방 청소기 돌리고 왔네요",
          "original_form": "금방 청소기 돌리고 왔네요",
          "speaker_id": "1",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100012166.1.7",
          "form": "가족분과 분담하시나요?",
          "original_form": "가족분과 분담하시나요?",
          "speaker_id": "1",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100012166.1.8",
          "form": "네 같이해요!",
          "original_form": "네 같이해요!",
          "speaker_id": "2",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100012166.1.9",
          "form": "룸메이트와 생활해본적 있으세요?",
          "original_form": "룸메이트와 생활해본적 있으세요?",
          "speaker_id": "1",
          "time": "20210824 20:22"
        },
        {
          "id": "MDRW2100012166.1.10",
          "form": "아뇨ㅋㅋ저는 자취해본 적도 없고 룸메랑 생활해본 적도 없어요ㅜ",
          "original_form": "아뇨ㅋㅋ저는 자취해본 적도 없고 룸메랑 생활해본 적도 없어요ㅜ",
          "speaker_id": "2",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012166.1.11",
          "form": "저도요ㅋㅋ",
          "original_form": "저도요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012166.1.12",
          "form": "언제 독립하실 생각이세요?",
          "original_form": "언제 독립하실 생각이세요?",
          "speaker_id": "1",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012166.1.13",
          "form": "저는 자가 생기면 나가려고요",
          "original_form": "저는 자가 생기면 나가려고요",
          "speaker_id": "1",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012166.1.14",
          "form": "저는 결혼 때나 가능하려나요...ㅜ",
          "original_form": "저는 결혼 때나 가능하려나요...ㅜ",
          "speaker_id": "2",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100012166.1.15",
          "form": "열심히 모아서 사봅시다ㅎㅎ",
          "original_form": "열심히 모아서 사봅시다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100012166.1.16",
          "form": "네ㅜㅠ 화이팅입니다",
          "original_form": "네ㅜㅠ 화이팅입니다",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012166.1.17",
          "form": "독립하는 그날까지 화이팅!",
          "original_form": "독립하는 그날까지 화이팅!",
          "speaker_id": "1",
