      "utterance": [
        {
          "id": "MDRW2100036703.1.1",
          "form": "다음에 또 남의 물건에 손 대는 일 있으면",
          "original_form": "다음에 또 남의 물건에 손 대는 일 있으면",
          "speaker_id": "2",
          "time": "20210831 09:43"
        },
        {
          "id": "MDRW2100036703.1.2",
          "form": "응응 아고 ㅋㅋ",
          "original_form": "응응 아고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036703.1.3",
          "form": "그때는 너도 똑같이 해줄거라고 해놨음",
          "original_form": "그때는 너도 똑같이 해줄거라고 해놨음",
          "speaker_id": "2",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036703.1.4",
          "form": "ㅋㅋ그랬더니 알겠대?",
          "original_form": "ㅋㅋ그랬더니 알겠대?",
          "speaker_id": "1",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036703.1.5",
          "form": "엉 잘못했다하대",
          "original_form": "엉 잘못했다하대",
          "speaker_id": "2",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036703.1.6",
          "form": "본인도 반성했다길래",
          "original_form": "본인도 반성했다길래",
          "speaker_id": "2",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036703.1.7",
          "form": "응 얘기 잘했네. ㅋㅋ",
          "original_form": "응 얘기 잘했네. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036703.1.8",
          "form": "그래 반성햇을거야.",
          "original_form": "그래 반성햇을거야.",
          "speaker_id": "1",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036703.1.9",
          "form": "짧게하지말고 길게",
          "original_form": "짧게하지말고 길게",
          "speaker_id": "2",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036703.1.10",
          "form": "두고두고 반성하라고 ㅋㅋㅋㅋㅋ",
          "original_form": "두고두고 반성하라고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036703.1.11",
          "form": "장난이 이렇게 크게 올줄은 몰랐겠지.",
          "original_form": "장난이 이렇게 크게 올줄은 몰랐겠지.",
          "speaker_id": "1",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036703.1.12",
          "form": "첫째두 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "첫째두 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036703.1.13",
          "form": "그래 근데 장나도 골탕먹일라고 했는거야",
          "original_form": "그래 근데 장나도 골탕먹일라고 했는거야",
          "speaker_id": "2",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036703.1.14",
          "form": "웅웅 ㅠㅠ 그랫구나",
          "original_form": "웅웅 ㅠㅠ 그랫구나",
          "speaker_id": "1",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036703.1.15",
          "form": "name3가 본인보다 잘하거든",
          "original_form": "&name3&가 본인보다 잘하거든",
          "speaker_id": "2",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036703.1.16",
          "form": "게임을",
          "original_form": "게임을",
          "speaker_id": "2",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036703.1.17",
          "form": "서로 맨날 약올리고",
          "original_form": "서로 맨날 약올리고",
          "speaker_id": "2",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036703.1.18",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ질투도 살짝 있었네 ㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ질투도 살짝 있었네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036703.1.19",
          "form": "그러다 주먹다짐하고",
          "original_form": "그러다 주먹다짐하고",
          "speaker_id": "2",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036703.1.20",
          "form": "어 있었지",
          "original_form": "어 있었지",
          "speaker_id": "2",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036703.1.21",
          "form": "아구 ㅠㅠㅠ",
          "original_form": "아구 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036703.1.22",
          "form": "내가 그걸 아니까 더 화나는거야",
          "original_form": "내가 그걸 아니까 더 화나는거야",
          "speaker_id": "2",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036703.1.23",
          "form": "맞어 그랬겠다.",
          "original_form": "맞어 그랬겠다.",
          "speaker_id": "1",
          "time": "20210831 09:45"
        }
