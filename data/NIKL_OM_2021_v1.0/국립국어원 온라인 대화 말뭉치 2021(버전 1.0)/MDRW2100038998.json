      "utterance": [
        {
          "id": "MDRW2100038998.1.1",
          "form": "내가 여기 일하기 전에 만났멋는데",
          "original_form": "내가 여기 일하기 전에 만났멋는데",
          "speaker_id": "2",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100038998.1.2",
          "form": "같이 다니기 부끄럽지는 않아야 될 거 같애요",
          "original_form": "같이 다니기 부끄럽지는 않아야 될 거 같애요",
          "speaker_id": "1",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100038998.1.3",
          "form": "여기 일하게 되면서",
          "original_form": "여기 일하게 되면서",
          "speaker_id": "2",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100038998.1.4",
          "form": "그분사는곳을",
          "original_form": "그분사는곳을",
          "speaker_id": "2",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100038998.1.5",
          "form": "맡게된거야",
          "original_form": "맡게된거야",
          "speaker_id": "2",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100038998.1.6",
          "form": "얘 요",
          "original_form": "얘 요",
          "speaker_id": "1",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100038998.1.7",
          "form": "한신휴",
          "original_form": "한신휴",
          "speaker_id": "2",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100038998.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100038998.1.9",
          "form": "유명한 만나면 어떡해",
          "original_form": "유명한 만나면 어떡해",
          "speaker_id": "1",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100038998.1.10",
          "form": "주차장서 얼핏본거같아서",
          "original_form": "주차장서 얼핏본거같아서",
          "speaker_id": "2",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100038998.1.11",
          "form": "깜놀함",
          "original_form": "깜놀함",
          "speaker_id": "2",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100038998.1.12",
          "form": "에 이 그냥 못 할 거야",
          "original_form": "에 이 그냥 못 할 거야",
          "speaker_id": "1",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100038998.1.13",
          "form": "헤어진남친도아닌데",
          "original_form": "헤어진남친도아닌데",
          "speaker_id": "2",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100038998.1.14",
          "form": "영어 못 할 거야",
          "original_form": "영어 못 할 거야",
          "speaker_id": "1",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100038998.1.15",
          "form": "그분은나못본듯",
          "original_form": "그분은나못본듯",
          "speaker_id": "2",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100038998.1.16",
          "form": "다행이지",
          "original_form": "다행이지",
          "speaker_id": "2",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100038998.1.17",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100038998.1.18",
          "form": "여튼 그분은",
          "original_form": "여튼 그분은",
          "speaker_id": "2",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100038998.1.19",
          "form": "인연이 아닌 걸로",
          "original_form": "인연이 아닌 걸로",
          "speaker_id": "1",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100038998.1.20",
          "form": "대시하셨는데",
          "original_form": "대시하셨는데",
          "speaker_id": "2",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100038998.1.21",
          "form": "외모가 극복이안돼서",
          "original_form": "외모가 극복이안돼서",
          "speaker_id": "2",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100038998.1.22",
          "form": "못만남",
          "original_form": "못만남",
          "speaker_id": "2",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100038998.1.23",
          "form": "그거 중요한 문제에요",
          "original_form": "그거 중요한 문제에요",
          "speaker_id": "1",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100038998.1.24",
          "form": "그거말고 또 선개팅",
          "original_form": "그거말고 또 선개팅",
          "speaker_id": "2",
          "time": "20210903 00:54"
        }
      ]
