      "utterance": [
        {
          "id": "MDRW2100034676.1.1",
          "form": "name2님 햄버거도 좋아하세요?",
          "original_form": "&name2&님 햄버거도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100034676.1.2",
          "form": "네 잘먹어요 ㅋㅋ버거킹 롯데리아 맥도날드 각자 선호 버거가 있어요",
          "original_form": "네 잘먹어요 ㅋㅋ버거킹 롯데리아 맥도날드 각자 선호 버거가 있어요",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100034676.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100034676.1.4",
          "form": "오호ㅋㅋㅋㅋ  name2님",
          "original_form": "오호ㅋㅋㅋㅋ  &name2&님",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100034676.1.5",
          "form": "혹시 창녕갈릭버거도 영접하셨나요?",
          "original_form": "혹시 창녕갈릭버거도 영접하셨나요?",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100034676.1.6",
          "form": "아뇨 사진은 많이 봣는데 마늘 들어간 너비아니 맛날것 같아서 패스했어요 ㅋㅋ",
          "original_form": "아뇨 사진은 많이 봣는데 마늘 들어간 너비아니 맛날것 같아서 패스했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100034676.1.7",
          "form": "name1님은 드셨나요?",
          "original_form": "&name1&님은 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100034676.1.8",
          "form": "오늘 나간김에 먹어보려고 했는데",
          "original_form": "오늘 나간김에 먹어보려고 했는데",
          "speaker_id": "1",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100034676.1.9",
          "form": "일시적으로 판매중단되었데용",
          "original_form": "일시적으로 판매중단되었데용",
          "speaker_id": "1",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100034676.1.10",
          "form": "인기가 좋아서 다 팔렸나봐요",
          "original_form": "인기가 좋아서 다 팔렸나봐요",
          "speaker_id": "2",
          "time": "20210828 17:47"
        },
        {
          "id": "MDRW2100034676.1.11",
          "form": "다음에 성공하심 알려주세요 맛이 궁금해요 ㅋㅋ",
          "original_form": "다음에 성공하심 알려주세요 맛이 궁금해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100034676.1.12",
          "form": "한정기간 판매라\r\n이제 기간이 얼마 안남았는데\r\n그 안에 갈 수 있을까요?ㅋㅋ",
          "original_form": "한정기간 판매라\r\n이제 기간이 얼마 안남았는데\r\n그 안에 갈 수 있을까요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:16"
        },
        {
          "id": "MDRW2100034676.1.13",
          "form": "맛은 저도 넘 궁금해요",
          "original_form": "맛은 저도 넘 궁금해요",
          "speaker_id": "1",
          "time": "20210828 21:16"
        },
        {
          "id": "MDRW2100034676.1.14",
          "form": "후기보면 아주 맛있진 않던것 같은데 궁금은 해요 ㅋㅋ",
          "original_form": "후기보면 아주 맛있진 않던것 같은데 궁금은 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100034676.1.15",
          "form": "앗 그래요?\r\n제가 본 글들은 다 호라고 해서 ㅋㅋ",
          "original_form": "앗 그래요?\r\n제가 본 글들은 다 호라고 해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100034676.1.16",
          "form": "헐 제가 본 글이랑 다른 분인가 봐요",
          "original_form": "헐 제가 본 글이랑 다른 분인가 봐요",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100034676.1.17",
          "form": "암튼 맥도날드 마늘버거가 맛은 궁금한데 주변이 먹고 말해줬으면 합니다 ㅋㅋ",
          "original_form": "암튼 맥도날드 마늘버거가 맛은 궁금한데 주변이 먹고 말해줬으면 합니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034676.1.18",
          "form": "전 사딸라 버거세트를 잘먹어요",
          "original_form": "전 사딸라 버거세트를 잘먹어요",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100034676.1.19",
          "form": "님은 어떤세트 위주로 드세요?",
          "original_form": "님은 어떤세트 위주로 드세요?",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100034676.1.20",
          "form": "사딸라면\r\n버거킹 말씀하시는거죵?ㅋㅋ",
          "original_form": "사딸라면\r\n버거킹 말씀하시는거죵?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100034676.1.21",
          "form": "네네 ㅋㅋ 가격이 적당하고 양이 맞아요",
          "original_form": "네네 ㅋㅋ 가격이 적당하고 양이 맞아요",
          "speaker_id": "2",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100034676.1.22",
          "form": "저는 머쉬룸와퍼요:)",
          "original_form": "저는 머쉬룸와퍼요:)",
          "speaker_id": "1",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100034676.1.23",
          "form": "ㅋㅋ저 오늘 햄버거 먹었는데\r\n낼은 버거킹 가야할까봐요 ㅋㅋ\r\n또 먹고싶네요",
          "original_form": "ㅋㅋ저 오늘 햄버거 먹었는데\r\n낼은 버거킹 가야할까봐요 ㅋㅋ\r\n또 먹고싶네요",
