      "utterance": [
        {
          "id": "MDRW2100036522.1.1",
          "form": "전 로투킹에서",
          "original_form": "전 로투킹에서",
          "speaker_id": "1",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036522.1.2",
          "form": "온앤오프도 좋았어요",
          "original_form": "온앤오프도 좋았어요",
          "speaker_id": "1",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036522.1.3",
          "form": "헐헐 저 그 무대 좋아해요",
          "original_form": "헐헐 저 그 무대 좋아해요",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036522.1.4",
          "form": "그 날 사랑하게 죌 거야??",
          "original_form": "그 날 사랑하게 죌 거야??",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036522.1.5",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036522.1.6",
          "form": "그 무대ㅜㅜ",
          "original_form": "그 무대ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036522.1.7",
          "form": "유아 나오구 !",
          "original_form": "유아 나오구 !",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036522.1.8",
          "form": "맞아요!! 유아 진짜 인형같이 나오더라고요",
          "original_form": "맞아요!! 유아 진짜 인형같이 나오더라고요",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036522.1.9",
          "form": "완전 공주님같은 ㅠ!",
          "original_form": "완전 공주님같은 ㅠ!",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036522.1.10",
          "form": "맞아요 ㅠㅠㅠ",
          "original_form": "맞아요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036522.1.11",
          "form": "근데 이 곡도 원곡들으면",
          "original_form": "근데 이 곡도 원곡들으면",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036522.1.12",
          "form": "살짝 심심해요ㅋㅋ",
          "original_form": "살짝 심심해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036522.1.13",
          "form": "편곡을 넘 잘해서ㅜㅜ",
          "original_form": "편곡을 넘 잘해서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036522.1.14",
          "form": "헐 원곡 안 들어보ㅓㅆ는데 그렇구나!!!!!!",
          "original_form": "헐 원곡 안 들어보ㅓㅆ는데 그렇구나!!!!!!",
          "speaker_id": "2",
          "time": "20210831 00:18"
        },
        {
          "id": "MDRW2100036522.1.15",
          "form": "ㅇ온앤오프는 정말 한 명도 모르거든요...",
          "original_form": "ㅇ온앤오프는 정말 한 명도 모르거든요...",
          "speaker_id": "2",
          "time": "20210831 00:18"
        },
        {
          "id": "MDRW2100036522.1.16",
          "form": "그 엔하이픈..이랑 가끔 헷..갈려요",
          "original_form": "그 엔하이픈..이랑 가끔 헷..갈려요",
          "speaker_id": "2",
          "time": "20210831 00:18"
        },
        {
          "id": "MDRW2100036522.1.17",
          "form": "저도 멤버는 한명만 알아욬ㅋㄱㄲ",
          "original_form": "저도 멤버는 한명만 알아욬ㅋㄱㄲ",
          "speaker_id": "1",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100036522.1.18",
          "form": "엔하이픈... 그 친구들",
          "original_form": "엔하이픈... 그 친구들",
          "speaker_id": "1",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100036522.1.19",
          "form": "하이브 친구들 아닙니까 ㅎㅎ",
          "original_form": "하이브 친구들 아닙니까 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100036522.1.20",
          "form": "맞아요!!! 무슨 서바이벌로? 데뷔한 건 아는데",
          "original_form": "맞아요!!! 무슨 서바이벌로? 데뷔한 건 아는데",
          "speaker_id": "2",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036522.1.21",
          "form": "얼굴이 진짜 안 외워지더라고요 ㅠ.^",
          "original_form": "얼굴이 진짜 안 외워지더라고요 ㅠ.^",
          "speaker_id": "2",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036522.1.22",
          "form": "엔하이픈 스키즈 온앤오프 이 세대는 앙0",
          "original_form": "엔하이픈 스키즈 온앤오프 이 세대는 앙0",
          "speaker_id": "2",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036522.1.23",
          "form": "아예 몰라요..",
          "original_form": "아예 몰라요..",
          "speaker_id": "2",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036522.1.24",
          "form": "아 맞아욬ㄱㅋㅋ",
          "original_form": "아 맞아욬ㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100036522.1.25",
          "form": "아니 전 스키즈가 그렇게 인기있는지 몰랐는데..",
          "original_form": "아니 전 스키즈가 그렇게 인기있는지 몰랐는데..",
          "speaker_id": "1",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100036522.1.26",
          "form": "이번에 앨범을 되게 잘팔더라구요?",
          "original_form": "이번에 앨범을 되게 잘팔더라구요?",
          "speaker_id": "1",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100036522.1.27",
          "form": "아 그르니까요!!!!!!!!",
          "original_form": "아 그르니까요!!!!!!!!",
          "speaker_id": "2",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100036522.1.28",
          "form": "전 멤버... 사회면에 나온 친구밖에 모르거든요...",
          "original_form": "전 멤버... 사회면에 나온 친구밖에 모르거든요...",
          "speaker_id": "2",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100036522.1.29",
          "form": "역시 전 엔시티까지가 취향인것 같어요 ㅠ.^",
          "original_form": "역시 전 엔시티까지가 취향인것 같어요 ㅠ.^",
          "speaker_id": "2",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100036522.1.30",
          "form": "더보이즈는 탐색중 ㅎㅎ",
          "original_form": "더보이즈는 탐색중 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100036522.1.31",
          "form": "맞아요 ㅎㅎㅎ",
          "original_form": "맞아요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036522.1.32",
          "form": "전 한국에서 인기 많은 그룹이 좋아요!",
          "original_form": "전 한국에서 인기 많은 그룹이 좋아요!",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036522.1.33",
          "form": "사람들이랑 말이 통한달까 ㅋㅋㅋ",
          "original_form": "사람들이랑 말이 통한달까 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036522.1.34",
          "form": "맞아요!!!!! 아니면 너무 혼자 덕질하는",
          "original_form": "맞아요!!!!! 아니면 너무 혼자 덕질하는",
          "speaker_id": "2",
          "time": "20210831 14:34"
        },
        {
          "id": "MDRW2100036522.1.35",
          "form": "기분이 들어서 재미없어지거든요 ㅠ.^",
          "original_form": "기분이 들어서 재미없어지거든요 ㅠ.^",
          "speaker_id": "2",
          "time": "20210831 14:34"
        },
        {
          "id": "MDRW2100036522.1.36",
          "form": "제 주변에는 케이팝 좋아하는 사람이 없어서",
          "original_form": "제 주변에는 케이팝 좋아하는 사람이 없어서",
          "speaker_id": "2",
          "time": "20210831 14:34"
        },
        {
          "id": "MDRW2100036522.1.37",
          "form": "거의 혼자 덕질해요",
          "original_form": "거의 혼자 덕질해요",
          "speaker_id": "2",
          "time": "20210831 14:34"
        },
        {
          "id": "MDRW2100036522.1.38",
          "form": "헐 혼자 덕질하면",
          "original_form": "헐 혼자 덕질하면",
          "speaker_id": "1",
          "time": "20210831 14:36"
        },
        {
          "id": "MDRW2100036522.1.39",
          "form": "외롭자나요 .. 흑희",
          "original_form": "외롭자나요 .. 흑희",
          "speaker_id": "1",
          "time": "20210831 14:36"
        },
        {
          "id": "MDRW2100036522.1.40",
          "form": "전 혼자 덕질하면 유튜브 댓글보면서 놀아욬ㅋㅋ ㅠㅠ",
          "original_form": "전 혼자 덕질하면 유튜브 댓글보면서 놀아욬ㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 14:37"
        },
        {
          "id": "MDRW2100036522.1.41",
          "form": "슬프다....",
          "original_form": "슬프다....",
