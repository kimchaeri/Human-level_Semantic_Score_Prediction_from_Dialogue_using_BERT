      "utterance": [
        {
          "id": "MDRW2100011625.1.1",
          "form": "재택근무 일자리 해보신거있으세요?",
          "original_form": "재택근무 일자리 해보신거있으세요?",
          "speaker_id": "1",
          "time": "20210824 14:38"
        },
        {
          "id": "MDRW2100011625.1.2",
          "form": "재택 알바는 해봤어요 재택근무로 봐도 된다면요",
          "original_form": "재택 알바는 해봤어요 재택근무로 봐도 된다면요",
          "speaker_id": "2",
          "time": "20210824 14:42"
        },
        {
          "id": "MDRW2100011625.1.3",
          "form": "아하 저동ㅅ",
          "original_form": "아하 저동ㅅ",
          "speaker_id": "1",
          "time": "20210824 14:42"
        },
        {
          "id": "MDRW2100011625.1.4",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210824 14:42"
        },
        {
          "id": "MDRW2100011625.1.5",
          "form": "근데 하다 죽을뻔했어요",
          "original_form": "근데 하다 죽을뻔했어요",
          "speaker_id": "1",
          "time": "20210824 14:42"
        },
        {
          "id": "MDRW2100011625.1.6",
          "form": "너무 일이 많은데 돈은 적게줘서 최저시급도안되는거에요",
          "original_form": "너무 일이 많은데 돈은 적게줘서 최저시급도안되는거에요",
          "speaker_id": "1",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011625.1.7",
          "form": "재택근무를 하고자 하는 사람이 많다보니",
          "original_form": "재택근무를 하고자 하는 사람이 많다보니",
          "speaker_id": "2",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011625.1.8",
          "form": "급여 상관없이 하게되는거같아요",
          "original_form": "급여 상관없이 하게되는거같아요",
          "speaker_id": "2",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011625.1.9",
          "form": "네 그런게있을거에요",
          "original_form": "네 그런게있을거에요",
          "speaker_id": "1",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011625.1.10",
          "form": "노조같은거도없고...",
          "original_form": "노조같은거도없고...",
          "speaker_id": "1",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011625.1.11",
          "form": "그래도 재택근무 장점이 있는거 아닌가요?",
          "original_form": "그래도 재택근무 장점이 있는거 아닌가요?",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011625.1.12",
          "form": "그죠 안나가니 그점은 좋죠",
          "original_form": "그죠 안나가니 그점은 좋죠",
          "speaker_id": "1",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011625.1.13",
          "form": "사람스트레스도적고요",
          "original_form": "사람스트레스도적고요",
          "speaker_id": "1",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011625.1.14",
          "form": "그것도 그렇고 두가지 일을 병행 할수 있으니깐요",
          "original_form": "그것도 그렇고 두가지 일을 병행 할수 있으니깐요",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011625.1.15",
          "form": "팽생직장이 없다보니 재택근무로도 수익을 얻는게 좋죠",
          "original_form": "팽생직장이 없다보니 재택근무로도 수익을 얻는게 좋죠",
          "speaker_id": "2",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011625.1.16",
          "form": "재택근무 일자리가 어떤게 있을거같아요?",
          "original_form": "재택근무 일자리가 어떤게 있을거같아요?",
          "speaker_id": "2",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011625.1.17",
          "form": "영상편집이랑요 글쓰기",
          "original_form": "영상편집이랑요 글쓰기",
          "speaker_id": "1",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011625.1.18",
          "form": "일러스트레이터쪽이랑요",
          "original_form": "일러스트레이터쪽이랑요",
          "speaker_id": "1",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011625.1.19",
          "form": "코딩도 재택가능할거같구요",
          "original_form": "코딩도 재택가능할거같구요",
          "speaker_id": "1",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011625.1.20",
          "form": "요즘엔 과외도 재택으로 하고..",
          "original_form": "요즘엔 과외도 재택으로 하고..",
          "speaker_id": "1",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011625.1.21",
          "form": "부담없이 쉽게 할 수있는 일인가보군요",
          "original_form": "부담없이 쉽게 할 수있는 일인가보군요",
          "speaker_id": "2",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011625.1.22",
          "form": "쉬운일은 아닌거같아요",
          "original_form": "쉬운일은 아닌거같아요",
          "speaker_id": "1",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011625.1.23",
          "form": "코로나라 재택근무 비중이 높아진거같아요",
          "original_form": "코로나라 재택근무 비중이 높아진거같아요",
          "speaker_id": "2",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011625.1.24",
          "form": "네 그런거같네요",
          "original_form": "네 그런거같네요",
          "speaker_id": "1",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011625.1.25",
          "form": "일자리는 적어진만큼 재택근무 일자리는 생기는거같네요.",
          "original_form": "일자리는 적어진만큼 재택근무 일자리는 생기는거같네요.",
          "speaker_id": "2",
          "time": "20210824 14:47"
        }
      ]
