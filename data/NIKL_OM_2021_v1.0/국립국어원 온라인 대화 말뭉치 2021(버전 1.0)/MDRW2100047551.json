      "utterance": [
        {
          "id": "MDRW2100047551.1.1",
          "form": "좋아하는 맛집 있으신가요 ㅎㅎ",
          "original_form": "좋아하는 맛집 있으신가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100047551.1.2",
          "form": "까이식당이요",
          "original_form": "까이식당이요",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100047551.1.3",
          "form": "오 어떤메뉴 팔아요?",
          "original_form": "오 어떤메뉴 팔아요?",
          "speaker_id": "1",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100047551.1.4",
          "form": "싱가포르 음식이요",
          "original_form": "싱가포르 음식이요",
          "speaker_id": "2",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100047551.1.5",
          "form": "오 한번도 안먹어봤어요",
          "original_form": "오 한번도 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100047551.1.6",
          "form": "닭고기 덮밥인데",
          "original_form": "닭고기 덮밥인데",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100047551.1.7",
          "form": "맛나요",
          "original_form": "맛나요",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100047551.1.8",
          "form": "오 닭고기 좋아하는데 ㅎㅎ 서울가면 가봐야겠어요 !",
          "original_form": "오 닭고기 좋아하는데 ㅎㅎ 서울가면 가봐야겠어요 !",
          "speaker_id": "1",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100047551.1.9",
          "form": "다음에 서울가면 가야겠어요 ㅎㅎ !",
          "original_form": "다음에 서울가면 가야겠어요 ㅎㅎ !",
          "speaker_id": "1",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100047551.1.10",
          "form": "맛집다니시나요?",
          "original_form": "맛집다니시나요?",
          "speaker_id": "2",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100047551.1.11",
          "form": "네 맛집가는거 좋아해요 !",
          "original_form": "네 맛집가는거 좋아해요 !",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100047551.1.12",
          "form": "어디가보셨어요?",
          "original_form": "어디가보셨어요?",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100047551.1.13",
          "form": "저는 돈까스 좋아하는 편이여서 최강금돈까스 좋아해요 ㅎㅎ",
          "original_form": "저는 돈까스 좋아하는 편이여서 최강금돈까스 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100047551.1.14",
          "form": "다른 돈까스집이랑 차이점이 뭔가요?",
          "original_form": "다른 돈까스집이랑 차이점이 뭔가요?",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100047551.1.15",
          "form": "음 확실히 고기결이 달라요!",
          "original_form": "음 확실히 고기결이 달라요!",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100047551.1.16",
          "form": "일본식 돈카츠 느낌인데",
          "original_form": "일본식 돈카츠 느낌인데",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100047551.1.17",
          "form": "씹으면 엄청 부드러워요 ㅎㅎ",
          "original_form": "씹으면 엄청 부드러워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:17"
        }
      ]
