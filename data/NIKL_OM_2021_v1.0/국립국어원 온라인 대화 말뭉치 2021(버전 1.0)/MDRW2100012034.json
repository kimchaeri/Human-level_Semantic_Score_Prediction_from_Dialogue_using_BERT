      "utterance": [
        {
          "id": "MDRW2100012034.1.1",
          "form": "안녕하세요 저녁은 드셨어요?",
          "original_form": "안녕하세요 저녁은 드셨어요?",
          "speaker_id": "1",
          "time": "20210824 18:34"
        },
        {
          "id": "MDRW2100012034.1.2",
          "form": "이제 막 먹으려구요!",
          "original_form": "이제 막 먹으려구요!",
          "speaker_id": "2",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100012034.1.3",
          "form": "맛있게 드세요! 저는 방금 콩나물 계란 넣고 라면 한그릇 뚝딱",
          "original_form": "맛있게 드세요! 저는 방금 콩나물 계란 넣고 라면 한그릇 뚝딱",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012034.1.4",
          "form": "와 침나오네요...",
          "original_form": "와 침나오네요...",
          "speaker_id": "2",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100012034.1.5",
          "form": "맛있었어요ㅎㅎ 가가구님은 저녁메뉴 뭐 드실 계획이신가요?",
          "original_form": "맛있었어요ㅎㅎ 가가구님은 저녁메뉴 뭐 드실 계획이신가요?",
          "speaker_id": "1",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100012034.1.6",
          "form": "육개장칼국수요!",
          "original_form": "육개장칼국수요!",
          "speaker_id": "2",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012034.1.7",
          "form": "얼큰한 맛이 일품이겠네요 직접 조리해서 드세요, 아님 배달?",
          "original_form": "얼큰한 맛이 일품이겠네요 직접 조리해서 드세요, 아님 배달?",
          "speaker_id": "1",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012034.1.8",
          "form": "포장해왔습니다ㅎㅎ",
          "original_form": "포장해왔습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012034.1.9",
          "form": "현명하시네요ㅎㅎㅎ",
          "original_form": "현명하시네요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:53"
        },
        {
          "id": "MDRW2100012034.1.10",
          "form": "가서먹는건 요즘 찝찝해서..",
          "original_form": "가서먹는건 요즘 찝찝해서..",
          "speaker_id": "2",
          "time": "20210824 18:54"
        },
        {
          "id": "MDRW2100012034.1.11",
          "form": "그쵸 저도 코로나때문에 항상 배달 아님 포장만",
          "original_form": "그쵸 저도 코로나때문에 항상 배달 아님 포장만",
          "speaker_id": "1",
          "time": "20210824 18:55"
        },
        {
          "id": "MDRW2100012034.1.12",
          "form": "식당에서 밥 먹어본게 언제인지 까마득하네요",
          "original_form": "식당에서 밥 먹어본게 언제인지 까마득하네요",
          "speaker_id": "1",
          "time": "20210824 18:55"
        },
        {
          "id": "MDRW2100012034.1.13",
          "form": "자영업자분들도 많이 힘들듯해요",
          "original_form": "자영업자분들도 많이 힘들듯해요",
          "speaker_id": "2",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100012034.1.14",
          "form": "그쵸 빨리 해결이 돼야할텐데 다들 힘들어서ㅜㅜ",
          "original_form": "그쵸 빨리 해결이 돼야할텐데 다들 힘들어서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 19:01"
        },
        {
          "id": "MDRW2100012034.1.15",
          "form": "그래도 또 저희동네보면 맛집은 여전히 웨이팅 길더라구요",
          "original_form": "그래도 또 저희동네보면 맛집은 여전히 웨이팅 길더라구요",
          "speaker_id": "1",
          "time": "20210824 19:02"
        }
      ]
