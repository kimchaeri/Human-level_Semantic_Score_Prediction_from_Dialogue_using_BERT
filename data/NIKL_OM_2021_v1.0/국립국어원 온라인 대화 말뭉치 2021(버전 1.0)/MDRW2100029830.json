        {
          "id": "MDRW2100029830.1.1",
          "form": "저는 결혼은 안했어요",
          "original_form": "저는 결혼은 안했어요",
          "speaker_id": "2",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100029830.1.2",
          "form": "언제쯤 하실 생각이세요",
          "original_form": "언제쯤 하실 생각이세요",
          "speaker_id": "1",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100029830.1.3",
          "form": "글쎄요 님은 하셨나요?",
          "original_form": "글쎄요 님은 하셨나요?",
          "speaker_id": "2",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100029830.1.4",
          "form": "네 전 했네요",
          "original_form": "네 전 했네요",
          "speaker_id": "1",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100029830.1.5",
          "form": "오래되셧나요",
          "original_form": "오래되셧나요",
          "speaker_id": "2",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100029830.1.6",
          "form": "8년이요",
          "original_form": "8년이요",
          "speaker_id": "1",
          "time": "20210903 14:31"
        },
        {
          "id": "MDRW2100029830.1.7",
          "form": "오래되셨네요",
          "original_form": "오래되셨네요",
          "speaker_id": "2",
          "time": "20210903 14:31"
        },
        {
          "id": "MDRW2100029830.1.8",
          "form": "네 그러네요",
          "original_form": "네 그러네요",
          "speaker_id": "1",
          "time": "20210903 14:32"
        },
        {
          "id": "MDRW2100029830.1.9",
          "form": "전 당장해도 늦엇어요",
          "original_form": "전 당장해도 늦엇어요",
          "speaker_id": "2",
          "time": "20210903 14:32"
        },
        {
          "id": "MDRW2100029830.1.10",
          "form": "아니에요",
          "original_form": "아니에요",
          "speaker_id": "1",
          "time": "20210903 14:34"
        },
        {
          "id": "MDRW2100029830.1.11",
          "form": "뭐 할수있을지도 모르겠고요",
          "original_form": "뭐 할수있을지도 모르겠고요",
          "speaker_id": "2",
          "time": "20210903 14:40"
        },
        {
          "id": "MDRW2100029830.1.12",
          "form": "인연이 있을거에요",
          "original_form": "인연이 있을거에요",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100029830.1.13",
          "form": "그게 참 쉽지않네요 ㅋㅋㅋ",
          "original_form": "그게 참 쉽지않네요 ㅋㅋㅋ",
