      "utterance": [
        {
          "id": "MDRW2100006260.1.1",
          "form": "어디 카페? 베이킹?",
          "original_form": "어디 카페? 베이킹?",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100006260.1.2",
          "form": "응 오븐엔조이 있자나",
          "original_form": "응 오븐엔조이 있자나",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006260.1.3",
          "form": "왜 무슨일임?",
          "original_form": "왜 무슨일임?",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006260.1.4",
          "form": "오프에서 유명한 쿠키가게에서 레시피북 출판했나봐",
          "original_form": "오프에서 유명한 쿠키가게에서 레시피북 출판했나봐",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006260.1.5",
          "form": "요즘은 유명하다고하면 무조건 책 내더라",
          "original_form": "요즘은 유명하다고하면 무조건 책 내더라",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006260.1.6",
          "form": "근데 이게 문제가 뭐냐면 ㅋ 내용이랑 사진이 죄다 복사 붙여넣기고",
          "original_form": "근데 이게 문제가 뭐냐면 ㅋ 내용이랑 사진이 죄다 복사 붙여넣기고",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006260.1.7",
          "form": "레시피 중량도 고정...",
          "original_form": "레시피 중량도 고정...",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006260.1.8",
          "form": "시나몬 파우더 코코아파우더 이런것만 조금씩 다름",
          "original_form": "시나몬 파우더 코코아파우더 이런것만 조금씩 다름",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006260.1.9",
          "form": "헐 그럴거면 뭐하러 책을 내... 유튜브 보는게 더 낫겠다",
          "original_form": "헐 그럴거면 뭐하러 책을 내... 유튜브 보는게 더 낫겠다",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006260.1.10",
          "form": "그래서 지금 네이버카페에서 난리가난거야",
          "original_form": "그래서 지금 네이버카페에서 난리가난거야",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006260.1.11",
          "form": "책 환불하겠다 돈 아깝다 이렇게",
          "original_form": "책 환불하겠다 돈 아깝다 이렇게",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006260.1.12",
          "form": "독자를 기만한거지 그거는 심했네",
          "original_form": "독자를 기만한거지 그거는 심했네",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006260.1.13",
          "form": "근데 책 팔 때 본인이 매장에서 파는 레시피 그대로 넣었다 라고 광고를함",
          "original_form": "근데 책 팔 때 본인이 매장에서 파는 레시피 그대로 넣었다 라고 광고를함",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006260.1.14",
          "form": "허위광고...",
          "original_form": "허위광고...",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006260.1.15",
          "form": "그래서 누가 인스타에 이 레시피 맞냐고 잘못 실린거 아니냐고 물었더니",
          "original_form": "그래서 누가 인스타에 이 레시피 맞냐고 잘못 실린거 아니냐고 물었더니",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006260.1.16",
          "form": "댓글창 죄다 막고",
          "original_form": "댓글창 죄다 막고",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006260.1.17",
          "form": "매장에 파는 레시피랑 다르다고 대문에 박제함",
          "original_form": "매장에 파는 레시피랑 다르다고 대문에 박제함",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006260.1.18",
          "form": "하하 대박이다 허위광고가 아니고 그냥 사람 기만하고 거짓말한거네",
          "original_form": "하하 대박이다 허위광고가 아니고 그냥 사람 기만하고 거짓말한거네",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006260.1.19",
          "form": "재료비만 몇만씩 깨진거지...",
          "original_form": "재료비만 몇만씩 깨진거지...",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006260.1.20",
          "form": "카페회원들이 책 레시피대로 만들어봤는데",
          "original_form": "카페회원들이 책 레시피대로 만들어봤는데",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006260.1.21",
          "form": "먹을수가없대ㅠㅠ",
          "original_form": "먹을수가없대ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006260.1.22",
          "form": "안타깝다 이래서 책도 내용 보고사야해",
          "original_form": "안타깝다 이래서 책도 내용 보고사야해",
          "speaker_id": "2",
          "time": "20210907 22:25"
        }
      ]
    }
  ]
