      "utterance": [
        {
          "id": "MDRW2100023101.1.1",
          "form": "차차차는 누구나오는 드라마에요?",
          "original_form": "차차차는 누구나오는 드라마에요?",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023101.1.2",
          "form": "신민아랑 김선호요!",
          "original_form": "신민아랑 김선호요!",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023101.1.3",
          "form": "김선호..사랑합니다ㅜㅜ",
          "original_form": "김선호..사랑합니다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100023101.1.4",
          "form": "목소리 너무 좋죠",
          "original_form": "목소리 너무 좋죠",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100023101.1.5",
          "form": "연극하는거 한번 보고싶어요 드라마도 좋지만 ㅋㅋㅋ",
          "original_form": "연극하는거 한번 보고싶어요 드라마도 좋지만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100023101.1.6",
          "form": "아 원래 김선호 연극했었나요?",
          "original_form": "아 원래 김선호 연극했었나요?",
          "speaker_id": "1",
          "time": "20210903 15:15"
        },
        {
          "id": "MDRW2100023101.1.7",
          "form": "네 연극할때도 인기 많았다고 들었어요 ㅋㅋ",
          "original_form": "네 연극할때도 인기 많았다고 들었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100023101.1.8",
          "form": "그 얼굴이면 뭔들...",
          "original_form": "그 얼굴이면 뭔들...",
          "speaker_id": "1",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100023101.1.9",
          "form": "근데 연극은 사실 분장도 더 과하고",
          "original_form": "근데 연극은 사실 분장도 더 과하고",
          "speaker_id": "1",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100023101.1.10",
          "form": "모션이랑 목소리를 더 과장되게해서",
          "original_form": "모션이랑 목소리를 더 과장되게해서",
          "speaker_id": "1",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100023101.1.11",
          "form": "느낌달랐을거 같어요",
          "original_form": "느낌달랐을거 같어요",
          "speaker_id": "1",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100023101.1.12",
          "form": "맞아요 연극할때는 톤도 다르고해서 근데 생각해보면 진짜 잘 어울리는 것 같아요",
          "original_form": "맞아요 연극할때는 톤도 다르고해서 근데 생각해보면 진짜 잘 어울리는 것 같아요",
          "speaker_id": "2",
          "time": "20210904 14:47"
        },
        {
          "id": "MDRW2100023101.1.13",
          "form": "진짜ㅋㅋㅋㅋㅋ스타트업 전까지는 1박2일 멤버인지더 몰랐어요ㅋㅋㅋㅋ",
          "original_form": "진짜ㅋㅋㅋㅋㅋ스타트업 전까지는 1박2일 멤버인지더 몰랐어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:28"
        },
        {
          "id": "MDRW2100023101.1.14",
          "form": "1박2일 새로운 시즌을 안봐가지구ㅋㅋㅋ",
          "original_form": "1박2일 새로운 시즌을 안봐가지구ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:28"
        },
        {
          "id": "MDRW2100023101.1.15",
          "form": "아 스타트업보다 1박 2일을 먼저 했었나요 ㅋㅋ",
          "original_form": "아 스타트업보다 1박 2일을 먼저 했었나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:22"
        },
        {
          "id": "MDRW2100023101.1.16",
          "form": "아마도요...? ㅋㅋㅋ",
          "original_form": "아마도요...? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:23"
        },
        {
          "id": "MDRW2100023101.1.17",
          "form": "그렇군요 ㅋㅋㅋ 저도 몰랐네요",
          "original_form": "그렇군요 ㅋㅋㅋ 저도 몰랐네요",
          "speaker_id": "2",
          "time": "20210905 16:23"
        },
        {
          "id": "MDRW2100023101.1.18",
          "form": "오늘도 백일의 낭군님? 그 엑소 도경수가 주연했던 드라마 보는데",
          "original_form": "오늘도 백일의 낭군님? 그 엑소 도경수가 주연했던 드라마 보는데",
          "speaker_id": "1",
          "time": "20210905 16:33"
        },
        {
          "id": "MDRW2100023101.1.19",
          "form": "김선호님 나오시더라구요?",
          "original_form": "김선호님 나오시더라구요?",
          "speaker_id": "1",
          "time": "20210905 16:33"
        },
        {
          "id": "MDRW2100023101.1.20",
          "form": "ㅋㅋㅋㅋ이러고 보면 한번에 갑자게 뜨는 사람은 거의 없고 다들 차곡차곡 쌓다가 하나 포텐터지면 이전꺼도 재조명되고 이러나봐요",
          "original_form": "ㅋㅋㅋㅋ이러고 보면 한번에 갑자게 뜨는 사람은 거의 없고 다들 차곡차곡 쌓다가 하나 포텐터지면 이전꺼도 재조명되고 이러나봐요",
          "speaker_id": "1",
          "time": "20210905 16:34"
        },
        {
          "id": "MDRW2100023101.1.21",
          "form": "아 정말요? 저도 그 드라마는 제목만 아는 수준이긴한데 거기도 나왔는지 몰랐어요",
          "original_form": "아 정말요? 저도 그 드라마는 제목만 아는 수준이긴한데 거기도 나왔는지 몰랐어요",
          "speaker_id": "2",
          "time": "20210905 16:34"
        },
        {
          "id": "MDRW2100023101.1.22",
          "form": "제말이ㅋㅋㅋㅋ",
          "original_form": "제말이ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:35"
        },
        {
          "id": "MDRW2100023101.1.23",
          "form": "요새 약간 드라마 요약본 올려주는 유투버들 덕분에",
          "original_form": "요새 약간 드라마 요약본 올려주는 유투버들 덕분에",
          "speaker_id": "1",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100023101.1.24",
          "form": "그런걸로 좀씩 찾아보니 재미있네요",
          "original_form": "그런걸로 좀씩 찾아보니 재미있네요",
          "speaker_id": "1",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100023101.1.25",
          "form": "맞아요 은근 요약해주는 컨텐츠 시간 순삭 ㅋㅋ",
          "original_form": "맞아요 은근 요약해주는 컨텐츠 시간 순삭 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:37"
        }
      ]
