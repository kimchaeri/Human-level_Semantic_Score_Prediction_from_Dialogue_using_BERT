      "utterance": [
        {
          "id": "MDRW2100002848.1.1",
          "form": "나 대탈출 완전 많이 봤어 요새ㅋㅋ",
          "original_form": "나 대탈출 완전 많이 봤어 요새ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002848.1.2",
          "form": "아진짜? 무서운것도?",
          "original_form": "아진짜? 무서운것도?",
          "speaker_id": "2",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002848.1.3",
          "form": "최근꺼 봤어?",
          "original_form": "최근꺼 봤어?",
          "speaker_id": "2",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002848.1.4",
          "form": "응 최근꺼봤어 좀비나오는거!",
          "original_form": "응 최근꺼봤어 좀비나오는거!",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002848.1.5",
          "form": "또 눈가리면서 봤어ㅋㅋ",
          "original_form": "또 눈가리면서 봤어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002848.1.6",
          "form": "오~ 그래도 보네? 혼자면 무서울거같은데ㅋㅋ",
          "original_form": "오~ 그래도 보네? 혼자면 무서울거같은데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002848.1.7",
          "form": "근데 나 생각보다 잘보는거같아ㅋㅋ",
          "original_form": "근데 나 생각보다 잘보는거같아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002848.1.8",
          "form": "ㅋㅋㅋㅋ 이게 보는건 괜찮은데 직접 하는게 무섭지않아?",
          "original_form": "ㅋㅋㅋㅋ 이게 보는건 괜찮은데 직접 하는게 무섭지않아?",
          "speaker_id": "2",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002848.1.9",
          "form": "무서웠지..ㅠ\r\n그 경비원?나올때 못봤어",
          "original_form": "무서웠지..ㅠ\r\n그 경비원?나올때 못봤어",
          "speaker_id": "1",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002848.1.10",
          "form": "아니 둘다 무서워ㅋㅋㅋ",
          "original_form": "아니 둘다 무서워ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002848.1.11",
          "form": "난 경비원도 그렇고 위에서 그러는게 진짜 무섭더라",
          "original_form": "난 경비원도 그렇고 위에서 그러는게 진짜 무섭더라",
          "speaker_id": "2",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002848.1.12",
          "form": "근데 그거보니까 방탈출하는거 더 무서울거같아",
          "original_form": "근데 그거보니까 방탈출하는거 더 무서울거같아",
          "speaker_id": "1",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002848.1.13",
          "form": "점점 연출이 장난아니야",
          "original_form": "점점 연출이 장난아니야",
          "speaker_id": "2",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002848.1.14",
          "form": "마자 위에서 그럴때 놀랐어",
          "original_form": "마자 위에서 그럴때 놀랐어",
          "speaker_id": "1",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002848.1.15",
          "form": "왜? 우리 하는건 무서운거 아니지않아?",
          "original_form": "왜? 우리 하는건 무서운거 아니지않아?",
          "speaker_id": "2",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002848.1.16",
          "form": "제트도 안무서운거라하지않았나?",
          "original_form": "제트도 안무서운거라하지않았나?",
          "speaker_id": "1",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002848.1.17",
          "form": "모르지 안무서운것처럼하고 무서운거나올지도ㅠ",
          "original_form": "모르지 안무서운것처럼하고 무서운거나올지도ㅠ",
          "speaker_id": "1",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002848.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002848.1.19",
          "form": "하이팜은 안무서울거같고",
          "original_form": "하이팜은 안무서울거같고",
          "speaker_id": "2",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002848.1.20",
          "form": "이제 밝아보이는것도 못믿어",
          "original_form": "이제 밝아보이는것도 못믿어",
          "speaker_id": "1",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002848.1.21",
          "form": "디코더는 감동이라고 하던데..모르겠네",
          "original_form": "디코더는 감동이라고 하던데..모르겠네",
          "speaker_id": "2",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002848.1.22",
          "form": "ㅋㅋㅋ 개웃겨 근데 진짜 제트는.. 안무서운건줄알았는데^^",
          "original_form": "ㅋㅋㅋ 개웃겨 근데 진짜 제트는.. 안무서운건줄알았는데^^",
          "speaker_id": "2",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002848.1.23",
          "form": "아 감동이래?",
          "original_form": "아 감동이래?",
          "speaker_id": "1",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002848.1.24",
          "form": "그냥 우리가 쫄보인걸까?",
          "original_form": "그냥 우리가 쫄보인걸까?",
          "speaker_id": "2",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002848.1.25",
          "form": "그니까 무섭다는얘기 못본거같은데",
          "original_form": "그니까 무섭다는얘기 못본거같은데",
          "speaker_id": "1",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002848.1.26",
          "form": "아니면 스포될까봐 얘기못한건가?",
          "original_form": "아니면 스포될까봐 얘기못한건가?",
          "speaker_id": "1",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002848.1.27",
          "form": "우리가 쫄보긴하지..",
          "original_form": "우리가 쫄보긴하지..",
          "speaker_id": "1",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002848.1.28",
          "form": "무서운구간있다 이런말은 할걸? 디코더꺼는 제대로 찾아보진않은거같네",
          "original_form": "무서운구간있다 이런말은 할걸? 디코더꺼는 제대로 찾아보진않은거같네",
          "speaker_id": "2",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002848.1.29",
          "form": "찾아봐야겠다 무서운거 나올수도있으니까",
          "original_form": "찾아봐야겠다 무서운거 나올수도있으니까",
          "speaker_id": "1",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002848.1.30",
          "form": "스포같으면 보지말구!",
          "original_form": "스포같으면 보지말구!",
          "speaker_id": "2",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002848.1.31",
          "form": "스포될거는 잘 안올리지않아?",
          "original_form": "스포될거는 잘 안올리지않아?",
          "speaker_id": "1",
          "time": "20210820 16:08"
        },
        {
          "id": "MDRW2100002848.1.32",
          "form": "찾아봤어?",
          "original_form": "찾아봤어?",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002848.1.33",
          "form": "아니ㅋㅋㅋ깜박했네ㅋㅋ",
          "original_form": "아니ㅋㅋㅋ깜박했네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:39"
        },
        {
          "id": "MDRW2100002848.1.34",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:43"
        },
        {
          "id": "MDRW2100002848.1.35",
          "form": "보고 무서운거있는지 나도 알려줘",
          "original_form": "보고 무서운거있는지 나도 알려줘",
          "speaker_id": "2",
          "time": "20210820 17:44"
        },
        {
          "id": "MDRW2100002848.1.36",
          "form": "알겠어ㅋㅋ",
          "original_form": "알겠어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:51"
        },
        {
          "id": "MDRW2100002848.1.37",
          "form": "근데 거기 가기전까지 찾아보려나모르겠네ㅋㅋ",
          "original_form": "근데 거기 가기전까지 찾아보려나모르겠네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:52"
        },
        {
          "id": "MDRW2100002848.1.38",
          "form": "ㅋㅋㅋ 그냥 안보고가도 나쁘진않지ㅋㅋ",
          "original_form": "ㅋㅋㅋ 그냥 안보고가도 나쁘진않지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:57"
        },
        {
          "id": "MDRW2100002848.1.39",
          "form": "난 게임하러 갈게ㅋㅋ 출발할때 연락줘!",
          "original_form": "난 게임하러 갈게ㅋㅋ 출발할때 연락줘!",
          "speaker_id": "2",
          "time": "20210820 17:57"
        },
        {
          "id": "MDRW2100002848.1.40",
          "form": "그래 열심히 하고있으렴!",
          "original_form": "그래 열심히 하고있으렴!",
