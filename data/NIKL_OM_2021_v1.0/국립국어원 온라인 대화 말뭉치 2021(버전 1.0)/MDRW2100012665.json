      "utterance": [
        {
          "id": "MDRW2100012665.1.1",
          "form": "name2 공연이나 전시회 좋아하세요?",
          "original_form": "&name2& 공연이나 전시회 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 11:07"
        },
        {
          "id": "MDRW2100012665.1.2",
          "form": "전시회는 몇번 가봤어요",
          "original_form": "전시회는 몇번 가봤어요",
          "speaker_id": "2",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100012665.1.3",
          "form": "근데 그것도 코로나 이후로는 안가게 되더라구요",
          "original_form": "근데 그것도 코로나 이후로는 안가게 되더라구요",
          "speaker_id": "2",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100012665.1.4",
          "form": "아 그런가요...! 코로나 전에는 어떤 전시회 가보셨어요?",
          "original_form": "아 그런가요...! 코로나 전에는 어떤 전시회 가보셨어요?",
          "speaker_id": "1",
          "time": "20210825 13:02"
        },
        {
          "id": "MDRW2100012665.1.5",
          "form": "무슨 타투 전시회",
          "original_form": "무슨 타투 전시회",
          "speaker_id": "2",
          "time": "20210825 13:03"
        },
        {
          "id": "MDRW2100012665.1.6",
          "form": "그런 곳도 가봤고",
          "original_form": "그런 곳도 가봤고",
          "speaker_id": "2",
          "time": "20210825 13:03"
        },
        {
          "id": "MDRW2100012665.1.7",
          "form": "어떤 외국 작가님 전시회도 봤는데",
          "original_form": "어떤 외국 작가님 전시회도 봤는데",
          "speaker_id": "2",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100012665.1.8",
          "form": "정확한 이름은 생각 안나네요",
          "original_form": "정확한 이름은 생각 안나네요",
          "speaker_id": "2",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100012665.1.9",
          "form": "오! 제일 인상적인 전시회는 어떤 건가요?",
          "original_form": "오! 제일 인상적인 전시회는 어떤 건가요?",
          "speaker_id": "1",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100012665.1.10",
          "form": "그 이름 생각안나는 외국 작가 전시회인데",
          "original_form": "그 이름 생각안나는 외국 작가 전시회인데",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100012665.1.11",
          "form": "한 방을 되게 비비드하고 형광색 물건으로 채워넣었더라구요",
          "original_form": "한 방을 되게 비비드하고 형광색 물건으로 채워넣었더라구요",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100012665.1.12",
          "form": "되게 인상적이었는데 알고보니 연예인들도 가고",
          "original_form": "되게 인상적이었는데 알고보니 연예인들도 가고",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100012665.1.13",
          "form": "꽤 유명한 사람이었나봐요",
          "original_form": "꽤 유명한 사람이었나봐요",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100012665.1.14",
          "form": "오ㅋㅋㅋ 신기하네요! 저는 그림만 있는 전시회보다는 활동할 수 있는 전시회가 더 좋더라구요",
          "original_form": "오ㅋㅋㅋ 신기하네요! 저는 그림만 있는 전시회보다는 활동할 수 있는 전시회가 더 좋더라구요",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100012665.1.15",
          "form": "아 저도요 활동하는게 아무래도",
          "original_form": "아 저도요 활동하는게 아무래도",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100012665.1.16",
          "form": "더 집중하기 좋죠",
          "original_form": "더 집중하기 좋죠",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100012665.1.17",
          "form": "제주도에 빛 관련된 전시회가 있던데",
          "original_form": "제주도에 빛 관련된 전시회가 있던데",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100012665.1.18",
          "form": "거기도 가보고 싶어요",
          "original_form": "거기도 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100012665.1.19",
          "form": "고흐?그림이 스크린으로 움직이더라구요",
          "original_form": "고흐?그림이 스크린으로 움직이더라구요",
          "speaker_id": "2",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100012665.1.20",
          "form": "와 대박이네요 저도 꼭 전시회 끝나기 전까지 가보고 싶어요",
          "original_form": "와 대박이네요 저도 꼭 전시회 끝나기 전까지 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100012665.1.21",
          "form": "제주도 빛의벙커로",
          "original_form": "제주도 빛의벙커로",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100012665.1.22",
          "form": "매년하는걸로 알고있어요",
          "original_form": "매년하는걸로 알고있어요",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100012665.1.23",
          "form": "오 정말요? 좋은 정보 감사해요!",
          "original_form": "오 정말요? 좋은 정보 감사해요!",
          "speaker_id": "1",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100012665.1.24",
          "form": "할인도 있나봐요",
          "original_form": "할인도 있나봐요",
          "speaker_id": "2",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100012665.1.25",
          "form": "할인 받으면 좋죠",
          "original_form": "할인 받으면 좋죠",
          "speaker_id": "2",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100012665.1.26",
          "form": "코로나 끝나면 꼭 가야할 리스트로 올려야겠어요",
          "original_form": "코로나 끝나면 꼭 가야할 리스트로 올려야겠어요",
          "speaker_id": "1",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100012665.1.27",
          "form": "오 할인도 한다니 대박이네요",
          "original_form": "오 할인도 한다니 대박이네요",
          "speaker_id": "1",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100012665.1.28",
          "form": "저도 코로나 끝나면 가보려구요!",
          "original_form": "저도 코로나 끝나면 가보려구요!",
          "speaker_id": "2",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100012665.1.29",
          "form": "코로나 전에는 마냥 미뤗는데",
          "original_form": "코로나 전에는 마냥 미뤗는데",
          "speaker_id": "2",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100012665.1.30",
          "form": "이젠 미루다간 또 못갈지도 모르니까",
          "original_form": "이젠 미루다간 또 못갈지도 모르니까",
          "speaker_id": "2",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100012665.1.31",
          "form": "진짜 코로나가 영향 크네요ㅜ",
          "original_form": "진짜 코로나가 영향 크네요ㅜ",
          "speaker_id": "2",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100012665.1.32",
          "form": "그러게요ㅠㅠㅠㅠ 정말 아쉬워요",
          "original_form": "그러게요ㅠㅠㅠㅠ 정말 아쉬워요",
          "speaker_id": "1",
          "time": "20210825 19:13"
        }
      ]
    }
