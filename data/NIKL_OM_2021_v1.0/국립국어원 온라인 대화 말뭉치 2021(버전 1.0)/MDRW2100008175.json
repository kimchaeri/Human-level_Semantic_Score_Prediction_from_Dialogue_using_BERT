      "utterance": [
        {
          "id": "MDRW2100008175.1.1",
          "form": "먹는얘기가 제일 편하지!!??",
          "original_form": "먹는얘기가 제일 편하지!!??",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100008175.1.2",
          "form": "나 지금 낮술하러가는중",
          "original_form": "나 지금 낮술하러가는중",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100008175.1.3",
          "form": "헐 누구랑",
          "original_form": "헐 누구랑",
          "speaker_id": "1",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100008175.1.4",
          "form": "친구랑 동인천에서",
          "original_form": "친구랑 동인천에서",
          "speaker_id": "2",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008175.1.5",
          "form": "명란구이??",
          "original_form": "명란구이??",
          "speaker_id": "2",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008175.1.6",
          "form": "나는 오코노미야끼 먹어야징",
          "original_form": "나는 오코노미야끼 먹어야징",
          "speaker_id": "2",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008175.1.7",
          "form": "명란구이 맛있지",
          "original_form": "명란구이 맛있지",
          "speaker_id": "1",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100008175.1.8",
          "form": "난 방금전에 해물파전 먹었는데",
          "original_form": "난 방금전에 해물파전 먹었는데",
          "speaker_id": "1",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100008175.1.9",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100008175.1.10",
          "form": "명란구이맛있어??",
          "original_form": "명란구이맛있어??",
          "speaker_id": "2",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100008175.1.11",
          "form": "나 해산물 잘안먹어서... 그쪼그만거 만원넘게 받던데",
          "original_form": "나 해산물 잘안먹어서... 그쪼그만거 만원넘게 받던데",
          "speaker_id": "2",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100008175.1.12",
          "form": "짭짤하니 맛있지",
          "original_form": "짭짤하니 맛있지",
          "speaker_id": "1",
          "time": "20210822 14:00"
        },
        {
          "id": "MDRW2100008175.1.13",
          "form": "맥주먹어?",
          "original_form": "맥주먹어?",
          "speaker_id": "1",
          "time": "20210822 14:00"
        },
        {
          "id": "MDRW2100008175.1.14",
          "form": "아마도?? 소주는 잘 못먹으니까",
          "original_form": "아마도?? 소주는 잘 못먹으니까",
          "speaker_id": "2",
          "time": "20210822 14:00"
        },
        {
          "id": "MDRW2100008175.1.15",
          "form": "막걸리두 있자나",
          "original_form": "막걸리두 있자나",
          "speaker_id": "1",
          "time": "20210822 14:01"
        },
        {
          "id": "MDRW2100008175.1.16",
          "form": "아님 사케?",
          "original_form": "아님 사케?",
          "speaker_id": "1",
          "time": "20210822 14:01"
        },
        {
          "id": "MDRW2100008175.1.17",
          "form": "하이볼 있긴하던데",
          "original_form": "하이볼 있긴하던데",
          "speaker_id": "2",
          "time": "20210822 14:02"
        },
        {
          "id": "MDRW2100008175.1.18",
          "form": "사케는 취할거같아",
          "original_form": "사케는 취할거같아",
          "speaker_id": "2",
          "time": "20210822 14:02"
        },
        {
          "id": "MDRW2100008175.1.19",
          "form": "하이볼 한번도 안먹어봤는데 맛있음?",
          "original_form": "하이볼 한번도 안먹어봤는데 맛있음?",
          "speaker_id": "1",
          "time": "20210822 14:04"
        },
        {
          "id": "MDRW2100008175.1.20",
          "form": "음... 술중에 맛있는건 없다지만",
          "original_form": "음... 술중에 맛있는건 없다지만",
          "speaker_id": "2",
          "time": "20210822 14:05"
        },
        {
          "id": "MDRW2100008175.1.21",
          "form": "맛있었엉",
          "original_form": "맛있었엉",
          "speaker_id": "2",
          "time": "20210822 14:05"
        },
        {
          "id": "MDRW2100008175.1.22",
          "form": "뭐 들어가는건데",
          "original_form": "뭐 들어가는건데",
          "speaker_id": "1",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008175.1.23",
          "form": "위스키?",
          "original_form": "위스키?",
          "speaker_id": "1",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008175.1.24",
          "form": "위스키 레몬즙 토닉워터??",
          "original_form": "위스키 레몬즙 토닉워터??",
          "speaker_id": "2",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100008175.1.25",
          "form": "소주랑 맥주보다 맛있어?",
          "original_form": "소주랑 맥주보다 맛있어?",
          "speaker_id": "1",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100008175.1.26",
          "form": "그2개보단 맛있어!!",
          "original_form": "그2개보단 맛있어!!",
          "speaker_id": "2",
          "time": "20210822 14:13"
        },
        {
          "id": "MDRW2100008175.1.27",
          "form": "그러쿤.. 담에 해먹어봐야지",
          "original_form": "그러쿤.. 담에 해먹어봐야지",
          "speaker_id": "1",
