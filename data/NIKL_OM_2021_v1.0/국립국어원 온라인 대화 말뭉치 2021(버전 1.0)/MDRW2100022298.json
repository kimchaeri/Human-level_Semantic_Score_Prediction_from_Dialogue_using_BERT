      "utterance": [
        {
          "id": "MDRW2100022298.1.1",
          "form": "네 확실히 흑돼지 삼겹살이 더 맛있어요",
          "original_form": "네 확실히 흑돼지 삼겹살이 더 맛있어요",
          "speaker_id": "2",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100022298.1.2",
          "form": "궁금하다 오겹살이랑 비슷한가.",
          "original_form": "궁금하다 오겹살이랑 비슷한가.",
          "speaker_id": "1",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100022298.1.3",
          "form": "오겹살ㅋㅋㅋ 비교 불가 ㅜ",
          "original_form": "오겹살ㅋㅋㅋ 비교 불가 ㅜ",
          "speaker_id": "2",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100022298.1.4",
          "form": "바삭 쫄깃이라니",
          "original_form": "바삭 쫄깃이라니",
          "speaker_id": "1",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100022298.1.5",
          "form": "앗..",
          "original_form": "앗..",
          "speaker_id": "1",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100022298.1.6",
          "form": "약간 그런느낌이 들긴 한데 비교불가 ㅜㅜ",
          "original_form": "약간 그런느낌이 들긴 한데 비교불가 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100022298.1.7",
          "form": "똥돼지는 먹어봤나요?ㅋㅋㅋ",
          "original_form": "똥돼지는 먹어봤나요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100022298.1.8",
          "form": "근데 오겹살이랑 삼겹살 같은부위인건 아시나요",
          "original_form": "근데 오겹살이랑 삼겹살 같은부위인건 아시나요",
          "speaker_id": "1",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100022298.1.9",
          "form": "똥돼지도 대박이에요 똥먹고 자랐다던데",
          "original_form": "똥돼지도 대박이에요 똥먹고 자랐다던데",
          "speaker_id": "2",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100022298.1.10",
          "form": "그건뭐에요",
          "original_form": "그건뭐에요",
          "speaker_id": "1",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100022298.1.11",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100022298.1.12",
          "form": "똥먹고 자란 돼지에요",
          "original_form": "똥먹고 자란 돼지에요",
          "speaker_id": "2",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100022298.1.13",
          "form": "그게 가능해요?",
          "original_form": "그게 가능해요?",
          "speaker_id": "1",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100022298.1.14",
          "form": "ㅋㅋㅋㅋ가능하다더군요 제주도는",
          "original_form": "ㅋㅋㅋㅋ가능하다더군요 제주도는",
          "speaker_id": "2",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100022298.1.15",
          "form": "원래 돼지가 아무거나 주워먹잖아요",
          "original_form": "원래 돼지가 아무거나 주워먹잖아요",
          "speaker_id": "2",
          "time": "20210829 12:16"
        },
        {
          "id": "MDRW2100022298.1.16",
          "form": "어.. 신기한 음식이 많네....요..?",
          "original_form": "어.. 신기한 음식이 많네....요..?",
          "speaker_id": "1",
          "time": "20210829 12:16"
        },
        {
          "id": "MDRW2100022298.1.17",
          "form": "처음 들어봐요ㅋㅋㅋ",
          "original_form": "처음 들어봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:16"
        },
        {
          "id": "MDRW2100022298.1.18",
          "form": "네 얘도 진짜 흑돼지 뺨쳐요",
          "original_form": "네 얘도 진짜 흑돼지 뺨쳐요",
          "speaker_id": "2",
          "time": "20210829 12:16"
        },
        {
          "id": "MDRW2100022298.1.19",
          "form": "저 이거 한 10년 전에 먹어봤어요",
          "original_form": "저 이거 한 10년 전에 먹어봤어요",
          "speaker_id": "2",
          "time": "20210829 12:16"
        },
        {
          "id": "MDRW2100022298.1.20",
          "form": "10년이 지나도 기억하는 맛",
          "original_form": "10년이 지나도 기억하는 맛",
