        },
        {
          "id": "MDRW2100038577.1.2",
          "form": "오늘 축규해요?",
          "original_form": "오늘 축규해요?",
          "speaker_id": "2",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100038577.1.3",
          "form": "네넹 ㅎㅎ 치킨에 쏘맥했슴다",
          "original_form": "네넹 ㅎㅎ 치킨에 쏘맥했슴다",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100038577.1.4",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "2",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100038577.1.5",
          "form": "저도 치킨 먹었어요 ㅎ",
          "original_form": "저도 치킨 먹었어요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100038577.1.6",
          "form": "역시 ㅎㅎ",
          "original_form": "역시 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100038577.1.7",
          "form": "어디거 드셨나여 ㅎ",
          "original_form": "어디거 드셨나여 ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100038577.1.8",
          "form": "전 비비큐 황올치욥!",
          "original_form": "전 비비큐 황올치욥!",
          "speaker_id": "1",
          "time": "20210903 08:33"
        },
        {
          "id": "MDRW2100038577.1.9",
          "form": "그냥 후라이드인가요?",
          "original_form": "그냥 후라이드인가요?",
          "speaker_id": "2",
          "time": "20210903 08:39"
        },
        {
          "id": "MDRW2100038577.1.10",
          "form": "네네 그렇슴당",
          "original_form": "네네 그렇슴당",
          "speaker_id": "1",
          "time": "20210903 09:28"
        },
        {
          "id": "MDRW2100038577.1.11",
          "form": "우와 그냥 후라이드인데",
          "original_form": "우와 그냥 후라이드인데",
          "speaker_id": "2",
          "time": "20210903 12:10"
        },
        {
          "id": "MDRW2100038577.1.12",
          "form": "거기꺼 엄청 유명하잖아요",
          "original_form": "거기꺼 엄청 유명하잖아요",
          "speaker_id": "2",
          "time": "20210903 12:10"
        },
        {
          "id": "MDRW2100038577.1.13",
          "form": "맛나용 ㅎㅎ 근데 요즘 맛있는곳이 워낙많아서요",
          "original_form": "맛나용 ㅎㅎ 근데 요즘 맛있는곳이 워낙많아서요",
          "speaker_id": "1",
          "time": "20210903 12:59"
        },
        {
          "id": "MDRW2100038577.1.14",
          "form": "다 치킨 맛집이죠",
          "original_form": "다 치킨 맛집이죠",
          "speaker_id": "2",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100038577.1.15",
          "form": "가격을 좀 내럈으면 해요ㅎ",
          "original_form": "가격을 좀 내럈으면 해요ㅎ",
          "speaker_id": "2",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100038577.1.16",
          "form": "진짜 너무비싸요 ㅠ",
          "original_form": "진짜 너무비싸요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100038577.1.17",
          "form": "치킨.. 예전에는 비싸도 만원대 중후반이었는데",
          "original_form": "치킨.. 예전에는 비싸도 만원대 중후반이었는데",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100038577.1.18",
          "form": "요새는 기본 2만원이더라구요",
          "original_form": "요새는 기본 2만원이더라구요",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100038577.1.19",
          "form": "더 비싸지면 직접 닭튀겨 먹어야할판..ㅋㅋ",
          "original_form": "더 비싸지면 직접 닭튀겨 먹어야할판..ㅋㅋ",
