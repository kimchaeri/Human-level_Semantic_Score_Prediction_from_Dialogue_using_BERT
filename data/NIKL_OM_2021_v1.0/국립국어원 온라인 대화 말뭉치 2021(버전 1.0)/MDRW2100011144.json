      "utterance": [
        {
          "id": "MDRW2100011144.1.1",
          "form": "앗 동아리 안해...",
          "original_form": "앗 동아리 안해...",
          "speaker_id": "2",
          "time": "20210824 01:45"
        },
        {
          "id": "MDRW2100011144.1.2",
          "form": "언니는?",
          "original_form": "언니는?",
          "speaker_id": "2",
          "time": "20210824 01:45"
        },
        {
          "id": "MDRW2100011144.1.3",
          "form": "나는 1학년때 하다가 나왔어~",
          "original_form": "나는 1학년때 하다가 나왔어~",
          "speaker_id": "1",
          "time": "20210824 01:45"
        },
        {
          "id": "MDRW2100011144.1.4",
          "form": "무슨 동아리?",
          "original_form": "무슨 동아리?",
          "speaker_id": "2",
          "time": "20210824 01:45"
        },
        {
          "id": "MDRW2100011144.1.5",
          "form": "동아리가 안맞더라고 ㅎㅎ 공연동아리여서 너무 힘들었어!",
          "original_form": "동아리가 안맞더라고 ㅎㅎ 공연동아리여서 너무 힘들었어!",
          "speaker_id": "1",
          "time": "20210824 01:45"
        },
        {
          "id": "MDRW2100011144.1.6",
          "form": "밴드!",
          "original_form": "밴드!",
          "speaker_id": "1",
          "time": "20210824 01:45"
        },
        {
          "id": "MDRW2100011144.1.7",
          "form": "헐 밴드부!!!",
          "original_form": "헐 밴드부!!!",
          "speaker_id": "2",
          "time": "20210824 01:46"
        },
        {
          "id": "MDRW2100011144.1.8",
          "form": "소모임 같은 것도 안해?",
          "original_form": "소모임 같은 것도 안해?",
          "speaker_id": "1",
          "time": "20210824 01:46"
        },
        {
          "id": "MDRW2100011144.1.9",
          "form": "ㅋㅋㅋㅋㅋ코로나 학번이라 그런 것 못해봤엉ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ코로나 학번이라 그런 것 못해봤엉ㅠ",
          "speaker_id": "2",
          "time": "20210824 01:46"
        },
        {
          "id": "MDRW2100011144.1.10",
          "form": "헐 아!!!!그렇구나 아오ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "헐 아!!!!그렇구나 아오ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 01:46"
        },
        {
          "id": "MDRW2100011144.1.11",
          "form": "학교는 가봤어?????",
          "original_form": "학교는 가봤어?????",
          "speaker_id": "1",
          "time": "20210824 01:46"
        },
        {
          "id": "MDRW2100011144.1.12",
          "form": "응응 실습과라서 자주 가!",
          "original_form": "응응 실습과라서 자주 가!",
          "speaker_id": "2",
          "time": "20210824 01:47"
        },
        {
          "id": "MDRW2100011144.1.13",
          "form": "헉 그렇구나!! 진짜 공부만 하는 거네ㅠㅠㅠ",
          "original_form": "헉 그렇구나!! 진짜 공부만 하는 거네ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 01:49"
        },
        {
          "id": "MDRW2100011144.1.14",
          "form": "이번 학기도 대면이야?",
          "original_form": "이번 학기도 대면이야?",
          "speaker_id": "1",
          "time": "20210824 01:49"
        },
        {
          "id": "MDRW2100011144.1.15",
          "form": "9월 30일까지는 비대면인데 아마 불려갈 듯ㅠㅠ",
          "original_form": "9월 30일까지는 비대면인데 아마 불려갈 듯ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 01:49"
        },
        {
          "id": "MDRW2100011144.1.16",
          "form": "컴퓨터 잘해?",
          "original_form": "컴퓨터 잘해?",
          "speaker_id": "2",
          "time": "20210824 01:50"
        },
        {
          "id": "MDRW2100011144.1.17",
          "form": "아이고ㅠㅠㅠㅠㅠ 컴퓨터 나 잘 못해~",
          "original_form": "아이고ㅠㅠㅠㅠㅠ 컴퓨터 나 잘 못해~",
          "speaker_id": "1",
          "time": "20210824 01:50"
        },
        {
          "id": "MDRW2100011144.1.18",
          "form": "최근에 컴활 따려고 하는데",
          "original_form": "최근에 컴활 따려고 하는데",
          "speaker_id": "1",
          "time": "20210824 01:50"
        },
        {
          "id": "MDRW2100011144.1.19",
          "form": "쉽지 않다",
          "original_form": "쉽지 않다",
          "speaker_id": "1",
          "time": "20210824 01:50"
        },
