        },
        {
          "id": "MDRW2100032469.1.2",
          "form": "네 있어요 ㅋㅋㅋ",
          "original_form": "네 있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:44"
        },
        {
          "id": "MDRW2100032469.1.3",
          "form": "name2님은 있으신가요?",
          "original_form": "&name2&님은 있으신가요?",
          "speaker_id": "1",
          "time": "20210905 18:45"
        },
        {
          "id": "MDRW2100032469.1.4",
          "form": "네네 저두욤",
          "original_form": "네네 저두욤",
          "speaker_id": "2",
          "time": "20210905 18:48"
        },
        {
          "id": "MDRW2100032469.1.5",
          "form": "오 왠지 있으실 것 같았어요",
          "original_form": "오 왠지 있으실 것 같았어요",
          "speaker_id": "1",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100032469.1.6",
          "form": "음.. 누나 있으시지 않으세요?",
          "original_form": "음.. 누나 있으시지 않으세요?",
          "speaker_id": "1",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100032469.1.7",
          "form": "왠지 모르게? ㅋㅋ",
          "original_form": "왠지 모르게? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:53"
        },
        {
          "id": "MDRW2100032469.1.8",
          "form": "오옹 아닙니당",
          "original_form": "오옹 아닙니당",
          "speaker_id": "2",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100032469.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100032469.1.10",
          "form": "어? 아니세요?",
          "original_form": "어? 아니세요?",
          "speaker_id": "1",
          "time": "20210905 19:33"
        },
        {
          "id": "MDRW2100032469.1.11",
          "form": "네네넨 아니에요!",
          "original_form": "네네넨 아니에요!",
          "speaker_id": "2",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032469.1.12",
          "form": "그러면 정답은 뭔가요? ㅋㅋ",
          "original_form": "그러면 정답은 뭔가요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032469.1.13",
          "form": "남동생이요!",
          "original_form": "남동생이요!",
          "speaker_id": "2",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032469.1.14",
          "form": "완전 틀렸네요 ㅋㅋ",
          "original_form": "완전 틀렸네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032469.1.15",
          "form": "남동생이랑은 사이가 좋으시나요?",
          "original_form": "남동생이랑은 사이가 좋으시나요?",
