      "utterance": [
        {
          "id": "MDRW2100019240.1.1",
          "form": "최근에 구매하신 물건이 있나요?",
          "original_form": "최근에 구매하신 물건이 있나요?",
          "speaker_id": "1",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019240.1.2",
          "form": "저는 장바구니만 늘어나고 산건 없어서요ㅠ",
          "original_form": "저는 장바구니만 늘어나고 산건 없어서요ㅠ",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019240.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019240.1.4",
          "form": "저는 최근에 버킷햇을 샀어요 ㅎㅎ",
          "original_form": "저는 최근에 버킷햇을 샀어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019240.1.5",
          "form": "적립금 모아서 사서 저렴하게 샀답니다!",
          "original_form": "적립금 모아서 사서 저렴하게 샀답니다!",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019240.1.6",
          "form": "우왕 그거 힙한 모자잖아요",
          "original_form": "우왕 그거 힙한 모자잖아요",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019240.1.7",
          "form": "제가 하면 그냥 썬캡인데",
          "original_form": "제가 하면 그냥 썬캡인데",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019240.1.8",
          "form": "어울리시나봐요 부럽당",
          "original_form": "어울리시나봐요 부럽당",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019240.1.9",
          "form": "어울리진않더라구요...흑흑ㅋㅋㅋ 근데 사고싶어서 사버렸어요!",
          "original_form": "어울리진않더라구요...흑흑ㅋㅋㅋ 근데 사고싶어서 사버렸어요!",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019240.1.10",
          "form": "운동용으로 써야할듯해요",
          "original_form": "운동용으로 써야할듯해요",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019240.1.11",
          "form": "머리가 잘 되어있지 않을때 항상 볼캡을 쓰고 나갔었는데",
          "original_form": "머리가 잘 되어있지 않을때 항상 볼캡을 쓰고 나갔었는데",
          "speaker_id": "2",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019240.1.12",
          "form": "버킷햇쓰면 뭔가 새로운 느낌이 나서 좋을 것같아요",
          "original_form": "버킷햇쓰면 뭔가 새로운 느낌이 나서 좋을 것같아요",
          "speaker_id": "2",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019240.1.13",
          "form": "약간 요구르트 배달해야할거같기도 하구...ㅎ",
          "original_form": "약간 요구르트 배달해야할거같기도 하구...ㅎ",
          "speaker_id": "1",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019240.1.14",
          "form": "검은색이라서 그나마 다행이에요..",
          "original_form": "검은색이라서 그나마 다행이에요..",
          "speaker_id": "1",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019240.1.15",
          "form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019240.1.16",
          "form": "아 베이지색 샀으면 큰일날 뻔했네요",
          "original_form": "아 베이지색 샀으면 큰일날 뻔했네요",
          "speaker_id": "2",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019240.1.17",
          "form": "ㅋㅋㅋㅋ비가 와서 운동도 못하구 얼른 써봐야하는데!!",
          "original_form": "ㅋㅋㅋㅋ비가 와서 운동도 못하구 얼른 써봐야하는데!!",
          "speaker_id": "1",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019240.1.18",
          "form": "다음주인가? 그때 비가 그친다구 들었어요",
          "original_form": "다음주인가? 그때 비가 그친다구 들었어요",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019240.1.19",
          "form": "그때 자주 쓰고 나가기~~~~",
          "original_form": "그때 자주 쓰고 나가기~~~~",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019240.1.20",
          "form": "넵!! 위시리스트엔 어떤 물건이 있나요? ㅎㅎ",
          "original_form": "넵!! 위시리스트엔 어떤 물건이 있나요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019240.1.21",
