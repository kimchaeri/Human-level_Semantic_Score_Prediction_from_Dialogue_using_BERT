      "utterance": [
        {
          "id": "MDRW2100040820.1.1",
          "form": "아침 드셨어요?",
          "original_form": "아침 드셨어요?",
          "speaker_id": "1",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100040820.1.2",
          "form": "아뇨 아직 안먹었어요",
          "original_form": "아뇨 아직 안먹었어요",
          "speaker_id": "2",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100040820.1.3",
          "form": "식사 하셨나요?",
          "original_form": "식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100040820.1.4",
          "form": "네 방금 아침식사 먹었어요",
          "original_form": "네 방금 아침식사 먹었어요",
          "speaker_id": "1",
          "time": "20210905 08:52"
        },
        {
          "id": "MDRW2100040820.1.5",
          "form": "어떤거 드셨나요?",
          "original_form": "어떤거 드셨나요?",
          "speaker_id": "2",
          "time": "20210905 08:52"
        },
        {
          "id": "MDRW2100040820.1.6",
          "form": "저는 호박전이요",
          "original_form": "저는 호박전이요",
          "speaker_id": "1",
          "time": "20210905 08:53"
        },
        {
          "id": "MDRW2100040820.1.7",
          "form": "어제 잔뜩 해놔서",
          "original_form": "어제 잔뜩 해놔서",
          "speaker_id": "1",
          "time": "20210905 08:53"
        },
        {
          "id": "MDRW2100040820.1.8",
          "form": "호박전 맛있겠네요",
          "original_form": "호박전 맛있겠네요",
          "speaker_id": "2",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100040820.1.9",
          "form": "네 늙은호박이 생겨서 어제 처음 전으로 부쳐봤는데",
          "original_form": "네 늙은호박이 생겨서 어제 처음 전으로 부쳐봤는데",
          "speaker_id": "1",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100040820.1.10",
          "form": "생각보다 맛있더라고요",
          "original_form": "생각보다 맛있더라고요",
          "speaker_id": "1",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100040820.1.11",
          "form": "아 늙은호박으로 만드신거면",
          "original_form": "아 늙은호박으로 만드신거면",
          "speaker_id": "2",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100040820.1.12",
          "form": "아침은 뭐 드실 예정이예요?",
          "original_form": "아침은 뭐 드실 예정이예요?",
          "speaker_id": "1",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100040820.1.13",
          "form": "채쳐서 부치나요?",
          "original_form": "채쳐서 부치나요?",
          "speaker_id": "2",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100040820.1.14",
          "form": "네네 맞아요 채치고 갈아서",
          "original_form": "네네 맞아요 채치고 갈아서",
          "speaker_id": "1",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100040820.1.15",
          "form": "저는 출근중이라",
          "original_form": "저는 출근중이라",
          "speaker_id": "2",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100040820.1.16",
          "form": "그냥 회사에서 우유랑 견과류 간단하게 먹으려구요",
          "original_form": "그냥 회사에서 우유랑 견과류 간단하게 먹으려구요",
          "speaker_id": "2",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100040820.1.17",
          "form": "일요일인데 출근하시는구나~",
          "original_form": "일요일인데 출근하시는구나~",
          "speaker_id": "1",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100040820.1.18",
          "form": "아침식사 거르지 말고 잘 챙겨드셔요ㅠ",
          "original_form": "아침식사 거르지 말고 잘 챙겨드셔요ㅠ",
