      "utterance": [
        {
          "id": "MDRW2100018976.1.1",
          "form": "요새 갖고싶은 물건이있으신가요~,",
          "original_form": "요새 갖고싶은 물건이있으신가요~,",
          "speaker_id": "2",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100018976.1.2",
          "form": "음 닌테도요",
          "original_form": "음 닌테도요",
          "speaker_id": "1",
          "time": "20210827 14:33"
        },
        {
          "id": "MDRW2100018976.1.3",
          "form": "닌텐도 있으세요?",
          "original_form": "닌텐도 있으세요?",
          "speaker_id": "1",
          "time": "20210827 14:33"
        },
        {
          "id": "MDRW2100018976.1.4",
          "form": "아님 미니빔?",
          "original_form": "아님 미니빔?",
          "speaker_id": "1",
          "time": "20210827 14:33"
        },
        {
          "id": "MDRW2100018976.1.5",
          "form": "둘다없는데! 빔은 요새 갖고싶었네요!",
          "original_form": "둘다없는데! 빔은 요새 갖고싶었네요!",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018976.1.6",
          "form": "생각보다 안쓸거 같은데 그냥 갖고 싶어요",
          "original_form": "생각보다 안쓸거 같은데 그냥 갖고 싶어요",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100018976.1.7",
          "form": "요즘 갖고싶은게 있나요?",
          "original_form": "요즘 갖고싶은게 있나요?",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100018976.1.8",
          "form": "저내년에 이사가면 빔설치하고싶더라구요!",
          "original_form": "저내년에 이사가면 빔설치하고싶더라구요!",
          "speaker_id": "2",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100018976.1.9",
          "form": "요샌 별로 낙이없네요ㅠㅠ 소비는 최소로하는지라ㅠㅠ",
          "original_form": "요샌 별로 낙이없네요ㅠㅠ 소비는 최소로하는지라ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100018976.1.10",
          "form": "저도 생각만 하고 있어서 빔은 꼭 언젠가 사야겠어요ㅋ",
          "original_form": "저도 생각만 하고 있어서 빔은 꼭 언젠가 사야겠어요ㅋ",
          "speaker_id": "1",
          "time": "20210827 15:06"
        },
        {
          "id": "MDRW2100018976.1.11",
          "form": "해방타운에서 봤는데 백지영이 설치만 스텐드빔인가? 알아보진않앗는데 ㅋㅋㅋ 이동도 편할꺼같고 하더라구요",
          "original_form": "해방타운에서 봤는데 백지영이 설치만 스텐드빔인가? 알아보진않앗는데 ㅋㅋㅋ 이동도 편할꺼같고 하더라구요",
          "speaker_id": "2",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100018976.1.12",
          "form": "이게 밝기루시안인가 보고 사야한대요",
          "original_form": "이게 밝기루시안인가 보고 사야한대요",
          "speaker_id": "1",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100018976.1.13",
          "form": "가격을 떠나서 그게 낮으면? 진짜 깜깜해야만 보여서",
          "original_form": "가격을 떠나서 그게 낮으면? 진짜 깜깜해야만 보여서",
          "speaker_id": "1",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100018976.1.14",
          "form": "쓰기 어렵다 하더라구여",
          "original_form": "쓰기 어렵다 하더라구여",
          "speaker_id": "1",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100018976.1.15",
          "form": "아 ! 또 그런 제한이있군요~ 사게되면 잘알아봐야겠어요!",
          "original_form": "아 ! 또 그런 제한이있군요~ 사게되면 잘알아봐야겠어요!",
          "speaker_id": "2",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100018976.1.16",
          "form": "전 막 물건구매시 그닥 신중한 성격은아닌거같아서ㅠㅠ 막 사고 후회도많이해요ㅠㅠ",
          "original_form": "전 막 물건구매시 그닥 신중한 성격은아닌거같아서ㅠㅠ 막 사고 후회도많이해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100018976.1.17",
          "form": "그럼안되요",
          "original_form": "그럼안되요",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100018976.1.18",
          "form": "싸다고 작다고 좋은게 아니더라구여",
          "original_form": "싸다고 작다고 좋은게 아니더라구여",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100018976.1.19",
          "form": "어느정도 빛이있어도 잘보이는거 사실길 바래여^^",
          "original_form": "어느정도 빛이있어도 잘보이는거 사실길 바래여^^",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100018976.1.20",
          "form": "네 ! 나중에 잘알아봐야겠어요!",
          "original_form": "네 ! 나중에 잘알아봐야겠어요!",
          "speaker_id": "2",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100018976.1.21",
          "form": "네넨 역시 돈쓰는건 상상만해도 즐겁네요",
          "original_form": "네넨 역시 돈쓰는건 상상만해도 즐겁네요",
          "speaker_id": "1",
          "time": "20210827 15:47"
        }
      ]
    }
