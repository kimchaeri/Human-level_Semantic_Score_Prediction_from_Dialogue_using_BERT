      "utterance": [
        {
          "id": "MDRW2100030874.1.1",
          "form": "내 직업은 쇼퍼다ㅋㅋ",
          "original_form": "내 직업은 쇼퍼다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:48"
        },
        {
          "id": "MDRW2100030874.1.2",
          "form": "쇼퍼가 뭐야 ㅋ",
          "original_form": "쇼퍼가 뭐야 ㅋ",
          "speaker_id": "1",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100030874.1.3",
          "form": "쇼핑하는사람?ㅋㅋ오빠가 하루죙일 집에서 쇼핑한다고 나  직업이 쇼퍼래",
          "original_form": "쇼핑하는사람?ㅋㅋ오빠가 하루죙일 집에서 쇼핑한다고 나  직업이 쇼퍼래",
          "speaker_id": "2",
          "time": "20210904 17:09"
        },
        {
          "id": "MDRW2100030874.1.4",
          "form": "쇼퍼라는 직업을 이번에 알았네",
          "original_form": "쇼퍼라는 직업을 이번에 알았네",
          "speaker_id": "1",
          "time": "20210904 17:09"
        },
        {
          "id": "MDRW2100030874.1.5",
          "form": "아까 풀바셋에서 파스타치오라떼 먹었는데 맛있드라",
          "original_form": "아까 풀바셋에서 파스타치오라떼 먹었는데 맛있드라",
          "speaker_id": "1",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100030874.1.6",
          "form": "오홍. 맛있는 음료 구매도 쇼핑이라고 할수있으려나ㅎ",
          "original_form": "오홍. 맛있는 음료 구매도 쇼핑이라고 할수있으려나ㅎ",
          "speaker_id": "2",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100030874.1.7",
          "form": "오 버스 바로왔다",
          "original_form": "오 버스 바로왔다",
          "speaker_id": "2",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100030874.1.8",
          "form": "ㅋㅋㅋㅋ 상품권 사야하는데 티몬에 안나오드라",
          "original_form": "ㅋㅋㅋㅋ 상품권 사야하는데 티몬에 안나오드라",
          "speaker_id": "1",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100030874.1.9",
          "form": "나도 지금 올라가는중",
          "original_form": "나도 지금 올라가는중",
          "speaker_id": "1",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100030874.1.10",
          "form": "ㅜㅜ 첫날 북앤을 바로 샀어야했는데",
          "original_form": "ㅜㅜ 첫날 북앤을 바로 샀어야했는데",
          "speaker_id": "2",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100030874.1.11",
          "form": "북앤은 본적이 없어",
          "original_form": "북앤은 본적이 없어",
          "speaker_id": "1",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100030874.1.12",
          "form": "아쉽게",
          "original_form": "아쉽게",
          "speaker_id": "1",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100030874.1.13",
          "form": "1일 12시 땡하고 몇시간 떠있었었어",
          "original_form": "1일 12시 땡하고 몇시간 떠있었었어",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100030874.1.14",
          "form": "11번가가 지난달처럼만 나와주면좋은데",
          "original_form": "11번가가 지난달처럼만 나와주면좋은데",
          "speaker_id": "2",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100030874.1.15",
          "form": "이번에 나올지 모르겟네 뽐뿌 들어가봐야 하나",
          "original_form": "이번에 나올지 모르겟네 뽐뿌 들어가봐야 하나",
          "speaker_id": "1",
          "time": "20210904 17:32"
        },
        {
          "id": "MDRW2100030874.1.16",
          "form": "없네 없어 카드결제가 안되는 것민 뜨는건가",
          "original_form": "없네 없어 카드결제가 안되는 것민 뜨는건가",
          "speaker_id": "1",
          "time": "20210904 17:34"
        },
        {
          "id": "MDRW2100030874.1.17",
          "form": "딥온체크를 버려야될까봐",
          "original_form": "딥온체크를 버려야될까봐",
          "speaker_id": "2",
          "time": "20210904 17:38"
        },
        {
          "id": "MDRW2100030874.1.18",
          "form": "아 오늘 차이카드 동원몰 부스터 쓸만한데",
          "original_form": "아 오늘 차이카드 동원몰 부스터 쓸만한데",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100030874.1.19",
          "form": "번개10개 5000원인데 최소사용금액이 없어서",
          "original_form": "번개10개 5000원인데 최소사용금액이 없어서",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100030874.1.20",
          "form": "ㅋㅋㅋ 아까 보기만 햇네",
          "original_form": "ㅋㅋㅋ 아까 보기만 햇네",
          "speaker_id": "1",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100030874.1.21",
          "form": "구매는 안함",
          "original_form": "구매는 안함",
          "speaker_id": "1",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100030874.1.22",
          "form": "종합비타민제 5900원짜리 무료배송있길래 900원실결제 샀어ㅎ",
          "original_form": "종합비타민제 5900원짜리 무료배송있길래 900원실결제 샀어ㅎ",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100030874.1.23",
          "form": "종합비타민 그런것도 괜찮기는 한0",
          "original_form": "종합비타민 그런것도 괜찮기는 한0",
          "speaker_id": "1",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100030874.1.24",
          "form": "석류젤리 40개 9900원도 땡겨서 너한테 링크보냈는데",
          "original_form": "석류젤리 40개 9900원도 땡겨서 너한테 링크보냈는데",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100030874.1.25",
          "form": "안샀어?",
          "original_form": "안샀어?",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100030874.1.26",
          "form": "개당 백원씩인가",
          "original_form": "개당 백원씩인가",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100030874.1.27",
          "form": "부스터한번만 쓸수있으니 못샀지",
          "original_form": "부스터한번만 쓸수있으니 못샀지",
          "speaker_id": "2",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100030874.1.28",
          "form": "오빠는번개가10개 없어",
          "original_form": "오빠는번개가10개 없어",
          "speaker_id": "2",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100030874.1.29",
          "form": "내꺼를 올리고 니가 쓸래 ?",
          "original_form": "내꺼를 올리고 니가 쓸래 ?",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100030874.1.30",
          "form": "그거 기간 길든데",
          "original_form": "그거 기간 길든데",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100030874.1.31",
          "form": "번개값은 어쩌려고ㅋ",
          "original_form": "번개값은 어쩌려고ㅋ",
          "speaker_id": "2",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100030874.1.32",
          "form": "낼 결제해",
          "original_form": "낼 결제해",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100030874.1.33",
          "form": "낼도 부스터있으면생각해보자",
          "original_form": "낼도 부스터있으면생각해보자",
          "speaker_id": "2",
          "time": "20210904 18:21"
        },
        {
          "id": "MDRW2100030874.1.34",
          "form": "그거 날짜 보니까 오래있을꺼 같드라",
          "original_form": "그거 날짜 보니까 오래있을꺼 같드라",
          "speaker_id": "1",
          "time": "20210904 18:21"
        },
        {
          "id": "MDRW2100030874.1.35",
          "form": "날짜가 길어",
          "original_form": "날짜가 길어",
          "speaker_id": "1",
          "time": "20210904 18:21"
        }
      ]
    }
