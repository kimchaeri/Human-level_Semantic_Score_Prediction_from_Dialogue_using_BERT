      "utterance": [
        {
          "id": "MDRW2100002794.1.1",
          "form": "모시모시",
          "original_form": "모시모시",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002794.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002794.1.3",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002794.1.4",
          "form": "방금 깔고 처음 말걸어봤어요",
          "original_form": "방금 깔고 처음 말걸어봤어요",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002794.1.5",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002794.1.6",
          "form": "주제가 종교네요",
          "original_form": "주제가 종교네요",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002794.1.7",
          "form": "어뇨 어릴 때 교회는 잠깐 다녀봤어용",
          "original_form": "어뇨 어릴 때 교회는 잠깐 다녀봤어용",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002794.1.8",
          "form": "근데 영 성경 내용이 못믿겠어서 ㅎㅎ",
          "original_form": "근데 영 성경 내용이 못믿겠어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002794.1.9",
          "form": "저도 어릴때 교회 다니다가 그만뒀어요",
          "original_form": "저도 어릴때 교회 다니다가 그만뒀어요",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002794.1.10",
          "form": "신앙심이랑은 거리가 먼가봐요ㅋㅋ",
          "original_form": "신앙심이랑은 거리가 먼가봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002794.1.11",
          "form": "역시 저처럼 말씀이 안믿겨서?",
          "original_form": "역시 저처럼 말씀이 안믿겨서?",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002794.1.12",
          "form": "전 어머니는 아직 다니시는데",
          "original_form": "전 어머니는 아직 다니시는데",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002794.1.13",
          "form": "네ㅎㅎ 믿음이 안가네요",
          "original_form": "네ㅎㅎ 믿음이 안가네요",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002794.1.14",
          "form": "저 전도하려고 아직도 노력중이심",
          "original_form": "저 전도하려고 아직도 노력중이심",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002794.1.15",
          "form": "저도 아버지는 독실한 크리스쳔이라",
          "original_form": "저도 아버지는 독실한 크리스쳔이라",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002794.1.16",
          "form": "안믿으면 지옥간다고 하는데",
          "original_form": "안믿으면 지옥간다고 하는데",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002794.1.17",
          "form": "뭐... 안갑니다",
          "original_form": "뭐... 안갑니다",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002794.1.18",
          "form": "김수환 추기경님이 안믿어도 천국 갈 수 있다고 하셨다고 말씀드려보세용",
          "original_form": "김수환 추기경님이 안믿어도 천국 갈 수 있다고 하셨다고 말씀드려보세용",
          "speaker_id": "1",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002794.1.19",
          "form": "오 이런 말이 있었나요",
          "original_form": "오 이런 말이 있었나요",
          "speaker_id": "2",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002794.1.20",
          "form": "기억해둘게요",
          "original_form": "기억해둘게요",
          "speaker_id": "2",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002794.1.21",
          "form": "저는 뭐 어차피 성경이 삼국지처럼 소설이라고 생각해서",
          "original_form": "저는 뭐 어차피 성경이 삼국지처럼 소설이라고 생각해서",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002794.1.22",
          "form": "그냥 마음가짐 다스리는 정도로만 쓰는건 좋을듯",
          "original_form": "그냥 마음가짐 다스리는 정도로만 쓰는건 좋을듯",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002794.1.23",
          "form": "저도 마찬가지에요",
          "original_form": "저도 마찬가지에요",
          "speaker_id": "2",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002794.1.24",
          "form": "최근에 제가 하는 일 때문에",
          "original_form": "최근에 제가 하는 일 때문에",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002794.1.25",
          "form": "이슬람 교인 여자아이를 만나고 있는데",
          "original_form": "이슬람 교인 여자아이를 만나고 있는데",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002794.1.26",
          "form": "이슬람 경전이랑 기독교 경전이 그렇게 내용이 똑같다는걸 처음 알았어요",
          "original_form": "이슬람 경전이랑 기독교 경전이 그렇게 내용이 똑같다는걸 처음 알았어요",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002794.1.27",
          "form": "오오 무슬림",
          "original_form": "오오 무슬림",
          "speaker_id": "1",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002794.1.28",
          "form": "기독교 경전 아니고 성경이",
          "original_form": "기독교 경전 아니고 성경이",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002794.1.29",
          "form": "무함마드 = 하나님 인듯",
          "original_form": "무함마드 = 하나님 인듯",
          "speaker_id": "1",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002794.1.30",
          "form": "하나님이 아니고 예순가",
          "original_form": "하나님이 아니고 예순가",
          "speaker_id": "1",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002794.1.31",
          "form": "제가 잘은 모르지만",
          "original_form": "제가 잘은 모르지만",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002794.1.32",
          "form": "들어봤는데 똑같더라고요",
          "original_form": "들어봤는데 똑같더라고요",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002794.1.33",
          "form": "예루살렘 성지도 그래서 기독교 성지이면서 이슬람성지라더라고요",
          "original_form": "예루살렘 성지도 그래서 기독교 성지이면서 이슬람성지라더라고요",
          "speaker_id": "1",
          "time": "20210820 15:45"
        }
      ]
    }
