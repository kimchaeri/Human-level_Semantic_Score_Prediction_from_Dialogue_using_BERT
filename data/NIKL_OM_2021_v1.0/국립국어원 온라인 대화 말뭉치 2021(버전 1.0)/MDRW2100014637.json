      "utterance": [
        {
          "id": "MDRW2100014637.1.1",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014637.1.2",
          "form": "네 파스타 먹었어요",
          "original_form": "네 파스타 먹었어요",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014637.1.3",
          "form": "어떤파스타요?  토마토,크림,오일. 어떤걸루드셨어요?",
          "original_form": "어떤파스타요?  토마토,크림,오일. 어떤걸루드셨어요?",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014637.1.4",
          "form": "로제 먹었습니당",
          "original_form": "로제 먹었습니당",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014637.1.5",
          "form": "샐러드도 먹었어요",
          "original_form": "샐러드도 먹었어요",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014637.1.6",
          "form": "로제 맛있죠ㅋㅋㅋ",
          "original_form": "로제 맛있죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014637.1.7",
          "form": "파스타에 시판소스넣구 진짜토마토를 추가로넣음",
          "original_form": "파스타에 시판소스넣구 진짜토마토를 추가로넣음",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014637.1.8",
          "form": "더 맛있더라구요",
          "original_form": "더 맛있더라구요",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014637.1.9",
          "form": "와 천재시다!",
          "original_form": "와 천재시다!",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014637.1.10",
          "form": "요리 원래 평소에 하세요?",
          "original_form": "요리 원래 평소에 하세요?",
          "speaker_id": "1",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014637.1.11",
          "form": "ㅋㅋ ㅋ 이정도가 천재인가요? ㅋㅋ",
          "original_form": "ㅋㅋ ㅋ 이정도가 천재인가요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014637.1.12",
          "form": "맛천재신데요?",
          "original_form": "맛천재신데요?",
          "speaker_id": "1",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014637.1.13",
          "form": "많이하진않지만 그래도먹고는 살아야하니ㅋㅋ",
          "original_form": "많이하진않지만 그래도먹고는 살아야하니ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014637.1.14",
          "form": "ㅋㅋ 요리 어느정도는 할 줄 알아야 건강하게 먹고 사는 것 같아요",
          "original_form": "ㅋㅋ 요리 어느정도는 할 줄 알아야 건강하게 먹고 사는 것 같아요",
          "speaker_id": "1",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014637.1.15",
          "form": "맞아요!  할수있으면 더 건강하고 맛있게 먹을수있죠",
          "original_form": "맞아요!  할수있으면 더 건강하고 맛있게 먹을수있죠",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014637.1.16",
          "form": "근데 요즘에는 유투브에서도",
          "original_form": "근데 요즘에는 유투브에서도",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014637.1.17",
          "form": "요리정보가많아서 접근하기쉬운것같아요",
          "original_form": "요리정보가많아서 접근하기쉬운것같아요",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014637.1.18",
          "form": "예전에는 요리책이나 엄마한테 물어봐서 했다면. . ㅋㅋ",
          "original_form": "예전에는 요리책이나 엄마한테 물어봐서 했다면. . ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014637.1.19",
          "form": "ㅋㅋ 저도 블로그로 요리 첫발 떼서 요즘은 유튜브 봐요",
          "original_form": "ㅋㅋ 저도 블로그로 요리 첫발 떼서 요즘은 유튜브 봐요",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014637.1.20",
          "form": "유튜브 레시피 좋더라구요",
          "original_form": "유튜브 레시피 좋더라구요",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014637.1.21",
          "form": "저녁 식사는 하셨어요?",
          "original_form": "저녁 식사는 하셨어요?",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014637.1.22",
          "form": "점심시간 언제세요?",
          "original_form": "점심시간 언제세요?",
          "speaker_id": "2",
          "time": "20210826 11:50"
        }
      ]
