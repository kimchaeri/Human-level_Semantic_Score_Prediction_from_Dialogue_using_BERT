      "utterance": [
        {
          "id": "MDRW2100036694.1.1",
          "form": "오 일리터 양많잖아요 ㅎㅎ",
          "original_form": "오 일리터 양많잖아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 09:07"
        },
        {
          "id": "MDRW2100036694.1.2",
          "form": "흑당은 일리터아니져?",
          "original_form": "흑당은 일리터아니져?",
          "speaker_id": "1",
          "time": "20210831 09:07"
        },
        {
          "id": "MDRW2100036694.1.3",
          "form": "공평하게 일리터입니당ㅎ",
          "original_form": "공평하게 일리터입니당ㅎ",
          "speaker_id": "2",
          "time": "20210831 09:07"
        },
        {
          "id": "MDRW2100036694.1.4",
          "form": "너무 좋아요ㅎㅎ",
          "original_form": "너무 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 09:08"
        },
        {
          "id": "MDRW2100036694.1.5",
          "form": "와 대박.. 가격은요??",
          "original_form": "와 대박.. 가격은요??",
          "speaker_id": "1",
          "time": "20210831 09:08"
        },
        {
          "id": "MDRW2100036694.1.6",
          "form": "6000원이요",
          "original_form": "6000원이요",
          "speaker_id": "2",
          "time": "20210831 09:10"
        },
        {
          "id": "MDRW2100036694.1.7",
          "form": "버블 안 넣으면 5500원",
          "original_form": "버블 안 넣으면 5500원",
          "speaker_id": "2",
          "time": "20210831 09:10"
        },
        {
          "id": "MDRW2100036694.1.8",
          "form": "근데 타피오카펄 빠지면 허전해서 꼭 추가하게 돼요",
          "original_form": "근데 타피오카펄 빠지면 허전해서 꼭 추가하게 돼요",
          "speaker_id": "2",
          "time": "20210831 09:11"
        },
        {
          "id": "MDRW2100036694.1.9",
          "form": "아 양이 많은 대신에 ㅎㅎㅎ 가격이 좀 있네요 ㅎㅎ 아메만 싸구나 ㅎㅎ",
          "original_form": "아 양이 많은 대신에 ㅎㅎㅎ 가격이 좀 있네요 ㅎㅎ 아메만 싸구나 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 09:12"
        },
        {
          "id": "MDRW2100036694.1.10",
          "form": "저가 브랜드인줄 알았는데 ㅎㅎ",
          "original_form": "저가 브랜드인줄 알았는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 09:12"
        },
        {
          "id": "MDRW2100036694.1.11",
          "form": "우유값이 있으니까요ㅎ",
          "original_form": "우유값이 있으니까요ㅎ",
          "speaker_id": "2",
          "time": "20210831 09:13"
        },
        {
          "id": "MDRW2100036694.1.12",
          "form": "아 ㅎㅎ 그쳐 ㅎㅎ 흑당이기도 하고 ㅎㅎ 얼마전에 이디야에서 흑당 먹었는데 작은거라.. 몇번 먹으니 없...ㅎㅎㅎ",
          "original_form": "아 ㅎㅎ 그쳐 ㅎㅎ 흑당이기도 하고 ㅎㅎ 얼마전에 이디야에서 흑당 먹었는데 작은거라.. 몇번 먹으니 없...ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 09:13"
        },
        {
          "id": "MDRW2100036694.1.13",
          "form": "저는 메뉴로 검색하다 취향저격한 거라ㅎ",
          "original_form": "저는 메뉴로 검색하다 취향저격한 거라ㅎ",
          "speaker_id": "2",
          "time": "20210831 09:13"
        },
        {
          "id": "MDRW2100036694.1.14",
          "form": "아쉬워요ㅜ",
          "original_form": "아쉬워요ㅜ",
          "speaker_id": "2",
          "time": "20210831 09:14"
        },
        {
          "id": "MDRW2100036694.1.15",
          "form": "일리터정도되면 먹을거 있겠네요 ㅎㅎ",
          "original_form": "일리터정도되면 먹을거 있겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 09:14"
        },
        {
          "id": "MDRW2100036694.1.16",
          "form": "사이즈도 그렇고 입맛에 맞는데도 잘 없어서ㅜ",
          "original_form": "사이즈도 그렇고 입맛에 맞는데도 잘 없어서ㅜ",
          "speaker_id": "2",
          "time": "20210831 09:14"
        },
        {
          "id": "MDRW2100036694.1.17",
          "form": "젤 유명한 공차는 또 안맞아요ㅎ",
          "original_form": "젤 유명한 공차는 또 안맞아요ㅎ",
          "speaker_id": "2",
          "time": "20210831 09:14"
        },
        {
          "id": "MDRW2100036694.1.18",
          "form": "넹넹 하루종일 마셔요",
          "original_form": "넹넹 하루종일 마셔요",
          "speaker_id": "2",
          "time": "20210831 09:14"
        },
        {
          "id": "MDRW2100036694.1.19",
          "form": "저도 공차 ...안맞더라구요 ㅠㅠ",
          "original_form": "저도 공차 ...안맞더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100036694.1.20",
          "form": "먹다 버렸어요 ㅠㅠ",
          "original_form": "먹다 버렸어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100036694.1.21",
          "form": "공차는 기본이 제일 나은 것 같아요ㅜ",
          "original_form": "공차는 기본이 제일 나은 것 같아요ㅜ",
          "speaker_id": "2",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100036694.1.22",
          "form": "아주 오래전 얘기이지만 ㅎㅎ 공차 나온지 얼마 안됬을때 ㅎㅎ",
          "original_form": "아주 오래전 얘기이지만 ㅎㅎ 공차 나온지 얼마 안됬을때 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100036694.1.23",
          "form": "먼지도 모르고 사람들 다 마트안에서 먹길래 우리도 먹자하면서 시켰는데 이게 뭐지..ㅎㅎ 했던 ㅎ",
          "original_form": "먼지도 모르고 사람들 다 마트안에서 먹길래 우리도 먹자하면서 시켰는데 이게 뭐지..ㅎㅎ 했던 ㅎ",
          "speaker_id": "1",
          "time": "20210831 09:16"
        },
        {
          "id": "MDRW2100036694.1.24",
          "form": "아...처음엔 적응 안가는 맛이죠ㅎㅎ",
