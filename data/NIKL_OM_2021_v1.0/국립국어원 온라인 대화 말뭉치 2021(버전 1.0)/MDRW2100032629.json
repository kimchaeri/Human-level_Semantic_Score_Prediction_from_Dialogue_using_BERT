      "utterance": [
        {
          "id": "MDRW2100032629.1.1",
          "form": "데못죽 이야기 하러 왔어요",
          "original_form": "데못죽 이야기 하러 왔어요",
          "speaker_id": "1",
          "time": "20210905 21:39"
        },
        {
          "id": "MDRW2100032629.1.2",
          "form": "어디까지 봤으?",
          "original_form": "어디까지 봤으?",
          "speaker_id": "1",
          "time": "20210905 21:39"
        },
        {
          "id": "MDRW2100032629.1.3",
          "form": "그때 이후로 못보고 잇는데 지금",
          "original_form": "그때 이후로 못보고 잇는데 지금",
          "speaker_id": "2",
          "time": "20210905 21:42"
        },
        {
          "id": "MDRW2100032629.1.4",
          "form": "기부 콘서트 까지 봄",
          "original_form": "기부 콘서트 까지 봄",
          "speaker_id": "2",
          "time": "20210905 21:42"
        },
        {
          "id": "MDRW2100032629.1.5",
          "form": "으잉? 왜때무네",
          "original_form": "으잉? 왜때무네",
          "speaker_id": "1",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032629.1.6",
          "form": "아직 핑발 문대 구간까지 못 읽음?",
          "original_form": "아직 핑발 문대 구간까지 못 읽음?",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032629.1.7",
          "form": "다른거 보느라 ㅋㅋㅋ",
          "original_form": "다른거 보느라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100032629.1.8",
          "form": "ㅇㅇ.. 아직 못봄..",
          "original_form": "ㅇㅇ.. 아직 못봄..",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032629.1.9",
          "form": "핑발.. 겁나 좋아하는데..",
          "original_form": "핑발.. 겁나 좋아하는데..",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032629.1.10",
          "form": "핑발 ... 그거 때문에 우리 결이랑 구별이 잘 안가잖아 ㅠ",
          "original_form": "핑발 ... 그거 때문에 우리 결이랑 구별이 잘 안가잖아 ㅠ",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100032629.1.11",
          "form": "내스급도 봐야지 말만 시전한지 어연 몇달째",
          "original_form": "내스급도 봐야지 말만 시전한지 어연 몇달째",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032629.1.12",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032629.1.13",
          "form": "왜 결이랑 구분을 못햌ㅋㅋㅋ",
          "original_form": "왜 결이랑 구분을 못햌ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032629.1.14",
          "form": "스급...",
          "original_form": "스급...",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032629.1.15",
          "form": "소장은 다 되어 잇는데 말이지..",
          "original_form": "소장은 다 되어 잇는데 말이지..",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032629.1.16",
          "form": "sd가 많아..",
          "original_form": "sd가 많아..",
          "speaker_id": "1",
          "time": "20210905 22:40"
        },
        {
          "id": "MDRW2100032629.1.17",
          "form": "애기들 세명 나오는 장면까진 봤는데",
          "original_form": "애기들 세명 나오는 장면까진 봤는데",
          "speaker_id": "1",
          "time": "20210905 22:40"
        },
        {
          "id": "MDRW2100032629.1.18",
          "form": "그 이후론 사실 기억이 잘 안나",
          "original_form": "그 이후론 사실 기억이 잘 안나",
          "speaker_id": "1",
          "time": "20210905 22:40"
        },
        {
          "id": "MDRW2100032629.1.19",
          "form": "인어여왕도 만났는데",
          "original_form": "인어여왕도 만났는데",
          "speaker_id": "1",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100032629.1.20",
          "form": "아 스디면.. 그럴만하네..",
          "original_form": "아 스디면.. 그럴만하네..",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100032629.1.21",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100032629.1.22",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100032629.1.23",
          "form": "애들 은근 다 비슷하다고 ㅠ",
          "original_form": "애들 은근 다 비슷하다고 ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100032629.1.24",
          "form": "나름 웹소설 계의 정점에 있는 친구들인데",
          "original_form": "나름 웹소설 계의 정점에 있는 친구들인데",
          "speaker_id": "1",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100032629.1.25",
          "form": "그래도 일단 공식일러는 다 잇는 친구들이긴 한데 ㅋㅋㅋ",
          "original_form": "그래도 일단 공식일러는 다 잇는 친구들이긴 한데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100032629.1.26",
          "form": "ㅇㅇㅇㅇㅇ 좀 짱이지 보는 책들 다 매이져다",
          "original_form": "ㅇㅇㅇㅇㅇ 좀 짱이지 보는 책들 다 매이져다",
          "speaker_id": "1",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100032629.1.27",
          "form": "흫헿 내 장르 다 메이저다 이거야~",
          "original_form": "흫헿 내 장르 다 메이저다 이거야~",
          "speaker_id": "2",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100032629.1.28",
          "form": "이게 마이너 장르를 안먹어봐서 잘 모르는데",
          "original_form": "이게 마이너 장르를 안먹어봐서 잘 모르는데",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100032629.1.29",
          "form": "잡는거 마다 다 대박작품인걸 어떻게해 ㅋㅋㅋ",
          "original_form": "잡는거 마다 다 대박작품인걸 어떻게해 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100032629.1.30",
          "form": "이게 장르 편식인가요..?ㅋㅋㅋㅋ",
          "original_form": "이게 장르 편식인가요..?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032629.1.31",
          "form": "아.. 장르 편식이라기보단",
          "original_form": "아.. 장르 편식이라기보단",
          "speaker_id": "1",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032629.1.32",
          "form": "잘 잡는 금손이지",
          "original_form": "잘 잡는 금손이지",
          "speaker_id": "1",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032629.1.33",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032629.1.34",
          "form": "메이저만 먹는ㅋㅋㅋ",
          "original_form": "메이저만 먹는ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032629.1.35",
          "form": "어쩌다보니 잡는 족족 메이저인걸ㅋㅋㅋ",
          "original_form": "어쩌다보니 잡는 족족 메이저인걸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032629.1.36",
          "form": "ㅇㅇㅇ 내가 잡는 일반 문학 도서도",
          "original_form": "ㅇㅇㅇ 내가 잡는 일반 문학 도서도",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032629.1.37",
          "form": "늦어도 2달 안엔 베스트 셀러니라",
          "original_form": "늦어도 2달 안엔 베스트 셀러니라",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032629.1.38",
          "form": "이야 보는 눈이 잇으시네 ㅋㅋㅋ",
          "original_form": "이야 보는 눈이 잇으시네 ㅋㅋㅋ",
