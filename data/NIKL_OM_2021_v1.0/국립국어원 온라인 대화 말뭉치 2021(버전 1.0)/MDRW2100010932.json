      "utterance": [
        {
          "id": "MDRW2100010932.1.1",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100010932.1.2",
          "form": "너무 잼있죠 ㅎ",
          "original_form": "너무 잼있죠 ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100010932.1.3",
          "form": "저는 본방은 못보고 맨날 재방만보는데 잼나요",
          "original_form": "저는 본방은 못보고 맨날 재방만보는데 잼나요",
          "speaker_id": "1",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100010932.1.4",
          "form": "시즌1부터 너무 재밌어요",
          "original_form": "시즌1부터 너무 재밌어요",
          "speaker_id": "2",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100010932.1.5",
          "form": "글쵸",
          "original_form": "글쵸",
          "speaker_id": "1",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100010932.1.6",
          "form": "어느 의사가 젤 맘에드세요?",
          "original_form": "어느 의사가 젤 맘에드세요?",
          "speaker_id": "1",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100010932.1.7",
          "form": "저도 넷플로 다시보기해요",
          "original_form": "저도 넷플로 다시보기해요",
          "speaker_id": "2",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100010932.1.8",
          "form": "아... 정원이도 익준이도",
          "original_form": "아... 정원이도 익준이도",
          "speaker_id": "2",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100010932.1.9",
          "form": "근데 시즌1이 더 잼나긴하죠?",
          "original_form": "근데 시즌1이 더 잼나긴하죠?",
          "speaker_id": "1",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100010932.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100010932.1.11",
          "form": "시즌2는 조금 지루한감이 없지않아 있어요",
          "original_form": "시즌2는 조금 지루한감이 없지않아 있어요",
          "speaker_id": "1",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100010932.1.12",
          "form": "1은 안정원 떠나는거 때메",
          "original_form": "1은 안정원 떠나는거 때메",
          "speaker_id": "2",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100010932.1.13",
          "form": "뭔가 끝날 것 같은 긴장감이 있었어요",
          "original_form": "뭔가 끝날 것 같은 긴장감이 있었어요",
          "speaker_id": "2",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100010932.1.14",
          "form": "저는 익준이 좋아요 ㅎㅎ",
          "original_form": "저는 익준이 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100010932.1.15",
          "form": "행복해졌음 좋겠어요",
          "original_form": "행복해졌음 좋겠어요",
          "speaker_id": "2",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100010932.1.16",
          "form": "언니들이 그런의사없다고 정신차리라고 그러더라구요 ㅎ",
          "original_form": "언니들이 그런의사없다고 정신차리라고 그러더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100010932.1.17",
          "form": "제 생각에도 슬의 의사드은 슬의에만 있는 것 같아요",
          "original_form": "제 생각에도 슬의 의사드은 슬의에만 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010932.1.18",
          "form": "저는 양석현 추민하 커플 잘됐으면 좋겠어요",
          "original_form": "저는 양석현 추민하 커플 잘됐으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010932.1.19",
          "form": "분량은 적지만 저도 응원해요",
          "original_form": "분량은 적지만 저도 응원해요",
          "speaker_id": "2",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010932.1.20",
          "form": "민하샘 성과 얻기를",
          "original_form": "민하샘 성과 얻기를",
          "speaker_id": "2",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010932.1.21",
          "form": "추민하 넘 귀여워요",
          "original_form": "추민하 넘 귀여워요",
          "speaker_id": "1",
          "time": "20210823 23:11"
        },
        {
          "id": "MDRW2100010932.1.22",
          "form": "석현모는 쉽지 않은 사람이지만",
          "original_form": "석현모는 쉽지 않은 사람이지만",
          "speaker_id": "2",
          "time": "20210823 23:11"
        },
        {
          "id": "MDRW2100010932.1.23",
          "form": "양석현쌤 겪은 아픔 추민하의 밝음으로 치유됐으면 좋겠어요",
          "original_form": "양석현쌤 겪은 아픔 추민하의 밝음으로 치유됐으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210823 23:11"
        },
        {
          "id": "MDRW2100010932.1.24",
          "form": "저정도의 순수함이라면 극복가능",
          "original_form": "저정도의 순수함이라면 극복가능",
          "speaker_id": "2",
          "time": "20210823 23:11"
        },
        {
          "id": "MDRW2100010932.1.25",
          "form": "석현 어머니도 추민하의 귀여운모습보면 그냥 녹을듯요",
          "original_form": "석현 어머니도 추민하의 귀여운모습보면 그냥 녹을듯요",
          "speaker_id": "1",
          "time": "20210823 23:13"
        }
      ]
    }
