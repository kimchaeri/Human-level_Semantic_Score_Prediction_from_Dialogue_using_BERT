        },
        {
          "id": "MDRW2100032406.1.2",
          "form": "요즘 사신 물건 있으세용?",
          "original_form": "요즘 사신 물건 있으세용?",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032406.1.3",
          "form": "최근에 무선청소기요",
          "original_form": "최근에 무선청소기요",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032406.1.4",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032406.1.5",
          "form": "우와 무선청소기 좋네요",
          "original_form": "우와 무선청소기 좋네요",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032406.1.6",
          "form": "저 이사했어서 침대용ㅋㅋ",
          "original_form": "저 이사했어서 침대용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032406.1.7",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032406.1.8",
          "form": "저도 매트보구있어요",
          "original_form": "저도 매트보구있어요",
          "speaker_id": "2",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032406.1.9",
          "form": "친구들 놀러온다구 해서",
          "original_form": "친구들 놀러온다구 해서",
          "speaker_id": "2",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032406.1.10",
          "form": "오 무슨 매트 사세요?",
          "original_form": "오 무슨 매트 사세요?",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032406.1.11",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032406.1.12",
          "form": "센스맘보구있는데",
          "original_form": "센스맘보구있는데",
          "speaker_id": "2",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032406.1.13",
          "form": "라텍스 사고싶당 이런느낌",
          "original_form": "라텍스 사고싶당 이런느낌",
          "speaker_id": "2",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100032406.1.14",
          "form": "요즘 라텍스 많이들 하시더라구요",
          "original_form": "요즘 라텍스 많이들 하시더라구요",
          "speaker_id": "1",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100032406.1.15",
          "form": "탄력성이 좋은거 같아요!",
          "original_form": "탄력성이 좋은거 같아요!",
          "speaker_id": "1",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100032406.1.16",
          "form": "저도 제주온지 3주인데",
          "original_form": "저도 제주온지 3주인데",
          "speaker_id": "2",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100032406.1.17",
          "form": "집 꾸미는 거 많이 보구있어요",
          "original_form": "집 꾸미는 거 많이 보구있어요",
          "speaker_id": "2",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100032406.1.18",
          "form": "1인쇼파나 스탠드 조명이 관심사예여",
          "original_form": "1인쇼파나 스탠드 조명이 관심사예여",
          "speaker_id": "2",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100032406.1.19",
          "form": "우와 제주도라니 대박이네요",
          "original_form": "우와 제주도라니 대박이네요",
          "speaker_id": "1",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032406.1.20",
          "form": "넘넘 부럽습니다ㅠ",
          "original_form": "넘넘 부럽습니다ㅠ",
          "speaker_id": "1",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032406.1.21",
          "form": "다 사람사는곳인데여^^",
          "original_form": "다 사람사는곳인데여^^",
          "speaker_id": "2",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032406.1.22",
          "form": "물건 사실때 주로 어떤곳에서 정보 구하세요",
          "original_form": "물건 사실때 주로 어떤곳에서 정보 구하세요",
          "speaker_id": "2",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100032406.1.23",
          "form": "저는 그냥 오프라인 마트 가서 사요!",
          "original_form": "저는 그냥 오프라인 마트 가서 사요!",
          "speaker_id": "1",
          "time": "20210905 17:15"
        },
        {
          "id": "MDRW2100032406.1.24",
          "form": "직접 보구 하는게 좋더라구용",
          "original_form": "직접 보구 하는게 좋더라구용",
          "speaker_id": "1",
          "time": "20210905 17:16"
        },
        {
          "id": "MDRW2100032406.1.25",
          "form": "오 맞아요 실제랑 다른 경우가 많더라구요",
          "original_form": "오 맞아요 실제랑 다른 경우가 많더라구요",
          "speaker_id": "2",
          "time": "20210905 17:48"
        },
        {
          "id": "MDRW2100032406.1.26",
          "form": "전 요즘 오늘의집 많이 봤어요",
          "original_form": "전 요즘 오늘의집 많이 봤어요",
          "speaker_id": "2",
          "time": "20210905 17:48"
        },
        {
          "id": "MDRW2100032406.1.27",
          "form": "아 오늘의 집!!! 알아요!!",
          "original_form": "아 오늘의 집!!! 알아요!!",
          "speaker_id": "1",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100032406.1.28",
          "form": "오늘의 집 보면서 커튼도 화이트로 바꿨어요 ㅋㅋㅋ",
