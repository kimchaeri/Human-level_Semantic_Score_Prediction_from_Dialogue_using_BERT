      "utterance": [
        {
          "id": "MDRW2100033945.1.1",
          "form": "그렇구나. 전 런닝맨 자주 봐요",
          "original_form": "그렇구나. 전 런닝맨 자주 봐요",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100033945.1.2",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100033945.1.3",
          "form": "광수가 나갔죠?",
          "original_form": "광수가 나갔죠?",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100033945.1.4",
          "form": "네 아쉽긴 한데 그래도 재밌어요",
          "original_form": "네 아쉽긴 한데 그래도 재밌어요",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100033945.1.5",
          "form": "오우",
          "original_form": "오우",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100033945.1.6",
          "form": "추가멤버는 없나요?",
          "original_form": "추가멤버는 없나요?",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100033945.1.7",
          "form": "아직은요.",
          "original_form": "아직은요.",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033945.1.8",
          "form": "보는 다른 프로그램 있나요?",
          "original_form": "보는 다른 프로그램 있나요?",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033945.1.9",
          "form": "골그녀를",
          "original_form": "골그녀를",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100033945.1.10",
          "form": "본답니다ㅋㅋㅋ",
          "original_form": "본답니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100033945.1.11",
          "form": "축구 하는 거죠? 모델팀이 시합하는거 봤어요",
          "original_form": "축구 하는 거죠? 모델팀이 시합하는거 봤어요",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100033945.1.12",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100033945.1.13",
          "form": "너무멋있어요",
          "original_form": "너무멋있어요",
          "speaker_id": "1",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100033945.1.14",
          "form": "요새 스포츠 예능이 많아졌잖아요",
          "original_form": "요새 스포츠 예능이 많아졌잖아요",
          "speaker_id": "2",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100033945.1.15",
          "form": "골프 예능도 채널마다 하더라구요",
          "original_form": "골프 예능도 채널마다 하더라구요",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100033945.1.16",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100033945.1.17",
          "form": "골프예능이 있나요?",
          "original_form": "골프예능이 있나요?",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100033945.1.18",
          "form": "네 늦게 하는 거 같긴 해요",
          "original_form": "네 늦게 하는 거 같긴 해요",
          "speaker_id": "2",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100033945.1.19",
          "form": "재밌겠네요ㅋㅋㅋ",
          "original_form": "재밌겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100033945.1.20",
          "form": "초심자들이라 그냥 공 치는것도 웃기더라구요",
          "original_form": "초심자들이라 그냥 공 치는것도 웃기더라구요",
          "speaker_id": "2",
          "time": "20210825 20:23"
        }
      ]
