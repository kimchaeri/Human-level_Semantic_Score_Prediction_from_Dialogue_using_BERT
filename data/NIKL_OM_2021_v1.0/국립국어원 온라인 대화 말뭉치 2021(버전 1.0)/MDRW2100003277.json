      "utterance": [
        {
          "id": "MDRW2100003277.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 21:20"
        },
        {
          "id": "MDRW2100003277.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 21:21"
        },
        {
          "id": "MDRW2100003277.1.3",
          "form": "혹시 해외여행 가보셨어요?",
          "original_form": "혹시 해외여행 가보셨어요?",
          "speaker_id": "1",
          "time": "20210820 21:22"
        },
        {
          "id": "MDRW2100003277.1.4",
          "form": "넵 대만 좋아해요",
          "original_form": "넵 대만 좋아해요",
          "speaker_id": "2",
          "time": "20210820 21:22"
        },
        {
          "id": "MDRW2100003277.1.5",
          "form": "대만 어디가보셨어요?",
          "original_form": "대만 어디가보셨어요?",
          "speaker_id": "1",
          "time": "20210820 21:25"
        },
        {
          "id": "MDRW2100003277.1.6",
          "form": "타이페이에 주로 있었어요",
          "original_form": "타이페이에 주로 있었어요",
          "speaker_id": "2",
          "time": "20210820 21:25"
        },
        {
          "id": "MDRW2100003277.1.7",
          "form": "타이페이 어디가 좋으셨어요?",
          "original_form": "타이페이 어디가 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210820 21:26"
        },
        {
          "id": "MDRW2100003277.1.8",
          "form": "박물관 아세요?",
          "original_form": "박물관 아세요?",
          "speaker_id": "2",
          "time": "20210820 21:27"
        },
        {
          "id": "MDRW2100003277.1.9",
          "form": "그 장개석이 도망올때 배를 격추시키지 못한 이유가",
          "original_form": "그 장개석이 도망올때 배를 격추시키지 못한 이유가",
          "speaker_id": "2",
          "time": "20210820 21:27"
        },
        {
          "id": "MDRW2100003277.1.10",
          "form": "보물때문이라는 설도 있거든요",
          "original_form": "보물때문이라는 설도 있거든요",
          "speaker_id": "2",
          "time": "20210820 21:27"
        },
        {
          "id": "MDRW2100003277.1.11",
          "form": "아뇨 장개석도 처음들어보네요...",
          "original_form": "아뇨 장개석도 처음들어보네요...",
          "speaker_id": "1",
          "time": "20210820 21:30"
        },
        {
          "id": "MDRW2100003277.1.12",
          "form": "거기가면 분위기는 어때요?",
          "original_form": "거기가면 분위기는 어때요?",
          "speaker_id": "1",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003277.1.13",
          "form": "박물관이 엄청 크거든요",
          "original_form": "박물관이 엄청 크거든요",
          "speaker_id": "2",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003277.1.14",
          "form": "분위기가 되게 웅장하고",
          "original_form": "분위기가 되게 웅장하고",
          "speaker_id": "2",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003277.1.15",
          "form": "보물을 어디 산속에 보관한대요",
          "original_form": "보물을 어디 산속에 보관한대요",
          "speaker_id": "2",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003277.1.16",
          "form": "박물관에서 보물을 몇개월에 한번씩 바꿔주는데",
          "original_form": "박물관에서 보물을 몇개월에 한번씩 바꿔주는데",
          "speaker_id": "2",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003277.1.17",
          "form": "그거 다 보려면 몇십년이? 걸린다고 그러더라구여",
          "original_form": "그거 다 보려면 몇십년이? 걸린다고 그러더라구여",
          "speaker_id": "2",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003277.1.18",
          "form": "보물은 어떤보물있어요?",
          "original_form": "보물은 어떤보물있어요?",
          "speaker_id": "1",
          "time": "20210820 21:35"
        },
        {
          "id": "MDRW2100003277.1.19",
          "form": "황금이나 그런건가?",
          "original_form": "황금이나 그런건가?",
          "speaker_id": "1",
          "time": "20210820 21:35"
        },
        {
          "id": "MDRW2100003277.1.20",
          "form": "그 삼겹살 돌 아세요?",
          "original_form": "그 삼겹살 돌 아세요?",
          "speaker_id": "2",
          "time": "20210820 21:35"
        },
        {
          "id": "MDRW2100003277.1.21",
          "form": "돌인데 진짜 삼겹살처럼 생겼어요",
          "original_form": "돌인데 진짜 삼겹살처럼 생겼어요",
          "speaker_id": "2",
          "time": "20210820 21:35"
        },
        {
          "id": "MDRW2100003277.1.22",
          "form": "돌이 되게 재밌는게 많은거같아요",
          "original_form": "돌이 되게 재밌는게 많은거같아요",
          "speaker_id": "1",
          "time": "20210820 21:39"
        }
      ]
