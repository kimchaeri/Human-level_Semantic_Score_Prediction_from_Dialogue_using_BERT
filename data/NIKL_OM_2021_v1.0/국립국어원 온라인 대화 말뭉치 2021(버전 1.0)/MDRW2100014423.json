      "utterance": [
        {
          "id": "MDRW2100014423.1.1",
          "form": "중고 거래",
          "original_form": "중고 거래",
          "speaker_id": "2",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100014423.1.2",
          "form": "덩근에서 했는데",
          "original_form": "덩근에서 했는데",
          "speaker_id": "2",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100014423.1.3",
          "form": "그래서 놔두면 더 X될까봐",
          "original_form": "그래서 놔두면 더 X될까봐",
          "speaker_id": "1",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100014423.1.4",
          "form": "막 꺽아달라시니 ㅜㅜ",
          "original_form": "막 꺽아달라시니 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100014423.1.5",
          "form": "오늘 일부 털었어요",
          "original_form": "오늘 일부 털었어요",
          "speaker_id": "1",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100014423.1.6",
          "form": "헐값에 팔았네요",
          "original_form": "헐값에 팔았네요",
          "speaker_id": "2",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100014423.1.7",
          "form": "깍아주셨어요?",
          "original_form": "깍아주셨어요?",
          "speaker_id": "1",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014423.1.8",
          "form": "잘했어요",
          "original_form": "잘했어요",
          "speaker_id": "2",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014423.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014423.1.10",
          "form": "좀심하게요 ㅜㅜ",
          "original_form": "좀심하게요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014423.1.11",
          "form": "허무하네요",
          "original_form": "허무하네요",
          "speaker_id": "2",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014423.1.12",
          "form": "올리신지 오래된건가요?",
          "original_form": "올리신지 오래된건가요?",
          "speaker_id": "1",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014423.1.13",
          "form": "아뇨 ㅋㅋ",
          "original_form": "아뇨 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014423.1.14",
          "form": "저는 그럼 안팔아요",
          "original_form": "저는 그럼 안팔아요",
          "speaker_id": "1",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014423.1.15",
          "form": "바로 팔렸어요",
          "original_form": "바로 팔렸어요",
          "speaker_id": "2",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014423.1.16",
          "form": "그럼 그냥 둬보시징...",
          "original_form": "그럼 그냥 둬보시징...",
          "speaker_id": "1",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014423.1.17",
          "form": "그래야겠어요",
          "original_form": "그래야겠어요",
          "speaker_id": "2",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014423.1.18",
          "form": "단호하게",
          "original_form": "단호하게",
          "speaker_id": "1",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014423.1.19",
          "form": "제가 좀 ㅇㅅ령이 없어요",
          "original_form": "제가 좀 ㅇㅅ령이 없어요",
          "speaker_id": "2",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014423.1.20",
          "form": "요령이",
          "original_form": "요령이",
          "speaker_id": "2",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014423.1.21",
          "form": "죄송합니다로 끝내요 ㅎ",
          "original_form": "죄송합니다로 끝내요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014423.1.22",
          "form": "그러고 싶네요 ㅋㅋ",
          "original_form": "그러고 싶네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014423.1.23",
          "form": "실은 저도 당근거래하고 들어가는길이요 ㅋ",
          "original_form": "실은 저도 당근거래하고 들어가는길이요 ㅋ",
          "speaker_id": "1",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100014423.1.24",
          "form": "장남감이라 잘팔렸어요 ㅎㅎ",
          "original_form": "장남감이라 잘팔렸어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100014423.1.25",
          "form": "오 뭐샀어요?",
          "original_form": "오 뭐샀어요?",
          "speaker_id": "2",
          "time": "20210825 18:58"
        }
      ]
