      "utterance": [
        {
          "id": "MDRW2100019655.1.1",
          "form": "지금 직장인이세요?",
          "original_form": "지금 직장인이세요?",
          "speaker_id": "1",
          "time": "20210827 20:59"
        },
        {
          "id": "MDRW2100019655.1.2",
          "form": "아니요..",
          "original_form": "아니요..",
          "speaker_id": "2",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100019655.1.3",
          "form": "취준중이에요ㅠㅠ",
          "original_form": "취준중이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100019655.1.4",
          "form": "아이쿠!",
          "original_form": "아이쿠!",
          "speaker_id": "1",
          "time": "20210827 21:03"
        },
        {
          "id": "MDRW2100019655.1.5",
          "form": "그럼 인턴이나 알바해본 경험 있어요?",
          "original_form": "그럼 인턴이나 알바해본 경험 있어요?",
          "speaker_id": "1",
          "time": "20210827 21:04"
        },
        {
          "id": "MDRW2100019655.1.6",
          "form": "네 알바 해봤어요",
          "original_form": "네 알바 해봤어요",
          "speaker_id": "2",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019655.1.7",
          "form": "어떤 알바 해보셨어요~~?",
          "original_form": "어떤 알바 해보셨어요~~?",
          "speaker_id": "1",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100019655.1.8",
          "form": "박람회 아르바이트요!",
          "original_form": "박람회 아르바이트요!",
          "speaker_id": "2",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100019655.1.9",
          "form": "처음 들어봤는데 소개해줄 수 있어요?",
          "original_form": "처음 들어봤는데 소개해줄 수 있어요?",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100019655.1.10",
          "form": "그냥 박람회에 오는 고객들에게 물건 팔고 그런거에요ㅋㅋ",
          "original_form": "그냥 박람회에 오는 고객들에게 물건 팔고 그런거에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:27"
        },
        {
          "id": "MDRW2100019655.1.11",
          "form": "단기 알바라서 급하게 돈벌때 좋아요",
          "original_form": "단기 알바라서 급하게 돈벌때 좋아요",
          "speaker_id": "2",
          "time": "20210827 21:27"
        },
        {
          "id": "MDRW2100019655.1.12",
          "form": "짧게 3-4일정도 하고 바로 받으니까요",
          "original_form": "짧게 3-4일정도 하고 바로 받으니까요",
          "speaker_id": "2",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100019655.1.13",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019655.1.14",
          "form": "그럼 물건 많이 팔수록 인센티브도 줘요?",
          "original_form": "그럼 물건 많이 팔수록 인센티브도 줘요?",
          "speaker_id": "1",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019655.1.15",
          "form": "그런건 잘없고 딱 한번 받아봤어요!",
          "original_form": "그런건 잘없고 딱 한번 받아봤어요!",
          "speaker_id": "2",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100019655.1.16",
          "form": "아~~그럼 막 열심히 안팔고 자리만 지켜도 돼요?",
          "original_form": "아~~그럼 막 열심히 안팔고 자리만 지켜도 돼요?",
          "speaker_id": "1",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019655.1.17",
          "form": "그럼 안돼죠ㅋㅋㅋㅋ 열심히는 해야해요",
          "original_form": "그럼 안돼죠ㅋㅋㅋㅋ 열심히는 해야해요",
          "speaker_id": "2",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100019655.1.18",
          "form": "근데 또 잘팔리는 종류가 있어서 안팔리는 종류면 파리만 날려요...ㅋㅋ",
          "original_form": "근데 또 잘팔리는 종류가 있어서 안팔리는 종류면 파리만 날려요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019655.1.19",
          "form": "되게 사명감 있어서 기업들이 탐낼 인재이실 거 같아요!",
          "original_form": "되게 사명감 있어서 기업들이 탐낼 인재이실 거 같아요!",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019655.1.20",
          "form": "무슨 일자리 알아보는 중이에요?",
          "original_form": "무슨 일자리 알아보는 중이에요?",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019655.1.21",
          "form": "아니에여ㅋㅋㅋㅋ",
          "original_form": "아니에여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019655.1.22",
          "form": "저는 상품기획이나 마케팅 이런쪽으로 알아보고 있어요!",
          "original_form": "저는 상품기획이나 마케팅 이런쪽으로 알아보고 있어요!",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019655.1.23",
          "form": "직장인이신가요??",
          "original_form": "직장인이신가요??",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019655.1.24",
          "form": "마인드가 좋으셔서 되게 잘하실 것 같아요!",
          "original_form": "마인드가 좋으셔서 되게 잘하실 것 같아요!",
          "speaker_id": "1",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019655.1.25",
          "form": "네 저 직장다니구 있어용 ㅎㅎ 오늘 칼퇴했죠",
          "original_form": "네 저 직장다니구 있어용 ㅎㅎ 오늘 칼퇴했죠",
