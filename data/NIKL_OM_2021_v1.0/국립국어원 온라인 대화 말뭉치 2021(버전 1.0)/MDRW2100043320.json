        },
        {
          "id": "MDRW2100043320.1.3",
          "form": "너연근싫다햇지",
          "original_form": "너연근싫다햇지",
          "speaker_id": "2",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100043320.1.4",
          "form": "시뤄...",
          "original_form": "시뤄...",
          "speaker_id": "1",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100043320.1.5",
          "form": "ㅋㅋㅋㅋ연근 근데 몸에 좋대",
          "original_form": "ㅋㅋㅋㅋ연근 근데 몸에 좋대",
          "speaker_id": "1",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100043320.1.6",
          "form": "먹어!!!",
          "original_form": "먹어!!!",
          "speaker_id": "1",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100043320.1.7",
          "form": "연근샐러드",
          "original_form": "연근샐러드",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100043320.1.8",
          "form": "참깨갈아서 만든 드레싱에",
          "original_form": "참깨갈아서 만든 드레싱에",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100043320.1.9",
          "form": "무친건데",
          "original_form": "무친건데",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100043320.1.10",
          "form": "진짜 ㅋㅋㅋㅋ",
          "original_form": "진짜 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100043320.1.11",
          "form": "ㅋㅋㅋ흠 드레싱 맛으로 먹어야지..",
          "original_form": "ㅋㅋㅋ흠 드레싱 맛으로 먹어야지..",
          "speaker_id": "1",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100043320.1.12",
          "form": "별미얌 ㅋㅋㅋ",
          "original_form": "별미얌 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100043320.1.13",
          "form": "ㅋㅋㅋ건강한 맛이겠균",
          "original_form": "ㅋㅋㅋ건강한 맛이겠균",
          "speaker_id": "1",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100043320.1.14",
          "form": "나진짜 1년넘게 아에 생각도안햇는데",
          "original_form": "나진짜 1년넘게 아에 생각도안햇는데",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100043320.1.15",
          "form": "한번먹곸 ㅋㅋㅋㅋ막손님한테 추천",
          "original_form": "한번먹곸 ㅋㅋㅋㅋ막손님한테 추천",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100043320.1.16",
          "form": "내추천에 먹엇던사람들은",
          "original_form": "내추천에 먹엇던사람들은",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100043320.1.17",
          "form": "나오면 꼭사머금 ㅋㅋㅋ",
          "original_form": "나오면 꼭사머금 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100043320.1.18",
          "form": "진짜 입맛두변해",
          "original_form": "진짜 입맛두변해",
          "speaker_id": "2",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100043320.1.19",
          "form": "ㅋㅋㅋㅋ오 대박인데 추천까지 할 정도라니!!!",
          "original_form": "ㅋㅋㅋㅋ오 대박인데 추천까지 할 정도라니!!!",
          "speaker_id": "1",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100043320.1.20",
          "form": "ㅋㅋㅋ응 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ응 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100043320.1.21",
          "form": "ㅋㅋㅋ난 엄마가 오이랑 양배추로 샐러드해준대",
          "original_form": "ㅋㅋㅋ난 엄마가 오이랑 양배추로 샐러드해준대",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100043320.1.22",
          "form": "맛잇겟다",
          "original_form": "맛잇겟다",
          "speaker_id": "2",
          "time": "20210907 17:44"
        },
        {
          "id": "MDRW2100043320.1.23",
          "form": "새콤하게 ?",
          "original_form": "새콤하게 ?",
          "speaker_id": "2",
          "time": "20210907 17:44"
        },
        {
          "id": "MDRW2100043320.1.24",
          "form": "ㅋㅋㅋ드레싱이 음슴..",
          "original_form": "ㅋㅋㅋ드레싱이 음슴..",
          "speaker_id": "1",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100043320.1.25",
          "form": "ㅋㅋㅋㅋ마요네즈에 케챱 섞어볼까나",
          "original_form": "ㅋㅋㅋㅋ마요네즈에 케챱 섞어볼까나",
          "speaker_id": "1",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100043320.1.26",
          "form": "네에?",
          "original_form": "네에?",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100043320.1.27",
          "form": "안되쥬",
          "original_form": "안되쥬",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100043320.1.28",
          "form": "ㅋㅋㅋ그 불닭소스는 더 안되겠쥬?",
          "original_form": "ㅋㅋㅋ그 불닭소스는 더 안되겠쥬?",
          "speaker_id": "1",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043320.1.29",
          "form": "화끈한 샐러드가 되겟구만",
          "original_form": "화끈한 샐러드가 되겟구만",
          "speaker_id": "2",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100043320.1.30",
          "form": "난 양배추 오리엔탈에 먹는거 맛잇더라 ㅋㅋㅋ",
          "original_form": "난 양배추 오리엔탈에 먹는거 맛잇더라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100043320.1.31",
          "form": "파스쿠치 조각케익한조각에 6천원이넘네",
          "original_form": "파스쿠치 조각케익한조각에 6천원이넘네",
          "speaker_id": "2",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100043320.1.32",
          "form": "ㅋㅋㅋ엄청비싸네 호엥...",
          "original_form": "ㅋㅋㅋ엄청비싸네 호엥...",
          "speaker_id": "1",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043320.1.33",
          "form": "샐러드 꾸역꾸역 다 먹음ㅋㅋㅋ첨엔 맛났는데",
          "original_form": "샐러드 꾸역꾸역 다 먹음ㅋㅋㅋ첨엔 맛났는데",
          "speaker_id": "1",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043320.1.34",
          "form": "상추대신이여!",
          "original_form": "상추대신이여!",
          "speaker_id": "2",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043320.1.35",
          "form": "봄봄 아메 5잔에 4천원 파스쿠치 아메2잔에 3천원",
          "original_form": "봄봄 아메 5잔에 4천원 파스쿠치 아메2잔에 3천원",
          "speaker_id": "2",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100043320.1.36",
          "form": "ㅋㅋㅋ헐 왜캐 싸?",
          "original_form": "ㅋㅋㅋ헐 왜캐 싸?",
          "speaker_id": "1",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043320.1.37",
          "form": "아니 ㅋㅋㅋㅋ",
          "original_form": "아니 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043320.1.38",
          "form": "내가 요기요 슈클해서 저렇게 나오는데",
          "original_form": "내가 요기요 슈클해서 저렇게 나오는데",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043320.1.39",
          "form": "너라면 어디 커피집을 가겟어?",
          "original_form": "너라면 어디 커피집을 가겟어?",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043320.1.40",
          "form": "참고도 ㅋㅋㅋㅋ 파스쿠치가 좀더 멀어 둘중에 ㅋㅋ",
          "original_form": "참고도 ㅋㅋㅋㅋ 파스쿠치가 좀더 멀어 둘중에 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043320.1.41",
          "form": "ㅋㅋㅋ와 근데 5잔 사서 언제 다마셔..?",
