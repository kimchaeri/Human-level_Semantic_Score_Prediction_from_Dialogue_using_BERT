      "utterance": [
        {
          "id": "MDRW2100043865.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043865.1.2",
          "form": "안녕하세요~!",
          "original_form": "안녕하세요~!",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043865.1.3",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043865.1.4",
          "form": "운동 요새는 산책밖에 잘 못하죠 ㅠ 좋아하는 운동은 많아요",
          "original_form": "운동 요새는 산책밖에 잘 못하죠 ㅠ 좋아하는 운동은 많아요",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043865.1.5",
          "form": "오 정말요?",
          "original_form": "오 정말요?",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043865.1.6",
          "form": "어떤 운동 좋아하세요?",
          "original_form": "어떤 운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043865.1.7",
          "form": "저 탁구도 좋고 배드민턴이나 배구도 좋아해요",
          "original_form": "저 탁구도 좋고 배드민턴이나 배구도 좋아해요",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043865.1.8",
          "form": "오 구기종목을 좋아하시네요",
          "original_form": "오 구기종목을 좋아하시네요",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043865.1.9",
          "form": "전 유산소 운동을 좋아해요",
          "original_form": "전 유산소 운동을 좋아해요",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043865.1.10",
          "form": "숨쉬기운동",
          "original_form": "숨쉬기운동",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043865.1.11",
          "form": "ㅋㅋㅋㅋㅋ 하지만 숨쉬기 운동은 다이어트하기가 힘들더라고요 저도 좋아합니다 짱짱",
          "original_form": "ㅋㅋㅋㅋㅋ 하지만 숨쉬기 운동은 다이어트하기가 힘들더라고요 저도 좋아합니다 짱짱",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043865.1.12",
          "form": "맞아요....",
          "original_form": "맞아요....",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043865.1.13",
          "form": "다이어트하고 계세요?",
          "original_form": "다이어트하고 계세요?",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043865.1.14",
          "form": "말로만 하고 몸으로는 실천안해욬ㅋㅋ",
          "original_form": "말로만 하고 몸으로는 실천안해욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043865.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043865.1.16",
          "form": "다이어트는",
          "original_form": "다이어트는",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043865.1.17",
          "form": "식이 90 운동 10이라던데",
          "original_form": "식이 90 운동 10이라던데",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043865.1.18",
          "form": "전 식이부터 망했어요",
          "original_form": "전 식이부터 망했어요",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043865.1.19",
          "form": "식이는 누구나 다 실패해요 ㅋㅋㅋㅋㅋㅋㅋㅋ 저도 오늘 배부르게 먹었는걸요 운동도 비가 쭈륵쭈륵 오는지라 못했어요!",
          "original_form": "식이는 누구나 다 실패해요 ㅋㅋㅋㅋㅋㅋㅋㅋ 저도 오늘 배부르게 먹었는걸요 운동도 비가 쭈륵쭈륵 오는지라 못했어요!",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043865.1.20",
          "form": "하지만 식이를 실패하면",
          "original_form": "하지만 식이를 실패하면",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043865.1.21",
          "form": "다이어트도 포기한 것...!",
          "original_form": "다이어트도 포기한 것...!",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043865.1.22",
          "form": "아아 ㅋ 건강하게만 쭉쭉 자라면 되겠죠 어떻게든",
          "original_form": "아아 ㅋ 건강하게만 쭉쭉 자라면 되겠죠 어떻게든",
          "speaker_id": "2",
          "time": "20210907 20:54"
        }
      ]
    }
