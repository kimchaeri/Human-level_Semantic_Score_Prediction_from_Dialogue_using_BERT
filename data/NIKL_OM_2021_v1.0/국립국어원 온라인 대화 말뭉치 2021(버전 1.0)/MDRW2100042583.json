      "utterance": [
        {
          "id": "MDRW2100042583.1.1",
          "form": "오늘 저녁 맛있게 먹었니",
          "original_form": "오늘 저녁 맛있게 먹었니",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100042583.1.2",
          "form": "응 뭐 먹었는지 맞춰볼래?",
          "original_form": "응 뭐 먹었는지 맞춰볼래?",
          "speaker_id": "1",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100042583.1.3",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100042583.1.4",
          "form": "오늘도 매운거니",
          "original_form": "오늘도 매운거니",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100042583.1.5",
          "form": "응 오늘은 좀 매웠어",
          "original_form": "응 오늘은 좀 매웠어",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100042583.1.6",
          "form": "허허 위장이 신나서 탭댄스추겠네",
          "original_form": "허허 위장이 신나서 탭댄스추겠네",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100042583.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ고춧가루를 많이 넣어서 그렇지 원래 별로 매운 음식이야",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ고춧가루를 많이 넣어서 그렇지 원래 별로 매운 음식이야",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100042583.1.8",
          "form": "너도 잘 먹는 음식",
          "original_form": "너도 잘 먹는 음식",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100042583.1.9",
          "form": "원래 별로 매운음식은 뭐야",
          "original_form": "원래 별로 매운음식은 뭐야",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100042583.1.10",
          "form": "그러게 안 매운 음식이라고 보내려고 했는데 ㅋㅋㅋㅋ웃기네",
          "original_form": "그러게 안 매운 음식이라고 보내려고 했는데 ㅋㅋㅋㅋ웃기네",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100042583.1.11",
          "form": "너도 거뜬히 먹을 만한 맵기의 요리야",
          "original_form": "너도 거뜬히 먹을 만한 맵기의 요리야",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100042583.1.12",
          "form": "흠 빨간양념이니",
          "original_form": "흠 빨간양념이니",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100042583.1.13",
          "form": "응 빨간 양념 요리야",
          "original_form": "응 빨간 양념 요리야",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100042583.1.14",
          "form": "생선이 들어가니",
          "original_form": "생선이 들어가니",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100042583.1.15",
          "form": "아니요 생선 안 들어가!",
          "original_form": "아니요 생선 안 들어가!",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100042583.1.16",
          "form": "고기가 들어가니",
          "original_form": "고기가 들어가니",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100042583.1.17",
          "form": "응 고기 들어가\n오늘은 진도가 빠른걸?",
          "original_form": "응 고기 들어가\n오늘은 진도가 빠른걸?",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100042583.1.18",
          "form": "예리해",
          "original_form": "예리해",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100042583.1.19",
          "form": "김치가 들어가니",
          "original_form": "김치가 들어가니",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100042583.1.20",
          "form": "헉 응 너무 결정적인 힌트다",
          "original_form": "헉 응 너무 결정적인 힌트다",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100042583.1.21",
          "form": "김치찌개구나",
          "original_form": "김치찌개구나",
          "speaker_id": "2",
          "time": "20210907 19:37"
        }
      ]
