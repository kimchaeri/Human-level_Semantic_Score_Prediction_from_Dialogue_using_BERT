        },
        {
          "id": "MDRW2100033944.1.2",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100033944.1.3",
          "form": "운동 말고 건강 챙기는 거 있어?",
          "original_form": "운동 말고 건강 챙기는 거 있어?",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100033944.1.4",
          "form": "비타민 챙겨 먹을 예정",
          "original_form": "비타민 챙겨 먹을 예정",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100033944.1.5",
          "form": "난 유산균이랑 비타민 먹어",
          "original_form": "난 유산균이랑 비타민 먹어",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100033944.1.6",
          "form": "유산균 좋아?",
          "original_form": "유산균 좋아?",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100033944.1.7",
          "form": "응 괜찮아",
          "original_form": "응 괜찮아",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100033944.1.8",
          "form": "효과 좋은 듯",
          "original_form": "효과 좋은 듯",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100033944.1.9",
          "form": "어디 꺼 먹는데?",
          "original_form": "어디 꺼 먹는데?",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100033944.1.10",
          "form": "프로스택인가 거기서 샀어",
          "original_form": "프로스택인가 거기서 샀어",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100033944.1.11",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100033944.1.12",
          "form": "비타민은 어떤 거 먹어?",
          "original_form": "비타민은 어떤 거 먹어?",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100033944.1.13",
          "form": "그냥 종합비타민 먹어",
          "original_form": "그냥 종합비타민 먹어",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100033944.1.14",
          "form": "너도 빨리 사서 먹어",
          "original_form": "너도 빨리 사서 먹어",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100033944.1.15",
          "form": "그래야지",
          "original_form": "그래야지",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100033944.1.16",
          "form": "근데 너무 비싸",
          "original_form": "근데 너무 비싸",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100033944.1.17",
          "form": "가격이 좀 있지ㅠㅠ",
          "original_form": "가격이 좀 있지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100033944.1.18",
          "form": "그래도 효과는 최고야",
          "original_form": "그래도 효과는 최고야",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100033944.1.19",
          "form": "피로 많이 풀어줘?",
          "original_form": "피로 많이 풀어줘?",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100033944.1.20",
          "form": "비타민이 대박이야",
          "original_form": "비타민이 대박이야",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100033944.1.21",
          "form": "커피보다 나아?",
          "original_form": "커피보다 나아?",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100033944.1.22",
          "form": "응 카페인보다 나은 거 같아",
          "original_form": "응 카페인보다 나은 거 같아",
          "speaker_id": "2",
          "time": "20210825 16:28"
        }
      ]
