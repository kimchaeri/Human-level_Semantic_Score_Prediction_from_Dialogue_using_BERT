      "utterance": [
        {
          "id": "MDRW2100033114.1.1",
          "form": "요즘 편의점에서 별거 다하더라",
          "original_form": "요즘 편의점에서 별거 다하더라",
          "speaker_id": "1",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100033114.1.2",
          "form": "나 이번에 택배도 보냈ㅇㅓ 가니까 진짜 미니마트같음",
          "original_form": "나 이번에 택배도 보냈ㅇㅓ 가니까 진짜 미니마트같음",
          "speaker_id": "1",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100033114.1.3",
          "form": "책 팔았어??",
          "original_form": "책 팔았어??",
          "speaker_id": "2",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100033114.1.4",
          "form": "웅웅 팔기도하고",
          "original_form": "웅웅 팔기도하고",
          "speaker_id": "1",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033114.1.5",
          "form": "사기도하고",
          "original_form": "사기도하고",
          "speaker_id": "1",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033114.1.6",
          "form": "뉴스보니까 전동킥보드도 판대",
          "original_form": "뉴스보니까 전동킥보드도 판대",
          "speaker_id": "1",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033114.1.7",
          "form": "편의점에섴ㅋㅋㅋㅋㅋㅋ",
          "original_form": "편의점에섴ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033114.1.8",
          "form": "진짜?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033114.1.9",
          "form": "이것저것 다 파네 이제",
          "original_form": "이것저것 다 파네 이제",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033114.1.10",
          "form": "온라인 쇼핑몰 축소판 같더라",
          "original_form": "온라인 쇼핑몰 축소판 같더라",
          "speaker_id": "1",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100033114.1.11",
          "form": "완전 ㅎㅎ",
          "original_form": "완전 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100033114.1.12",
          "form": "편의점에서 이걸 사? 싶음 ㅋㅋㅋ 난 먹는거밖에 안사봤던거 같은데",
          "original_form": "편의점에서 이걸 사? 싶음 ㅋㅋㅋ 난 먹는거밖에 안사봤던거 같은데",
          "speaker_id": "1",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100033114.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ그치 먹는게 거의 다 이긴하지",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그치 먹는게 거의 다 이긴하지",
          "speaker_id": "2",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100033114.1.14",
          "form": "편의점 음식 사랑해",
          "original_form": "편의점 음식 사랑해",
          "speaker_id": "2",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033114.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033114.1.16",
          "form": "편의점 할인하는건 마트보다 싸더라",
          "original_form": "편의점 할인하는건 마트보다 싸더라",
          "speaker_id": "1",
          "time": "20210906 19:28"
        },
        {
          "id": "MDRW2100033114.1.17",
          "form": "맞아 1+1만 찾아다녀",
          "original_form": "맞아 1+1만 찾아다녀",
          "speaker_id": "2",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100033114.1.18",
          "form": "완전 이득!!",
          "original_form": "완전 이득!!",
          "speaker_id": "2",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100033114.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ1+1생각한 사람진짜",
          "original_form": "ㅋㅋㅋㅋㅋㅋ1+1생각한 사람진짜",
