        },
        {
          "id": "MDRW2100003660.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210821 10:06"
        },
        {
          "id": "MDRW2100003660.1.3",
          "form": "비가오네요",
          "original_form": "비가오네요",
          "speaker_id": "1",
          "time": "20210821 10:06"
        },
        {
          "id": "MDRW2100003660.1.4",
          "form": "맞아요 장마라고 하더라구요",
          "original_form": "맞아요 장마라고 하더라구요",
          "speaker_id": "2",
          "time": "20210821 10:07"
        },
        {
          "id": "MDRW2100003660.1.5",
          "form": "비오는 날 듣는 음악있으신가요",
          "original_form": "비오는 날 듣는 음악있으신가요",
          "speaker_id": "2",
          "time": "20210821 10:07"
        },
        {
          "id": "MDRW2100003660.1.6",
          "form": "너무많아서 골고루들어요 음원만 2000개정도있어요 앨범단위로요",
          "original_form": "너무많아서 골고루들어요 음원만 2000개정도있어요 앨범단위로요",
          "speaker_id": "1",
          "time": "20210821 10:08"
        },
        {
          "id": "MDRW2100003660.1.7",
          "form": "되게 다양하시네요",
          "original_form": "되게 다양하시네요",
          "speaker_id": "2",
          "time": "20210821 10:08"
        },
        {
          "id": "MDRW2100003660.1.8",
          "form": "추천음악 있으실까요",
          "original_form": "추천음악 있으실까요",
          "speaker_id": "2",
          "time": "20210821 10:08"
        },
        {
          "id": "MDRW2100003660.1.9",
          "form": "조지윈스턴이나 드뷔시같은 잔잔한 음악 어떨까요",
          "original_form": "조지윈스턴이나 드뷔시같은 잔잔한 음악 어떨까요",
          "speaker_id": "1",
          "time": "20210821 10:09"
        },
        {
          "id": "MDRW2100003660.1.10",
          "form": "잔잔한 음악도 좋죠",
          "original_form": "잔잔한 음악도 좋죠",
          "speaker_id": "2",
          "time": "20210821 10:09"
        },
        {
          "id": "MDRW2100003660.1.11",
          "form": "조지원스턴 곡 중에도 추천해주실 게 있을까요",
          "original_form": "조지원스턴 곡 중에도 추천해주실 게 있을까요",
          "speaker_id": "2",
          "time": "20210821 10:10"
        },
        {
          "id": "MDRW2100003660.1.12",
          "form": "추수감사절  세기의 명곡이잔아요 비오는날 듣기좋죠",
          "original_form": "추수감사절  세기의 명곡이잔아요 비오는날 듣기좋죠",
          "speaker_id": "1",
          "time": "20210821 10:11"
        },
        {
          "id": "MDRW2100003660.1.13",
          "form": "한 번 들어볼게요 엄청 좋을 것 같아요",
          "original_form": "한 번 들어볼게요 엄청 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20210821 10:11"
        },
        {
          "id": "MDRW2100003660.1.14",
          "form": "드뷔시는 추천 곡 있으실까요",
          "original_form": "드뷔시는 추천 곡 있으실까요",
          "speaker_id": "2",
          "time": "20210821 10:12"
        },
        {
          "id": "MDRW2100003660.1.15",
          "form": "세기의 명곡 달빛 좋아요 들으시면 아 거리실거예요",
          "original_form": "세기의 명곡 달빛 좋아요 들으시면 아 거리실거예요",
          "speaker_id": "1",
          "time": "20210821 10:12"
        },
        {
          "id": "MDRW2100003660.1.16",
          "form": "앗 네 추천고맙습니다",
          "original_form": "앗 네 추천고맙습니다",
          "speaker_id": "2",
          "time": "20210821 10:13"
        },
        {
          "id": "MDRW2100003660.1.17",
          "form": "꼭 들어볼게요",
          "original_form": "꼭 들어볼게요",
          "speaker_id": "2",
          "time": "20210821 10:13"
        },
        {
          "id": "MDRW2100003660.1.18",
          "form": "멀요  근데 들으면 다아는곡들이예요 길가다 지나면서 나오는곡들이라",
          "original_form": "멀요  근데 들으면 다아는곡들이예요 길가다 지나면서 나오는곡들이라",
