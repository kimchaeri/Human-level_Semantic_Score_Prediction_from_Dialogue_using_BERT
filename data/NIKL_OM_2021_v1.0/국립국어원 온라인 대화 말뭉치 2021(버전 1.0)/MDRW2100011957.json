        },
        {
          "id": "MDRW2100011957.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011957.1.3",
          "form": "맛집 좋아하시나요",
          "original_form": "맛집 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011957.1.4",
          "form": "네 맛집좋아해요 ㅎㅎ",
          "original_form": "네 맛집좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100011957.1.5",
          "form": "오 어떤음식 좋아하시나요",
          "original_form": "오 어떤음식 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100011957.1.6",
          "form": "맛있는 음식은 거의 안가리고 다 좋아해요! 타코도 좋아하고 감자탕도 좋아하고 중식도 좋아하고!",
          "original_form": "맛있는 음식은 거의 안가리고 다 좋아해요! 타코도 좋아하고 감자탕도 좋아하고 중식도 좋아하고!",
          "speaker_id": "2",
          "time": "20210824 21:09"
        },
        {
          "id": "MDRW2100011957.1.7",
          "form": "가가구님은 어떤 음식 좋아하세요?",
          "original_form": "가가구님은 어떤 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 21:09"
        },
        {
          "id": "MDRW2100011957.1.8",
          "form": "오 저도 저거 다 좋아요!",
          "original_form": "오 저도 저거 다 좋아요!",
          "speaker_id": "1",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100011957.1.9",
          "form": "고수도 좋아하시나요?",
          "original_form": "고수도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100011957.1.10",
          "form": "헉 고수는 샴푸맛이나서",
          "original_form": "헉 고수는 샴푸맛이나서",
          "speaker_id": "1",
          "time": "20210824 22:18"
        },
        {
          "id": "MDRW2100011957.1.11",
          "form": "저도 고수는 안 맞더라고요 ㅎㅎ",
          "original_form": "저도 고수는 안 맞더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100011957.1.12",
          "form": "타코랑 베트남쌀국수 좋아하는데 고수는 항상 빼달라고 해요",
          "original_form": "타코랑 베트남쌀국수 좋아하는데 고수는 항상 빼달라고 해요",
          "speaker_id": "2",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100011957.1.13",
          "form": "타코 쌀국수 최고네요",
          "original_form": "타코 쌀국수 최고네요",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100011957.1.14",
          "form": "네 ㅎㅎ 맛있어요",
          "original_form": "네 ㅎㅎ 맛있어요",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100011957.1.15",
          "form": "타코 맛집아시나요",
          "original_form": "타코 맛집아시나요",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100011957.1.16",
          "form": "코레아노스도 맛있고 낙원타코도 맛있어요",
          "original_form": "코레아노스도 맛있고 낙원타코도 맛있어요",
          "speaker_id": "2",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100011957.1.17",
          "form": "추천하는 맛집있나요?",
          "original_form": "추천하는 맛집있나요?",
          "speaker_id": "2",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100011957.1.18",
          "form": "크레이지카츠요!",
          "original_form": "크레이지카츠요!",
