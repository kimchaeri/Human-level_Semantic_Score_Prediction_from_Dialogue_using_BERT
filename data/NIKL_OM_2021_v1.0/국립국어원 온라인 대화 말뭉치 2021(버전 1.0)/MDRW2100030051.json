      "utterance": [
        {
          "id": "MDRW2100030051.1.1",
          "form": "오늘 뭐했니?",
          "original_form": "오늘 뭐했니?",
          "speaker_id": "1",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100030051.1.2",
          "form": "오늘 도서관에 가서 공부했어",
          "original_form": "오늘 도서관에 가서 공부했어",
          "speaker_id": "2",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100030051.1.3",
          "form": "무슨 공부했는데?",
          "original_form": "무슨 공부했는데?",
          "speaker_id": "1",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100030051.1.4",
          "form": "한국사 공부했어 재미없었어ㅠ",
          "original_form": "한국사 공부했어 재미없었어ㅠ",
          "speaker_id": "2",
          "time": "20210903 20:05"
        },
        {
          "id": "MDRW2100030051.1.5",
          "form": "한국사 어떤 부분을 공부했어?",
          "original_form": "한국사 어떤 부분을 공부했어?",
          "speaker_id": "1",
          "time": "20210903 20:06"
        },
        {
          "id": "MDRW2100030051.1.6",
          "form": "한국사 삼국시대를 공부했어! 신라가 너무했어ㅠ",
          "original_form": "한국사 삼국시대를 공부했어! 신라가 너무했어ㅠ",
          "speaker_id": "2",
          "time": "20210903 20:07"
        },
        {
          "id": "MDRW2100030051.1.7",
          "form": "왜? 신라가 왜 너무했어?",
          "original_form": "왜? 신라가 왜 너무했어?",
          "speaker_id": "1",
          "time": "20210903 20:07"
        },
        {
          "id": "MDRW2100030051.1.8",
          "form": "내 친구가 강아지를 키우는데 너무 귀여워요!",
          "original_form": "내 친구가 강아지를 키우는데 너무 귀여워요!",
          "speaker_id": "2",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100030051.1.9",
          "form": "강아지? 아파트에서?",
          "original_form": "강아지? 아파트에서?",
          "speaker_id": "1",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100030051.1.10",
          "form": "아니 주택에서 키워요",
          "original_form": "아니 주택에서 키워요",
          "speaker_id": "2",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100030051.1.11",
          "form": "주택이면 괜찮겠다 아파트는 너무 좁아",
          "original_form": "주택이면 괜찮겠다 아파트는 너무 좁아",
          "speaker_id": "1",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100030051.1.12",
          "form": "그래서 강아지 키우고싶은데 못키우잖아요...",
          "original_form": "그래서 강아지 키우고싶은데 못키우잖아요...",
          "speaker_id": "2",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100030051.1.13",
          "form": "강아지도 행복해야지 아파트에서 키우니까 맨날 짖더라",
          "original_form": "강아지도 행복해야지 아파트에서 키우니까 맨날 짖더라",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100030051.1.14",
          "form": "그건 그 사람들이 훈련을 잘못시킨건 아닐까요?",
          "original_form": "그건 그 사람들이 훈련을 잘못시킨건 아닐까요?",
          "speaker_id": "2",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100030051.1.15",
          "form": "그래? 고모집 개도 짖던데",
          "original_form": "그래? 고모집 개도 짖던데",
          "speaker_id": "1",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100030051.1.16",
          "form": "다른 이유도 있나봐요 아무튼 독립하면 키우고싶어요",
          "original_form": "다른 이유도 있나봐요 아무튼 독립하면 키우고싶어요",
          "speaker_id": "2",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100030051.1.17",
          "form": "독립해서도 원룸에 살면 키우면 안돼",
          "original_form": "독립해서도 원룸에 살면 키우면 안돼",
          "speaker_id": "1",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100030051.1.18",
          "form": "그러면 저도 안키울거예요 조금 넓은집 가면 키우려고요",
          "original_form": "그러면 저도 안키울거예요 조금 넓은집 가면 키우려고요",
          "speaker_id": "2",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100030051.1.19",
          "form": "그럼 강아지 한참 뒤에 키우겠네",
          "original_form": "그럼 강아지 한참 뒤에 키우겠네",
          "speaker_id": "1",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100030051.1.20",
          "form": "강아지는 거의 아기라니까 어쩔수없죠",
          "original_form": "강아지는 거의 아기라니까 어쩔수없죠",
          "speaker_id": "2",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100030051.1.21",
          "form": "그건 맞지",
          "original_form": "그건 맞지",
          "speaker_id": "1",
          "time": "20210903 20:28"
        }
      ]
