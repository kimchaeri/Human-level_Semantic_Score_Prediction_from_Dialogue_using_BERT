      "utterance": [
        {
          "id": "MDRW2100029388.1.1",
          "form": "좋아하는 음식은 있으신가요?",
          "original_form": "좋아하는 음식은 있으신가요?",
          "speaker_id": "1",
          "time": "20210903 04:25"
        },
        {
          "id": "MDRW2100029388.1.2",
          "form": "음식이면 다 좋아요!",
          "original_form": "음식이면 다 좋아요!",
          "speaker_id": "2",
          "time": "20210903 04:27"
        },
        {
          "id": "MDRW2100029388.1.3",
          "form": "그쪽은 좋아하는 음식 뭐 있어요~?",
          "original_form": "그쪽은 좋아하는 음식 뭐 있어요~?",
          "speaker_id": "2",
          "time": "20210903 04:27"
        },
        {
          "id": "MDRW2100029388.1.4",
          "form": "저는 요즘에 마라탕 진짜 좋아해요",
          "original_form": "저는 요즘에 마라탕 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210903 04:29"
        },
        {
          "id": "MDRW2100029388.1.5",
          "form": "마라탕 드셔보셧나요?!",
          "original_form": "마라탕 드셔보셧나요?!",
          "speaker_id": "1",
          "time": "20210903 04:29"
        },
        {
          "id": "MDRW2100029388.1.6",
          "form": "헐 마라탕",
          "original_form": "헐 마라탕",
          "speaker_id": "2",
          "time": "20210903 04:29"
        },
        {
          "id": "MDRW2100029388.1.7",
          "form": "진짜 좋아해요!!",
          "original_form": "진짜 좋아해요!!",
          "speaker_id": "2",
          "time": "20210903 04:29"
        },
        {
          "id": "MDRW2100029388.1.8",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210903 04:30"
        },
        {
          "id": "MDRW2100029388.1.9",
          "form": "주로 뭐 넣어서 드세요?!",
          "original_form": "주로 뭐 넣어서 드세요?!",
          "speaker_id": "1",
          "time": "20210903 04:30"
        },
