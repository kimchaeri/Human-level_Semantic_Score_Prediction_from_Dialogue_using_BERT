      "utterance": [
        {
          "id": "MDRW2100009466.1.1",
          "form": "저희는 코로나전에는 회식 많았는데",
          "original_form": "저희는 코로나전에는 회식 많았는데",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009466.1.2",
          "form": "코로나후에는 거의 없어요",
          "original_form": "코로나후에는 거의 없어요",
          "speaker_id": "1",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009466.1.3",
          "form": "없죠",
          "original_form": "없죠",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009466.1.4",
          "form": "그래서 술마실일이 없어요ㅎ",
          "original_form": "그래서 술마실일이 없어요ㅎ",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009466.1.5",
          "form": "덕분에 주량이 줄고있어요 ㅋ",
          "original_form": "덕분에 주량이 줄고있어요 ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009466.1.6",
          "form": "근데 저희남푠 요새 나 안가지 맨날 회식 했네여",
          "original_form": "근데 저희남푠 요새 나 안가지 맨날 회식 했네여",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009466.1.7",
          "form": "언먹음 줄어요",
          "original_form": "언먹음 줄어요",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009466.1.8",
          "form": "제가 그 케이스요",
          "original_form": "제가 그 케이스요",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009466.1.9",
          "form": "글쵸? 저두요",
          "original_form": "글쵸? 저두요",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009466.1.10",
          "form": "저 예전에 소주 2병 거뜬했는데",
          "original_form": "저 예전에 소주 2병 거뜬했는데",
          "speaker_id": "2",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009466.1.11",
          "form": "아가씨때는 진짜 잘마신다고 소문이나서 ㅋ",
          "original_form": "아가씨때는 진짜 잘마신다고 소문이나서 ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009466.1.12",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009466.1.13",
          "form": "와 진짜 잘드시네요",
          "original_form": "와 진짜 잘드시네요",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009466.1.14",
          "form": "요샌 3잔 ㅜㅜ",
          "original_form": "요샌 3잔 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009466.1.15",
          "form": "정신력으로 마신건데 ㅎㅎ",
          "original_form": "정신력으로 마신건데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009466.1.16",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009466.1.17",
          "form": "다들 술이 세다고 ㅎㅎ 소문이 소문이...",
          "original_form": "다들 술이 세다고 ㅎㅎ 소문이 소문이...",
          "speaker_id": "1",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009466.1.18",
          "form": "부담스럽더라구요",
          "original_form": "부담스럽더라구요",
          "speaker_id": "1",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009466.1.19",
          "form": "ㅋㅋ왜곡되는거죠",
          "original_form": "ㅋㅋ왜곡되는거죠",
          "speaker_id": "2",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009466.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009466.1.21",
          "form": "그럼 자꾸 주고 그럴텐데",
          "original_form": "그럼 자꾸 주고 그럴텐데",
          "speaker_id": "2",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009466.1.22",
          "form": "힘드시죠",
          "original_form": "힘드시죠",
          "speaker_id": "2",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009466.1.23",
          "form": "맞아요 한사람 입에서 넘어갈때마다 한병씩 늘어나는거같더라구요",
          "original_form": "맞아요 한사람 입에서 넘어갈때마다 한병씩 늘어나는거같더라구요",
          "speaker_id": "1",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009466.1.24",
          "form": "헉 건좀 심하네요",
          "original_form": "헉 건좀 심하네요",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009466.1.25",
          "form": "안주를 좀 챙겨드세요",
          "original_form": "안주를 좀 챙겨드세요",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009466.1.26",
          "form": "그러다 위장 빵구나요",
          "original_form": "그러다 위장 빵구나요",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009466.1.27",
          "form": "어떤분이 저더러 회식 없어서 술못먹어서 아쉽겠네 이러시길래",
          "original_form": "어떤분이 저더러 회식 없어서 술못먹어서 아쉽겠네 이러시길래",
          "speaker_id": "1",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009466.1.28",
          "form": "저 술안좋아합니다 그 분위기를 즐기는거죠 라고 했네요",
          "original_form": "저 술안좋아합니다 그 분위기를 즐기는거죠 라고 했네요",
          "speaker_id": "1",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100009466.1.29",
          "form": "ㅡㅡ완전 이미지 버렸네요",
          "original_form": "ㅡㅡ완전 이미지 버렸네요",
          "speaker_id": "2",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100009466.1.30",
          "form": "잘했어요",
          "original_form": "잘했어요",
          "speaker_id": "2",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100009466.1.31",
          "form": "참나 별꼴입니다",
          "original_form": "참나 별꼴입니다",
          "speaker_id": "2",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100009466.1.32",
          "form": "무슨 술꾼인마냥 말씀하셔서 기분이 확 다운됐었네요",
          "original_form": "무슨 술꾼인마냥 말씀하셔서 기분이 확 다운됐었네요",
          "speaker_id": "1",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100009466.1.33",
          "form": "맞아요 상당히 기분나쁜 발언이예요",
          "original_form": "맞아요 상당히 기분나쁜 발언이예요",
          "speaker_id": "2",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100009466.1.34",
          "form": "아 술 진짜 ㅡㅡ확 먹지 마라 하고싶네요",
          "original_form": "아 술 진짜 ㅡㅡ확 먹지 마라 하고싶네요",
          "speaker_id": "2",
          "time": "20210822 23:47"
        }
      ]
    }
