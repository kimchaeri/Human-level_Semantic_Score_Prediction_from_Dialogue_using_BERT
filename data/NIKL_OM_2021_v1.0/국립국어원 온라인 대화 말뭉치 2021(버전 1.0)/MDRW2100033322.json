      "utterance": [
        {
          "id": "MDRW2100033322.1.1",
          "form": "보통 잘 내선 받을 수 없는 장학금이라고 알고 있는데",
          "original_form": "보통 잘 내선 받을 수 없는 장학금이라고 알고 있는데",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100033322.1.2",
          "form": "허허... 공부 잘하는 비법.. 좀 알려주세요",
          "original_form": "허허... 공부 잘하는 비법.. 좀 알려주세요",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100033322.1.3",
          "form": "에이...고등학교공부랑",
          "original_form": "에이...고등학교공부랑",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100033322.1.4",
          "form": "대학공부는 다르죠옹",
          "original_form": "대학공부는 다르죠옹",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100033322.1.5",
          "form": "고등학교 내신은..그냥 1.7정도...",
          "original_form": "고등학교 내신은..그냥 1.7정도...",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100033322.1.6",
          "form": "1.7? ㅋㅋㅋㅋ 우리 좀 먼 사이 일지도 몰라요 ㅋㅋㅋ",
          "original_form": "1.7? ㅋㅋㅋㅋ 우리 좀 먼 사이 일지도 몰라요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100033322.1.7",
          "form": "저는 고등학교때 공부가 너무 싫어졌었거든요",
          "original_form": "저는 고등학교때 공부가 너무 싫어졌었거든요",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100033322.1.8",
          "form": "잘 안나와서...",
          "original_form": "잘 안나와서...",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100033322.1.9",
          "form": "대학교와서 다시 즐거워지긴 했지만",
          "original_form": "대학교와서 다시 즐거워지긴 했지만",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100033322.1.10",
          "form": "인내심도 좋고 정말 노력도 많이하시는 분이라",
          "original_form": "인내심도 좋고 정말 노력도 많이하시는 분이라",
          "speaker_id": "1",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100033322.1.11",
          "form": "이렇게 대화 해본 것 만으로도",
          "original_form": "이렇게 대화 해본 것 만으로도",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100033322.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ전 공부가 싫은 사암이라...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ전 공부가 싫은 사암이라...",
          "speaker_id": "2",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100033322.1.13",
          "form": "대학와서 놀고있어요,,,,",
          "original_form": "대학와서 놀고있어요,,,,",
          "speaker_id": "2",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100033322.1.14",
          "form": "인싸이실게 틀림 없다며..",
          "original_form": "인싸이실게 틀림 없다며..",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100033322.1.15",
          "form": "인기가 상당히 많으실 것 같아요 ㅎㅎ",
          "original_form": "인기가 상당히 많으실 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100033322.1.16",
          "form": "대학때 안놀면 언제 놀아요!",
          "original_form": "대학때 안놀면 언제 놀아요!",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100033322.1.17",
          "form": "더 놀아요 더 놀아",
          "original_form": "더 놀아요 더 놀아",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100033322.1.18",
          "form": "제가 졸업하고 후회하는게 딱 하난데",
          "original_form": "제가 졸업하고 후회하는게 딱 하난데",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100033322.1.19",
          "form": "아주 미친듯이 못 놀았단거에요",
          "original_form": "아주 미친듯이 못 놀았단거에요",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100033322.1.20",
          "form": "더 놀아요!",
          "original_form": "더 놀아요!",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100033322.1.21",
          "form": "하 자발적 아싸에요...^^ㅋㅋㅋㅋ",
          "original_form": "하 자발적 아싸에요...^^ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100033322.1.22",
          "form": "사실 1 2핟년은 잘놀았는데",
          "original_form": "사실 1 2핟년은 잘놀았는데",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100033322.1.23",
          "form": "지금은 코로나,,,",
          "original_form": "지금은 코로나,,,",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100033322.1.24",
          "form": "아.. 지금은 시국때문에 그런거니까요 ㅎㅎ",
          "original_form": "아.. 지금은 시국때문에 그런거니까요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100033322.1.25",
          "form": "이렇게 다 하시면서 자발적 아싸라니...",
          "original_form": "이렇게 다 하시면서 자발적 아싸라니...",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033322.1.26",
          "form": "혹시 사람이 아니라 유니콘 아니에요?",
          "original_form": "혹시 사람이 아니라 유니콘 아니에요?",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033322.1.27",
          "form": "전 혼자가좋아서",
          "original_form": "전 혼자가좋아서",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033322.1.28",
          "form": "인기 엄청 많을 것 같은데",
          "original_form": "인기 엄청 많을 것 같은데",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033322.1.29",
          "form": "대화하는데 상당히 섬세하고 다정한 분 인것 같은걸요 ㅋㅋ",
          "original_form": "대화하는데 상당히 섬세하고 다정한 분 인것 같은걸요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100033322.1.30",
          "form": "앗....우앙아",
          "original_form": "앗....우앙아",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100033322.1.31",
          "form": "감사합니다<<",
          "original_form": "감사합니다<<",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100033322.1.32",
          "form": "><",
          "original_form": "><",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100033322.1.33",
          "form": "그러면..",
          "original_form": "그러면..",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100033322.1.34",
          "form": "최근엔 여행도 어려울거고",
          "original_form": "최근엔 여행도 어려울거고",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100033322.1.35",
          "form": "뭐 하면서 놀고 계시나요??",
          "original_form": "뭐 하면서 놀고 계시나요??",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100033322.1.36",
          "form": "최근에 음...랜선으로 여행하거나",
          "original_form": "최근에 음...랜선으로 여행하거나",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100033322.1.37",
          "form": "아이면",
          "original_form": "아이면",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100033322.1.38",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100033322.1.39",
          "form": "게임좋아해요!",
          "original_form": "게임좋아해요!",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100033322.1.40",
          "form": "랜선여행이랑 게임!",
          "original_form": "랜선여행이랑 게임!",
          "speaker_id": "1",
          "time": "20210906 22:25"
        }
      ]
