      "utterance": [
        {
          "id": "MDRW2100040704.1.1",
          "form": "원이랑 수가 태권도 마치고 놀이터에 오면",
          "original_form": "원이랑 수가 태권도 마치고 놀이터에 오면",
          "speaker_id": "2",
          "time": "20210905 00:19"
        },
        {
          "id": "MDRW2100040704.1.2",
          "form": "항상 아이스크림을 사줬잖아",
          "original_form": "항상 아이스크림을 사줬잖아",
          "speaker_id": "2",
          "time": "20210905 00:19"
        },
        {
          "id": "MDRW2100040704.1.3",
          "form": "근데 하루는 수가 너무 목이 말라서 자기는 편의점에서 물을 사달라는 거야",
          "original_form": "근데 하루는 수가 너무 목이 말라서 자기는 편의점에서 물을 사달라는 거야",
          "speaker_id": "2",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100040704.1.4",
          "form": "근데 돈이 너무 아깝잖아 생수는",
          "original_form": "근데 돈이 너무 아깝잖아 생수는",
          "speaker_id": "2",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100040704.1.5",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100040704.1.6",
          "form": "그래서 수 가방에 물통 있길래",
          "original_form": "그래서 수 가방에 물통 있길래",
          "speaker_id": "2",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100040704.1.7",
          "form": "아바바 가서",
          "original_form": "아바바 가서",
          "speaker_id": "2",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100040704.1.8",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100040704.1.9",
          "form": "죄송한데요 생수 좀 담아 주세요",
          "original_form": "죄송한데요 생수 좀 담아 주세요",
          "speaker_id": "2",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100040704.1.10",
          "form": "하고 물 담아왔다",
          "original_form": "하고 물 담아왔다",
          "speaker_id": "2",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100040704.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100040704.1.12",
          "form": "매일 아이스크림사줬나?",
          "original_form": "매일 아이스크림사줬나?",
          "speaker_id": "1",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100040704.1.13",
