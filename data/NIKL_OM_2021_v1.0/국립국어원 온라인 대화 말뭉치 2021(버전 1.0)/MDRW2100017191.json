      "utterance": [
        {
          "id": "MDRW2100017191.1.1",
          "form": "우리 돈모아서",
          "original_form": "우리 돈모아서",
          "speaker_id": "2",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017191.1.2",
          "form": "나는 컴퓨터사고 넌 카메라쓰자",
          "original_form": "나는 컴퓨터사고 넌 카메라쓰자",
          "speaker_id": "2",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017191.1.3",
          "form": "아니 사자",
          "original_form": "아니 사자",
          "speaker_id": "2",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017191.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017191.1.5",
          "form": "좋다좋다",
          "original_form": "좋다좋다",
          "speaker_id": "1",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017191.1.6",
          "form": "난는 풍경 찍는게 좋아서",
          "original_form": "난는 풍경 찍는게 좋아서",
          "speaker_id": "1",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017191.1.7",
          "form": "망원 렌즈 사고 싶어",
          "original_form": "망원 렌즈 사고 싶어",
          "speaker_id": "1",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017191.1.8",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017191.1.9",
          "form": "멀리있는거 찍는렌즌가?",
          "original_form": "멀리있는거 찍는렌즌가?",
          "speaker_id": "2",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017191.1.10",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017191.1.11",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017191.1.12",
          "form": "렌즈 엄청 긴거",
          "original_form": "렌즈 엄청 긴거",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017191.1.13",
          "form": "ㅇㅎ그 대포라고 부르는거?",
          "original_form": "ㅇㅎ그 대포라고 부르는거?",
          "speaker_id": "2",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017191.1.14",
          "form": "중고로 많이 팔기는 하는데",
          "original_form": "중고로 많이 팔기는 하는데",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017191.1.15",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017191.1.16",
          "form": "아 그거 비싸다며",
          "original_form": "아 그거 비싸다며",
          "speaker_id": "2",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017191.1.17",
          "form": "중고도 잘 알아보고 사야하니까",
          "original_form": "중고도 잘 알아보고 사야하니까",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017191.1.18",
          "form": "응 렌즈만 300백 기본이더라",
          "original_form": "응 렌즈만 300백 기본이더라",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017191.1.19",
          "form": "와.........",
          "original_form": "와.........",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017191.1.20",
          "form": "몸체는 몇십 만원대인데",
          "original_form": "몸체는 몇십 만원대인데",
          "speaker_id": "1",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017191.1.21",
          "form": "렌즈만 저정도라",
          "original_form": "렌즈만 저정도라",
          "speaker_id": "1",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017191.1.22",
          "form": "사면 후회할것 같아",
          "original_form": "사면 후회할것 같아",
          "speaker_id": "1",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017191.1.23",
          "form": "개비싸네 진짜",
          "original_form": "개비싸네 진짜",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017191.1.24",
          "form": "응ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "응ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017191.1.25",
          "form": "그런거 사도 후회안할정도로 돈을벌자",
          "original_form": "그런거 사도 후회안할정도로 돈을벌자",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017191.1.26",
          "form": "넌 무슨 컴퓨토 사고 싶은거야?",
          "original_form": "넌 무슨 컴퓨토 사고 싶은거야?",
          "speaker_id": "1",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017191.1.27",
          "form": "진짜 좋다",
          "original_form": "진짜 좋다",
          "speaker_id": "1",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017191.1.28",
          "form": "무슨 컴퓨터 사고 싶은거야?",
          "original_form": "무슨 컴퓨터 사고 싶은거야?",
          "speaker_id": "1",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017191.1.29",
          "form": "난 사양 제일좋은거로 사고싶어",
          "original_form": "난 사양 제일좋은거로 사고싶어",
          "speaker_id": "2",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100017191.1.30",
          "form": "근데 사양 좋으면",
          "original_form": "근데 사양 좋으면",
          "speaker_id": "1",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100017191.1.31",
          "form": "그래픽카드 메인보드 ssd 램 이런거 싹다",
          "original_form": "그래픽카드 메인보드 ssd 램 이런거 싹다",
          "speaker_id": "2",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100017191.1.32",
          "form": "최상급사양으로",
          "original_form": "최상급사양으로",
          "speaker_id": "2",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100017191.1.33",
          "form": "지르고싶음",
