      "utterance": [
        {
          "id": "MDRW2100028466.1.1",
          "form": "정형돈과에요ㅋㅋㅋ",
          "original_form": "정형돈과에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028466.1.2",
          "form": "앜ㅋㅋㅌ 뜨랑낄로? 그분꺼로 저는 시작해써요",
          "original_form": "앜ㅋㅋㅌ 뜨랑낄로? 그분꺼로 저는 시작해써요",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028466.1.3",
          "form": "여행유투브는",
          "original_form": "여행유투브는",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028466.1.4",
          "form": "전 빠니보틀입니당",
          "original_form": "전 빠니보틀입니당",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028466.1.5",
          "form": "거기 댓글에 빠보 추천해서 갈아타버린 ㅋㅋㅋ",
          "original_form": "거기 댓글에 빠보 추천해서 갈아타버린 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100028466.1.6",
          "form": "ㅋㅋ 빠보가 승인가요",
          "original_form": "ㅋㅋ 빠보가 승인가요",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028466.1.7",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100028466.1.8",
          "form": "여행도 하고 돈도 벌고 뭔가 부러워요",
          "original_form": "여행도 하고 돈도 벌고 뭔가 부러워요",
          "speaker_id": "1",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100028466.1.9",
          "form": "ㅋㅋ좋좋소도 대박나고....",
          "original_form": "ㅋㅋ좋좋소도 대박나고....",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028466.1.10",
          "form": "넘 궁금해지네요 ㅋㅋㅋ 좋좋소",
          "original_form": "넘 궁금해지네요 ㅋㅋㅋ 좋좋소",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028466.1.11",
          "form": "학생이신가요ㅋㅋ",
          "original_form": "학생이신가요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028466.1.12",
          "form": "저는 직장인이요 ㅎㅎ",
          "original_form": "저는 직장인이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100028466.1.13",
          "form": "게임 영상은 안보시나요?",
          "original_form": "게임 영상은 안보시나요?",
          "speaker_id": "1",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100028466.1.14",
          "form": "풍월량이요 ㅋㅋ",
          "original_form": "풍월량이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100028466.1.15",
          "form": "그럼 직딩의 삶을",
          "original_form": "그럼 직딩의 삶을",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100028466.1.16",
          "form": "좋좋소를 통해 ㅎㅎ",
          "original_form": "좋좋소를 통해 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100028466.1.17",
          "form": "풍뎅이신가요?!",
          "original_form": "풍뎅이신가요?!",
          "speaker_id": "1",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100028466.1.18",
          "form": "아뇨 저 자유로운 영혼입니다",
          "original_form": "아뇨 저 자유로운 영혼입니다",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100028466.1.19",
          "form": "아하 ㅋㅋㅋ 저는 거의 풍월량님 많이봐요",
          "original_form": "아하 ㅋㅋㅋ 저는 거의 풍월량님 많이봐요",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100028466.1.20",
          "form": "욕을 안해서 좋더라구요",
          "original_form": "욕을 안해서 좋더라구요",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100028466.1.21",
          "form": "저는 침착맨 ㅎㅎ",
          "original_form": "저는 침착맨 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100028466.1.22",
          "form": "주호민ㅈ놀리는게 재밌어서 ㅎㅎ",
          "original_form": "주호민ㅈ놀리는게 재밌어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100028466.1.23",
          "form": "오 그분도 잼죠 ㅋㅋㅋ 킹받게하지만",
          "original_form": "오 그분도 잼죠 ㅋㅋㅋ 킹받게하지만",
          "speaker_id": "1",
          "time": "20210902 23:37"
        }
