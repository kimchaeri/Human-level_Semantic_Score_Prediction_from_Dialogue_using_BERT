        },
        {
          "id": "MDRW2100043271.1.2",
          "form": "저녁은 먹었엉? ㅋㅋ",
          "original_form": "저녁은 먹었엉? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100043271.1.3",
          "form": "뭐해먹지",
          "original_form": "뭐해먹지",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100043271.1.4",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100043271.1.5",
          "form": "움",
          "original_form": "움",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100043271.1.6",
          "form": "그냥 남은걸로먹었쥬",
          "original_form": "그냥 남은걸로먹었쥬",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100043271.1.7",
          "form": "하..근뎅",
          "original_form": "하..근뎅",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100043271.1.8",
          "form": "내일 뭐먹을지 ....",
          "original_form": "내일 뭐먹을지 ....",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100043271.1.9",
          "form": "남은걸로ㅠㅠㅋㅋ",
          "original_form": "남은걸로ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100043271.1.10",
          "form": "메뉴정하기가 제일 귀찮",
          "original_form": "메뉴정하기가 제일 귀찮",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100043271.1.11",
          "form": "나 오늘 곱창볶음 시키려다 말았는데",
          "original_form": "나 오늘 곱창볶음 시키려다 말았는데",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100043271.1.12",
          "form": "해치워야지!",
          "original_form": "해치워야지!",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100043271.1.13",
          "form": "남은걸",
          "original_form": "남은걸",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100043271.1.14",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100043271.1.15",
          "form": "내일 시킬까ㅋㅋ",
          "original_form": "내일 시킬까ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100043271.1.16",
          "form": "왜안시켰옹ㅋㅋㅋ",
          "original_form": "왜안시켰옹ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100043271.1.17",
          "form": "맛있겠당",
          "original_form": "맛있겠당",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100043271.1.18",
          "form": "남편 늦는데 혼자 시켜먹기 그래서ㅠ",
          "original_form": "남편 늦는데 혼자 시켜먹기 그래서ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100043271.1.19",
          "form": "곱분이 곱창 드셔보셨어영?",
          "original_form": "곱분이 곱창 드셔보셨어영?",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100043271.1.20",
          "form": "곱분이?",
          "original_form": "곱분이?",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100043271.1.21",
          "form": "어디꺼에유?",
          "original_form": "어디꺼에유?",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100043271.1.22",
          "form": "곱분이꺼욪ㅋㅋㅋㅋㅈㅋㅋㄱㅈ",
          "original_form": "곱분이꺼욪ㅋㅋㅋㅋㅈㅋㅋㄱㅈ",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100043271.1.23",
          "form": "아그냥ㅋㅋㅋ이름잌ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아그냥ㅋㅋㅋ이름잌ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100043271.1.24",
          "form": "아니아니",
          "original_form": "아니아니",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100043271.1.25",
          "form": "네..... 곱분이 곱창ㅋㅋ",
          "original_form": "네..... 곱분이 곱창ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100043271.1.26",
          "form": "어디서 주문하는지",
          "original_form": "어디서 주문하는지",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100043271.1.27",
          "form": "이게 상호입니다만. ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이게 상호입니다만. ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100043271.1.28",
          "form": "물어보려다갘ㅋㅋㅋ",
          "original_form": "물어보려다갘ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100043271.1.29",
          "form": "배민으로 주문하려구염ㅋㅋ",
          "original_form": "배민으로 주문하려구염ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100043271.1.30",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100043271.1.31",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100043271.1.32",
          "form": "왕십리 곱찬",
          "original_form": "왕십리 곱찬",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100043271.1.33",
          "form": "아세욤 ??ㅋㅋ",
          "original_form": "아세욤 ??ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100043271.1.34",
          "form": "거기 대창 엄청맛있어...",
          "original_form": "거기 대창 엄청맛있어...",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100043271.1.35",
          "form": "근데 비싸",
          "original_form": "근데 비싸",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100043271.1.36",
          "form": "거기도 배달인가여?",
          "original_form": "거기도 배달인가여?",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100043271.1.37",
          "form": "거긴",
          "original_form": "거긴",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100043271.1.38",
          "form": "네이버에서 주문가능한데",
          "original_form": "네이버에서 주문가능한데",
