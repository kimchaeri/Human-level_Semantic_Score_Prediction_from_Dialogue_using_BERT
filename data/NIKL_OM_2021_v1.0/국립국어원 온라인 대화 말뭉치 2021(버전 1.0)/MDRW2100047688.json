        },
        {
          "id": "MDRW2100047688.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210827 01:18"
        },
        {
          "id": "MDRW2100047688.1.4",
          "form": "이게 기준이 애매하잖아요",
          "original_form": "이게 기준이 애매하잖아요",
          "speaker_id": "2",
          "time": "20210827 01:18"
        },
        {
          "id": "MDRW2100047688.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 01:18"
        },
        {
          "id": "MDRW2100047688.1.6",
          "form": "그거 법적으로 가면",
          "original_form": "그거 법적으로 가면",
          "speaker_id": "1",
          "time": "20210827 01:18"
        },
        {
          "id": "MDRW2100047688.1.7",
          "form": "사실 보는 눈따라 엥...아닌 거 같은데 싶은 것도 있고",
          "original_form": "사실 보는 눈따라 엥...아닌 거 같은데 싶은 것도 있고",
          "speaker_id": "2",
          "time": "20210827 01:18"
        },
        {
          "id": "MDRW2100047688.1.8",
          "form": "표절이라고 못박히는 거 거의 없어요",
          "original_form": "표절이라고 못박히는 거 거의 없어요",
          "speaker_id": "1",
          "time": "20210827 01:18"
        },
        {
          "id": "MDRW2100047688.1.9",
          "form": "이제 인민 재판 이런거죠",
          "original_form": "이제 인민 재판 이런거죠",
          "speaker_id": "1",
          "time": "20210827 01:18"
        },
        {
          "id": "MDRW2100047688.1.10",
          "form": "근데 연재종료 하게 되는건 대부분 그거죠? 뒷말 나오는 게 싫어서?",
          "original_form": "근데 연재종료 하게 되는건 대부분 그거죠? 뒷말 나오는 게 싫어서?",
          "speaker_id": "2",
          "time": "20210827 01:18"
        },
        {
          "id": "MDRW2100047688.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210827 01:18"
        },
        {
          "id": "MDRW2100047688.1.12",
          "form": "글고 거의 연종됐으면 빼박인게 많아서 그럴수도 있어요",
          "original_form": "글고 거의 연종됐으면 빼박인게 많아서 그럴수도 있어요",
          "speaker_id": "1",
          "time": "20210827 01:18"
        },
        {
          "id": "MDRW2100047688.1.13",
          "form": "왜 그 다음웹툰 그 트레이싱 했다는",
          "original_form": "왜 그 다음웹툰 그 트레이싱 했다는",
          "speaker_id": "2",
          "time": "20210827 01:18"
        },
        {
          "id": "MDRW2100047688.1.14",
          "form": "그 작품도 거의...",
          "original_form": "그 작품도 거의...",
          "speaker_id": "2",
          "time": "20210827 01:18"
        },
        {
          "id": "MDRW2100047688.1.15",
          "form": "네이버같은데 아니고서는",
          "original_form": "네이버같은데 아니고서는",
          "speaker_id": "1",
          "time": "20210827 01:18"
        },
        {
          "id": "MDRW2100047688.1.16",
          "form": "안하는 거 같더라고요?",
          "original_form": "안하는 거 같더라고요?",
          "speaker_id": "2",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100047688.1.17",
          "form": "맞아요 모래시계",
          "original_form": "맞아요 모래시계",
          "speaker_id": "1",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100047688.1.18",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "2",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100047688.1.19",
          "form": "그 사람은 업계 쫙 퍼졌잖아요",
          "original_form": "그 사람은 업계 쫙 퍼졌잖아요",
          "speaker_id": "1",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100047688.1.20",
          "form": "그것도요?!",
          "original_form": "그것도요?!",
          "speaker_id": "2",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100047688.1.21",
          "form": "그 감방남주",
          "original_form": "그 감방남주",
          "speaker_id": "1",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100047688.1.22",
          "form": "아니 전 화양연화<<<<<<<<<",
          "original_form": "아니 전 화양연화<<<<<<<<<",
          "speaker_id": "2",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100047688.1.23",
          "form": "아뇨 모래시계를 조각조각 붙여서",
          "original_form": "아뇨 모래시계를 조각조각 붙여서",
          "speaker_id": "1",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100047688.1.24",
          "form": "이 웹툰 다시 연재안하길ㄹㅐ......",
          "original_form": "이 웹툰 다시 연재안하길ㄹㅐ......",
          "speaker_id": "2",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100047688.1.25",
          "form": "감방남주 만들었더라고요",
          "original_form": "감방남주 만들었더라고요",
          "speaker_id": "1",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100047688.1.26",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100047688.1.27",
          "form": "아앗...",
          "original_form": "아앗...",
          "speaker_id": "1",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100047688.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아놔 그거알아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아놔 그거알아요",
          "speaker_id": "2",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100047688.1.29",
          "form": "그거...작가들 엄청 난리났었던..",
          "original_form": "그거...작가들 엄청 난리났었던..",
          "speaker_id": "2",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100047688.1.30",
          "form": "ㄹㅇ 그건 좀 너무했어요",
          "original_form": "ㄹㅇ 그건 좀 너무했어요",
          "speaker_id": "1",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100047688.1.31",
          "form": "화양연화< 요거 작가님 트레이싱 어쩌고하면서",
          "original_form": "화양연화< 요거 작가님 트레이싱 어쩌고하면서",
          "speaker_id": "2",
          "time": "20210827 01:20"
        },
        {
          "id": "MDRW2100047688.1.32",
          "form": "댓글 난리났잖아요",
          "original_form": "댓글 난리났잖아요",
          "speaker_id": "2",
          "time": "20210827 01:20"
        },
        {
          "id": "MDRW2100047688.1.33",
          "form": "연재 끝날때까지 안놔주던데",
          "original_form": "연재 끝날때까지 안놔주던데",
          "speaker_id": "2",
          "time": "20210827 01:20"
        },
        {
          "id": "MDRW2100047688.1.34",
          "form": "깜짝놀랐어요 무슨....",
          "original_form": "깜짝놀랐어요 무슨....",
          "speaker_id": "2",
          "time": "20210827 01:20"
        },
        {
          "id": "MDRW2100047688.1.35",
          "form": "오우",
          "original_form": "오우",
          "speaker_id": "1",
          "time": "20210827 01:20"
        },
        {
          "id": "MDRW2100047688.1.36",
          "form": "댓글창 지금도 안좋더라고요",
          "original_form": "댓글창 지금도 안좋더라고요",
          "speaker_id": "2",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047688.1.37",
          "form": "공지있나...보러갔다가 후진",
          "original_form": "공지있나...보러갔다가 후진",
          "speaker_id": "2",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047688.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047688.1.39",
          "form": "근데 약간",
          "original_form": "근데 약간",
          "speaker_id": "1",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047688.1.40",
          "form": "뭐라해야하지",
          "original_form": "뭐라해야하지",
          "speaker_id": "1",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047688.1.41",
          "form": "크게 공론화 안되면",
          "original_form": "크게 공론화 안되면",
          "speaker_id": "1",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047688.1.42",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047688.1.43",
          "form": "걍 하는것같지 않나요",
          "original_form": "걍 하는것같지 않나요",
          "speaker_id": "1",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047688.1.44",
          "form": "작가가 죽어도 아니라하면",
          "original_form": "작가가 죽어도 아니라하면",
          "speaker_id": "1",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047688.1.45",
          "form": "아 그건 맞아요...",
          "original_form": "아 그건 맞아요...",
          "speaker_id": "2",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047688.1.46",
          "form": "회사는 그냥 한다더라고요",
          "original_form": "회사는 그냥 한다더라고요",
          "speaker_id": "1",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047688.1.47",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047688.1.48",
          "form": "왜냐면 그게 계약된 상태고 작가",
          "original_form": "왜냐면 그게 계약된 상태고 작가",
          "speaker_id": "1",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047688.1.49",
          "form": "팽당하면",
          "original_form": "팽당하면",
          "speaker_id": "1",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047688.1.50",
          "form": "어후..그거 뒷감당 어케해여",
          "original_form": "어후..그거 뒷감당 어케해여",
          "speaker_id": "1",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100047688.1.51",
          "form": "근데 그게 그 애매하기도 해요 아니라고 하는 사람들도 있는데 무조건 내리면",
          "original_form": "근데 그게 그 애매하기도 해요 아니라고 하는 사람들도 있는데 무조건 내리면",
          "speaker_id": "2",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100047688.1.52",
          "form": "걍 인정한다고 생각하잖아요",
          "original_form": "걍 인정한다고 생각하잖아요",
          "speaker_id": "2",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100047688.1.53",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100047688.1.54",
          "form": "그그",
          "original_form": "그그",
          "speaker_id": "1",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100047688.1.55",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100047688.1.56",
          "form": "그 여우 사막여우?",
          "original_form": "그 여우 사막여우?",
          "speaker_id": "1",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100047688.1.57",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "2",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100047688.1.58",
          "form": "그 네이버에서 연재되는건데 표절논란 났던거",
          "original_form": "그 네이버에서 연재되는건데 표절논란 났던거",
          "speaker_id": "1",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100047688.1.59",
          "form": "그 수요웾툰....? 목요웹툰....?",
          "original_form": "그 수요웾툰....? 목요웹툰....?",
          "speaker_id": "2",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100047688.1.60",
          "form": "동양풍인데",
          "original_form": "동양풍인데",
          "speaker_id": "1",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100047688.1.61",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100047688.1.62",
          "form": "네 그 칼국수머리카락인가",
          "original_form": "네 그 칼국수머리카락인가",
          "speaker_id": "2",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100047688.1.63",
          "form": "그 머리카락얘기나온",
          "original_form": "그 머리카락얘기나온",
          "speaker_id": "2",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100047688.1.64",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210827 01:23"
        },
        {
          "id": "MDRW2100047688.1.65",
          "form": "맞죠 색감이랑!!",
          "original_form": "맞죠 색감이랑!!",
          "speaker_id": "2",
          "time": "20210827 01:23"
        },
        {
          "id": "MDRW2100047688.1.66",
          "form": "그것도 흐지부지됐고 걍 댓글만 난리난 상태에서",
          "original_form": "그것도 흐지부지됐고 걍 댓글만 난리난 상태에서",
          "speaker_id": "1",
          "time": "20210827 01:23"
        },
        {
          "id": "MDRW2100047688.1.67",
          "form": "킵고잉하는거 보면",
