        {
          "id": "MDRW2100000422.1.31",
          "form": "넷플릭스 화면공유해서 영화도보고",
          "original_form": "넷플릭스 화면공유해서 영화도보고",
          "speaker_id": "1",
          "time": "20210819 16:58"
