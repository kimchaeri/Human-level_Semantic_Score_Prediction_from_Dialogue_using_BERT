      "utterance": [
        {
          "id": "MDRW2100000770.1.1",
          "form": "혹시 호두마루가 아이스크림 맞나요?!!!",
          "original_form": "혹시 호두마루가 아이스크림 맞나요?!!!",
          "speaker_id": "1",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100000770.1.2",
          "form": "저도 호두마루 아이스크림 .. 사랑합니다",
          "original_form": "저도 호두마루 아이스크림 .. 사랑합니다",
          "speaker_id": "1",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100000770.1.3",
          "form": "안녕하세요~!! 아이스크림이기도 하고 ㅋㅋㅋ 저희집 강아쥐들 이름이에요 호두랑 마루!",
          "original_form": "안녕하세요~!! 아이스크림이기도 하고 ㅋㅋㅋ 저희집 강아쥐들 이름이에요 호두랑 마루!",
          "speaker_id": "2",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100000770.1.4",
          "form": "아하!!",
          "original_form": "아하!!",
          "speaker_id": "1",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100000770.1.5",
          "form": "아이스크림 완전 맛잇죠 꼬숩달달",
          "original_form": "아이스크림 완전 맛잇죠 꼬숩달달",
          "speaker_id": "2",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100000770.1.6",
          "form": "호두마루 조아하시면 피스타치오맛 아이스크림도 혿시 좋아하세요!?",
          "original_form": "호두마루 조아하시면 피스타치오맛 아이스크림도 혿시 좋아하세요!?",
          "speaker_id": "2",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100000770.1.7",
          "form": "당근이죠!!",
          "original_form": "당근이죠!!",
          "speaker_id": "1",
          "time": "20210819 19:09"
        },
        {
          "id": "MDRW2100000770.1.8",
          "form": "견과류 느낌",
          "original_form": "견과류 느낌",
          "speaker_id": "1",
          "time": "20210819 19:09"
        },
        {
          "id": "MDRW2100000770.1.9",
          "form": "저 엄청 러버",
          "original_form": "저 엄청 러버",
          "speaker_id": "1",
          "time": "20210819 19:09"
        },
        {
          "id": "MDRW2100000770.1.10",
          "form": "좋아합니다!!",
          "original_form": "좋아합니다!!",
          "speaker_id": "1",
          "time": "20210819 19:09"
        },
        {
          "id": "MDRW2100000770.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ오 대박대박",
          "original_form": "ㅋㅋㅋㅋㅋㅋ오 대박대박",
          "speaker_id": "2",
          "time": "20210819 19:09"
        },
        {
          "id": "MDRW2100000770.1.12",
          "form": "집에 견과류 쌓여 있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "집에 견과류 쌓여 있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 19:10"
        },
        {
          "id": "MDRW2100000770.1.13",
          "form": "저도 견과류 좋아해요 ㅠㅠ\r\n너무 좋아해서 아몬드향 핸드크림도 조아합니다",
          "original_form": "저도 견과류 좋아해요 ㅠㅠ\r\n너무 좋아해서 아몬드향 핸드크림도 조아합니다",
          "speaker_id": "2",
          "time": "20210819 19:10"
        },
        {
          "id": "MDRW2100000770.1.14",
          "form": "와.. 어케 저랑 똑같죠?",
          "original_form": "와.. 어케 저랑 똑같죠?",
          "speaker_id": "1",
          "time": "20210819 19:10"
        },
        {
          "id": "MDRW2100000770.1.15",
          "form": "헐 진짜여? ㅋㅋㅋ 어디꺼요? 저는 더바디샵꺼!!",
          "original_form": "헐 진짜여? ㅋㅋㅋ 어디꺼요? 저는 더바디샵꺼!!",
          "speaker_id": "2",
          "time": "20210819 19:10"
        },
        {
          "id": "MDRW2100000770.1.16",
          "form": "아몬드향 핸드크림..",
          "original_form": "아몬드향 핸드크림..",
          "speaker_id": "1",
          "time": "20210819 19:10"
        },
        {
          "id": "MDRW2100000770.1.17",
          "form": "저도 선물 받았거든요",
          "original_form": "저도 선물 받았거든요",
          "speaker_id": "1",
          "time": "20210819 19:10"
        },
        {
          "id": "MDRW2100000770.1.18",
          "form": "어디꺼인지는 밖이라 모르지만요 ㅋㅋㅎㅋ",
          "original_form": "어디꺼인지는 밖이라 모르지만요 ㅋㅋㅎㅋ",
          "speaker_id": "1",
          "time": "20210819 19:11"
        },
        {
          "id": "MDRW2100000770.1.19",
          "form": "우와 견과류는 진짜 어디 넣어도 맛잇잖아요",
          "original_form": "우와 견과류는 진짜 어디 넣어도 맛잇잖아요",
          "speaker_id": "2",
          "time": "20210819 19:11"
        },
        {
          "id": "MDRW2100000770.1.20",
          "form": "인정요..",
          "original_form": "인정요..",
          "speaker_id": "1",
          "time": "20210819 19:11"
        },
        {
          "id": "MDRW2100000770.1.21",
          "form": "요거트에 넣어도 맛있고...!",
          "original_form": "요거트에 넣어도 맛있고...!",
          "speaker_id": "2",
          "time": "20210819 19:11"
        },
        {
          "id": "MDRW2100000770.1.22",
          "form": "멸치볶음에 들어간것도 맛나고",
          "original_form": "멸치볶음에 들어간것도 맛나고",
          "speaker_id": "2",
          "time": "20210819 19:11"
        },
        {
          "id": "MDRW2100000770.1.23",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "1",
          "time": "20210819 19:11"
        },
        {
          "id": "MDRW2100000770.1.24",
          "form": "멸치 볶음 아몬드 호두",
          "original_form": "멸치 볶음 아몬드 호두",
          "speaker_id": "1",
          "time": "20210819 19:11"
        },
        {
          "id": "MDRW2100000770.1.25",
          "form": "그냥 꿀맛..",
          "original_form": "그냥 꿀맛..",
          "speaker_id": "1",
          "time": "20210819 19:11"
        },
        {
          "id": "MDRW2100000770.1.26",
          "form": "배우신 분인가 봐요?",
          "original_form": "배우신 분인가 봐요?",
          "speaker_id": "1",
          "time": "20210819 19:12"
        },
        {
          "id": "MDRW2100000770.1.27",
          "form": "대단하시네",
          "original_form": "대단하시네",
          "speaker_id": "1",
          "time": "20210819 19:12"
        },
        {
          "id": "MDRW2100000770.1.28",
          "form": "전 허니버터 아몬드... 집에 쌓아두고 먹고 있어요",
          "original_form": "전 허니버터 아몬드... 집에 쌓아두고 먹고 있어요",
          "speaker_id": "1",
          "time": "20210819 19:12"
        },
        {
          "id": "MDRW2100000770.1.29",
          "form": "헉 대박 \r\n그 시리즈중에 카라멜프레첼 어쩌고 그거 드셔보셨어요? 그것고 진짜 맛잇어요ㅠㅠ",
          "original_form": "헉 대박 \r\n그 시리즈중에 카라멜프레첼 어쩌고 그거 드셔보셨어요? 그것고 진짜 맛잇어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 19:13"
        },
        {
          "id": "MDRW2100000770.1.30",
          "form": "아 저 그것도 진짜",
          "original_form": "아 저 그것도 진짜",
          "speaker_id": "1",
          "time": "20210819 19:13"
        },
        {
          "id": "MDRW2100000770.1.31",
          "form": "과자 들어가 있는거!!",
          "original_form": "과자 들어가 있는거!!",
          "speaker_id": "1",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000770.1.32",
          "form": "진짜 좋아해요 대박..",
          "original_form": "진짜 좋아해요 대박..",
          "speaker_id": "1",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000770.1.33",
          "form": "혹시..",
          "original_form": "혹시..",
          "speaker_id": "1",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000770.1.34",
          "form": "민트는...",
          "original_form": "민트는...",
          "speaker_id": "1",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000770.1.35",
          "form": "저 완전 민초단이에요",
          "original_form": "저 완전 민초단이에요",
          "speaker_id": "2",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000770.1.36",
          "form": "어떻게 생각하시는지..",
          "original_form": "어떻게 생각하시는지..",
          "speaker_id": "1",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000770.1.37",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "1",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000770.1.38",
          "form": "역시 배우신분이야",
          "original_form": "역시 배우신분이야",
          "speaker_id": "1",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000770.1.39",
          "form": "최근에 나온 민초과자류? 거의 다 사먹었어요!!",
          "original_form": "최근에 나온 민초과자류? 거의 다 사먹었어요!!",
          "speaker_id": "2",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000770.1.40",
          "form": "저도 민초단 입니다",
          "original_form": "저도 민초단 입니다",
          "speaker_id": "1",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000770.1.41",
          "form": "민초가 세상을 지배한다",
          "original_form": "민초가 세상을 지배한다",
          "speaker_id": "2",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000770.1.42",
          "form": "와.. 그 정도에요?",
          "original_form": "와.. 그 정도에요?",
          "speaker_id": "1",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000770.1.43",
          "form": "김연경 선수도 민초단이래요",
          "original_form": "김연경 선수도 민초단이래요",
          "speaker_id": "2",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000770.1.44",
          "form": "저는 민초 입문한지 몇달 안됐습니다..",
          "original_form": "저는 민초 입문한지 몇달 안됐습니다..",
          "speaker_id": "1",
          "time": "20210819 19:15"
        },
        {
          "id": "MDRW2100000770.1.45",
          "form": "역시 멋진 사람들은...민초를 좋아해",
          "original_form": "역시 멋진 사람들은...민초를 좋아해",
          "speaker_id": "2",
          "time": "20210819 19:15"
        },
        {
          "id": "MDRW2100000770.1.46",
          "form": ". 민초단으로 넘어왔지요.",
          "original_form": ". 민초단으로 넘어왔지요.",
          "speaker_id": "1",
          "time": "20210819 19:15"
        },
        {
          "id": "MDRW2100000770.1.47",
          "form": "어떤거 먹고 입문햌ㅅ어요!?ㅋㅋㅋ",
          "original_form": "어떤거 먹고 입문햌ㅅ어요!?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:15"
        },
        {
          "id": "MDRW2100000770.1.48",
          "form": "저 당근 아이스크림이랑 그",
          "original_form": "저 당근 아이스크림이랑 그",
          "speaker_id": "1",
          "time": "20210819 19:16"
        },
        {
          "id": "MDRW2100000770.1.49",
          "form": "과자?",
          "original_form": "과자?",
          "speaker_id": "1",
          "time": "20210819 19:16"
        },
        {
          "id": "MDRW2100000770.1.50",
          "form": "민초볼이였나",
          "original_form": "민초볼이였나",
          "speaker_id": "1",
          "time": "20210819 19:16"
        },
        {
          "id": "MDRW2100000770.1.51",
          "form": "그리고 민트초코우유!",
          "original_form": "그리고 민트초코우유!",
          "speaker_id": "1",
          "time": "20210819 19:16"
        },
        {
          "id": "MDRW2100000770.1.52",
          "form": "제가 왜 이걸 안먹어나 싶더라구요",
          "original_form": "제가 왜 이걸 안먹어나 싶더라구요",
          "speaker_id": "1",
          "time": "20210819 19:16"
        },
        {
          "id": "MDRW2100000770.1.53",
          "form": "민초볼 맛있죠 ㅠㅠㅋㅋㅋ 민트초코송이 드셔보셨어요!? 이것두 맛잇어용!!",
          "original_form": "민초볼 맛있죠 ㅠㅠㅋㅋㅋ 민트초코송이 드셔보셨어요!? 이것두 맛잇어용!!",
          "speaker_id": "2",
          "time": "20210819 19:17"
        },
        {
          "id": "MDRW2100000770.1.54",
          "form": "아 진짜요? 아직 안먹어봤는데",
          "original_form": "아 진짜요? 아직 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210819 19:17"
        },
        {
          "id": "MDRW2100000770.1.55",
          "form": "먹어봐여겠어요!!",
          "original_form": "먹어봐여겠어요!!",
          "speaker_id": "1",
          "time": "20210819 19:17"
        },
        {
          "id": "MDRW2100000770.1.56",
          "form": "왕추천입니다!!",
          "original_form": "왕추천입니다!!",
          "speaker_id": "2",
          "time": "20210819 19:18"
        }
      ]
    }
