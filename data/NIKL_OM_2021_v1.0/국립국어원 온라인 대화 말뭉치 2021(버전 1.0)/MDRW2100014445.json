      "utterance": [
        {
          "id": "MDRW2100014445.1.1",
          "form": "혹시 재택근무 하시나요?",
          "original_form": "혹시 재택근무 하시나요?",
          "speaker_id": "1",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014445.1.2",
          "form": "재택 한번 했었어요ㅎㅎ 재택하세요?",
          "original_form": "재택 한번 했었어요ㅎㅎ 재택하세요?",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014445.1.3",
          "form": "아니오... 저도 안 해요...",
          "original_form": "아니오... 저도 안 해요...",
          "speaker_id": "1",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014445.1.4",
          "form": "코로나 검사 받고 대기할 때 하루 정도?",
          "original_form": "코로나 검사 받고 대기할 때 하루 정도?",
          "speaker_id": "1",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014445.1.5",
          "form": "아...ㅎㅎ 저희도 억지로 2주정도 하고 복귀하게 하더라구요ㅋㅋ",
          "original_form": "아...ㅎㅎ 저희도 억지로 2주정도 하고 복귀하게 하더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014445.1.6",
          "form": "물론 재택하면 끼니가 걱정이긴 한데",
          "original_form": "물론 재택하면 끼니가 걱정이긴 한데",
          "speaker_id": "1",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014445.1.7",
          "form": "그래도 통근 시간이 주니깐.... 하고 싶어요 재택",
          "original_form": "그래도 통근 시간이 주니깐.... 하고 싶어요 재택",
          "speaker_id": "1",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014445.1.8",
          "form": "ㅠㅠ진짜 편하다고 하던데..제주변에도 하는데 부럽더라구요",
          "original_form": "ㅠㅠ진짜 편하다고 하던데..제주변에도 하는데 부럽더라구요",
          "speaker_id": "2",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014445.1.9",
          "form": "주변에선 꽤 하는 것 같어요",
          "original_form": "주변에선 꽤 하는 것 같어요",
          "speaker_id": "1",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014445.1.10",
          "form": "전면은 아니도라도 일주일 한 두번만 출근하고ㅠㅠ",
          "original_form": "전면은 아니도라도 일주일 한 두번만 출근하고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014445.1.11",
          "form": "맞아요ㅠㅠ휴가같은 기분이라던데...아쉬워요",
          "original_form": "맞아요ㅠㅠ휴가같은 기분이라던데...아쉬워요",
          "speaker_id": "2",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014445.1.12",
          "form": "솔직히 저는 한편으로는",
          "original_form": "솔직히 저는 한편으로는",
          "speaker_id": "1",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100014445.1.13",
          "form": "나이 많으신 부장님들만 재택하셔도 좋을 것 같아요 ㅎㅎㅎㅎㅎ",
          "original_form": "나이 많으신 부장님들만 재택하셔도 좋을 것 같아요 ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100014445.1.14",
          "form": "ㅋㅋㅋ저는 오늘 다 외근이셔서 너무 행복해서 그맘알죠ㅋㅋ",
          "original_form": "ㅋㅋㅋ저는 오늘 다 외근이셔서 너무 행복해서 그맘알죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014445.1.15",
          "form": "정말 사무실에서 혼자 일하고 싶어요 ㅋㅋ",
          "original_form": "정말 사무실에서 혼자 일하고 싶어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014445.1.16",
          "form": "그 누구도 절 찾지 않고 ㅋㅋ",
          "original_form": "그 누구도 절 찾지 않고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014445.1.17",
          "form": "진짜 천국이죠ㅜㅜㅜ많이 오지않는...",
          "original_form": "진짜 천국이죠ㅜㅜㅜ많이 오지않는...",
          "speaker_id": "2",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014445.1.18",
          "form": "정말 일 하는 건 업무보다 사람문제인가봐요",
          "original_form": "정말 일 하는 건 업무보다 사람문제인가봐요",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014445.1.19",
          "form": "그쵸 다니다보면 사람문제더라구요ㅜㅜ",
          "original_form": "그쵸 다니다보면 사람문제더라구요ㅜㅜ",
          "speaker_id": "2",
