      "utterance": [
        {
          "id": "MDRW2100044191.1.1",
          "form": "저 ㅋㅋㅋㅋ",
          "original_form": "저 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044191.1.2",
          "form": "아직 강원랜드 안가봐서",
          "original_form": "아직 강원랜드 안가봐서",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044191.1.3",
          "form": "한번 가보고싶어요 ㅋㅋㅋ",
          "original_form": "한번 가보고싶어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044191.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044191.1.5",
          "form": "안가봤어요",
          "original_form": "안가봤어요",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044191.1.6",
          "form": "카지노요?",
          "original_form": "카지노요?",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044191.1.7",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044191.1.8",
          "form": "해외도 마카오랑 라스베가스쪽은 안가봤거든요",
          "original_form": "해외도 마카오랑 라스베가스쪽은 안가봤거든요",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044191.1.9",
          "form": "저도 가보고 싶어요 게임할 자신은.없지만요 ㅎ",
          "original_form": "저도 가보고 싶어요 게임할 자신은.없지만요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044191.1.10",
          "form": "그래서 한 번 가보고싶어요 ㅋㅋㅋ",
          "original_form": "그래서 한 번 가보고싶어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044191.1.11",
          "form": "저도요~ 카지노는 아예 한번도 안가봤어요",
          "original_form": "저도요~ 카지노는 아예 한번도 안가봤어요",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044191.1.12",
          "form": "강원랜드 카지노 가면",
          "original_form": "강원랜드 카지노 가면",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044191.1.13",
          "form": "룰렛머신마다 테마가 있어서",
          "original_form": "룰렛머신마다 테마가 있어서",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044191.1.14",
          "form": "거기 여권 있어야 가나요?",
          "original_form": "거기 여권 있어야 가나요?",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044191.1.15",
          "form": "효과음같은게 재밌다고 하더라구요",
          "original_form": "효과음같은게 재밌다고 하더라구요",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044191.1.16",
          "form": "그건 모르겠어요",
          "original_form": "그건 모르겠어요",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044191.1.17",
          "form": "안가보셨는데 많이 아시네요 많이 찾아보셨나봐요",
          "original_form": "안가보셨는데 많이 아시네요 많이 찾아보셨나봐요",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044191.1.18",
          "form": "요즘은 코로나라 전화로 예약받는대요",
          "original_form": "요즘은 코로나라 전화로 예약받는대요",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044191.1.19",
          "form": "주변에 최근에 갔다온 사람이 있어요 ㅋㅋㅋ",
          "original_form": "주변에 최근에 갔다온 사람이 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044191.1.20",
          "form": "우와!! 예약하고 가야 하다니",
          "original_form": "우와!! 예약하고 가야 하다니",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044191.1.21",
          "form": "재밌대요?",
          "original_form": "재밌대요?",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044191.1.22",
          "form": "돈 잃고 왔는데",
          "original_form": "돈 잃고 왔는데",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044191.1.23",
          "form": "한 번 가볼만은 한거같대요",
          "original_form": "한 번 가볼만은 한거같대요",
          "speaker_id": "2",
          "time": "20210907 21:54"
        }
      ]
