        },
        {
          "id": "MDRW2100011969.1.2",
          "form": "ㅋㅋㅋㅋㅋ저두요",
          "original_form": "ㅋㅋㅋㅋㅋ저두요",
          "speaker_id": "2",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011969.1.3",
          "form": "음 요즘은 맘스터치 좋아하는데",
          "original_form": "음 요즘은 맘스터치 좋아하는데",
          "speaker_id": "2",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011969.1.4",
          "form": "사실 다 좋아해요ㅎㅎ",
          "original_form": "사실 다 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011969.1.5",
          "form": "전 사실 햄버거도 가리지않아서 다 잘먹어요",
          "original_form": "전 사실 햄버거도 가리지않아서 다 잘먹어요",
          "speaker_id": "1",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011969.1.6",
          "form": "저두욬ㅋㅋㄲ",
          "original_form": "저두욬ㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011969.1.7",
          "form": "와 진짜 우리 한층 더 가까워졌네요",
          "original_form": "와 진짜 우리 한층 더 가까워졌네요",
          "speaker_id": "1",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011969.1.8",
          "form": "너무좋네요~~",
          "original_form": "너무좋네요~~",
          "speaker_id": "2",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011969.1.9",
          "form": "아까 민초따위 잊어버려요",
          "original_form": "아까 민초따위 잊어버려요",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011969.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ넼ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ넼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011969.1.11",
          "form": "그게 뭐가중요해 이렇게 좋아하는게 많은데",
          "original_form": "그게 뭐가중요해 이렇게 좋아하는게 많은데",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011969.1.12",
          "form": "요거트만 기억해요",
          "original_form": "요거트만 기억해요",
          "speaker_id": "2",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011969.1.13",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011969.1.14",
          "form": "ㅋㅋㅋㅋㅋ 당연하죠",
          "original_form": "ㅋㅋㅋㅋㅋ 당연하죠",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011969.1.15",
          "form": "좋아하는 버거있어요?",
          "original_form": "좋아하는 버거있어요?",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011969.1.16",
          "form": "요즘엔 싸이버거요!",
          "original_form": "요즘엔 싸이버거요!",
          "speaker_id": "2",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011969.1.17",
          "form": "어떤 버거좋아해요?",
          "original_form": "어떤 버거좋아해요?",
