      "utterance": [
        {
          "id": "MDRW2100017804.1.1",
          "form": "혹시 백신 맞으셨나요?",
          "original_form": "혹시 백신 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017804.1.2",
          "form": "아뇨 아직 학생이라 못 맞았어요 ㅠ",
          "original_form": "아뇨 아직 학생이라 못 맞았어요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017804.1.3",
          "form": "name1님은 맞으셨나요?",
          "original_form": "&name1&님은 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017804.1.4",
          "form": "아하~ 저는 2차까지 다맞았어요 ㅎㅎ",
          "original_form": "아하~ 저는 2차까지 다맞았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017804.1.5",
          "form": "많이 아픈가요? 사람마다 다 증상이 달라서요 ㅋㅋ",
          "original_form": "많이 아픈가요? 사람마다 다 증상이 달라서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017804.1.6",
          "form": "저는 운좋게도 1,2차 둘다 부작용없었어요 ㅎㅎ",
          "original_form": "저는 운좋게도 1,2차 둘다 부작용없었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017804.1.7",
          "form": "부럽네요 주변 사람들은 다 아프다해서 좀 걱정했는데 케바케인가봐요 ㅋㅋ",
          "original_form": "부럽네요 주변 사람들은 다 아프다해서 좀 걱정했는데 케바케인가봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017804.1.8",
          "form": "네 ㅋㅋ 완전 사바사인거같더라구요 ㅋㅋ",
          "original_form": "네 ㅋㅋ 완전 사바사인거같더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017804.1.9",
          "form": "혹시 운동하시는거 있나요?",
          "original_form": "혹시 운동하시는거 있나요?",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017804.1.10",
          "form": "저는 운동안해요.. 땀나는거 싫어해서요ㅠ ㅋㅋㅋ",
          "original_form": "저는 운동안해요.. 땀나는거 싫어해서요ㅠ ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017804.1.11",
          "form": "아 그러시구나 저는 요즘 링피트하고 있는데",
          "original_form": "아 그러시구나 저는 요즘 링피트하고 있는데",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017804.1.12",
          "form": "이게 생각보다 힘들더라구요 ㅋㅋㅋ",
          "original_form": "이게 생각보다 힘들더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017804.1.13",
          "form": "방송이나 그런거에서 볼땐 안힘들어보였는데 하고나면 항상 땀범벅이 되서 ㅋㅋㅋㅋ",
          "original_form": "방송이나 그런거에서 볼땐 안힘들어보였는데 하고나면 항상 땀범벅이 되서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017804.1.14",
          "form": "오호랏!! 링피트! 재밌겠어요 추천할만한가요??",
          "original_form": "오호랏!! 링피트! 재밌겠어요 추천할만한가요??",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017804.1.15",
          "form": "네! 확실히 운동도 되고 재미도있어서 할만해요 ㅎㅎ",
          "original_form": "네! 확실히 운동도 되고 재미도있어서 할만해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017804.1.16",
          "form": "저는 최근에 그나마 자전거 타려고했는데 날씨가 안도와주네요",
          "original_form": "저는 최근에 그나마 자전거 타려고했는데 날씨가 안도와주네요",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017804.1.17",
          "form": "아~ 자전거도 재밌죠!",
          "original_form": "아~ 자전거도 재밌죠!",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017804.1.18",
          "form": "근데 요즘은 비도 많이오고 그전엔 무지하게 더웠고 자전거 탈 날씨는 아니었죠 ㅠㅠ",
          "original_form": "근데 요즘은 비도 많이오고 그전엔 무지하게 더웠고 자전거 탈 날씨는 아니었죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017804.1.19",
          "form": "맞아요 별로 안힘들고 재밌어요~ 그쵸 여기부산이라 물난리때문에 여간 힘든게아니었어요",
          "original_form": "맞아요 별로 안힘들고 재밌어요~ 그쵸 여기부산이라 물난리때문에 여간 힘든게아니었어요",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017804.1.20",
          "form": "힘들었겠네요 ㅠ",
          "original_form": "힘들었겠네요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017804.1.21",
          "form": "그래도 앞으론 날씨도 풀릴거니깐 자전거도 맘껏 탈수있겠어요 ㅎㅎ",
          "original_form": "그래도 앞으론 날씨도 풀릴거니깐 자전거도 맘껏 탈수있겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017804.1.22",
          "form": "ㅋㅋ 아파트살아서 딱히 제가피해본건없었지만 도로나가면 첨벙첨벙 하더라고요",
          "original_form": "ㅋㅋ 아파트살아서 딱히 제가피해본건없었지만 도로나가면 첨벙첨벙 하더라고요",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017804.1.23",
          "form": "맞아요! 오늘은 날씨 참좋긴했어요!",
          "original_form": "맞아요! 오늘은 날씨 참좋긴했어요!",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017804.1.24",
          "form": "그러게요 이젠 곧 가을이네요",
          "original_form": "그러게요 이젠 곧 가을이네요",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017804.1.25",
          "form": "시간이 어찌나 빨리가던지 ㅠ",
          "original_form": "시간이 어찌나 빨리가던지 ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:38"
        }
      ]
    }
