      "utterance": [
        {
          "id": "MDRW2100039760.1.1",
          "form": "게임하세요?",
          "original_form": "게임하세요?",
          "speaker_id": "2",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039760.1.2",
          "form": "음 저 한동안 심즈 하다가 지금은 딱히 안하고 있어요!",
          "original_form": "음 저 한동안 심즈 하다가 지금은 딱히 안하고 있어요!",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039760.1.3",
          "form": "게임 하시나요?",
          "original_form": "게임 하시나요?",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039760.1.4",
          "form": "저는 모바일게임만 가끔해요",
          "original_form": "저는 모바일게임만 가끔해요",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039760.1.5",
          "form": "캐쥬얼 게임같은 간단한것만요",
          "original_form": "캐쥬얼 게임같은 간단한것만요",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039760.1.6",
          "form": "캐쥬얼 게임이 재밌죠 ㅎㅎ",
          "original_form": "캐쥬얼 게임이 재밌죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039760.1.7",
          "form": "저도 모바일을 종종 하는 것 같아요",
          "original_form": "저도 모바일을 종종 하는 것 같아요",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039760.1.8",
          "form": "추천하실만한 모바일 게임있으시나요?",
          "original_form": "추천하실만한 모바일 게임있으시나요?",
          "speaker_id": "1",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039760.1.9",
          "form": "요즘에는 toon blast이거만해요",
          "original_form": "요즘에는 toon blast이거만해요",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039760.1.10",
          "form": "퍼즐깨기 같은거예요",
          "original_form": "퍼즐깨기 같은거예요",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039760.1.11",
          "form": "오 한번 구경해봐야겠네요!",
          "original_form": "오 한번 구경해봐야겠네요!",
          "speaker_id": "1",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039760.1.12",
          "form": "그냥 심심풀이 게임이예요",
          "original_form": "그냥 심심풀이 게임이예요",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039760.1.13",
          "form": "저는 방탈출게임을 모바일로 하는게 그렇게 재밌더라구요",
          "original_form": "저는 방탈출게임을 모바일로 하는게 그렇게 재밌더라구요",
          "speaker_id": "1",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039760.1.14",
          "form": "방탈출 좋죠",
          "original_form": "방탈출 좋죠",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039760.1.15",
          "form": "재밌는거 있나요?",
          "original_form": "재밌는거 있나요?",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039760.1.16",
          "form": "그냥 앱스토에 방탈출게임 검색하면 일본 회사에서 만든 게임이 재밌더라고요",
          "original_form": "그냥 앱스토에 방탈출게임 검색하면 일본 회사에서 만든 게임이 재밌더라고요",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039760.1.17",
          "form": "예쁘고 아기자기한 느낌이에요!",
          "original_form": "예쁘고 아기자기한 느낌이에요!",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039760.1.18",
          "form": "어 친구가 추천해줘서 한거있는데",
          "original_form": "어 친구가 추천해줘서 한거있는데",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039760.1.19",
          "form": "길잃은 고양이였나 ᆢ?",
          "original_form": "길잃은 고양이였나 ᆢ?",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039760.1.20",
          "form": "혹시 이거해보셨어요?",
          "original_form": "혹시 이거해보셨어요?",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039760.1.21",
          "form": "음 제목은 잘 기억 안나는데 처음듣는 게임 같아요!",
          "original_form": "음 제목은 잘 기억 안나는데 처음듣는 게임 같아요!",
          "speaker_id": "1",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039760.1.22",
          "form": "한 번 찾아봐야겠네요",
          "original_form": "한 번 찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039760.1.23",
          "form": "아기자기해요",
          "original_form": "아기자기해요",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039760.1.24",
          "form": "디자인이",
          "original_form": "디자인이",
          "speaker_id": "2",
          "time": "20210903 21:37"
        }
      ]
    }
