      "utterance": [
        {
          "id": "MDRW2100040252.1.1",
          "form": "여행도 좋아하시죠?",
          "original_form": "여행도 좋아하시죠?",
          "speaker_id": "2",
          "time": "20210905 08:36"
        },
        {
          "id": "MDRW2100040252.1.2",
          "form": "넹넹 일상에서 잠시 벗어나는 건 다 신나는 일이져",
          "original_form": "넹넹 일상에서 잠시 벗어나는 건 다 신나는 일이져",
          "speaker_id": "1",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100040252.1.3",
          "form": "ㅋㅋ 맞아요 주로 활동적인 여행 좋아하서요? 휴양이 목적이셔요?",
          "original_form": "ㅋㅋ 맞아요 주로 활동적인 여행 좋아하서요? 휴양이 목적이셔요?",
          "speaker_id": "2",
          "time": "20210905 08:40"
        },
        {
          "id": "MDRW2100040252.1.4",
          "form": "전 활동을 좋아해요ㅋㅋㅋ 뭔가 늘어져있으면 아까운 기분이 들어서",
          "original_form": "전 활동을 좋아해요ㅋㅋㅋ 뭔가 늘어져있으면 아까운 기분이 들어서",
          "speaker_id": "1",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100040252.1.5",
          "form": "저도 멀리갔음 보긴해야지 하는데 휴양이 이젠 더 좋구ㅋㅋ",
          "original_form": "저도 멀리갔음 보긴해야지 하는데 휴양이 이젠 더 좋구ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100040252.1.6",
          "form": "어디가셨었어요?",
          "original_form": "어디가셨었어요?",
          "speaker_id": "2",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100040252.1.7",
          "form": "전 아직 휴양지를 안다녀서 그런가봐요",
          "original_form": "전 아직 휴양지를 안다녀서 그런가봐요",
          "speaker_id": "1",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100040252.1.8",
          "form": "해외는 도쿄 타이페이 베이징 이렇게 볼거리많은 수도 위주 ㅋㅋ",
          "original_form": "해외는 도쿄 타이페이 베이징 이렇게 볼거리많은 수도 위주 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:58"
        },
        {
          "id": "MDRW2100040252.1.9",
          "form": "국내도 서울 여행 젤 좋아하거든요",
          "original_form": "국내도 서울 여행 젤 좋아하거든요",
          "speaker_id": "1",
          "time": "20210905 08:58"
        },
        {
          "id": "MDRW2100040252.1.10",
          "form": "아 그럼 돌아보기 편해요!",
          "original_form": "아 그럼 돌아보기 편해요!",
          "speaker_id": "2",
          "time": "20210905 08:59"
        },
        {
          "id": "MDRW2100040252.1.11",
          "form": "베이징도 자유로 가셨어요?",
          "original_form": "베이징도 자유로 가셨어요?",
          "speaker_id": "2",
          "time": "20210905 08:59"
        },
        {
          "id": "MDRW2100040252.1.12",
          "form": "나머지는 다 자유였는데 베이징은 단체였어요ㅋㅋㅋ",
          "original_form": "나머지는 다 자유였는데 베이징은 단체였어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100040252.1.13",
          "form": "아 저도 패키지로 갔었어요!",
          "original_form": "아 저도 패키지로 갔었어요!",
          "speaker_id": "2",
          "time": "20210905 09:44"
        },
        {
          "id": "MDRW2100040252.1.14",
          "form": "네ㅋㅋㅋ아무래도 중국은 패키지로 가야할거같더라구요 ㅋㅋ",
          "original_form": "네ㅋㅋㅋ아무래도 중국은 패키지로 가야할거같더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 09:48"
        },
        {
          "id": "MDRW2100040252.1.15",
          "form": "호텔이 제일 좋았어요",
          "original_form": "호텔이 제일 좋았어요",
          "speaker_id": "2",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100040252.1.16",
          "form": "앜ㅋㅋㅋ 어디가셧는데옹",
          "original_form": "앜ㅋㅋㅋ 어디가셧는데옹",
          "speaker_id": "1",
          "time": "20210905 10:53"
        },
        {
          "id": "MDRW2100040252.1.17",
          "form": "북경패키지요ㅋㅋ",
          "original_form": "북경패키지요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:48"
        },
        {
          "id": "MDRW2100040252.1.18",
          "form": "아침 7시부터 패키지도니까ㅋㅋ 힘들어요",
          "original_form": "아침 7시부터 패키지도니까ㅋㅋ 힘들어요",
          "speaker_id": "2",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100040252.1.19",
          "form": "나름 자금성리나 만리장성 볼만하지 않았나요 ㅋㅋㅋ",
          "original_form": "나름 자금성리나 만리장성 볼만하지 않았나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:50"
        },
        {
          "id": "MDRW2100040252.1.20",
          "form": "볼만했는데 삼십분보고 이동하니까요ㅋㅋ 벌써 십년전ㅋㅋ",
          "original_form": "볼만했는데 삼십분보고 이동하니까요ㅋㅋ 벌써 십년전ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:51"
        }
      ]
    }
  ]
