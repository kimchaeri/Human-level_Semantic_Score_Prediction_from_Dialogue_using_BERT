      "utterance": [
        {
          "id": "MDRW2100028311.1.1",
          "form": "근데 아까 그건 너무하셨어요",
          "original_form": "근데 아까 그건 너무하셨어요",
          "speaker_id": "2",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100028311.1.2",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "1",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100028311.1.3",
          "form": "저 결혼시키려고 키우셨다는 얘기!!",
          "original_form": "저 결혼시키려고 키우셨다는 얘기!!",
          "speaker_id": "2",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100028311.1.4",
          "form": "손자 보고 싶어서 그런거지",
          "original_form": "손자 보고 싶어서 그런거지",
          "speaker_id": "1",
          "time": "20210902 19:03"
        },
        {
          "id": "MDRW2100028311.1.5",
          "form": "다들 그런거 아니겠니",
          "original_form": "다들 그런거 아니겠니",
          "speaker_id": "1",
          "time": "20210902 19:03"
        },
        {
          "id": "MDRW2100028311.1.6",
          "form": "요즘은 다르다구요 ㅠㅠ",
          "original_form": "요즘은 다르다구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 19:03"
        },
        {
          "id": "MDRW2100028311.1.7",
          "form": "다들 결혼 했는데 주변에",
          "original_form": "다들 결혼 했는데 주변에",
          "speaker_id": "1",
          "time": "20210902 19:03"
        },
        {
          "id": "MDRW2100028311.1.8",
          "form": "요즘에는 혼자 사는 사람들도 대세예요",
          "original_form": "요즘에는 혼자 사는 사람들도 대세예요",
          "speaker_id": "2",
          "time": "20210902 19:03"
        },
        {
          "id": "MDRW2100028311.1.9",
          "form": "그러니?",
          "original_form": "그러니?",
          "speaker_id": "1",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100028311.1.10",
          "form": "그래도 사람하고 같이 사는게 좋단다",
          "original_form": "그래도 사람하고 같이 사는게 좋단다",
          "speaker_id": "1",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100028311.1.11",
          "form": "그건 그렇지만요...",
          "original_form": "그건 그렇지만요...",
          "speaker_id": "2",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100028311.1.12",
          "form": "너 쓰러지면 누가 연락해주니 따로살면",
          "original_form": "너 쓰러지면 누가 연락해주니 따로살면",
          "speaker_id": "1",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100028311.1.13",
          "form": "친구 번호 알려드렸잖아요!!",
          "original_form": "친구 번호 알려드렸잖아요!!",
          "speaker_id": "2",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100028311.1.14",
          "form": "나중에는 어떻게 될지 모르니까",
          "original_form": "나중에는 어떻게 될지 모르니까",
          "speaker_id": "1",
          "time": "20210902 19:04"
        },
        {
