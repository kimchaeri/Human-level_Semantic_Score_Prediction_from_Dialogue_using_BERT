      "utterance": [
        {
          "id": "MDRW2100005576.1.1",
          "form": "나는 한국 작가 김진명 책 좋아해",
          "original_form": "나는 한국 작가 김진명 책 좋아해",
          "speaker_id": "2",
          "time": "20210821 23:09"
        },
        {
          "id": "MDRW2100005576.1.2",
          "form": "고구려?",
          "original_form": "고구려?",
          "speaker_id": "1",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005576.1.3",
          "form": "어 아네?",
          "original_form": "어 아네?",
          "speaker_id": "2",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005576.1.4",
          "form": "엄청 유명하잖아",
          "original_form": "엄청 유명하잖아",
          "speaker_id": "1",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005576.1.5",
          "form": "무궁화꽃이 피었습니다 엄청 유명했는데",
          "original_form": "무궁화꽃이 피었습니다 엄청 유명했는데",
          "speaker_id": "2",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005576.1.6",
          "form": "내가 빠지게된 계기 ㅋㅋ",
          "original_form": "내가 빠지게된 계기 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005576.1.7",
          "form": "나도 본 거 같은데 기억이 안난다",
          "original_form": "나도 본 거 같은데 기억이 안난다",
          "speaker_id": "1",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005576.1.8",
          "form": "김진명 문체가 너무 매력적이고 역사 소설 이야기가 재미있어",
          "original_form": "김진명 문체가 너무 매력적이고 역사 소설 이야기가 재미있어",
          "speaker_id": "2",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005576.1.9",
          "form": "역사소설 오",
          "original_form": "역사소설 오",
          "speaker_id": "1",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005576.1.10",
          "form": "뭔가 허구인데 진짜 있을 법하게 이야기를 써서",
          "original_form": "뭔가 허구인데 진짜 있을 법하게 이야기를 써서",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005576.1.11",
          "form": "듣기만 해도 흥미로운걸",
          "original_form": "듣기만 해도 흥미로운걸",
          "speaker_id": "1",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005576.1.12",
          "form": "한 달 내내 쉬면서 책만 읽었음 좋겠다",
          "original_form": "한 달 내내 쉬면서 책만 읽었음 좋겠다",
          "speaker_id": "2",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005576.1.13",
          "form": "ㅋㅋㅋㅋ 책 좋아하구나",
          "original_form": "ㅋㅋㅋㅋ 책 좋아하구나",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005576.1.14",
          "form": "좋아하긴 한데 읽을 시간이 별로 없어 ㅋㅋ",
          "original_form": "좋아하긴 한데 읽을 시간이 별로 없어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:13"
        }
      ]
