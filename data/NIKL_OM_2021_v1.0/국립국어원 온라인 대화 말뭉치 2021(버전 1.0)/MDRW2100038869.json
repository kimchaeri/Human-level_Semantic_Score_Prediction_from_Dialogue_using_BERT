      "utterance": [
        {
          "id": "MDRW2100038869.1.1",
          "form": "챙겨보는 프로그램 있으세요?",
          "original_form": "챙겨보는 프로그램 있으세요?",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100038869.1.2",
          "form": "전개훌륭이요",
          "original_form": "전개훌륭이요",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100038869.1.3",
          "form": "강형욱 나오는거용ㅋㅋ",
          "original_form": "강형욱 나오는거용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100038869.1.4",
          "form": "전 방금 놀뭐 봤는데 엄청 웃기네요 ㅎㅎㅎ",
          "original_form": "전 방금 놀뭐 봤는데 엄청 웃기네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100038869.1.5",
          "form": "요즘 놀뭐엔 뭐나오나요?",
          "original_form": "요즘 놀뭐엔 뭐나오나요?",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100038869.1.6",
          "form": "이번엔  10분전에 뉴스 진행한다는 사실 알려주고",
          "original_form": "이번엔  10분전에 뉴스 진행한다는 사실 알려주고",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100038869.1.7",
          "form": "허겁지겁 준비한 후 뉴스 진행해요 ㅋㅋ",
          "original_form": "허겁지겁 준비한 후 뉴스 진행해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100038869.1.8",
          "form": "무한도전때ㅐ도 뉴스 진행을 했던 것 같은데",
          "original_form": "무한도전때ㅐ도 뉴스 진행을 했던 것 같은데",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100038869.1.9",
          "form": "그 때 생각이 나네요",
          "original_form": "그 때 생각이 나네요",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100038869.1.10",
          "form": "그랬었나요? 이번엔 뭐 하는지도 모르고 와요 ㅋㅋ",
          "original_form": "그랬었나요? 이번엔 뭐 하는지도 모르고 와요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100038869.1.11",
          "form": "네 무도가 워낙 오래했다보니 어지간한건 다 했었어요 ㅋㅋ",
          "original_form": "네 무도가 워낙 오래했다보니 어지간한건 다 했었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100038869.1.12",
          "form": "맞아요. 놀뭐도 그냥 무한도전의 연장선같아요 ㅎ",
          "original_form": "맞아요. 놀뭐도 그냥 무한도전의 연장선같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100038869.1.13",
          "form": "유재석이 무도피디의 최애더니,,",
          "original_form": "유재석이 무도피디의 최애더니,,",
          "speaker_id": "1",
          "time": "20210902 22:31"
        },
        {
          "id": "MDRW2100038869.1.14",
          "form": "가지를 쳐낸 무도 같은 느낌이네요",
          "original_form": "가지를 쳐낸 무도 같은 느낌이네요",
          "speaker_id": "1",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100038869.1.15",
          "form": "맞아요 유재석 외 다른 멤버들이 없는 무도예요. 멤버 구성이 자유로운 무도느낌?",
          "original_form": "맞아요 유재석 외 다른 멤버들이 없는 무도예요. 멤버 구성이 자유로운 무도느낌?",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100038869.1.16",
          "form": "김태호 피디 무도 다른멤버들 구박할때가 살ㅉ가 있었는데 ㅋㅋ",
          "original_form": "김태호 피디 무도 다른멤버들 구박할때가 살ㅉ가 있었는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100038869.1.17",
          "form": "완전 자유로워졌네요,,,",
          "original_form": "완전 자유로워졌네요,,,",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100038869.1.18",
          "form": "맞아요 유재석 마음대로 구성하는것 같죠?",
          "original_form": "맞아요 유재석 마음대로 구성하는것 같죠?",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100038869.1.19",
          "form": "네 ㅋㅋ그래서 더 유재석에게 권력이 집중되네요",
          "original_form": "네 ㅋㅋ그래서 더 유재석에게 권력이 집중되네요",
          "speaker_id": "1",
          "time": "20210902 22:35"
        }
      ]
