      "utterance": [
        {
          "id": "MDRW2100019416.1.1",
          "form": "싸이월드..!!",
          "original_form": "싸이월드..!!",
          "speaker_id": "1",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100019416.1.2",
          "form": "부활을 기다리셨나요..!!",
          "original_form": "부활을 기다리셨나요..!!",
          "speaker_id": "2",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100019416.1.3",
          "form": "저는...아니요!!ㅋㅋㅋ근데 궁금하긴해요",
          "original_form": "저는...아니요!!ㅋㅋㅋ근데 궁금하긴해요",
          "speaker_id": "1",
          "time": "20210827 19:13"
        },
        {
          "id": "MDRW2100019416.1.4",
          "form": "아이디 찾으셨어요?",
          "original_form": "아이디 찾으셨어요?",
          "speaker_id": "1",
          "time": "20210827 19:13"
        },
        {
          "id": "MDRW2100019416.1.5",
          "form": "정식오픈하면 찾아보려구요!",
          "original_form": "정식오픈하면 찾아보려구요!",
          "speaker_id": "2",
          "time": "20210827 19:16"
        },
        {
          "id": "MDRW2100019416.1.6",
          "form": "탈퇴를 했던 것 같은데 망설였던 것 같기도하고...",
          "original_form": "탈퇴를 했던 것 같은데 망설였던 것 같기도하고...",
          "speaker_id": "2",
          "time": "20210827 19:16"
        },
        {
          "id": "MDRW2100019416.1.7",
          "form": "부활해도 사진 데이터는 다 없었으면 좋겠네요 ㅋㅋㅋ",
          "original_form": "부활해도 사진 데이터는 다 없었으면 좋겠네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:16"
        },
        {
          "id": "MDRW2100019416.1.8",
          "form": "사진이 다 남아있다는것 같더라구요",
          "original_form": "사진이 다 남아있다는것 같더라구요",
          "speaker_id": "1",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019416.1.9",
          "form": "맛보기로 한장 보여주던데 놀랐어요",
          "original_form": "맛보기로 한장 보여주던데 놀랐어요",
          "speaker_id": "1",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019416.1.10",
          "form": "오 마이 갓...",
          "original_form": "오 마이 갓...",
          "speaker_id": "2",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019416.1.11",
          "form": "아이디 살아있나 확인해보기 더 무서워졌어요 ㅎㅎ",
          "original_form": "아이디 살아있나 확인해보기 더 무서워졌어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019416.1.12",
          "form": "무분별하게 퍼져간 퍼가요 사진들 때문에 좀 없어졌으면 했는데ㅠ",
          "original_form": "무분별하게 퍼져간 퍼가요 사진들 때문에 좀 없어졌으면 했는데ㅠ",
          "speaker_id": "2",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019416.1.13",
          "form": "ㅠㅜㅎㅎ흑역사가 공개되는거죠...",
          "original_form": "ㅠㅜㅎㅎ흑역사가 공개되는거죠...",
          "speaker_id": "1",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100019416.1.14",
          "form": "아.. 사회적 파란이 예상됩니다 ㅎㅎ",
          "original_form": "아.. 사회적 파란이 예상됩니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019416.1.15",
          "form": "미니미들은 열심히 꾸몄던 기억이 있는데 얼른 보고싶기도하네용",
          "original_form": "미니미들은 열심히 꾸몄던 기억이 있는데 얼른 보고싶기도하네용",
          "speaker_id": "1",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019416.1.16",
          "form": "도토리 한푼 두푼 사서 ...",
          "original_form": "도토리 한푼 두푼 사서 ...",
          "speaker_id": "1",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019416.1.17",
          "form": "맞아요 가구 넣고 벽지 바꾸고 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 가구 넣고 벽지 바꾸고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019416.1.18",
          "form": "온 방 안에 미니미 다 집어 넣고 ㅋㅋㅋ",
          "original_form": "온 방 안에 미니미 다 집어 넣고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019416.1.19",
          "form": "ㅋㅋㅋ아기자기한거 좋아해서 다시 오픈해도 할거같아요",
          "original_form": "ㅋㅋㅋ아기자기한거 좋아해서 다시 오픈해도 할거같아요",
          "speaker_id": "1",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019416.1.20",
          "form": "예전 기억에 미니미 아이템이 진짜 많았던 거 같아요",
          "original_form": "예전 기억에 미니미 아이템이 진짜 많았던 거 같아요",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100019416.1.21",
          "form": "그거 다시 귀엽게 잘 살려도 게임하는 것 같고 재밌겠어요 ㅎ",
          "original_form": "그거 다시 귀엽게 잘 살려도 게임하는 것 같고 재밌겠어요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100019416.1.22",
          "form": "개인적으로 싸이 특유의 감성문구들은... 사라졌으면 합니다 ㅋㅋㅋ",
          "original_form": "개인적으로 싸이 특유의 감성문구들은... 사라졌으면 합니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019416.1.23",
