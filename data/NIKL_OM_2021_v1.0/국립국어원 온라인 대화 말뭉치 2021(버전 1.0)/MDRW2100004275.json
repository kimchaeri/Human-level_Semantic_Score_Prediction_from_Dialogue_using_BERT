      "utterance": [
        {
          "id": "MDRW2100004275.1.1",
          "form": "반려동물 키우시나요??",
          "original_form": "반려동물 키우시나요??",
          "speaker_id": "1",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004275.1.2",
          "form": "아니요 강아지 좋아하는데 여건이 안되네요",
          "original_form": "아니요 강아지 좋아하는데 여건이 안되네요",
          "speaker_id": "2",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004275.1.3",
          "form": "키우시는 동물있으세요?",
          "original_form": "키우시는 동물있으세요?",
          "speaker_id": "2",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004275.1.4",
          "form": "맞아요 저도 여건이 안되다보니ㅠㅠ",
          "original_form": "맞아요 저도 여건이 안되다보니ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004275.1.5",
          "form": "지금은 없는데",
          "original_form": "지금은 없는데",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004275.1.6",
          "form": "2년전쯤에",
          "original_form": "2년전쯤에",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004275.1.7",
          "form": "햄스터 키웠었어요",
          "original_form": "햄스터 키웠었어요",
          "speaker_id": "1",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004275.1.8",
          "form": "오 귀엽겠어요 ㅎㅎ",
          "original_form": "오 귀엽겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004275.1.9",
          "form": "네네 펄햄스터라고 흰색인데 정말 귀여웠어요",
          "original_form": "네네 펄햄스터라고 흰색인데 정말 귀여웠어요",
          "speaker_id": "1",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004275.1.10",
          "form": "간식 주면 잘먹고 그랬어요",
          "original_form": "간식 주면 잘먹고 그랬어요",
          "speaker_id": "1",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004275.1.11",
          "form": "애기낳음 분리해야된다는데",
          "original_form": "애기낳음 분리해야된다는데",
          "speaker_id": "2",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004275.1.12",
          "form": "진짜 그런가요?",
          "original_form": "진짜 그런가요?",
          "speaker_id": "2",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004275.1.13",
          "form": "맞아요 스트레스받으면 애기 잡아먹어서..",
          "original_form": "맞아요 스트레스받으면 애기 잡아먹어서..",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004275.1.14",
          "form": "무섭네요 애기를 잡아먹다니",
          "original_form": "무섭네요 애기를 잡아먹다니",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004275.1.15",
          "form": "그쵸 ㅜㅜ 근데 전 한마리만 키워서 괜찮았어유",
          "original_form": "그쵸 ㅜㅜ 근데 전 한마리만 키워서 괜찮았어유",
          "speaker_id": "1",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004275.1.16",
          "form": "먹이 먹는거보면 넘 귀여울거같아요ㅎㅎ",
          "original_form": "먹이 먹는거보면 넘 귀여울거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004275.1.17",
          "form": "맞아요 과일 줘도 잘먹어요",
          "original_form": "맞아요 과일 줘도 잘먹어요",
          "speaker_id": "1",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004275.1.18",
          "form": "잡식인가보네요 ㅎ 과일도먹고",
          "original_form": "잡식인가보네요 ㅎ 과일도먹고",
          "speaker_id": "2",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004275.1.19",
          "form": "맞아요 잡식!!",
          "original_form": "맞아요 잡식!!",
          "speaker_id": "1",
          "time": "20210821 16:04"
        }
      ]
