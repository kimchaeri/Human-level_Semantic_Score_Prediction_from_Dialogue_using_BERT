      "utterance": [
        {
          "id": "MDRW2100036485.1.1",
          "form": "요즘은 안갔는데",
          "original_form": "요즘은 안갔는데",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036485.1.2",
          "form": "빽다방 신메뉴인가? 몽땅쉐이크를 먹어봤어",
          "original_form": "빽다방 신메뉴인가? 몽땅쉐이크를 먹어봤어",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036485.1.3",
          "form": "이제 가까워져서 한번 가볼까해",
          "original_form": "이제 가까워져서 한번 가볼까해",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036485.1.4",
          "form": "그게 니 추천메뉴야?",
          "original_form": "그게 니 추천메뉴야?",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036485.1.5",
          "form": "아이스크림이랑 커피를 갈아줘",
          "original_form": "아이스크림이랑 커피를 갈아줘",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036485.1.6",
          "form": "응 근데 비싸",
          "original_form": "응 근데 비싸",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036485.1.7",
          "form": "빽다방은 원래 싸잖아",
          "original_form": "빽다방은 원래 싸잖아",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036485.1.8",
          "form": "유와 이름만 들으면 엄청 맛있어보여",
          "original_form": "유와 이름만 들으면 엄청 맛있어보여",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036485.1.9",
          "form": "근데 그건 4000원이야",
          "original_form": "근데 그건 4000원이야",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036485.1.10",
          "form": "그거 마시면 다음날은 커피 마시지마",
          "original_form": "그거 마시면 다음날은 커피 마시지마",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036485.1.11",
          "form": "흠 니가 생각하기엔",
          "original_form": "흠 니가 생각하기엔",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036485.1.12",
          "form": "비싼거 마셨으니까.",
          "original_form": "비싼거 마셨으니까.",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036485.1.13",
          "form": "그 값어치를 하는 것 같아?",
          "original_form": "그 값어치를 하는 것 같아?",
          "speaker_id": "1",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036485.1.14",
          "form": "나는 몽땅쉐이크 모카를 마셨어",
          "original_form": "나는 몽땅쉐이크 모카를 마셨어",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036485.1.15",
          "form": "응 달아",
          "original_form": "응 달아",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036485.1.16",
          "form": "아주 현명한 소비형태야",
          "original_form": "아주 현명한 소비형태야",
          "speaker_id": "1",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036485.1.17",
          "form": "양이 많아",
          "original_form": "양이 많아",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036485.1.18",
          "form": "위에는 아이스크림이야",
          "original_form": "위에는 아이스크림이야",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036485.1.19",
          "form": "일단 떠먿어",
          "original_form": "일단 떠먿어",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036485.1.20",
          "form": "아포가토같아",
          "original_form": "아포가토같아",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036485.1.21",
          "form": "근데 그거한번에 다 먹을 수 있어?",
          "original_form": "근데 그거한번에 다 먹을 수 있어?",
          "speaker_id": "1",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036485.1.22",
          "form": "맛있어",
          "original_form": "맛있어",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036485.1.23",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036485.1.24",
          "form": "반 남겼어",
          "original_form": "반 남겼어",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036485.1.25",
          "form": "버렸어ㅠ 아까워",
          "original_form": "버렸어ㅠ 아까워",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036485.1.26",
          "form": "그런건 남기면 맛없어지잖아",
          "original_form": "그런건 남기면 맛없어지잖아",
          "speaker_id": "1",
          "time": "20210830 23:43"
        }
      ]
