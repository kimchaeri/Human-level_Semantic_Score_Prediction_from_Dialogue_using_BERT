      "utterance": [
        {
          "id": "MDRW2100020936.1.1",
          "form": "저도 엽떡은 기본맛만",
          "original_form": "저도 엽떡은 기본맛만",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020936.1.2",
          "form": "아하 ㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020936.1.3",
          "form": "기본맛도 매운거같아요",
          "original_form": "기본맛도 매운거같아요",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020936.1.4",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "1",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020936.1.5",
          "form": "매운맛 드시는분들 신기해요",
          "original_form": "매운맛 드시는분들 신기해요",
          "speaker_id": "1",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020936.1.6",
          "form": "완전매운건 저도 속아파서리 ㅎㅎ",
          "original_form": "완전매운건 저도 속아파서리 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020936.1.7",
          "form": "죽죠 ㅠㅠ",
          "original_form": "죽죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020936.1.8",
          "form": "매운음식 아예 못드세요?",
          "original_form": "매운음식 아예 못드세요?",
          "speaker_id": "2",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020936.1.9",
          "form": "불닭볶음면 전에 좋아해서 자주 먹었는데",
          "original_form": "불닭볶음면 전에 좋아해서 자주 먹었는데",
          "speaker_id": "1",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020936.1.10",
          "form": "못 먹는다기보다 속이 쓰려서 피해요 ㅋㅋㅋ",
          "original_form": "못 먹는다기보다 속이 쓰려서 피해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020936.1.11",
          "form": "아맛있죠",
          "original_form": "아맛있죠",
          "speaker_id": "2",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020936.1.12",
          "form": "인기많져",
          "original_form": "인기많져",
          "speaker_id": "1",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020936.1.13",
          "form": "네 저도 매워서 치즈넣어먹어요",
          "original_form": "네 저도 매워서 치즈넣어먹어요",
          "speaker_id": "2",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100020936.1.14",
          "form": "치즈필수죠",
          "original_form": "치즈필수죠",
          "speaker_id": "1",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100020936.1.15",
          "form": "그 떡볶이 양념 가득 묻혀진 치즈 ㅜㅜ",
          "original_form": "그 떡볶이 양념 가득 묻혀진 치즈 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100020936.1.16",
          "form": "네 진짜 치즈넣으면 맛있죠",
          "original_form": "네 진짜 치즈넣으면 맛있죠",
          "speaker_id": "2",
          "time": "20210828 14:19"
        }
