      "utterance": [
        {
          "id": "MDRW2100032106.1.1",
          "form": "오프는 이마트구",
          "original_form": "오프는 이마트구",
          "speaker_id": "2",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100032106.1.2",
          "form": "온라인은 11번가요",
          "original_form": "온라인은 11번가요",
          "speaker_id": "2",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100032106.1.3",
          "form": "전 오프는 롯데마트 고요 온라인은 쿠팡을 해요",
          "original_form": "전 오프는 롯데마트 고요 온라인은 쿠팡을 해요",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100032106.1.4",
          "form": "최근에 무엇을 사셨나요",
          "original_form": "최근에 무엇을 사셨나요",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032106.1.5",
          "form": "장보고~ 11번가는 뭐샀더라",
          "original_form": "장보고~ 11번가는 뭐샀더라",
          "speaker_id": "2",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100032106.1.6",
          "form": "아 그거 신세계푸드",
          "original_form": "아 그거 신세계푸드",
          "speaker_id": "2",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100032106.1.7",
          "form": "쿠폰대란때 소불고기요",
          "original_form": "쿠폰대란때 소불고기요",
          "speaker_id": "2",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100032106.1.8",
          "form": "소불고기도 인터넷으로 사나요",
          "original_form": "소불고기도 인터넷으로 사나요",
          "speaker_id": "1",
          "time": "20210905 13:58"
        },
        {
          "id": "MDRW2100032106.1.9",
          "form": "안전 한가요",
          "original_form": "안전 한가요",
          "speaker_id": "1",
          "time": "20210905 13:58"
        },
        {
          "id": "MDRW2100032106.1.10",
          "form": "꽁꽁 얼어서 올껄요?ㅎㅎ",
          "original_form": "꽁꽁 얼어서 올껄요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:10"
        },
        {
          "id": "MDRW2100032106.1.11",
          "form": "아 그럼 괜찮겠네요",
          "original_form": "아 그럼 괜찮겠네요",
          "speaker_id": "1",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032106.1.12",
          "form": "전 최근에 쿠팡에서 일회용 마스크 구매 했어요",
          "original_form": "전 최근에 쿠팡에서 일회용 마스크 구매 했어요",
          "speaker_id": "1",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100032106.1.13",
          "form": "무료라 좋아요",
          "original_form": "무료라 좋아요",
          "speaker_id": "1",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100032106.1.14",
          "form": "무료배송이요?",
          "original_form": "무료배송이요?",
          "speaker_id": "2",
          "time": "20210905 14:54"
        },
        {
          "id": "MDRW2100032106.1.15",
          "form": "네 무료라 엄청 저렴하게 구매했어요",
          "original_form": "네 무료라 엄청 저렴하게 구매했어요",
          "speaker_id": "1",
          "time": "20210905 14:55"
        },
        {
          "id": "MDRW2100032106.1.16",
          "form": "하나에 얼마인가요",
          "original_form": "하나에 얼마인가요",
          "speaker_id": "2",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100032106.1.17",
          "form": "일회용 중국산이라 박스에 삼천원 이요",
          "original_form": "일회용 중국산이라 박스에 삼천원 이요",
          "speaker_id": "1",
          "time": "20210905 15:39"
        },
        {
          "id": "MDRW2100032106.1.18",
          "form": "두박스 삿어요",
          "original_form": "두박스 삿어요",
          "speaker_id": "1",
          "time": "20210905 15:39"
        },
        {
          "id": "MDRW2100032106.1.19",
          "form": "와 싸네요 ㅎㅎ",
          "original_form": "와 싸네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:07"
        },
        {
          "id": "MDRW2100032106.1.20",
          "form": "매우 저렴하죠",
          "original_form": "매우 저렴하죠",
          "speaker_id": "1",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100032106.1.21",
          "form": "소불고기는 저렴하게 구매하신건가요",
          "original_form": "소불고기는 저렴하게 구매하신건가요",
          "speaker_id": "1",
          "time": "20210906 07:16"
        },
        {
          "id": "MDRW2100032106.1.22",
          "form": "네 배송비 포함",
          "original_form": "네 배송비 포함",
          "speaker_id": "2",
          "time": "20210906 08:41"
        },
        {
          "id": "MDRW2100032106.1.23",
          "form": "180g 세개에 7천원인가",
          "original_form": "180g 세개에 7천원인가",
          "speaker_id": "2",
          "time": "20210906 08:41"
        },
        {
          "id": "MDRW2100032106.1.24",
          "form": "엄 첨저렴하네요",
          "original_form": "엄 첨저렴하네요",
          "speaker_id": "1",
          "time": "20210906 13:16"
        },
        {
          "id": "MDRW2100032106.1.25",
          "form": "최근에 구매하신 물건이 있나요",
          "original_form": "최근에 구매하신 물건이 있나요",
          "speaker_id": "1",
          "time": "20210907 18:28"
        }
      ]
