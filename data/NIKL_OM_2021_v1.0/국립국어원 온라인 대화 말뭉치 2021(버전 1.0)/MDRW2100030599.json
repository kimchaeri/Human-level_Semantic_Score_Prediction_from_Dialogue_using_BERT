      "utterance": [
        {
          "id": "MDRW2100030599.1.1",
          "form": "지금 그럼 어떤 일 하시나요?",
          "original_form": "지금 그럼 어떤 일 하시나요?",
          "speaker_id": "2",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100030599.1.2",
          "form": "지금은 굉장히 다양하게 하고 있어요",
          "original_form": "지금은 굉장히 다양하게 하고 있어요",
          "speaker_id": "1",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100030599.1.3",
          "form": "프리랜서 신가요??",
          "original_form": "프리랜서 신가요??",
          "speaker_id": "2",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100030599.1.4",
          "form": "네 맞아요 ㅎㅎ",
          "original_form": "네 맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100030599.1.5",
          "form": "name2님께서는 주로 어떤 일을 하시나요?",
          "original_form": "&name2&님께서는 주로 어떤 일을 하시나요?",
          "speaker_id": "1",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100030599.1.6",
          "form": "저는 학생이에요!!",
          "original_form": "저는 학생이에요!!",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030599.1.7",
          "form": "근데 나이가 엄청 적진 않아요 ㅎㅎㅎ",
          "original_form": "근데 나이가 엄청 적진 않아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030599.1.8",
          "form": "학생! 부러워요",
          "original_form": "학생! 부러워요",
          "speaker_id": "1",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030599.1.9",
          "form": "저도 요샌 대학원을 갈까 싶어서 준비중이에요",
          "original_form": "저도 요샌 대학원을 갈까 싶어서 준비중이에요",
          "speaker_id": "1",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100030599.1.10",
          "form": "어떤 공부가 제일 재미있으신가요?",
          "original_form": "어떤 공부가 제일 재미있으신가요?",
          "speaker_id": "1",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100030599.1.11",
          "form": "그러시구나!!",
          "original_form": "그러시구나!!",
          "speaker_id": "2",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100030599.1.12",
          "form": "아뇨! 그건 절대 아니죠!! ㅋㅋㅋㅋㅋ 그냥 해야 돼서 하는거죠 ㅋㅋㅋㅋ",
          "original_form": "아뇨! 그건 절대 아니죠!! ㅋㅋㅋㅋㅋ 그냥 해야 돼서 하는거죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:48"
        },
        {
          "id": "MDRW2100030599.1.13",
          "form": "아핳핳 ㅋㅋㅋㅋㅋ",
          "original_form": "아핳핳 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:48"
        },
        {
          "id": "MDRW2100030599.1.14",
          "form": "저도 사실 대학원생이에요! ㅋㅋㅋㅋ",
          "original_form": "저도 사실 대학원생이에요! ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:48"
        },
        {
          "id": "MDRW2100030599.1.15",
          "form": "앗 정말요?",
          "original_form": "앗 정말요?",
          "speaker_id": "1",
          "time": "20210904 10:48"
        },
        {
          "id": "MDRW2100030599.1.16",
          "form": "혹시 실례가 되지 않는다면 전공이...?",
          "original_form": "혹시 실례가 되지 않는다면 전공이...?",
          "speaker_id": "1",
          "time": "20210904 10:49"
        },
        {
          "id": "MDRW2100030599.1.17",
          "form": "저는 화학이요! ㅋㅋㅋㅋㅋ",
          "original_form": "저는 화학이요! ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:49"
        },
        {
          "id": "MDRW2100030599.1.18",
          "form": "어떤 전공으로 가고싶으세요??",
          "original_form": "어떤 전공으로 가고싶으세요??",
          "speaker_id": "2",
          "time": "20210904 10:49"
        },
        {
          "id": "MDRW2100030599.1.19",
          "form": "세상 부러운 전공이에요 ㅠ",
          "original_form": "세상 부러운 전공이에요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 10:49"
        },
        {
          "id": "MDRW2100030599.1.20",
          "form": "저는 생명과학 전공해서 마저 이어서 갈까 고민중이에요",
          "original_form": "저는 생명과학 전공해서 마저 이어서 갈까 고민중이에요",
          "speaker_id": "1",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030599.1.21",
          "form": "화학쪽으로 건너가기엔 잘 받아주지 않고 ㅠ",
          "original_form": "화학쪽으로 건너가기엔 잘 받아주지 않고 ㅠ",
          "speaker_id": "1",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030599.1.22",
          "form": "생명과학이 요즘엔 더 뜨고있지 않나요?!?",
          "original_form": "생명과학이 요즘엔 더 뜨고있지 않나요?!?",
          "speaker_id": "2",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030599.1.23",
          "form": "10년째 미래만 밝은 학과랍니다",
          "original_form": "10년째 미래만 밝은 학과랍니다",
          "speaker_id": "1",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030599.1.24",
          "form": "언제 밝아질지는 아무도 몰라요 ㅋㅋㅋㅋ",
          "original_form": "언제 밝아질지는 아무도 몰라요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030599.1.25",
          "form": "음... 생화학은요??!",
          "original_form": "음... 생화학은요??!",
          "speaker_id": "2",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030599.1.26",
          "form": "ㅋㅋㅋㅋㅋ ㅜㅜ 너무 생명인가요ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ ㅜㅜ 너무 생명인가요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030599.1.27",
          "form": "생화학쪽도 괜찮긴 한데",
          "original_form": "생화학쪽도 괜찮긴 한데",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030599.1.28",
          "form": "사실 잘 할 수 있을지 확신이 없어요 ㅠ",
          "original_form": "사실 잘 할 수 있을지 확신이 없어요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030599.1.29",
          "form": "지금 랩실에 다니고 계신거죠?",
          "original_form": "지금 랩실에 다니고 계신거죠?",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030599.1.30",
          "form": "힘들진 않으세요?",
          "original_form": "힘들진 않으세요?",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030599.1.31",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210904 10:53"
        },
        {
          "id": "MDRW2100030599.1.32",
          "form": "대학원 잘 알아보고 가긴 해야죠ㅠㅠ",
          "original_form": "대학원 잘 알아보고 가긴 해야죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 10:54"
        },
        {
          "id": "MDRW2100030599.1.33",
          "form": "ㅋㅋㅋㅋㅋ 힘들지 않진 않겠지만 ㅋㅋㅋ 해야하니까요ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋㅋ 힘들지 않진 않겠지만 ㅋㅋㅋ 해야하니까요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 10:54"
        },
        {
          "id": "MDRW2100030599.1.34",
          "form": "그냥 열심히 해보려고요ㅜㅜ",
          "original_form": "그냥 열심히 해보려고요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 10:54"
        },
        {
          "id": "MDRW2100030599.1.35",
          "form": "연구직 희망하세요??",
          "original_form": "연구직 희망하세요??",
          "speaker_id": "2",
          "time": "20210904 10:55"
        },
        {
          "id": "MDRW2100030599.1.36",
          "form": "연구직쪽으로 생각하고 있어요",
          "original_form": "연구직쪽으로 생각하고 있어요",
          "speaker_id": "1",
          "time": "20210904 10:56"
        },
        {
          "id": "MDRW2100030599.1.37",
          "form": "물론 잘 알아보고 가야죠 ㅎㅎ...",
          "original_form": "물론 잘 알아보고 가야죠 ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210904 10:57"
        },
        {
          "id": "MDRW2100030599.1.38",
          "form": "연구직을 추천하시나요?",
          "original_form": "연구직을 추천하시나요?",
          "speaker_id": "1",
          "time": "20210904 10:58"
        },
        {
          "id": "MDRW2100030599.1.39",
          "form": "저도 생명쪽도 관심 있었는데 과목들이 다 너무 암기폭탄이라서ㅠㅠ 포기했어요ㅜㅜ",
          "original_form": "저도 생명쪽도 관심 있었는데 과목들이 다 너무 암기폭탄이라서ㅠㅠ 포기했어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100030599.1.40",
          "form": "저는 연구직 하고싶긴해요!!",
          "original_form": "저는 연구직 하고싶긴해요!!",
          "speaker_id": "2",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100030599.1.41",
          "form": "대학원은 보통 그래서들 많이 가니까요...ㅎㅎ",
          "original_form": "대학원은 보통 그래서들 많이 가니까요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100030599.1.42",
          "form": "오히려 화학을 하고계신게 더 대단하게 느껴져요",
          "original_form": "오히려 화학을 하고계신게 더 대단하게 느껴져요",
          "speaker_id": "1",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100030599.1.43",
          "form": "응용력이 정말 좋아야하잖아요!",
          "original_form": "응용력이 정말 좋아야하잖아요!",
          "speaker_id": "1",
          "time": "20210904 11:01"
        },
        {
          "id": "MDRW2100030599.1.44",
          "form": "ㅎㅎㅎ 응용력을... 앞으로 기를 생각입니다!!",
          "original_form": "ㅎㅎㅎ 응용력을... 앞으로 기를 생각입니다!!",
          "speaker_id": "2",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030599.1.45",
          "form": "그래도 화학과 과목 좀 들어보지 않으셨나요??",
          "original_form": "그래도 화학과 과목 좀 들어보지 않으셨나요??",
          "speaker_id": "2",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030599.1.46",
          "form": "들어봤죠 ㅎㅎ",
          "original_form": "들어봤죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030599.1.47",
          "form": "다른 과목들보다 좀 어렵게 느껴졌어요",
          "original_form": "다른 과목들보다 좀 어렵게 느껴졌어요",
          "speaker_id": "1",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100030599.1.48",
          "form": "그쪽으로 머리가 슝슝 돌아가질 않나봐요 ㅠ",
          "original_form": "그쪽으로 머리가 슝슝 돌아가질 않나봐요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100030599.1.49",
          "form": "어떤과목 들으셨나요?? 유기화학??",
          "original_form": "어떤과목 들으셨나요?? 유기화학??",
          "speaker_id": "2",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100030599.1.50",
          "form": "유기화학도 들었었죠",
          "original_form": "유기화학도 들었었죠",
          "speaker_id": "1",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100030599.1.51",
          "form": "제가 화학을 포기하게 해준 과목이랄까...",
          "original_form": "제가 화학을 포기하게 해준 과목이랄까...",
          "speaker_id": "1",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100030599.1.52",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030599.1.53",
          "form": "ㅋㅋㅋㅋㅋ 저는 화학중에서는 유기화학이 그나마 제일 낫더라구요...",
          "original_form": "ㅋㅋㅋㅋㅋ 저는 화학중에서는 유기화학이 그나마 제일 낫더라구요...",
          "speaker_id": "2",
          "time": "20210904 14:49"
        },
        {
          "id": "MDRW2100030599.1.54",
          "form": "물리...분석...전기... 진짜 노답이에요...!!",
          "original_form": "물리...분석...전기... 진짜 노답이에요...!!",
          "speaker_id": "2",
          "time": "20210904 14:49"
        },
        {
          "id": "MDRW2100030599.1.55",
          "form": "물리... 분석.... 전기!!!!",
          "original_form": "물리... 분석.... 전기!!!!",
          "speaker_id": "1",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030599.1.56",
          "form": "유기화학이 나은편이라니... 굉장히 훌륭한분께 배우셨나봐요 ㅠ",
          "original_form": "유기화학이 나은편이라니... 굉장히 훌륭한분께 배우셨나봐요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030599.1.57",
          "form": "분석쪽도 재미있을 것 같아요",
          "original_form": "분석쪽도 재미있을 것 같아요",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
