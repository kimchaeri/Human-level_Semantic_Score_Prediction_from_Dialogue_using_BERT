      "utterance": [
        {
          "id": "MDRW2100041008.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100041008.1.2",
          "form": "운동하시나요?",
          "original_form": "운동하시나요?",
          "speaker_id": "1",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100041008.1.3",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210905 15:08"
        },
        {
          "id": "MDRW2100041008.1.4",
          "form": "일주일에 3번 정도 필라테스 해요",
          "original_form": "일주일에 3번 정도 필라테스 해요",
          "speaker_id": "2",
          "time": "20210905 15:08"
        },
        {
          "id": "MDRW2100041008.1.5",
          "form": "꾸준히하시네요",
          "original_form": "꾸준히하시네요",
          "speaker_id": "1",
          "time": "20210905 15:08"
        },
        {
          "id": "MDRW2100041008.1.6",
          "form": "건강에 많이 도움이 되나요?",
          "original_form": "건강에 많이 도움이 되나요?",
          "speaker_id": "1",
          "time": "20210905 15:09"
        },
        {
          "id": "MDRW2100041008.1.7",
          "form": "음 확실히 근육 뭉치는 건 덜하고",
          "original_form": "음 확실히 근육 뭉치는 건 덜하고",
          "speaker_id": "2",
          "time": "20210905 15:09"
        },
        {
          "id": "MDRW2100041008.1.8",
          "form": "자세교정이 되는 것 같아요",
          "original_form": "자세교정이 되는 것 같아요",
          "speaker_id": "2",
          "time": "20210905 15:09"
        },
        {
          "id": "MDRW2100041008.1.9",
          "form": "그렇구나 요가는 해봤는데",
          "original_form": "그렇구나 요가는 해봤는데",
          "speaker_id": "1",
          "time": "20210905 15:10"
        },
        {
          "id": "MDRW2100041008.1.10",
          "form": "필라테스도 어려워보이더라고요",
          "original_form": "필라테스도 어려워보이더라고요",
          "speaker_id": "1",
          "time": "20210905 15:10"
        },
        {
          "id": "MDRW2100041008.1.11",
          "form": "요가보단 필라테스가 더 쉽대요",
          "original_form": "요가보단 필라테스가 더 쉽대요",
          "speaker_id": "2",
          "time": "20210905 15:10"
        },
        {
          "id": "MDRW2100041008.1.12",
          "form": "기구 도움을 받아서",
          "original_form": "기구 도움을 받아서",
          "speaker_id": "2",
          "time": "20210905 15:10"
        },
        {
          "id": "MDRW2100041008.1.13",
          "form": "아 기구를 쓰나요? 맨몸으로 하는줄 알았는데",
          "original_form": "아 기구를 쓰나요? 맨몸으로 하는줄 알았는데",
          "speaker_id": "1",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100041008.1.14",
          "form": "아 저는 기구필라테스 해요",
          "original_form": "아 저는 기구필라테스 해요",
          "speaker_id": "2",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100041008.1.15",
          "form": "아 기구쓰는걸로 하시는군요",
          "original_form": "아 기구쓰는걸로 하시는군요",
          "speaker_id": "1",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100041008.1.16",
          "form": "네 기구필라테스로",
          "original_form": "네 기구필라테스로",
          "speaker_id": "2",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100041008.1.17",
          "form": "또 다른운동은 어떤거하셨나요",
          "original_form": "또 다른운동은 어떤거하셨나요",
          "speaker_id": "1",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100041008.1.18",
          "form": "수영했었는데",
          "original_form": "수영했었는데",
          "speaker_id": "2",
          "time": "20210905 15:15"
        },
        {
          "id": "MDRW2100041008.1.19",
          "form": "코로나 이후엔 계속 못갔어요",
          "original_form": "코로나 이후엔 계속 못갔어요",
          "speaker_id": "2",
          "time": "20210905 15:15"
        },
        {
          "id": "MDRW2100041008.1.20",
          "form": "수영은 다른운동보다 코로나때메 제약이 크죠..",
          "original_form": "수영은 다른운동보다 코로나때메 제약이 크죠..",
          "speaker_id": "1",
          "time": "20210905 15:16"
        }
      ]
