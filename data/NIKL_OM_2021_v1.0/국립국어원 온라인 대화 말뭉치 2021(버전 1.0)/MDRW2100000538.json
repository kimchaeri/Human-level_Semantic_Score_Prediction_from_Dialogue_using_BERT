      "utterance": [
        {
          "id": "MDRW2100000538.1.1",
          "form": "좋아하는 음식 있어?",
          "original_form": "좋아하는 음식 있어?",
          "speaker_id": "2",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000538.1.2",
          "form": "나 삼겹살!! 요즘 너무 땡겨",
          "original_form": "나 삼겹살!! 요즘 너무 땡겨",
          "speaker_id": "1",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000538.1.3",
          "form": "삼겹살 맛있지 ㅠㅠ난 삼겹살보다 항정살이 더맛있는거같아",
          "original_form": "삼겹살 맛있지 ㅠㅠ난 삼겹살보다 항정살이 더맛있는거같아",
          "speaker_id": "2",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000538.1.4",
          "form": "맞아 ㅋㅋㄱ가브리살도 맛있구!!",
          "original_form": "맞아 ㅋㅋㄱ가브리살도 맛있구!!",
          "speaker_id": "1",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000538.1.5",
          "form": "대학교다닐때는 돈없어서 무한리필삼겹살 엄청먹었는데 ㅋㅋㅋㅋ",
          "original_form": "대학교다닐때는 돈없어서 무한리필삼겹살 엄청먹었는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:48"
        },
        {
          "id": "MDRW2100000538.1.6",
          "form": "요즘엔 양보다 질이야",
          "original_form": "요즘엔 양보다 질이야",
          "speaker_id": "2",
          "time": "20210819 16:48"
        },
        {
          "id": "MDRW2100000538.1.7",
          "form": "나 최근에 소고기오마카세 먹으러 갔어..",
          "original_form": "나 최근에 소고기오마카세 먹으러 갔어..",
          "speaker_id": "1",
          "time": "20210819 16:48"
        },
        {
          "id": "MDRW2100000538.1.8",
          "form": "가격이 후덜덜",
          "original_form": "가격이 후덜덜",
          "speaker_id": "1",
          "time": "20210819 16:48"
        },
        {
          "id": "MDRW2100000538.1.9",
          "form": "세상에,,어디로 갔었어?",
          "original_form": "세상에,,어디로 갔었어?",
          "speaker_id": "2",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000538.1.10",
          "form": "압구정으로?",
          "original_form": "압구정으로?",
          "speaker_id": "2",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000538.1.11",
          "form": "아니 석촌호수 ㅠ 둘이서 18마넌 ㅠ",
          "original_form": "아니 석촌호수 ㅠ 둘이서 18마넌 ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000538.1.12",
          "form": "둘이서 18만원이면 아유,,,",
          "original_form": "둘이서 18만원이면 아유,,,",
          "speaker_id": "2",
          "time": "20210819 16:53"
        },
        {
          "id": "MDRW2100000538.1.13",
          "form": "내 남친이 요리하는데 압구정에서 1인에 25만원짜리 하고그랬거든 ㅋㅋㅋ진짜 한번 먹어보고싶었는데 너무비싸서 엄두가안나더라",
          "original_form": "내 남친이 요리하는데 압구정에서 1인에 25만원짜리 하고그랬거든 ㅋㅋㅋ진짜 한번 먹어보고싶었는데 너무비싸서 엄두가안나더라",
          "speaker_id": "2",
          "time": "20210819 16:53"
        },
        {
          "id": "MDRW2100000538.1.14",
          "form": "코로나 더 심해져서 여행가는 대신 비싼거 먹어보자 해서 간거거든ㅠ",
          "original_form": "코로나 더 심해져서 여행가는 대신 비싼거 먹어보자 해서 간거거든ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000538.1.15",
          "form": "남자친구가 일하는 음식점이??",
          "original_form": "남자친구가 일하는 음식점이??",
          "speaker_id": "1",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000538.1.16",
          "form": "그래도 맛있게 먹었으면 됐어~!",
          "original_form": "그래도 맛있게 먹었으면 됐어~!",
          "speaker_id": "2",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000538.1.17",
          "form": "응응 저번에 티비도 나오드라",
          "original_form": "응응 저번에 티비도 나오드라",
          "speaker_id": "2",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000538.1.18",
          "form": "어디길래!! 유명한 곳인가보다!!",
          "original_form": "어디길래!! 유명한 곳인가보다!!",
          "speaker_id": "1",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000538.1.19",
          "form": "비플리끄!! 이지혜가 남편사주던데 ㅋㅋㅋㅋ 나 저번에 놀러갔는데 송혜교 와서 밥먹구있또라",
          "original_form": "비플리끄!! 이지혜가 남편사주던데 ㅋㅋㅋㅋ 나 저번에 놀러갔는데 송혜교 와서 밥먹구있또라",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000538.1.20",
          "form": "와 대박 ㅋㅋㅋ 연예인 많이 오네? 엄청 맛있나보다",
          "original_form": "와 대박 ㅋㅋㅋ 연예인 많이 오네? 엄청 맛있나보다",
          "speaker_id": "1",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000538.1.21",
