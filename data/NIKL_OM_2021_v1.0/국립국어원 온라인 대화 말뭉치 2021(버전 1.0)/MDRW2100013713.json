      "utterance": [
        {
          "id": "MDRW2100013713.1.1",
          "form": "자취해보신적 있으세요?",
          "original_form": "자취해보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013713.1.2",
          "form": "아직없어요",
          "original_form": "아직없어요",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013713.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013713.1.4",
          "form": "6개월정도 해봤어요",
          "original_form": "6개월정도 해봤어요",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013713.1.5",
          "form": "자취에 대해 궁금한거 있으세요?",
          "original_form": "자취에 대해 궁금한거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013713.1.6",
          "form": "그렇구나 지금은 가족들이랑 사시나봐요",
          "original_form": "그렇구나 지금은 가족들이랑 사시나봐요",
          "speaker_id": "2",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013713.1.7",
          "form": "혼자있으면 뭐가 제일 안좋아요?",
          "original_form": "혼자있으면 뭐가 제일 안좋아요?",
          "speaker_id": "2",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013713.1.8",
          "form": "음... 음식 남는거?",
          "original_form": "음... 음식 남는거?",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013713.1.9",
          "form": "ㅋㅋㅋㅋ아하..",
          "original_form": "ㅋㅋㅋㅋ아하..",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013713.1.10",
          "form": "뭐 심심하거나 외롭거나그런느낌은 없으셨나요",
          "original_form": "뭐 심심하거나 외롭거나그런느낌은 없으셨나요",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013713.1.11",
          "form": "어우 전혀 없죠",
          "original_form": "어우 전혀 없죠",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013713.1.12",
          "form": "제 방이 거의 공공재가 되는걸요",
          "original_form": "제 방이 거의 공공재가 되는걸요",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013713.1.13",
          "form": "앜ㅋㅋㅋㅋㅋ예상과 정반대네요",
          "original_form": "앜ㅋㅋㅋㅋㅋ예상과 정반대네요",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013713.1.14",
          "form": "자취는 거의 뭐",
          "original_form": "자취는 거의 뭐",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013713.1.15",
          "form": "외로움과 동떨어지는 삶이죠",
          "original_form": "외로움과 동떨어지는 삶이죠",
          "speaker_id": "1",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013713.1.16",
          "form": "집안일 중에서는",
          "original_form": "집안일 중에서는",
          "speaker_id": "1",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013713.1.17",
          "form": "뭐가 제일 귀찮아요?",
          "original_form": "뭐가 제일 귀찮아요?",
          "speaker_id": "1",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013713.1.18",
          "form": "전반적으로 다??ㅋㅋㅋ",
          "original_form": "전반적으로 다??ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013713.1.19",
          "form": "그렇지 않나요",
          "original_form": "그렇지 않나요",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013713.1.20",
          "form": "기계가 다 대신해줬음좋겠어요",
          "original_form": "기계가 다 대신해줬음좋겠어요",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013713.1.21",
          "form": "전 설거지",
          "original_form": "전 설거지",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013713.1.22",
          "form": "설거지는 기계가 있어도 사람이 또 해야되잖아요ㅋㅋ",
          "original_form": "설거지는 기계가 있어도 사람이 또 해야되잖아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013713.1.23",
          "form": "그니까요,,먹고나서 그거까지 하는게 참 일이에요",
          "original_form": "그니까요,,먹고나서 그거까지 하는게 참 일이에요",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013713.1.24",
          "form": "설거지 그릇 안만드는게 진짜 고민되죠",
          "original_form": "설거지 그릇 안만드는게 진짜 고민되죠",
          "speaker_id": "1",
          "time": "20210825 16:14"
        }
      ]
