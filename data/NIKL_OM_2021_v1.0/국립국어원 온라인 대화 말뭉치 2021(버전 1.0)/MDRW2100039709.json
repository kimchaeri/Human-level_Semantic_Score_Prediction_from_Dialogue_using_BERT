      "utterance": [
        {
          "id": "MDRW2100039709.1.1",
          "form": "역시 우린 먹는거지",
          "original_form": "역시 우린 먹는거지",
          "speaker_id": "1",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100039709.1.2",
          "form": "저녁 뭐 먹었냥",
          "original_form": "저녁 뭐 먹었냥",
          "speaker_id": "1",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100039709.1.3",
          "form": "나 굶었어ㅋㅋ\r\n대신 낮에 고구마튀김 먹었어",
          "original_form": "나 굶었어ㅋㅋ\r\n대신 낮에 고구마튀김 먹었어",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039709.1.4",
          "form": "안그래도 오늘 회사에서 배달음식얘기했는데",
          "original_form": "안그래도 오늘 회사에서 배달음식얘기했는데",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039709.1.5",
          "form": "왜 굶었노",
          "original_form": "왜 굶었노",
          "speaker_id": "1",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039709.1.6",
          "form": "다이어트땜에?",
          "original_form": "다이어트땜에?",
          "speaker_id": "1",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100039709.1.7",
          "form": "집에 생선냄새가 나서 입맛뚝떨",
          "original_form": "집에 생선냄새가 나서 입맛뚝떨",
          "speaker_id": "2",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100039709.1.8",
          "form": "ㅋㅋㅋ일욜에  영화보고 밥먹어야죠",
          "original_form": "ㅋㅋㅋ일욜에  영화보고 밥먹어야죠",
          "speaker_id": "2",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039709.1.9",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039709.1.10",
          "form": "아 저런 ㅎ ...생선",
          "original_form": "아 저런 ㅎ ...생선",
          "speaker_id": "1",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039709.1.11",
          "form": "밥먹어야죠!!!!!!",
          "original_form": "밥먹어야죠!!!!!!",
          "speaker_id": "1",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039709.1.12",
          "form": "뭐 먹으까",
          "original_form": "뭐 먹으까",
          "speaker_id": "1",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039709.1.13",
          "form": "광천이지????  맛집을 검색해볼깡?",
          "original_form": "광천이지????  맛집을 검색해볼깡?",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039709.1.14",
          "form": "네네 ㅎ",
          "original_form": "네네 ㅎ",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039709.1.15",
          "form": "최근에 거기 가봤었음",
          "original_form": "최근에 거기 가봤었음",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039709.1.16",
          "form": "터미널에 백종원 식당",
          "original_form": "터미널에 백종원 식당",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039709.1.17",
          "form": "요즘 뭐먹고사니?\r\n\r\n어디?",
          "original_form": "요즘 뭐먹고사니?\r\n\r\n어디?",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039709.1.18",
          "form": "롤링파스타",
          "original_form": "롤링파스타",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039709.1.19",
          "form": "나,,최근에 죽먹었지 ㅎ 소화안되서ㅋㅋ",
          "original_form": "나,,최근에 죽먹었지 ㅎ 소화안되서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039709.1.20",
          "form": "아하!  어때???",
          "original_form": "아하!  어때???",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039709.1.21",
          "form": "나쁘지않았어ㅋ가성비 괜춘",
          "original_form": "나쁘지않았어ㅋ가성비 괜춘",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039709.1.22",
          "form": "글고 거기 엔젤리너스 반미도 궁금하긴함",
          "original_form": "글고 거기 엔젤리너스 반미도 궁금하긴함",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039709.1.23",
          "form": "그냥 먹을만하대",
          "original_form": "그냥 먹을만하대",
          "speaker_id": "2",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039709.1.24",
          "form": "미친....반미 먹고싶다...ㅂ",
          "original_form": "미친....반미 먹고싶다...ㅂ",
          "speaker_id": "2",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039709.1.25",
          "form": "그치 ㅋㅋ 반미먹으까?",
          "original_form": "그치 ㅋㅋ 반미먹으까?",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039709.1.26",
          "form": "그걸로 성에차겠니",
          "original_form": "그걸로 성에차겠니",
          "speaker_id": "2",
          "time": "20210903 22:03"
        },
        {
          "id": "MDRW2100039709.1.27",
          "form": "친구 고기 구워먹은지 얼마나되었나",
          "original_form": "친구 고기 구워먹은지 얼마나되었나",
          "speaker_id": "2",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039709.1.28",
          "form": "고기....!!",
          "original_form": "고기....!!",
          "speaker_id": "1",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039709.1.29",
          "form": "오래되었지",
          "original_form": "오래되었지",
          "speaker_id": "1",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039709.1.30",
          "form": "고기먹게?",
          "original_form": "고기먹게?",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039709.1.31",
          "form": "ㅋㅋㄱㄱ구워먹은건 오래됐지 ??  간만에 구워먹을랭???",
          "original_form": "ㅋㅋㄱㄱ구워먹은건 오래됐지 ??  간만에 구워먹을랭???",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039709.1.32",
          "form": "아니면 보쌈???",
          "original_form": "아니면 보쌈???",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039709.1.33",
          "form": "보쌈!!!!",
          "original_form": "보쌈!!!!",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039709.1.34",
          "form": "구워먹어도 좋고 보쌈도 좋아 와우",
          "original_form": "구워먹어도 좋고 보쌈도 좋아 와우",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100039709.1.35",
          "form": "요즘 피가 펑펑 쏟아져서 어질어질함ㅋㅋ",
          "original_form": "요즘 피가 펑펑 쏟아져서 어질어질함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100039709.1.36",
          "form": "오늘출근해서 밥 생각해봐야징ㄱㄲ",
          "original_form": "오늘출근해서 밥 생각해봐야징ㄱㄲ",
          "speaker_id": "2",
          "time": "20210904 07:46"
        },
        {
          "id": "MDRW2100039709.1.37",
          "form": "출근하구나 점심 뭐먹을거냥",
          "original_form": "출근하구나 점심 뭐먹을거냥",
          "speaker_id": "1",
          "time": "20210904 09:07"
        },
        {
          "id": "MDRW2100039709.1.38",
          "form": "나도 출근 ...",
          "original_form": "나도 출근 ...",
          "speaker_id": "1",
          "time": "20210904 09:07"
        },
        {
          "id": "MDRW2100039709.1.39",
          "form": "구운고기먹고싶은데...점심에 문연곳이 없네..더 찾아봐야징",
          "original_form": "구운고기먹고싶은데...점심에 문연곳이 없네..더 찾아봐야징",
          "speaker_id": "2",
          "time": "20210904 09:49"
        },
        {
          "id": "MDRW2100039709.1.40",
          "form": "ㅋㅋ 고기생각하니깐 배고프당",
          "original_form": "ㅋㅋ 고기생각하니깐 배고프당",
          "speaker_id": "1",
          "time": "20210904 14:33"
        },
        {
          "id": "MDRW2100039709.1.41",
          "form": "점심 뭐먹었어?",
          "original_form": "점심 뭐먹었어?",
          "speaker_id": "1",
          "time": "20210904 14:33"
        },
        {
          "id": "MDRW2100039709.1.42",
          "form": "점심은..회사밥.노맛이었어ㅋㅋ",
          "original_form": "점심은..회사밥.노맛이었어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:13"
        },
        {
          "id": "MDRW2100039709.1.43",
          "form": "ㅋㅋ메뉴가 뭐였어",
          "original_form": "ㅋㅋ메뉴가 뭐였어",
          "speaker_id": "1",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100039709.1.44",
          "form": "그냥  그냥 진짜그냥밥이야",
          "original_form": "그냥  그냥 진짜그냥밥이야",
          "speaker_id": "2",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100039709.1.45",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:09"
        },
        {
          "id": "MDRW2100039709.1.46",
          "form": "난 두부찜 나와서 참기름 비벼먹음",
          "original_form": "난 두부찜 나와서 참기름 비벼먹음",
          "speaker_id": "1",
          "time": "20210904 16:09"
        },
        {
          "id": "MDRW2100039709.1.47",
          "form": "요즘  카페는 자주가니?",
          "original_form": "요즘  카페는 자주가니?",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100039709.1.48",
          "form": "노우ㅋㅋㅋ카페 간지 기억도 안납니다",
          "original_form": "노우ㅋㅋㅋ카페 간지 기억도 안납니다",
          "speaker_id": "1",
          "time": "20210904 19:06"
        },
        {
