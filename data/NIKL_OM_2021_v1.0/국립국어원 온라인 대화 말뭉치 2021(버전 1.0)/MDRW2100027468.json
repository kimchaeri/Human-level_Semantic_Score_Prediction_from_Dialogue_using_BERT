      "utterance": [
        {
          "id": "MDRW2100027468.1.1",
          "form": "내가 아는 뜻 말고 또 다른게 있음?",
          "original_form": "내가 아는 뜻 말고 또 다른게 있음?",
          "speaker_id": "2",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027468.1.2",
          "form": "교과서",
          "original_form": "교과서",
          "speaker_id": "1",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027468.1.3",
          "form": "봅여름가을겨울",
          "original_form": "봅여름가을겨울",
          "speaker_id": "1",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027468.1.4",
          "form": "아 그거",
          "original_form": "아 그거",
          "speaker_id": "2",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027468.1.5",
          "form": "뭐임 그게?",
          "original_form": "뭐임 그게?",
          "speaker_id": "2",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027468.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027468.1.7",
          "form": "초등학교",
          "original_form": "초등학교",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027468.1.8",
          "form": "요즘교과서",
          "original_form": "요즘교과서",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027468.1.9",
          "form": "수학이런거없고",
          "original_form": "수학이런거없고",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027468.1.10",
          "form": "그냥봄여름가을겨울이래",
          "original_form": "그냥봄여름가을겨울이래",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027468.1.11",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027468.1.12",
          "form": "안나눠 과목???",
          "original_form": "안나눠 과목???",
          "speaker_id": "2",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027468.1.13",
