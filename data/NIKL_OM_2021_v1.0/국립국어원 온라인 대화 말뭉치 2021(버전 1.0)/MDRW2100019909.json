      "utterance": [
        {
          "id": "MDRW2100019909.1.1",
          "form": "원래 저는 학원강사 알아보고있었는데...",
          "original_form": "원래 저는 학원강사 알아보고있었는데...",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019909.1.2",
          "form": "어쩌다보니 회사 마케팅자리까지 오게 되었거든요...",
          "original_form": "어쩌다보니 회사 마케팅자리까지 오게 되었거든요...",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019909.1.3",
          "form": "오 다시 학원 가시는건가요?",
          "original_form": "오 다시 학원 가시는건가요?",
          "speaker_id": "1",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019909.1.4",
          "form": "그래야 할 거같아요ㅜ",
          "original_form": "그래야 할 거같아요ㅜ",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019909.1.5",
          "form": "무슨과목인가요??",
          "original_form": "무슨과목인가요??",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019909.1.6",
          "form": "제대로 되는 일이 없네요...",
          "original_form": "제대로 되는 일이 없네요...",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019909.1.7",
          "form": "영어과목이요ㅎ",
          "original_form": "영어과목이요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019909.1.8",
          "form": "영어 잘하시나봐요 멋잇어요",
          "original_form": "영어 잘하시나봐요 멋잇어요",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019909.1.9",
          "form": "지금은 많이 잊어먹었어요ㅋㅋ",
          "original_form": "지금은 많이 잊어먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019909.1.10",
          "form": "학원가도 가기싫었던게",
          "original_form": "학원가도 가기싫었던게",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019909.1.11",
          "form": "대치동어학원에 면접은 보러갔더니 오후 2시부터 밤12시까지 풀타임 수업을 하겠다 라는 서명을 하라고 종이를 주는거에요?",
          "original_form": "대치동어학원에 면접은 보러갔더니 오후 2시부터 밤12시까지 풀타임 수업을 하겠다 라는 서명을 하라고 종이를 주는거에요?",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019909.1.12",
          "form": "대뜸이요?",
          "original_form": "대뜸이요?",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019909.1.13",
          "form": "근무시간 무엇",
          "original_form": "근무시간 무엇",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019909.1.14",
          "form": "네ㅋㅋㅋ아니 종이 뭉탱이 주더니",
          "original_form": "네ㅋㅋㅋ아니 종이 뭉탱이 주더니",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019909.1.15",
          "form": "설문조사도 아니고",
          "original_form": "설문조사도 아니고",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019909.1.16",
          "form": "뭘 막 쓰래요",
          "original_form": "뭘 막 쓰래요",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019909.1.17",
          "form": "그래서 넘기고 나니까ㅋㅋㅋ각서도 아니고ㅋㅋ",
          "original_form": "그래서 넘기고 나니까ㅋㅋㅋ각서도 아니고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019909.1.18",
          "form": "아니 이걸 뭐라해야하는거죠...?",
          "original_form": "아니 이걸 뭐라해야하는거죠...?",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019909.1.19",
          "form": "무례하네요",
          "original_form": "무례하네요",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019909.1.20",
          "form": "아니면 얼굴 보자마자 합격이라서?",
          "original_form": "아니면 얼굴 보자마자 합격이라서?",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019909.1.21",
          "form": "진짜 어이없었어요",
          "original_form": "진짜 어이없었어요",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019909.1.22",
          "form": "아뇨 그건 아닌거같구요ㅋㅋㅋ",
          "original_form": "아뇨 그건 아닌거같구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019909.1.23",
          "form": "제가봤을 때",
          "original_form": "제가봤을 때",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019909.1.24",
          "form": "그 학원이 선생님들이 좀 자주 바뀐다는 얘길 듣긴했어요 다른 선생님한테",
          "original_form": "그 학원이 선생님들이 좀 자주 바뀐다는 얘길 듣긴했어요 다른 선생님한테",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019909.1.25",
          "form": "그렇게 빡세면 자주 바뀔수밖에 없을것같아요",
          "original_form": "그렇게 빡세면 자주 바뀔수밖에 없을것같아요",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019909.1.26",
          "form": "초봉이 쎈만큼 많이 부려먹나보더라구요... 3년인가 버티면 월500씩 주더라구요ㅋ 근데 다들 못버티고 나간 것...",
          "original_form": "초봉이 쎈만큼 많이 부려먹나보더라구요... 3년인가 버티면 월500씩 주더라구요ㅋ 근데 다들 못버티고 나간 것...",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019909.1.27",
          "form": "대치동이라서그런가",
          "original_form": "대치동이라서그런가",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019909.1.28",
          "form": "많이주네요",
          "original_form": "많이주네요",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019909.1.29",
          "form": "하지만 3년...",
          "original_form": "하지만 3년...",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019909.1.30",
          "form": "네 많이는 주는데",
          "original_form": "네 많이는 주는데",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019909.1.31",
          "form": "담임제다보니 책임이 막중하고...",
          "original_form": "담임제다보니 책임이 막중하고...",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019909.1.32",
          "form": "풀타임....",
          "original_form": "풀타임....",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019909.1.33",
          "form": "담임제...",
          "original_form": "담임제...",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019909.1.34",
          "form": "부담이 어마어마할것같아요",
          "original_form": "부담이 어마어마할것같아요",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019909.1.35",
          "form": "네 엄청  ㅋㅋㅋㅋㅋ",
          "original_form": "네 엄청  ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019909.1.36",
          "form": "학부모들도 전화 엄청하잖아요ㅋㅋㅋ",
          "original_form": "학부모들도 전화 엄청하잖아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019909.1.37",
          "form": "맞아요 심지어 대치동이야 ㄷㄷㄷㄷ",
          "original_form": "맞아요 심지어 대치동이야 ㄷㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019909.1.38",
          "form": "그래서 회사를 선택했더니..? 아늬 ?",
          "original_form": "그래서 회사를 선택했더니..? 아늬 ?",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019909.1.39",
          "form": "마케팅은 일하신 지 얼마나되셨어요?",
          "original_form": "마케팅은 일하신 지 얼마나되셨어요?",
          "speaker_id": "1",
          "time": "20210827 22:56"
        }
      ]
    }
