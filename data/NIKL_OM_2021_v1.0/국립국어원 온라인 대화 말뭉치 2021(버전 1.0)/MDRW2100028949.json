      "utterance": [
        {
          "id": "MDRW2100028949.1.1",
          "form": "안녕하세요.\r\n편안한 밤 보내고 계신가요?",
          "original_form": "안녕하세요.\r\n편안한 밤 보내고 계신가요?",
          "speaker_id": "1",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028949.1.2",
          "form": "자려고요.ㅎ",
          "original_form": "자려고요.ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028949.1.3",
          "form": "안주무세요?",
          "original_form": "안주무세요?",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028949.1.4",
          "form": "잠이 깼어요",
          "original_form": "잠이 깼어요",
          "speaker_id": "2",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100028949.1.5",
          "form": "몇시에 주무세요?",
          "original_form": "몇시에 주무세요?",
          "speaker_id": "2",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100028949.1.6",
          "form": "요즘 무슨 책 읽으세요?",
          "original_form": "요즘 무슨 책 읽으세요?",
          "speaker_id": "1",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100028949.1.7",
          "form": "스포츠 관련 책자요.\r\n머슬지요",
          "original_form": "스포츠 관련 책자요.\r\n머슬지요",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028949.1.8",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028949.1.9",
          "form": "저는 역사서요",
          "original_form": "저는 역사서요",
          "speaker_id": "1",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028949.1.10",
          "form": "아~~제목이?",
          "original_form": "아~~제목이?",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028949.1.11",
          "form": "일과 관련된 책만 봐요.",
          "original_form": "일과 관련된 책만 봐요.",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028949.1.12",
          "form": "조선 왕실 관련책이요?",
          "original_form": "조선 왕실 관련책이요?",
          "speaker_id": "1",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028949.1.13",
          "form": "재미는 없는데 이상하게 다른책이 안봐지네요",
          "original_form": "재미는 없는데 이상하게 다른책이 안봐지네요",
          "speaker_id": "1",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028949.1.14",
          "form": "문화해설사 세요?",
          "original_form": "문화해설사 세요?",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028949.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028949.1.16",
          "form": "당구관련책도 보고싶은데 너무 어려워요",
          "original_form": "당구관련책도 보고싶은데 너무 어려워요",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028949.1.17",
          "form": "쉬운책이 없어요.\r\n이해력이 부족해서",
          "original_form": "쉬운책이 없어요.\r\n이해력이 부족해서",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028949.1.18",
          "form": "아~~좋은일 하시네요",
          "original_form": "아~~좋은일 하시네요",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028949.1.19",
          "form": "어디에서 계신거에요?",
          "original_form": "어디에서 계신거에요?",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028949.1.20",
          "form": "책은  하루에 얼마나 보시나요?",
          "original_form": "책은  하루에 얼마나 보시나요?",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028949.1.21",
          "form": "하루요!!",
          "original_form": "하루요!!",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028949.1.22",
          "form": "책을 보는 시간은 언제인가요?",
          "original_form": "책을 보는 시간은 언제인가요?",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028949.1.23",
          "form": "1년에 2권요'ㅎ",
          "original_form": "1년에 2권요'ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028949.1.24",
