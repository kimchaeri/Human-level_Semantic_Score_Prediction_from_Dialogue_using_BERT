      "utterance": [
        {
          "id": "MDRW2100044668.1.1",
          "form": "삼겹살 시켜먹고싶어요",
          "original_form": "삼겹살 시켜먹고싶어요",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044668.1.2",
          "form": "닭가슴살 드셧는데 삼겹살요?",
          "original_form": "닭가슴살 드셧는데 삼겹살요?",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044668.1.3",
          "form": "고민만 하다가 결국 안먹겠지...",
          "original_form": "고민만 하다가 결국 안먹겠지...",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044668.1.4",
          "form": "언니는 야식 뭐 좋아해?",
          "original_form": "언니는 야식 뭐 좋아해?",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044668.1.5",
          "form": "나는 오징어나 쥐포..",
          "original_form": "나는 오징어나 쥐포..",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044668.1.6",
          "form": "버터구이 오징어 좋아해?",
          "original_form": "버터구이 오징어 좋아해?",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044668.1.7",
          "form": "그런것도 좋아하고 그냥 마른오징어도 좋아해",
          "original_form": "그런것도 좋아하고 그냥 마른오징어도 좋아해",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044668.1.8",
          "form": "나는 마른 오징어 마요네즈 찍어먹는거 좋아해",
          "original_form": "나는 마른 오징어 마요네즈 찍어먹는거 좋아해",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044668.1.9",
          "form": "씹는맛 있는거..!",
          "original_form": "씹는맛 있는거..!",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044668.1.10",
          "form": "아 사람들 그렇게 많이 먹잖아",
          "original_form": "아 사람들 그렇게 많이 먹잖아",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044668.1.11",
          "form": "엉엉 씹는맛ㅋㅋ 난 고추장보다 마요네즈가 좋더라구",
          "original_form": "엉엉 씹는맛ㅋㅋ 난 고추장보다 마요네즈가 좋더라구",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044668.1.12",
          "form": "마요네즈에 와사비 넣어서도 많이들 먹더라",
          "original_form": "마요네즈에 와사비 넣어서도 많이들 먹더라",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044668.1.13",
          "form": "오! 난 와사비는 코 뻥 맛 때문에 못먹어 잘ㅠㅠ",
          "original_form": "오! 난 와사비는 코 뻥 맛 때문에 못먹어 잘ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044668.1.14",
          "form": "아 정말? 초밥도 와사비 안먹어?",
          "original_form": "아 정말? 초밥도 와사비 안먹어?",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044668.1.15",
          "form": "초밥 와사비 살짝만 넣어서 먹어ㅋㅋ",
          "original_form": "초밥 와사비 살짝만 넣어서 먹어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044668.1.16",
          "form": "아 나는 소고기에도 와사비 얹어서 먹어",
          "original_form": "아 나는 소고기에도 와사비 얹어서 먹어",
          "speaker_id": "1",
          "time": "20210907 23:29"
        }
      ]
