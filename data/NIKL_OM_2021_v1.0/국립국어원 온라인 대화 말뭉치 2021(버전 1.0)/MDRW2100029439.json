      "utterance": [
        {
          "id": "MDRW2100029439.1.1",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 08:09"
        },
        {
          "id": "MDRW2100029439.1.2",
          "form": "네 저는 농구 종종 봐요!",
          "original_form": "네 저는 농구 종종 봐요!",
          "speaker_id": "2",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100029439.1.3",
          "form": "다른 스포츠들도 좋아하고 축구도 가끔 보고 그래요 ㅎㅎ",
          "original_form": "다른 스포츠들도 좋아하고 축구도 가끔 보고 그래요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100029439.1.4",
          "form": "아 야구는요? ㅋㅋㅋㅋ",
          "original_form": "아 야구는요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:05"
        },
        {
          "id": "MDRW2100029439.1.5",
          "form": "야구도 종종 보는데 룰을 잘 몰라요ㅜ",
          "original_form": "야구도 종종 보는데 룰을 잘 몰라요ㅜ",
          "speaker_id": "2",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100029439.1.6",
          "form": "야구 좋아하시나봐요!",
          "original_form": "야구 좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100029439.1.7",
          "form": "코로나 전에는\r\n직관하러 자주 다녔었어요:)",
          "original_form": "코로나 전에는\r\n직관하러 자주 다녔었어요:)",
          "speaker_id": "1",
          "time": "20210904 22:19"
        },
        {
          "id": "MDRW2100029439.1.8",
          "form": "오오 무슨팀 응원하세요?",
          "original_form": "오오 무슨팀 응원하세요?",
          "speaker_id": "2",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100029439.1.9",
          "form": "보통 지역따라 간다던데!",
          "original_form": "보통 지역따라 간다던데!",
          "speaker_id": "2",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100029439.1.10",
          "form": "저는 엘지트윈스요",
          "original_form": "저는 엘지트윈스요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100029439.1.11",
          "form": "저는 지역말고\r\n남편따라 갔네요ㅋㅋㅋㅋ",
          "original_form": "저는 지역말고\r\n남편따라 갔네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100029439.1.12",
          "form": "아하ㅋㅋㅋ 좋아하는 선수는 누구에요?",
          "original_form": "아하ㅋㅋㅋ 좋아하는 선수는 누구에요?",
          "speaker_id": "2",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100029439.1.13",
          "form": "전 오타니랑 이정후 밖에 몰라요ㅠㅠㅎㅎ",
          "original_form": "전 오타니랑 이정후 밖에 몰라요ㅠㅠㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100029439.1.14",
          "form": "윌슨 좋아했었는데",
          "original_form": "윌슨 좋아했었는데",
          "speaker_id": "1",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100029439.1.15",
          "form": "이제 없어요",
          "original_form": "이제 없어요",
          "speaker_id": "1",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100029439.1.16",
          "form": "ㅠㅠㅠㅠ 저도 윌슨 이름 들어본거같아요",
          "original_form": "ㅠㅠㅠㅠ 저도 윌슨 이름 들어본거같아요",
          "speaker_id": "2",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100029439.1.17",
          "form": "잘하는 선수였나봐요?",
          "original_form": "잘하는 선수였나봐요?",
          "speaker_id": "2",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100029439.1.18",
          "form": "나혼산에서",
          "original_form": "나혼산에서",
          "speaker_id": "1",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100029439.1.19",
          "form": "들으신건 아니겠쥬? ㅠㅠㅋ",
          "original_form": "들으신건 아니겠쥬? ㅠㅠㅋ",
          "speaker_id": "1",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100029439.1.20",
          "form": "앜ㅋㅋㅋㅋ그런가?ㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ그런가?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:42"
        },
        {
          "id": "MDRW2100029439.1.21",
          "form": "뉴스에서 지나가다 봤어요ㅋㅋㅋ",
          "original_form": "뉴스에서 지나가다 봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:42"
        },
        {
          "id": "MDRW2100029439.1.22",
          "form": "실력도 좋고 매너도 좋은\r\n선수였는데 아쉬워요ㅠㅋ",
          "original_form": "실력도 좋고 매너도 좋은\r\n선수였는데 아쉬워요ㅠㅋ",
