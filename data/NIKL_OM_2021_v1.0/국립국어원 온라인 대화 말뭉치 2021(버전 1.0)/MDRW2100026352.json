      "utterance": [
        {
          "id": "MDRW2100026352.1.1",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026352.1.2",
          "form": "저는 회나 해산물 좋아해요",
          "original_form": "저는 회나 해산물 좋아해요",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026352.1.3",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026352.1.4",
          "form": "네!! 저는 게장먹었습니다",
          "original_form": "네!! 저는 게장먹었습니다",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026352.1.5",
          "form": "저도 회나 해산물 좋아하는데",
          "original_form": "저도 회나 해산물 좋아하는데",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026352.1.6",
          "form": "헉 대박...",
          "original_form": "헉 대박...",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026352.1.7",
          "form": "너무 맛있었겠어요",
          "original_form": "너무 맛있었겠어요",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026352.1.8",
          "form": "ㅎㅎ 오랜만에 게장 먹으니까 맛있더라구요!",
          "original_form": "ㅎㅎ 오랜만에 게장 먹으니까 맛있더라구요!",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026352.1.9",
          "form": "간장인가요?",
          "original_form": "간장인가요?",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026352.1.10",
          "form": "양념인가요?",
          "original_form": "양념인가요?",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026352.1.11",
          "form": "양념이요!!",
          "original_form": "양념이요!!",
          "speaker_id": "2",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026352.1.12",
          "form": "없어서 못 먹는게 게장인데 부럽네요",
          "original_form": "없어서 못 먹는게 게장인데 부럽네요",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026352.1.13",
          "form": "양념게장을 더 좋아하시나요?",
          "original_form": "양념게장을 더 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026352.1.14",
          "form": "음 집에서 만든거라 집에서는 늘 양념으로 만들게 되더라구요 ㅎㅎ",
          "original_form": "음 집에서 만든거라 집에서는 늘 양념으로 만들게 되더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026352.1.15",
          "form": "간장은 식당이 더 맛있는 느낌이에요",
          "original_form": "간장은 식당이 더 맛있는 느낌이에요",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026352.1.16",
          "form": "헉 만들어드셨어요?",
          "original_form": "헉 만들어드셨어요?",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026352.1.17",
          "form": "금손이시네요",
          "original_form": "금손이시네요",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026352.1.18",
          "form": "요즘 금어기 끝나서 꽃게 다시 나오던데",
          "original_form": "요즘 금어기 끝나서 꽃게 다시 나오던데",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026352.1.19",
          "form": "엄마가 만들어 주셨어요 ㅎㅎㅎ 저는 옆에서 보조만 ㅋㅋㅋㅋ",
          "original_form": "엄마가 만들어 주셨어요 ㅎㅎㅎ 저는 옆에서 보조만 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026352.1.20",
          "form": "그걸로 만드신거예요?ㅠㅠ",
          "original_form": "그걸로 만드신거예요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026352.1.21",
          "form": "헉 부러워요",
          "original_form": "헉 부러워요",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026352.1.22",
          "form": "그런거 같아요! 요새 꽃게 가격이 조금 내려가서 엄마가 주문하신 것 같더라구요",
          "original_form": "그런거 같아요! 요새 꽃게 가격이 조금 내려가서 엄마가 주문하신 것 같더라구요",
          "speaker_id": "2",
          "time": "20210901 21:52"
        }
      ]
