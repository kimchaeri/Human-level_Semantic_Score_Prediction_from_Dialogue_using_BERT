        {
          "id": "MDRW2100047426.1.1",
          "form": "영화좋아하세요?",
          "original_form": "영화좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 15:30"
        },
        {
          "id": "MDRW2100047426.1.2",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100047426.1.3",
          "form": "어떤 장르 좋아하세요?",
          "original_form": "어떤 장르 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100047426.1.4",
          "form": "장르안가리는 편이에요",
          "original_form": "장르안가리는 편이에요",
          "speaker_id": "2",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100047426.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100047426.1.6",
          "form": "저도 공포빼고는 장르 안가려요",
          "original_form": "저도 공포빼고는 장르 안가려요",
          "speaker_id": "1",
          "time": "20210826 15:33"
        },
        {
          "id": "MDRW2100047426.1.7",
          "form": "저도 공포는 싫어요 ㅋㅋ",
          "original_form": "저도 공포는 싫어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100047426.1.8",
          "form": "인생영화 있으세요?",
          "original_form": "인생영화 있으세요?",
          "speaker_id": "1",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100047426.1.9",
          "form": "그냥 우리들 인상깊게 봤어요",
          "original_form": "그냥 우리들 인상깊게 봤어요",
          "speaker_id": "2",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100047426.1.10",
          "form": "벌새 감독 영화 맞죠?",
          "original_form": "벌새 감독 영화 맞죠?",
          "speaker_id": "1",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100047426.1.11",
          "form": "아 아닌가?",
          "original_form": "아 아닌가?",
          "speaker_id": "1",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100047426.1.12",
          "form": "다른 분이에요",
          "original_form": "다른 분이에요",
          "speaker_id": "2",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100047426.1.13",
          "form": "영화 분위기가 비슷하긴하죠",
          "original_form": "영화 분위기가 비슷하긴하죠",
          "speaker_id": "2",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100047426.1.14",
          "form": "네 그래서 헷갈렷나봐요 ㅋㅋㅋㅋ",
          "original_form": "네 그래서 헷갈렷나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100047426.1.15",
          "form": "둘다 독립영화기도 해서 ㅋㅋㅋㅋ",
          "original_form": "둘다 독립영화기도 해서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100047426.1.16",
          "form": "name1님은 어떤 영화를 잘보셨나요?",
          "original_form": "&name1&님은 어떤 영화를 잘보셨나요?",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100047426.1.17",
          "form": "저는 그래비티 좋아해요",
          "original_form": "저는 그래비티 좋아해요",
