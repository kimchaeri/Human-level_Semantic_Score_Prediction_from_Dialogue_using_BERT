      "utterance": [
        {
          "id": "MDRW2100028464.1.1",
          "form": "스포츠요",
          "original_form": "스포츠요",
          "speaker_id": "2",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100028464.1.2",
          "form": "저는 본적이없어요 야구도",
          "original_form": "저는 본적이없어요 야구도",
          "speaker_id": "2",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100028464.1.3",
          "form": "스포츠하면 뭐다!?",
          "original_form": "스포츠하면 뭐다!?",
          "speaker_id": "1",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100028464.1.4",
          "form": "2020 도쿄 올림픽이다~",
          "original_form": "2020 도쿄 올림픽이다~",
          "speaker_id": "1",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100028464.1.5",
          "form": "아~",
          "original_form": "아~",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028464.1.6",
          "form": "그렇네",
          "original_form": "그렇네",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028464.1.7",
          "form": "근데ㅜ올림픽도 안봣어요 ㅋㅋㅋ",
          "original_form": "근데ㅜ올림픽도 안봣어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028464.1.8",
          "form": "이번에 국대 선수들이 잘했다는 건 알지만",
          "original_form": "이번에 국대 선수들이 잘했다는 건 알지만",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028464.1.9",
          "form": "아 진짜!?",
          "original_form": "아 진짜!?",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028464.1.10",
          "form": "나 엄청 열심히 봤어",
          "original_form": "나 엄청 열심히 봤어",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028464.1.11",
          "form": "양궁, 기계체조, 수영, 배구, 펜싱...",
          "original_form": "양궁, 기계체조, 수영, 배구, 펜싱...",
          "speaker_id": "1",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028464.1.12",
          "form": "웅 우리집 티비 치우고 수신료 끊엇거든",
          "original_form": "웅 우리집 티비 치우고 수신료 끊엇거든",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028464.1.13",
          "form": "유툽같은것도 잇지만 귀찮앗으",
          "original_form": "유툽같은것도 잇지만 귀찮앗으",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028464.1.14",
          "form": "많이 챙겨봣네~",
          "original_form": "많이 챙겨봣네~",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028464.1.15",
          "form": "헉 그렇구나 그럼 따로 안볼만도",
          "original_form": "헉 그렇구나 그럼 따로 안볼만도",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028464.1.16",
          "form": "나 김연경을 사랑하게 되어서 그 국대 옷도 주문했어",
          "original_form": "나 김연경을 사랑하게 되어서 그 국대 옷도 주문했어",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028464.1.17",
          "form": "이번이 마지막이라",
          "original_form": "이번이 마지막이라",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028464.1.18",
          "form": "헉.....",
          "original_form": "헉.....",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028464.1.19",
          "form": "하긴 그럴만해.ㅠㅠㅠㅠ",
          "original_form": "하긴 그럴만해.ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028464.1.20",
          "form": "좋긴좋드리구",
          "original_form": "좋긴좋드리구",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028464.1.21",
          "form": "식빵언니 이번에 광고도 받았다고 하고",
          "original_form": "식빵언니 이번에 광고도 받았다고 하고",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028464.1.22",
          "form": "너무 좋아",
          "original_form": "너무 좋아",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028464.1.23",
          "form": "은퇴하고 자유롭게 하고싶은거 하고 살았음 해",
          "original_form": "은퇴하고 자유롭게 하고싶은거 하고 살았음 해",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028464.1.24",
          "form": "왜 식빵인지 궁금해 ㅋㅋㅋㅋㅋ 빵굽는게 머야??? 공치는게 빵반죽 같아서 그런가?",
          "original_form": "왜 식빵인지 궁금해 ㅋㅋㅋㅋㅋ 빵굽는게 머야??? 공치는게 빵반죽 같아서 그런가?",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028464.1.25",
          "form": "아무래도 .... 일할만큼 했으면 하고싶은거 하고 살아도 되겟지",
          "original_form": "아무래도 .... 일할만큼 했으면 하고싶은거 하고 살아도 되겟지",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028464.1.26",
          "form": "거기엔 유래가 있어",
          "original_form": "거기엔 유래가 있어",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028464.1.27",
          "form": "그냥 김연경 선수가 배구 경기 하다가",
          "original_form": "그냥 김연경 선수가 배구 경기 하다가",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028464.1.28",
          "form": "실수했는지",
          "original_form": "실수했는지",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028464.1.29",
          "form": "비속어를 말했는데 흔히 말하는 열여덟",
          "original_form": "비속어를 말했는데 흔히 말하는 열여덟",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028464.1.30",
          "form": "근데 팬들이 욕한거 아니라고",
          "original_form": "근데 팬들이 욕한거 아니라고",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028464.1.31",
          "form": "식빵이라 말한거라구~",
          "original_form": "식빵이라 말한거라구~",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028464.1.32",
          "form": "엄청 부둥부둥 쉴드 해줘서",
          "original_form": "엄청 부둥부둥 쉴드 해줘서",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028464.1.33",
          "form": "식빵언니 타이틀 달게 된거야",
          "original_form": "식빵언니 타이틀 달게 된거야",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028464.1.34",
          "form": "귀여워 ㅋㅋㅋㅋㅋ",
          "original_form": "귀여워 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028464.1.35",
          "form": "식빵!",
          "original_form": "식빵!",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028464.1.36",
          "form": "진짜 마인드 본받을만해...",
          "original_form": "진짜 마인드 본받을만해...",
          "speaker_id": "1",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100028464.1.37",
          "form": "매번 경기 분석하고 자존감도 높고",
          "original_form": "매번 경기 분석하고 자존감도 높고",
          "speaker_id": "1",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100028464.1.38",
          "form": "좋아..멋지다",
          "original_form": "좋아..멋지다",
          "speaker_id": "2",
          "time": "20210902 20:33"
        }
      ]
    }
