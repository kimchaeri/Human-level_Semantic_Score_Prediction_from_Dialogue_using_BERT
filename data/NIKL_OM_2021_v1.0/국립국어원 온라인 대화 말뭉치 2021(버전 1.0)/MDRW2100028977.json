      "utterance": [
        {
          "id": "MDRW2100028977.1.1",
          "form": "집안일하시나요?",
          "original_form": "집안일하시나요?",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100028977.1.2",
          "form": "네ㅋㅋ님은 집안일하시나요?",
          "original_form": "네ㅋㅋ님은 집안일하시나요?",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100028977.1.3",
          "form": "네..지금도 아침하네요",
          "original_form": "네..지금도 아침하네요",
          "speaker_id": "1",
          "time": "20210903 07:39"
        },
        {
          "id": "MDRW2100028977.1.4",
          "form": "아고 제가 백신맞고 아파서 지금 톡하네요ㅜ",
          "original_form": "아고 제가 백신맞고 아파서 지금 톡하네요ㅜ",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100028977.1.5",
          "form": "집안일 분담안하시는건가요?",
          "original_form": "집안일 분담안하시는건가요?",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100028977.1.6",
          "form": "네~~백신맞으셨으면 쉬시는게",
          "original_form": "네~~백신맞으셨으면 쉬시는게",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100028977.1.7",
          "form": "1차맞으셨나요?",
          "original_form": "1차맞으셨나요?",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100028977.1.8",
          "form": "2차 다 맞았어요ㅎ",
          "original_form": "2차 다 맞았어요ㅎ",
          "speaker_id": "2",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100028977.1.9",
          "form": "아파서 집안일도 하나도 못했네요ㅜ",
          "original_form": "아파서 집안일도 하나도 못했네요ㅜ",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100028977.1.10",
          "form": "안심되겠네요",
          "original_form": "안심되겠네요",
          "speaker_id": "1",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100028977.1.11",
          "form": "네 홀가분해요ㅎ 지금은 좀 덜 아파서 정리못한 거 좀 정리중이에요",
          "original_form": "네 홀가분해요ㅎ 지금은 좀 덜 아파서 정리못한 거 좀 정리중이에요",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100028977.1.12",
          "form": "냉장고가 엉망이 되었네요...ㅎ",
          "original_form": "냉장고가 엉망이 되었네요...ㅎ",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100028977.1.13",
          "form": "쉬셔야하는데",
          "original_form": "쉬셔야하는데",
          "speaker_id": "1",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100028977.1.14",
          "form": "타이레놀 먹어서 괜찮아졌어요ㅎㅎ",
          "original_form": "타이레놀 먹어서 괜찮아졌어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100028977.1.15",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100028977.1.16",
          "form": "저는 얀센맞아서 한번에",
          "original_form": "저는 얀센맞아서 한번에",
          "speaker_id": "1",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100028977.1.17",
          "form": "6개월다되가서 부스터샷맞아야될지도",
          "original_form": "6개월다되가서 부스터샷맞아야될지도",
          "speaker_id": "1",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100028977.1.18",
          "form": "아아 다른 가족분들도 맞으셨나요?",
          "original_form": "아아 다른 가족분들도 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100028977.1.19",
