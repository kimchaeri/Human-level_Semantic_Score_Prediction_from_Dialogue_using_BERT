      "utterance": [
        {
          "id": "MDRW2100014634.1.1",
          "form": "컨테이젼아세요?",
          "original_form": "컨테이젼아세요?",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014634.1.2",
          "form": "그 영화도 오래전건데",
          "original_form": "그 영화도 오래전건데",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014634.1.3",
          "form": "첨볼때는 몰랐어요",
          "original_form": "첨볼때는 몰랐어요",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014634.1.4",
          "form": "어 죽나보다",
          "original_form": "어 죽나보다",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014634.1.5",
          "form": "전염병이다",
          "original_form": "전염병이다",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014634.1.6",
          "form": "아 본거 같은데..",
          "original_form": "아 본거 같은데..",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014634.1.7",
          "form": "기억이 잘 안나네요",
          "original_form": "기억이 잘 안나네요",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014634.1.8",
          "form": "근데 딱 코로나닥치니 티비에서 막하는거예요",
          "original_form": "근데 딱 코로나닥치니 티비에서 막하는거예요",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014634.1.9",
          "form": "느낌이 확달라요",
          "original_form": "느낌이 확달라요",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014634.1.10",
          "form": "ㅋㅋㅋㅋㅋ 그쵸... 드라마.. 같은걸 봐도",
          "original_form": "ㅋㅋㅋㅋㅋ 그쵸... 드라마.. 같은걸 봐도",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014634.1.11",
          "form": "진짜 코로나 같은거네 너무 무서워졌죠",
          "original_form": "진짜 코로나 같은거네 너무 무서워졌죠",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014634.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014634.1.13",
          "form": "이젠 생각이 틀려요",
          "original_form": "이젠 생각이 틀려요",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014634.1.14",
          "form": "전염병있는거나오면 와.. 현재상황같다고..",
          "original_form": "전염병있는거나오면 와.. 현재상황같다고..",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014634.1.15",
          "form": "그래서 예전 좋을깨보며 잊고 싶은가봐요",
          "original_form": "그래서 예전 좋을깨보며 잊고 싶은가봐요",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014634.1.16",
          "form": "네 완전 감정이입 ㅜㅜ",
          "original_form": "네 완전 감정이입 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014634.1.17",
          "form": "아 만할 코로나",
          "original_form": "아 만할 코로나",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014634.1.18",
          "form": "사라짐 좋겠어요",
          "original_form": "사라짐 좋겠어요",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014634.1.19",
          "form": "개발 해결하던데",
          "original_form": "개발 해결하던데",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014634.1.20",
          "form": "치료제가 현실엔 없죠",
          "original_form": "치료제가 현실엔 없죠",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014634.1.21",
          "form": "그러게요..  영화에서는 어떻게든 늘 해결하던데.. 저희도.. ㅎㅎㅎ 그런날이 오겠죠",
          "original_form": "그러게요..  영화에서는 어떻게든 늘 해결하던데.. 저희도.. ㅎㅎㅎ 그런날이 오겠죠",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014634.1.22",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014634.1.23",
          "form": "치료제를 구매하고싶어요 ㅋㅋ",
          "original_form": "치료제를 구매하고싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014634.1.24",
          "form": "오늘은 전 주식방송밖에 못봤네요 ㅡㅡ",
          "original_form": "오늘은 전 주식방송밖에 못봤네요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014634.1.25",
          "form": "유튜브 보고 ㅋㅌ",
          "original_form": "유튜브 보고 ㅋㅌ",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014634.1.26",
          "form": "현대차 ㅋㅋ",
          "original_form": "현대차 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014634.1.27",
          "form": "2주",
          "original_form": "2주",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014634.1.28",
          "form": "균등도 비례도 다 윈윈 현대차였네요",
          "original_form": "균등도 비례도 다 윈윈 현대차였네요",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014634.1.29",
          "form": "이러니 유튜브 보나봐요",
          "original_form": "이러니 유튜브 보나봐요",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014634.1.30",
          "form": "어디 넣으셨어요?",
          "original_form": "어디 넣으셨어요?",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014634.1.31",
          "form": "어제 정신없이 시크릿호텔 봤어요 10화까지ㅋㅋ",
          "original_form": "어제 정신없이 시크릿호텔 봤어요 10화까지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100014634.1.32",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100014634.1.33",
          "form": "진짜 대박이세요",
          "original_form": "진짜 대박이세요",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100014634.1.34",
          "form": "많이 보셨네요",
          "original_form": "많이 보셨네요",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100014634.1.35",
          "form": "유튜브보면서 균등만 노리고 있어서 현대차 넣어서",
          "original_form": "유튜브보면서 균등만 노리고 있어서 현대차 넣어서",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100014634.1.36",
          "form": "피곤하신데 못자고 무리하셨네요",
          "original_form": "피곤하신데 못자고 무리하셨네요",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100014634.1.37",
          "form": "2주라고 카톡왔더라구요",
          "original_form": "2주라고 카톡왔더라구요",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100014634.1.38",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100014634.1.39",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100014634.1.40",
          "form": "3주도 많던데 ㅜㅜ",
          "original_form": "3주도 많던데 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100014634.1.41",
          "form": "네...너무 궁금해서 어제 12시넘게까지 티빙만 달렸어요.. ㅋㅋ",
          "original_form": "네...너무 궁금해서 어제 12시넘게까지 티빙만 달렸어요.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100014634.1.42",
          "form": "그래도 현대차가 승리 입니다",
          "original_form": "그래도 현대차가 승리 입니다",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100014634.1.43",
          "form": "앙 그럴땐 달혀야해요",
          "original_form": "앙 그럴땐 달혀야해요",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100014634.1.44",
          "form": "저도 보고 싶은데ㅠ",
          "original_form": "저도 보고 싶은데ㅠ",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100014634.1.45",
          "form": "티빙이 없네요",
          "original_form": "티빙이 없네요",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100014634.1.46",
          "form": "앙 남궁민 좋은데",
          "original_form": "앙 남궁민 좋은데",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100014634.1.47",
          "form": "그래서 딴거 아무것도 한하고 딱집중!",
          "original_form": "그래서 딴거 아무것도 한하고 딱집중!",
          "speaker_id": "1",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100014634.1.48",
          "form": "잘하셨어요 ㅋㅋ",
          "original_form": "잘하셨어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100014634.1.49",
          "form": "근데 그 남자주인공",
          "original_form": "근데 그 남자주인공",
          "speaker_id": "1",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100014634.1.50",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100014634.1.51",
          "form": "원래남편도",
          "original_form": "원래남편도",
          "speaker_id": "1",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100014634.1.52",
          "form": "요새 안보여요",
          "original_form": "요새 안보여요",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100014634.1.53",
          "form": "넘달달해요",
          "original_form": "넘달달해요",
          "speaker_id": "1",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100014634.1.54",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100014634.1.55",
          "form": "두근두근ㅋㅋ질투하는게 귀엽고요",
          "original_form": "두근두근ㅋㅋ질투하는게 귀엽고요",
          "speaker_id": "1",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100014634.1.56",
          "form": "저 그분 다시 보고싶은데",
          "original_form": "저 그분 다시 보고싶은데",
          "speaker_id": "2",
