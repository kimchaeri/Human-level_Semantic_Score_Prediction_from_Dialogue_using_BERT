      "utterance": [
        {
          "id": "MDRW2100009455.1.1",
          "form": "다음번엔 고단백두유로 마시려고요",
          "original_form": "다음번엔 고단백두유로 마시려고요",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009455.1.2",
          "form": "벌크업 하시나요 ㅋㅋㅋㅋ",
          "original_form": "벌크업 하시나요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009455.1.3",
          "form": "야식 두유먹는거 좋은습관같아요",
          "original_form": "야식 두유먹는거 좋은습관같아요",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009455.1.4",
          "form": "밤에 뭐 먹으면 건강에 해롭다니까",
          "original_form": "밤에 뭐 먹으면 건강에 해롭다니까",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009455.1.5",
          "form": "벌크업ㅋㅋㅋ살짝 지금 하고 있는 거 같아요",
          "original_form": "벌크업ㅋㅋㅋ살짝 지금 하고 있는 거 같아요",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009455.1.6",
          "form": "요즘 군것질도 많이 하고 운동도 쬐금 해서요",
          "original_form": "요즘 군것질도 많이 하고 운동도 쬐금 해서요",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009455.1.7",
          "form": "근데 먹는게 군것질이라 벌크업은 아닌 것 같긴해요",
          "original_form": "근데 먹는게 군것질이라 벌크업은 아닌 것 같긴해요",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009455.1.8",
          "form": "먹는양 늘리면서 운동하기",
          "original_form": "먹는양 늘리면서 운동하기",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009455.1.9",
          "form": "건강해지시겠는데요",
          "original_form": "건강해지시겠는데요",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009455.1.10",
          "form": "name1님 저는 자야해서 저희 내일 아침식사에 대해 이야기해볼까요?",
          "original_form": "&name1&님 저는 자야해서 저희 내일 아침식사에 대해 이야기해볼까요?",
          "speaker_id": "2",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009455.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 너무 좋아요 안녕히주무세요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 너무 좋아요 안녕히주무세요",
          "speaker_id": "1",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009455.1.12",
          "form": "재밌었어요~ 좋은 밤 되셔요",
          "original_form": "재밌었어요~ 좋은 밤 되셔요",
          "speaker_id": "2",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009455.1.13",
          "form": "ㅎㅎ 아침 맛있게 드셨나요 저는 토스트 먹었어요",
          "original_form": "ㅎㅎ 아침 맛있게 드셨나요 저는 토스트 먹었어요",
          "speaker_id": "1",
          "time": "20210823 08:51"
        },
        {
          "id": "MDRW2100009455.1.14",
          "form": "전 방금 일어났네요ㅎㅎ",
          "original_form": "전 방금 일어났네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 09:36"
        },
        {
          "id": "MDRW2100009455.1.15",
          "form": "어떤 토스트 드셨어요?",
          "original_form": "어떤 토스트 드셨어요?",
          "speaker_id": "2",
          "time": "20210823 09:36"
        },
        {
          "id": "MDRW2100009455.1.16",
          "form": "식빵에 딸기잼에 계란후라이요 바빠서 ㅋㅋ ㅜ",
          "original_form": "식빵에 딸기잼에 계란후라이요 바빠서 ㅋㅋ ㅜ",
          "speaker_id": "1",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009455.1.17",
          "form": "이삭토스트 좋아하시나요",
          "original_form": "이삭토스트 좋아하시나요",
          "speaker_id": "1",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009455.1.18",
          "form": "맛있었겠네요",
          "original_form": "맛있었겠네요",
          "speaker_id": "2",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009455.1.19",
          "form": "클래식입니다",
          "original_form": "클래식입니다",
          "speaker_id": "1",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009455.1.20",
          "form": "아뇨 한 번도 안 먹어봤어요 친구들은 되게 좋아하더라고요",
          "original_form": "아뇨 한 번도 안 먹어봤어요 친구들은 되게 좋아하더라고요",
          "speaker_id": "2",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009455.1.21",
          "form": "헉 이삭 안드셔보셨구나",
          "original_form": "헉 이삭 안드셔보셨구나",
          "speaker_id": "1",
          "time": "20210823 10:36"
        },
        {
          "id": "MDRW2100009455.1.22",
          "form": "ㅋㅋㅋㅋ 혼자가 편해서 식음료로 돌아왔습니다",
          "original_form": "ㅋㅋㅋㅋ 혼자가 편해서 식음료로 돌아왔습니다",
