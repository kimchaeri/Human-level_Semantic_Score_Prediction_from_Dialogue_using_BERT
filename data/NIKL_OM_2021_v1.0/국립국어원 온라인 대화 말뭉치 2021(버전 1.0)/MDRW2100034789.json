        {
          "id": "MDRW2100034789.1.1",
          "form": "동아리 활동 해보셨나요?",
          "original_form": "동아리 활동 해보셨나요?",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100034789.1.2",
          "form": "네 ㅎ",
          "original_form": "네 ㅎ",
          "speaker_id": "2",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100034789.1.3",
          "form": "해보셨어요?",
          "original_form": "해보셨어요?",
          "speaker_id": "2",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100034789.1.4",
          "form": "네네! 어떤 동아리였나요?",
          "original_form": "네네! 어떤 동아리였나요?",
          "speaker_id": "1",
          "time": "20210828 20:46"
        },
        {
          "id": "MDRW2100034789.1.5",
          "form": "옷에 관한 동아리요",
          "original_form": "옷에 관한 동아리요",
          "speaker_id": "2",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100034789.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100034789.1.7",
          "form": "저는 영어동아리요!",
          "original_form": "저는 영어동아리요!",
          "speaker_id": "1",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100034789.1.8",
          "form": "우와..",
          "original_form": "우와..",
          "speaker_id": "2",
          "time": "20210828 20:52"
        },
        {
          "id": "MDRW2100034789.1.9",
          "form": "영어동아리면",
          "original_form": "영어동아리면",
          "speaker_id": "2",
          "time": "20210828 20:52"
        },
        {
          "id": "MDRW2100034789.1.10",
          "form": "영어로 뭐해요?",
          "original_form": "영어로 뭐해요?",
          "speaker_id": "2",
          "time": "20210828 20:52"
        },
        {
          "id": "MDRW2100034789.1.11",
          "form": "영어지문 해석 후 발표했어요",
          "original_form": "영어지문 해석 후 발표했어요",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100034789.1.12",
          "form": "와..그러면",
          "original_form": "와..그러면",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100034789.1.13",
          "form": "영어 잘하시겠어요!",
          "original_form": "영어 잘하시겠어요!",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100034789.1.14",
          "form": "음.....영어 실력은 그에 비례하지 않는 것 같습니다...ㅎ",
          "original_form": "음.....영어 실력은 그에 비례하지 않는 것 같습니다...ㅎ",
          "speaker_id": "1",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100034789.1.15",
          "form": "ㅋㅋㅋㅋㅋ 아 그래도",
          "original_form": "ㅋㅋㅋㅋㅋ 아 그래도",
          "speaker_id": "2",
          "time": "20210829 01:29"
        },
        {
          "id": "MDRW2100034789.1.16",
          "form": "나름 추억이네요..",
          "original_form": "나름 추억이네요..",
          "speaker_id": "2",
          "time": "20210829 01:29"
        },
        {
          "id": "MDRW2100034789.1.17",
          "form": "정말 학창시절만의 추억이에요ㅋㅋㅋㅋ",
          "original_form": "정말 학창시절만의 추억이에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:17"
        },
        {
          "id": "MDRW2100034789.1.18",
          "form": "ㅋㅋㅋㅋㅋ네 전 처음듣고",
          "original_form": "ㅋㅋㅋㅋㅋ네 전 처음듣고",
          "speaker_id": "2",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100034789.1.19",
          "form": "영어동아리 너무 신기했어요",
          "original_form": "영어동아리 너무 신기했어요",
          "speaker_id": "2",
          "time": "20210829 12:02"
        },
        {
