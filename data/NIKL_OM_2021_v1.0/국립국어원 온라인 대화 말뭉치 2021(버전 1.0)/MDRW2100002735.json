      "utterance": [
        {
          "id": "MDRW2100002735.1.1",
          "form": "연봉이 내년엔 꼭 오르길 바라며 !! ㅋ",
          "original_form": "연봉이 내년엔 꼭 오르길 바라며 !! ㅋ",
          "speaker_id": "1",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002735.1.2",
          "form": "맞아요 전 내년에 엄청 낼것 같아요",
          "original_form": "맞아요 전 내년에 엄청 낼것 같아요",
          "speaker_id": "2",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002735.1.3",
          "form": "연봉 올라야죠",
          "original_form": "연봉 올라야죠",
          "speaker_id": "2",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002735.1.4",
          "form": "너무 동결되어 있었어요",
          "original_form": "너무 동결되어 있었어요",
          "speaker_id": "2",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002735.1.5",
          "form": "물가는 계란 1만원 ... 무섭",
          "original_form": "물가는 계란 1만원 ... 무섭",
          "speaker_id": "1",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002735.1.6",
          "form": "다올라도 월급만 안오른단말 정녕 깨고싶네요",
          "original_form": "다올라도 월급만 안오른단말 정녕 깨고싶네요",
          "speaker_id": "2",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002735.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002735.1.8",
          "form": "계란 필수인데",
          "original_form": "계란 필수인데",
          "speaker_id": "2",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002735.1.9",
          "form": "더 어제 마트 오픈 행사가서 달걀 플렉스 햇네요",
          "original_form": "더 어제 마트 오픈 행사가서 달걀 플렉스 햇네요",
          "speaker_id": "1",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002735.1.10",
          "form": "3890원 판 ㄷㄷㄷ",
          "original_form": "3890원 판 ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002735.1.11",
          "form": "오 부러워요",
          "original_form": "오 부러워요",
          "speaker_id": "2",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002735.1.12",
          "form": "대박인데요",
          "original_form": "대박인데요",
          "speaker_id": "2",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002735.1.13",
          "form": "세상에 없을 가격이네요",
          "original_form": "세상에 없을 가격이네요",
          "speaker_id": "2",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002735.1.14",
          "form": "저도 깜놀래서 와이건 사야대 하고",
          "original_form": "저도 깜놀래서 와이건 사야대 하고",
          "speaker_id": "1",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002735.1.15",
          "form": "달걀 실컷 먹으려면 얼마 벌어야 하죠 ㅋㅋ",
          "original_form": "달걀 실컷 먹으려면 얼마 벌어야 하죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002735.1.16",
          "form": "막사야죠",
          "original_form": "막사야죠",
          "speaker_id": "2",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002735.1.17",
          "form": "으으으음 ㅜㅜ",
          "original_form": "으으으음 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002735.1.18",
          "form": "달걀만 먹고 사는거 아니니 흑흑",
          "original_form": "달걀만 먹고 사는거 아니니 흑흑",
          "speaker_id": "1",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002735.1.19",
          "form": "네 ㅋㅋ그렇죠",
          "original_form": "네 ㅋㅋ그렇죠",
          "speaker_id": "2",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002735.1.20",
          "form": "월급이 많아요 먹고 싶은거소 냄마대로 ㅎㅎㅎ",
          "original_form": "월급이 많아요 먹고 싶은거소 냄마대로 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002735.1.21",
          "form": "둘이 버시면 혼자보다 여유가 생기죠?",
          "original_form": "둘이 버시면 혼자보다 여유가 생기죠?",
          "speaker_id": "2",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002735.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002735.1.23",
          "form": "그게 정답이죠 ㅎㅎ",
          "original_form": "그게 정답이죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002735.1.24",
          "form": "전 이상하게 버는데 어디로 다가고 없네요 ㅋㅋ",
          "original_form": "전 이상하게 버는데 어디로 다가고 없네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002735.1.25",
          "form": "여유는.생기는데 장단점이 있더라구요 ㅎ",
          "original_form": "여유는.생기는데 장단점이 있더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002735.1.26",
          "form": "그렇죠 완벽한게 없어요",
          "original_form": "그렇죠 완벽한게 없어요",
          "speaker_id": "2",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002735.1.27",
          "form": "집안일에 직장에 고생 많으세요",
          "original_form": "집안일에 직장에 고생 많으세요",
          "speaker_id": "2",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002735.1.28",
          "form": "전 능력자신거 같아 좋아보여요 ㅎㅎ 취업 요새 고민중이라",
          "original_form": "전 능력자신거 같아 좋아보여요 ㅎㅎ 취업 요새 고민중이라",
          "speaker_id": "2",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002735.1.29",
          "form": "취업이 생각보다 어려우니 \r\n일하는 사람 부럽단 말이 나오는거 같아요",
          "original_form": "취업이 생각보다 어려우니 \r\n일하는 사람 부럽단 말이 나오는거 같아요",
          "speaker_id": "1",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002735.1.30",
          "form": "그렇죠 취업 엄두도 안나요",
          "original_form": "그렇죠 취업 엄두도 안나요",
          "speaker_id": "2",
          "time": "20210820 15:17"
        },
        {
          "id": "MDRW2100002735.1.31",
          "form": "경제가 얼른 풀리고  코로나도  후",
          "original_form": "경제가 얼른 풀리고  코로나도  후",
          "speaker_id": "1",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002735.1.32",
          "form": "네 어서 풀려야죠 코로나도 사라지고요",
          "original_form": "네 어서 풀려야죠 코로나도 사라지고요",
          "speaker_id": "2",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002735.1.33",
          "form": "서민들이 살만한 나라 됐음 좋겠어요",
          "original_form": "서민들이 살만한 나라 됐음 좋겠어요",
          "speaker_id": "1",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002735.1.34",
          "form": "맞아요 지금은 힘들어요",
          "original_form": "맞아요 지금은 힘들어요",
          "speaker_id": "2",
          "time": "20210820 15:20"
        }
      ]
