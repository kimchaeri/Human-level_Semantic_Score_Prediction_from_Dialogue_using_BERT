      "utterance": [
        {
          "id": "MDRW2100031644.1.1",
          "form": "덕분에 새로운 음식도 알아가네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "덕분에 새로운 음식도 알아가네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031644.1.2",
          "form": "건과일치즈딥 맛있어보여요",
          "original_form": "건과일치즈딥 맛있어보여요",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031644.1.3",
          "form": "아녜요ㅋㅋㅋ 빨리 넘기면 좋죠ㅋㅋㅋㅋㅋ!!",
          "original_form": "아녜요ㅋㅋㅋ 빨리 넘기면 좋죠ㅋㅋㅋㅋㅋ!!",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031644.1.4",
          "form": "저도 처음 알게됐는데 맛있어보여요!",
          "original_form": "저도 처음 알게됐는데 맛있어보여요!",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031644.1.5",
          "form": "서양요리? 인 것 같은데",
          "original_form": "서양요리? 인 것 같은데",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031644.1.6",
          "form": "역시 여기서도 단짠을...!",
          "original_form": "역시 여기서도 단짠을...!",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031644.1.7",
          "form": "맞아요 약간 이런저런 치즈를 잔뜩넣어서 건과일이랑 섞어먹는 건가봐요",
          "original_form": "맞아요 약간 이런저런 치즈를 잔뜩넣어서 건과일이랑 섞어먹는 건가봐요",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031644.1.8",
          "form": "앗 ㅋㅋㅋ 여기서도 나타났네요",
          "original_form": "앗 ㅋㅋㅋ 여기서도 나타났네요",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031644.1.9",
          "form": "와 나쵸 찍어먹는 사진을 봤어요",
          "original_form": "와 나쵸 찍어먹는 사진을 봤어요",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031644.1.10",
          "form": "나쵸... 너무 잘 어울리겠네요",
          "original_form": "나쵸... 너무 잘 어울리겠네요",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031644.1.11",
          "form": "단짠단짠의 황금밸런스",
          "original_form": "단짠단짠의 황금밸런스",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031644.1.12",
          "form": "엇 저랑 같은걸 보셨나봐요",
          "original_form": "엇 저랑 같은걸 보셨나봐요",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031644.1.13",
          "form": "비주얼도 그렇고 맛있어보어요 ㅠㅠ",
          "original_form": "비주얼도 그렇고 맛있어보어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031644.1.14",
          "form": "크래커나 빵일줄 알았는데",
          "original_form": "크래커나 빵일줄 알았는데",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031644.1.15",
          "form": "나쵸 너무 천재적이에요...!",
          "original_form": "나쵸 너무 천재적이에요...!",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031644.1.16",
          "form": "그러고보면 기본 치즈딥도 나쵸랑 잘 어울리고",
          "original_form": "그러고보면 기본 치즈딥도 나쵸랑 잘 어울리고",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031644.1.17",
          "form": "그니까요 나쵸에 치즈 올려먹어도 맛있겠어요",
          "original_form": "그니까요 나쵸에 치즈 올려먹어도 맛있겠어요",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031644.1.18",
          "form": "세상에 치즈가 안 어울릴 음식이 참 드물 것 같아요ㅋㅋㅋㅋ",
          "original_form": "세상에 치즈가 안 어울릴 음식이 참 드물 것 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031644.1.19",
          "form": "나쵸 치즈 최고죠ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "나쵸 치즈 최고죠ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031644.1.20",
          "form": "그니까요 ㅋㅋ 완전 간식에 맥주안주느낌이에요 ㅠㅠ",
          "original_form": "그니까요 ㅋㅋ 완전 간식에 맥주안주느낌이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031644.1.21",
          "form": "치즈 진짜 조화의 정석이에요",
          "original_form": "치즈 진짜 조화의 정석이에요",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031644.1.22",
          "form": "막 찌개에도 넣어먹잖아요 치즈는 ㅎㅎ",
          "original_form": "막 찌개에도 넣어먹잖아요 치즈는 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031644.1.23",
          "form": "가벼운 간식 디저트부터 고급 와인까지 치즈가 안어울리는 곳이 없네요",
          "original_form": "가벼운 간식 디저트부터 고급 와인까지 치즈가 안어울리는 곳이 없네요",
          "speaker_id": "2",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031644.1.24",
          "form": "부대찌개에 치즈도 최고죠!!",
          "original_form": "부대찌개에 치즈도 최고죠!!",
          "speaker_id": "2",
          "time": "20210904 23:24"
        }
      ]
    }
