      "utterance": [
        {
          "id": "MDRW2100003825.1.1",
          "form": "혹시 어떤음식 좋아하세요?",
          "original_form": "혹시 어떤음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 12:32"
        },
        {
          "id": "MDRW2100003825.1.2",
          "form": "저는 삼겹살이욯",
          "original_form": "저는 삼겹살이욯",
          "speaker_id": "2",
          "time": "20210821 12:33"
        },
        {
          "id": "MDRW2100003825.1.3",
          "form": "배달음식은 자주 시켜드세요?",
          "original_form": "배달음식은 자주 시켜드세요?",
          "speaker_id": "1",
          "time": "20210821 12:35"
        },
        {
          "id": "MDRW2100003825.1.4",
          "form": "네~일주일에 한번은 시키는것 같아요",
          "original_form": "네~일주일에 한번은 시키는것 같아요",
          "speaker_id": "2",
          "time": "20210821 12:35"
        },
        {
          "id": "MDRW2100003825.1.5",
          "form": "어제도 오늘 먹으려고 피자 시켜놨어요",
          "original_form": "어제도 오늘 먹으려고 피자 시켜놨어요",
          "speaker_id": "2",
          "time": "20210821 12:36"
        },
        {
          "id": "MDRW2100003825.1.6",
          "form": "어디 피자 시키셨어요?",
          "original_form": "어디 피자 시키셨어요?",
          "speaker_id": "1",
          "time": "20210821 12:38"
        },
        {
          "id": "MDRW2100003825.1.7",
          "form": "청년피자요",
          "original_form": "청년피자요",
          "speaker_id": "2",
          "time": "20210821 12:39"
        },
        {
          "id": "MDRW2100003825.1.8",
          "form": "아시나요?",
          "original_form": "아시나요?",
          "speaker_id": "2",
          "time": "20210821 12:39"
        },
        {
          "id": "MDRW2100003825.1.9",
          "form": "처음 들어보네요",
          "original_form": "처음 들어보네요",
          "speaker_id": "1",
          "time": "20210821 12:40"
        },
        {
          "id": "MDRW2100003825.1.10",
          "form": "맛있어요?",
          "original_form": "맛있어요?",
          "speaker_id": "1",
          "time": "20210821 12:40"
        },
        {
          "id": "MDRW2100003825.1.11",
          "form": "요즘 태연이 광고하는건데 ㅎ",
          "original_form": "요즘 태연이 광고하는건데 ㅎ",
          "speaker_id": "2",
          "time": "20210821 12:41"
        },
        {
          "id": "MDRW2100003825.1.12",
          "form": "맛있어요 ㅎ 가성비도 좋고요",
          "original_form": "맛있어요 ㅎ 가성비도 좋고요",
          "speaker_id": "2",
          "time": "20210821 12:41"
        },
        {
          "id": "MDRW2100003825.1.13",
          "form": "면요리는 안좋아하세요?",
          "original_form": "면요리는 안좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 12:58"
        },
        {
          "id": "MDRW2100003825.1.14",
          "form": "좋아해요 국수~",
          "original_form": "좋아해요 국수~",
          "speaker_id": "2",
          "time": "20210821 12:59"
        },
        {
          "id": "MDRW2100003825.1.15",
          "form": "name1님은 면요리 좋아하시군요",
          "original_form": "&name1&님은 면요리 좋아하시군요",
          "speaker_id": "2",
          "time": "20210821 13:00"
        },
        {
          "id": "MDRW2100003825.1.16",
          "form": "네 라면이나 짜장면같은게 좋더라구요",
          "original_form": "네 라면이나 짜장면같은게 좋더라구요",
          "speaker_id": "1",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100003825.1.17",
          "form": "짜장면 말씀하시니 먹고 싶네요 ㅎ",
          "original_form": "짜장면 말씀하시니 먹고 싶네요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100003825.1.18",
          "form": "비 많이 내리는날에는 짜장면이 좋더라구요",
