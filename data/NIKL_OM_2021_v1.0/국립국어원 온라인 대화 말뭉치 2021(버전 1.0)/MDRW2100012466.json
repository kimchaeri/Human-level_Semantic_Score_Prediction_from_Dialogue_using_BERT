        {
          "id": "MDRW2100012466.1.1",
          "form": "집안일 자주하시나요?",
          "original_form": "집안일 자주하시나요?",
          "speaker_id": "1",
          "time": "20210824 22:16"
        },
        {
          "id": "MDRW2100012466.1.2",
          "form": "아니요. 방 청소만 하는 거 같아요.",
          "original_form": "아니요. 방 청소만 하는 거 같아요.",
          "speaker_id": "2",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100012466.1.3",
          "form": "자취 하세요?",
          "original_form": "자취 하세요?",
          "speaker_id": "1",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100012466.1.4",
          "form": "가족이랑 같이 살아요",
          "original_form": "가족이랑 같이 살아요",
          "speaker_id": "2",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100012466.1.5",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210826 14:36"
        },
        {
          "id": "MDRW2100012466.1.6",
          "form": "가족들이 집안일 같이 해주니까 좀 나아요",
          "original_form": "가족들이 집안일 같이 해주니까 좀 나아요",
          "speaker_id": "2",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100012466.1.7",
          "form": "저도 분담하는 편이엥ᆢ",
          "original_form": "저도 분담하는 편이엥ᆢ",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100012466.1.8",
          "form": "좋네요. 전 빨래가 가장 귀찮더라구요",
          "original_form": "좋네요. 전 빨래가 가장 귀찮더라구요",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100012466.1.9",
          "form": "저는 음쓰 버리는거요",
          "original_form": "저는 음쓰 버리는거요",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100012466.1.10",
          "form": "맞아요ㅠ 여름철엔 냄새도 너무 심하구요",
          "original_form": "맞아요ㅠ 여름철엔 냄새도 너무 심하구요",
          "speaker_id": "2",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100012466.1.11",
          "form": "벌레도 생기죠",
          "original_form": "벌레도 생기죠",
          "speaker_id": "1",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100012466.1.12",
          "form": "네 그래서 빨리 치워야되잖아요",
          "original_form": "네 그래서 빨리 치워야되잖아요",
          "speaker_id": "2",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100012466.1.13",
