      "utterance": [
        {
          "id": "MDRW2100034795.1.1",
          "form": "네네ㅋㅋㅋㅋ 저는 호불호 갈리는것들 거의 대부분을",
          "original_form": "네네ㅋㅋㅋㅋ 저는 호불호 갈리는것들 거의 대부분을",
          "speaker_id": "2",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100034795.1.2",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100034795.1.3",
          "form": "데자와 음료수도 좋아하고",
          "original_form": "데자와 음료수도 좋아하고",
          "speaker_id": "2",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100034795.1.4",
          "form": "하와이안피자도 좋아해요",
          "original_form": "하와이안피자도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100034795.1.5",
          "form": "데자와가 밀크티 음료수죠?",
          "original_form": "데자와가 밀크티 음료수죠?",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034795.1.6",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034795.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034795.1.8",
          "form": "그 베이지색? 음료수",
          "original_form": "그 베이지색? 음료수",
          "speaker_id": "2",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034795.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034795.1.10",
          "form": "저 그걸로 밀크티 처음 알게 됐어요",
          "original_form": "저 그걸로 밀크티 처음 알게 됐어요",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034795.1.11",
          "form": "너무 맛있더라고요ㅋㅋㅋ",
          "original_form": "너무 맛있더라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034795.1.12",
          "form": "저두 이걸로 밀크티 입문했어요ㅋㅋㅋ",
          "original_form": "저두 이걸로 밀크티 입문했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034795.1.13",
          "form": "이것도 호불호가 엄청 갈리더라구요",
          "original_form": "이것도 호불호가 엄청 갈리더라구요",
          "speaker_id": "2",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100034795.1.14",
          "form": "차랑 밀크티 자체가 호불호 갈리는 거 같아요",
          "original_form": "차랑 밀크티 자체가 호불호 갈리는 거 같아요",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034795.1.15",
          "form": "제 친구들은 무슨 맛으로 먹는지 모르겠다고 하더라고요ㅋㅋㅋㅋㅋ",
          "original_form": "제 친구들은 무슨 맛으로 먹는지 모르겠다고 하더라고요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034795.1.16",
          "form": "차도 향이 좋아서 마신다니까 잘 모르겠다고ㅋㅋㅋ",
          "original_form": "차도 향이 좋아서 마신다니까 잘 모르겠다고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034795.1.17",
          "form": "제 주위도 그래요ㅋㅋㅋ 약간 특이입맛 취급 당하고 있어요ㅋㅋ",
          "original_form": "제 주위도 그래요ㅋㅋㅋ 약간 특이입맛 취급 당하고 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034795.1.18",
          "form": "저도요 어른이라고~ 자기들도 동갑이면서ㅋㅋㅋㅋ",
          "original_form": "저도요 어른이라고~ 자기들도 동갑이면서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034795.1.19",
          "form": "ㅋㅋㅋㅋ 그러게요",
          "original_form": "ㅋㅋㅋㅋ 그러게요",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034795.1.20",
          "form": "떡볶이 피자 이런거 좋아하는",
          "original_form": "떡볶이 피자 이런거 좋아하는",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100034795.1.21",
          "form": "애입맛인데",
          "original_form": "애입맛인데",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100034795.1.22",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100034795.1.23",
          "form": "맞아요~재밌어요ㅋㅋㅋㅋ",
          "original_form": "맞아요~재밌어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034795.1.24",
          "form": "술은 좋아하시나요?",
          "original_form": "술은 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034795.1.25",
          "form": "술은 잘 못마셔요ㅠㅠ",
          "original_form": "술은 잘 못마셔요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034795.1.26",
          "form": "저번에 와인마시고",
          "original_form": "저번에 와인마시고",
          "speaker_id": "2",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034795.1.27",
          "form": "숙취때문에",
          "original_form": "숙취때문에",
          "speaker_id": "2",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034795.1.28",
          "form": "아우ㅋㅋㅋㅋ",
          "original_form": "아우ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034795.1.29",
          "form": "저는 와인은 스파클링 와인밖에 안 마셔봤어요. 도수가 높은가요?",
          "original_form": "저는 와인은 스파클링 와인밖에 안 마셔봤어요. 도수가 높은가요?",
          "speaker_id": "1",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100034795.1.30",
          "form": "음 제가 먹었던 레드와인은",
          "original_form": "음 제가 먹었던 레드와인은",
          "speaker_id": "2",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100034795.1.31",
          "form": "13도? 정도 됐었어용",
          "original_form": "13도? 정도 됐었어용",
          "speaker_id": "2",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100034795.1.32",
          "form": "소주보단 낮아용",
          "original_form": "소주보단 낮아용",
          "speaker_id": "2",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100034795.1.33",
          "form": "오 그랬군요!",
          "original_form": "오 그랬군요!",
          "speaker_id": "1",
          "time": "20210828 19:58"
        }
      ]
