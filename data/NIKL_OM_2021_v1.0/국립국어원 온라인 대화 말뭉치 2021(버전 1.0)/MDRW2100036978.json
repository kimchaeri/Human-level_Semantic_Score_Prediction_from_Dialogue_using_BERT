      "utterance": [
        {
          "id": "MDRW2100036978.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 16:02"
        },
        {
          "id": "MDRW2100036978.1.2",
          "form": "여행좋아하세요",
          "original_form": "여행좋아하세요",
          "speaker_id": "1",
          "time": "20210831 16:02"
        },
        {
          "id": "MDRW2100036978.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210831 16:11"
        },
        {
          "id": "MDRW2100036978.1.4",
          "form": "네 좋아하기는 하는데 기회가 없어서 별로 다녀오진 못했네요 ㅎㅎ",
          "original_form": "네 좋아하기는 하는데 기회가 없어서 별로 다녀오진 못했네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 16:11"
        },
        {
          "id": "MDRW2100036978.1.5",
          "form": "name1님은 최근에 다녀오신 데 있으세요?",
          "original_form": "&name1&님은 최근에 다녀오신 데 있으세요?",
          "speaker_id": "2",
          "time": "20210831 16:11"
        },
        {
          "id": "MDRW2100036978.1.6",
          "form": "최근이래도ㅜ1년넘게 전인 코로나전에 베트남다녀왔었어요",
          "original_form": "최근이래도ㅜ1년넘게 전인 코로나전에 베트남다녀왔었어요",
          "speaker_id": "1",
          "time": "20210831 16:12"
        },
        {
          "id": "MDRW2100036978.1.7",
          "form": "name2님은 가장최근에 어디다녀오셨나요",
          "original_form": "&name2&님은 가장최근에 어디다녀오셨나요",
          "speaker_id": "1",
          "time": "20210831 16:12"
        },
        {
          "id": "MDRW2100036978.1.8",
          "form": "우와 베트남...!!!",
          "original_form": "우와 베트남...!!!",
          "speaker_id": "2",
          "time": "20210831 16:13"
        },
        {
          "id": "MDRW2100036978.1.9",
          "form": "베트남은 음식이 참 맛있죠",
          "original_form": "베트남은 음식이 참 맛있죠",
          "speaker_id": "2",
          "time": "20210831 16:13"
        },
        {
          "id": "MDRW2100036978.1.10",
          "form": "저는 그냥 가까운 양수리 다녀왔어요~",
          "original_form": "저는 그냥 가까운 양수리 다녀왔어요~",
          "speaker_id": "2",
          "time": "20210831 16:13"
        },
        {
          "id": "MDRW2100036978.1.11",
          "form": "부모님이랑 같이요 ㅎㅎ",
          "original_form": "부모님이랑 같이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 16:13"
        },
        {
          "id": "MDRW2100036978.1.12",
          "form": "여행이라기엔 그냥 나들이에 가까웠지만요!",
          "original_form": "여행이라기엔 그냥 나들이에 가까웠지만요!",
          "speaker_id": "2",
          "time": "20210831 16:13"
        },
        {
          "id": "MDRW2100036978.1.13",
          "form": "네 한국도 베트남음식많아져서 가서먹어도익숙하더라구여 ㅋㅋㅋㅋ",
          "original_form": "네 한국도 베트남음식많아져서 가서먹어도익숙하더라구여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:16"
        },
        {
          "id": "MDRW2100036978.1.14",
          "form": "양수리 구경어디하시나요",
          "original_form": "양수리 구경어디하시나요",
          "speaker_id": "1",
          "time": "20210831 16:16"
        },
        {
          "id": "MDRW2100036978.1.15",
          "form": "베트남에서 먹는 베트남 음식도 비슷한가요? ㅎㅎㅎ",
          "original_form": "베트남에서 먹는 베트남 음식도 비슷한가요? ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 16:17"
        },
        {
          "id": "MDRW2100036978.1.16",
          "form": "저는 세미원 다녀왔었어요!",
          "original_form": "저는 세미원 다녀왔었어요!",
          "speaker_id": "2",
          "time": "20210831 16:17"
        },
        {
          "id": "MDRW2100036978.1.17",
          "form": "연꽃 보러 갔었는데",
          "original_form": "연꽃 보러 갔었는데",
          "speaker_id": "2",
          "time": "20210831 16:17"
        },
        {
          "id": "MDRW2100036978.1.18",
          "form": "알고보니 연꽃은",
          "original_form": "알고보니 연꽃은",
          "speaker_id": "2",
          "time": "20210831 16:17"
        },
        {
          "id": "MDRW2100036978.1.19",
          "form": "오전에 가야 활짝 핀 걸 볼 수 있다고 하더라구요",
          "original_form": "오전에 가야 활짝 핀 걸 볼 수 있다고 하더라구요",
          "speaker_id": "2",
          "time": "20210831 16:18"
        },
        {
          "id": "MDRW2100036978.1.20",
          "form": "좀더 달고 고수만땅이었어요",
          "original_form": "좀더 달고 고수만땅이었어요",
          "speaker_id": "1",
          "time": "20210831 16:19"
        },
        {
          "id": "MDRW2100036978.1.21",
          "form": "연꽃 너무이쁜데  저도 가보고싶네요ㅜ",
          "original_form": "연꽃 너무이쁜데  저도 가보고싶네요ㅜ",
          "speaker_id": "1",
          "time": "20210831 16:19"
        },
        {
          "id": "MDRW2100036978.1.22",
          "form": "우와 고수 잘 드시나요?",
          "original_form": "우와 고수 잘 드시나요?",
          "speaker_id": "2",
          "time": "20210831 16:20"
        },
        {
          "id": "MDRW2100036978.1.23",
          "form": "저도 그 때 햇빛 짱짱할 때 가서",
          "original_form": "저도 그 때 햇빛 짱짱할 때 가서",
          "speaker_id": "2",
          "time": "20210831 16:20"
        },
        {
          "id": "MDRW2100036978.1.24",
          "form": "넵 원래 잘못먹었는데",
          "original_form": "넵 원래 잘못먹었는데",
          "speaker_id": "1",
          "time": "20210831 16:20"
        },
        {
          "id": "MDRW2100036978.1.25",
          "form": "다 오므라진 연꽃만 보고 왔네요...",
          "original_form": "다 오므라진 연꽃만 보고 왔네요...",
          "speaker_id": "2",
          "time": "20210831 16:20"
        },
        {
          "id": "MDRW2100036978.1.26",
          "form": "반미에들어간고수 먹고부터 잘먹게됐어요",
          "original_form": "반미에들어간고수 먹고부터 잘먹게됐어요",
          "speaker_id": "1",
          "time": "20210831 16:20"
        },
        {
          "id": "MDRW2100036978.1.27",
          "form": "다오므라진 ㅜㅜㅜㅜㅜ이런",
          "original_form": "다오므라진 ㅜㅜㅜㅜㅜ이런",
          "speaker_id": "1",
          "time": "20210831 16:20"
        },
        {
          "id": "MDRW2100036978.1.28",
          "form": "오오 반미!!!",
          "original_form": "오오 반미!!!",
          "speaker_id": "2",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100036978.1.29",
          "form": "이따가 맛있는 음식에 대해서도 이야기 나눠야겠네요 ㅎㅎ",
          "original_form": "이따가 맛있는 음식에 대해서도 이야기 나눠야겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100036978.1.30",
          "form": "양수리 세미원에 가시면",
          "original_form": "양수리 세미원에 가시면",
          "speaker_id": "2",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100036978.1.31",
          "form": "주차는 무료라서",
          "original_form": "주차는 무료라서",
          "speaker_id": "2",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100036978.1.32",
          "form": "오 주차무료 중요한데",
          "original_form": "오 주차무료 중요한데",
          "speaker_id": "1",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100036978.1.33",
          "form": "가성비 있게 연꽃 구경 실컷 하실 수 있으실거예요!!!",
          "original_form": "가성비 있게 연꽃 구경 실컷 하실 수 있으실거예요!!!",
          "speaker_id": "2",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100036978.1.34",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100036978.1.35",
          "form": "팁감사해요",
          "original_form": "팁감사해요",
