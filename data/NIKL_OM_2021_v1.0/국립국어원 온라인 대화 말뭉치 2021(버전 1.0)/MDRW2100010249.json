      "utterance": [
        {
          "id": "MDRW2100010249.1.1",
          "form": "처서네요 환절기",
          "original_form": "처서네요 환절기",
          "speaker_id": "1",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010249.1.2",
          "form": "네 신기하게 절기가 다맞네요",
          "original_form": "네 신기하게 절기가 다맞네요",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010249.1.3",
          "form": "태풍으로 글케되네요ㅋㅋ",
          "original_form": "태풍으로 글케되네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010249.1.4",
          "form": "가을에 태풍이라니..흔하지않은거같구",
          "original_form": "가을에 태풍이라니..흔하지않은거같구",
          "speaker_id": "2",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010249.1.5",
          "form": "이상기후라서 점점 심해질듯해요ㅜ",
          "original_form": "이상기후라서 점점 심해질듯해요ㅜ",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010249.1.6",
          "form": "맞아요 진짜 피부에 와닿는거같아요ㅠㅠ",
          "original_form": "맞아요 진짜 피부에 와닿는거같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010249.1.7",
          "form": "우산챙기세요",
          "original_form": "우산챙기세요",
          "speaker_id": "1",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010249.1.8",
          "form": "전 양산으로도써요",
          "original_form": "전 양산으로도써요",
          "speaker_id": "1",
          "time": "20210823 16:07"
        },
        {
          "id": "MDRW2100010249.1.9",
          "form": "우양산ㅎㅎ 안갖고 다니면 갑자기 비내릴때 낭패더라구요",
          "original_form": "우양산ㅎㅎ 안갖고 다니면 갑자기 비내릴때 낭패더라구요",
          "speaker_id": "2",
          "time": "20210823 16:07"
        },
        {
          "id": "MDRW2100010249.1.10",
          "form": "이번여름은 뜨거워서 UV차단 양산이 필수더라구요",
          "original_form": "이번여름은 뜨거워서 UV차단 양산이 필수더라구요",
          "speaker_id": "1",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010249.1.11",
          "form": "그걸우산으로 또 쓰는거져ㅋ",
          "original_form": "그걸우산으로 또 쓰는거져ㅋ",
          "speaker_id": "1",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010249.1.12",
          "form": "맞아요 여름 짧은줄 알았는데 살이 탔더라구요ㅎㅎ",
          "original_form": "맞아요 여름 짧은줄 알았는데 살이 탔더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:12"
        },
        {
          "id": "MDRW2100010249.1.13",
          "form": "열심히 썬크림발랐어요ㅋㅋ",
          "original_form": "열심히 썬크림발랐어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100010249.1.14",
          "form": "저도 신경썼어야하는데ㅎㅎ",
          "original_form": "저도 신경썼어야하는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100010249.1.15",
          "form": "새살이 돋겠죠 뭐 ㅎ",
          "original_form": "새살이 돋겠죠 뭐 ㅎ",
          "speaker_id": "1",
          "time": "20210823 16:14"
        },
        {
          "id": "MDRW2100010249.1.16",
          "form": "하긴 또 겨울되면 돌아오니까요ㅎㅎ",
