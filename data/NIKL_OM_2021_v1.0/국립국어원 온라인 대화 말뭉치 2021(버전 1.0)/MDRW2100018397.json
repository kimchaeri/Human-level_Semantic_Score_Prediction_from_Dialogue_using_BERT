      "utterance": [
        {
          "id": "MDRW2100018397.1.1",
          "form": "앤티앤스언제먹어볼겨",
          "original_form": "앤티앤스언제먹어볼겨",
          "speaker_id": "1",
          "time": "20210827 01:04"
        },
        {
          "id": "MDRW2100018397.1.2",
          "form": "이번에 대학병원가면!",
          "original_form": "이번에 대학병원가면!",
          "speaker_id": "2",
          "time": "20210827 01:04"
        },
        {
          "id": "MDRW2100018397.1.3",
          "form": "아근데 1층에 있는거야?",
          "original_form": "아근데 1층에 있는거야?",
          "speaker_id": "2",
          "time": "20210827 01:05"
        },
        {
          "id": "MDRW2100018397.1.4",
          "form": "ㅇㅇ 그 구관 입구 들어서면 바로보임",
          "original_form": "ㅇㅇ 그 구관 입구 들어서면 바로보임",
          "speaker_id": "1",
          "time": "20210827 01:06"
        },
        {
          "id": "MDRW2100018397.1.5",
          "form": "아 갈때마다 지나치면서 사먹어볼 생각을 못함",
          "original_form": "아 갈때마다 지나치면서 사먹어볼 생각을 못함",
          "speaker_id": "2",
          "time": "20210827 01:06"
        },
        {
          "id": "MDRW2100018397.1.6",
          "form": "거기에서만 파는건가?",
          "original_form": "거기에서만 파는건가?",
          "speaker_id": "2",
          "time": "20210827 01:06"
        },
        {
          "id": "MDRW2100018397.1.7",
          "form": "그거 체인점이자너",
          "original_form": "그거 체인점이자너",
          "speaker_id": "1",
          "time": "20210827 01:07"
        },
        {
          "id": "MDRW2100018397.1.8",
          "form": "이쪽에서는 거기밖에 못본것같애",
          "original_form": "이쪽에서는 거기밖에 못본것같애",
          "speaker_id": "2",
          "time": "20210827 01:07"
        },
        {
          "id": "MDRW2100018397.1.9",
          "form": "거기도있음 업스퀘어",
          "original_form": "거기도있음 업스퀘어",
          "speaker_id": "1",
          "time": "20210827 01:08"
        },
        {
          "id": "MDRW2100018397.1.10",
          "form": "아! 업스퀘어 안간지 진짜오래됨",
          "original_form": "아! 업스퀘어 안간지 진짜오래됨",
          "speaker_id": "2",
          "time": "20210827 01:09"
        },
        {
          "id": "MDRW2100018397.1.11",
          "form": "거기 이상한 통에 음료수 팔았던거 기억난다",
          "original_form": "거기 이상한 통에 음료수 팔았던거 기억난다",
          "speaker_id": "2",
          "time": "20210827 01:09"
        },
        {
          "id": "MDRW2100018397.1.12",
          "form": "이상한통?... 나전혀모르겠어 ㅋㅋㅋ",
          "original_form": "이상한통?... 나전혀모르겠어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:10"
        },
        {
          "id": "MDRW2100018397.1.13",
          "form": "물고기모양해가지고 기다란 통에 음료수 팔았음ㅋㅋ",
          "original_form": "물고기모양해가지고 기다란 통에 음료수 팔았음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:11"
        },
        {
          "id": "MDRW2100018397.1.14",
          "form": "엥? 진짜??업스퀘어에서??",
          "original_form": "엥? 진짜??업스퀘어에서??",
          "speaker_id": "1",
          "time": "20210827 01:11"
        },
        {
          "id": "MDRW2100018397.1.15",
          "form": "응응ㅋㅋ",
          "original_form": "응응ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:12"
        },
        {
          "id": "MDRW2100018397.1.16",
          "form": "슬러시같은거 넣었던듯",
          "original_form": "슬러시같은거 넣었던듯",
          "speaker_id": "2",
          "time": "20210827 01:12"
        },
        {
          "id": "MDRW2100018397.1.17",
          "form": "아 층층이 색 다른거 ?",
          "original_form": "아 층층이 색 다른거 ?",
          "speaker_id": "1",
          "time": "20210827 01:13"
        },
        {
          "id": "MDRW2100018397.1.18",
          "form": "그것도 있었던듯 내가 먹은건 그냥 한가지색ㅋㅋㅋ",
          "original_form": "그것도 있었던듯 내가 먹은건 그냥 한가지색ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:13"
        }
      ]
