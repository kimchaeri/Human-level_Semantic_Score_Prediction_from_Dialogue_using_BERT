      "utterance": [
        {
          "id": "MDRW2100027055.1.1",
          "form": "여행좋아하죠",
          "original_form": "여행좋아하죠",
          "speaker_id": "2",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100027055.1.2",
          "form": "당연히",
          "original_form": "당연히",
          "speaker_id": "2",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100027055.1.3",
          "form": "기억에 남는 여행지 있나요?",
          "original_form": "기억에 남는 여행지 있나요?",
          "speaker_id": "1",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100027055.1.4",
          "form": "부산해운대요",
          "original_form": "부산해운대요",
          "speaker_id": "2",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100027055.1.5",
          "form": "그옆에 아쿠아리움도 관람 구경도하구요",
          "original_form": "그옆에 아쿠아리움도 관람 구경도하구요",
          "speaker_id": "2",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100027055.1.6",
          "form": "그리고 북한 경계지?거기도 가봤구요",
          "original_form": "그리고 북한 경계지?거기도 가봤구요",
          "speaker_id": "2",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100027055.1.7",
          "form": "오호 부산 벡스코 말씀이시군요",
          "original_form": "오호 부산 벡스코 말씀이시군요",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100027055.1.8",
          "form": "아하 그런가요?",
          "original_form": "아하 그런가요?",
          "speaker_id": "2",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100027055.1.9",
          "form": "거기 놀러가서 재미있었죠",
          "original_form": "거기 놀러가서 재미있었죠",
          "speaker_id": "2",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100027055.1.10",
          "form": "오호 벡스코는 안가봣어요",
          "original_form": "오호 벡스코는 안가봣어요",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027055.1.11",
          "form": "코로나빨리끝나서 여행가고싶어요",
          "original_form": "코로나빨리끝나서 여행가고싶어요",
          "speaker_id": "2",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027055.1.12",
          "form": "저도요ㅜㅜ 놀러 다니고 싶어요!",
          "original_form": "저도요ㅜㅜ 놀러 다니고 싶어요!",
          "speaker_id": "1",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100027055.1.13",
          "form": "때를 기다리고있어요",
          "original_form": "때를 기다리고있어요",
          "speaker_id": "2",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100027055.1.14",
          "form": "추석에 되게 걱정되넹요",
          "original_form": "추석에 되게 걱정되넹요",
          "speaker_id": "2",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100027055.1.15",
          "form": "추석이왜요",
          "original_form": "추석이왜요",
          "speaker_id": "1",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100027055.1.16",
          "form": "사람들이동많이하자나요",
          "original_form": "사람들이동많이하자나요",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100027055.1.17",
          "form": "여행하는사람있지싶은데",
          "original_form": "여행하는사람있지싶은데",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100027055.1.18",
          "form": "갈 계획있나요?",
          "original_form": "갈 계획있나요?",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100027055.1.19",
          "form": "추석때 여행 계획있으세요?",
          "original_form": "추석때 여행 계획있으세요?",
