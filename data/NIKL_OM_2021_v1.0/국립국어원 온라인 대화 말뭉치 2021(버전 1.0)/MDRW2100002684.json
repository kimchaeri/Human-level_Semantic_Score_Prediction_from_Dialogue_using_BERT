      "utterance": [
        {
          "id": "MDRW2100002684.1.1",
          "form": "그렇구만 안하는줄",
          "original_form": "그렇구만 안하는줄",
          "speaker_id": "2",
          "time": "20210820 14:51"
        },
        {
          "id": "MDRW2100002684.1.2",
          "form": "많이 바쁘지않아",
          "original_form": "많이 바쁘지않아",
          "speaker_id": "1",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002684.1.3",
          "form": "소재 200만원치 폐기하는구먼..",
          "original_form": "소재 200만원치 폐기하는구먼..",
          "speaker_id": "2",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002684.1.4",
          "form": "와이 왓 헤픈",
          "original_form": "와이 왓 헤픈",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002684.1.5",
          "form": "오염됐어?",
          "original_form": "오염됐어?",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002684.1.6",
          "form": "국산이라 가공시 변형되네",
          "original_form": "국산이라 가공시 변형되네",
          "speaker_id": "2",
          "time": "20210820 16:11"
        },
        {
          "id": "MDRW2100002684.1.7",
          "form": "알루미늄",
          "original_form": "알루미늄",
          "speaker_id": "2",
          "time": "20210820 16:11"
        },
        {
          "id": "MDRW2100002684.1.8",
          "form": "알루미늄 고대로 재판매해도 비싸지않아",
          "original_form": "알루미늄 고대로 재판매해도 비싸지않아",
          "speaker_id": "1",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002684.1.9",
          "form": "고철로넘겨야되서",
          "original_form": "고철로넘겨야되서",
          "speaker_id": "2",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002684.1.10",
          "form": "그르치 그것도 꽤 비싸지 않아",
          "original_form": "그르치 그것도 꽤 비싸지 않아",
          "speaker_id": "1",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002684.1.11",
          "form": "요새 원자재가격이 다 비싸서",
          "original_form": "요새 원자재가격이 다 비싸서",
          "speaker_id": "1",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002684.1.12",
          "form": "놉 KG당 4000원할건데",
          "original_form": "놉 KG당 4000원할건데",
          "speaker_id": "2",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002684.1.13",
          "form": "200만원치니",
          "original_form": "200만원치니",
          "speaker_id": "2",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002684.1.14",
          "form": "고철로 팔면 1/4 이하",
          "original_form": "고철로 팔면 1/4 이하",
          "speaker_id": "2",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002684.1.15",
          "form": "500kg인가",
          "original_form": "500kg인가",
          "speaker_id": "2",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002684.1.16",
          "form": "1300정도네 kg당",
          "original_form": "1300정도네 kg당",
          "speaker_id": "2",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002684.1.17",
          "form": "65만원",
          "original_form": "65만원",
          "speaker_id": "2",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002684.1.18",
          "form": "팔아서 빼돌리거 싶네",
          "original_form": "팔아서 빼돌리거 싶네",
          "speaker_id": "2",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002684.1.19",
          "form": "그런것도 다 장부에 기록하지않아?",
          "original_form": "그런것도 다 장부에 기록하지않아?",
          "speaker_id": "1",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002684.1.20",
          "form": "뭐 내가 관리하는데",
          "original_form": "뭐 내가 관리하는데",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002684.1.21",
          "form": "나 말고 몰라",
          "original_form": "나 말고 몰라",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002684.1.22",
          "form": "내가 몰래 버려도",
          "original_form": "내가 몰래 버려도",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002684.1.23",
          "form": "아 그래",
          "original_form": "아 그래",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002684.1.24",
          "form": "그래도 위험한대",
          "original_form": "그래도 위험한대",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002684.1.25",
          "form": "안그래도 다른 팀장들이 팔아서 술이나 먹자하네",
          "original_form": "안그래도 다른 팀장들이 팔아서 술이나 먹자하네",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002684.1.26",
          "form": "걍 짱박아둬야지",
          "original_form": "걍 짱박아둬야지",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002684.1.27",
          "form": "ㅋㅋㅋㅋㅋ 대박이네",
          "original_form": "ㅋㅋㅋㅋㅋ 대박이네",
          "speaker_id": "1",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002684.1.28",
          "form": "한달전으로 날 돌려보내줟",
          "original_form": "한달전으로 날 돌려보내줟",
          "speaker_id": "2",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002684.1.29",
          "form": "왜 한달전에 뭐있어?",
          "original_form": "왜 한달전에 뭐있어?",
          "speaker_id": "1",
          "time": "20210820 16:27"
        },
        {
          "id": "MDRW2100002684.1.30",
          "form": "ㅇㅇ 엑시 4천에 사게",
          "original_form": "ㅇㅇ 엑시 4천에 사게",
          "speaker_id": "2",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002684.1.31",
          "form": "6-7만에 팔고 아이오텍스 사서 먹다가 바이프로스트로 갈아타게",
          "original_form": "6-7만에 팔고 아이오텍스 사서 먹다가 바이프로스트로 갈아타게",
          "speaker_id": "2",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002684.1.32",
          "form": "450배",
          "original_form": "450배",
          "speaker_id": "2",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002684.1.33",
          "form": "나도 그러고싶다",
          "original_form": "나도 그러고싶다",
          "speaker_id": "1",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002684.1.34",
          "form": "제길슨",
          "original_form": "제길슨",
          "speaker_id": "1",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002684.1.35",
          "form": "보내줘~~~~",
          "original_form": "보내줘~~~~",
          "speaker_id": "2",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002684.1.36",
          "form": "머릿속에 펌핑 순서 다 들어있다",
          "original_form": "머릿속에 펌핑 순서 다 들어있다",
          "speaker_id": "2",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002684.1.37",
          "form": "2~3월로가면 100만원이 조단위 된다",
          "original_form": "2~3월로가면 100만원이 조단위 된다",
          "speaker_id": "2",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002684.1.38",
          "form": "나도 돌아가고싶다",
          "original_form": "나도 돌아가고싶다",
          "speaker_id": "1",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002684.1.39",
          "form": "제길슨",
          "original_form": "제길슨",
          "speaker_id": "1",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002684.1.40",
          "form": "간절히 바라면",
          "original_form": "간절히 바라면",
          "speaker_id": "2",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002684.1.41",
          "form": "안이루어진다",
          "original_form": "안이루어진다",
          "speaker_id": "2",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002684.1.42",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002684.1.43",
          "form": "그때로 돌아가도 니가 사면 떨어진다",
          "original_form": "그때로 돌아가도 니가 사면 떨어진다",
          "speaker_id": "1",
          "time": "20210820 16:35"
        }
      ]
    }
