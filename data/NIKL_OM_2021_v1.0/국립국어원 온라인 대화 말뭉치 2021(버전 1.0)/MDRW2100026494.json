      "utterance": [
        {
          "id": "MDRW2100026494.1.1",
          "form": "종교는 뭐니",
          "original_form": "종교는 뭐니",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026494.1.2",
          "form": "불교죠",
          "original_form": "불교죠",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026494.1.3",
          "form": "전 정토회여",
          "original_form": "전 정토회여",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026494.1.4",
          "form": "법륜스님 아시나여",
          "original_form": "법륜스님 아시나여",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026494.1.5",
          "form": "저두알죠 유명하시더라구오",
          "original_form": "저두알죠 유명하시더라구오",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026494.1.6",
          "form": "코시국에 종교단체가.모여서 행사하는건 어떻게 생각하세여",
          "original_form": "코시국에 종교단체가.모여서 행사하는건 어떻게 생각하세여",
          "speaker_id": "1",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026494.1.7",
          "form": "길가다 봤는데 중년아저씨가 친구랑 통화하시는데 법륜스님과 빙의되서 친구 잔소리하고 계시더래요",
          "original_form": "길가다 봤는데 중년아저씨가 친구랑 통화하시는데 법륜스님과 빙의되서 친구 잔소리하고 계시더래요",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026494.1.8",
          "form": "비대면해야쥬",
          "original_form": "비대면해야쥬",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026494.1.9",
          "form": "빙의.너무 무서운데여",
          "original_form": "빙의.너무 무서운데여",
          "speaker_id": "1",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026494.1.10",
          "form": "기독교 광신도님들은 너무 강성이라..",
          "original_form": "기독교 광신도님들은 너무 강성이라..",
          "speaker_id": "1",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100026494.1.11",
          "form": "그들도 그분들의 사정이 있겄쥬 분위기같은거라던가",
          "original_form": "그들도 그분들의 사정이 있겄쥬 분위기같은거라던가",
          "speaker_id": "2",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026494.1.12",
          "form": "마음이 좁아서 ㅎㅎ",
          "original_form": "마음이 좁아서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026494.1.13",
          "form": "ㅋㅋ 비대면하기위해 백신을철저히 맞으셨으려나ㅜ",
          "original_form": "ㅋㅋ 비대면하기위해 백신을철저히 맞으셨으려나ㅜ",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026494.1.14",
          "form": "백신.다 맞으면 사람들이 모일까",
          "original_form": "백신.다 맞으면 사람들이 모일까",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026494.1.15",
          "form": "덴마크였나 거기는 접종율80%채운뒤",
          "original_form": "덴마크였나 거기는 접종율80%채운뒤",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026494.1.16",
          "form": "마스크안낌",
          "original_form": "마스크안낌",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026494.1.17",
          "form": "역시 비대면 종교활동을..",
          "original_form": "역시 비대면 종교활동을..",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026494.1.18",
          "form": "아 이번 추석때",
          "original_form": "아 이번 추석때",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026494.1.19",
          "form": "ㅇ",
          "original_form": "ㅇ",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026494.1.20",
          "form": "가심?",
          "original_form": "가심?",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026494.1.21",
          "form": "한가람법우랑 추석명상 울집에서 하기로 함",
          "original_form": "한가람법우랑 추석명상 울집에서 하기로 함",
