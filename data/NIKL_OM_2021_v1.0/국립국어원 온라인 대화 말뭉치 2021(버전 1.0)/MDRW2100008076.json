      "utterance": [
        {
          "id": "MDRW2100008076.1.1",
          "form": "이게 맨날 그밥에 그반찬",
          "original_form": "이게 맨날 그밥에 그반찬",
          "speaker_id": "2",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100008076.1.2",
          "form": "은",
          "original_form": "은",
          "speaker_id": "2",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008076.1.3",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008076.1.4",
          "form": "메뉴 골라주는 앱 있으면 좋겠다",
          "original_form": "메뉴 골라주는 앱 있으면 좋겠다",
          "speaker_id": "1",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008076.1.5",
          "form": "ㅋㅋㅋ여러개 중에 고르게ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ여러개 중에 고르게ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008076.1.6",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008076.1.7",
          "form": "약간 그런거 있자나 점심에 뭐 먹고 나면 저녁에 왠지 뭐가 안땡기구 뭐는 땡기고",
          "original_form": "약간 그런거 있자나 점심에 뭐 먹고 나면 저녁에 왠지 뭐가 안땡기구 뭐는 땡기고",
          "speaker_id": "1",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008076.1.8",
          "form": "있을거야 아마도 요새 없는게 앖어",
          "original_form": "있을거야 아마도 요새 없는게 앖어",
          "speaker_id": "2",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008076.1.9",
          "form": "개인맞춤으로다가ㅋㅋㅋㅋㅋ",
          "original_form": "개인맞춤으로다가ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008076.1.10",
          "form": "자꾸 배달 시켜서ㅠ큰일이야",
          "original_form": "자꾸 배달 시켜서ㅠ큰일이야",
          "speaker_id": "2",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008076.1.11",
          "form": "오 고거 좋다",
          "original_form": "오 고거 좋다",
          "speaker_id": "2",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100008076.1.12",
          "form": "그런가ㅋㅋㅋㅋ차라리 운동하면 트레이너가 막 정해도 주자나ㅋㅋㅋㅋ",
          "original_form": "그런가ㅋㅋㅋㅋ차라리 운동하면 트레이너가 막 정해도 주자나ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100008076.1.13",
          "form": "선호도 반영하고 해서",
          "original_form": "선호도 반영하고 해서",
          "speaker_id": "2",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100008076.1.14",
          "form": "뭐만 먹어라이러고ㅋㅋㅋ",
          "original_form": "뭐만 먹어라이러고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100008076.1.15",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100008076.1.16",
          "form": "아 싫다",
          "original_form": "아 싫다",
          "speaker_id": "2",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100008076.1.17",
          "form": "배가고프겠지만... 골라주는거 편할듯",
          "original_form": "배가고프겠지만... 골라주는거 편할듯",
          "speaker_id": "1",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100008076.1.18",
          "form": "나  넘 안먹어 탈모 오던데",
          "original_form": "나  넘 안먹어 탈모 오던데",
          "speaker_id": "2",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100008076.1.19",
          "form": "편해 엄청",
          "original_form": "편해 엄청",
          "speaker_id": "2",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100008076.1.20",
          "form": "ㅋㅋㅋㅋ응?",
          "original_form": "ㅋㅋㅋㅋ응?",
          "speaker_id": "1",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008076.1.21",
          "form": "정해주면",
          "original_form": "정해주면",
          "speaker_id": "2",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008076.1.22",
          "form": "다욧할때",
          "original_form": "다욧할때",
          "speaker_id": "2",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008076.1.23",
          "form": "안먹으니까 편하다고? ㅋㅋㅋㅋㅋㅋ탈모가 오는데?",
          "original_form": "안먹으니까 편하다고? ㅋㅋㅋㅋㅋㅋ탈모가 오는데?",
          "speaker_id": "1",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008076.1.24",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008076.1.25",
          "form": "다욧할때 넘 덜먹고 편식되서",
          "original_form": "다욧할때 넘 덜먹고 편식되서",
          "speaker_id": "2",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008076.1.26",
          "form": "탈모가 ㅜㅜ",
          "original_form": "탈모가 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008076.1.27",
          "form": "딸이 중국인형 머리같대",
          "original_form": "딸이 중국인형 머리같대",
          "speaker_id": "2",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008076.1.28",
          "form": "난 잘 안먹어 본적 거의 넚어서 탈모도 안와...",
          "original_form": "난 잘 안먹어 본적 거의 넚어서 탈모도 안와...",
          "speaker_id": "1",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008076.1.29",
          "form": "알지 그 인형",
          "original_form": "알지 그 인형",
          "speaker_id": "2",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008076.1.30",
          "form": "얼마전 얘기 지금 막먹음",
          "original_form": "얼마전 얘기 지금 막먹음",
          "speaker_id": "2",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008076.1.31",
          "form": "으잉 다시 먹어도 안 늘어나?",
          "original_form": "으잉 다시 먹어도 안 늘어나?",
          "speaker_id": "1",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008076.1.32",
          "form": "RGRG",
          "original_form": "RGRG",
          "speaker_id": "1",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008076.1.33",
          "form": "아늘어나",
          "original_form": "아늘어나",
          "speaker_id": "2",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008076.1.34",
          "form": "안",
          "original_form": "안",
          "speaker_id": "2",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008076.1.35",
          "form": "이게 없어지면 끝이지",
          "original_form": "이게 없어지면 끝이지",
          "speaker_id": "2",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008076.1.36",
          "form": "검정콩 먹어",
          "original_form": "검정콩 먹어",
          "speaker_id": "2",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008076.1.37",
          "form": "ㅜㅜ두유도 검정콩",
          "original_form": "ㅜㅜ두유도 검정콩",
          "speaker_id": "2",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008076.1.38",
          "form": "ㅋㅋㅋㅋㅋ노력하면 다시 올라올것이다",
          "original_form": "ㅋㅋㅋㅋㅋ노력하면 다시 올라올것이다",
          "speaker_id": "1",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008076.1.39",
          "form": "또 굳이 다욧 아니더라도 애 낳으면 그런 경우도 많다며",
          "original_form": "또 굳이 다욧 아니더라도 애 낳으면 그런 경우도 많다며",
          "speaker_id": "1",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008076.1.40",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008076.1.41",
          "form": "털갈이된다",
          "original_form": "털갈이된다",
          "speaker_id": "2",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008076.1.42",
          "form": "강아지처럼",
          "original_form": "강아지처럼",
          "speaker_id": "2",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008076.1.43",
          "form": "잘먹어야해",
          "original_form": "잘먹어야해",
          "speaker_id": "2",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008076.1.44",
          "form": "탈모가 무서워",
          "original_form": "탈모가 무서워",
          "speaker_id": "2",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008076.1.45",
          "form": "검정콩 두유가 더비싸 ㅋㅋ",
          "original_form": "검정콩 두유가 더비싸 ㅋㅋ",
