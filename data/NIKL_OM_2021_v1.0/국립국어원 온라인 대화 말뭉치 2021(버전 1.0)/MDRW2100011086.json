        {
          "id": "MDRW2100011086.1.1",
          "form": "반려동물 키우시나요",
          "original_form": "반려동물 키우시나요",
          "speaker_id": "2",
          "time": "20210824 00:49"
        },
        {
          "id": "MDRW2100011086.1.2",
          "form": "혼자라ㅠㅠ 못키워요.. \r\n뭐 키우는거 있어요ㅡ",
          "original_form": "혼자라ㅠㅠ 못키워요.. \r\n뭐 키우는거 있어요ㅡ",
          "speaker_id": "1",
          "time": "20210824 00:50"
        },
        {
          "id": "MDRW2100011086.1.3",
          "form": "아뇨 저두 ㅎㅎ",
          "original_form": "아뇨 저두 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 00:50"
        },
        {
          "id": "MDRW2100011086.1.4",
          "form": "손 많이 갈거 같아요",
          "original_form": "손 많이 갈거 같아요",
          "speaker_id": "2",
          "time": "20210824 00:50"
        },
        {
          "id": "MDRW2100011086.1.5",
          "form": "키워보고 싶은거 있나여?",
          "original_form": "키워보고 싶은거 있나여?",
          "speaker_id": "1",
          "time": "20210824 00:52"
        },
        {
          "id": "MDRW2100011086.1.6",
          "form": "음 워너비는 원숭이인데ㅋㅋㅋ",
          "original_form": "음 워너비는 원숭이인데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 00:54"
        },
        {
          "id": "MDRW2100011086.1.7",
          "form": "헉..",
          "original_form": "헉..",
          "speaker_id": "1",
          "time": "20210824 00:54"
        },
        {
          "id": "MDRW2100011086.1.8",
          "form": "돈 많이 버셔야... ㅎㅎㅎ",
          "original_form": "돈 많이 버셔야... ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 00:55"
        },
        {
          "id": "MDRW2100011086.1.9",
          "form": "키울수는 있지 않나요?",
          "original_form": "키울수는 있지 않나요?",
          "speaker_id": "1",
          "time": "20210824 00:55"
        },
        {
          "id": "MDRW2100011086.1.10",
          "form": "비쌀거 같아요ㅋㅋㅋ",
          "original_form": "비쌀거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 00:55"
        },
        {
          "id": "MDRW2100011086.1.11",
          "form": "돈이.....",
          "original_form": "돈이.....",
          "speaker_id": "2",
          "time": "20210824 00:55"
        },
        {
          "id": "MDRW2100011086.1.12",
          "form": "원숭이 어떤 종류요? 고릴라같은 대형?",
          "original_form": "원숭이 어떤 종류요? 고릴라같은 대형?",
          "speaker_id": "1",
          "time": "20210824 00:57"
        },
        {
          "id": "MDRW2100011086.1.13",
          "form": "아뇨 조그만 친구요ㅋㅋㅋㅋ",
          "original_form": "아뇨 조그만 친구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 00:58"
        },
        {
          "id": "MDRW2100011086.1.14",
          "form": "심부름 시키려구요",
          "original_form": "심부름 시키려구요",
          "speaker_id": "2",
          "time": "20210824 00:58"
        },
        {
          "id": "MDRW2100011086.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ대박 웃기다..",
