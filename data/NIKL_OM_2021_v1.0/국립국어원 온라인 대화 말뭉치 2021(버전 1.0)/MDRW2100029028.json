        },
        {
          "id": "MDRW2100029028.1.2",
          "form": "ㅋㅋㅋ싫어하세요?",
          "original_form": "ㅋㅋㅋ싫어하세요?",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029028.1.3",
          "form": "좋아하시나여?",
          "original_form": "좋아하시나여?",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029028.1.4",
          "form": "좋아하진 않아요",
          "original_form": "좋아하진 않아요",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029028.1.5",
          "form": "청소 빨래 도대체왜..",
          "original_form": "청소 빨래 도대체왜..",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029028.1.6",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029028.1.7",
          "form": "해야죠",
          "original_form": "해야죠",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029028.1.8",
          "form": "인간답게살려고하긴하는데",
          "original_form": "인간답게살려고하긴하는데",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029028.1.9",
          "form": "하기싫죠",
          "original_form": "하기싫죠",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029028.1.10",
          "form": "로봇이 필요합니다",
          "original_form": "로봇이 필요합니다",
          "speaker_id": "1",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029028.1.11",
          "form": "그래도 요리는 재밌지않낭노?",
          "original_form": "그래도 요리는 재밌지않낭노?",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029028.1.12",
          "form": "아뇨?",
          "original_form": "아뇨?",
          "speaker_id": "1",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029028.1.13",
          "form": "제일 싫은데요?",
          "original_form": "제일 싫은데요?",
          "speaker_id": "1",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029028.1.14",
          "form": "전 요린 좋아해요",
          "original_form": "전 요린 좋아해요",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029028.1.15",
          "form": "누가 밥해주고 설거지하는 건 좋아요",
          "original_form": "누가 밥해주고 설거지하는 건 좋아요",
          "speaker_id": "1",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029028.1.16",
          "form": "전 치우는 게싫은데",
          "original_form": "전 치우는 게싫은데",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029028.1.17",
          "form": "저랑 천생연분이시네요ㅋㅋ",
          "original_form": "저랑 천생연분이시네요ㅋㅋ",
