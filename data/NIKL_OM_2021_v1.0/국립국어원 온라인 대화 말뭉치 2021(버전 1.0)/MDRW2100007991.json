      "utterance": [
        {
          "id": "MDRW2100007991.1.1",
          "form": "요즘 배달음식 자주 시키나요",
          "original_form": "요즘 배달음식 자주 시키나요",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100007991.1.2",
          "form": "아니요 잘 안시켜요",
          "original_form": "아니요 잘 안시켜요",
          "speaker_id": "1",
          "time": "20210822 13:51"
        },
        {
          "id": "MDRW2100007991.1.3",
          "form": "아이들이 치킨이나 족발먹고싶다하면 시키는데",
          "original_form": "아이들이 치킨이나 족발먹고싶다하면 시키는데",
          "speaker_id": "1",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100007991.1.4",
          "form": "자주 있는일이 아니라서 ㅎ",
          "original_form": "자주 있는일이 아니라서 ㅎ",
          "speaker_id": "1",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100007991.1.5",
          "form": "저흰 코로나로 외식보단 배달을 주로 이용을 해요",
          "original_form": "저흰 코로나로 외식보단 배달을 주로 이용을 해요",
          "speaker_id": "2",
          "time": "20210822 13:53"
        },
        {
          "id": "MDRW2100007991.1.6",
          "form": "아무래도 코로나땜에 외식하기가 좀 그렇긴하죠",
          "original_form": "아무래도 코로나땜에 외식하기가 좀 그렇긴하죠",
          "speaker_id": "1",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100007991.1.7",
          "form": "저는 덕분에 집밥해먹으려고 많이 했어요",
          "original_form": "저는 덕분에 집밥해먹으려고 많이 했어요",
          "speaker_id": "1",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100007991.1.8",
          "form": "또 주변에 밭일구시는분들이 많아서",
          "original_form": "또 주변에 밭일구시는분들이 많아서",
          "speaker_id": "1",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100007991.1.9",
          "form": "주시는 가지 호박 먹느라고 ㅎㅎ",
          "original_form": "주시는 가지 호박 먹느라고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100007991.1.10",
          "form": "가지로 냉국 해먹으면 시원하고맛있겠어요",
          "original_form": "가지로 냉국 해먹으면 시원하고맛있겠어요",
          "speaker_id": "2",
          "time": "20210822 14:01"
        },
        {
          "id": "MDRW2100007991.1.11",
          "form": "가지로 냉국도 되요?",
          "original_form": "가지로 냉국도 되요?",
          "speaker_id": "1",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100007991.1.12",
          "form": "오이로만 한다고 생각했어요",
          "original_form": "오이로만 한다고 생각했어요",
          "speaker_id": "1",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100007991.1.13",
          "form": "가지요리가 다양하군요",
          "original_form": "가지요리가 다양하군요",
          "speaker_id": "1",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100007991.1.14",
          "form": "저희 가족은 가지 삶아서 냉국만든것 좋아해요",
          "original_form": "저희 가족은 가지 삶아서 냉국만든것 좋아해요",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100007991.1.15",
          "form": "찾아봐야겠어요 어떻게하는지...궁금해지네요",
          "original_form": "찾아봐야겠어요 어떻게하는지...궁금해지네요",
          "speaker_id": "1",
          "time": "20210822 14:20"
        },
        {
          "id": "MDRW2100007991.1.16",
          "form": "점심은 머드셨어요?",
          "original_form": "점심은 머드셨어요?",
          "speaker_id": "1",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100007991.1.17",
          "form": "애들과 김치볶음밥 해서 먹었어요",
          "original_form": "애들과 김치볶음밥 해서 먹었어요",
          "speaker_id": "2",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100007991.1.18",
          "form": "와 김치볶음밥 맛있겠어요",
          "original_form": "와 김치볶음밥 맛있겠어요",
          "speaker_id": "1",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100007991.1.19",
          "form": "집에 김치가 다떨어져서 못먹는 메뉴중 하나예요 ㅎ",
          "original_form": "집에 김치가 다떨어져서 못먹는 메뉴중 하나예요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100007991.1.20",
          "form": "전 시댁에서 같이 김장을 해요",
          "original_form": "전 시댁에서 같이 김장을 해요",
          "speaker_id": "2",
          "time": "20210822 14:40"
        },
        {
          "id": "MDRW2100007991.1.21",
          "form": "많이하세요?",
          "original_form": "많이하세요?",
          "speaker_id": "1",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100007991.1.22",
          "form": "저희는 김치를 잘안먹어서...간혹가다 얻어와요",
          "original_form": "저희는 김치를 잘안먹어서...간혹가다 얻어와요",
          "speaker_id": "1",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100007991.1.23",
          "form": "네 형제들 다같이 모여서 해서 좀 많아요",
          "original_form": "네 형제들 다같이 모여서 해서 좀 많아요",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100007991.1.24",
          "form": "많이하시겠네요",
          "original_form": "많이하시겠네요",
          "speaker_id": "1",
          "time": "20210822 15:21"
        }
      ]
    }
  ]
