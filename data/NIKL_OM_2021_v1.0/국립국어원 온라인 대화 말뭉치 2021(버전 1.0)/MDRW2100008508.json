      "utterance": [
        {
          "id": "MDRW2100008508.1.1",
          "form": "오 여행이군요",
          "original_form": "오 여행이군요",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008508.1.2",
          "form": "좋아하시나요? ㅎㅎ",
          "original_form": "좋아하시나요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008508.1.3",
          "form": "오 저는 집순이라 자주는 안다니지만",
          "original_form": "오 저는 집순이라 자주는 안다니지만",
          "speaker_id": "2",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008508.1.4",
          "form": "요즘 시기가 시기인지라",
          "original_form": "요즘 시기가 시기인지라",
          "speaker_id": "2",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008508.1.5",
          "form": "그건 그래요 ㅠㅠ 코로나 ㅠㅠ",
          "original_form": "그건 그래요 ㅠㅠ 코로나 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008508.1.6",
          "form": "완전 끊긴 느낌이에욯ㅎㅎ",
          "original_form": "완전 끊긴 느낌이에욯ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008508.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008508.1.8",
          "form": "저는 완전 막 자주 다닌 건 아니지만 좋아해요",
          "original_form": "저는 완전 막 자주 다닌 건 아니지만 좋아해요",
          "speaker_id": "1",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100008508.1.9",
          "form": "여행 가기 전의 그 설렘이 너무 좋아요 ㅋㅋㅋ",
          "original_form": "여행 가기 전의 그 설렘이 너무 좋아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100008508.1.10",
          "form": "여행의 설렘 저도 이해가 가네요",
          "original_form": "여행의 설렘 저도 이해가 가네요",
          "speaker_id": "2",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100008508.1.11",
          "form": "멀리는 못나가봤지만",
          "original_form": "멀리는 못나가봤지만",
          "speaker_id": "2",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008508.1.12",
          "form": "계획 세우는 건 잘 못해요 ㅋㅋㅋ",
          "original_form": "계획 세우는 건 잘 못해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008508.1.13",
          "form": "좋은분들과 다니는게 참 좋아요",
          "original_form": "좋은분들과 다니는게 참 좋아요",
          "speaker_id": "2",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008508.1.14",
          "form": "제친구는 다니는거 넘좋아하는데 해외도 못나가서",
          "original_form": "제친구는 다니는거 넘좋아하는데 해외도 못나가서",
          "speaker_id": "2",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008508.1.15",
          "form": "이번에 아예 제주도로 이사갔어요",
          "original_form": "이번에 아예 제주도로 이사갔어요",
          "speaker_id": "2",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008508.1.16",
          "form": "극단적ㅋㅋ",
          "original_form": "극단적ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008508.1.17",
          "form": "와 저도 해본 생각인데 ㅋㅋㅋ 물론 실행은 못할 거 같지만요",
          "original_form": "와 저도 해본 생각인데 ㅋㅋㅋ 물론 실행은 못할 거 같지만요",
          "speaker_id": "1",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008508.1.18",
          "form": "제주도 같은 휴양지에서 사는 건 또 다른 세계일거 같아요 ㅎㅎ",
          "original_form": "제주도 같은 휴양지에서 사는 건 또 다른 세계일거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008508.1.19",
          "form": "저도 따라가보고싶었는데 매여가지고..ㅜㅜ",
          "original_form": "저도 따라가보고싶었는데 매여가지고..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008508.1.20",
          "form": "그래도 제주도에 코로나 잠잠해지면",
          "original_form": "그래도 제주도에 코로나 잠잠해지면",
          "speaker_id": "2",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008508.1.21",
          "form": "신세질 집이 생겨서 좋네요",
          "original_form": "신세질 집이 생겨서 좋네요",
          "speaker_id": "2",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008508.1.22",
          "form": "근데 매일 비온대요 지금ㅋㅋ",
          "original_form": "근데 매일 비온대요 지금ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008508.1.23",
          "form": "그건 너무 부럽네요.. 제주에 맛집 카페들도 많고..",
          "original_form": "그건 너무 부럽네요.. 제주에 맛집 카페들도 많고..",
          "speaker_id": "1",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008508.1.24",
          "form": "앗 저런 ㅠㅠㅠ",
          "original_form": "앗 저런 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008508.1.25",
          "form": "내려간지 일주일 안됐거든요 ㅋㅋ",
          "original_form": "내려간지 일주일 안됐거든요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008508.1.26",
          "form": "네 맛집 카페 넘 많더라구요",
          "original_form": "네 맛집 카페 넘 많더라구요",
          "speaker_id": "2",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008508.1.27",
          "form": "제주공항에 마음샌드먹어봤는데",
          "original_form": "제주공항에 마음샌드먹어봤는데",
          "speaker_id": "2",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008508.1.28",
          "form": "맛있었어요",
          "original_form": "맛있었어요",
          "speaker_id": "2",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008508.1.29",
          "form": "마음샌드.. 샌드위치인가요?",
          "original_form": "마음샌드.. 샌드위치인가요?",
          "speaker_id": "1",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008508.1.30",
          "form": "아..제주공항 빠바에만 파는",
          "original_form": "아..제주공항 빠바에만 파는",
          "speaker_id": "2",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008508.1.31",
          "form": "한정판 메뉴에요",
          "original_form": "한정판 메뉴에요",
          "speaker_id": "2",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008508.1.32",
          "form": "쿠키인데 안에 버터크림이 있어요",
          "original_form": "쿠키인데 안에 버터크림이 있어요",
          "speaker_id": "2",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008508.1.33",
          "form": "아!!! 맛있겠네요",
          "original_form": "아!!! 맛있겠네요",
          "speaker_id": "1",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008508.1.34",
          "form": "스벅도 그런 메뉴 팔던데",
          "original_form": "스벅도 그런 메뉴 팔던데",
          "speaker_id": "1",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100008508.1.35",
          "form": "스벅에도 파나요?",
          "original_form": "스벅에도 파나요?",
          "speaker_id": "2",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100008508.1.36",
          "form": "마지막 여행지가 어딘가요?",
          "original_form": "마지막 여행지가 어딘가요?",
          "speaker_id": "1",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100008508.1.37",
          "form": "찾아봐야겠네요",
          "original_form": "찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100008508.1.38",
          "form": "음..가물가물 ..",
          "original_form": "음..가물가물 ..",
          "speaker_id": "2",
          "time": "20210822 16:26"
        },
        {
          "id": "MDRW2100008508.1.39",
          "form": "아 스벅 한정메뉴요 ㅋㅋ 무슨 호지차 어쩌구 ㅎㅎ",
          "original_form": "아 스벅 한정메뉴요 ㅋㅋ 무슨 호지차 어쩌구 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 16:26"
        },
        {
          "id": "MDRW2100008508.1.40",
          "form": "출장은갔는데 여행이라...",
          "original_form": "출장은갔는데 여행이라...",
          "speaker_id": "2",
          "time": "20210822 16:26"
        },
        {
          "id": "MDRW2100008508.1.41",
          "form": "넘 오래됐네요ㅋㅋ 요샌 가도 눈치보여서 갔다고 티도안내요",
          "original_form": "넘 오래됐네요ㅋㅋ 요샌 가도 눈치보여서 갔다고 티도안내요",
          "speaker_id": "2",
          "time": "20210822 16:26"
        },
        {
          "id": "MDRW2100008508.1.42",
          "form": "그건 그래요 또 ㅋㅋㅋ",
          "original_form": "그건 그래요 또 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008508.1.43",
          "form": "저도 코로나 전인 거 같네요 ㅠㅠ",
          "original_form": "저도 코로나 전인 거 같네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008508.1.44",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008508.1.45",
          "form": "베트남 다낭이 마지막 여행지요!",
          "original_form": "베트남 다낭이 마지막 여행지요!",
          "speaker_id": "1",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008508.1.46",
          "form": "넘..까마득하네요",
          "original_form": "넘..까마득하네요",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008508.1.47",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008508.1.48",
          "form": "해외 베트남",
          "original_form": "해외 베트남",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008508.1.49",
          "form": "좋았겠네요",
          "original_form": "좋았겠네요",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008508.1.50",
          "form": "맛있는게 많아서 좋아요",
          "original_form": "맛있는게 많아서 좋아요",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008508.1.51",
          "form": "전 안가봤어욧 부럽네요",
          "original_form": "전 안가봤어욧 부럽네요",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008508.1.52",
          "form": "근데 한국사람이 넘 많아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 한국사람이 넘 많아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008508.1.53",
          "form": "오 음식또 싼가요?",
          "original_form": "오 음식또 싼가요?",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008508.1.54",
          "form": "네 저렴해요!",
          "original_form": "네 저렴해요!",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008508.1.55",
          "form": "아놔 ㅋㅋ 한국인들이 여기저기 많네",
          "original_form": "아놔 ㅋㅋ 한국인들이 여기저기 많네",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008508.1.56",
          "form": "저도 가보고싶네요",
          "original_form": "저도 가보고싶네요",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008508.1.57",
          "form": "다낭이 진짜 가본 데중에 젤 많았어요 오사카 이런 데보다 많아요..",
          "original_form": "다낭이 진짜 가본 데중에 젤 많았어요 오사카 이런 데보다 많아요..",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008508.1.58",
          "form": "헐 한국인들이 좋아할만한데인가보네요 친구들도 가던데",
          "original_form": "헐 한국인들이 좋아할만한데인가보네요 친구들도 가던데",
          "speaker_id": "2",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008508.1.59",
          "form": "가족여행으로",
          "original_form": "가족여행으로",
          "speaker_id": "2",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008508.1.60",
          "form": "그러게요 뭔가 한국인맞춤인가봐요 ㅋㅋㅋ",
          "original_form": "그러게요 뭔가 한국인맞춤인가봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:29"
        },
        {
