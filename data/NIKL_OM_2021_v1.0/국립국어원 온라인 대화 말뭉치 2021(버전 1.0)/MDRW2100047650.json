      "utterance": [
        {
          "id": "MDRW2100047650.1.1",
          "form": "이젠 버리거나 거저 주거나 해야할듯해요",
          "original_form": "이젠 버리거나 거저 주거나 해야할듯해요",
          "speaker_id": "2",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100047650.1.2",
          "form": "가망이 없어요",
          "original_form": "가망이 없어요",
          "speaker_id": "2",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100047650.1.3",
          "form": "티폿이 안팔리더라구요",
          "original_form": "티폿이 안팔리더라구요",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100047650.1.4",
          "form": "누가 물어보지도 않아요",
          "original_form": "누가 물어보지도 않아요",
          "speaker_id": "2",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100047650.1.5",
          "form": "어려운 중고거래ㅠㅠ",
          "original_form": "어려운 중고거래ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100047650.1.6",
          "form": "사려고",
          "original_form": "사려고",
          "speaker_id": "2",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100047650.1.7",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100047650.1.8",
          "form": "예쁜 주전자인데ㅠㅠ",
          "original_form": "예쁜 주전자인데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100047650.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100047650.1.10",
          "form": "그게 살때 좋은데 안팔리죠",
          "original_form": "그게 살때 좋은데 안팔리죠",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100047650.1.11",
          "form": "저도 버렸어요 ㅜㅜ",
          "original_form": "저도 버렸어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100047650.1.12",
          "form": "안팔려서",
          "original_form": "안팔려서",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100047650.1.13",
          "form": "맞아요 사려는 사람이 없어요 ㅠㅠ",
          "original_form": "맞아요 사려는 사람이 없어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100047650.1.14",
          "form": "저도 정 안되면 정말 가격을 많이 깎던가 그냥 버려야겟어요 ㅠㅠ",
          "original_form": "저도 정 안되면 정말 가격을 많이 깎던가 그냥 버려야겟어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100047650.1.15",
          "form": "그러세요 ㅜㅜ",
          "original_form": "그러세요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100047650.1.16",
          "form": "기녕 정리해야햐요",
          "original_form": "기녕 정리해야햐요",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100047650.1.17",
          "form": "기냥",
          "original_form": "기냥",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100047650.1.18",
          "form": "낼 보내줄래요",
          "original_form": "낼 보내줄래요",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100047650.1.19",
          "form": "제 물건도 ㅠㅜ",
          "original_form": "제 물건도 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100047650.1.20",
          "form": "무슨 물건인가요?",
          "original_form": "무슨 물건인가요?",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100047650.1.21",
          "form": "자동차 핫휠요 장난감",
          "original_form": "자동차 핫휠요 장난감",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100047650.1.22",
          "form": "장난감 보통 잘 팔리는 것 같던데 ㅠㅠ",
          "original_form": "장난감 보통 잘 팔리는 것 같던데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100047650.1.23",
          "form": "다들 장난감이 너무 많은 걸까요 ㅋㅋ큐ㅠㅠㅠ",
          "original_form": "다들 장난감이 너무 많은 걸까요 ㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100047650.1.24",
          "form": "제꺼가 구리나봐요 ㅋㅋ",
          "original_form": "제꺼가 구리나봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100047650.1.25",
          "form": "그런데 진짜 예측할수가 없더라구요",
          "original_form": "그런데 진짜 예측할수가 없더라구요",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100047650.1.26",
          "form": "이건 진짜 금방 팔릴거야 싶은건 안팔리고",
          "original_form": "이건 진짜 금방 팔릴거야 싶은건 안팔리고",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100047650.1.27",
          "form": "이런걸 누가 살까 싶은건 의외로 팔리고 ㅋㅋㅋㅋㅋ",
          "original_form": "이런걸 누가 살까 싶은건 의외로 팔리고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100047650.1.28",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100047650.1.29",
          "form": "진짜 알수가 없어요",
          "original_form": "진짜 알수가 없어요",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100047650.1.30",
          "form": "얼마전 모르고 판게 헉 어쩐지 막 난리더니",
          "original_form": "얼마전 모르고 판게 헉 어쩐지 막 난리더니",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100047650.1.31",
          "form": "대박 희귀하데요 ㅜㅜ",
          "original_form": "대박 희귀하데요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100047650.1.32",
          "form": "아이고 그건 아깝네요 ㅠㅠ 희귀한거라니",
          "original_form": "아이고 그건 아깝네요 ㅠㅠ 희귀한거라니",
          "speaker_id": "1",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100047650.1.33",
          "form": "네 제가 미쳤어요 ㅡㅡ",
          "original_form": "네 제가 미쳤어요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100047650.1.34",
          "form": "모르고 팔고",
          "original_form": "모르고 팔고",
          "speaker_id": "2",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100047650.1.35",
          "form": "막 계속 비싸게 사겠다하고",
          "original_form": "막 계속 비싸게 사겠다하고",
          "speaker_id": "2",
          "time": "20210826 23:55"
        }
      ]
    }
