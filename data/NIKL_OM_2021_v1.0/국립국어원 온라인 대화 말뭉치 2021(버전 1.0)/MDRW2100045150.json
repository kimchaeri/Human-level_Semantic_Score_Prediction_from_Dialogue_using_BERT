      "utterance": [
        {
          "id": "MDRW2100045150.1.1",
          "form": "ㅇㅇㅇ...",
          "original_form": "ㅇㅇㅇ...",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045150.1.2",
          "form": "그걸 재밌다고 웃는거",
          "original_form": "그걸 재밌다고 웃는거",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045150.1.3",
          "form": "기괴한데",
          "original_form": "기괴한데",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045150.1.4",
          "form": "우리나라에서는 했다간 당장에 프로그램이 폐지될 급의",
          "original_form": "우리나라에서는 했다간 당장에 프로그램이 폐지될 급의",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045150.1.5",
          "form": "근데 그런식으로",
          "original_form": "근데 그런식으로",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045150.1.6",
          "form": "샤워중인데 천장에서 찍으면서 나타난다거나",
          "original_form": "샤워중인데 천장에서 찍으면서 나타난다거나",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045150.1.7",
          "form": "?\r\n어나",
          "original_form": "?\r\n어나",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045150.1.8",
          "form": "걔네는",
          "original_form": "걔네는",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045150.1.9",
          "form": "속에입는 옷을",
          "original_form": "속에입는 옷을",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045150.1.10",
          "form": "도덕성이란게 없나",
          "original_form": "도덕성이란게 없나",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045150.1.11",
          "form": "속",
          "original_form": "속",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045150.1.12",
          "form": "옷이라하ㅣ그럼",
          "original_form": "옷이라하ㅣ그럼",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045150.1.13",
          "form": "ㅁ이게나쁜말이냐",
          "original_form": "ㅁ이게나쁜말이냐",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045150.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ....",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045150.1.15",
          "form": "암튼 거기다",
          "original_form": "암튼 거기다",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045150.1.16",
          "form": "멘톨발라서",
          "original_form": "멘톨발라서",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045150.1.17",
          "form": "아 ㅇㅇㅇ...",
          "original_form": "아 ㅇㅇㅇ...",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045150.1.18",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045150.1.19",
          "form": "애가 민감한 부분에 화한게 닿으니가",
          "original_form": "애가 민감한 부분에 화한게 닿으니가",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045150.1.20",
          "form": "신고당해도",
          "original_form": "신고당해도",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045150.1.21",
          "form": "온몸에 땀을 흘리며괴로워하는걸",
          "original_form": "온몸에 땀을 흘리며괴로워하는걸",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045150.1.22",
          "form": "할말 없는거 아니냐고",
          "original_form": "할말 없는거 아니냐고",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045150.1.23",
          "form": "폭력이지",
          "original_form": "폭력이지",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045150.1.24",
          "form": "깔깔거리면서 지켜보고",
          "original_form": "깔깔거리면서 지켜보고",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045150.1.25",
          "form": "온천가는 컨텐츠에",
          "original_form": "온천가는 컨텐츠에",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045150.1.26",
          "form": "일부러 모자이크안하고",
          "original_form": "일부러 모자이크안하고",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045150.1.27",
          "form": "수영장 간다해놓고 물에 녹는 옷 줘갖지고",
          "original_form": "수영장 간다해놓고 물에 녹는 옷 줘갖지고",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045150.1.28",
          "form": "아니 진짜 미개해",
          "original_form": "아니 진짜 미개해",
          "speaker_id": "2",
          "time": "20210908 01:03"
        }
      ]
    }
