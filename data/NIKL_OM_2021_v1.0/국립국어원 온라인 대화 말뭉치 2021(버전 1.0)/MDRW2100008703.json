      "utterance": [
        {
          "id": "MDRW2100008703.1.1",
          "form": "아이구..ㅠㅠㅠ",
          "original_form": "아이구..ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 17:33"
        },
        {
          "id": "MDRW2100008703.1.2",
          "form": "그집이 튀김을 잘튀기더라구요",
          "original_form": "그집이 튀김을 잘튀기더라구요",
          "speaker_id": "2",
          "time": "20210822 17:33"
        },
        {
          "id": "MDRW2100008703.1.3",
          "form": "튀김맛집ㅋㅋㅋㅋ",
          "original_form": "튀김맛집ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008703.1.4",
          "form": "튀김에 기름진게 많이느껴지면 느끼하잖아요",
          "original_form": "튀김에 기름진게 많이느껴지면 느끼하잖아요",
          "speaker_id": "2",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008703.1.5",
          "form": "그집은 그런게 없더라구요...",
          "original_form": "그집은 그런게 없더라구요...",
          "speaker_id": "2",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008703.1.6",
          "form": "맞아요 바삭해야해요",
          "original_form": "맞아요 바삭해야해요",
          "speaker_id": "1",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008703.1.7",
          "form": "최고..",
          "original_form": "최고..",
          "speaker_id": "1",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008703.1.8",
          "form": "아 떡볶이에 튀김도 좋아하시나요",
          "original_form": "아 떡볶이에 튀김도 좋아하시나요",
          "speaker_id": "1",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008703.1.9",
          "form": "캬... 그것도 좋죠",
          "original_form": "캬... 그것도 좋죠",
          "speaker_id": "2",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008703.1.10",
          "form": "순대에 튀김에!!",
          "original_form": "순대에 튀김에!!",
          "speaker_id": "2",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008703.1.11",
          "form": "필수 아닌가요..ㅎㅎ",
          "original_form": "필수 아닌가요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:35"
        },
        {
          "id": "MDRW2100008703.1.12",
          "form": "아 진짜 맛있겠어요 ㅜㅜ",
          "original_form": "아 진짜 맛있겠어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008703.1.13",
          "form": "어떤 튀김 젤 좋아하세요",
          "original_form": "어떤 튀김 젤 좋아하세요",
          "speaker_id": "1",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008703.1.14",
          "form": "저 김말이요!!",
          "original_form": "저 김말이요!!",
          "speaker_id": "2",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008703.1.15",
          "form": "고추튀김도 좋아해요!",
          "original_form": "고추튀김도 좋아해요!",
          "speaker_id": "2",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008703.1.16",
          "form": "만두는 납작만두가 좋고..!!",
          "original_form": "만두는 납작만두가 좋고..!!",
          "speaker_id": "2",
          "time": "20210822 17:38"
        },
        {
          "id": "MDRW2100008703.1.17",
          "form": "그냥 다 좋아하네요...^^",
          "original_form": "그냥 다 좋아하네요...^^",
          "speaker_id": "2",
          "time": "20210822 17:38"
        },
        {
          "id": "MDRW2100008703.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:38"
        },
        {
          "id": "MDRW2100008703.1.19",
          "form": "ㅋㅋㅋㅋ아 진짜 맛있겠어요 저는 깻잎튀김 진짜 좋아하는데 아시는지..",
          "original_form": "ㅋㅋㅋㅋ아 진짜 맛있겠어요 저는 깻잎튀김 진짜 좋아하는데 아시는지..",
          "speaker_id": "1",
          "time": "20210822 17:54"
        },
        {
          "id": "MDRW2100008703.1.20",
          "form": "저녁은 뭐 드세요?",
          "original_form": "저녁은 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210822 17:54"
        },
        {
          "id": "MDRW2100008703.1.21",
          "form": "깻잎튀김... 맛있을것같아요..!ㅠㅠㅠ",
          "original_form": "깻잎튀김... 맛있을것같아요..!ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008703.1.22",
          "form": "어디파는걸까요...",
          "original_form": "어디파는걸까요...",
          "speaker_id": "2",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008703.1.23",
          "form": "저는 묵은지가 땡겨서 김치찜 시켜먹으려구요..!",
          "original_form": "저는 묵은지가 땡겨서 김치찜 시켜먹으려구요..!",
          "speaker_id": "2",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008703.1.24",
          "form": "김치찜 댑악.. 돼지고기김치찜이죠?",
          "original_form": "김치찜 댑악.. 돼지고기김치찜이죠?",
          "speaker_id": "1",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008703.1.25",
          "form": "저는 돈까스 에어프라이어에 돌려먹고왔어요",
          "original_form": "저는 돈까스 에어프라이어에 돌려먹고왔어요",
          "speaker_id": "1",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008703.1.26",
          "form": "네네 돼지고기 김치찜이요!!",
          "original_form": "네네 돼지고기 김치찜이요!!",
          "speaker_id": "2",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100008703.1.27",
          "form": "오 돈까스도 맛있죠!!",
          "original_form": "오 돈까스도 맛있죠!!",
          "speaker_id": "2",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100008703.1.28",
          "form": "돼지고기김치찜에 쌀밥 먹으면 진짜 맛있죠 ㅜㅜ",
          "original_form": "돼지고기김치찜에 쌀밥 먹으면 진짜 맛있죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008703.1.29",
          "form": "돈까스 얇은 시장돈까스 먹었는데",
          "original_form": "돈까스 얇은 시장돈까스 먹었는데",
          "speaker_id": "1",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008703.1.30",
          "form": "그 바삭한 일식돈까스 먹구싶습니다",
          "original_form": "그 바삭한 일식돈까스 먹구싶습니다",
          "speaker_id": "1",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008703.1.31",
          "form": "알죠알죠 그 돈가쓰 소스에",
          "original_form": "알죠알죠 그 돈가쓰 소스에",
          "speaker_id": "2",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008703.1.32",
          "form": "한국이랑 일식돈까스는 많이 다르죠",
          "original_form": "한국이랑 일식돈까스는 많이 다르죠",
