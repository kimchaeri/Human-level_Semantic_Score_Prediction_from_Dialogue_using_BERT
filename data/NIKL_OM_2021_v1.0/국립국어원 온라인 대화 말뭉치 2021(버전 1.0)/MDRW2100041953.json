      "utterance": [
        {
          "id": "MDRW2100041953.1.1",
          "form": "아니아니 블루베리먹음!",
          "original_form": "아니아니 블루베리먹음!",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100041953.1.2",
          "form": "ㅋㅋㅋ 왜 아직도 다 안먹어봤는데 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 왜 아직도 다 안먹어봤는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100041953.1.3",
          "form": "최애맛이 궁금했는데",
          "original_form": "최애맛이 궁금했는데",
          "speaker_id": "1",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100041953.1.4",
          "form": "나 그동안 피자먹고 탕수육먹고 호이호이먹는다고ㅋㅋ",
          "original_form": "나 그동안 피자먹고 탕수육먹고 호이호이먹는다고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:19"
        },
        {
          "id": "MDRW2100041953.1.5",
          "form": "호이호이는뭐야??",
          "original_form": "호이호이는뭐야??",
          "speaker_id": "1",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100041953.1.6",
          "form": "그그 호떡같이 생긴 빵!",
          "original_form": "그그 호떡같이 생긴 빵!",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100041953.1.7",
          "form": "에? ㅋㅋㅋㅋㅋ그런게있어?",
          "original_form": "에? ㅋㅋㅋㅋㅋ그런게있어?",
          "speaker_id": "1",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100041953.1.8",
          "form": "헐 호이호이몰라?????",
          "original_form": "헐 호이호이몰라?????",
          "speaker_id": "2",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100041953.1.9",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100041953.1.10",
          "form": "에서 파는",
          "original_form": "에서 파는",
          "speaker_id": "2",
          "time": "20210906 23:23"
        },
        {
          "id": "MDRW2100041953.1.11",
          "form": "마트에서 파는 빵",
          "original_form": "마트에서 파는 빵",
          "speaker_id": "2",
          "time": "20210906 23:23"
        },
        {
          "id": "MDRW2100041953.1.12",
          "form": "어디서판다고??",
          "original_form": "어디서판다고??",
          "speaker_id": "1",
          "time": "20210906 23:23"
        },
        {
          "id": "MDRW2100041953.1.13",
          "form": "아 마트 호떡빵??",
          "original_form": "아 마트 호떡빵??",
          "speaker_id": "1",
          "time": "20210906 23:23"
        },
        {
          "id": "MDRW2100041953.1.14",
          "form": "그 막 여러개든거?",
          "original_form": "그 막 여러개든거?",
          "speaker_id": "1",
          "time": "20210906 23:23"
        },
        {
          "id": "MDRW2100041953.1.15",
          "form": "안에 흑설탕잼같은거들은? ㅋㅋㅋㅋ",
          "original_form": "안에 흑설탕잼같은거들은? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:24"
        },
        {
          "id": "MDRW2100041953.1.16",
          "form": "응응 맞아맞아ㅋㅋㅋ",
          "original_form": "응응 맞아맞아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:24"
        },
        {
          "id": "MDRW2100041953.1.17",
          "form": "지금도?",
          "original_form": "지금도?",
          "speaker_id": "2",
          "time": "20210906 23:46"
        },
        {
          "id": "MDRW2100041953.1.18",
          "form": "그거 에프에 돌려봤어?",
          "original_form": "그거 에프에 돌려봤어?",
          "speaker_id": "1",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100041953.1.19",
          "form": "아니! 남으면 돌려먹어야지하면서 항상 사온날 다먹음ㅜㅜ",
          "original_form": "아니! 남으면 돌려먹어야지하면서 항상 사온날 다먹음ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 23:53"
        },
        {
          "id": "MDRW2100041953.1.20",
          "form": "ㅋㅋㅋㅋㅋ 처음부터 돌려! 돌린게더맛있어",
          "original_form": "ㅋㅋㅋㅋㅋ 처음부터 돌려! 돌린게더맛있어",
          "speaker_id": "1",
          "time": "20210906 23:54"
        },
        {
          "id": "MDRW2100041953.1.21",
          "form": "그래? 맛잇다고들은 하던데 ㅋㅋㅋ",
          "original_form": "그래? 맛잇다고들은 하던데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:55"
        },
        {
          "id": "MDRW2100041953.1.22",
          "form": "귀찮아서 매번그냥먹음",
          "original_form": "귀찮아서 매번그냥먹음",
          "speaker_id": "2",
          "time": "20210906 23:55"
        },
        {
          "id": "MDRW2100041953.1.23",
          "form": "이번에 사오면 돌려먹어봐야지",
          "original_form": "이번에 사오면 돌려먹어봐야지",
          "speaker_id": "2",
          "time": "20210906 23:55"
        },
        {
          "id": "MDRW2100041953.1.24",
          "form": "겉에바삭해져!!",
          "original_form": "겉에바삭해져!!",
          "speaker_id": "1",
          "time": "20210906 23:56"
        },
        {
          "id": "MDRW2100041953.1.25",
          "form": "아니근데 그거 이름 처음알았다 ㅋㅋㅋ",
          "original_form": "아니근데 그거 이름 처음알았다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:56"
        }
      ]
