      "utterance": [
        {
          "id": "MDRW2100043681.1.1",
          "form": "요즘 쇼핑하신게 있으신가요?",
          "original_form": "요즘 쇼핑하신게 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043681.1.2",
          "form": "최근에는 식재료를 구입했는데",
          "original_form": "최근에는 식재료를 구입했는데",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043681.1.3",
          "form": "요즘 아이패드 사고 싶어서 둘러보고 있어요ㅋㅋ",
          "original_form": "요즘 아이패드 사고 싶어서 둘러보고 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043681.1.4",
          "form": "오오 곧 새로운게 출시된다는 기사를",
          "original_form": "오오 곧 새로운게 출시된다는 기사를",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043681.1.5",
          "form": "아까봤어요",
          "original_form": "아까봤어요",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043681.1.6",
          "form": "앗 새로운 게 출시된대요?",
          "original_form": "앗 새로운 게 출시된대요?",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043681.1.7",
          "form": "네 ㅋㅋㅋ 정확히 무슨 모델인지는",
          "original_form": "네 ㅋㅋㅋ 정확히 무슨 모델인지는",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043681.1.8",
          "form": "전 그건 모르고 추석때쯤 사야지 했는데ㅋ",
          "original_form": "전 그건 모르고 추석때쯤 사야지 했는데ㅋ",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043681.1.9",
          "form": "기억이안나요",
          "original_form": "기억이안나요",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043681.1.10",
          "form": "ㅋㅋㅋ지금이라두 알아보세요!",
          "original_form": "ㅋㅋㅋ지금이라두 알아보세요!",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043681.1.11",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043681.1.12",
          "form": "알아봐야겠네요 신제품!",
          "original_form": "알아봐야겠네요 신제품!",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043681.1.13",
          "form": "좋아요 신제품!",
          "original_form": "좋아요 신제품!",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043681.1.14",
          "form": "저는 디퓨저에 빠져서",
          "original_form": "저는 디퓨저에 빠져서",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043681.1.15",
          "form": "이것저것 알아보네요 ㅎㅎ",
          "original_form": "이것저것 알아보네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043681.1.16",
          "form": "아 디퓨저 좋죠",
          "original_form": "아 디퓨저 좋죠",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043681.1.17",
          "form": "플라워향이넘 좋아서 ㅎㅎ",
          "original_form": "플라워향이넘 좋아서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043681.1.18",
          "form": "디퓨저 하나 있음 기분전환도 되고 좋더라고요",
          "original_form": "디퓨저 하나 있음 기분전환도 되고 좋더라고요",
          "speaker_id": "2",
          "time": "20210907 20:02"
