      "utterance": [
        {
          "id": "MDRW2100014148.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014148.1.2",
          "form": "아녀 완전 집순이라 ㅋㅋㅋ",
          "original_form": "아녀 완전 집순이라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014148.1.3",
          "form": "근데 바다는 좋아요",
          "original_form": "근데 바다는 좋아요",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014148.1.4",
          "form": "아하!!ㅋㅋㅋ집에서 노는것도 너무좋죠ㅎㅎ",
          "original_form": "아하!!ㅋㅋㅋ집에서 노는것도 너무좋죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014148.1.5",
          "form": "그럼 여름에 바다 자주가시나요?",
          "original_form": "그럼 여름에 바다 자주가시나요?",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014148.1.6",
          "form": "자주 못 가요.. ㅠㅠㅠㅠ",
          "original_form": "자주 못 가요.. ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100014148.1.7",
          "form": "name1님은 좋앗던 여행지 있으세여?",
          "original_form": "&name1&님은 좋앗던 여행지 있으세여?",
          "speaker_id": "2",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100014148.1.8",
          "form": "저는 국내여행은 잘 안가봤는데 가봤던 곳 중에서 부산 좋았어요ㅎㅎ",
          "original_form": "저는 국내여행은 잘 안가봤는데 가봤던 곳 중에서 부산 좋았어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014148.1.9",
          "form": "왜 그렇게 다들 놀러가는지 완전 이해..!",
          "original_form": "왜 그렇게 다들 놀러가는지 완전 이해..!",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014148.1.10",
          "form": "마자요 ㅠㅠ 부산 좋져...",
          "original_form": "마자요 ㅠㅠ 부산 좋져...",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014148.1.11",
          "form": "바다 너무 가고 싶네여",
          "original_form": "바다 너무 가고 싶네여",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014148.1.12",
          "form": "특별히 좋아하는 바다 있으세요?",
          "original_form": "특별히 좋아하는 바다 있으세요?",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014148.1.13",
          "form": "인천 앞바다는 생각보다 별로더라구요",
          "original_form": "인천 앞바다는 생각보다 별로더라구요",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014148.1.14",
          "form": "부산이 제일 좋았던 거 같아요",
          "original_form": "부산이 제일 좋았던 거 같아요",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014148.1.15",
          "form": "인천이 가까워서 뭔가 만만한데 역시 부산이 제일 좋나봐요~~ㅎㅎ",
          "original_form": "인천이 가까워서 뭔가 만만한데 역시 부산이 제일 좋나봐요~~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100014148.1.16",
          "form": "해외여행도 그럼 잘 안가시는 편이세요?",
          "original_form": "해외여행도 그럼 잘 안가시는 편이세요?",
          "speaker_id": "1",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100014148.1.17",
          "form": "넹 저 고소공포증 심해서",
          "original_form": "넹 저 고소공포증 심해서",
          "speaker_id": "2",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014148.1.18",
          "form": "제주도 갈때 비행기 타는것도 힘들어서 ㅠㅠㅠ",
          "original_form": "제주도 갈때 비행기 타는것도 힘들어서 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014148.1.19",
          "form": "헉 고소공포증이시면 진짜 힘드시겠어요ㅠㅡㅠ",
          "original_form": "헉 고소공포증이시면 진짜 힘드시겠어요ㅠㅡㅠ",
          "speaker_id": "1",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100014148.1.20",
          "form": "크크 근데 집좋아하면 집에서도 재밌게 놀구 그래서 딱히 여행생각은 안들잖아요",
          "original_form": "크크 근데 집좋아하면 집에서도 재밌게 놀구 그래서 딱히 여행생각은 안들잖아요",
          "speaker_id": "1",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014148.1.21",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014148.1.22",
          "form": "여행가도 호캉스가 좋더라고요 ㅋㅋㅋ",
          "original_form": "여행가도 호캉스가 좋더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014148.1.23",
          "form": "ㅋㅋㅋㅋㅋ맞아요 요즘 코로나때문에도 호캉스나 풀빌라 가는 분들이 는것같아요!",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 요즘 코로나때문에도 호캉스나 풀빌라 가는 분들이 는것같아요!",
          "speaker_id": "1",
