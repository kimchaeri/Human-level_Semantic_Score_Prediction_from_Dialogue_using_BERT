      "utterance": [
        {
          "id": "MDRW2100015426.1.1",
          "form": "애완동물 기르시나요?",
          "original_form": "애완동물 기르시나요?",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015426.1.2",
          "form": "네 강아지 두마리요",
          "original_form": "네 강아지 두마리요",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015426.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015426.1.4",
          "form": "저는 기르고싶다는 생각만 ㅜ",
          "original_form": "저는 기르고싶다는 생각만 ㅜ",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015426.1.5",
          "form": "강아지 이름이 뭐예요?",
          "original_form": "강아지 이름이 뭐예요?",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015426.1.6",
          "form": "name3와 name4에요",
          "original_form": "&name3&와 &name4&에요",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015426.1.7",
          "form": "앗 귀엽겠다",
          "original_form": "앗 귀엽겠다",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015426.1.8",
          "form": "무슨 종인가요?",
          "original_form": "무슨 종인가요?",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015426.1.9",
          "form": "믹스예요 재패니즈 스피치 믹스와 말티즈믹스",
          "original_form": "믹스예요 재패니즈 스피치 믹스와 말티즈믹스",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015426.1.10",
          "form": "정말 귀여워요",
          "original_form": "정말 귀여워요",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015426.1.11",
          "form": "믹스들이 더 귀엽더라구요",
          "original_form": "믹스들이 더 귀엽더라구요",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015426.1.12",
          "form": "가끔영상보면 ㅎㅎ",
          "original_form": "가끔영상보면 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015426.1.13",
          "form": "ㅎㅎ 건강하기도 더 건강해요",
          "original_form": "ㅎㅎ 건강하기도 더 건강해요",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015426.1.14",
          "form": "반려동물 기르는데 얼마씩 들어요?",
          "original_form": "반려동물 기르는데 얼마씩 들어요?",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015426.1.15",
          "form": "어;; 어떻게 기르느냐에 따라 다른데요;",
          "original_form": "어;; 어떻게 기르느냐에 따라 다른데요;",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015426.1.16",
          "form": "아 그쵸그쵸",
          "original_form": "아 그쵸그쵸",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015426.1.17",
          "form": "병원비가 안들어도 한달에 최소 십만원은 잡아야 할거예요",
          "original_form": "병원비가 안들어도 한달에 최소 십만원은 잡아야 할거예요",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015426.1.18",
          "form": "돈이 많아야 기르겠네요ㅜ",
          "original_form": "돈이 많아야 기르겠네요ㅜ",
          "speaker_id": "1",
