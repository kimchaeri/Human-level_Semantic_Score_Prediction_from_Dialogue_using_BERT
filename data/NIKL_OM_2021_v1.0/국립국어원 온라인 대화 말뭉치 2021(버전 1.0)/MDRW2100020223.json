      "utterance": [
        {
          "id": "MDRW2100020223.1.1",
          "form": "테두리 바삭은",
          "original_form": "테두리 바삭은",
          "speaker_id": "2",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100020223.1.2",
          "form": "모든 전에 해당해죠",
          "original_form": "모든 전에 해당해죠",
          "speaker_id": "2",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100020223.1.3",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100020223.1.4",
          "form": "얇은거vs두꺼운거",
          "original_form": "얇은거vs두꺼운거",
          "speaker_id": "1",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100020223.1.5",
          "form": "침이고이네요",
          "original_form": "침이고이네요",
          "speaker_id": "2",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100020223.1.6",
          "form": "얇은거",
          "original_form": "얇은거",
          "speaker_id": "2",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100020223.1.7",
          "form": "저도 얇게 부친거 좋아합니다",
          "original_form": "저도 얇게 부친거 좋아합니다",
          "speaker_id": "1",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020223.1.8",
          "form": "두꺼우면 너무 빵같아서 싫더라구요",
          "original_form": "두꺼우면 너무 빵같아서 싫더라구요",
          "speaker_id": "1",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020223.1.9",
          "form": "맞아요 얇아야 더 바삭하기도하고",
          "original_form": "맞아요 얇아야 더 바삭하기도하고",
          "speaker_id": "2",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020223.1.10",
          "form": "맞아요 먹기도 편하고",
          "original_form": "맞아요 먹기도 편하고",
          "speaker_id": "1",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020223.1.11",
          "form": "간장 찍어먹는다 그냥 먹는다",
          "original_form": "간장 찍어먹는다 그냥 먹는다",
          "speaker_id": "1",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020223.1.12",
          "form": "그냥먹는다",
          "original_form": "그냥먹는다",
          "speaker_id": "2",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020223.1.13",
          "form": "김치전은 짭짤하니까",
          "original_form": "김치전은 짭짤하니까",
          "speaker_id": "2",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020223.1.14",
          "form": "저도 본연의 맛을 즐깁니다",
          "original_form": "저도 본연의 맛을 즐깁니다",
          "speaker_id": "1",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020223.1.15",
          "form": "감자전 이런거는 찍어먹는데",
          "original_form": "감자전 이런거는 찍어먹는데",
          "speaker_id": "2",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020223.1.16",
          "form": "김치전은 그냥 먹어야죠",
          "original_form": "김치전은 그냥 먹어야죠",
          "speaker_id": "2",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020223.1.17",
          "form": "감자전 너무좋아",
          "original_form": "감자전 너무좋아",
          "speaker_id": "1",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020223.1.18",
          "form": "감자전 좋아하시나요",
          "original_form": "감자전 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020223.1.19",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020223.1.20",
          "form": "약간 감자로 만든건 다 맛있지않나요",
          "original_form": "약간 감자로 만든건 다 맛있지않나요",
          "speaker_id": "1",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020223.1.21",
          "form": "감자전  vs 감자채전",
          "original_form": "감자전  vs 감자채전",
          "speaker_id": "2",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020223.1.22",
          "form": "감자전",
          "original_form": "감자전",
          "speaker_id": "1",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020223.1.23",
          "form": "쫀득한거 너무좋아",
          "original_form": "쫀득한거 너무좋아",
          "speaker_id": "1",
          "time": "20210828 00:29"
