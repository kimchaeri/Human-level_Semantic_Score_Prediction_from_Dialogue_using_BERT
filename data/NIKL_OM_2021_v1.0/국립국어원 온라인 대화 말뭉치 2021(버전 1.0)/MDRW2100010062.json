      "utterance": [
        {
          "id": "MDRW2100010062.1.1",
          "form": "안녕하세요. 챙겨먹는 영양제 있으세요?",
          "original_form": "안녕하세요. 챙겨먹는 영양제 있으세요?",
          "speaker_id": "1",
          "time": "20210823 13:42"
        },
        {
          "id": "MDRW2100010062.1.2",
          "form": "네 저는 비타민이랑 밀크시슬 챙겨먹어요",
          "original_form": "네 저는 비타민이랑 밀크시슬 챙겨먹어요",
          "speaker_id": "2",
          "time": "20210823 13:44"
        },
        {
          "id": "MDRW2100010062.1.3",
          "form": "mk님은요?",
          "original_form": "mk님은요?",
          "speaker_id": "2",
          "time": "20210823 13:44"
        },
        {
          "id": "MDRW2100010062.1.4",
          "form": "비타민이랑 유산균이요. 밀크시슬이 효과가 있나요?",
          "original_form": "비타민이랑 유산균이요. 밀크시슬이 효과가 있나요?",
          "speaker_id": "1",
          "time": "20210823 13:45"
        },
        {
          "id": "MDRW2100010062.1.5",
          "form": "네 저는 얼굴쪽에 열이 많이 올라와서 먹기 시작했는데",
          "original_form": "네 저는 얼굴쪽에 열이 많이 올라와서 먹기 시작했는데",
          "speaker_id": "2",
          "time": "20210823 13:45"
        },
        {
          "id": "MDRW2100010062.1.6",
          "form": "효과가 좀 있는 것 같아요",
          "original_form": "효과가 좀 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010062.1.7",
          "form": "비타민은 뭐 챙겨드세요",
          "original_form": "비타민은 뭐 챙겨드세요",
          "speaker_id": "1",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010062.1.8",
          "form": "전 C 먹습니다",
          "original_form": "전 C 먹습니다",
          "speaker_id": "1",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010062.1.9",
          "form": "비타민c랑 루테인이요",
          "original_form": "비타민c랑 루테인이요",
          "speaker_id": "2",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010062.1.10",
          "form": "운동도 자주 하시나요?",
          "original_form": "운동도 자주 하시나요?",
          "speaker_id": "2",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010062.1.11",
          "form": "일주일에 2번 pt 합니다",
          "original_form": "일주일에 2번 pt 합니다",
          "speaker_id": "1",
          "time": "20210823 13:47"
        },
        {
          "id": "MDRW2100010062.1.12",
          "form": "겨우겨우 하고 있네요",
          "original_form": "겨우겨우 하고 있네요",
          "speaker_id": "1",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100010062.1.13",
          "form": "메이님은요?",
          "original_form": "메이님은요?",
          "speaker_id": "1",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100010062.1.14",
          "form": "아 저는 주 2-3회 필라테스 해요",
          "original_form": "아 저는 주 2-3회 필라테스 해요",
          "speaker_id": "2",
          "time": "20210823 13:50"
        },
        {
          "id": "MDRW2100010062.1.15",
          "form": "pt 이전엔 저도 필라테스 했어요",
          "original_form": "pt 이전엔 저도 필라테스 했어요",
          "speaker_id": "1",
          "time": "20210823 13:50"
        },
        {
          "id": "MDRW2100010062.1.16",
          "form": "재밌더라고요",
          "original_form": "재밌더라고요",
          "speaker_id": "1",
          "time": "20210823 13:50"
        },
        {
          "id": "MDRW2100010062.1.17",
          "form": "저는 유연성이 별로 없어서 힘들긴 한데",
          "original_form": "저는 유연성이 별로 없어서 힘들긴 한데",
          "speaker_id": "2",
          "time": "20210823 13:51"
        },
        {
          "id": "MDRW2100010062.1.18",
          "form": "그래도 스트레칭 겸 하고 있어요",
          "original_form": "그래도 스트레칭 겸 하고 있어요",
          "speaker_id": "2",
          "time": "20210823 13:51"
        },
        {
          "id": "MDRW2100010062.1.19",
          "form": "제가 컴퓨터 앞에 앉아있는 시간이",
          "original_form": "제가 컴퓨터 앞에 앉아있는 시간이",
          "speaker_id": "1",
          "time": "20210823 13:57"
        },
        {
          "id": "MDRW2100010062.1.20",
          "form": "많은 일을해서 필라테스가 제 몸에 좋은 것 같았어요",
          "original_form": "많은 일을해서 필라테스가 제 몸에 좋은 것 같았어요",
          "speaker_id": "1",
          "time": "20210823 13:57"
        },
        {
          "id": "MDRW2100010062.1.21",
          "form": "패션엔 관심있으세요?",
          "original_form": "패션엔 관심있으세요?",
          "speaker_id": "1",
          "time": "20210823 13:58"
        },
        {
          "id": "MDRW2100010062.1.22",
          "form": "요즘은 재택하는 경우가 많아 패션은 신경을 덜 쓰는데",
          "original_form": "요즘은 재택하는 경우가 많아 패션은 신경을 덜 쓰는데",
          "speaker_id": "2",
          "time": "20210823 14:14"
        },
        {
          "id": "MDRW2100010062.1.23",
          "form": "저도 비슷해요. 평소 편하게 입고 다니기도 하고요",
          "original_form": "저도 비슷해요. 평소 편하게 입고 다니기도 하고요",
          "speaker_id": "1",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100010062.1.24",
          "form": "쇼핑은 한 달에 얼마나 하세요",
          "original_form": "쇼핑은 한 달에 얼마나 하세요",
          "speaker_id": "1",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100010062.1.25",
          "form": "네 저도요ㅎㅎ",
          "original_form": "네 저도요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100010062.1.26",
          "form": "음 그때그때 다른데",
          "original_form": "음 그때그때 다른데",
          "speaker_id": "2",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100010062.1.27",
          "form": "계절 바뀔 때는 옷 쇼핑 의욕이 샘솟고",
          "original_form": "계절 바뀔 때는 옷 쇼핑 의욕이 샘솟고",
          "speaker_id": "2",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100010062.1.28",
          "form": "아닐때는 좀 덜해요",
          "original_form": "아닐때는 좀 덜해요",
          "speaker_id": "2",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100010062.1.29",
          "form": "쇼핑 자주 하세요?",
          "original_form": "쇼핑 자주 하세요?",
          "speaker_id": "2",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100010062.1.30",
          "form": "한 달에 2번 정도 인터넷으로 사는 것 같아요",
          "original_form": "한 달에 2번 정도 인터넷으로 사는 것 같아요",
          "speaker_id": "1",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100010062.1.31",
          "form": "말하고 나니 적게 하는건 아닌것 같네요 ㅋㅋ",
          "original_form": "말하고 나니 적게 하는건 아닌것 같네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:19"
        },
        {
          "id": "MDRW2100010062.1.32",
          "form": "많이 사는 건 아니고ㅋㅋㅋ",
          "original_form": "많이 사는 건 아니고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:19"
        },
        {
          "id": "MDRW2100010062.1.33",
          "form": "요즘은 자켓 사고 싶네요",
          "original_form": "요즘은 자켓 사고 싶네요",
          "speaker_id": "1",
          "time": "20210823 14:20"
        },
        {
          "id": "MDRW2100010062.1.34",
          "form": "안 사본 옷에 도전하고 싶어서요",
          "original_form": "안 사본 옷에 도전하고 싶어서요",
          "speaker_id": "1",
          "time": "20210823 14:20"
        }
      ]
