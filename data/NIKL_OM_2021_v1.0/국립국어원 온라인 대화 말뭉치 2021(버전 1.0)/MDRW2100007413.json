      "utterance": [
        {
          "id": "MDRW2100007413.1.1",
          "form": "좋아 하는 거 있엉? 맛있는거",
          "original_form": "좋아 하는 거 있엉? 맛있는거",
          "speaker_id": "1",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007413.1.2",
          "form": "나는 먹는 거 진짜 좋아해",
          "original_form": "나는 먹는 거 진짜 좋아해",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007413.1.3",
          "form": "닉네임도 음식이잖아",
          "original_form": "닉네임도 음식이잖아",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007413.1.4",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007413.1.5",
          "form": "그렇구낰ㅋㅋㅋ",
          "original_form": "그렇구낰ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007413.1.6",
          "form": "안 좋아하는 거 찾는 게 빠를걸",
          "original_form": "안 좋아하는 거 찾는 게 빠를걸",
          "speaker_id": "2",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007413.1.7",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007413.1.8",
          "form": "홍어",
          "original_form": "홍어",
          "speaker_id": "1",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007413.1.9",
          "form": "는??",
          "original_form": "는??",
          "speaker_id": "1",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007413.1.10",
          "form": "홍어는...찾아먹지는 않고 있으면 먹긴해",
          "original_form": "홍어는...찾아먹지는 않고 있으면 먹긴해",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007413.1.11",
          "form": "너무 삭힌거는 못 먹어",
          "original_form": "너무 삭힌거는 못 먹어",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007413.1.12",
          "form": "나 홍어 완전 좋아하는데 ㅠ 먹어줄 사람이 너무 없엉 결국 명절마다 부모님이랑 먹는다 ㅎㅎㅠ",
          "original_form": "나 홍어 완전 좋아하는데 ㅠ 먹어줄 사람이 너무 없엉 결국 명절마다 부모님이랑 먹는다 ㅎㅎㅠ",
          "speaker_id": "1",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007413.1.13",
          "form": "다른 좋아하는 건 뭐 있어? ",
          "original_form": "다른 좋아하는 건 뭐 있어? ",
          "speaker_id": "1",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007413.1.14",
          "form": "음식중에?",
          "original_form": "음식중에?",
          "speaker_id": "2",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007413.1.15",
          "form": "흔히 말하는 소울푸드는 김치찌개야",
          "original_form": "흔히 말하는 소울푸드는 김치찌개야",
          "speaker_id": "2",
          "time": "20210822 02:02"
        },
        {
          "id": "MDRW2100007413.1.16",
          "form": "명절에 홍어 먹는 거면 내 고향이랑 비슷한 지역일수도 있겠다ㅎㅎ",
          "original_form": "명절에 홍어 먹는 거면 내 고향이랑 비슷한 지역일수도 있겠다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:02"
        },
        {
          "id": "MDRW2100007413.1.17",
          "form": "김치찌개는 확실히 엄마가 끓여준게 넘 맛있어",
          "original_form": "김치찌개는 확실히 엄마가 끓여준게 넘 맛있어",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007413.1.18",
          "form": "엇 우리집 홍어는 명절이라서가 아니라 걍 기호 식품이라큐ㅠㅠㅠ 아마 아닐수도 ㅎㅎ",
          "original_form": "엇 우리집 홍어는 명절이라서가 아니라 걍 기호 식품이라큐ㅠㅠㅠ 아마 아닐수도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007413.1.19",
          "form": "아 그렇구낭ㅋㅋㅋㅋ 난 그쪽 지역 출신이라 잔치? 그런날마다 항상 보거든ㅋㅋㅋㅋ",
          "original_form": "아 그렇구낭ㅋㅋㅋㅋ 난 그쪽 지역 출신이라 잔치? 그런날마다 항상 보거든ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007413.1.20",
          "form": "맞아 김치찌개는 진짜 아무리 맛있다는 데서 먹어봐도 엄마가 끓여준 게 더 맛있음",
          "original_form": "맞아 김치찌개는 진짜 아무리 맛있다는 데서 먹어봐도 엄마가 끓여준 게 더 맛있음",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007413.1.21",
          "form": "넘 좋,,겠다,,, 홍어 있는 명절 너무 부러운 부분ㅋㅋ",
          "original_form": "넘 좋,,겠다,,, 홍어 있는 명절 너무 부러운 부분ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007413.1.22",
          "form": "그치ㅋㅋㅋㅋ 어릴때부터 먹는거라 그런가 ㅋㅋㅋㅋ",
          "original_form": "그치ㅋㅋㅋㅋ 어릴때부터 먹는거라 그런가 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007413.1.23",
          "form": "입맛이 엄마 손맛 따라가나봐 ㅋㅋ",
          "original_form": "입맛이 엄마 손맛 따라가나봐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007413.1.24",
          "form": "맞아 딱 그거야 엄마 손맛이 내입맛에 딱임ㅋㅋ",
          "original_form": "맞아 딱 그거야 엄마 손맛이 내입맛에 딱임ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007413.1.25",
          "form": "또 남들이 안 좋아하는 것 중에 좋아하는 거 있어?",
          "original_form": "또 남들이 안 좋아하는 것 중에 좋아하는 거 있어?",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007413.1.26",
          "form": "뭐가 있을까 ㅋㅋ",
          "original_form": "뭐가 있을까 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007413.1.27",
          "form": "나 생각보다 호불호 생각 안하고 사나봐 ㅋㅋㅋㅋ",
          "original_form": "나 생각보다 호불호 생각 안하고 사나봐 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007413.1.28",
          "form": "싫어하는 건 견과류랑 깻잎인듯! 너무 싫어",
          "original_form": "싫어하는 건 견과류랑 깻잎인듯! 너무 싫어",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007413.1.29",
          "form": "와 나는 견과류는 너무 좋아해",
          "original_form": "와 나는 견과류는 너무 좋아해",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007413.1.30",
          "form": "깻잎이 들어간 거는 다 싫어?",
          "original_form": "깻잎이 들어간 거는 다 싫어?",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007413.1.31",
          "form": "참치김밥도?",
          "original_form": "참치김밥도?",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007413.1.32",
          "form": "헛 완전 내 헛점이야 ㅋㅋㅋ",
          "original_form": "헛 완전 내 헛점이야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007413.1.33",
          "form": "참치김밥 걘 애정으로 봐줄 수 있엉 ㅋㅋㅋ",
          "original_form": "참치김밥 걘 애정으로 봐줄 수 있엉 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007413.1.34",
          "form": "그외에 반찬이나 쌈으로는 안건드는거 같아 ㅎ 헛점 걸려버렸당",
          "original_form": "그외에 반찬이나 쌈으로는 안건드는거 같아 ㅎ 헛점 걸려버렸당",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007413.1.35",
          "form": "나 그거 뭔지 알아ㅋㅋㅋ",
          "original_form": "나 그거 뭔지 알아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007413.1.36",
          "form": "싫어하는 거 들어있는데도 먹을 수 있는 거 있잖아",
          "original_form": "싫어하는 거 들어있는데도 먹을 수 있는 거 있잖아",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007413.1.37",
          "form": "맞앜ㅋㅋㅌ깻잎 무시 할 수 있당 ㅋㅋ",
          "original_form": "맞앜ㅋㅋㅌ깻잎 무시 할 수 있당 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:14"
        }
      ]
    }
