      "utterance": [
        {
          "id": "MDRW2100016491.1.1",
          "form": "지우개 좀 더 사야겠어",
          "original_form": "지우개 좀 더 사야겠어",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016491.1.2",
          "form": "집에 굴러다니는 거 많던데",
          "original_form": "집에 굴러다니는 거 많던데",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016491.1.3",
          "form": "때타서 지우면 더 시커멓게 변해 ㅋㅋ",
          "original_form": "때타서 지우면 더 시커멓게 변해 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016491.1.4",
          "form": "ㅎㅎ 미술용 지우개로 사",
          "original_form": "ㅎㅎ 미술용 지우개로 사",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016491.1.5",
          "form": "그건 잘 지워지긴 하는데 지우개똥이 너무 많이 생겨서",
          "original_form": "그건 잘 지워지긴 하는데 지우개똥이 너무 많이 생겨서",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016491.1.6",
          "form": "ㅎㅎ 봐둔 지우개 있어?",
          "original_form": "ㅎㅎ 봐둔 지우개 있어?",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016491.1.7",
          "form": "ㅇㅇ 좀 비싸긴 한데 잘 지워지는 거 있더라고",
          "original_form": "ㅇㅇ 좀 비싸긴 한데 잘 지워지는 거 있더라고",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016491.1.8",
          "form": "이름이 뭔데?",
          "original_form": "이름이 뭔데?",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016491.1.9",
          "form": "ain 지우개",
          "original_form": "ain 지우개",
          "speaker_id": "2",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016491.1.10",
          "form": "아 지금 쓰는거?",
          "original_form": "아 지금 쓰는거?",
          "speaker_id": "1",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016491.1.11",
          "form": "ㅇㅇ 크기가 두 가지인데 작은거로 바꿔가면서 쓰는 게 나은 것 같아",
          "original_form": "ㅇㅇ 크기가 두 가지인데 작은거로 바꿔가면서 쓰는 게 나은 것 같아",
          "speaker_id": "2",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016491.1.12",
          "form": "맞아 큰거 사면 괜히 때만 많이 타",
          "original_form": "맞아 큰거 사면 괜히 때만 많이 타",
          "speaker_id": "1",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016491.1.13",
          "form": "근처 문방구 가서 사야겠다",
          "original_form": "근처 문방구 가서 사야겠다",
