        },
        {
          "id": "MDRW2100015243.1.2",
          "form": "넘어가유",
          "original_form": "넘어가유",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015243.1.3",
          "form": "일 하시나용?",
          "original_form": "일 하시나용?",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015243.1.4",
          "form": "전 합니다",
          "original_form": "전 합니다",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015243.1.5",
          "form": "아녕 전 아직 취준 ㅜㅜ",
          "original_form": "아녕 전 아직 취준 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015243.1.6",
          "form": "돈많은 밬수가 최고입니다",
          "original_form": "돈많은 밬수가 최고입니다",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015243.1.7",
          "form": "헉 부러버여 ㅋㅋㅋㅋㅋ",
          "original_form": "헉 부러버여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015243.1.8",
          "form": "제남친도 취준생이에요",
          "original_form": "제남친도 취준생이에요",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015243.1.9",
          "form": "파이팅",
          "original_form": "파이팅",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015243.1.10",
          "form": "응원합니다",
          "original_form": "응원합니다",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015243.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ고마워여 여러사람한테 응원받네여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ고마워여 여러사람한테 응원받네여",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015243.1.12",
          "form": "ㅋㅋㄱㄲ",
          "original_form": "ㅋㅋㄱㄲ",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015243.1.13",
          "form": "꿈이 뭐예요?",
          "original_form": "꿈이 뭐예요?",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015243.1.14",
          "form": "저는 나이가 있걸랑요",
          "original_form": "저는 나이가 있걸랑요",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015243.1.15",
          "form": "전 걍 편한 일..ㅋㅋㅋㅋ앉아서 일하구시퍼여",
          "original_form": "전 걍 편한 일..ㅋㅋㅋㅋ앉아서 일하구시퍼여",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015243.1.16",
          "form": "하",
          "original_form": "하",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015243.1.17",
          "form": "저는 판매직이에요",
          "original_form": "저는 판매직이에요",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015243.1.18",
          "form": "서서일해서 죽겠습니다",
          "original_form": "서서일해서 죽겠습니다",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015243.1.19",
          "form": "오호 제 전남친 판매직이였어여",
          "original_form": "오호 제 전남친 판매직이였어여",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015243.1.20",
          "form": "ㅋㅋㅋㅋㅋ전남친도 죽을라하더라구여 많아 힘드시져",
          "original_form": "ㅋㅋㅋㅋㅋ전남친도 죽을라하더라구여 많아 힘드시져",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015243.1.21",
          "form": "으헉ㅜㅜ진짜요?",
          "original_form": "으헉ㅜㅜ진짜요?",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015243.1.22",
          "form": "미ㅏㅈ아요",
          "original_form": "미ㅏㅈ아요",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015243.1.23",
          "form": "이 일을 선택한 제가 후회스럽습니다",
          "original_form": "이 일을 선택한 제가 후회스럽습니다",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015243.1.24",
          "form": "근데사무직은 안맞아요",
          "original_form": "근데사무직은 안맞아요",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015243.1.25",
          "form": "ㄲㄲ",
          "original_form": "ㄲㄲ",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015243.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ머얌 판매직이면 인센티브도 받나여??",
          "original_form": "ㅋㅋㅋㅋㅋㅋ머얌 판매직이면 인센티브도 받나여??",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015243.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015243.1.28",
          "form": "근데 힘든게",
          "original_form": "근데 힘든게",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015243.1.29",
          "form": "멍같은게..",
          "original_form": "멍같은게..",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015243.1.30",
          "form": "인센티브 받으려면",
          "original_form": "인센티브 받으려면",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015243.1.31",
          "form": "뼈갈아야됩니다",
          "original_form": "뼈갈아야됩니다",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015243.1.32",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015243.1.33",
          "form": "못받아요",
          "original_form": "못받아요",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015243.1.34",
          "form": "헉스 많이 힘들군요ㅜㅜ",
          "original_form": "헉스 많이 힘들군요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015243.1.35",
          "form": "힘내세용!",
          "original_form": "힘내세용!",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015243.1.36",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015243.1.37",
          "form": "다들 그렇죠 뭐",
          "original_form": "다들 그렇죠 뭐",
          "speaker_id": "1",
