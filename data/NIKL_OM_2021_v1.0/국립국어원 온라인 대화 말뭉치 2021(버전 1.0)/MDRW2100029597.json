      "utterance": [
        {
          "id": "MDRW2100029597.1.1",
          "form": "맵돌아다니면서",
          "original_form": "맵돌아다니면서",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029597.1.2",
          "form": "물풍선터뜨리는겜",
          "original_form": "물풍선터뜨리는겜",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029597.1.3",
          "form": "아 그 뭐드라",
          "original_form": "아 그 뭐드라",
          "speaker_id": "1",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029597.1.4",
          "form": "있었는데",
          "original_form": "있었는데",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029597.1.5",
          "form": "스릴있지요",
          "original_form": "스릴있지요",
          "speaker_id": "1",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029597.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029597.1.7",
          "form": "사천성비슷한데",
          "original_form": "사천성비슷한데",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029597.1.8",
          "form": "스타크같은것도 재밌죵",
          "original_form": "스타크같은것도 재밌죵",
          "speaker_id": "1",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029597.1.9",
          "form": "타일 마스터라고",
          "original_form": "타일 마스터라고",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029597.1.10",
          "form": "혼자하는건데",
          "original_form": "혼자하는건데",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029597.1.11",
          "form": "그것도재밌어요",
          "original_form": "그것도재밌어요",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029597.1.12",
          "form": "비행기게임도 좋고",
          "original_form": "비행기게임도 좋고",
          "speaker_id": "1",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029597.1.13",
          "form": "스타크는 안해봤어요",
          "original_form": "스타크는 안해봤어요",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029597.1.14",
          "form": "역시 게임이야기는 끝없군요",
          "original_form": "역시 게임이야기는 끝없군요",
          "speaker_id": "1",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100029597.1.15",
          "form": "보드게임도 요즘엔",
          "original_form": "보드게임도 요즘엔",
          "speaker_id": "2",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100029597.1.16",
          "form": "폰겜으로 즐길수 있더군요",
          "original_form": "폰겜으로 즐길수 있더군요",
          "speaker_id": "2",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100029597.1.17",
          "form": "대항해시대도 신기했어요",
          "original_form": "대항해시대도 신기했어요",
          "speaker_id": "1",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100029597.1.18",
          "form": "세트나 루미큐브",
          "original_form": "세트나 루미큐브",
          "speaker_id": "2",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100029597.1.19",
          "form": "바다한가운데서 굶어죽었지요",
          "original_form": "바다한가운데서 굶어죽었지요",
          "speaker_id": "1",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100029597.1.20",
          "form": "한번씩 합니다",
          "original_form": "한번씩 합니다",
          "speaker_id": "2",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100029597.1.21",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "2",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100029597.1.22",
          "form": "보드게임방 유행했죠",
