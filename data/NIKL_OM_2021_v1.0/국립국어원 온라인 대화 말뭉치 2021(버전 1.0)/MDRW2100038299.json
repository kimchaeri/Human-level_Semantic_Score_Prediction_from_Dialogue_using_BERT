      "utterance": [
        {
          "id": "MDRW2100038299.1.1",
          "form": "세대가 무슨 상관인가요",
          "original_form": "세대가 무슨 상관인가요",
          "speaker_id": "2",
          "time": "20210902 09:07"
        },
        {
          "id": "MDRW2100038299.1.2",
          "form": "ㅋㅋㅋㅋ그런가요!?",
          "original_form": "ㅋㅋㅋㅋ그런가요!?",
          "speaker_id": "1",
          "time": "20210902 09:07"
        },
        {
          "id": "MDRW2100038299.1.3",
          "form": "저도 예전 노래 많이 들어요",
          "original_form": "저도 예전 노래 많이 들어요",
          "speaker_id": "2",
          "time": "20210902 09:07"
        },
        {
          "id": "MDRW2100038299.1.4",
          "form": "오 어떤거요",
          "original_form": "오 어떤거요",
          "speaker_id": "1",
          "time": "20210902 09:07"
        },
        {
          "id": "MDRW2100038299.1.5",
          "form": "신화 노래도 가끔 들었어요",
          "original_form": "신화 노래도 가끔 들었어요",
          "speaker_id": "2",
          "time": "20210902 09:09"
        },
        {
          "id": "MDRW2100038299.1.6",
          "form": "신화!!",
          "original_form": "신화!!",
          "speaker_id": "1",
          "time": "20210902 09:09"
        },
        {
          "id": "MDRW2100038299.1.7",
          "form": "그건 또 제 윗세대네요...",
          "original_form": "그건 또 제 윗세대네요...",
          "speaker_id": "1",
          "time": "20210902 09:09"
        },
        {
          "id": "MDRW2100038299.1.8",
          "form": "전 요즘 연예인중엔 NCT가 좋더라구요",
          "original_form": "전 요즘 연예인중엔 NCT가 좋더라구요",
          "speaker_id": "1",
          "time": "20210902 09:09"
        },
        {
          "id": "MDRW2100038299.1.9",
          "form": "저는 투모로우바이투게더요",
          "original_form": "저는 투모로우바이투게더요",
          "speaker_id": "2",
          "time": "20210902 09:11"
        },
        {
          "id": "MDRW2100038299.1.10",
          "form": "오 투바투 들어봤어요",
          "original_form": "오 투바투 들어봤어요",
          "speaker_id": "1",
          "time": "20210902 09:12"
        },
        {
          "id": "MDRW2100038299.1.11",
          "form": "약간 마이너중에 메이저 취향이시네요...!",
          "original_form": "약간 마이너중에 메이저 취향이시네요...!",
          "speaker_id": "1",
          "time": "20210902 09:12"
        },
        {
          "id": "MDRW2100038299.1.12",
          "form": "ㅋㅋㅋ 마이너의 탑이었군요",
          "original_form": "ㅋㅋㅋ 마이너의 탑이었군요",
          "speaker_id": "2",
          "time": "20210902 09:13"
        },
        {
          "id": "MDRW2100038299.1.13",
          "form": "ㅋㅋㅋㅋ맞아요 그런거죠",
          "original_form": "ㅋㅋㅋㅋ맞아요 그런거죠",
          "speaker_id": "1",
          "time": "20210902 09:20"
        },
        {
          "id": "MDRW2100038299.1.14",
          "form": "티비는 어떤 거 보나요",
          "original_form": "티비는 어떤 거 보나요",
          "speaker_id": "1",
          "time": "20210902 09:20"
        },
        {
          "id": "MDRW2100038299.1.15",
          "form": "전 백종원 나오는것만가끔",
          "original_form": "전 백종원 나오는것만가끔",
          "speaker_id": "1",
          "time": "20210902 09:20"
        },
        {
          "id": "MDRW2100038299.1.16",
          "form": "저는 티비 음악방송만 봐요",
          "original_form": "저는 티비 음악방송만 봐요",
          "speaker_id": "2",
          "time": "20210902 09:20"
        },
        {
          "id": "MDRW2100038299.1.17",
          "form": "음악방송!!",
          "original_form": "음악방송!!",
          "speaker_id": "1",
          "time": "20210902 09:20"
        },
        {
          "id": "MDRW2100038299.1.18",
          "form": "그거 요즘도 학교 점심시간에 틀어주나요?",
          "original_form": "그거 요즘도 학교 점심시간에 틀어주나요?",
          "speaker_id": "1",
          "time": "20210902 09:21"
        },
        {
          "id": "MDRW2100038299.1.19",
          "form": "저는 잘 모르겠어요...",
          "original_form": "저는 잘 모르겠어요...",
          "speaker_id": "2",
          "time": "20210902 09:21"
        },
        {
          "id": "MDRW2100038299.1.20",
          "form": "아ㅠ학생이신줄알았어요",
          "original_form": "아ㅠ학생이신줄알았어요",
          "speaker_id": "1",
          "time": "20210902 09:22"
        },
        {
          "id": "MDRW2100038299.1.21",
          "form": "제가 다닐때는 틀어주기도 하고 안틀어주기도 해서요",
          "original_form": "제가 다닐때는 틀어주기도 하고 안틀어주기도 해서요",
          "speaker_id": "2",
          "time": "20210902 09:22"
        }
      ]
