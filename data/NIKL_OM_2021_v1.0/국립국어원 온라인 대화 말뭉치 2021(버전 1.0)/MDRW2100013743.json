      "utterance": [
        {
          "id": "MDRW2100013743.1.1",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋ 정말 게임들이 사악했어요",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋ 정말 게임들이 사악했어요",
          "speaker_id": "2",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013743.1.2",
          "form": "전 그리고 시간제한있는 그럼 게임들하면 너무 숨막혀서 아바타 옷입히기 이런 거나 집꾸미기를 더 자주 했어요",
          "original_form": "전 그리고 시간제한있는 그럼 게임들하면 너무 숨막혀서 아바타 옷입히기 이런 거나 집꾸미기를 더 자주 했어요",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013743.1.3",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013743.1.4",
          "form": "그냥 슈 애니메이션 보고",
          "original_form": "그냥 슈 애니메이션 보고",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013743.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013743.1.6",
          "form": "아 비비빅 그런 게임도 있었네요",
          "original_form": "아 비비빅 그런 게임도 있었네요",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013743.1.7",
          "form": "아 그렇네요! 그거랑 애니메이션 게임 하니까 뚱? 무슨 그런 케릭터 나오는 것도 봤었는데",
          "original_form": "아 그렇네요! 그거랑 애니메이션 게임 하니까 뚱? 무슨 그런 케릭터 나오는 것도 봤었는데",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013743.1.8",
          "form": "옹... 맞아요",
          "original_form": "옹... 맞아요",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013743.1.9",
          "form": "뿌까도!",
          "original_form": "뿌까도!",
          "speaker_id": "1",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013743.1.10",
          "form": "그 좀 어린아이 캐릭터인데 머리 빠글빠글해서 양갈래로 묶고있는 캐릭터 그거 재밌었어요",
          "original_form": "그 좀 어린아이 캐릭터인데 머리 빠글빠글해서 양갈래로 묶고있는 캐릭터 그거 재밌었어요",
          "speaker_id": "2",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013743.1.11",
          "form": "와 뿌까 진짜 오랜만이네요",
          "original_form": "와 뿌까 진짜 오랜만이네요",
          "speaker_id": "1",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013743.1.12",
          "form": "뿌까ㅋㅋㅋㅋㅋㅋ 맞아요 그거 중국거인줄 알았는데 국산 캐릭터였다는 사실이 놀라월죠",
          "original_form": "뿌까ㅋㅋㅋㅋㅋㅋ 맞아요 그거 중국거인줄 알았는데 국산 캐릭터였다는 사실이 놀라월죠",
          "speaker_id": "2",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013743.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013743.1.14",
          "form": "가루와 뿌까!",
          "original_form": "가루와 뿌까!",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013743.1.15",
          "form": "성우가 있었다는게 더 신기했어요",
          "original_form": "성우가 있었다는게 더 신기했어요",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013743.1.16",
          "form": "헐 걔들이 말을 했어요?",
          "original_form": "헐 걔들이 말을 했어요?",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013743.1.17",
          "form": "말은 안하셨던것같은데",
          "original_form": "말은 안하셨던것같은데",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013743.1.18",
          "form": "성우진이 있었어욬ㅋㅋㅋㅋ",
          "original_form": "성우진이 있었어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013743.1.19",
          "form": "아 약간 감탄사 같은 것만 하셨나?",
          "original_form": "아 약간 감탄사 같은 것만 하셨나?",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013743.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013743.1.21",
          "form": "뿌까에 성우진이 있다고..? 하고 놀랬었죠",
          "original_form": "뿌까에 성우진이 있다고..? 하고 놀랬었죠",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013743.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그리고 예전엔 CD로 된 게임팩? 같은 것ㄷㅎ 많이 했어ㅛ는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그리고 예전엔 CD로 된 게임팩? 같은 것ㄷㅎ 많이 했어ㅛ는데",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013743.1.23",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "1",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013743.1.24",
          "form": "저는 무슨 쿠키뭐시기..?",
          "original_form": "저는 무슨 쿠키뭐시기..?",
          "speaker_id": "1",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013743.1.25",
          "form": "전 짱구게임이랑 무슨 강아지 나오는 거? 많이 핳어요",
          "original_form": "전 짱구게임이랑 무슨 강아지 나오는 거? 많이 핳어요",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013743.1.26",
          "form": "있었는데 30분하고 포기했어요",
          "original_form": "있었는데 30분하고 포기했어요",
          "speaker_id": "1",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013743.1.27",
          "form": "어떻게하는지 몰라서",
          "original_form": "어떻게하는지 몰라서",
          "speaker_id": "1",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013743.1.28",
          "form": "헐 짱구",
          "original_form": "헐 짱구",
          "speaker_id": "1",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013743.1.29",
          "form": "저도 했었엉ㅅ",
          "original_form": "저도 했었엉ㅅ",
          "speaker_id": "1",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013743.1.30",
          "form": "와 기억력 좋으시네요",
          "original_form": "와 기억력 좋으시네요",
