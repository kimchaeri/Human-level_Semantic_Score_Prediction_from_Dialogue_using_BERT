        {
          "id": "MDRW2100008709.1.1",
          "form": "홀홀",
          "original_form": "홀홀",
          "speaker_id": "2",
          "time": "20210822 17:39"
        },
        {
          "id": "MDRW2100008709.1.2",
          "form": "물론이죠 레전드",
          "original_form": "물론이죠 레전드",
          "speaker_id": "2",
          "time": "20210822 17:39"
        },
        {
          "id": "MDRW2100008709.1.3",
          "form": "손민수라는 캐릭터가 나오는 전설의 웹튠",
          "original_form": "손민수라는 캐릭터가 나오는 전설의 웹튠",
          "speaker_id": "2",
          "time": "20210822 17:39"
        },
        {
          "id": "MDRW2100008709.1.4",
          "form": "그럼 그 풋사과는 안보시나요",
          "original_form": "그럼 그 풋사과는 안보시나요",
          "speaker_id": "2",
          "time": "20210822 17:39"
        },
        {
          "id": "MDRW2100008709.1.5",
          "form": "헐 너무 좋아해요",
          "original_form": "헐 너무 좋아해요",
          "speaker_id": "1",
          "time": "20210822 17:40"
        },
        {
          "id": "MDRW2100008709.1.6",
          "form": "유료분까지 챙겨봐요",
          "original_form": "유료분까지 챙겨봐요",
          "speaker_id": "1",
          "time": "20210822 17:40"
        },
        {
          "id": "MDRW2100008709.1.7",
          "form": "말랑말랑 귀여워서 ㅠㅠㅠ",
          "original_form": "말랑말랑 귀여워서 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 17:40"
        },
        {
          "id": "MDRW2100008709.1.8",
          "form": "아아악 약간 귀여운거 좋아하시는군요",
          "original_form": "아아악 약간 귀여운거 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008709.1.9",
          "form": "마쉬멜로 같은",
          "original_form": "마쉬멜로 같은",
          "speaker_id": "2",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008709.1.10",
          "form": "근데 스토리는 또 좋은",
          "original_form": "근데 스토리는 또 좋은",
          "speaker_id": "2",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008709.1.11",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008709.1.12",
          "form": "귀여운거 너무좋아해요",
          "original_form": "귀여운거 너무좋아해요",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008709.1.13",
          "form": "지금 추천해주신 웹툰보는데 참 재밌내요",
          "original_form": "지금 추천해주신 웹툰보는데 참 재밌내요",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008709.1.14",
          "form": "혹시 그 수영하는 웹튼고 보시나요",
          "original_form": "혹시 그 수영하는 웹튼고 보시나요",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008709.1.15",
          "form": "수영..수영이 뭐죠",
          "original_form": "수영..수영이 뭐죠",
          "speaker_id": "2",
          "time": "20210822 19:37"
        },
        {
          "id": "MDRW2100008709.1.16",
          "form": "어떤거죠?!",
          "original_form": "어떤거죠?!",
          "speaker_id": "2",
          "time": "20210822 19:37"
        },
        {
          "id": "MDRW2100008709.1.17",
          "form": "수영만화일기예요 제목이!!",
          "original_form": "수영만화일기예요 제목이!!",
          "speaker_id": "1",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008709.1.18",
          "form": "어푸어푸 하는거요 ㅋㅋㅋ",
          "original_form": "어푸어푸 하는거요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008709.1.19",
          "form": "헐 못봤어요",
          "original_form": "헐 못봤어요",
          "speaker_id": "2",
          "time": "20210822 19:41"
        },
        {
          "id": "MDRW2100008709.1.20",
          "form": "귀엽나보죠?!",
          "original_form": "귀엽나보죠?!",
          "speaker_id": "2",
          "time": "20210822 19:41"
        },
        {
          "id": "MDRW2100008709.1.21",
          "form": "일상툰인데",
          "original_form": "일상툰인데",
          "speaker_id": "1",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100008709.1.22",
          "form": "막 수영하고싶어지고그래요",
          "original_form": "막 수영하고싶어지고그래요",
          "speaker_id": "1",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100008709.1.23",
          "form": "코시국만 아니었으면 바로 등록했을걸요 ㅋㅋㅋ",
          "original_form": "코시국만 아니었으면 바로 등록했을걸요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100008709.1.24",
          "form": "오 40화까지 나와있군요",
          "original_form": "오 40화까지 나와있군요",
          "speaker_id": "2",
          "time": "20210822 20:33"
        },
        {
          "id": "MDRW2100008709.1.25",
          "form": "달리기 좋다 그림체 넘 귀여워요!!!!!",
          "original_form": "달리기 좋다 그림체 넘 귀여워요!!!!!",
          "speaker_id": "2",
          "time": "20210822 20:33"
        },
        {
          "id": "MDRW2100008709.1.26",
          "form": "혹시 정글쥬스 보시나요",
          "original_form": "혹시 정글쥬스 보시나요",
          "speaker_id": "2",
          "time": "20210822 20:33"
        },
        {
          "id": "MDRW2100008709.1.27",
          "form": "오 그건 뭐죠?",
          "original_form": "오 그건 뭐죠?",
          "speaker_id": "1",
          "time": "20210822 20:42"
        },
        {
          "id": "MDRW2100008709.1.28",
          "form": "무슨내용일지 감이안오네요 ㅎ",
          "original_form": "무슨내용일지 감이안오네요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 20:42"
        },
        {
          "id": "MDRW2100008709.1.29",
          "form": "그그 어떤 곤충이든 죽일 수 있는 에프킬라같은게 있는데",
          "original_form": "그그 어떤 곤충이든 죽일 수 있는 에프킬라같은게 있는데",
          "speaker_id": "2",
          "time": "20210822 20:45"
        },
        {
          "id": "MDRW2100008709.1.30",
          "form": "정글쥬스가 곤충을 dna 분자로 분해시켜버리는 약이거든요. 근데 분해된 곤충 분자를 사람이 들이마시게 되면 부작용으로 곤충들 능력을 갖게 되는데",
          "original_form": "정글쥬스가 곤충을 dna 분자로 분해시켜버리는 약이거든요. 근데 분해된 곤충 분자를 사람이 들이마시게 되면 부작용으로 곤충들 능력을 갖게 되는데",
          "speaker_id": "2",
          "time": "20210822 20:46"
        },
        {
          "id": "MDRW2100008709.1.31",
          "form": "일반인들은 징그러워하니까 이 부작용을 없애기위해 주인공이 곤충 능력을 가지고 있는 사람들이 배우는 학교에 들어가요. 거기서 수석으로 졸업하면 부작용을 없애는 약(?)을 줘서요",
          "original_form": "일반인들은 징그러워하니까 이 부작용을 없애기위해 주인공이 곤충 능력을 가지고 있는 사람들이 배우는 학교에 들어가요. 거기서 수석으로 졸업하면 부작용을 없애는 약(?)을 줘서요",
          "speaker_id": "2",
          "time": "20210822 20:47"
        },
        {
          "id": "MDRW2100008709.1.32",
          "form": "얼핏봐서 정확하지는 않은데 이런내용입니다",
          "original_form": "얼핏봐서 정확하지는 않은데 이런내용입니다",
          "speaker_id": "2",
          "time": "20210822 20:48"
        },
        {
          "id": "MDRW2100008709.1.33",
          "form": "와 진짜 신박하네요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 진짜 신박하네요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:04"
        },
        {
          "id": "MDRW2100008709.1.34",
          "form": "아이디어 진찌 참신하다 이것도 한번 봐야갰어요",
          "original_form": "아이디어 진찌 참신하다 이것도 한번 봐야갰어요",
          "speaker_id": "1",
          "time": "20210822 21:05"
        },
        {
          "id": "MDRW2100008709.1.35",
          "form": "그 무협만화는 연재분 벌써 다봤거든요 ㅋㅋ",
          "original_form": "그 무협만화는 연재분 벌써 다봤거든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:05"
        },
        {
          "id": "MDRW2100008709.1.36",
          "form": "화산귀환 재밋죠!!!!!!!!!!!",
          "original_form": "화산귀환 재밋죠!!!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210822 21:14"
        },
