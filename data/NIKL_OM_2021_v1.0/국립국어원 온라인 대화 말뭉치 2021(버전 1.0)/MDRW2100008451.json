      "utterance": [
        {
          "id": "MDRW2100008451.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008451.1.2",
          "form": "종종요? 특히 이사다닐3ㅐ 많이 하는거같네요",
          "original_form": "종종요? 특히 이사다닐3ㅐ 많이 하는거같네요",
          "speaker_id": "2",
          "time": "20210822 17:00"
        },
        {
          "id": "MDRW2100008451.1.3",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210822 17:05"
        },
        {
          "id": "MDRW2100008451.1.4",
          "form": "중고거래요!",
          "original_form": "중고거래요!",
          "speaker_id": "2",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008451.1.5",
          "form": "맨날 당근마켓 이용해요 ㅋㅋㅋㅋㅋ",
          "original_form": "맨날 당근마켓 이용해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008451.1.6",
          "form": "저도 당근",
          "original_form": "저도 당근",
          "speaker_id": "1",
          "time": "20210823 07:22"
        },
        {
          "id": "MDRW2100008451.1.7",
          "form": "당근이 아무래도 젤 편하더라구요 ㅎㅎ",
          "original_form": "당근이 아무래도 젤 편하더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 09:19"
        },
        {
          "id": "MDRW2100008451.1.8",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210823 09:52"
        },
        {
          "id": "MDRW2100008451.1.9",
          "form": "공기청정기 팔아었요",
          "original_form": "공기청정기 팔아었요",
          "speaker_id": "1",
          "time": "20210901 08:41"
        },
        {
          "id": "MDRW2100008451.1.10",
          "form": "헉 공기청정기요?",
          "original_form": "헉 공기청정기요?",
          "speaker_id": "2",
          "time": "20210901 13:21"
        },
        {
          "id": "MDRW2100008451.1.11",
          "form": "당근으로 파셨나욯ㅎㅎㅎ",
          "original_form": "당근으로 파셨나욯ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 13:21"
        },
        {
          "id": "MDRW2100008451.1.12",
          "form": "차량용이라소형이에요",
          "original_form": "차량용이라소형이에요",
          "speaker_id": "1",
          "time": "20210901 14:23"
        },
        {
          "id": "MDRW2100008451.1.13",
          "form": "글쿤요~ 아무래도 차가 있으면 그 용퓸을 더 사고팔게되는 것 같아요",
          "original_form": "글쿤요~ 아무래도 차가 있으면 그 용퓸을 더 사고팔게되는 것 같아요",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100008451.1.14",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100008451.1.15",
          "form": "전 아직 차는 없어서 차량용은 선물로만 하게 되더라구요 ㅋㅋㅋㅋ",
          "original_form": "전 아직 차는 없어서 차량용은 선물로만 하게 되더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:39"
        }
