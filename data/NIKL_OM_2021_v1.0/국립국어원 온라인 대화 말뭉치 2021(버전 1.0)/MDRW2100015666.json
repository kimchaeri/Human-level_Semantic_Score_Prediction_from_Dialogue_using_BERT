      "utterance": [
        {
          "id": "MDRW2100015666.1.1",
          "form": "보는 건 좋아해요",
          "original_form": "보는 건 좋아해요",
          "speaker_id": "2",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015666.1.2",
          "form": "키우는 건 못하겠어요",
          "original_form": "키우는 건 못하겠어요",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015666.1.3",
          "form": "반려동물 키우고 싶으시죠",
          "original_form": "반려동물 키우고 싶으시죠",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015666.1.4",
          "form": "네 정말요 솔직히 책임질자신이 없어서 아직 못키워요..",
          "original_form": "네 정말요 솔직히 책임질자신이 없어서 아직 못키워요..",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015666.1.5",
          "form": "돈 많았으면 바로키웠을거같아요 \r\n근데사실돈많이없어도 사랑으로 키우는분도많잖아요",
          "original_form": "돈 많았으면 바로키웠을거같아요 \r\n근데사실돈많이없어도 사랑으로 키우는분도많잖아요",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015666.1.6",
          "form": "전 그거까진안되는거같아요",
          "original_form": "전 그거까진안되는거같아요",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015666.1.7",
          "form": "현실적이예요;-;",
          "original_form": "현실적이예요;-;",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015666.1.8",
          "form": "근데 진짜 돈 많이 들어서",
          "original_form": "근데 진짜 돈 많이 들어서",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015666.1.9",
          "form": "신중하게 생각하셔야할거 같긴해요",
          "original_form": "신중하게 생각하셔야할거 같긴해요",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015666.1.10",
          "form": "병원비가 어마어마해요",
          "original_form": "병원비가 어마어마해요",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015666.1.11",
          "form": "맞아요 청소도되게많이해줘야하구",
          "original_form": "맞아요 청소도되게많이해줘야하구",
          "speaker_id": "1",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015666.1.12",
          "form": "병원비..장난아니지요",
          "original_form": "병원비..장난아니지요",
          "speaker_id": "1",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015666.1.13",
          "form": "내병원도안가고호랑이연고바르는디",
          "original_form": "내병원도안가고호랑이연고바르는디",
          "speaker_id": "1",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015666.1.14",
          "form": "ㅋㄱㅋㅈㅋㅈㅋㅈㄱㅈㄱㅈㅋ",
          "original_form": "ㅋㄱㅋㅈㅋㅈㅋㅈㄱㅈㄱㅈㅋ",
          "speaker_id": "1",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015666.1.15",
          "form": "반려동물은 있으면 치료잘해주야지ㅜㅜ...",
          "original_form": "반려동물은 있으면 치료잘해주야지ㅜㅜ...",
          "speaker_id": "1",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015666.1.16",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015666.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015666.1.18",
          "form": "호랑이 연고ㅋㅋㅋㅋㅋㅋ",
          "original_form": "호랑이 연고ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015666.1.19",
          "form": "근데 전 키운다면 고양이 강아지는 자신없고",
          "original_form": "근데 전 키운다면 고양이 강아지는 자신없고",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015666.1.20",
          "form": "사실 물고기 키우고싶어요",
          "original_form": "사실 물고기 키우고싶어요",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015666.1.21",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015666.1.22",
          "form": "어울려요",
          "original_form": "어울려요",
          "speaker_id": "1",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015666.1.23",
          "form": "잘키울거같은ㅋㄱㅋㅋㅋ",
          "original_form": "잘키울거같은ㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015666.1.24",
          "form": "게을러서 반려식물이나 키우지만",
          "original_form": "게을러서 반려식물이나 키우지만",
          "speaker_id": "2",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015666.1.25",
          "form": "어항 갖다놓고 물멍 때리고싶네요",
          "original_form": "어항 갖다놓고 물멍 때리고싶네요",
          "speaker_id": "2",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015666.1.26",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015666.1.27",
          "form": "어항청소하기도은근빡시더라구요",
          "original_form": "어항청소하기도은근빡시더라구요",
          "speaker_id": "1",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015666.1.28",
          "form": "저는물고기무서워요",
          "original_form": "저는물고기무서워요",
          "speaker_id": "1",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015666.1.29",
          "form": "힝",
          "original_form": "힝",
          "speaker_id": "1",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015666.1.30",
          "form": "무서울 수 있죠",
          "original_form": "무서울 수 있죠",
          "speaker_id": "2",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015666.1.31",
          "form": "귀여운 생물이 최고긴 하지만",
          "original_form": "귀여운 생물이 최고긴 하지만",
          "speaker_id": "2",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015666.1.32",
          "form": "털이 감당이 안될거 같아요",
          "original_form": "털이 감당이 안될거 같아요",
          "speaker_id": "2",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015666.1.33",
          "form": "ㅋㅋㅋㅋㅋ맞아요 강력청소기필수",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 강력청소기필수",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015666.1.34",
          "form": "내 머리카락도 감당이 안되는데",
          "original_form": "내 머리카락도 감당이 안되는데",
          "speaker_id": "2",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015666.1.35",
          "form": "그건그래여..",
          "original_form": "그건그래여..",
          "speaker_id": "1",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015666.1.36",
          "form": "특히검은옷에털묻으면",
          "original_form": "특히검은옷에털묻으면",
          "speaker_id": "1",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015666.1.37",
          "form": "오우",
          "original_form": "오우",
          "speaker_id": "1",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015666.1.38",
          "form": "매일 부장님의 강아지 얘기를 듣는데",
          "original_form": "매일 부장님의 강아지 얘기를 듣는데",
          "speaker_id": "2",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015666.1.39",
          "form": "근데 옷에 털이 안묻어있는 것이 신기",
          "original_form": "근데 옷에 털이 안묻어있는 것이 신기",
          "speaker_id": "2",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015666.1.40",
          "form": "마당 넓은 집이라 애들도 건강하고",
