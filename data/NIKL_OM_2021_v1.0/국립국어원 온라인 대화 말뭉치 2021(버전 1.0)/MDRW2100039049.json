      "utterance": [
        {
          "id": "MDRW2100039049.1.1",
          "form": "오늘 아점 먹을 생각인데 괜찮니?",
          "original_form": "오늘 아점 먹을 생각인데 괜찮니?",
          "speaker_id": "2",
          "time": "20210903 07:16"
        },
        {
          "id": "MDRW2100039049.1.2",
          "form": "아점이요?",
          "original_form": "아점이요?",
          "speaker_id": "1",
          "time": "20210903 07:16"
        },
        {
          "id": "MDRW2100039049.1.3",
          "form": "응 니 동생이 아점으로 먹고 싶대",
          "original_form": "응 니 동생이 아점으로 먹고 싶대",
          "speaker_id": "2",
          "time": "20210903 07:16"
        },
        {
          "id": "MDRW2100039049.1.4",
          "form": "저야 상관없어요",
          "original_form": "저야 상관없어요",
          "speaker_id": "1",
          "time": "20210903 07:16"
        },
        {
          "id": "MDRW2100039049.1.5",
          "form": "미리 견과류 좀 먹어놔라",
          "original_form": "미리 견과류 좀 먹어놔라",
          "speaker_id": "2",
          "time": "20210903 07:16"
        },
        {
          "id": "MDRW2100039049.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 07:16"
        },
        {
          "id": "MDRW2100039049.1.7",
          "form": "넌 배고픈거 못참으니까",
          "original_form": "넌 배고픈거 못참으니까",
          "speaker_id": "2",
          "time": "20210903 07:17"
        },
        {
          "id": "MDRW2100039049.1.8",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210903 07:17"
        },
        {
          "id": "MDRW2100039049.1.9",
          "form": "감자샐러드 샌드위치 할거야",
          "original_form": "감자샐러드 샌드위치 할거야",
          "speaker_id": "2",
          "time": "20210903 07:17"
        },
        {
          "id": "MDRW2100039049.1.10",
          "form": "재료는요?",
          "original_form": "재료는요?",
          "speaker_id": "1",
          "time": "20210903 07:17"
        },
        {
          "id": "MDRW2100039049.1.11",
          "form": "어마가 미리 다 사놨지",
          "original_form": "어마가 미리 다 사놨지",
          "speaker_id": "2",
          "time": "20210903 07:17"
        },
        {
          "id": "MDRW2100039049.1.12",
          "form": "이번주는 먹을게 많네요",
          "original_form": "이번주는 먹을게 많네요",
          "speaker_id": "1",
          "time": "20210903 07:17"
        },
        {
          "id": "MDRW2100039049.1.13",
          "form": "그럼그럼",
          "original_form": "그럼그럼",
          "speaker_id": "2",
          "time": "20210903 07:17"
        },
        {
          "id": "MDRW2100039049.1.14",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210903 07:17"
        },
        {
          "id": "MDRW2100039049.1.15",
          "form": "만드는 거 좀 도와줄거지?",
          "original_form": "만드는 거 좀 도와줄거지?",
          "speaker_id": "2",
          "time": "20210903 07:18"
        },
        {
          "id": "MDRW2100039049.1.16",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210903 07:18"
        },
        {
          "id": "MDRW2100039049.1.17",
          "form": "팔아프니까 무리하지는 말고",
          "original_form": "팔아프니까 무리하지는 말고",
          "speaker_id": "2",
          "time": "20210903 07:18"
        },
        {
          "id": "MDRW2100039049.1.18",
          "form": "그건 일도 아니에요",
          "original_form": "그건 일도 아니에요",
          "speaker_id": "1",
          "time": "20210903 07:18"
        },
        {
          "id": "MDRW2100039049.1.19",
          "form": "그래도 지금 백신 맞았기 때문에 조심은 해야해",
          "original_form": "그래도 지금 백신 맞았기 때문에 조심은 해야해",
          "speaker_id": "2",
          "time": "20210903 07:18"
        },
