      "utterance": [
        {
          "id": "MDRW2100009141.1.1",
          "form": "제가 할수 있는건 청소밖에 없었네요",
          "original_form": "제가 할수 있는건 청소밖에 없었네요",
          "speaker_id": "2",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100009141.1.2",
          "form": "단체로 옷을 소독하니까",
          "original_form": "단체로 옷을 소독하니까",
          "speaker_id": "1",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100009141.1.3",
          "form": "하루종일 소독냄새가 나더라구요",
          "original_form": "하루종일 소독냄새가 나더라구요",
          "speaker_id": "1",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100009141.1.4",
          "form": "네소독해야죺많이 열악하죠",
          "original_form": "네소독해야죺많이 열악하죠",
          "speaker_id": "2",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100009141.1.5",
          "form": "환경이 진짜 열악하다던데",
          "original_form": "환경이 진짜 열악하다던데",
          "speaker_id": "2",
          "time": "20210822 21:42"
        },
        {
          "id": "MDRW2100009141.1.6",
          "form": "세상에 그럼 몸에 안좋을텐게",
          "original_form": "세상에 그럼 몸에 안좋을텐게",
          "speaker_id": "2",
          "time": "20210822 21:42"
        },
        {
          "id": "MDRW2100009141.1.7",
          "form": "데",
          "original_form": "데",
          "speaker_id": "2",
          "time": "20210822 21:42"
        },
        {
          "id": "MDRW2100009141.1.8",
          "form": "할머니들  공간에만 있었는데 친절한분도 계시고",
          "original_form": "할머니들  공간에만 있었는데 친절한분도 계시고",
          "speaker_id": "1",
          "time": "20210822 21:42"
        },
        {
          "id": "MDRW2100009141.1.9",
          "form": "치매가오신분도 계시고",
          "original_form": "치매가오신분도 계시고",
          "speaker_id": "1",
          "time": "20210822 21:42"
        },
        {
          "id": "MDRW2100009141.1.10",
          "form": "놀아드리기만해도 되는분도 계시고",
          "original_form": "놀아드리기만해도 되는분도 계시고",
          "speaker_id": "1",
          "time": "20210822 21:42"
        },
        {
          "id": "MDRW2100009141.1.11",
          "form": "각양각색인데 문제는 인력이 부족하단거죵",
          "original_form": "각양각색인데 문제는 인력이 부족하단거죵",
          "speaker_id": "1",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100009141.1.12",
          "form": "그게ㅠ진짜 힘들어요",
          "original_form": "그게ㅠ진짜 힘들어요",
          "speaker_id": "2",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100009141.1.13",
          "form": "말이 통하셨어요?",
          "original_form": "말이 통하셨어요?",
          "speaker_id": "2",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100009141.1.14",
          "form": "많이 가면 좋겧지만 다들바쁘니까요",
          "original_form": "많이 가면 좋겧지만 다들바쁘니까요",
          "speaker_id": "1",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100009141.1.15",
          "form": "영어나 힌디어를쓰는데",
          "original_form": "영어나 힌디어를쓰는데",
          "speaker_id": "1",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100009141.1.16",
          "form": "캘커타는 뱅갈어를 쓰지만 그냥 몸쓰는일이라",
          "original_form": "캘커타는 뱅갈어를 쓰지만 그냥 몸쓰는일이라",
          "speaker_id": "1",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100009141.1.17",
          "form": "대충다 알더라구요",
          "original_form": "대충다 알더라구요",
          "speaker_id": "1",
          "time": "20210822 21:44"
        },
        {
          "id": "MDRW2100009141.1.18",
          "form": "시범을보이면 다 따라해요",
          "original_form": "시범을보이면 다 따라해요",
          "speaker_id": "1",
          "time": "20210822 21:44"
        },
        {
          "id": "MDRW2100009141.1.19",
          "form": "아 그렇군요 언어가 되야 해외 봉사도 하는데",
          "original_form": "아 그렇군요 언어가 되야 해외 봉사도 하는데",
          "speaker_id": "2",
          "time": "20210822 21:44"
        },
        {
          "id": "MDRW2100009141.1.20",
          "form": "갖추셨네요",
          "original_form": "갖추셨네요",
          "speaker_id": "2",
          "time": "20210822 21:44"
        },
        {
          "id": "MDRW2100009141.1.21",
          "form": "저는 암껏도 모르고 그냥 살만큼만 배우고 다녔어요",
          "original_form": "저는 암껏도 모르고 그냥 살만큼만 배우고 다녔어요",
          "speaker_id": "1",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009141.1.22",
          "form": "서로 눈과 동작으로도 알아듣는 게 있는것 같아요",
          "original_form": "서로 눈과 동작으로도 알아듣는 게 있는것 같아요",
          "speaker_id": "2",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009141.1.23",
          "form": "에이 겸손하세요",
          "original_form": "에이 겸손하세요",
          "speaker_id": "2",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009141.1.24",
          "form": "그냥 우연히 가게된거라 ㅎㅎ",
          "original_form": "그냥 우연히 가게된거라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009141.1.25",
          "form": "인도는 아무나 못가요",
          "original_form": "인도는 아무나 못가요",
          "speaker_id": "2",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009141.1.26",
          "form": "일 같이 많이 하면",
          "original_form": "일 같이 많이 하면",
          "speaker_id": "1",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009141.1.27",
          "form": "전 절대 못했을듯요",
          "original_form": "전 절대 못했을듯요",
          "speaker_id": "2",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009141.1.28",
          "form": "친해지더래요",
          "original_form": "친해지더래요",
          "speaker_id": "1",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009141.1.29",
          "form": "지금은 위험하지만 글케 어려운일은 아니에요",
          "original_form": "지금은 위험하지만 글케 어려운일은 아니에요",
          "speaker_id": "1",
          "time": "20210822 21:46"
        }
      ]
