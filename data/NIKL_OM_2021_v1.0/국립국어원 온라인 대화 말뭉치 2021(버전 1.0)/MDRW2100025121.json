      "utterance": [
        {
          "id": "MDRW2100025121.1.1",
          "form": "언니 일 할만 하나??",
          "original_form": "언니 일 할만 하나??",
          "speaker_id": "1",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100025121.1.2",
          "form": "일이야 뭐ㅋㅋ",
          "original_form": "일이야 뭐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100025121.1.3",
          "form": "니는?",
          "original_form": "니는?",
          "speaker_id": "2",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100025121.1.4",
          "form": "ㅋㅋㅋㅋ 엉 나도 일이 바쁘지는 않아서 할만 합니다",
          "original_form": "ㅋㅋㅋㅋ 엉 나도 일이 바쁘지는 않아서 할만 합니다",
          "speaker_id": "1",
          "time": "20210901 08:26"
        },
        {
          "id": "MDRW2100025121.1.5",
          "form": "사회복지는 땄어요??",
          "original_form": "사회복지는 땄어요??",
          "speaker_id": "1",
          "time": "20210901 08:27"
        },
        {
          "id": "MDRW2100025121.1.6",
          "form": "아니 내년까지다ㅋㅋ",
          "original_form": "아니 내년까지다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:41"
        },
        {
          "id": "MDRW2100025121.1.7",
          "form": "니 일진 했었나",
          "original_form": "니 일진 했었나",
          "speaker_id": "2",
          "time": "20210901 09:41"
        },
        {
          "id": "MDRW2100025121.1.8",
          "form": "일진이 뭔데\r\n언니 ㅋㅋㅋㅋㅋㅋㄱㅋㅋ",
          "original_form": "일진이 뭔데\r\n언니 ㅋㅋㅋㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 09:49"
        },
        {
          "id": "MDRW2100025121.1.9",
          "form": "학교일진??",
          "original_form": "학교일진??",
          "speaker_id": "1",
          "time": "20210901 09:49"
        },
        {
          "id": "MDRW2100025121.1.10",
          "form": "일진하이",
          "original_form": "일진하이",
          "speaker_id": "2",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100025121.1.11",
          "form": "주식말이야ㅡㅡㅋ",
          "original_form": "주식말이야ㅡㅡㅋ",
          "speaker_id": "2",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100025121.1.12",
          "form": "아니 언니\r\nㅋㅋㅋㅋㄱㄱㄱㅋㅋㄱㄱ",
          "original_form": "아니 언니\r\nㅋㅋㅋㅋㄱㄱㄱㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100025121.1.13",
          "form": "나 주식안하잖아",
          "original_form": "나 주식안하잖아",
          "speaker_id": "1",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100025121.1.14",
          "form": "이제 주식 좀 해볼려고 하는데 아직 뭐가 뭔지 모르겠음",
          "original_form": "이제 주식 좀 해볼려고 하는데 아직 뭐가 뭔지 모르겠음",
          "speaker_id": "1",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100025121.1.15",
          "form": "그긧도 스트레스지ㅋㅋ",
          "original_form": "그긧도 스트레스지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100025121.1.16",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:53"
        },
        {
          "id": "MDRW2100025121.1.17",
          "form": "대충하자",
          "original_form": "대충하자",
          "speaker_id": "2",
          "time": "20210901 10:53"
        },
        {
          "id": "MDRW2100025121.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100025121.1.19",
          "form": "사회복지 공부 2년 하는거예요??",
          "original_form": "사회복지 공부 2년 하는거예요??",
          "speaker_id": "1",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100025121.1.20",
          "form": "힘들구먼",
          "original_form": "힘들구먼",
          "speaker_id": "1",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100025121.1.21",
          "form": "2년",
          "original_form": "2년",
          "speaker_id": "2",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100025121.1.22",
          "form": "대학교다",
          "original_form": "대학교다",
          "speaker_id": "2",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100025121.1.23",
          "form": "공부해야지",
          "original_form": "공부해야지",
          "speaker_id": "2",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100025121.1.24",
          "form": "글쿠만요",
          "original_form": "글쿠만요",
          "speaker_id": "1",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025121.1.25",
          "form": "공부....힘들다....",
          "original_form": "공부....힘들다....",
          "speaker_id": "1",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025121.1.26",
          "form": "학사과정 이예요??",
          "original_form": "학사과정 이예요??",
          "speaker_id": "1",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025121.1.27",
          "form": "그렇지ㅋㅋ",
          "original_form": "그렇지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100025121.1.28",
          "form": "학점따기도ㅡㅡ 시험문제어려음",
          "original_form": "학점따기도ㅡㅡ 시험문제어려음",
          "speaker_id": "2",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100025121.1.29",
          "form": "레포트 싫음",
          "original_form": "레포트 싫음",
          "speaker_id": "2",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100025121.1.30",
          "form": "아 레포트...",
          "original_form": "아 레포트...",
          "speaker_id": "1",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100025121.1.31",
          "form": "문제는 오픈북 아니라요??",
          "original_form": "문제는 오픈북 아니라요??",
          "speaker_id": "1",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100025121.1.32",
          "form": "어렵다근데",
          "original_form": "어렵다근데",
          "speaker_id": "2",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100025121.1.33",
