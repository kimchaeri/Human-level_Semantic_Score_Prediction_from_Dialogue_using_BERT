      "utterance": [
        {
          "id": "MDRW2100000479.1.1",
          "form": "요즘 운동할만해??",
          "original_form": "요즘 운동할만해??",
          "speaker_id": "1",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000479.1.2",
          "form": "요즘 운동 할만하십니까",
          "original_form": "요즘 운동 할만하십니까",
          "speaker_id": "1",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000479.1.3",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000479.1.4",
          "form": "안피곤해?",
          "original_form": "안피곤해?",
          "speaker_id": "1",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000479.1.5",
          "form": "귀찮아",
          "original_form": "귀찮아",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000479.1.6",
          "form": "난어제 80분 걷고 쓰러져서 잤어",
          "original_form": "난어제 80분 걷고 쓰러져서 잤어",
          "speaker_id": "1",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000479.1.7",
          "form": "피곤하진 않은데",
          "original_form": "피곤하진 않은데",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000479.1.8",
          "form": "살빼야지...60키로 넘으면 죽을고라며",
          "original_form": "살빼야지...60키로 넘으면 죽을고라며",
          "speaker_id": "1",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000479.1.9",
          "form": "살이 안빠지는것 같아",
          "original_form": "살이 안빠지는것 같아",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000479.1.10",
          "form": "걷기만해선",
          "original_form": "걷기만해선",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000479.1.11",
          "form": "뛰어야지 그럼",
          "original_form": "뛰어야지 그럼",
          "speaker_id": "1",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000479.1.12",
          "form": "언니는 안뛰어??",
          "original_form": "언니는 안뛰어??",
          "speaker_id": "1",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000479.1.13",
          "form": "뛰어야겠대",
          "original_form": "뛰어야겠대",
          "speaker_id": "2",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000479.1.14",
          "form": "걘 뛰는것보다 걷는게 더 힘들대",
          "original_form": "걘 뛰는것보다 걷는게 더 힘들대",
          "speaker_id": "2",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000479.1.15",
          "form": "마라톤 선수들 보면 엄청말랏잖아",
          "original_form": "마라톤 선수들 보면 엄청말랏잖아",
          "speaker_id": "1",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000479.1.16",
          "form": "오래뛰면 그렇게 살빠짐..",
          "original_form": "오래뛰면 그렇게 살빠짐..",
          "speaker_id": "1",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000479.1.17",
          "form": "그전에 내 무릎이 먼저 나갈까봐",
          "original_form": "그전에 내 무릎이 먼저 나갈까봐",
          "speaker_id": "2",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000479.1.18",
          "form": "못뛰어",
          "original_form": "못뛰어",
          "speaker_id": "2",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000479.1.19",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000479.1.20",
          "form": "그렇지 무릎조심해야지",
          "original_form": "그렇지 무릎조심해야지",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000479.1.21",
          "form": "할머니집 가서 자전거나 타",
          "original_form": "할머니집 가서 자전거나 타",
          "speaker_id": "1",
          "time": "20210819 16:26"
        }
      ]
