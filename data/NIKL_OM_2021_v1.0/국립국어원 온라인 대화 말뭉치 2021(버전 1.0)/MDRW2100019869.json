      "utterance": [
        {
          "id": "MDRW2100019869.1.1",
          "form": "커플에 휘말리면 힘들어징께..ㅌㅋㅋㅋㅋ",
          "original_form": "커플에 휘말리면 힘들어징께..ㅌㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019869.1.2",
          "form": "ㄹㅇ입니다",
          "original_form": "ㄹㅇ입니다",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019869.1.3",
          "form": "휘말린 적 있으신가요",
          "original_form": "휘말린 적 있으신가요",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019869.1.4",
          "form": "저 플러팅을 ..",
          "original_form": "저 플러팅을 ..",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019869.1.5",
          "form": "저는 아직 다행히 없시야",
          "original_form": "저는 아직 다행히 없시야",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019869.1.6",
          "form": "많이 당했거든요..",
          "original_form": "많이 당했거든요..",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019869.1.7",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019869.1.8",
          "form": "당황스러우셨겠어요",
          "original_form": "당황스러우셨겠어요",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019869.1.9",
          "form": "되게 선택의 기로에",
          "original_form": "되게 선택의 기로에",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019869.1.10",
          "form": "근데 제가 이쪽 문화를 잘 몰라서 그게 플러팅인줄 몰랏어요..",
          "original_form": "근데 제가 이쪽 문화를 잘 몰라서 그게 플러팅인줄 몰랏어요..",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019869.1.11",
          "form": "제가 선을 제대로 못그어서 안타깝게 생각합니다.. 단짝친구가 된줄알았어요",
          "original_form": "제가 선을 제대로 못그어서 안타깝게 생각합니다.. 단짝친구가 된줄알았어요",
          "speaker_id": "1",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019869.1.12",
          "form": "아 진짜요? 그 정도로 은근한거였군요...",
          "original_form": "아 진짜요? 그 정도로 은근한거였군요...",
          "speaker_id": "2",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019869.1.13",
          "form": "제가 눈치가 없어서",
          "original_form": "제가 눈치가 없어서",
          "speaker_id": "2",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019869.1.14",
          "form": "ㅋㅋㅋㅋㅋ전 하는 줄도 모르고",
          "original_form": "ㅋㅋㅋㅋㅋ전 하는 줄도 모르고",
          "speaker_id": "2",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019869.1.15",
          "form": "넘어갔을수도 있겠어요",
          "original_form": "넘어갔을수도 있겠어요",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019869.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ 저는 고백 받고서야 알아서..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저는 고백 받고서야 알아서..",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019869.1.17",
          "form": "플러팅 받으면 뭐랄까",
          "original_form": "플러팅 받으면 뭐랄까",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019869.1.18",
          "form": "너무 당황했읍니다",
          "original_form": "너무 당황했읍니다",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019869.1.19",
          "form": "거절하면 친구를 잃을까봐 고민되는게",
          "original_form": "거절하면 친구를 잃을까봐 고민되는게",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019869.1.20",
          "form": "있지않나요...ㅠ",
          "original_form": "있지않나요...ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019869.1.21",
          "form": "맞아요 ㅜㅜ",
          "original_form": "맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019869.1.22",
          "form": "저는 단짝친구였는데..",
          "original_form": "저는 단짝친구였는데..",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019869.1.23",
          "form": "진짜 놀라셨겠어요",
          "original_form": "진짜 놀라셨겠어요",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019869.1.24",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019869.1.25",
          "form": "안타깝네요.. 그런일도있고 커플들 사이에 끼고싶지않아서",
          "original_form": "안타깝네요.. 그런일도있고 커플들 사이에 끼고싶지않아서",
          "speaker_id": "1",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019869.1.26",
          "form": "전 최근에 사랑이란 무엇인가",
          "original_form": "전 최근에 사랑이란 무엇인가",
          "speaker_id": "2",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019869.1.27",
          "form": "생가갸본적이았다요",
          "original_form": "생가갸본적이았다요",
          "speaker_id": "2",
          "time": "20210827 22:38"
        }
      ]
    }
