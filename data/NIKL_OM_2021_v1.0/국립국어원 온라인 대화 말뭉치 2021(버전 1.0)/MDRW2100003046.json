      "utterance": [
        {
          "id": "MDRW2100003046.1.1",
          "form": "놀러가고싶어요",
          "original_form": "놀러가고싶어요",
          "speaker_id": "1",
          "time": "20210820 17:54"
        },
        {
          "id": "MDRW2100003046.1.2",
          "form": "여행가고싶어요",
          "original_form": "여행가고싶어요",
          "speaker_id": "1",
          "time": "20210820 17:56"
        },
        {
          "id": "MDRW2100003046.1.3",
          "form": "어디로요?",
          "original_form": "어디로요?",
          "speaker_id": "2",
          "time": "20210820 23:10"
        },
        {
          "id": "MDRW2100003046.1.4",
          "form": "가고싶은 곳 있어요?",
          "original_form": "가고싶은 곳 있어요?",
          "speaker_id": "2",
          "time": "20210820 23:10"
        },
        {
          "id": "MDRW2100003046.1.5",
          "form": "흠 요즘엔 바다도 산도 가고싶은거같아요",
          "original_form": "흠 요즘엔 바다도 산도 가고싶은거같아요",
          "speaker_id": "1",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100003046.1.6",
          "form": "등산좋아하세요?",
          "original_form": "등산좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100003046.1.7",
          "form": "가파른 산은 넘힘들그 ㅜ",
          "original_form": "가파른 산은 넘힘들그 ㅜ",
          "speaker_id": "1",
          "time": "20210821 08:17"
        },
        {
          "id": "MDRW2100003046.1.8",
          "form": "어느정도면 좋아해영 ..",
          "original_form": "어느정도면 좋아해영 ..",
          "speaker_id": "1",
          "time": "20210821 08:17"
        },
        {
          "id": "MDRW2100003046.1.9",
          "form": "가볍게 오를 수 있는 곳?",
          "original_form": "가볍게 오를 수 있는 곳?",
          "speaker_id": "2",
          "time": "20210821 08:18"
        },
        {
          "id": "MDRW2100003046.1.10",
          "form": "음 어디가있지",
          "original_form": "음 어디가있지",
          "speaker_id": "2",
          "time": "20210821 08:18"
        },
        {
          "id": "MDRW2100003046.1.11",
          "form": "저도 잘멀라영 ...ㅋㅋㅋㅋㅋ 어디놀러가면",
          "original_form": "저도 잘멀라영 ...ㅋㅋㅋㅋㅋ 어디놀러가면",
          "speaker_id": "1",
          "time": "20210821 08:18"
        },
        {
          "id": "MDRW2100003046.1.12",
          "form": "검색해보도 가는폄이라",
          "original_form": "검색해보도 가는폄이라",
          "speaker_id": "1",
          "time": "20210821 08:18"
        },
        {
          "id": "MDRW2100003046.1.13",
          "form": "산이랑 바다 중 어디가 더 좋아요?",
          "original_form": "산이랑 바다 중 어디가 더 좋아요?",
          "speaker_id": "2",
          "time": "20210821 08:19"
        },
        {
          "id": "MDRW2100003046.1.14",
          "form": "전 산을 좀 더 좋아하긴 하는데 체력이 안받쳐줘서ㅎ",
          "original_form": "전 산을 좀 더 좋아하긴 하는데 체력이 안받쳐줘서ㅎ",
          "speaker_id": "2",
          "time": "20210821 08:19"
        },
        {
          "id": "MDRW2100003046.1.15",
          "form": "아진짜요? 저는 선택하라면 둘중엔 바다요!",
          "original_form": "아진짜요? 저는 선택하라면 둘중엔 바다요!",
          "speaker_id": "1",
          "time": "20210821 08:19"
        },
        {
          "id": "MDRW2100003046.1.16",
          "form": "바다? 음 이유는요?",
          "original_form": "바다? 음 이유는요?",
          "speaker_id": "2",
          "time": "20210821 08:20"
        },
        {
          "id": "MDRW2100003046.1.17",
          "form": "이유는 뭐딱히없는데 보고있으면 마음에 안정이 ㅎㅎ 산은 왜좋아하세요?",
          "original_form": "이유는 뭐딱히없는데 보고있으면 마음에 안정이 ㅎㅎ 산은 왜좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 08:21"
        },
        {
          "id": "MDRW2100003046.1.18",
          "form": "뭔가 푸르러서 좋더라구요",
          "original_form": "뭔가 푸르러서 좋더라구요",
          "speaker_id": "2",
          "time": "20210821 08:21"
        },
        {
          "id": "MDRW2100003046.1.19",
          "form": "바다좋아하면 해산물도 좋아하나요",
          "original_form": "바다좋아하면 해산물도 좋아하나요",
          "speaker_id": "2",
          "time": "20210821 08:21"
        },
        {
          "id": "MDRW2100003046.1.20",
          "form": "네네 잘먹죠 ㅎㅎ",
          "original_form": "네네 잘먹죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 08:22"
        }
      ]
    }
