      "utterance": [
        {
          "id": "MDRW2100027991.1.1",
          "form": "패션에 관심이 많구나",
          "original_form": "패션에 관심이 많구나",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100027991.1.2",
          "form": "패션의 완성은 얼굴이지",
          "original_form": "패션의 완성은 얼굴이지",
          "speaker_id": "1",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100027991.1.3",
          "form": "옷보다 얼굴과 미용관리해야지",
          "original_form": "옷보다 얼굴과 미용관리해야지",
          "speaker_id": "1",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100027991.1.4",
          "form": "미용은 중요해",
          "original_form": "미용은 중요해",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100027991.1.5",
          "form": "보이는게 얼굴이니까",
          "original_form": "보이는게 얼굴이니까",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100027991.1.6",
          "form": "손발관리는 피곤하겠어",
          "original_form": "손발관리는 피곤하겠어",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100027991.1.7",
          "form": "패디큐어같은거",
          "original_form": "패디큐어같은거",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100027991.1.8",
          "form": "패딩이 역시즌 세일하더라",
          "original_form": "패딩이 역시즌 세일하더라",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100027991.1.9",
          "form": "오 좋지. 미리사는것도",
          "original_form": "오 좋지. 미리사는것도",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100027991.1.10",
          "form": "옷은 비수기때 사야지",
          "original_form": "옷은 비수기때 사야지",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100027991.1.11",
          "form": "여름옷도 지금은 저렴해",
          "original_form": "여름옷도 지금은 저렴해",
          "speaker_id": "1",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100027991.1.12",
          "form": "파카사야지",
          "original_form": "파카사야지",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100027991.1.13",
          "form": "추우면 건강도 나빠져",
          "original_form": "추우면 건강도 나빠져",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100027991.1.14",
          "form": "겨울 오기전에 건강 대비해야지",
          "original_form": "겨울 오기전에 건강 대비해야지",
          "speaker_id": "1",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100027991.1.15",
          "form": "성형도 할까",
          "original_form": "성형도 할까",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100027991.1.16",
          "form": "배 지방은  겨울에 유리하자",
          "original_form": "배 지방은  겨울에 유리하자",
          "speaker_id": "1",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100027991.1.17",
          "form": "겨울엔 지방과 긴머리가 필요해",
          "original_form": "겨울엔 지방과 긴머리가 필요해",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100027991.1.18",
          "form": "코로나 어떻게 생각해",
          "original_form": "코로나 어떻게 생각해",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100027991.1.19",
          "form": "세상이 바꼈어",
          "original_form": "세상이 바꼈어",
          "speaker_id": "2",
