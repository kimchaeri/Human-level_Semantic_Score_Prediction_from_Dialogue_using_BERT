      "utterance": [
        {
          "id": "MDRW2100038016.1.1",
          "form": "둘째는 뭐하노?",
          "original_form": "둘째는 뭐하노?",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100038016.1.2",
          "form": "ㅋㅋㅋㅋㅋ마음에 안든다 안들어 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ마음에 안든다 안들어 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100038016.1.3",
          "form": "계속 대화해도 괜찮나",
          "original_form": "계속 대화해도 괜찮나",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100038016.1.4",
          "form": "신랑이 재우고있다.",
          "original_form": "신랑이 재우고있다.",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100038016.1.5",
          "form": "조용하네 ㅋㅋ",
          "original_form": "조용하네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100038016.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100038016.1.7",
          "form": "니는 언제자노?",
          "original_form": "니는 언제자노?",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100038016.1.8",
          "form": "그래도 신랑이랑 자니 다행이다",
          "original_form": "그래도 신랑이랑 자니 다행이다",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100038016.1.9",
          "form": "우린 내 아니면 안잠 ㅠ",
          "original_form": "우린 내 아니면 안잠 ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100038016.1.10",
          "form": "나 아까 애들 재우다가 쫌 잤다ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나 아까 애들 재우다가 쫌 잤다ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100038016.1.11",
          "form": "ㅋㅋㅋ응응",
          "original_form": "ㅋㅋㅋ응응",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100038016.1.12",
          "form": "깼나 ㅠㅠ",
          "original_form": "깼나 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100038016.1.13",
          "form": "그럼 잠이 더 잘 안오잖아",
          "original_form": "그럼 잠이 더 잘 안오잖아",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100038016.1.14",
          "form": "어 원래는 그냥 자는데   며칠 전부터 일어나",
          "original_form": "어 원래는 그냥 자는데   며칠 전부터 일어나",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100038016.1.15",
          "form": "내가 그래서 백신 맞아서",
          "original_form": "내가 그래서 백신 맞아서",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100038016.1.16",
          "form": "그런다고 진미언니한테 얘기 했는데",
          "original_form": "그런다고 진미언니한테 얘기 했는데",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100038016.1.17",
          "form": "응응 ㅋㅋㅋ",
          "original_form": "응응 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100038016.1.18",
          "form": "팔이 많이 욱씬거리는가보다",
          "original_form": "팔이 많이 욱씬거리는가보다",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100038016.1.19",
          "form": "화이자가 니한테 학을 학을 떼겠다고ㅋㅋㅋ",
          "original_form": "화이자가 니한테 학을 학을 떼겠다고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100038016.1.20",
          "form": "아무 데도 아픈 데가 없어ㅋㅋㅋㅋ",
          "original_form": "아무 데도 아픈 데가 없어ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100038016.1.21",
          "form": "피곤해 도화 이자",
          "original_form": "피곤해 도화 이자",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100038016.1.22",
          "form": "잠이 안 와도  화이자",
          "original_form": "잠이 안 와도  화이자",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100038016.1.23",
          "form": "ㅋㅋㅋ아 웃겨 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ아 웃겨 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100038016.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100038016.1.25",
          "form": "언니가 그만 좀 하라고ㅋㅋㅋ",
          "original_form": "언니가 그만 좀 하라고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100038016.1.26",
