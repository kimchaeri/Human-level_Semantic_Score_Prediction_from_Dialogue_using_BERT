        {
          "id": "MDRW2100024465.1.1",
          "form": "워치가 스트레칭하라네요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "워치가 스트레칭하라네요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:48"
        },
        {
          "id": "MDRW2100024465.1.2",
          "form": "똑똑해",
          "original_form": "똑똑해",
          "speaker_id": "1",
          "time": "20210831 10:48"
        },
        {
          "id": "MDRW2100024465.1.3",
          "form": "현질의 위대함",
          "original_form": "현질의 위대함",
          "speaker_id": "2",
          "time": "20210831 10:48"
        },
        {
          "id": "MDRW2100024465.1.4",
          "form": "아 아까 가사에서",
          "original_form": "아 아까 가사에서",
          "speaker_id": "2",
          "time": "20210831 10:48"
        },
        {
          "id": "MDRW2100024465.1.5",
          "form": "빨래",
          "original_form": "빨래",
          "speaker_id": "2",
          "time": "20210831 10:48"
        },
        {
          "id": "MDRW2100024465.1.6",
          "form": "너는거는",
          "original_form": "너는거는",
          "speaker_id": "2",
          "time": "20210831 10:48"
        },
        {
          "id": "MDRW2100024465.1.7",
          "form": "진짜 하기시름",
          "original_form": "진짜 하기시름",
          "speaker_id": "2",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100024465.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100024465.1.9",
          "form": "나중에 자취하면 건조기 삽니다",
          "original_form": "나중에 자취하면 건조기 삽니다",
          "speaker_id": "2",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100024465.1.10",
          "form": "우리집 건조기있음",
          "original_form": "우리집 건조기있음",
          "speaker_id": "1",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100024465.1.11",
          "form": "식세.기랑",
          "original_form": "식세.기랑",
          "speaker_id": "2",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100024465.1.12",
          "form": "건조기 없으면 못살아요",
          "original_form": "건조기 없으면 못살아요",
          "speaker_id": "1",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100024465.1.13",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "2",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100024465.1.14",
          "form": "수건만 돌려도 행복할듯요",
          "original_form": "수건만 돌려도 행복할듯요",
          "speaker_id": "2",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100024465.1.15",
          "form": "옷이 완전뽀송뽀송",
          "original_form": "옷이 완전뽀송뽀송",
          "speaker_id": "1",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100024465.1.16",
          "form": "군대에서 건조기 썼었는데",
          "original_form": "군대에서 건조기 썼었는데",
          "speaker_id": "2",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100024465.1.17",
          "form": "저 니트도 그냥돌리는데 다쪼그라들음 ㅋㅋㅋㅋㅋ",
          "original_form": "저 니트도 그냥돌리는데 다쪼그라들음 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100024465.1.18",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100024465.1.19",
          "form": "그건 좀..",
          "original_form": "그건 좀..",
          "speaker_id": "2",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100024465.1.20",
          "form": "먼지가 하두많아서",
          "original_form": "먼지가 하두많아서",
          "speaker_id": "1",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100024465.1.21",
          "form": "비싼 옷 머 있어요?",
          "original_form": "비싼 옷 머 있어요?",
          "speaker_id": "2",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100024465.1.22",
          "form": "건조기 먼지다빠지는거알죠 먼지진짜미쳐서",
          "original_form": "건조기 먼지다빠지는거알죠 먼지진짜미쳐서",
          "speaker_id": "1",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100024465.1.23",
          "form": "비싼옷없음ㅋㅋㅋㅋ비싼옷은 다 맡겨요",
          "original_form": "비싼옷없음ㅋㅋㅋㅋ비싼옷은 다 맡겨요",
          "speaker_id": "1",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100024465.1.24",
          "form": "세탁소가 괜히있는게아님",
          "original_form": "세탁소가 괜히있는게아님",
          "speaker_id": "1",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100024465.1.25",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100024465.1.26",
          "form": "똑똑하시네",
          "original_form": "똑똑하시네",
          "speaker_id": "2",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100024465.1.27",
          "form": "저번주 엄청습할때 하필 건조기고장나서",
          "original_form": "저번주 엄청습할때 하필 건조기고장나서",
          "speaker_id": "1",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100024465.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100024465.1.29",
          "form": "있다가 없으니까 또 엄청불편하드라구요",
          "original_form": "있다가 없으니까 또 엄청불편하드라구요",
          "speaker_id": "1",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100024465.1.30",
          "form": "저번주에 수건 널고나서",
          "original_form": "저번주에 수건 널고나서",
          "speaker_id": "2",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100024465.1.31",
          "form": "선풍기바람 쐬어줬습니다",
          "original_form": "선풍기바람 쐬어줬습니다",
          "speaker_id": "2",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100024465.1.32",
          "form": "우린 방에 제습기틀어놓고",
          "original_form": "우린 방에 제습기틀어놓고",
          "speaker_id": "1",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100024465.1.33",
          "form": "널었씁니다 그럼 완전잘마름",
          "original_form": "널었씁니다 그럼 완전잘마름",
          "speaker_id": "1",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100024465.1.34",
          "form": "흙수저와 금수저의 차이인가요",
          "original_form": "흙수저와 금수저의 차이인가요",
          "speaker_id": "2",
          "time": "20210831 10:52"
        },
        {
          "id": "MDRW2100024465.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:52"
        },
        {
          "id": "MDRW2100024465.1.36",
          "form": "아니 제습기완전 필수인데 ㅠ 저 습한거 완전싫어해서",
          "original_form": "아니 제습기완전 필수인데 ㅠ 저 습한거 완전싫어해서",
          "speaker_id": "1",
          "time": "20210831 10:52"
        },
        {
          "id": "MDRW2100024465.1.37",
          "form": "제습기는 하나 있는데요",
          "original_form": "제습기는 하나 있는데요",
          "speaker_id": "2",
          "time": "20210831 10:52"
        },
        {
          "id": "MDRW2100024465.1.38",
          "form": "너무 더워져요",
          "original_form": "너무 더워져요",
          "speaker_id": "2",
          "time": "20210831 10:52"
        },
        {
          "id": "MDRW2100024465.1.39",
          "form": "물도 빨리차서 갈아줘야대고",
          "original_form": "물도 빨리차서 갈아줘야대고",
          "speaker_id": "2",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024465.1.40",
          "form": "근데 건조하면 피부 빨리 상해요",
          "original_form": "근데 건조하면 피부 빨리 상해요",
          "speaker_id": "2",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024465.1.41",
          "form": "요새안덥지않아요?",
          "original_form": "요새안덥지않아요?",
          "speaker_id": "1",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024465.1.42",
          "form": "아 건조기 사람없을때만씀ㅋㅋㅋㅋ",
          "original_form": "아 건조기 사람없을때만씀ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024465.1.43",
          "form": "건조기래 제습기",
          "original_form": "건조기래 제습기",
          "speaker_id": "1",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024465.1.44",
          "form": "비오는날 출근하면서 집문다닫아놓고 틀어놓고",
          "original_form": "비오는날 출근하면서 집문다닫아놓고 틀어놓고",
          "speaker_id": "1",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024465.1.45",
          "form": "요샌 안 더운데 여름에 쓰면 그 방 찜질방댐",
          "original_form": "요샌 안 더운데 여름에 쓰면 그 방 찜질방댐",
          "speaker_id": "2",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024465.1.46",
          "form": "ㅇㅈ",
          "original_form": "ㅇㅈ",
          "speaker_id": "1",
          "time": "20210831 10:54"
        },
        {
          "id": "MDRW2100024465.1.47",
          "form": "그래도 안습하면 덜덥던데",
          "original_form": "그래도 안습하면 덜덥던데",
          "speaker_id": "1",
          "time": "20210831 10:54"
        },
        {
          "id": "MDRW2100024465.1.48",
          "form": "동남아가서 찝찝함에 함 적셔져야",
          "original_form": "동남아가서 찝찝함에 함 적셔져야",
          "speaker_id": "2",
          "time": "20210831 10:54"
        },
        {
          "id": "MDRW2100024465.1.49",
          "form": "한국에서 잘 견디실듯",
          "original_form": "한국에서 잘 견디실듯",
          "speaker_id": "2",
          "time": "20210831 10:54"
        },
        {
          "id": "MDRW2100024465.1.50",
          "form": "놀라운거말해줄까요",
          "original_form": "놀라운거말해줄까요",
          "speaker_id": "1",
          "time": "20210831 10:54"
        },
        {
          "id": "MDRW2100024465.1.51",
          "form": "뭔데요",
          "original_form": "뭔데요",
          "speaker_id": "2",
          "time": "20210831 10:54"
        },
        {
          "id": "MDRW2100024465.1.52",
          "form": "사무실에 에어컨없음",
          "original_form": "사무실에 에어컨없음",
          "speaker_id": "1",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100024465.1.53",
          "form": "별거아니면 차단핮니다",
          "original_form": "별거아니면 차단핮니다",
          "speaker_id": "2",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100024465.1.54",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100024465.1.55",
          "form": "여름어캐버텻는지",
          "original_form": "여름어캐버텻는지",
          "speaker_id": "1",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100024465.1.56",
          "form": "정신나갔네",
          "original_form": "정신나갔네",
          "speaker_id": "2",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100024465.1.57",
          "form": "컴퓨터 두대에 모니터만 4댄데 ㅋㅋㅋㅋ",
          "original_form": "컴퓨터 두대에 모니터만 4댄데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100024465.1.58",
          "form": "선풍기로버팀",
          "original_form": "선풍기로버팀",
          "speaker_id": "1",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100024465.1.59",
          "form": "솔직히 상줘야댐",
          "original_form": "솔직히 상줘야댐",
          "speaker_id": "1",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100024465.1.60",
          "form": "아니? 월급두배로줘야댐",
          "original_form": "아니? 월급두배로줘야댐",
          "speaker_id": "1",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100024465.1.61",
          "form": "염전노.예도 아니고",
          "original_form": "염전노.예도 아니고",
          "speaker_id": "2",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100024465.1.62",
          "form": "이야..",
          "original_form": "이야..",
          "speaker_id": "2",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100024465.1.63",
          "form": "대단하신 분이구나",
          "original_form": "대단하신 분이구나",
          "speaker_id": "2",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100024465.1.64",
          "form": "애들있는곳은 에어컨있음ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "애들있는곳은 에어컨있음ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100024465.1.65",
          "form": "정부에서 직원들이 쓰는 에어컨은 지원안된대요",
          "original_form": "정부에서 직원들이 쓰는 에어컨은 지원안된대요",
          "speaker_id": "1",
          "time": "20210831 10:56"
