        {
          "id": "MDRW2100034596.1.1",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100034596.1.2",
          "form": "어릴때 식습관이 중요한거같아요 몸매나",
          "original_form": "어릴때 식습관이 중요한거같아요 몸매나",
          "speaker_id": "1",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100034596.1.3",
          "form": "잘먹는사람은 어릴때부터 잘 안가리고 먹구요ㅋㅋ",
          "original_form": "잘먹는사람은 어릴때부터 잘 안가리고 먹구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100034596.1.4",
          "form": "맞아옄ㅋㅋ",
          "original_form": "맞아옄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:00"
        },
        {
          "id": "MDRW2100034596.1.5",
          "form": "전 편식 심했는데 크니까 좀 나아졌어요",
          "original_form": "전 편식 심했는데 크니까 좀 나아졌어요",
          "speaker_id": "1",
          "time": "20210828 16:00"
        },
        {
          "id": "MDRW2100034596.1.6",
          "form": "저는 어릴때도 참 뭐 안가리고 잘먹가지구 이렇게 됐나봐요ㅠ",
          "original_form": "저는 어릴때도 참 뭐 안가리고 잘먹가지구 이렇게 됐나봐요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:00"
        },
        {
          "id": "MDRW2100034596.1.7",
          "form": "오호! 반대로 된 경우도 있네요",
          "original_form": "오호! 반대로 된 경우도 있네요",
          "speaker_id": "2",
          "time": "20210828 16:00"
        },
        {
          "id": "MDRW2100034596.1.8",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100034596.1.9",
          "form": "어릴때 날씬하면",
          "original_form": "어릴때 날씬하면",
          "speaker_id": "1",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100034596.1.10",
          "form": "입맛이라는게 크면서 변하더라구요",
          "original_form": "입맛이라는게 크면서 변하더라구요",
          "speaker_id": "2",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100034596.1.11",
          "form": "성인되고 쪄도 좀 덜하대요",
          "original_form": "성인되고 쪄도 좀 덜하대요",
          "speaker_id": "1",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100034596.1.12",
          "form": "그건그래요!",
          "original_form": "그건그래요!",
          "speaker_id": "1",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100034596.1.13",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100034596.1.14",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "1",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100034596.1.15",
          "form": "세포자체가 어릴때 비만세포로 된다고 하더라구요ㅠ",
          "original_form": "세포자체가 어릴때 비만세포로 된다고 하더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100034596.1.16",
          "form": "이세상에 맛난게 이리많은지 몰랐어요",
          "original_form": "이세상에 맛난게 이리많은지 몰랐어요",
          "speaker_id": "1",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100034596.1.17",
          "form": "맞아요 저도 그말 들었어요 ㅜㅜ",
          "original_form": "맞아요 저도 그말 들었어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100034596.1.18",
          "form": "ㅋㅋㅋ커서라도 알아서 다행이네요ㅋㅋ",
          "original_form": "ㅋㅋㅋ커서라도 알아서 다행이네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100034596.1.19",
          "form": "어릴때 관리를 했었어야 했는데ㅠ",
          "original_form": "어릴때 관리를 했었어야 했는데ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100034596.1.20",
          "form": "맞아요 ㅜㅜ",
          "original_form": "맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100034596.1.21",
          "form": "어릴때 살은 키로 간다고해서 믿었는데",
          "original_form": "어릴때 살은 키로 간다고해서 믿었는데",
          "speaker_id": "2",
          "time": "20210828 16:04"
        },
        {
          "id": "MDRW2100034596.1.22",
          "form": "키로도 가고 살로도 가더라구요ㅋㅋㅋㅋ",
          "original_form": "키로도 가고 살로도 가더라구요ㅋㅋㅋㅋ",
