      "utterance": [
        {
          "id": "MDRW2100026705.1.1",
          "form": "제주살이는 어떠세요?",
          "original_form": "제주살이는 어떠세요?",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026705.1.2",
          "form": "좋아요 아직까지는 여름이라 해변 가구",
          "original_form": "좋아요 아직까지는 여름이라 해변 가구",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026705.1.3",
          "form": "이제 오름가야죠",
          "original_form": "이제 오름가야죠",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026705.1.4",
          "form": "보통 제 또래 친구들은 다 게스트하우스 스탭으로 일하면서 한달씩 두달씩 살더라구요",
          "original_form": "보통 제 또래 친구들은 다 게스트하우스 스탭으로 일하면서 한달씩 두달씩 살더라구요",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026705.1.5",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026705.1.6",
          "form": "그런 방법이",
          "original_form": "그런 방법이",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026705.1.7",
          "form": "여행가면서까지 일하면 조금 힘들긴 하겠지만",
          "original_form": "여행가면서까지 일하면 조금 힘들긴 하겠지만",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026705.1.8",
          "form": "그래도 재밌을 것 같더라구요 손님이랑도 친해질수도있구",
          "original_form": "그래도 재밌을 것 같더라구요 손님이랑도 친해질수도있구",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026705.1.9",
          "form": "저두 올해 초에 제주도 올레길 다녀왔었는데 좋았어요정마",
          "original_form": "저두 올해 초에 제주도 올레길 다녀왔었는데 좋았어요정마",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026705.1.10",
          "form": "외향적이시군요!",
          "original_form": "외향적이시군요!",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026705.1.11",
          "form": "저는 천천히 둘러보는 편이라",
          "original_form": "저는 천천히 둘러보는 편이라",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026705.1.12",
          "form": "3월부터 7월까지 매달 내려오다",
          "original_form": "3월부터 7월까지 매달 내려오다",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026705.1.13",
          "form": "제주에 아예 장기 여행 생각하고 집을 빌렸어요",
          "original_form": "제주에 아예 장기 여행 생각하고 집을 빌렸어요",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026705.1.14",
          "form": "오그럼 얼마나 계셨던 거에요??",
          "original_form": "오그럼 얼마나 계셨던 거에요??",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026705.1.15",
          "form": "제주진짜 여름에 좋던데",
          "original_form": "제주진짜 여름에 좋던데",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026705.1.16",
          "form": "짐 2주째구여",
          "original_form": "짐 2주째구여",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026705.1.17",
          "form": "내년 3월까지 일단 계약했어요",
          "original_form": "내년 3월까지 일단 계약했어요",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026705.1.18",
          "form": "어디어디 가보셨나요?",
          "original_form": "어디어디 가보셨나요?",
          "speaker_id": "2",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026705.1.19",
          "form": "제 친구가 저한테 금오름을 꼭 추천하던데 가보고싶더라구요",
          "original_form": "제 친구가 저한테 금오름을 꼭 추천하던데 가보고싶더라구요",
          "speaker_id": "2",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026705.1.20",
          "form": "오 아직 근처만 가보고",
          "original_form": "오 아직 근처만 가보고",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026705.1.21",
          "form": "오름오름 이라는 책사서",
          "original_form": "오름오름 이라는 책사서",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026705.1.22",
          "form": "동선짜고 있어요",
          "original_form": "동선짜고 있어요",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026705.1.23",
          "form": "진짜 재밌으시겠다",
          "original_form": "진짜 재밌으시겠다",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026705.1.24",
          "form": "제주가면 괜히 도시에서는 손도안대는 책읽고 그러잖아요",
          "original_form": "제주가면 괜히 도시에서는 손도안대는 책읽고 그러잖아요",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026705.1.25",
          "form": "제가 딱 그랬거든요",
          "original_form": "제가 딱 그랬거든요",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026705.1.26",
          "form": "근데 꼭 관광 명소 안가도 다 낯설고 새롭고 그런 마음이 좋으니까여",
          "original_form": "근데 꼭 관광 명소 안가도 다 낯설고 새롭고 그런 마음이 좋으니까여",
          "speaker_id": "1",
          "time": "20210902 00:00"
        }
      ]
    }
  ]
