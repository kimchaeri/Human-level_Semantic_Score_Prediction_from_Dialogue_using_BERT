      "utterance": [
        {
          "id": "MDRW2100020210.1.1",
          "form": "name3랑 name4 환절기인데 건강 괜찮을까?",
          "original_form": "&name3&랑 &name4& 환절기인데 건강 괜찮을까?",
          "speaker_id": "1",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020210.1.2",
          "form": "name3는 감기 좀 하지않겠나",
          "original_form": "&name3&는 감기 좀 하지않겠나",
          "speaker_id": "2",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020210.1.3",
          "form": "눈 주의해서 봐야지..",
          "original_form": "눈 주의해서 봐야지..",
          "speaker_id": "2",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020210.1.4",
          "form": "name4 요새 또 토 자주하는거나 잘 놀라는게 걱정스럽긴 함",
          "original_form": "&name4& 요새 또 토 자주하는거나 잘 놀라는게 걱정스럽긴 함",
          "speaker_id": "2",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020210.1.5",
          "form": "name4는 밤에 이불 안에 들어오던데",
          "original_form": "&name4&는 밤에 이불 안에 들어오던데",
          "speaker_id": "1",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020210.1.6",
          "form": "추운가..",
          "original_form": "추운가..",
          "speaker_id": "2",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020210.1.7",
          "form": "맞아 사료토한거 name4일 가능성이 높음",
          "original_form": "맞아 사료토한거 &name4&일 가능성이 높음",
          "speaker_id": "1",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020210.1.8",
          "form": "털옷 입었으면서",
          "original_form": "털옷 입었으면서",
          "speaker_id": "2",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020210.1.9",
          "form": "바버들",
          "original_form": "바버들",
          "speaker_id": "1",
          "time": "20210828 00:23"
        },
        {
          "id": "MDRW2100020210.1.10",
          "form": "털도 북슬북슬하면서",
          "original_form": "털도 북슬북슬하면서",
          "speaker_id": "1",
          "time": "20210828 00:23"
        },
        {
          "id": "MDRW2100020210.1.11",
          "form": "환절기면 또 털갈이 하려나",
          "original_form": "환절기면 또 털갈이 하려나",
          "speaker_id": "1",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020210.1.12",
          "form": "그러게 말야",
          "original_form": "그러게 말야",
          "speaker_id": "2",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020210.1.13",
          "form": "더위는 또 겁나 타면서",
          "original_form": "더위는 또 겁나 타면서",
          "speaker_id": "2",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020210.1.14",
          "form": "털갈이 안할때가 있나 싶기는 한데",
          "original_form": "털갈이 안할때가 있나 싶기는 한데",
          "speaker_id": "1",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020210.1.15",
          "form": "털 좀 빗겨줘..",
          "original_form": "털 좀 빗겨줘..",
          "speaker_id": "2",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020210.1.16",
          "form": "털갈이 하겠지",
          "original_form": "털갈이 하겠지",
          "speaker_id": "2",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020210.1.17",
          "form": "ㅇㅇㅋㅋ",
          "original_form": "ㅇㅇㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020210.1.18",
          "form": "니가 빗겨줘ㅋㅋ",
          "original_form": "니가 빗겨줘ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020210.1.19",
          "form": "ㅎㅎ귀차나",
          "original_form": "ㅎㅎ귀차나",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020210.1.20",
          "form": "어떻게 그럴수가",
          "original_form": "어떻게 그럴수가",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020210.1.21",
          "form": "아까 name4의 이마콩까지 받았으면서",
          "original_form": "아까 &name4&의 이마콩까지 받았으면서",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020210.1.22",
          "form": "ㅋㅋ그럴수도 있지",
          "original_form": "ㅋㅋ그럴수도 있지",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020210.1.23",
          "form": "애들 목욕은 올해 포기해야하나",
          "original_form": "애들 목욕은 올해 포기해야하나",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020210.1.24",
          "form": "그러게...날씨가 애매해서...",
          "original_form": "그러게...날씨가 애매해서...",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020210.1.25",
          "form": "내일 날씨 좋으면 도전해볼까",
          "original_form": "내일 날씨 좋으면 도전해볼까",
          "speaker_id": "1",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020210.1.26",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020210.1.27",
          "form": "되겠낭",
          "original_form": "되겠낭",
          "speaker_id": "2",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020210.1.28",
          "form": "뭐 목욕 1년 안시킬 수도 있지...",
          "original_form": "뭐 목욕 1년 안시킬 수도 있지...",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020210.1.29",
          "form": "1년 안시키는게 아니라 2년 되는거잖아ㅋㅋ",
          "original_form": "1년 안시키는게 아니라 2년 되는거잖아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:35"
        },
        {
          "id": "MDRW2100020210.1.30",
          "form": "지금이 1년째 안씻긴건데",
          "original_form": "지금이 1년째 안씻긴건데",
          "speaker_id": "1",
          "time": "20210828 01:36"
        },
        {
          "id": "MDRW2100020210.1.31",
          "form": "ㅎㅎ애들 깨끗하자나",
          "original_form": "ㅎㅎ애들 깨끗하자나",
          "speaker_id": "2",
          "time": "20210828 12:02"
        },
        {
          "id": "MDRW2100020210.1.32",
          "form": "확실하나ㅋㅋ",
          "original_form": "확실하나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100020210.1.33",
          "form": "ㅋㅋ겉보기는ㅎㅎ",
          "original_form": "ㅋㅋ겉보기는ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020210.1.34",
          "form": "name4 비듬있다고 뭐라 한 사람 누구지?",
          "original_form": "&name4& 비듬있다고 뭐라 한 사람 누구지?",
          "speaker_id": "1",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100020210.1.35",
          "form": "그건 건조해서..",
          "original_form": "그건 건조해서..",
          "speaker_id": "2",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100020210.1.36",
          "form": "name4는 빗이면 그냥 다 좋은걸까",
          "original_form": "&name4&는 빗이면 그냥 다 좋은걸까",
          "speaker_id": "1",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100020210.1.37",
          "form": "ㅋㅋ그럴수도",
          "original_form": "ㅋㅋ그럴수도",
          "speaker_id": "2",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100020210.1.38",
          "form": "생각보다 얌전해서 다행",
          "original_form": "생각보다 얌전해서 다행",
          "speaker_id": "2",
          "time": "20210828 15:48"
        }
      ]
