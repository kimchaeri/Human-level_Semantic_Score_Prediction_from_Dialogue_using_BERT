      "utterance": [
        {
          "id": "MDRW2100024606.1.1",
          "form": "나도 뭐 염색은 안하지 오래됐음",
          "original_form": "나도 뭐 염색은 안하지 오래됐음",
          "speaker_id": "2",
          "time": "20210831 15:31"
        },
        {
          "id": "MDRW2100024606.1.2",
          "form": "이제 속눈썹 펌도 안하고",
          "original_form": "이제 속눈썹 펌도 안하고",
          "speaker_id": "2",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100024606.1.3",
          "form": "발톱 시즌도 끝났고",
          "original_form": "발톱 시즌도 끝났고",
          "speaker_id": "2",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100024606.1.4",
          "form": "그래 미용 끝이군ㅋㅋ",
          "original_form": "그래 미용 끝이군ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100024606.1.5",
          "form": "이제뭐 없나?",
          "original_form": "이제뭐 없나?",
          "speaker_id": "1",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100024606.1.6",
          "form": "매일 머리를 묶고 있으니까",
          "original_form": "매일 머리를 묶고 있으니까",
          "speaker_id": "2",
          "time": "20210831 15:40"
        },
        {
          "id": "MDRW2100024606.1.7",
          "form": "딱히 돈 들여서 하기가 싫음",
          "original_form": "딱히 돈 들여서 하기가 싫음",
          "speaker_id": "2",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100024606.1.8",
          "form": "속눈썹 파마도 계속 하니까 속눈썹 빠지는 거 같고",
          "original_form": "속눈썹 파마도 계속 하니까 속눈썹 빠지는 거 같고",
          "speaker_id": "2",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100024606.1.9",
          "form": "미용에 관심이 없어졌음",
          "original_form": "미용에 관심이 없어졌음",
          "speaker_id": "2",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100024606.1.10",
          "form": "그래 난 뿌염은 계속하고싶다",
          "original_form": "그래 난 뿌염은 계속하고싶다",
          "speaker_id": "1",
          "time": "20210831 16:11"
        },
        {
          "id": "MDRW2100024606.1.11",
          "form": "놀이터가나?",
          "original_form": "놀이터가나?",
          "speaker_id": "1",
          "time": "20210831 16:12"
        },
        {
          "id": "MDRW2100024606.1.12",
          "form": "놀이터지 언니 ㅠ",
          "original_form": "놀이터지 언니 ㅠ",
          "speaker_id": "2",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100024606.1.13",
          "form": "우리 name3은 개학전에 파마했으 ㅋㅋ",
          "original_form": "우리 &name3&은 개학전에 파마했으 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100024606.1.14",
          "form": "잘했네ㅋㅋ",
          "original_form": "잘했네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100024606.1.15",
          "form": "지금은 뭐하노",
          "original_form": "지금은 뭐하노",
          "speaker_id": "1",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100024606.1.16",
          "form": "놀이터에서 놀고 집으로 가는 길",
          "original_form": "놀이터에서 놀고 집으로 가는 길",
          "speaker_id": "2",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100024606.1.17",
          "form": "오늘 투 언어 센터 가는 날이라서",
          "original_form": "오늘 투 언어 센터 가는 날이라서",
          "speaker_id": "2",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100024606.1.18",
          "form": "아 바쁘네 난내일부터운동할까 고민",
          "original_form": "아 바쁘네 난내일부터운동할까 고민",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024606.1.19",
          "form": "운동 뭐할라고요??",
          "original_form": "운동 뭐할라고요??",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100024606.1.20",
          "form": "그냥 아파트 가지뭐ㅋ",
          "original_form": "그냥 아파트 가지뭐ㅋ",
          "speaker_id": "1",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100024606.1.21",
          "form": "다른건 못함",
          "original_form": "다른건 못함",
          "speaker_id": "1",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100024606.1.22",
          "form": "밑에 문열었어요??",
          "original_form": "밑에 문열었어요??",
          "speaker_id": "2",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100024606.1.23",
          "form": "우린 아직 문닫은거 같던데",
          "original_form": "우린 아직 문닫은거 같던데",
          "speaker_id": "2",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100024606.1.24",
          "form": "앗 그래?ㅋㅋㅋ",
          "original_form": "앗 그래?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100024606.1.25",
          "form": "안 알아봄ㅋㅋ",
          "original_form": "안 알아봄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100024606.1.26",
          "form": "ㅋㅋㅋㅋㅋ 알아보슈",
          "original_form": "ㅋㅋㅋㅋㅋ 알아보슈",
          "speaker_id": "2",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100024606.1.27",
          "form": "우린 골프장 헬스장 다 문닫은거 같더라구요",
          "original_form": "우린 골프장 헬스장 다 문닫은거 같더라구요",
          "speaker_id": "2",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100024606.1.28",
          "form": "그래ㅋ 이제자나?",
          "original_form": "그래ㅋ 이제자나?",
          "speaker_id": "1",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100024606.1.29",
          "form": "어 언니 이제 잘라고",
          "original_form": "어 언니 이제 잘라고",
          "speaker_id": "2",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100024606.1.30",
          "form": "나도운동하고 싶다",
          "original_form": "나도운동하고 싶다",
          "speaker_id": "2",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100024606.1.31",
          "form": "작년에 파워워킹할때 좋았는데",
          "original_form": "작년에 파워워킹할때 좋았는데",
          "speaker_id": "2",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100024606.1.32",
          "form": "그마저도 할시간이 없구먼",
          "original_form": "그마저도 할시간이 없구먼",
          "speaker_id": "2",
          "time": "20210831 23:21"
        }
      ]
    }
