      "utterance": [
        {
          "id": "MDRW2100033723.1.1",
          "form": "무배가아니라 고민하다보니ㅋㅋ 쿠폰을 놓쳤네요",
          "original_form": "무배가아니라 고민하다보니ㅋㅋ 쿠폰을 놓쳤네요",
          "speaker_id": "1",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100033723.1.2",
          "form": "우선 모든지 받고봐야ㅎ",
          "original_form": "우선 모든지 받고봐야ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100033723.1.3",
          "form": "주문은 그후에결정",
          "original_form": "주문은 그후에결정",
          "speaker_id": "2",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033723.1.4",
          "form": "ㅋㅋㅋ그걸 근데 못봐서 ㅋㄱ아쉽",
          "original_form": "ㅋㅋㅋ그걸 근데 못봐서 ㅋㄱ아쉽",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033723.1.5",
          "form": "놓치고 항상 후회",
          "original_form": "놓치고 항상 후회",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033723.1.6",
          "form": "그러게요 아까 말씀드렸을땐 됐을건데",
          "original_form": "그러게요 아까 말씀드렸을땐 됐을건데",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033723.1.7",
          "form": "업체배송은 무배도있는데 아쉽네요",
          "original_form": "업체배송은 무배도있는데 아쉽네요",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033723.1.8",
          "form": "그쵸ㅋㅋ아쉽ㅋㅋ",
          "original_form": "그쵸ㅋㅋ아쉽ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033723.1.9",
          "form": "돈 절약했다 생각하셔요",
          "original_form": "돈 절약했다 생각하셔요",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033723.1.10",
          "form": "안쓰면 0원ㅎ",
          "original_form": "안쓰면 0원ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033723.1.11",
          "form": "오늘도 이렇게 강제절약",
          "original_form": "오늘도 이렇게 강제절약",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033723.1.12",
          "form": "아마 또 사실게 생길거예요",
          "original_form": "아마 또 사실게 생길거예요",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100033723.1.13",
          "form": "핫딜은 돌고도니까요ㅋㅋ 기다려야죵",
          "original_form": "핫딜은 돌고도니까요ㅋㅋ 기다려야죵",
          "speaker_id": "1",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033723.1.14",
          "form": "카페만 가면 자꾸생겨요",
          "original_form": "카페만 가면 자꾸생겨요",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033723.1.15",
          "form": "안사면 안 될거같고ㅋㅋ",
          "original_form": "안사면 안 될거같고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033723.1.16",
          "form": "그니까요 아는게 병이예요",
          "original_form": "그니까요 아는게 병이예요",
          "speaker_id": "2",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033723.1.17",
          "form": "심플리쿡도 모올라왔던데",
          "original_form": "심플리쿡도 모올라왔던데",
          "speaker_id": "2",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100033723.1.18",
          "form": "쿡킷? 말고요?",
          "original_form": "쿡킷? 말고요?",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100033723.1.19",
          "form": "혹시 수도권되는거 아닌가용",
          "original_form": "혹시 수도권되는거 아닌가용",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100033723.1.20",
          "form": "네 지에스프레시몰",
          "original_form": "네 지에스프레시몰",
          "speaker_id": "2",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100033723.1.21",
          "form": "지방은ㅈ안되는곳 있는거같아요",
          "original_form": "지방은ㅈ안되는곳 있는거같아요",
          "speaker_id": "2",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100033723.1.22",
          "form": "새벽배송이라",
          "original_form": "새벽배송이라",
          "speaker_id": "2",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100033723.1.23",
          "form": "아ㅠ안되요ㅠ여긴 택배ㅠㅠ",
          "original_form": "아ㅠ안되요ㅠ여긴 택배ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100033723.1.24",
          "form": "아쉽네요 근데 비싸서 별로예요",
          "original_form": "아쉽네요 근데 비싸서 별로예요",
          "speaker_id": "2",
          "time": "20210824 23:50"
        }
      ]
    }
