      "utterance": [
        {
          "id": "MDRW2100043876.1.1",
          "form": "나도 망고 좀 먹고 싶다",
          "original_form": "나도 망고 좀 먹고 싶다",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043876.1.2",
          "form": "웅 여긴 넘 비싸다",
          "original_form": "웅 여긴 넘 비싸다",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043876.1.3",
          "form": "망고 한 소쿠리 몇 천원 했는데",
          "original_form": "망고 한 소쿠리 몇 천원 했는데",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043876.1.4",
          "form": "마자 ㅋㅋ 진짜 싸더만",
          "original_form": "마자 ㅋㅋ 진짜 싸더만",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043876.1.5",
          "form": "맛있고 좋았다",
          "original_form": "맛있고 좋았다",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043876.1.6",
          "form": "우웅 거기 날씨가 덥고그래서 망고가 잘 나나보다",
          "original_form": "우웅 거기 날씨가 덥고그래서 망고가 잘 나나보다",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043876.1.7",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043876.1.8",
          "form": "우리나라에도 망고 좀 나면 좋겠다 ㅋㅋ",
          "original_form": "우리나라에도 망고 좀 나면 좋겠다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043876.1.9",
          "form": "그러게 좀 싸지면 좋겠다",
          "original_form": "그러게 좀 싸지면 좋겠다",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043876.1.10",
          "form": "웅 ㅠㅠ 망고먹으러 베트남 빨리 가고싶네",
          "original_form": "웅 ㅠㅠ 망고먹으러 베트남 빨리 가고싶네",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043876.1.11",
          "form": "내년에 한번 가자",
          "original_form": "내년에 한번 가자",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043876.1.12",
          "form": "웅웅",
          "original_form": "웅웅",
