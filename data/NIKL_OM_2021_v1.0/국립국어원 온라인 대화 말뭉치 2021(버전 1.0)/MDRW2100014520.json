      "utterance": [
        {
          "id": "MDRW2100014520.1.1",
          "form": "주말에 가족들과 뭐 하세요?",
          "original_form": "주말에 가족들과 뭐 하세요?",
          "speaker_id": "1",
          "time": "20210825 19:32"
        },
        {
          "id": "MDRW2100014520.1.2",
          "form": "이번주말엔 딱히 계획이없는데 아마 집에서 다같이 있을것같아요",
          "original_form": "이번주말엔 딱히 계획이없는데 아마 집에서 다같이 있을것같아요",
          "speaker_id": "2",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014520.1.3",
          "form": "name1님은 어떠세요",
          "original_form": "&name1&님은 어떠세요",
          "speaker_id": "2",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014520.1.4",
          "form": "전 이번 주말엔 여행계획이 있고 지난주엔 보드게임 했어요",
          "original_form": "전 이번 주말엔 여행계획이 있고 지난주엔 보드게임 했어요",
          "speaker_id": "1",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014520.1.5",
          "form": "오 가족끼리 보드게임 자주하시나봐요 전 동생들이랑 부루마블정도만해봤는데",
          "original_form": "오 가족끼리 보드게임 자주하시나봐요 전 동생들이랑 부루마블정도만해봤는데",
          "speaker_id": "2",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014520.1.6",
          "form": "저도 부루마블 했어요 ㅎㄹ",
          "original_form": "저도 부루마블 했어요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100014520.1.7",
          "form": "아하ㅋㅋㅋㅋ또 가족들이랑 보드게임 해본거 있으시나요",
          "original_form": "아하ㅋㅋㅋㅋ또 가족들이랑 보드게임 해본거 있으시나요",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014520.1.8",
          "form": "펭귄 얼음깨기랑 셈셈 수놀이 러쉬아어 우봉구 해요",
          "original_form": "펭귄 얼음깨기랑 셈셈 수놀이 러쉬아어 우봉구 해요",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014520.1.9",
          "form": "오 펭귄얼음깨기랑 우봉고해봤어요 둘다넘재밌던데",
          "original_form": "오 펭귄얼음깨기랑 우봉고해봤어요 둘다넘재밌던데",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014520.1.10",
          "form": "보드게임카페에서 하시는건가요 집에 사놓고 가족들이랑 같이하시는건가여",
          "original_form": "보드게임카페에서 하시는건가요 집에 사놓고 가족들이랑 같이하시는건가여",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014520.1.11",
          "form": "할리갈리도 있어요",
          "original_form": "할리갈리도 있어요",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014520.1.12",
          "form": "집에 이것저것 사놨어요",
          "original_form": "집에 이것저것 사놨어요",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014520.1.13",
          "form": "우와 집에서 재밌겠어요",
          "original_form": "우와 집에서 재밌겠어요",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014520.1.14",
          "form": "루미큐브도 있는데 아직 애들이 할 수준이 안되는것 같아요",
          "original_form": "루미큐브도 있는데 아직 애들이 할 수준이 안되는것 같아요",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014520.1.15",
          "form": "오 애기들 몇살이에요",
          "original_form": "오 애기들 몇살이에요",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014520.1.16",
          "form": "아직 유치원 생이예요 ㅎ",
          "original_form": "아직 유치원 생이예요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014520.1.17",
          "form": "악 넘귀여울때네요",
          "original_form": "악 넘귀여울때네요",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014520.1.18",
          "form": "애기들인거보면 한명이 아닌가봐여",
          "original_form": "애기들인거보면 한명이 아닌가봐여",
          "speaker_id": "2",
          "time": "20210825 20:26"
        }
      ]
