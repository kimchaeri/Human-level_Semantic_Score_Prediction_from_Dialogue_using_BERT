      "utterance": [
        {
          "id": "MDRW2100034449.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100034449.1.2",
          "form": "오늘 점심 뭐드세요?",
          "original_form": "오늘 점심 뭐드세요?",
          "speaker_id": "2",
          "time": "20210828 11:35"
        },
        {
          "id": "MDRW2100034449.1.3",
          "form": "오늘점심 피자땡기네요 ㅋㅋ",
          "original_form": "오늘점심 피자땡기네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100034449.1.4",
          "form": "피자 좋죠~ 배달 ㅅㅣ킬 거에요?",
          "original_form": "피자 좋죠~ 배달 ㅅㅣ킬 거에요?",
          "speaker_id": "2",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100034449.1.5",
          "form": "네~ 전 파파존스좋아해요!",
          "original_form": "네~ 전 파파존스좋아해요!",
          "speaker_id": "1",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100034449.1.6",
          "form": "전 미스터피자 가끔 가서 먹어요.",
          "original_form": "전 미스터피자 가끔 가서 먹어요.",
          "speaker_id": "2",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100034449.1.7",
          "form": "오 어떤메뉴 좋아하세요??",
          "original_form": "오 어떤메뉴 좋아하세요??",
          "speaker_id": "1",
          "time": "20210828 11:39"
        },
        {
          "id": "MDRW2100034449.1.8",
          "form": "불고기피자가 젤 맛나더라구요",
          "original_form": "불고기피자가 젤 맛나더라구요",
          "speaker_id": "2",
          "time": "20210828 11:40"
        },
        {
          "id": "MDRW2100034449.1.9",
          "form": "파파존스는 어떤 피자가 맛있나요",
          "original_form": "파파존스는 어떤 피자가 맛있나요",
          "speaker_id": "2",
          "time": "20210828 11:52"
        },
        {
          "id": "MDRW2100034449.1.10",
          "form": "파파존스 저는 존스페이버릿이나 스치랜 젤좋아해요!!",
          "original_form": "파파존스 저는 존스페이버릿이나 스치랜 젤좋아해요!!",
          "speaker_id": "1",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100034449.1.11",
          "form": "파파존스 한 번도 안 먹어봤는데 먹어보고 싶네요ㅡ",
          "original_form": "파파존스 한 번도 안 먹어봤는데 먹어보고 싶네요ㅡ",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034449.1.12",
          "form": "점심은 드셨나요",
          "original_form": "점심은 드셨나요",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034449.1.13",
          "form": "아니요~ 일단 찜닭시켜뒀어요",
          "original_form": "아니요~ 일단 찜닭시켜뒀어요",
          "speaker_id": "1",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100034449.1.14",
          "form": "찜닭 맛있죠ㅡ 전 어제 밤에 치킨 시켜먹었어요",
          "original_form": "찜닭 맛있죠ㅡ 전 어제 밤에 치킨 시켜먹었어요",
          "speaker_id": "2",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100034449.1.15",
          "form": "오 ㅋㅋㅋ 야식드셧군요?",
          "original_form": "오 ㅋㅋㅋ 야식드셧군요?",
          "speaker_id": "1",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100034449.1.16",
          "form": "네 애들이 하도 시켜달라고 해서요!",
          "original_form": "네 애들이 하도 시켜달라고 해서요!",
          "speaker_id": "2",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100034449.1.17",
          "form": "치킨 좋아하세요?",
          "original_form": "치킨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100034449.1.18",
          "form": "네 너무좋아해여 ㅋㅋㅋ",
          "original_form": "네 너무좋아해여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100034449.1.19",
          "form": "어떤치킨 주로사드시나요?",
          "original_form": "어떤치킨 주로사드시나요?",
          "speaker_id": "1",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100034449.1.20",
          "form": "네네 치킨이나 BHC요",
          "original_form": "네네 치킨이나 BHC요",
          "speaker_id": "2",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100034449.1.21",
          "form": "혹시 뿌링클 좋아하나요?",
          "original_form": "혹시 뿌링클 좋아하나요?",
          "speaker_id": "1",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100034449.1.22",
          "form": "네 뿌링클 좋아요",
          "original_form": "네 뿌링클 좋아요",
          "speaker_id": "2",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100034449.1.23",
          "form": "아하! 호불호많이갈리던데 전 안좋아해요 ㅋㅋ",
          "original_form": "아하! 호불호많이갈리던데 전 안좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100034449.1.24",
          "form": "ㅋㅋ 뿌링클도 좋고 네네 양념이 젤 좋아요",
          "original_form": "ㅋㅋ 뿌링클도 좋고 네네 양념이 젤 좋아요",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100034449.1.25",
          "form": "name1님은 어떤 치킨 조아요?",
          "original_form": "&name1&님은 어떤 치킨 조아요?",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100034449.1.26",
          "form": "아하! 저는 요새 체인점은 좀 질려서 개인사업장에서 픽업해서먹어요~",
          "original_form": "아하! 저는 요새 체인점은 좀 질려서 개인사업장에서 픽업해서먹어요~",
          "speaker_id": "1",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100034449.1.27",
          "form": "전 시장표 닭튀김도 맛나더라구요ㅡ 저렴하기도 하고",
          "original_form": "전 시장표 닭튀김도 맛나더라구요ㅡ 저렴하기도 하고",
          "speaker_id": "2",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100034449.1.28",
          "form": "저두 시장표닭튀김좋아해여",
          "original_form": "저두 시장표닭튀김좋아해여",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100034449.1.29",
