      "utterance": [
        {
          "id": "MDRW2100029498.1.1",
          "form": "냉동음식이죠",
          "original_form": "냉동음식이죠",
          "speaker_id": "2",
          "time": "20210903 09:41"
        },
        {
          "id": "MDRW2100029498.1.2",
          "form": "오뎅조금 있을거에요",
          "original_form": "오뎅조금 있을거에요",
          "speaker_id": "2",
          "time": "20210903 09:42"
        },
        {
          "id": "MDRW2100029498.1.3",
          "form": "저녁에 감자샐러드 요리할거에요",
          "original_form": "저녁에 감자샐러드 요리할거에요",
          "speaker_id": "1",
          "time": "20210903 09:42"
        },
        {
          "id": "MDRW2100029498.1.4",
          "form": "감자가 많네요",
          "original_form": "감자가 많네요",
          "speaker_id": "2",
          "time": "20210903 09:42"
        },
        {
          "id": "MDRW2100029498.1.5",
          "form": "모닝빵 배 갈라서 채워넣을거에요",
          "original_form": "모닝빵 배 갈라서 채워넣을거에요",
          "speaker_id": "1",
          "time": "20210903 09:42"
        },
        {
          "id": "MDRW2100029498.1.6",
          "form": "안질리나요",
          "original_form": "안질리나요",
          "speaker_id": "2",
          "time": "20210903 09:42"
        },
        {
          "id": "MDRW2100029498.1.7",
          "form": "감자샐러드 샌드위치아시나요",
          "original_form": "감자샐러드 샌드위치아시나요",
          "speaker_id": "1",
          "time": "20210903 09:42"
        },
        {
          "id": "MDRW2100029498.1.8",
          "form": "몰라요",
          "original_form": "몰라요",
          "speaker_id": "2",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100029498.1.9",
          "form": "감자는 주식입니다",
          "original_form": "감자는 주식입니다",
          "speaker_id": "1",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100029498.1.10",
          "form": "굽나요 삶나요",
          "original_form": "굽나요 삶나요",
          "speaker_id": "2",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100029498.1.11",
          "form": "삶아서 으깨서 마요네즈 넣어요",
          "original_form": "삶아서 으깨서 마요네즈 넣어요",
          "speaker_id": "1",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100029498.1.12",
          "form": "예전엔 악마의 열매라고 불렀죠",
          "original_form": "예전엔 악마의 열매라고 불렀죠",
          "speaker_id": "2",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100029498.1.13",
          "form": "구운감자도 맛나요",
          "original_form": "구운감자도 맛나요",
          "speaker_id": "2",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100029498.1.14",
          "form": "구황작물이지요",
          "original_form": "구황작물이지요",
          "speaker_id": "1",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100029498.1.15",
          "form": "존맛이라는게 중요",
          "original_form": "존맛이라는게 중요",
          "speaker_id": "2",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100029498.1.16",
          "form": "감자는 튀겨도 맛있어요",
          "original_form": "감자는 튀겨도 맛있어요",
