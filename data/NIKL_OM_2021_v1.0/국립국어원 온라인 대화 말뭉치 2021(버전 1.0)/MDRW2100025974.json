      "utterance": [
        {
          "id": "MDRW2100025974.1.1",
          "form": "안녕. 최근에 구매한 물건이 있어?",
          "original_form": "안녕. 최근에 구매한 물건이 있어?",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025974.1.2",
          "form": "폰",
          "original_form": "폰",
          "speaker_id": "2",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100025974.1.3",
          "form": "혹시 플립? 요새 그거 인기 많은데",
          "original_form": "혹시 플립? 요새 그거 인기 많은데",
          "speaker_id": "1",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025974.1.4",
          "form": "플립 맞음",
          "original_form": "플립 맞음",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025974.1.5",
          "form": "후기 듣고 싶은데 어때?",
          "original_form": "후기 듣고 싶은데 어때?",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025974.1.6",
          "form": "예뻐서 마음에 들어",
          "original_form": "예뻐서 마음에 들어",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025974.1.7",
          "form": "그거 닫으면 움짤도 된다던데... 예쁘겠다. 나도 플립 고민했었는데 아직 쓰는 폰이 괜찮아서 넘기게 됐어",
          "original_form": "그거 닫으면 움짤도 된다던데... 예쁘겠다. 나도 플립 고민했었는데 아직 쓰는 폰이 괜찮아서 넘기게 됐어",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025974.1.8",
          "form": "혹시 사봤던 물건 중에 추천하고 싶은거 있을까?",
          "original_form": "혹시 사봤던 물건 중에 추천하고 싶은거 있을까?",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025974.1.9",
          "form": "나도 뭔가 사볼까 싶어서",
          "original_form": "나도 뭔가 사볼까 싶어서",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025974.1.10",
          "form": "너무 광범위한데",
          "original_form": "너무 광범위한데",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025974.1.11",
          "form": "사봣던 물건 중에 추천할게 잇는지 바로 떠오르는게 없네",
          "original_form": "사봣던 물건 중에 추천할게 잇는지 바로 떠오르는게 없네",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025974.1.12",
          "form": "하지만 사고싶다고 생각하는 물건은 잇어",
          "original_form": "하지만 사고싶다고 생각하는 물건은 잇어",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100025974.1.13",
          "form": "음 그럼.. 좀 범위를 줄일까",
          "original_form": "음 그럼.. 좀 범위를 줄일까",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100025974.1.14",
          "form": "난 최근에 스티커랑 앨범을 구매했어. 폴라로이드 꾸미는데 관심이 있거든.",
          "original_form": "난 최근에 스티커랑 앨범을 구매했어. 폴라로이드 꾸미는데 관심이 있거든.",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100025974.1.15",
          "form": "넌 뭐가 사고 싶었는데?",
          "original_form": "넌 뭐가 사고 싶었는데?",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100025974.1.16",
          "form": "음 스티커랑 앨범류는 잘 사지 않아서 추천을 할 수가 없네",
          "original_form": "음 스티커랑 앨범류는 잘 사지 않아서 추천을 할 수가 없네",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025974.1.17",
          "form": "난 갤워치 사고싶어",
          "original_form": "난 갤워치 사고싶어",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025974.1.18",
          "form": "나도 예전엔 관심 없었는데 갑자기 생겨서... 정신 차려보니까 엄청 쇼핑해버렸어",
          "original_form": "나도 예전엔 관심 없었는데 갑자기 생겨서... 정신 차려보니까 엄청 쇼핑해버렸어",
          "speaker_id": "1",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100025974.1.19",
          "form": "사람 일 알수가 없지 ㅋㅋ",
          "original_form": "사람 일 알수가 없지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100025974.1.20",
          "form": "전자기기에 관심이 많은 편인가봐!",
          "original_form": "전자기기에 관심이 많은 편인가봐!",
          "speaker_id": "1",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100025974.1.21",
          "form": "아니 그런건 아니고 ㅋㅋㅋㅋㅋ",
          "original_form": "아니 그런건 아니고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100025974.1.22",
          "form": "폰은 오래써서 바꿔야 할 타이밍이엇는데 마침 플립이 예뻐서 바꾼거고",
          "original_form": "폰은 오래써서 바꿔야 할 타이밍이엇는데 마침 플립이 예뻐서 바꾼거고",
          "speaker_id": "2",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100025974.1.23",
          "form": "갤워치는 이번에 나온게 생각보다 예쁜데다 인바디가 된다는게 신기해서 사고싶어",
          "original_form": "갤워치는 이번에 나온게 생각보다 예쁜데다 인바디가 된다는게 신기해서 사고싶어",
          "speaker_id": "2",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100025974.1.24",
          "form": "아 그렇구나 전자기기가 많길래 그런 거에 취미가 있나 했어 ㅋㅋㅋㅋ",
          "original_form": "아 그렇구나 전자기기가 많길래 그런 거에 취미가 있나 했어 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100025974.1.25",
          "form": "그러네 인바디가 되면 쓸데가 많겠다.",
          "original_form": "그러네 인바디가 되면 쓸데가 많겠다.",
          "speaker_id": "1",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100025974.1.26",
          "form": "다이어트라거나... 몸건강이라거나...",
          "original_form": "다이어트라거나... 몸건강이라거나...",
          "speaker_id": "1",
          "time": "20210901 18:53"
        }
      ]
    }
