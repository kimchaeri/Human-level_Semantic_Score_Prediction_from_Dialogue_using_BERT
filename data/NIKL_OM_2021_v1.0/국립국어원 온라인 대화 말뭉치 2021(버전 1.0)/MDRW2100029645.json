        {
          "id": "MDRW2100029645.1.1",
          "form": "여럿이서 대화해요",
          "original_form": "여럿이서 대화해요",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100029645.1.2",
          "form": "오늘의 미션좀",
          "original_form": "오늘의 미션좀",
          "speaker_id": "1",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100029645.1.3",
          "form": "하고 올게요",
          "original_form": "하고 올게요",
          "speaker_id": "1",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100029645.1.4",
          "form": "둘이서만 하나요",
          "original_form": "둘이서만 하나요",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100029645.1.5",
          "form": "ㅎㅎ네",
          "original_form": "ㅎㅎ네",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100029645.1.6",
          "form": "조금만기다려주세요",
          "original_form": "조금만기다려주세요",
          "speaker_id": "1",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100029645.1.7",
          "form": "여러명 가능해요",
          "original_form": "여러명 가능해요",
          "speaker_id": "1",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100029645.1.8",
          "form": "둘도되구요",
          "original_form": "둘도되구요",
          "speaker_id": "1",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100029645.1.9",
          "form": "원장격퇴중이에이",
          "original_form": "원장격퇴중이에이",
          "speaker_id": "1",
          "time": "20210903 11:17"
