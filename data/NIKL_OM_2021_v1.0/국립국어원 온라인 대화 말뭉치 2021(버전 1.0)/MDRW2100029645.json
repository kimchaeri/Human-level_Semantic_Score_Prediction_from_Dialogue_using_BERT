          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100029645.1.11",
          "form": "빠르시네요",
          "original_form": "빠르시네요",
          "speaker_id": "2",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100029645.1.12",
          "form": "원장격퇴가 뭐죠",
          "original_form": "원장격퇴가 뭐죠",
          "speaker_id": "2",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029645.1.13",
          "form": "오늘의 미션이에요",
          "original_form": "오늘의 미션이에요",
          "speaker_id": "1",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029645.1.14",
          "form": "원장이 적으로나오고",
          "original_form": "원장이 적으로나오고",
          "speaker_id": "1",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029645.1.15",
          "form": "웃기네요",
          "original_form": "웃기네요",
          "speaker_id": "2",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029645.1.16",
          "form": "룰대로 없애야해요",
          "original_form": "룰대로 없애야해요",
          "speaker_id": "1",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029645.1.17",
          "form": "격퇴하셨나요",
          "original_form": "격퇴하셨나요",
          "speaker_id": "2",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029645.1.18",
          "form": "5번없애야하는데",
          "original_form": "5번없애야하는데",
          "speaker_id": "1",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029645.1.19",
          "form": "난이도가올라가요",
          "original_form": "난이도가올라가요",
          "speaker_id": "1",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029645.1.20",
          "form": "격퇴하고오세요",
          "original_form": "격퇴하고오세요",
