      "utterance": [
        {
          "id": "MDRW2100047214.1.1",
          "form": "책정리 어케하시나요",
          "original_form": "책정리 어케하시나요",
          "speaker_id": "1",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100047214.1.2",
          "form": "음 가족들이랑 같이 살아서",
          "original_form": "음 가족들이랑 같이 살아서",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100047214.1.3",
          "form": "누가 보려고 산 책이냐에 따라",
          "original_form": "누가 보려고 산 책이냐에 따라",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100047214.1.4",
          "form": "책장 영역을 나눠두고...",
          "original_form": "책장 영역을 나눠두고...",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100047214.1.5",
          "form": "책 크기에 맞춰서 순서대로 차곡차곡 넣어둬요 ㅋㅋㅋㅋㅋ",
          "original_form": "책 크기에 맞춰서 순서대로 차곡차곡 넣어둬요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100047214.1.6",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100047214.1.7",
          "form": "책크기 맞아요 깔끔해보여요",
          "original_form": "책크기 맞아요 깔끔해보여요",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100047214.1.8",
          "form": "먼지는요?",
          "original_form": "먼지는요?",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100047214.1.9",
          "form": "어디 넣어둔 책들은 먼지걱정안하는데",
          "original_form": "어디 넣어둔 책들은 먼지걱정안하는데",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100047214.1.10",
          "form": "서랍속책ㅋㅋ",
          "original_form": "서랍속책ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100047214.1.11",
          "form": "신간이겠군요 이미 자리가 없을테니 ㅋㅋ",
          "original_form": "신간이겠군요 이미 자리가 없을테니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100047214.1.12",
          "form": "꺼내져있는 애들은 그냥... 특별히 관리는 안하는거 같아요 보거나 정리하거나 할 때 닦고 ㅋㅋㅋㅋㅋ",
          "original_form": "꺼내져있는 애들은 그냥... 특별히 관리는 안하는거 같아요 보거나 정리하거나 할 때 닦고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100047214.1.13",
          "form": "그춐ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그춐ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100047214.1.14",
          "form": "부모님이 뭐라시나요",
          "original_form": "부모님이 뭐라시나요",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100047214.1.15",
          "form": "이제...그만...사래요...",
          "original_form": "이제...그만...사래요...",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100047214.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100047214.1.17",
          "form": "ㅎㅎ...",
          "original_form": "ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100047214.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100047214.1.19",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㅋ슬퍼요",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㅋ슬퍼요",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100047214.1.20",
          "form": "그래서 이북으로 넘어왔는데",
          "original_form": "그래서 이북으로 넘어왔는데",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100047214.1.21",
          "form": "몰래 뒤에서 버리구계시는지 체크하세요",
          "original_form": "몰래 뒤에서 버리구계시는지 체크하세요",
          "speaker_id": "1",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100047214.1.22",
          "form": "모든 책이 다 이북이 나오진 않아서...",
          "original_form": "모든 책이 다 이북이 나오진 않아서...",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100047214.1.23",
          "form": "아 ㅠㅠ 이사오면서",
          "original_form": "아 ㅠㅠ 이사오면서",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100047214.1.24",
          "form": "시간이 걸리죠",
          "original_form": "시간이 걸리죠",
          "speaker_id": "1",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100047214.1.25",
          "form": "많이 버렸어요...ㄸㄹㄹ...",
          "original_form": "많이 버렸어요...ㄸㄹㄹ...",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100047214.1.26",
          "form": "ㅜㅜ벌써ㅋㅇㅋ",
          "original_form": "ㅜㅜ벌써ㅋㅇㅋ",
          "speaker_id": "1",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100047214.1.27",
          "form": "ㅎㅎ...ㅠㅠㅠㅠ",
          "original_form": "ㅎㅎ...ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100047214.1.28",
          "form": "책장을 좀 효율적인 수납되는 걸로",
          "original_form": "책장을 좀 효율적인 수납되는 걸로",
          "speaker_id": "2",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100047214.1.29",
          "form": "사고 싶더라고요",
          "original_form": "사고 싶더라고요",
          "speaker_id": "2",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100047214.1.30",
          "form": "제친구는 벽하나를 여닫이 책장짰어요",
          "original_form": "제친구는 벽하나를 여닫이 책장짰어요",
          "speaker_id": "1",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100047214.1.31",
          "form": "와 멋지다",
          "original_form": "와 멋지다",
          "speaker_id": "2",
          "time": "20210826 00:13"
        }
      ]
    }
