      "utterance": [
        {
          "id": "MDRW2100043576.1.1",
          "form": "혹시 지렁이같이 생긴거",
          "original_form": "혹시 지렁이같이 생긴거",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043576.1.2",
          "form": "먹어봣어??",
          "original_form": "먹어봣어??",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043576.1.3",
          "form": "이름이 먼데?",
          "original_form": "이름이 먼데?",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043576.1.4",
          "form": "명란젓이라구..",
          "original_form": "명란젓이라구..",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043576.1.5",
          "form": "아악안먹어봣어ㅠㅠㅠ",
          "original_form": "아악안먹어봣어ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043576.1.6",
          "form": "나두...",
          "original_form": "나두...",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043576.1.7",
          "form": "맛없을거같지?",
          "original_form": "맛없을거같지?",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043576.1.8",
          "form": "응...좀..",
          "original_form": "응...좀..",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043576.1.9",
          "form": "짤거같기도 하구",
          "original_form": "짤거같기도 하구",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043576.1.10",
          "form": "맛이 생각못할 맛이야",
          "original_form": "맛이 생각못할 맛이야",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043576.1.11",
          "form": "오잉 먹어봣슈?",
          "original_form": "오잉 먹어봣슈?",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043576.1.12",
          "form": "그정도로 별로일거같애",
          "original_form": "그정도로 별로일거같애",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043576.1.13",
          "form": "그렇군....",
          "original_form": "그렇군....",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043576.1.14",
          "form": "나도 굳이 먹고싶지는않아",
          "original_form": "나도 굳이 먹고싶지는않아",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043576.1.15",
          "form": "그래 먹지말자",
          "original_form": "그래 먹지말자",
