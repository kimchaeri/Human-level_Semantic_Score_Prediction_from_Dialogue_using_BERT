      "utterance": [
        {
          "id": "MDRW2100041032.1.1",
          "form": "오늘 점심엔 뭐 드셨어요??",
          "original_form": "오늘 점심엔 뭐 드셨어요??",
          "speaker_id": "2",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100041032.1.2",
          "form": "아 제주산 곤드레로 만든",
          "original_form": "아 제주산 곤드레로 만든",
          "speaker_id": "1",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100041032.1.3",
          "form": "곤드레밥이요",
          "original_form": "곤드레밥이요",
          "speaker_id": "1",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100041032.1.4",
          "form": "곤드레밥 맛있었겠어요",
          "original_form": "곤드레밥 맛있었겠어요",
          "speaker_id": "2",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100041032.1.5",
          "form": "제주에 살아보니 어떤 점이 좋으세요?ㅋㅋ",
          "original_form": "제주에 살아보니 어떤 점이 좋으세요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:29"
        },
        {
          "id": "MDRW2100041032.1.6",
          "form": "정말정말 궁금해서요 ㅋㅋㅋ",
          "original_form": "정말정말 궁금해서요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:29"
        },
        {
          "id": "MDRW2100041032.1.7",
          "form": "바다 오름 땡기는날 버스타고 갈수있는거랑",
          "original_form": "바다 오름 땡기는날 버스타고 갈수있는거랑",
          "speaker_id": "1",
          "time": "20210905 15:29"
        },
        {
          "id": "MDRW2100041032.1.8",
          "form": "친구들이 우리집에 관관지처럼 놀러온다는거 정도요",
          "original_form": "친구들이 우리집에 관관지처럼 놀러온다는거 정도요",
          "speaker_id": "1",
          "time": "20210905 15:30"
        },
        {
          "id": "MDRW2100041032.1.9",
          "form": "이건 단점일라나 ㅋㅋ",
          "original_form": "이건 단점일라나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:30"
        },
        {
          "id": "MDRW2100041032.1.10",
          "form": "와 진짜 버스타고 바다가고 오름가면 좋네요",
          "original_form": "와 진짜 버스타고 바다가고 오름가면 좋네요",
          "speaker_id": "2",
          "time": "20210905 15:30"
        },
        {
          "id": "MDRW2100041032.1.11",
          "form": "ㅋㅋㅋㅋ 살짝?",
          "original_form": "ㅋㅋㅋㅋ 살짝?",
          "speaker_id": "2",
          "time": "20210905 15:30"
        },
        {
          "id": "MDRW2100041032.1.12",
          "form": "그래도 비행기는 타야되니까 자주는 못오지 않나요?ㅋㅋㅋ",
          "original_form": "그래도 비행기는 타야되니까 자주는 못오지 않나요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:31"
        },
        {
          "id": "MDRW2100041032.1.13",
          "form": "요즘 비행기값이 싸서",
          "original_form": "요즘 비행기값이 싸서",
          "speaker_id": "1",
          "time": "20210905 15:34"
        },
        {
          "id": "MDRW2100041032.1.14",
          "form": "서울에서 내키면 오더라구요",
          "original_form": "서울에서 내키면 오더라구요",
          "speaker_id": "1",
          "time": "20210905 15:35"
        },
        {
          "id": "MDRW2100041032.1.15",
          "form": "이사온지 3주인데",
          "original_form": "이사온지 3주인데",
          "speaker_id": "1",
          "time": "20210905 15:35"
        },
        {
          "id": "MDRW2100041032.1.16",
          "form": "벌써 한명씩 두번 왔다감요^^",
          "original_form": "벌써 한명씩 두번 왔다감요^^",
          "speaker_id": "1",
          "time": "20210905 15:35"
        },
        {
          "id": "MDRW2100041032.1.17",
          "form": "ㅋㅋㅋ 그렇긴 하겠어요",
          "original_form": "ㅋㅋㅋ 그렇긴 하겠어요",
          "speaker_id": "2",
          "time": "20210905 15:38"
        },
        {
          "id": "MDRW2100041032.1.18",
          "form": "그래도 사람들 놀러와서 맛있는거도 같이 먹고 그러면 좋으실거 같아요",
          "original_form": "그래도 사람들 놀러와서 맛있는거도 같이 먹고 그러면 좋으실거 같아요",
          "speaker_id": "2",
          "time": "20210905 15:39"
        },
        {
          "id": "MDRW2100041032.1.19",
          "form": "제주는 8시이후엔 식당을 닫는데",
          "original_form": "제주는 8시이후엔 식당을 닫는데",
          "speaker_id": "1",
          "time": "20210905 16:45"
        },
        {
          "id": "MDRW2100041032.1.20",
          "form": "얼마전에 마라탕 맛없는데 배달시켜서 혼났어요",
          "original_form": "얼마전에 마라탕 맛없는데 배달시켜서 혼났어요",
          "speaker_id": "1",
          "time": "20210905 16:45"
        },
        {
          "id": "MDRW2100041032.1.21",
          "form": "마라탕은 진짜 맛없으면 엄청 맛없어서 ㅋㅋ",
          "original_form": "마라탕은 진짜 맛없으면 엄청 맛없어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100041032.1.22",
          "form": "제주가 한시간 빨리 닫긴 하네요 코로나라 그런게 아니라 원래 그런건가요?",
          "original_form": "제주가 한시간 빨리 닫긴 하네요 코로나라 그런게 아니라 원래 그런건가요?",
          "speaker_id": "2",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100041032.1.23",
          "form": "네 일찍들 닫으셔요",
          "original_form": "네 일찍들 닫으셔요",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100041032.1.24",
          "form": "마라탕은 진라면 스프로 부활시켜 먹었지요",
          "original_form": "마라탕은 진라면 스프로 부활시켜 먹었지요",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100041032.1.25",
          "form": "아!! 마법의 스프",
          "original_form": "아!! 마법의 스프",
          "speaker_id": "2",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100041032.1.26",
          "form": "야식은 좀 불편하겠어요",
          "original_form": "야식은 좀 불편하겠어요",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041032.1.27",
          "form": "네 그냥 안먹는걸로 ㅋㅋ",
          "original_form": "네 그냥 안먹는걸로 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041032.1.28",
          "form": "안먹으면 되니까 건강에도 좋고 ㅋㅋㅋㅋ",
          "original_form": "안먹으면 되니까 건강에도 좋고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041032.1.29",
          "form": "넹ㅋㅋㅋ",
          "original_form": "넹ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041032.1.30",
          "form": "아 최근에 맛초킹 먹었어요 1인분짜리",
          "original_form": "아 최근에 맛초킹 먹었어요 1인분짜리",
          "speaker_id": "1",
          "time": "20210905 23:01"
        },
        {
