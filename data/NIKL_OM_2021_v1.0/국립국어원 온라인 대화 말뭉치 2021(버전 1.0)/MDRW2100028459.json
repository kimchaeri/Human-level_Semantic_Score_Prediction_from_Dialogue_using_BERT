      "utterance": [
        {
          "id": "MDRW2100028459.1.1",
          "form": "저 계란하니까 그거 먹어보고 싶어요",
          "original_form": "저 계란하니까 그거 먹어보고 싶어요",
          "speaker_id": "1",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100028459.1.2",
          "form": "굵은 소금 잔뜩 깔아두고 홈 살짝 파서",
          "original_form": "굵은 소금 잔뜩 깔아두고 홈 살짝 파서",
          "speaker_id": "1",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100028459.1.3",
          "form": "노른자 쏙 넣고 소금으로 덮어서",
          "original_form": "노른자 쏙 넣고 소금으로 덮어서",
          "speaker_id": "1",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100028459.1.4",
          "form": "재워두는...",
          "original_form": "재워두는...",
          "speaker_id": "1",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100028459.1.5",
          "form": "우아 그거는 뭐예요???",
          "original_form": "우아 그거는 뭐예요???",
          "speaker_id": "2",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100028459.1.6",
          "form": "첨들어봤어요 노른자만 넣는건가요",
          "original_form": "첨들어봤어요 노른자만 넣는건가요",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028459.1.7",
          "form": "저도 이거 이름은 모르는데 이렇게 소금에 노른자만 넣고 재워두면",
          "original_form": "저도 이거 이름은 모르는데 이렇게 소금에 노른자만 넣고 재워두면",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028459.1.8",
          "form": "노른자가 소금먹어서 단단해져가지구",
          "original_form": "노른자가 소금먹어서 단단해져가지구",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028459.1.9",
          "form": "요리해서 위에 곁들임용으로 강판에 갈아서 놓더라구요",
          "original_form": "요리해서 위에 곁들임용으로 강판에 갈아서 놓더라구요",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028459.1.10",
          "form": "헐... 신기하다 처음 알았어요",
          "original_form": "헐... 신기하다 처음 알았어요",
          "speaker_id": "2",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028459.1.11",
          "form": "짤 거 같았는데 곁들임용이라면야",
          "original_form": "짤 거 같았는데 곁들임용이라면야",
          "speaker_id": "2",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028459.1.12",
          "form": "맛있겠네... 계란과 닭은 최고의 재료",
          "original_form": "맛있겠네... 계란과 닭은 최고의 재료",
          "speaker_id": "2",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028459.1.13",
          "form": "검색해봤는데",
          "original_form": "검색해봤는데",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028459.1.14",
          "form": "노른자 소금절임 아니면 노른자장 이라고 나오는거 같아요!",
          "original_form": "노른자 소금절임 아니면 노른자장 이라고 나오는거 같아요!",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028459.1.15",
          "form": "집에서 해먹기에는 소금이 너무 아깝지만 레스토랑 같은 곳에서 쓰는 거 같은데 막 무슨 맛일지 궁금해요...",
          "original_form": "집에서 해먹기에는 소금이 너무 아깝지만 레스토랑 같은 곳에서 쓰는 거 같은데 막 무슨 맛일지 궁금해요...",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028459.1.16",
          "form": "진짜루...",
          "original_form": "진짜루...",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028459.1.17",
          "form": "소금 이야기하다보니 생각났는데",
          "original_form": "소금 이야기하다보니 생각났는데",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028459.1.18",
          "form": "name1님은 고기드실때",
          "original_form": "&name1&님은 고기드실때",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028459.1.19",
          "form": "어디에 찍어먹는? 편이신가요",
          "original_form": "어디에 찍어먹는? 편이신가요",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028459.1.20",
          "form": "소금 쌈장 그런거",
          "original_form": "소금 쌈장 그런거",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028459.1.21",
          "form": "저 삼겹살은 쌈장에 먹구",
          "original_form": "저 삼겹살은 쌈장에 먹구",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028459.1.22",
          "form": "소고기는 스테이크 소스요!",
          "original_form": "소고기는 스테이크 소스요!",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028459.1.23",
          "form": "아하~~ 좋앙",
          "original_form": "아하~~ 좋앙",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028459.1.24",
          "form": "저는 소고기는 소금을 젤 좋아하구...",
          "original_form": "저는 소고기는 소금을 젤 좋아하구...",
          "speaker_id": "2",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028459.1.25",
          "form": "삼겹살은 와사비랑 같이 먹으면 맛있어요",
          "original_form": "삼겹살은 와사비랑 같이 먹으면 맛있어요",
          "speaker_id": "2",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028459.1.26",
          "form": "다음에 소고기에 소금찍어먹어볼까봐요",
          "original_form": "다음에 소고기에 소금찍어먹어볼까봐요",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028459.1.27",
          "form": "집에 핑크솔트 받은 게 있어요",
          "original_form": "집에 핑크솔트 받은 게 있어요",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028459.1.28",
          "form": "두근두근... 삼겹살에 와사비는 처음들어봐요",
          "original_form": "두근두근... 삼겹살에 와사비는 처음들어봐요",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028459.1.29",
          "form": "응?? 뭐지",
          "original_form": "응?? 뭐지",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028459.1.30",
          "form": "꺄~~ 좋아요좋아요",
          "original_form": "꺄~~ 좋아요좋아요",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028459.1.31",
          "form": "제가 평소에 짠맛을 좋아해서일지도 모르지만... 소금 조금 찍어먹는게 맛이 젤 좋은거같아서",
          "original_form": "제가 평소에 짠맛을 좋아해서일지도 모르지만... 소금 조금 찍어먹는게 맛이 젤 좋은거같아서",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028459.1.32",
          "form": "와사비랑 명이나물 해서 먹음 맛있어요 ",
          "original_form": "와사비랑 명이나물 해서 먹음 맛있어요 ",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028459.1.33",
          "form": "저도 짭짤한거 되게 좋아하는데 저희집은 그냥 스테이크 소스로 퉁쳐서 그런거같기두...",
          "original_form": "저도 짭짤한거 되게 좋아하는데 저희집은 그냥 스테이크 소스로 퉁쳐서 그런거같기두...",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028459.1.34",
          "form": "그렇다보니 소금보다 소스에 손이 가나봐요",
          "original_form": "그렇다보니 소금보다 소스에 손이 가나봐요",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028459.1.35",
          "form": "아앗!!! 명이나물 제 친구도 좋아하던데",
          "original_form": "아앗!!! 명이나물 제 친구도 좋아하던데",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028459.1.36",
          "form": "저는 나물을 잘 안먹어요 SAD",
          "original_form": "저는 나물을 잘 안먹어요 SAD",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028459.1.37",
          "form": "으아 그렇군요",
          "original_form": "으아 그렇군요",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028459.1.38",
          "form": "저 나물도 좀 절여둔거 자주 먹어가지고 (ㅋㅋㅠ",
          "original_form": "저 나물도 좀 절여둔거 자주 먹어가지고 (ㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028459.1.39",
          "form": "깻잎장아찌나... 막 그런것들이요",
          "original_form": "깻잎장아찌나... 막 그런것들이요",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028459.1.40",
          "form": "아~ name2님 진짜 짭짤한거 엄청 좋아하시는구나",
          "original_form": "아~ &name2&님 진짜 짭짤한거 엄청 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028459.1.41",
          "form": "장 종류 잘드실거같아요 왠지",
          "original_form": "장 종류 잘드실거같아요 왠지",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028459.1.42",
          "form": "장아찌도 결국 '장'이니까",
          "original_form": "장아찌도 결국 '장'이니까",
          "speaker_id": "1",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028459.1.43",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028459.1.44",
          "form": "짠거... 좋아.",
          "original_form": "짠거... 좋아.",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028459.1.45",
          "form": "슴슴한 것도 잘 먹긴 하는데",
          "original_form": "슴슴한 것도 잘 먹긴 하는데",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
