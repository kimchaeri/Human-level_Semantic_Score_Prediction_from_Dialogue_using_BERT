      "utterance": [
        {
          "id": "MDRW2100020900.1.1",
          "form": "책 자주 읽어요?",
          "original_form": "책 자주 읽어요?",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020900.1.2",
          "form": "웹소설만요 ㅎㅎ",
          "original_form": "웹소설만요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020900.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020900.1.4",
          "form": "저는 종종 읽는데 주로 책을 사서 봐요",
          "original_form": "저는 종종 읽는데 주로 책을 사서 봐요",
          "speaker_id": "1",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100020900.1.5",
          "form": "근데 스마트 도서관이 동네에 생겨서 이젠 대출해서 봐요",
          "original_form": "근데 스마트 도서관이 동네에 생겨서 이젠 대출해서 봐요",
          "speaker_id": "1",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100020900.1.6",
          "form": "와 전 도서관간지 오래됐어요",
          "original_form": "와 전 도서관간지 오래됐어요",
          "speaker_id": "2",
          "time": "20210828 14:04"
        },
        {
          "id": "MDRW2100020900.1.7",
          "form": "무슨책 좋아하세요?",
          "original_form": "무슨책 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 14:04"
        },
        {
          "id": "MDRW2100020900.1.8",
          "form": "아무 생각 없이 읽을 수 있는 거요",
          "original_form": "아무 생각 없이 읽을 수 있는 거요",
          "speaker_id": "1",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020900.1.9",
          "form": "그때 그때 다르긴 한데 한 때 시집 많이 봤어요",
          "original_form": "그때 그때 다르긴 한데 한 때 시집 많이 봤어요",
          "speaker_id": "1",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020900.1.10",
          "form": "시집 좋죠 좋아하는 작가님이 있나요?",
          "original_form": "시집 좋죠 좋아하는 작가님이 있나요?",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020900.1.11",
          "form": "하상욱 작가님이요 ㅎㅎ",
          "original_form": "하상욱 작가님이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020900.1.12",
          "form": "되게 시를 재치있게 잘 쓰시는 것 같아요",
          "original_form": "되게 시를 재치있게 잘 쓰시는 것 같아요",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020900.1.13",
          "form": "아 서울시 재밌죠",
          "original_form": "아 서울시 재밌죠",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020900.1.14",
          "form": "서울시요?",
          "original_form": "서울시요?",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020900.1.15",
          "form": "저도 재밌게 봤어요",
          "original_form": "저도 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020900.1.16",
          "form": "ㅋㅋㅋ그래서 시집 두개나 샀어요",
          "original_form": "ㅋㅋㅋ그래서 시집 두개나 샀어요",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020900.1.17",
          "form": "책 이름 서울시 아니었나요?",
          "original_form": "책 이름 서울시 아니었나요?",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020900.1.18",
          "form": "아 책 다양하게 내놓으셨더라구요",
          "original_form": "아 책 다양하게 내놓으셨더라구요",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020900.1.19",
          "form": "아 저는 저것만 알아요 ㅎㅎ",
          "original_form": "아 저는 저것만 알아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:13"
        }
      ]
