      "utterance": [
        {
          "id": "MDRW2100026650.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026650.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026650.1.3",
          "form": "최근 여행은?",
          "original_form": "최근 여행은?",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026650.1.4",
          "form": "저는 코로나로..못 갔습니다",
          "original_form": "저는 코로나로..못 갔습니다",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026650.1.5",
          "form": "저도 못가죠 그냥 근교 캠핑",
          "original_form": "저도 못가죠 그냥 근교 캠핑",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026650.1.6",
          "form": "코로나가 끝나야 할텐데",
          "original_form": "코로나가 끝나야 할텐데",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026650.1.7",
          "form": "요즘은 비도 자주오네요",
          "original_form": "요즘은 비도 자주오네요",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026650.1.8",
          "form": "이제 비 끝 여름끝~바람쇄러 고고",
          "original_form": "이제 비 끝 여름끝~바람쇄러 고고",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026650.1.9",
          "form": "명절에떠나야하나..",
          "original_form": "명절에떠나야하나..",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026650.1.10",
          "form": "아- 명절은 고향",
          "original_form": "아- 명절은 고향",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026650.1.11",
          "form": "캠핑은 불멍이 최고",
          "original_form": "캠핑은 불멍이 최고",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026650.1.12",
          "form": "고향에 살고있습니다",
          "original_form": "고향에 살고있습니다",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026650.1.13",
          "form": "와인과",
          "original_form": "와인과",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026650.1.14",
          "form": "와인은 치즈지요",
          "original_form": "와인은 치즈지요",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026650.1.15",
          "form": "아 전 멀리와서 고향가는게 여행임",
          "original_form": "아 전 멀리와서 고향가는게 여행임",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026650.1.16",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026650.1.17",
          "form": "450km",
          "original_form": "450km",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026650.1.18",
          "form": "요즘은 장거리 이동이 어렵네요",
          "original_form": "요즘은 장거리 이동이 어렵네요",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026650.1.19",
          "form": "차타고 4시간30",
          "original_form": "차타고 4시간30",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026650.1.20",
          "form": "끝과 끝이신가봐요",
          "original_form": "끝과 끝이신가봐요",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026650.1.21",
          "form": "네~거진 그래요 서울로 여행가요",
          "original_form": "네~거진 그래요 서울로 여행가요",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026650.1.22",
          "form": "서울은 놀거리가 다양해서 좋아요",
          "original_form": "서울은 놀거리가 다양해서 좋아요",
          "speaker_id": "1",
          "time": "20210901 23:31"
        }
