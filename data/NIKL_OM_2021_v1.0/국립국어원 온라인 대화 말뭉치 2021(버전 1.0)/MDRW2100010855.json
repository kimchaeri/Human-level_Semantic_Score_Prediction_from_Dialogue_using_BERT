      "utterance": [
        {
          "id": "MDRW2100010855.1.1",
          "form": "중고거래 해보셨어요?",
          "original_form": "중고거래 해보셨어요?",
          "speaker_id": "2",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100010855.1.2",
          "form": "중고거래 한 번도 안해봤어요ㅋㅋ",
          "original_form": "중고거래 한 번도 안해봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100010855.1.3",
          "form": "해보셨나요?",
          "original_form": "해보셨나요?",
          "speaker_id": "1",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100010855.1.4",
          "form": "앗 그렇군요",
          "original_form": "앗 그렇군요",
          "speaker_id": "2",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100010855.1.5",
          "form": "오늘 옷 팔았어요ㅋㅋㅋ",
          "original_form": "오늘 옷 팔았어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100010855.1.6",
          "form": "당근으로 하신건가요ㅋㅋ",
          "original_form": "당근으로 하신건가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100010855.1.7",
          "form": "원래 당근으로 하다가",
          "original_form": "원래 당근으로 하다가",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100010855.1.8",
          "form": "이번에 번개장터로 했어요",
          "original_form": "이번에 번개장터로 했어요",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100010855.1.9",
          "form": "직거래하신거에요?",
          "original_form": "직거래하신거에요?",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100010855.1.10",
          "form": "아뇨! 택배거래했어요~",
          "original_form": "아뇨! 택배거래했어요~",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100010855.1.11",
          "form": "택배거래는 어떻게 하나요? ㅜ",
          "original_form": "택배거래는 어떻게 하나요? ㅜ",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100010855.1.12",
          "form": "박스를 따로 사야하죠?",
          "original_form": "박스를 따로 사야하죠?",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100010855.1.13",
          "form": "우체국택배배송은 해봤는데  일반적인거는 어떻게 하는지 잘 몰라서요ㅜ",
          "original_form": "우체국택배배송은 해봤는데  일반적인거는 어떻게 하는지 잘 몰라서요ㅜ",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100010855.1.14",
          "form": "저도 우체국만 하다가 오늘 편의점 반값택배 처음 해봤어요!",
          "original_form": "저도 우체국만 하다가 오늘 편의점 반값택배 처음 해봤어요!",
          "speaker_id": "2",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100010855.1.15",
          "form": "그냥 택배회사는 저도 안 해봤는데",
          "original_form": "그냥 택배회사는 저도 안 해봤는데",
          "speaker_id": "2",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100010855.1.16",
          "form": "박스는 자기가 준비해야 할거에요",
          "original_form": "박스는 자기가 준비해야 할거에요",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100010855.1.17",
          "form": "그렇군여ㅜ 그 송장은 직접 써야하나요?",
          "original_form": "그렇군여ㅜ 그 송장은 직접 써야하나요?",
          "speaker_id": "1",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100010855.1.18",
          "form": "편의점은 그냥 인쇄해서 나와요",
          "original_form": "편의점은 그냥 인쇄해서 나와요",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100010855.1.19",
          "form": "우체국보다 더 편하더라구요",
          "original_form": "우체국보다 더 편하더라구요",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100010855.1.20",
          "form": "오...나중에 중고거래하게 되면 편의점 이용해야겠어요",
          "original_form": "오...나중에 중고거래하게 되면 편의점 이용해야겠어요",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100010855.1.21",
          "form": "알려주셔서 감사해요!",
          "original_form": "알려주셔서 감사해요!",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100010855.1.22",
          "form": "네 편의점 택배 좋아요! 해보세요ㅎㅎ",
          "original_form": "네 편의점 택배 좋아요! 해보세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100010855.1.23",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
