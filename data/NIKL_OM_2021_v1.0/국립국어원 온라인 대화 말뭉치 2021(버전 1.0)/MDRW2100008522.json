      "utterance": [
        {
          "id": "MDRW2100008522.1.1",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "1",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008522.1.2",
          "form": "저는 올해 졸업했어요!",
          "original_form": "저는 올해 졸업했어요!",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008522.1.3",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008522.1.4",
          "form": "네 학생이에요!",
          "original_form": "네 학생이에요!",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008522.1.5",
          "form": "뭐 동아리같은거 하셨나요?",
          "original_form": "뭐 동아리같은거 하셨나요?",
          "speaker_id": "1",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008522.1.6",
          "form": "저는 제과동아리했어요! 베이킹하는걸 좋아했어서ㅎㅎ",
          "original_form": "저는 제과동아리했어요! 베이킹하는걸 좋아했어서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008522.1.7",
          "form": "동아리드신거 있으세요?",
          "original_form": "동아리드신거 있으세요?",
          "speaker_id": "2",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008522.1.8",
          "form": "우와 멋져요!",
          "original_form": "우와 멋져요!",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008522.1.9",
          "form": "저도 베이킹 배우고싶은데 ㅠㅠ",
          "original_form": "저도 베이킹 배우고싶은데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008522.1.10",
          "form": "지금은 전공동아리만 하고있네요",
          "original_form": "지금은 전공동아리만 하고있네요",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008522.1.11",
          "form": "아 저 내일 수강신청이에요 ㅎ",
          "original_form": "아 저 내일 수강신청이에요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008522.1.12",
          "form": "수강신청 잘되셔야할텐데 ..",
          "original_form": "수강신청 잘되셔야할텐데 ..",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008522.1.13",
          "form": "이번에도 비대면수업인가요?",
          "original_form": "이번에도 비대면수업인가요?",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008522.1.14",
          "form": "저는 원래 대면했는데",
          "original_form": "저는 원래 대면했는데",
          "speaker_id": "1",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008522.1.15",
          "form": "이번애는 어려울것같아요",
          "original_form": "이번애는 어려울것같아요",
          "speaker_id": "1",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008522.1.16",
          "form": "ㅠㅠ 축제도 못하고",
          "original_form": "ㅠㅠ 축제도 못하고",
          "speaker_id": "1",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008522.1.17",
          "form": "서글프네요..",
          "original_form": "서글프네요..",
          "speaker_id": "1",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008522.1.18",
          "form": "그쵸ㅠㅠ 대학축제가 진짜 재밌는데 아쉬워요..",
          "original_form": "그쵸ㅠㅠ 대학축제가 진짜 재밌는데 아쉬워요..",
          "speaker_id": "2",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008522.1.19",
          "form": "비대면하면 장점도 있지만 솔직히 집중하기가 쉽지 않더라구요",
          "original_form": "비대면하면 장점도 있지만 솔직히 집중하기가 쉽지 않더라구요",
          "speaker_id": "2",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008522.1.20",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008522.1.21",
          "form": "저 완전 비대면할때 학점이 ㄷㄷ..",
          "original_form": "저 완전 비대면할때 학점이 ㄷㄷ..",
          "speaker_id": "1",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008522.1.22",
          "form": "충격적이었잖아요",
          "original_form": "충격적이었잖아요",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008522.1.23",
          "form": "과제는 많았었나요?",
          "original_form": "과제는 많았었나요?",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008522.1.24",
          "form": "대면일때는 별로 많지 않았는데 비대면하니 과제가 엄청 많아졌어요..ㅠㅠ",
          "original_form": "대면일때는 별로 많지 않았는데 비대면하니 과제가 엄청 많아졌어요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 17:01"
        },
        {
          "id": "MDRW2100008522.1.25",
          "form": "name1님은 과제 괜찮으신가요?",
          "original_form": "&name1&님은 과제 괜찮으신가요?",
          "speaker_id": "2",
          "time": "20210822 17:01"
        },
        {
          "id": "MDRW2100008522.1.26",
          "form": "어유 ㅠㅠ 팀플 죽겠어요..",
          "original_form": "어유 ㅠㅠ 팀플 죽겠어요..",
          "speaker_id": "1",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008522.1.27",
          "form": "사회의 악..",
          "original_form": "사회의 악..",
          "speaker_id": "1",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008522.1.28",
          "form": "맞아요 팀플하다보면은 스트레스를 너무 받죠..",
          "original_form": "맞아요 팀플하다보면은 스트레스를 너무 받죠..",
          "speaker_id": "2",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008522.1.29",
          "form": "정말 열심히하는 사람만 하는 팀플 ..",
          "original_form": "정말 열심히하는 사람만 하는 팀플 ..",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008522.1.30",
          "form": "그 열심히하는 사람이 저라..",
          "original_form": "그 열심히하는 사람이 저라..",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008522.1.31",
          "form": "항상 고통받네요 ㅠㅠ",
          "original_form": "항상 고통받네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008522.1.32",
          "form": "시험은 알아서 하면되는대..",
          "original_form": "시험은 알아서 하면되는대..",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008522.1.33",
          "form": "그쵸 .. 저도 항상 의견 먼저내고 빨리끝내야되는 성격이라 너무 공감가요...",
          "original_form": "그쵸 .. 저도 항상 의견 먼저내고 빨리끝내야되는 성격이라 너무 공감가요...",
          "speaker_id": "2",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100008522.1.34",
          "form": "팀플말고도 다른과제도 있는데 그럴땐 스트레스 받죠",
          "original_form": "팀플말고도 다른과제도 있는데 그럴땐 스트레스 받죠",
          "speaker_id": "2",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100008522.1.35",
          "form": "맞아요 ㅠㅠㅠ",
          "original_form": "맞아요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008522.1.36",
          "form": "카톡 안읽을때가 제일 열받는 ㅠㅠㅠ",
          "original_form": "카톡 안읽을때가 제일 열받는 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008522.1.37",
          "form": "나는 이거 끝내고 다른거 해여하는대..",
          "original_form": "나는 이거 끝내고 다른거 해여하는대..",
