      "utterance": [
        {
          "id": "MDRW2100007341.1.1",
          "form": "tvn 초창기 드라마예요",
          "original_form": "tvn 초창기 드라마예요",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007341.1.2",
          "form": "보기 힘들수도 있어요",
          "original_form": "보기 힘들수도 있어요",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007341.1.3",
          "form": "그럼 티빙에 있을수도 !",
          "original_form": "그럼 티빙에 있을수도 !",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007341.1.4",
          "form": "저 잼나게 봤어요",
          "original_form": "저 잼나게 봤어요",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007341.1.5",
          "form": "한번 봐보세요",
          "original_form": "한번 봐보세요",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007341.1.6",
          "form": "스릴러도 좋지만",
          "original_form": "스릴러도 좋지만",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007341.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007341.1.8",
          "form": "범죄 수사 이런것도 좋더라구요",
          "original_form": "범죄 수사 이런것도 좋더라구요",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007341.1.9",
          "form": "요것도ㅠ살인자 찾기요",
          "original_form": "요것도ㅠ살인자 찾기요",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007341.1.10",
          "form": "아아아 !!그럼 딱 제 스타일인데 왜 못봤나 모르겠네요",
          "original_form": "아아아 !!그럼 딱 제 스타일인데 왜 못봤나 모르겠네요",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007341.1.11",
          "form": "예전에 별순검... 이런것도 많이 봤는데",
          "original_form": "예전에 별순검... 이런것도 많이 봤는데",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007341.1.12",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007341.1.13",
          "form": "넷플 에얼리어니스트 보셨어요?",
          "original_form": "넷플 에얼리어니스트 보셨어요?",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007341.1.14",
          "form": "의외의 인물이 범인일때도 있죠 ㅎㅎ",
          "original_form": "의외의 인물이 범인일때도 있죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007341.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007341.1.16",
          "form": "요건 추리인데 좀 잔인하고 무거워요",
          "original_form": "요건 추리인데 좀 잔인하고 무거워요",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007341.1.17",
          "form": "근데 전 넘 재밌게 봤어요",
          "original_form": "근데 전 넘 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007341.1.18",
          "form": "프로파일링 기법",
          "original_form": "프로파일링 기법",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007341.1.19",
          "form": "보통 영상보고 들으면서 딴것도 병행하면서 하거든요. 그래서 해외드라마는 자막을 봐야하지 딱 !! 집중해야해서 잘 못봤어요",
          "original_form": "보통 영상보고 들으면서 딴것도 병행하면서 하거든요. 그래서 해외드라마는 자막을 봐야하지 딱 !! 집중해야해서 잘 못봤어요",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007341.1.20",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007341.1.21",
          "form": "아 요건 집중해서 한번 봐봐야겠네요 ! 넷플에서 살짝 보니 재미있을 것 같애요",
          "original_form": "아 요건 집중해서 한번 봐봐야겠네요 ! 넷플에서 살짝 보니 재미있을 것 같애요",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007341.1.22",
          "form": "요 마이 시크릿은 초창기 드라마라 잘모르는 사람이 많아요",
          "original_form": "요 마이 시크릿은 초창기 드라마라 잘모르는 사람이 많아요",
          "speaker_id": "2",
          "time": "20210822 01:35"
        }
      ]
    }
