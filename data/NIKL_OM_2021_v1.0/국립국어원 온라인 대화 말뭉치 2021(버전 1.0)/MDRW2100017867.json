      "utterance": [
        {
          "id": "MDRW2100017867.1.1",
          "form": "내일부터 운동하자구",
          "original_form": "내일부터 운동하자구",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017867.1.2",
          "form": "워치오면 할게요",
          "original_form": "워치오면 할게요",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017867.1.3",
          "form": "런데이?",
          "original_form": "런데이?",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017867.1.4",
          "form": "뭐든 하면 좋지 전 런데이 추천해요",
          "original_form": "뭐든 하면 좋지 전 런데이 추천해요",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017867.1.5",
          "form": "어떠신가요",
          "original_form": "어떠신가요",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017867.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017867.1.7",
          "form": "근데 워치 런데이 안되더라ㅠ",
          "original_form": "근데 워치 런데이 안되더라ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017867.1.8",
          "form": "걍 걷기라도 해야겠어",
          "original_form": "걍 걷기라도 해야겠어",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017867.1.9",
          "form": "요새 무릎아파ㅠ",
          "original_form": "요새 무릎아파ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017867.1.10",
          "form": "핸드폰 들고 하시라구요...",
          "original_form": "핸드폰 들고 하시라구요...",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017867.1.11",
          "form": "아니 그럼.. 무릎보호대도 사야해...(물욕)",
          "original_form": "아니 그럼.. 무릎보호대도 사야해...(물욕)",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017867.1.12",
          "form": "안돼..ㅠ",
          "original_form": "안돼..ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017867.1.13",
          "form": "아 근데 크록스 계속 신으니까",
          "original_form": "아 근데 크록스 계속 신으니까",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017867.1.14",
          "form": "무릎지켜야한다고...!!ㅠ",
          "original_form": "무릎지켜야한다고...!!ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017867.1.15",
          "form": "무릎에 안좋은가봐",
          "original_form": "무릎에 안좋은가봐",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017867.1.16",
          "form": "아 그치 크록스 신으면 걸음이 살짝 이상해져 나도 그래",
          "original_form": "아 그치 크록스 신으면 걸음이 살짝 이상해져 나도 그래",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017867.1.17",
          "form": "잘 걷는거 너무 중요ㅠ",
          "original_form": "잘 걷는거 너무 중요ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017867.1.18",
          "form": "그니까ㅠㅠ",
          "original_form": "그니까ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017867.1.19",
          "form": "무릎아픈적 한번도 없었는데",
          "original_form": "무릎아픈적 한번도 없었는데",
          "speaker_id": "2",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017867.1.20",
          "form": "괜히무서워",
          "original_form": "괜히무서워",
          "speaker_id": "2",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017867.1.21",
          "form": "ㅠㅠ안돼..어디든 아프기 시잗하면 조심해야돼...쿠록스 그냥 실내화로 신어야하나??ㅠ",
          "original_form": "ㅠㅠ안돼..어디든 아프기 시잗하면 조심해야돼...쿠록스 그냥 실내화로 신어야하나??ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017867.1.22",
          "form": "편해서 자주 신었는데ㅠ",
          "original_form": "편해서 자주 신었는데ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017867.1.23",
          "form": "운동화가 최곤가봐",
          "original_form": "운동화가 최곤가봐",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017867.1.24",
          "form": "맞아 그런가봐",
          "original_form": "맞아 그런가봐",
          "speaker_id": "1",
          "time": "20210826 21:42"
        }
      ]
    }
