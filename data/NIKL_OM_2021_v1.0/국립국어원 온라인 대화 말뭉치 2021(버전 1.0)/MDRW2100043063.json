      "utterance": [
        {
          "id": "MDRW2100043063.1.1",
          "form": "고기팬은 너무튀고ㅠㅠ 애들이 있으니ㅠ",
          "original_form": "고기팬은 너무튀고ㅠㅠ 애들이 있으니ㅠ",
          "speaker_id": "2",
          "time": "20210907 13:44"
        },
        {
          "id": "MDRW2100043063.1.2",
          "form": "쳐박템 됏어요",
          "original_form": "쳐박템 됏어요",
          "speaker_id": "1",
          "time": "20210907 13:44"
        },
        {
          "id": "MDRW2100043063.1.3",
          "form": "자리차지도많이하궁",
          "original_form": "자리차지도많이하궁",
          "speaker_id": "1",
          "time": "20210907 13:44"
        },
        {
          "id": "MDRW2100043063.1.4",
          "form": ".요새는 오븐식으로 된거 나오잖아여",
          "original_form": ".요새는 오븐식으로 된거 나오잖아여",
          "speaker_id": "2",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100043063.1.5",
          "form": "거기에 구우면 맛은 있는데..... 기름이 너무 텨서...",
          "original_form": "거기에 구우면 맛은 있는데..... 기름이 너무 텨서...",
          "speaker_id": "2",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100043063.1.6",
          "form": "안에 닦는게 또 ㅠㅠㅠ",
          "original_form": "안에 닦는게 또 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100043063.1.7",
          "form": "ㅋㅋㅋ귀찮아유",
          "original_form": "ㅋㅋㅋ귀찮아유",
          "speaker_id": "1",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100043063.1.8",
          "form": "그래도 맛잇으니깐 ㅎㅎ",
          "original_form": "그래도 맛잇으니깐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100043063.1.9",
          "form": "고기는사랑이니깐 ㅎㅎ",
          "original_form": "고기는사랑이니깐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100043063.1.10",
          "form": "고기는 근데 후라이팬에 구워야 맛나요 바삭바삭",
          "original_form": "고기는 근데 후라이팬에 구워야 맛나요 바삭바삭",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043063.1.11",
          "form": "맞아유 ㅋㅋㅋㅋ",
          "original_form": "맞아유 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043063.1.12",
          "form": "기름튀는건 감수해야져ㅠㅠㅠㅠ 맛있는걸 먹기위해ㅠ",
          "original_form": "기름튀는건 감수해야져ㅠㅠㅠㅠ 맛있는걸 먹기위해ㅠ",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043063.1.13",
          "form": "ㅋㅋㅋㅋ아 저는 배민으로라도 시켜먹어야하나싶네요",
          "original_form": "ㅋㅋㅋㅋ아 저는 배민으로라도 시켜먹어야하나싶네요",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043063.1.14",
          "form": "고기이야기하니깐",
          "original_form": "고기이야기하니깐",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043063.1.15",
          "form": "넘 맛나겟어요",
          "original_form": "넘 맛나겟어요",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043063.1.16",
          "form": "다이어트!!!!!!!해야하는뎅ㅠㅠ",
          "original_form": "다이어트!!!!!!!해야하는뎅ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043063.1.17",
          "form": "다이어트는 내일부터",
          "original_form": "다이어트는 내일부터",
          "speaker_id": "2",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043063.1.18",
          "form": "자 고기를 시키십시오 ㅋㅋㅋ",
          "original_form": "자 고기를 시키십시오 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043063.1.19",
          "form": "맞슴다ㅜㅜ",
          "original_form": "맞슴다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043063.1.20",
          "form": "요즘 펩시제로에 꽂혀서",
          "original_form": "요즘 펩시제로에 꽂혀서",
          "speaker_id": "1",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043063.1.21",
          "form": "엄청 마시구잇어요 집에서",
          "original_form": "엄청 마시구잇어요 집에서",
          "speaker_id": "1",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043063.1.22",
          "form": "제로요??",
          "original_form": "제로요??",
          "speaker_id": "2",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100043063.1.23",
          "form": "다이어트 하시는거 맞네요 ㅎㅎㅎ",
          "original_form": "다이어트 하시는거 맞네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100043063.1.24",
          "form": "네네ㅋㅋ 맛잇더라구용",
          "original_form": "네네ㅋㅋ 맛잇더라구용",
          "speaker_id": "1",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100043063.1.25",
          "form": "저는 제로는 안맞아여ㅠㅠㅠ 단맛이 없잔아여ㅠㅠㅠ",
          "original_form": "저는 제로는 안맞아여ㅠㅠㅠ 단맛이 없잔아여ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 13:51"
        },
        {
          "id": "MDRW2100043063.1.26",
          "form": "펩시 제로 한번만 시도해보세요",
          "original_form": "펩시 제로 한번만 시도해보세요",
          "speaker_id": "1",
          "time": "20210907 13:52"
        },
        {
          "id": "MDRW2100043063.1.27",
          "form": "강추강추",
