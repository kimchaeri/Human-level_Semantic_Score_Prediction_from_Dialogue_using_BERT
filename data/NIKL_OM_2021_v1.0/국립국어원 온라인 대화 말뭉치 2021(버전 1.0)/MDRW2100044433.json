      "utterance": [
        {
          "id": "MDRW2100044433.1.1",
          "form": "요즘에는 소개팅 안해",
          "original_form": "요즘에는 소개팅 안해",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044433.1.2",
          "form": "요즘에는 일이 바빠서 할 시간이 없네",
          "original_form": "요즘에는 일이 바빠서 할 시간이 없네",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044433.1.3",
          "form": "누가 소개시켜준다는 사람도 없어",
          "original_form": "누가 소개시켜준다는 사람도 없어",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044433.1.4",
          "form": "있긴 있는데 들어보면 맘이 영",
          "original_form": "있긴 있는데 들어보면 맘이 영",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044433.1.5",
          "form": "그래도 한번은  만나보는 거 어때",
          "original_form": "그래도 한번은  만나보는 거 어때",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044433.1.6",
          "form": "귀찮아서",
          "original_form": "귀찮아서",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044433.1.7",
          "form": "소개시켜준다고 할 때 한번 만나봐",
          "original_form": "소개시켜준다고 할 때 한번 만나봐",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044433.1.8",
          "form": "맘에 들면 은 만날 거에요",
          "original_form": "맘에 들면 은 만날 거에요",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044433.1.9",
          "form": "어떤 남자가 좋은데",
          "original_form": "어떤 남자가 좋은데",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044433.1.10",
          "form": "지금은 그냥 별생각 없어요",
          "original_form": "지금은 그냥 별생각 없어요",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044433.1.11",
          "form": "우리 딸은 독립심이 강하구나",
          "original_form": "우리 딸은 독립심이 강하구나",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044433.1.12",
          "form": "그런 거 같애 혼자서도 잘 살 것 같애",
          "original_form": "그런 거 같애 혼자서도 잘 살 것 같애",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044433.1.13",
          "form": "응 기특하네",
          "original_form": "응 기특하네",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044433.1.14",
          "form": "이러다가 언젠간 누구를 만나겠지",
          "original_form": "이러다가 언젠간 누구를 만나겠지",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044433.1.15",
          "form": "맞아 인연이라는 게 다 때가 있더라",
          "original_form": "맞아 인연이라는 게 다 때가 있더라",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044433.1.16",
          "form": "나는 그때가 아직 안 왔나 봐요",
          "original_form": "나는 그때가 아직 안 왔나 봐요",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044433.1.17",
          "form": "그런 걸 수도 있지",
          "original_form": "그런 걸 수도 있지",
          "speaker_id": "2",
          "time": "20210907 22:42"
        }
      ]
