      "utterance": [
        {
          "id": "MDRW2100007706.1.1",
          "form": "어떤 계절 좋아하시나요",
          "original_form": "어떤 계절 좋아하시나요",
          "speaker_id": "1",
          "time": "20210822 09:49"
        },
        {
          "id": "MDRW2100007706.1.2",
          "form": "가을날씨 좋아요",
          "original_form": "가을날씨 좋아요",
          "speaker_id": "2",
          "time": "20210822 09:49"
        },
        {
          "id": "MDRW2100007706.1.3",
          "form": "저도요 여름은 싫어해요",
          "original_form": "저도요 여름은 싫어해요",
          "speaker_id": "1",
          "time": "20210822 09:49"
        },
        {
          "id": "MDRW2100007706.1.4",
          "form": "올해는 벌써 반이 지나갔네요",
          "original_form": "올해는 벌써 반이 지나갔네요",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007706.1.5",
          "form": "맞아요 일기예보 자주 보시나요",
          "original_form": "맞아요 일기예보 자주 보시나요",
          "speaker_id": "1",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007706.1.6",
          "form": "아뇨 ㅋ 나갈일이 없어서요",
          "original_form": "아뇨 ㅋ 나갈일이 없어서요",
          "speaker_id": "2",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007706.1.7",
          "form": "어떤 날씨 좋아하세요?",
          "original_form": "어떤 날씨 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 09:56"
        },
        {
          "id": "MDRW2100007706.1.8",
          "form": "맑은 날이요",
          "original_form": "맑은 날이요",
          "speaker_id": "2",
          "time": "20210822 09:59"
        },
        {
          "id": "MDRW2100007706.1.9",
          "form": "비오고 갠뒤도 좋아요",
          "original_form": "비오고 갠뒤도 좋아요",
          "speaker_id": "2",
          "time": "20210822 09:59"
        },
        {
          "id": "MDRW2100007706.1.10",
          "form": "어떤 계절 싫어하세요?",
          "original_form": "어떤 계절 싫어하세요?",
          "speaker_id": "1",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007706.1.11",
          "form": "겨울이요 추위를많이타서요",
          "original_form": "겨울이요 추위를많이타서요",
          "speaker_id": "2",
          "time": "20210822 10:42"
        },
        {
          "id": "MDRW2100007706.1.12",
          "form": "당신은요?",
          "original_form": "당신은요?",
          "speaker_id": "2",
          "time": "20210822 10:42"
        },
        {
          "id": "MDRW2100007706.1.13",
          "form": "전 여름이요 더운거 싫어해서요 미세먼지 신경쓰시나요?",
          "original_form": "전 여름이요 더운거 싫어해서요 미세먼지 신경쓰시나요?",
          "speaker_id": "1",
          "time": "20210822 11:12"
        },
        {
          "id": "MDRW2100007706.1.14",
          "form": "네 어플로도 매일봐요",
          "original_form": "네 어플로도 매일봐요",
          "speaker_id": "2",
          "time": "20210822 11:34"
        },
        {
          "id": "MDRW2100007706.1.15",
          "form": "저도요 근데 요새는 미세먼지가 좀 괜찮아진거 같기도 해요",
          "original_form": "저도요 근데 요새는 미세먼지가 좀 괜찮아진거 같기도 해요",
          "speaker_id": "1",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007706.1.16",
          "form": "가을이네요",
          "original_form": "가을이네요",
