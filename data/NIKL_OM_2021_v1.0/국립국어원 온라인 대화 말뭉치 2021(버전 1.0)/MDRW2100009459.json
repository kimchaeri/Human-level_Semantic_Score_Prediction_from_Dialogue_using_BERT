      "utterance": [
        {
          "id": "MDRW2100009459.1.1",
          "form": "새터민분들이 운동회 많이 오셨다던데",
          "original_form": "새터민분들이 운동회 많이 오셨다던데",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009459.1.2",
          "form": "다들 남한분들하고 똑같이 생겨서",
          "original_form": "다들 남한분들하고 똑같이 생겨서",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009459.1.3",
          "form": "많이 왔는지 안왔는지 분간이 안가더래요ㅎㅎ",
          "original_form": "많이 왔는지 안왔는지 분간이 안가더래요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009459.1.4",
          "form": "ㅎㅎ 구분이 가지않죠",
          "original_form": "ㅎㅎ 구분이 가지않죠",
          "speaker_id": "2",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009459.1.5",
          "form": "잘 적응해서 사시는거 같더라구요",
          "original_form": "잘 적응해서 사시는거 같더라구요",
          "speaker_id": "1",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009459.1.6",
          "form": "김밥집갔는데 새터민아주머니셔서",
          "original_form": "김밥집갔는데 새터민아주머니셔서",
          "speaker_id": "1",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009459.1.7",
          "form": "맘속으로 아 이북말투네 일케생각하고있었는데",
          "original_form": "맘속으로 아 이북말투네 일케생각하고있었는데",
          "speaker_id": "1",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009459.1.8",
          "form": "친구가 옆에서 북한사람이세요? 물어서 갑분싸되었었어요",
          "original_form": "친구가 옆에서 북한사람이세요? 물어서 갑분싸되었었어요",
          "speaker_id": "1",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009459.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009459.1.10",
          "form": "잘안드러내시더라구요",
          "original_form": "잘안드러내시더라구요",
          "speaker_id": "1",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009459.1.11",
          "form": "헉 친구분",
          "original_form": "헉 친구분",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009459.1.12",
          "form": "눈치가 ㅋㅋ",
          "original_form": "눈치가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009459.1.13",
          "form": "이북에 가족이 남아있거나 하면 알려지면 위험하거든요",
          "original_form": "이북에 가족이 남아있거나 하면 알려지면 위험하거든요",
          "speaker_id": "1",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009459.1.14",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009459.1.15",
          "form": "글게요 친구한테 언질했어야했는데ㅋㅋㅋ",
          "original_form": "글게요 친구한테 언질했어야했는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009459.1.16",
          "form": "계신지몰라서",
          "original_form": "계신지몰라서",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009459.1.17",
          "form": "그러게요 근데 저라도 그럴줄 예상못하죠 ㅋㅋ",
          "original_form": "그러게요 근데 저라도 그럴줄 예상못하죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009459.1.18",
          "form": "갑분싸되버렸었네요 담부턴 조심하겠지요",
          "original_form": "갑분싸되버렸었네요 담부턴 조심하겠지요",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009459.1.19",
          "form": "그래야죠",
          "original_form": "그래야죠",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009459.1.20",
          "form": "어렵게 직장 다니실텐데",
          "original_form": "어렵게 직장 다니실텐데",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009459.1.21",
          "form": "북한분들 도움주는 일하세요?",
          "original_form": "북한분들 도움주는 일하세요?",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009459.1.22",
          "form": "유투브보니까 영화엑스트라일도하시더라구요",
          "original_form": "유투브보니까 영화엑스트라일도하시더라구요",
          "speaker_id": "1",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009459.1.23",
          "form": "북한사람배역ㅋㅋ",
          "original_form": "북한사람배역ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009459.1.24",
          "form": "아 저말고 제 주위분이 북한지원 통일지원쪽 일하세요",
          "original_form": "아 저말고 제 주위분이 북한지원 통일지원쪽 일하세요",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009459.1.25",
          "form": "그래서 늘 관심이 있었어요 ",
          "original_form": "그래서 늘 관심이 있었어요 ",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009459.1.26",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210822 23:42"
        }
      ]
    }
