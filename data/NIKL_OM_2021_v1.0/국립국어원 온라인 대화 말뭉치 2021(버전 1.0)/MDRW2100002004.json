      "utterance": [
        {
          "id": "MDRW2100002004.1.1",
          "form": "배달해요?",
          "original_form": "배달해요?",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100002004.1.2",
          "form": "배달이요?",
          "original_form": "배달이요?",
          "speaker_id": "2",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100002004.1.3",
          "form": "무슨 배달요?",
          "original_form": "무슨 배달요?",
          "speaker_id": "2",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100002004.1.4",
          "form": "배달음식이여",
          "original_form": "배달음식이여",
          "speaker_id": "1",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100002004.1.5",
          "form": "오늘도 두번이나 시켜먹었어요",
          "original_form": "오늘도 두번이나 시켜먹었어요",
          "speaker_id": "2",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100002004.1.6",
          "form": "크로플이랑",
          "original_form": "크로플이랑",
          "speaker_id": "2",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100002004.1.7",
          "form": "저녁엔 쌀국수....",
          "original_form": "저녁엔 쌀국수....",
          "speaker_id": "2",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100002004.1.8",
          "form": "전 치킨이요",
          "original_form": "전 치킨이요",
          "speaker_id": "1",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100002004.1.9",
          "form": "배달 자주 시켜드세요?",
          "original_form": "배달 자주 시켜드세요?",
          "speaker_id": "2",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100002004.1.10",
          "form": "네 일주일에 1번 정도요",
          "original_form": "네 일주일에 1번 정도요",
          "speaker_id": "1",
          "time": "20210819 22:53"
        },
        {
          "id": "MDRW2100002004.1.11",
          "form": "비슷하네요 전 많으면 3번?",
          "original_form": "비슷하네요 전 많으면 3번?",
          "speaker_id": "2",
          "time": "20210819 22:53"
        },
        {
          "id": "MDRW2100002004.1.12",
          "form": "특히 주말에",
          "original_form": "특히 주말에",
          "speaker_id": "2",
          "time": "20210819 22:53"
        },
        {
          "id": "MDRW2100002004.1.13",
          "form": "코로나땜에 외식하기가 좀 그래서 더 시켜먹는듯해요",
          "original_form": "코로나땜에 외식하기가 좀 그래서 더 시켜먹는듯해요",
          "speaker_id": "2",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100002004.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100002004.1.15",
          "form": "요즘 배민에 배달 다 해주니까",
          "original_form": "요즘 배민에 배달 다 해주니까",
          "speaker_id": "2",
          "time": "20210819 23:03"
        },
        {
          "id": "MDRW2100002004.1.16",
          "form": "고기도 다 구워다 가져다 주니",
          "original_form": "고기도 다 구워다 가져다 주니",
          "speaker_id": "2",
          "time": "20210819 23:03"
        },
        {
          "id": "MDRW2100002004.1.17",
          "form": "그냥 시켜먹게 되더라구요",
          "original_form": "그냥 시켜먹게 되더라구요",
          "speaker_id": "2",
          "time": "20210819 23:03"
        },
        {
          "id": "MDRW2100002004.1.18",
          "form": "곱창같은것도 구워다 주긴 하던데 가서 구워먹는거보단 별로겠죠?",
          "original_form": "곱창같은것도 구워다 주긴 하던데 가서 구워먹는거보단 별로겠죠?",
          "speaker_id": "2",
          "time": "20210819 23:03"
        },
        {
          "id": "MDRW2100002004.1.19",
          "form": "아무래도 조금 식으니",
          "original_form": "아무래도 조금 식으니",
          "speaker_id": "1",
          "time": "20210819 23:04"
        },
        {
          "id": "MDRW2100002004.1.20",
          "form": "굽자마자 호호 불어서 먹어야 맛있는데 ㅋㅋㅋ",
          "original_form": "굽자마자 호호 불어서 먹어야 맛있는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100002004.1.21",
          "form": "먹는얘기 하니까 조금 배고픈 느낌..",
          "original_form": "먹는얘기 하니까 조금 배고픈 느낌..",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100002004.1.22",
          "form": "저도요 야식이 ㅋㅋ",
          "original_form": "저도요 야식이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100002004.1.23",
          "form": "졸린데 배고픈...ㅋㅋ",
          "original_form": "졸린데 배고픈...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:10"
        },
        {
          "id": "MDRW2100002004.1.24",
          "form": "ㅋㅋ 맞아요",
          "original_form": "ㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210819 23:11"
        }
      ]
