      "utterance": [
        {
          "id": "MDRW2100034516.1.1",
          "form": "점심 드셨어뇨?",
          "original_form": "점심 드셨어뇨?",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034516.1.2",
          "form": "네 칼국수 먹었어요. 님은요?",
          "original_form": "네 칼국수 먹었어요. 님은요?",
          "speaker_id": "1",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100034516.1.3",
          "form": "저는 파스타!",
          "original_form": "저는 파스타!",
          "speaker_id": "2",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100034516.1.4",
          "form": "레몬콤부차로 마무리하고 있어요",
          "original_form": "레몬콤부차로 마무리하고 있어요",
          "speaker_id": "2",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100034516.1.5",
          "form": "맛있겠네요. 콤부차는 어떤 맛인가요?",
          "original_form": "맛있겠네요. 콤부차는 어떤 맛인가요?",
          "speaker_id": "1",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100034516.1.6",
          "form": "콤부가 다시마라고 하더라구요?",
          "original_form": "콤부가 다시마라고 하더라구요?",
          "speaker_id": "2",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100034516.1.7",
          "form": "근데 다시마 맛은 안나고 그냥 레몬맛만 나요",
          "original_form": "근데 다시마 맛은 안나고 그냥 레몬맛만 나요",
          "speaker_id": "2",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100034516.1.8",
          "form": "다시마라니ㅋㅋ 몰랐어요. 몸에 좋다고 하더라구요",
          "original_form": "다시마라니ㅋㅋ 몰랐어요. 몸에 좋다고 하더라구요",
          "speaker_id": "1",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100034516.1.9",
          "form": "방탄소년단 정국이 좋다고 언급해서 엄청 품절이더라구요",
          "original_form": "방탄소년단 정국이 좋다고 언급해서 엄청 품절이더라구요",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100034516.1.10",
          "form": "아 얘기 들어봤어요 ㅋㅋ 차 종류 좋아하세요?",
          "original_form": "아 얘기 들어봤어요 ㅋㅋ 차 종류 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100034516.1.11",
          "form": "이왕이면 차를 마시려고 노력하는데",
          "original_form": "이왕이면 차를 마시려고 노력하는데",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100034516.1.12",
          "form": "결국은 커피를 마시게되어여ㅋㅋㅋ",
          "original_form": "결국은 커피를 마시게되어여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100034516.1.13",
          "form": "ㅋㅋ 전 가끔 밀크티 해 마시는데 유튜브에 어떤 카페 사장님이 올리신 레시피대로 하니까 진짜 맛있더라구요",
          "original_form": "ㅋㅋ 전 가끔 밀크티 해 마시는데 유튜브에 어떤 카페 사장님이 올리신 레시피대로 하니까 진짜 맛있더라구요",
          "speaker_id": "1",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100034516.1.14",
          "form": "오ㅋㅋㅋ 홍차에 우유넣는 거죠?",
          "original_form": "오ㅋㅋㅋ 홍차에 우유넣는 거죠?",
          "speaker_id": "2",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100034516.1.15",
          "form": "네 근데 설탕도 상당히 들어가야 맛있더라구요ㅋㅋ",
          "original_form": "네 근데 설탕도 상당히 들어가야 맛있더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100034516.1.16",
          "form": "오호 그렇구나 저희 직장 부서에 늘 밀크티 해서 드시는 분 계신데",
          "original_form": "오호 그렇구나 저희 직장 부서에 늘 밀크티 해서 드시는 분 계신데",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100034516.1.17",
          "form": "찻잔도 엄청 고급스러운거 다 갖춰놓고 ㅋㅋㅋ",
          "original_form": "찻잔도 엄청 고급스러운거 다 갖춰놓고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100034516.1.18",
          "form": "진짜 영국 귀족처럼 드시거든여ㅋㅋ",
          "original_form": "진짜 영국 귀족처럼 드시거든여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100034516.1.19",
          "form": "ㅋㅋ 전 그 정도는 아니고 가끔 맛으로만 먹어요",
          "original_form": "ㅋㅋ 전 그 정도는 아니고 가끔 맛으로만 먹어요",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100034516.1.20",
          "form": "맞아여 밀크티 맛있어요ㅋㅋㅋ",
          "original_form": "맞아여 밀크티 맛있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100034516.1.21",
          "form": "커피도 엄청 묽게 차 느낌으러 마시는 걸 좋아해요",
          "original_form": "커피도 엄청 묽게 차 느낌으러 마시는 걸 좋아해요",
          "speaker_id": "2",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100034516.1.22",
          "form": "전 커피는 안 마셔서 카페 가면 다른 음료만 마셔요 ㅋㅋ",
          "original_form": "전 커피는 안 마셔서 카페 가면 다른 음료만 마셔요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:31"
        }
      ]
    }
