      "utterance": [
        {
          "id": "MDRW2100007188.1.1",
          "form": "사망맛 부상맛 이런식인데",
          "original_form": "사망맛 부상맛 이런식인데",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007188.1.2",
          "form": "어르신들은 음식에 그런거붙인다구",
          "original_form": "어르신들은 음식에 그런거붙인다구",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007188.1.3",
          "form": "악 그경사,,,ㅇㅋㅋㅋ",
          "original_form": "악 그경사,,,ㅇㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007188.1.4",
          "form": "싫어하더라구요ㅠ",
          "original_form": "싫어하더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007188.1.5",
          "form": "이상하긴 하네요 ㅋㅋㅋ",
          "original_form": "이상하긴 하네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007188.1.6",
          "form": "응급실에는 맞는데",
          "original_form": "응급실에는 맞는데",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007188.1.7",
          "form": "대구어르신들의 냉정함...",
          "original_form": "대구어르신들의 냉정함...",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007188.1.8",
          "form": "사망맛은 진짜 저도 안먹어봤네요",
          "original_form": "사망맛은 진짜 저도 안먹어봤네요",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007188.1.9",
          "form": "으르신..ㅋㅋㅋㅋ",
          "original_form": "으르신..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007188.1.10",
          "form": "먹다가죽을까봐...",
          "original_form": "먹다가죽을까봐...",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007188.1.11",
          "form": "사망각일덧 같아",
          "original_form": "사망각일덧 같아",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007188.1.12",
          "form": "못먹어볼것같아요 ㅋㅋㅋㅋ",
          "original_form": "못먹어볼것같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007188.1.13",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007188.1.14",
          "form": "먹는것도좋지만",
          "original_form": "먹는것도좋지만",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007188.1.15",
          "form": "엽떡도 2단계 나오기전에 3단계 먹다가 눈물 나서",
          "original_form": "엽떡도 2단계 나오기전에 3단계 먹다가 눈물 나서",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007188.1.16",
          "form": "위장생각도!!!",
          "original_form": "위장생각도!!!",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007188.1.17",
          "form": "그러고 나니 2단계가 나왔더라구요",
          "original_form": "그러고 나니 2단계가 나왔더라구요",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007188.1.18",
          "form": "ㅋㅋㅋㅋ오..... 엽떡진짜 근데 마성의매력이에요",
          "original_form": "ㅋㅋㅋㅋ오..... 엽떡진짜 근데 마성의매력이에요",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007188.1.19",
          "form": "치즈추가하시나요",
          "original_form": "치즈추가하시나요",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007188.1.20",
          "form": "집에서 뿌려요 ㅋㅋㅋㅋㅋ",
          "original_form": "집에서 뿌려요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007188.1.21",
          "form": "너무 적어서 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무 적어서 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007188.1.22",
          "form": "아하ㅋㅋㅋㅋ그런방법이",
          "original_form": "아하ㅋㅋㅋㅋ그런방법이",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007188.1.23",
          "form": "하긴 엽떡이 은근",
          "original_form": "하긴 엽떡이 은근",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007188.1.24",
          "form": "엽떡시키면 일딴 만두 굽고 계란삶고 대기",
          "original_form": "엽떡시키면 일딴 만두 굽고 계란삶고 대기",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007188.1.25",
          "form": "양이 떡말곤적더라구요",
          "original_form": "양이 떡말곤적더라구요",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007188.1.26",
          "form": "와!! 진짜먹잘알분",
          "original_form": "와!! 진짜먹잘알분",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007188.1.27",
          "form": "만두는 군만두죠",
          "original_form": "만두는 군만두죠",
          "speaker_id": "2",
