      "utterance": [
        {
          "id": "MDRW2100010415.1.1",
          "form": "혹시 운동하시나요??",
          "original_form": "혹시 운동하시나요??",
          "speaker_id": "1",
          "time": "20210823 17:46"
        },
        {
          "id": "MDRW2100010415.1.2",
          "form": "하루에 1~2시간 정도 유산소해요!",
          "original_form": "하루에 1~2시간 정도 유산소해요!",
          "speaker_id": "2",
          "time": "20210823 17:47"
        },
        {
          "id": "MDRW2100010415.1.3",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210823 17:47"
        },
        {
          "id": "MDRW2100010415.1.4",
          "form": "우와 대단하시네요",
          "original_form": "우와 대단하시네요",
          "speaker_id": "1",
          "time": "20210823 17:50"
        },
        {
          "id": "MDRW2100010415.1.5",
          "form": "저는 수영합니다!",
          "original_form": "저는 수영합니다!",
          "speaker_id": "1",
          "time": "20210823 17:50"
        },
        {
          "id": "MDRW2100010415.1.6",
          "form": "오 저도 수영 배워보고 싶었어요",
          "original_form": "오 저도 수영 배워보고 싶었어요",
          "speaker_id": "2",
          "time": "20210823 17:51"
        },
        {
          "id": "MDRW2100010415.1.7",
          "form": "근데 요즘 수영할 수 있나요?",
          "original_form": "근데 요즘 수영할 수 있나요?",
          "speaker_id": "2",
          "time": "20210823 17:51"
        },
        {
          "id": "MDRW2100010415.1.8",
          "form": "네 저희 센터는 열더라고요!",
          "original_form": "네 저희 센터는 열더라고요!",
          "speaker_id": "1",
          "time": "20210823 17:51"
        },
        {
          "id": "MDRW2100010415.1.9",
          "form": "수영 배워본 적 있으신가요?",
          "original_form": "수영 배워본 적 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 17:52"
        },
        {
          "id": "MDRW2100010415.1.10",
          "form": "아뇨 한번도 없어요 어릴 때는 물을 너무 무서워 해서",
          "original_form": "아뇨 한번도 없어요 어릴 때는 물을 너무 무서워 해서",
          "speaker_id": "2",
          "time": "20210823 17:53"
        },
        {
          "id": "MDRW2100010415.1.11",
          "form": "근데 요즘은 너무 배워보고 싶어요",
          "original_form": "근데 요즘은 너무 배워보고 싶어요",
          "speaker_id": "2",
          "time": "20210823 17:53"
        },
        {
          "id": "MDRW2100010415.1.12",
          "form": "저도 그랬는데 배워보니까 좋더라고요!",
          "original_form": "저도 그랬는데 배워보니까 좋더라고요!",
          "speaker_id": "1",
          "time": "20210823 17:57"
        },
        {
          "id": "MDRW2100010415.1.13",
          "form": "ㅋㅋname2님도 할 수 있을거에요",
          "original_form": "ㅋㅋ&name2&님도 할 수 있을거에요",
          "speaker_id": "1",
          "time": "20210823 17:57"
        },
        {
          "id": "MDRW2100010415.1.14",
          "form": "수영 얼마나 하셨어요?",
          "original_form": "수영 얼마나 하셨어요?",
          "speaker_id": "2",
          "time": "20210823 17:58"
        },
        {
          "id": "MDRW2100010415.1.15",
          "form": "저 2년 됐네요",
          "original_form": "저 2년 됐네요",
          "speaker_id": "1",
          "time": "20210823 17:59"
        },
        {
          "id": "MDRW2100010415.1.16",
          "form": "와 대단하세요",
          "original_form": "와 대단하세요",
          "speaker_id": "2",
          "time": "20210823 18:02"
        },
        {
          "id": "MDRW2100010415.1.17",
          "form": "수영이 칼로리 소모가 그렇게 크다면서요",
          "original_form": "수영이 칼로리 소모가 그렇게 크다면서요",
          "speaker_id": "2",
          "time": "20210823 18:02"
        },
        {
          "id": "MDRW2100010415.1.18",
          "form": "그래서 그런지 살은 더 빠지고 근육은 별로 안 붙는거 같아요ㅋㅋㅋ",
          "original_form": "그래서 그런지 살은 더 빠지고 근육은 별로 안 붙는거 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010415.1.19",
          "form": "유산소는 어떤거 하세요?",
          "original_form": "유산소는 어떤거 하세요?",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010415.1.20",
          "form": "집 앞에 공원이 있어서 1~2시간 정도 걷고 뛰고 반복해요",
          "original_form": "집 앞에 공원이 있어서 1~2시간 정도 걷고 뛰고 반복해요",
          "speaker_id": "2",
          "time": "20210823 18:30"
        },
        {
          "id": "MDRW2100010415.1.21",
          "form": "좋네요! 오늘은 비 와서 못 가시겠군요",
          "original_form": "좋네요! 오늘은 비 와서 못 가시겠군요",
