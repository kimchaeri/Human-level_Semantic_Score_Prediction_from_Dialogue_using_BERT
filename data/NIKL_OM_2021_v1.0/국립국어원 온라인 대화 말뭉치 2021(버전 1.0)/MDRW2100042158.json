        {
          "id": "MDRW2100042158.1.1",
          "form": "요즘 즐기는 컨텐츠가 있으세요?",
          "original_form": "요즘 즐기는 컨텐츠가 있으세요?",
          "speaker_id": "2",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100042158.1.2",
          "form": "코로나 이후에 넷플릭스 열심히 보다가",
          "original_form": "코로나 이후에 넷플릭스 열심히 보다가",
          "speaker_id": "1",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100042158.1.3",
          "form": "여름에 왓챠 이용권 생겨서",
          "original_form": "여름에 왓챠 이용권 생겨서",
          "speaker_id": "1",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100042158.1.4",
          "form": "해리포터시리즈 정주행했어요ㅎㅎ",
          "original_form": "해리포터시리즈 정주행했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100042158.1.5",
          "form": "우와ㅋㅋ 그럼 엄청 오래 걸리죠?",
          "original_form": "우와ㅋㅋ 그럼 엄청 오래 걸리죠?",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100042158.1.6",
          "form": "네 휴가 때 집콕하면서 하루에 한두편씩 봤어요",
          "original_form": "네 휴가 때 집콕하면서 하루에 한두편씩 봤어요",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042158.1.7",
          "form": "전 사실 해리포터 안봤어요ㅋㅋ 다들 놀래요ㅋㅋ",
          "original_form": "전 사실 해리포터 안봤어요ㅋㅋ 다들 놀래요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042158.1.8",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100042158.1.9",
          "form": "저도 영화관 가서 본건",
          "original_form": "저도 영화관 가서 본건",
          "speaker_id": "1",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100042158.1.10",
          "form": "3편까지인가?",
          "original_form": "3편까지인가?",
          "speaker_id": "1",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100042158.1.11",
          "form": "ㅋㅋ 날잡고 보고싶은데 이번 성탄에 볼까봐요",
          "original_form": "ㅋㅋ 날잡고 보고싶은데 이번 성탄에 볼까봐요",
          "speaker_id": "2",
          "time": "20210906 20:22"
        },
        {
          "id": "MDRW2100042158.1.12",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042158.1.13",
          "form": "시리즈물은",
          "original_form": "시리즈물은",
          "speaker_id": "1",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042158.1.14",
          "form": "휴가때 보는 게 제일이죠",
          "original_form": "휴가때 보는 게 제일이죠",
          "speaker_id": "1",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042158.1.15",
          "form": "그러니까요ㅋㅋ 뒷북으로ㅎㅎ",
          "original_form": "그러니까요ㅋㅋ 뒷북으로ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042158.1.16",
          "form": "유투브는 잘 안보세요?",
          "original_form": "유투브는 잘 안보세요?",
          "speaker_id": "2",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042158.1.17",
          "form": "유튜브는 그때그때 봐요",
          "original_form": "유튜브는 그때그때 봐요",
          "speaker_id": "1",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042158.1.18",
          "form": "구독하는 건 특별히 없고",
          "original_form": "구독하는 건 특별히 없고",
          "speaker_id": "1",
