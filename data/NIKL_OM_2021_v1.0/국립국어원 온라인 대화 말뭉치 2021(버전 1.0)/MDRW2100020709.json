      "utterance": [
        {
          "id": "MDRW2100020709.1.1",
          "form": "그런거 생각했는데",
          "original_form": "그런거 생각했는데",
          "speaker_id": "2",
          "time": "20210828 11:26"
        },
        {
          "id": "MDRW2100020709.1.2",
          "form": "너무 말캉거리고 별로였어요",
          "original_form": "너무 말캉거리고 별로였어요",
          "speaker_id": "2",
          "time": "20210828 11:26"
        },
        {
          "id": "MDRW2100020709.1.3",
          "form": "스크류바 젤리가 그맛이라니..",
          "original_form": "스크류바 젤리가 그맛이라니..",
          "speaker_id": "1",
          "time": "20210828 11:26"
        },
        {
          "id": "MDRW2100020709.1.4",
          "form": "아 맛은 스크류바 맛이 나더라고요",
          "original_form": "아 맛은 스크류바 맛이 나더라고요",
          "speaker_id": "2",
          "time": "20210828 11:26"
        },
        {
          "id": "MDRW2100020709.1.5",
          "form": "탱글함도 없었나요",
          "original_form": "탱글함도 없었나요",
          "speaker_id": "1",
          "time": "20210828 11:26"
        },
        {
          "id": "MDRW2100020709.1.6",
          "form": "아뇨 제가 생각하길 그런 식감을 원했는데",
          "original_form": "아뇨 제가 생각하길 그런 식감을 원했는데",
          "speaker_id": "2",
          "time": "20210828 11:26"
        },
        {
          "id": "MDRW2100020709.1.7",
          "form": "젤리는 식감이 젤 중요하죠",
          "original_form": "젤리는 식감이 젤 중요하죠",
          "speaker_id": "1",
          "time": "20210828 11:26"
        },
        {
          "id": "MDRW2100020709.1.8",
          "form": "흐물거리던..",
          "original_form": "흐물거리던..",
          "speaker_id": "2",
          "time": "20210828 11:26"
        },
        {
          "id": "MDRW2100020709.1.9",
          "form": "뚝뚝 끊기던가요",
          "original_form": "뚝뚝 끊기던가요",
          "speaker_id": "1",
          "time": "20210828 11:27"
        },
        {
          "id": "MDRW2100020709.1.10",
          "form": "약간?",
          "original_form": "약간?",
          "speaker_id": "2",
          "time": "20210828 11:27"
        },
        {
          "id": "MDRW2100020709.1.11",
          "form": "ㅋㅋㅋㅋㅋ 저는 젤리중에",
          "original_form": "ㅋㅋㅋㅋㅋ 저는 젤리중에",
          "speaker_id": "2",
          "time": "20210828 11:27"
        },
        {
          "id": "MDRW2100020709.1.12",
          "form": "대충 식감알거같아요",
          "original_form": "대충 식감알거같아요",
          "speaker_id": "1",
          "time": "20210828 11:27"
        },
        {
          "id": "MDRW2100020709.1.13",
          "form": "올리브영에서 파는 레옹젤리있는데",
          "original_form": "올리브영에서 파는 레옹젤리있는데",
          "speaker_id": "2",
          "time": "20210828 11:27"
        },
        {
          "id": "MDRW2100020709.1.14",
          "form": "그거좋아해요",
          "original_form": "그거좋아해요",
          "speaker_id": "2",
          "time": "20210828 11:27"
        },
        {
          "id": "MDRW2100020709.1.15",
          "form": "오호 올영 은근 맛집같아요",
          "original_form": "오호 올영 은근 맛집같아요",
          "speaker_id": "1",
          "time": "20210828 11:28"
        },
        {
          "id": "MDRW2100020709.1.16",
          "form": "레옹젤리도 한번 도전해볼게요",
          "original_form": "레옹젤리도 한번 도전해볼게요",
          "speaker_id": "1",
          "time": "20210828 11:28"
        },
        {
          "id": "MDRW2100020709.1.17",
          "form": "그 복숭아젤리랑 비슷한데 식감이 좀더 탄탄",
          "original_form": "그 복숭아젤리랑 비슷한데 식감이 좀더 탄탄",
          "speaker_id": "2",
          "time": "20210828 11:28"
        },
