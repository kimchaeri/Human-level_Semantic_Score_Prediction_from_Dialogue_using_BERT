        {
          "id": "MDRW2100019286.1.1",
          "form": "술 가끔 마셔요",
          "original_form": "술 가끔 마셔요",
          "speaker_id": "2",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019286.1.2",
          "form": "근데 민초소주는 도저히...",
          "original_form": "근데 민초소주는 도저히...",
          "speaker_id": "2",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019286.1.3",
          "form": "상상도 안가는 맛이네요..알코올이랑 민초라니!",
          "original_form": "상상도 안가는 맛이네요..알코올이랑 민초라니!",
          "speaker_id": "1",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019286.1.4",
          "form": "그러니까요... 좀 아닌것 같아요",
          "original_form": "그러니까요... 좀 아닌것 같아요",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019286.1.5",
          "form": "술은 어느거 드세요?",
          "original_form": "술은 어느거 드세요?",
          "speaker_id": "1",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019286.1.6",
          "form": "맞아여 ㅠ",
          "original_form": "맞아여 ㅠ",
          "speaker_id": "1",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019286.1.7",
          "form": "전 과일맥주 좋아해요!",
          "original_form": "전 과일맥주 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100019286.1.8",
          "form": "오 추천해주실만한게 있나요?",
          "original_form": "오 추천해주실만한게 있나요?",
          "speaker_id": "1",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100019286.1.9",
          "form": "예거 맛있어요!",
          "original_form": "예거 맛있어요!",
          "speaker_id": "2",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100019286.1.10",
          "form": "오 유명하더라구요",
          "original_form": "오 유명하더라구요",
          "speaker_id": "1",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100019286.1.11",
          "form": "맛있어서 그런것 같아요!",
          "original_form": "맛있어서 그런것 같아요!",
          "speaker_id": "2",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100019286.1.12",
          "form": "맞아요 무슨맛으로 드세요?",
          "original_form": "맞아요 무슨맛으로 드세요?",
          "speaker_id": "1",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100019286.1.13",
          "form": "전 복숭아로 먹어요!",
          "original_form": "전 복숭아로 먹어요!",
          "speaker_id": "2",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100019286.1.14",
          "form": "복숭아맛도 있군여 이슬톡톡 생각나네요 ㅋㅋㅋ",
          "original_form": "복숭아맛도 있군여 이슬톡톡 생각나네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100019286.1.15",
          "form": "이슬톡톡 맛있죠",
          "original_form": "이슬톡톡 맛있죠",
          "speaker_id": "2",
