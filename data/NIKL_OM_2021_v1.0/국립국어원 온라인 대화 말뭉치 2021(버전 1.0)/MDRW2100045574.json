        },
        {
          "id": "MDRW2100045574.1.6",
          "form": "넵 새콤해요",
          "original_form": "넵 새콤해요",
          "speaker_id": "2",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100045574.1.7",
          "form": "껍질째 먹기도 하나요?",
          "original_form": "껍질째 먹기도 하나요?",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100045574.1.8",
          "form": "이 과일을?",
          "original_form": "이 과일을?",
