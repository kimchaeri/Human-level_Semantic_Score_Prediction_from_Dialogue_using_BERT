      "utterance": [
        {
          "id": "MDRW2100004267.1.1",
          "form": "혹시 최근에 사거나 사실 예정인 물건 있으신가요?",
          "original_form": "혹시 최근에 사거나 사실 예정인 물건 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004267.1.2",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004267.1.3",
          "form": "좋아하는 아이돌이 컴백해서",
          "original_form": "좋아하는 아이돌이 컴백해서",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004267.1.4",
          "form": "앨범을 살 예정입니다",
          "original_form": "앨범을 살 예정입니다",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004267.1.5",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004267.1.6",
          "form": "저는 온라인 공연 티켓도 살 예정",
          "original_form": "저는 온라인 공연 티켓도 살 예정",
          "speaker_id": "1",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004267.1.7",
          "form": "아 저두요!",
          "original_form": "아 저두요!",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004267.1.8",
          "form": "온라인이라 고민했는데 그냥 사려구요",
          "original_form": "온라인이라 고민했는데 그냥 사려구요",
          "speaker_id": "1",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004267.1.9",
          "form": "저는 이건 안사면 100퍼 후회한다 싶어서",
          "original_form": "저는 이건 안사면 100퍼 후회한다 싶어서",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004267.1.10",
          "form": "사려구요",
          "original_form": "사려구요",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004267.1.11",
          "form": "포토카드도 있으면 사고싶은데",
          "original_form": "포토카드도 있으면 사고싶은데",
          "speaker_id": "1",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004267.1.12",
          "form": "돈이 없어서 슬퍼요",
          "original_form": "돈이 없어서 슬퍼요",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004267.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004267.1.14",
          "form": "통장 아니라 텅장",
          "original_form": "통장 아니라 텅장",
          "speaker_id": "2",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004267.1.15",
          "form": "그 포토카드 보호하는 탑로더 아시나요",
          "original_form": "그 포토카드 보호하는 탑로더 아시나요",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004267.1.16",
          "form": "그것도 사고싶던데",
          "original_form": "그것도 사고싶던데",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004267.1.17",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004267.1.18",
          "form": "알아요",
          "original_form": "알아요",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004267.1.19",
          "form": "저도 사고싶어요ㅠㅠ",
          "original_form": "저도 사고싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004267.1.20",
          "form": "그거 사려구요",
          "original_form": "그거 사려구요",
          "speaker_id": "1",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004267.1.21",
          "form": "같이 사요",
          "original_form": "같이 사요",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004267.1.22",
          "form": "포토카드는 소중하니까",
          "original_form": "포토카드는 소중하니까",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004267.1.23",
          "form": "앨범 먼저 사고 살 예정이에여",
          "original_form": "앨범 먼저 사고 살 예정이에여",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004267.1.24",
          "form": "그래요 같이 사요",
          "original_form": "그래요 같이 사요",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004267.1.25",
          "form": "앨범은 몇개나 사실 예정인가요",
          "original_form": "앨범은 몇개나 사실 예정인가요",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004267.1.26",
          "form": "저 일단 버전당 하나씩 사고",
          "original_form": "저 일단 버전당 하나씩 사고",
          "speaker_id": "2",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004267.1.27",
          "form": "더 살지 말지는 고민중이에요",
          "original_form": "더 살지 말지는 고민중이에요",
          "speaker_id": "2",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004267.1.28",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004267.1.29",
          "form": "가격이 중요하죠",
          "original_form": "가격이 중요하죠",
          "speaker_id": "1",
          "time": "20210821 15:56"
        }
      ]
    }
  ]
