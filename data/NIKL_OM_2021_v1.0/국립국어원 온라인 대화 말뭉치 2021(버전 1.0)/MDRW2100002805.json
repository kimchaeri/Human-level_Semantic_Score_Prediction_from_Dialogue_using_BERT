      "utterance": [
        {
          "id": "MDRW2100002805.1.1",
          "form": "오늘 너무 덥네요 거기도 덥나요?",
          "original_form": "오늘 너무 덥네요 거기도 덥나요?",
          "speaker_id": "1",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002805.1.2",
          "form": "여기는 오늘 선선한편이에요",
          "original_form": "여기는 오늘 선선한편이에요",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002805.1.3",
          "form": "많이 덥나요?",
          "original_form": "많이 덥나요?",
          "speaker_id": "2",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002805.1.4",
          "form": "막 엄청 덥기보다",
          "original_form": "막 엄청 덥기보다",
          "speaker_id": "1",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002805.1.5",
          "form": "햇빛이 너무 뜨거워요",
          "original_form": "햇빛이 너무 뜨거워요",
          "speaker_id": "1",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002805.1.6",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002805.1.7",
          "form": "그거 뭔지 알아요",
          "original_form": "그거 뭔지 알아요",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002805.1.8",
          "form": "양산 필수입니다",
          "original_form": "양산 필수입니다",
          "speaker_id": "1",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002805.1.9",
          "form": "선크림두요ㅎ",
          "original_form": "선크림두요ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002805.1.10",
          "form": "요즘 자외선 지수가 높아져서",
          "original_form": "요즘 자외선 지수가 높아져서",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002805.1.11",
          "form": "전 원래 양산 안썼는데 요새는 쓰게 되더라구요",
          "original_form": "전 원래 양산 안썼는데 요새는 쓰게 되더라구요",
          "speaker_id": "1",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002805.1.12",
          "form": "피부보호하려면 양산이랑 선크림은 필수인거같아요",
          "original_form": "피부보호하려면 양산이랑 선크림은 필수인거같아요",
          "speaker_id": "2",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002805.1.13",
          "form": "맞아요 근데 선크림은 양산 쓴다고 안바르게 되네요 ㅠ",
          "original_form": "맞아요 근데 선크림은 양산 쓴다고 안바르게 되네요 ㅠ",
          "speaker_id": "1",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002805.1.14",
          "form": "에고 조금씩은 바르세요",
          "original_form": "에고 조금씩은 바르세요",
          "speaker_id": "2",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002805.1.15",
          "form": "근데 양산 비싸지않나요?",
          "original_form": "근데 양산 비싸지않나요?",
          "speaker_id": "2",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002805.1.16",
          "form": "전 비싼 거는 아니고",
          "original_form": "전 비싼 거는 아니고",
          "speaker_id": "1",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002805.1.17",
          "form": "그냥 우양산이라 그런지 그럭저럭이에여 ㅎ",
          "original_form": "그냥 우양산이라 그런지 그럭저럭이에여 ㅎ",
          "speaker_id": "1",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002805.1.18",
          "form": "앗 그러시구나..! 양산하면 뭔가 좀 비싼이미지가 있어서 궁금했었는데 알려주셔서 고마워요",
          "original_form": "앗 그러시구나..! 양산하면 뭔가 좀 비싼이미지가 있어서 궁금했었는데 알려주셔서 고마워요",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002805.1.19",
          "form": "일교차는 어때요?",
          "original_form": "일교차는 어때요?",
          "speaker_id": "2",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002805.1.20",
          "form": "여기는 좀 있는 편이라 아침 저녁으로는 좀 춥더라구요",
          "original_form": "여기는 좀 있는 편이라 아침 저녁으로는 좀 춥더라구요",
          "speaker_id": "2",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002805.1.21",
          "form": "여기도 오전에랑은 엄청 더운데 새벽이랑은 추워요",
          "original_form": "여기도 오전에랑은 엄청 더운데 새벽이랑은 추워요",
          "speaker_id": "1",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002805.1.22",
          "form": "신기하게 선풍기릉 틀어도 요즘은 춥더라구요",
          "original_form": "신기하게 선풍기릉 틀어도 요즘은 춥더라구요",
          "speaker_id": "1",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002805.1.23",
          "form": "습도가 없어서 그런가봐요. 공기가 시원해졌더라구요",
          "original_form": "습도가 없어서 그런가봐요. 공기가 시원해졌더라구요",
          "speaker_id": "2",
          "time": "20210820 15:50"
        }
      ]
    }
  ]
