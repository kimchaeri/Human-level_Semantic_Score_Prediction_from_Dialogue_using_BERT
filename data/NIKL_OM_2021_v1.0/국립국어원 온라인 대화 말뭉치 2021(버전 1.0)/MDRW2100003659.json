      "utterance": [
        {
          "id": "MDRW2100003659.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 10:05"
        },
        {
          "id": "MDRW2100003659.1.2",
          "form": "안녕하세요 좋아하는 게임 있으신가요?",
          "original_form": "안녕하세요 좋아하는 게임 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 10:15"
        },
        {
          "id": "MDRW2100003659.1.3",
          "form": "네 너무많아 문제에요",
          "original_form": "네 너무많아 문제에요",
          "speaker_id": "1",
          "time": "20210821 10:15"
        },
        {
          "id": "MDRW2100003659.1.4",
          "form": "최애 게임이 뭔가요?",
          "original_form": "최애 게임이 뭔가요?",
          "speaker_id": "2",
          "time": "20210821 10:16"
        },
        {
          "id": "MDRW2100003659.1.5",
          "form": "음 참어려운데 한개고르라면 세기의 명작 슈퍼마리오",
          "original_form": "음 참어려운데 한개고르라면 세기의 명작 슈퍼마리오",
          "speaker_id": "1",
          "time": "20210821 10:16"
        },
        {
          "id": "MDRW2100003659.1.6",
          "form": "슈퍼마리오요?",
          "original_form": "슈퍼마리오요?",
          "speaker_id": "2",
          "time": "20210821 10:17"
        },
        {
          "id": "MDRW2100003659.1.7",
          "form": "뭘로 하세요?",
          "original_form": "뭘로 하세요?",
          "speaker_id": "2",
          "time": "20210821 10:17"
        },
        {
          "id": "MDRW2100003659.1.8",
          "form": "폰으로도하고 컴퓨터로도하죠 엿날엔겜기로도했어요",
          "original_form": "폰으로도하고 컴퓨터로도하죠 엿날엔겜기로도했어요",
          "speaker_id": "1",
          "time": "20210821 10:18"
        },
        {
          "id": "MDRW2100003659.1.9",
          "form": "아 다양하군요 ㅎ",
          "original_form": "아 다양하군요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 11:32"
        },
        {
          "id": "MDRW2100003659.1.10",
          "form": "전 닌ㅇ텐도에만 있는 줄 알았어요",
          "original_form": "전 닌ㅇ텐도에만 있는 줄 알았어요",
          "speaker_id": "2",
          "time": "20210821 11:32"
        },
        {
          "id": "MDRW2100003659.1.11",
          "form": "오래된버전이죠  최신이아니고 옛날에나온 슈퍼마리오1요",
          "original_form": "오래된버전이죠  최신이아니고 옛날에나온 슈퍼마리오1요",
          "speaker_id": "1",
          "time": "20210821 11:34"
        },
        {
          "id": "MDRW2100003659.1.12",
          "form": "아 완전 옛날이요",
          "original_form": "아 완전 옛날이요",
          "speaker_id": "2",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003659.1.13",
          "form": "궁금하네요 ㅎ",
          "original_form": "궁금하네요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003659.1.14",
          "form": "간단해요 피하고 넘고 앞으로만가면되죠",
          "original_form": "간단해요 피하고 넘고 앞으로만가면되죠",
          "speaker_id": "1",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003659.1.15",
          "form": "ㅋㅋ ㅋㅋ 캐릭터 예전에서 요새로 많이 바꼈나요?",
          "original_form": "ㅋㅋ ㅋㅋ 캐릭터 예전에서 요새로 많이 바꼈나요?",
          "speaker_id": "2",
          "time": "20210821 11:59"
        },
        {
          "id": "MDRW2100003659.1.16",
          "form": "1980년대 나온고대로죠  옛날게임을 컴퓨터로  프로그래밍한 게임이니깐요 팬픽이랄까",
          "original_form": "1980년대 나온고대로죠  옛날게임을 컴퓨터로  프로그래밍한 게임이니깐요 팬픽이랄까",
          "speaker_id": "1",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003659.1.17",
          "form": "그게슈퍼마리오잔아요",
          "original_form": "그게슈퍼마리오잔아요",
