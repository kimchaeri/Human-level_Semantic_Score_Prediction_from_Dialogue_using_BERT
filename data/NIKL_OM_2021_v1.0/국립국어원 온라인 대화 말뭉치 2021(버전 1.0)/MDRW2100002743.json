      "utterance": [
        {
          "id": "MDRW2100002743.1.1",
          "form": "쇼핑 즐겨하세요?",
          "original_form": "쇼핑 즐겨하세요?",
          "speaker_id": "1",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002743.1.2",
          "form": "물욕이 없어서 쇼핑은 잘 안하는 거 같아요",
          "original_form": "물욕이 없어서 쇼핑은 잘 안하는 거 같아요",
          "speaker_id": "2",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002743.1.3",
          "form": "최근에 쇼핑하신 적 있으신가요?",
          "original_form": "최근에 쇼핑하신 적 있으신가요?",
          "speaker_id": "2",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002743.1.4",
          "form": "최근에 와이드팬츠 주문용 !",
          "original_form": "최근에 와이드팬츠 주문용 !",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002743.1.5",
          "form": "구매하고싶은 제품있으세요?",
          "original_form": "구매하고싶은 제품있으세요?",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002743.1.6",
          "form": "와이드팬츠 편해서 좋더라구요",
          "original_form": "와이드팬츠 편해서 좋더라구요",
          "speaker_id": "2",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002743.1.7",
          "form": "음.. 구매하고 싶은거 음...아",
          "original_form": "음.. 구매하고 싶은거 음...아",
          "speaker_id": "2",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002743.1.8",
          "form": "여름 다 가긴했는데 선풍기 고장나서 선풍기 하나 살까 생긱하고있어요",
          "original_form": "여름 다 가긴했는데 선풍기 고장나서 선풍기 하나 살까 생긱하고있어요",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002743.1.9",
          "form": "내년에 사세요!",
          "original_form": "내년에 사세요!",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002743.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ 여름다갔으니깐요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 여름다갔으니깐요",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002743.1.11",
          "form": "아무래도 그게 좋겠죠?",
          "original_form": "아무래도 그게 좋겠죠?",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002743.1.12",
          "form": "와이드팬츠 무슨 색 사셨어요?",
          "original_form": "와이드팬츠 무슨 색 사셨어요?",
          "speaker_id": "2",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002743.1.13",
          "form": "데님도 사구용",
          "original_form": "데님도 사구용",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002743.1.14",
          "form": "검정도 샀어요 !",
          "original_form": "검정도 샀어요 !",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002743.1.15",
          "form": "예쁘겠다",
          "original_form": "예쁘겠다",
          "speaker_id": "2",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002743.1.16",
          "form": "나이들수록 편한것만 찾게되네요 ㅠㅠ",
          "original_form": "나이들수록 편한것만 찾게되네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002743.1.17",
          "form": "아니에요 연령불문 편한게 최고죠!!",
          "original_form": "아니에요 연령불문 편한게 최고죠!!",
          "speaker_id": "2",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002743.1.18",
          "form": "옷이 불편하면 너무 힘들더라구요",
          "original_form": "옷이 불편하면 너무 힘들더라구요",
          "speaker_id": "2",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002743.1.19",
          "form": "그래서 주문하신 옷은 마음에 드시나요?",
          "original_form": "그래서 주문하신 옷은 마음에 드시나요?",
          "speaker_id": "2",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002743.1.20",
          "form": "네넹 !! 작년부터 8키로가 쪄서 옷살마음도없다가",
          "original_form": "네넹 !! 작년부터 8키로가 쪄서 옷살마음도없다가",
          "speaker_id": "1",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002743.1.21",
          "form": "최근에 3키로빠졌더니 그새 맵시가 사는거있죠;;",
          "original_form": "최근에 3키로빠졌더니 그새 맵시가 사는거있죠;;",
          "speaker_id": "1",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002743.1.22",
          "form": "더 빼야돼영 ㅠㅠ",
          "original_form": "더 빼야돼영 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002743.1.23",
          "form": "에고 너무 스트레스 받지 마셔요ㅠㅠ",
          "original_form": "에고 너무 스트레스 받지 마셔요ㅠㅠ",
