      "utterance": [
        {
          "id": "MDRW2100011849.1.1",
          "form": "신부는 방에 가만히 앉아있고",
          "original_form": "신부는 방에 가만히 앉아있고",
          "speaker_id": "2",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011849.1.2",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011849.1.3",
          "form": "시끄럽고 산만한 분위기속에서 느껴지는",
          "original_form": "시끄럽고 산만한 분위기속에서 느껴지는",
          "speaker_id": "2",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011849.1.4",
          "form": "기묘함이 대박이었어요",
          "original_form": "기묘함이 대박이었어요",
          "speaker_id": "2",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100011849.1.5",
          "form": "여자가 무슨 트로피같지않나요",
          "original_form": "여자가 무슨 트로피같지않나요",
          "speaker_id": "1",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100011849.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100011849.1.7",
          "form": "아까 말씀하신것처럼 신부랑 신부가족 울고",
          "original_form": "아까 말씀하신것처럼 신부랑 신부가족 울고",
          "speaker_id": "1",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100011849.1.8",
          "form": "애초에 신부 아버지 손에서 신랑 손으로 옮겨가는 것고 그렇고",
          "original_form": "애초에 신부 아버지 손에서 신랑 손으로 옮겨가는 것고 그렇고",
          "speaker_id": "2",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100011849.1.9",
          "form": "아 저도 그거치고잇엇어요 ㄹㅇ",
          "original_form": "아 저도 그거치고잇엇어요 ㄹㅇ",
          "speaker_id": "1",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100011849.1.10",
          "form": "부조리그자체",
          "original_form": "부조리그자체",
          "speaker_id": "1",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100011849.1.11",
          "form": "한쪽은 글썽이는데",
          "original_form": "한쪽은 글썽이는데",
          "speaker_id": "2",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100011849.1.12",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100011849.1.13",
          "form": "신랑쪽은 행복해하고",
          "original_form": "신랑쪽은 행복해하고",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100011849.1.14",
          "form": "이상한걸 너무 아름답게 꾸민게",
          "original_form": "이상한걸 너무 아름답게 꾸민게",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100011849.1.15",
          "form": "현대예술 그 자체",
          "original_form": "현대예술 그 자체",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100011849.1.16",
          "form": "여자 팔려가는거같지않나요???",
          "original_form": "여자 팔려가는거같지않나요???",
          "speaker_id": "1",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100011849.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100011849.1.18",
          "form": "현대예술ㅋㅋㅋㅋㅋ ㅜㅜㅜ",
          "original_form": "현대예술ㅋㅋㅋㅋㅋ ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100011849.1.19",
          "form": "솔직하게 느낀걸로는",
          "original_form": "솔직하게 느낀걸로는",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100011849.1.20",
          "form": "여자가 구렁텅이로 끌려가는 걸",
          "original_form": "여자가 구렁텅이로 끌려가는 걸",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100011849.1.21",
          "form": "아름답게 꾸미는 느낌이에요",
          "original_form": "아름답게 꾸미는 느낌이에요",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100011849.1.22",
          "form": "맞아요 무슨 제물 바치는 의례도아니고",
          "original_form": "맞아요 무슨 제물 바치는 의례도아니고",
          "speaker_id": "1",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011849.1.23",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011849.1.24",
          "form": "와 맞아요",
          "original_form": "와 맞아요",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011849.1.25",
          "form": "제가 신부쪽, 신랑쪽 지인으로 둘다 가봤는데",
          "original_form": "제가 신부쪽, 신랑쪽 지인으로 둘다 가봤는데",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011849.1.26",
          "form": "신부쪽에서는",
          "original_form": "신부쪽에서는",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011849.1.27",
          "form": "진짜 눈물 흘리는 사람이 꽤 있었어요",
          "original_form": "진짜 눈물 흘리는 사람이 꽤 있었어요",
          "speaker_id": "2",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100011849.1.28",
          "form": "그 자체가 사실상 다들 알고 있는게 아닌가",
          "original_form": "그 자체가 사실상 다들 알고 있는게 아닌가",
          "speaker_id": "2",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100011849.1.29",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100011849.1.30",
          "form": "신부 친구들도 막 울고",
          "original_form": "신부 친구들도 막 울고",
          "speaker_id": "1",
