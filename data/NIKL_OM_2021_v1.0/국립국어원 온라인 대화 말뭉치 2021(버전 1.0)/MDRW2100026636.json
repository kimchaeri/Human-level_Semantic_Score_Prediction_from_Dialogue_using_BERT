      "utterance": [
        {
          "id": "MDRW2100026636.1.1",
          "form": "나 친구에게 집들이 선물을 받았어",
          "original_form": "나 친구에게 집들이 선물을 받았어",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026636.1.2",
          "form": "정말?",
          "original_form": "정말?",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026636.1.3",
          "form": "뭔데?",
          "original_form": "뭔데?",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026636.1.4",
          "form": "궁금해!",
          "original_form": "궁금해!",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026636.1.5",
          "form": "바로 지금 베스트셀러인 책이야",
          "original_form": "바로 지금 베스트셀러인 책이야",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026636.1.6",
          "form": "헉 뭘까? 궁금해",
          "original_form": "헉 뭘까? 궁금해",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026636.1.7",
          "form": "혹시 정유정작가 알아?",
          "original_form": "혹시 정유정작가 알아?",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026636.1.8",
          "form": "요즘 서점에 잘 안가거든 나는",
          "original_form": "요즘 서점에 잘 안가거든 나는",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026636.1.9",
          "form": "나는 서점냄새를 좋아해",
          "original_form": "나는 서점냄새를 좋아해",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026636.1.10",
          "form": "정유정작가?",
          "original_form": "정유정작가?",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026636.1.11",
          "form": "나도 좋아해",
          "original_form": "나도 좋아해",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026636.1.12",
          "form": "새책 냄새를 좋아해 나는",
          "original_form": "새책 냄새를 좋아해 나는",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026636.1.13",
          "form": "응응 7년의 밤으로 유명한 작가님인데",
          "original_form": "응응 7년의 밤으로 유명한 작가님인데",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026636.1.14",
          "form": "나는 책을 소장하는 것도 좋아해",
          "original_form": "나는 책을 소장하는 것도 좋아해",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026636.1.15",
          "form": "얼마전에 신작을 내셨어",
          "original_form": "얼마전에 신작을 내셨어",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026636.1.16",
          "form": "7년의밤?",
          "original_form": "7년의밤?",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026636.1.17",
          "form": "그리고 내친구가 그걸 선물로줬어!",
          "original_form": "그리고 내친구가 그걸 선물로줬어!",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026636.1.18",
          "form": "추리소설이야?",
          "original_form": "추리소설이야?",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026636.1.19",
          "form": "응 비슷해 추리 스릴러야",
          "original_form": "응 비슷해 추리 스릴러야",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026636.1.20",
          "form": "친구가 너의 취향을 잘 아는구나!",
          "original_form": "친구가 너의 취향을 잘 아는구나!",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026636.1.21",
          "form": "추리스릴러 나도 좋앟9",
          "original_form": "추리스릴러 나도 좋앟9",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026636.1.22",
          "form": "맞아 너무 고마웠어",
          "original_form": "맞아 너무 고마웠어",
          "speaker_id": "1",
          "time": "20210901 23:28"
        }
      ]
    }
