      "utterance": [
        {
          "id": "MDRW2100010925.1.1",
          "form": "머리 많이 빠져서 단발로 자르고 싶어",
          "original_form": "머리 많이 빠져서 단발로 자르고 싶어",
          "speaker_id": "2",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100010925.1.2",
          "form": "미용실에 한 번 가서 잘라",
          "original_form": "미용실에 한 번 가서 잘라",
          "speaker_id": "1",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100010925.1.3",
          "form": "미용실에 백만년 전에 다녀온 것 같은디 ㅋㅋ",
          "original_form": "미용실에 백만년 전에 다녀온 것 같은디 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100010925.1.4",
          "form": "ㅎㅎ 너무 안가긴 하더라",
          "original_form": "ㅎㅎ 너무 안가긴 하더라",
          "speaker_id": "1",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100010925.1.5",
          "form": "귀찮아서 계속 길렀는데 너무 많이 머리가 빠져서..",
          "original_form": "귀찮아서 계속 길렀는데 너무 많이 머리가 빠져서..",
          "speaker_id": "2",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100010925.1.6",
          "form": "편할 때 아무때나 다녀와",
          "original_form": "편할 때 아무때나 다녀와",
          "speaker_id": "1",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100010925.1.7",
          "form": "요즘에 애들이랑 하루종일 같이 있으니 집을 비울 수가 없어 ㅠㅠ",
          "original_form": "요즘에 애들이랑 하루종일 같이 있으니 집을 비울 수가 없어 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100010925.1.8",
          "form": "나 쉬는 날 다녀오면 되지",
          "original_form": "나 쉬는 날 다녀오면 되지",
          "speaker_id": "1",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100010925.1.9",
          "form": "그래도 될까?",
          "original_form": "그래도 될까?",
          "speaker_id": "2",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100010925.1.10",
          "form": "ㅇㅇ 언제든 얘기하고 다녀와요",
          "original_form": "ㅇㅇ 언제든 얘기하고 다녀와요",
          "speaker_id": "1",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100010925.1.11",
          "form": "한 번 가긴 해야할 것 같아",
          "original_form": "한 번 가긴 해야할 것 같아",
          "speaker_id": "2",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100010925.1.12",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100010925.1.13",
          "form": "나만큼 색시도 미용실 안가 ㅋㅋㅋ",
          "original_form": "나만큼 색시도 미용실 안가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100010925.1.14",
          "form": "너무 귀찮아 ㅎㅎ 오래 앉아있는 게",
