      "utterance": [
        {
          "id": "MDRW2100011006.1.1",
          "form": "닭볶음키트는 가격이 얼마나해??",
          "original_form": "닭볶음키트는 가격이 얼마나해??",
          "speaker_id": "2",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100011006.1.2",
          "form": "닭볶음키트면 만원 넘지 않을까?",
          "original_form": "닭볶음키트면 만원 넘지 않을까?",
          "speaker_id": "1",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100011006.1.3",
          "form": "담다디에선 안팔어",
          "original_form": "담다디에선 안팔어",
          "speaker_id": "1",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100011006.1.4",
          "form": "그냥 내가 그런 제품을 원하는거야.",
          "original_form": "그냥 내가 그런 제품을 원하는거야.",
          "speaker_id": "1",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100011006.1.5",
          "form": "요즘 키트로 나오는 상품들 아주 좋은 것 같아",
          "original_form": "요즘 키트로 나오는 상품들 아주 좋은 것 같아",
          "speaker_id": "2",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100011006.1.6",
          "form": "가격이 조금 비싸긴하지만",
          "original_form": "가격이 조금 비싸긴하지만",
          "speaker_id": "2",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100011006.1.7",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100011006.1.8",
          "form": "다른 부재료들이 필요없잖아",
          "original_form": "다른 부재료들이 필요없잖아",
          "speaker_id": "2",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011006.1.9",
          "form": "너 자취하니까 프레시지 제품을 잘 이용해봐",
          "original_form": "너 자취하니까 프레시지 제품을 잘 이용해봐",
          "speaker_id": "1",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011006.1.10",
          "form": "제법 맛이있어",
          "original_form": "제법 맛이있어",
          "speaker_id": "1",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011006.1.11",
          "form": "마라샹궈키트도 사고싶다.",
          "original_form": "마라샹궈키트도 사고싶다.",
          "speaker_id": "1",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011006.1.12",
          "form": "그래그래 근데 그거 유통기한이 짧다며?",
          "original_form": "그래그래 근데 그거 유통기한이 짧다며?",
          "speaker_id": "2",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011006.1.13",
          "form": "맥주와 먹으면 정말 환상이야",
          "original_form": "맥주와 먹으면 정말 환상이야",
          "speaker_id": "1",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011006.1.14",
          "form": "그래? 근데 어차피 가격도 꽤 해서",
          "original_form": "그래? 근데 어차피 가격도 꽤 해서",
          "speaker_id": "1",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011006.1.15",
          "form": "어우 지금 당장 먹고싶은데 어떡하지?",
          "original_form": "어우 지금 당장 먹고싶은데 어떡하지?",
          "speaker_id": "2",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011006.1.16",
          "form": "한번살 때 한두개씩밖에 못사.",
          "original_form": "한번살 때 한두개씩밖에 못사.",
          "speaker_id": "1",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011006.1.17",
          "form": "참자. 너의 잔고를 봐.",
          "original_form": "참자. 너의 잔고를 봐.",
          "speaker_id": "1",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011006.1.18",
          "form": "ㅠㅠ정말 슬프다 자취생들을 위해",
          "original_form": "ㅠㅠ정말 슬프다 자취생들을 위해",
          "speaker_id": "2",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011006.1.19",
          "form": "나도 내 은행잔고를 보며 참을게.",
          "original_form": "나도 내 은행잔고를 보며 참을게.",
          "speaker_id": "1",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011006.1.20",
          "form": "조금만 할인해줬으면해",
          "original_form": "조금만 할인해줬으면해",
          "speaker_id": "2",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011006.1.21",
          "form": "세상에는 정말 갖고싶은 개 많다. 그렇지?",
          "original_form": "세상에는 정말 갖고싶은 개 많다. 그렇지?",
          "speaker_id": "1",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011006.1.22",
          "form": "그래 나는 일단 사야하는 것들부터 사야겠어",
          "original_form": "그래 나는 일단 사야하는 것들부터 사야겠어",
          "speaker_id": "2",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011006.1.23",
          "form": "너 비비고 만두도 사놓을거야?",
          "original_form": "너 비비고 만두도 사놓을거야?",
          "speaker_id": "1",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011006.1.24",
          "form": "맞아...",
          "original_form": "맞아...",
          "speaker_id": "2",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011006.1.25",
          "form": "식사대용으로 참 좋아",
          "original_form": "식사대용으로 참 좋아",
          "speaker_id": "1",
          "time": "20210823 23:43"
        }
      ]
    }
  ]
