        },
        {
          "id": "MDRW2100030155.1.2",
          "form": "예스.....",
          "original_form": "예스.....",
          "speaker_id": "2",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030155.1.3",
          "form": "헉 그래두 name2님은 꾸준히 보시는듯... 전 포기했어요",
          "original_form": "헉 그래두 &name2&님은 꾸준히 보시는듯... 전 포기했어요",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030155.1.4",
          "form": "나 궁금한 거 name2님 최애 정해졌나요?",
          "original_form": "나 궁금한 거 &name2&님 최애 정해졌나요?",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030155.1.5",
          "form": "그중최애꼽자면 뵤...? 근데 최애라기엔 사실 그렇게까지 좋은건 아닌거같구... 솔직히 테스타에 아이돌 최애상이 딱히......ㅜ",
          "original_form": "그중최애꼽자면 뵤...? 근데 최애라기엔 사실 그렇게까지 좋은건 아닌거같구... 솔직히 테스타에 아이돌 최애상이 딱히......ㅜ",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030155.1.6",
          "form": "그치그치 나두 없어서...",
          "original_form": "그치그치 나두 없어서...",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030155.1.7",
          "form": "걍 먼가 캐릭터로 보면 좋겠는데",
          "original_form": "걍 먼가 캐릭터로 보면 좋겠는데",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030155.1.8",
          "form": "아이돌로 보이니까 문젠가봐ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아이돌로 보이니까 문젠가봐ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030155.1.9",
          "form": "마자......그런거같어...",
          "original_form": "마자......그런거같어...",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030155.1.10",
          "form": "애초에 레티가 비중 더 컸었으면",
          "original_form": "애초에 레티가 비중 더 컸었으면",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030155.1.11",
          "form": "나 쓸디투디최애상이 좀 다른편이라그런지",
          "original_form": "나 쓸디투디최애상이 좀 다른편이라그런지",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030155.1.12",
          "form": "그중에...최애...있었을지도",
          "original_form": "그중에...최애...있었을지도",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030155.1.13",
          "form": "헐 마저",
          "original_form": "헐 마저",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030155.1.14",
          "form": "마자마자",
          "original_form": "마자마자",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030155.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030155.1.16",
          "form": "그래서 분량이 더 나와줫음 해...작가님...들리시나요?",
          "original_form": "그래서 분량이 더 나와줫음 해...작가님...들리시나요?",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030155.1.17",
          "form": "그나마 최신화에 좀 나옴...",
          "original_form": "그나마 최신화에 좀 나옴...",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030155.1.18",
          "form": "근데 작가님도..힘드시겠지 뭐",
          "original_form": "근데 작가님도..힘드시겠지 뭐",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030155.1.19",
          "form": "다른멤...",
          "original_form": "다른멤...",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030155.1.20",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030155.1.21",
          "form": "웅웅 연말시상식하면서",
          "original_form": "웅웅 연말시상식하면서",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030155.1.22",
          "form": "이제 슬슬 나오려나?",
          "original_form": "이제 슬슬 나오려나?",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030155.1.23",
          "form": "대받....연재물의 묘미가 이거다",
          "original_form": "대받....연재물의 묘미가 이거다",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030155.1.24",
          "form": "그랬음 좋겟다...",
          "original_form": "그랬음 좋겟다...",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030155.1.25",
          "form": "이 전개면 나는 완결 나고 몰아서 읽을듯...",
          "original_form": "이 전개면 나는 완결 나고 몰아서 읽을듯...",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030155.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030155.1.27",
          "form": "약간 청려가...",
          "original_form": "약간 청려가...",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030155.1.28",
          "form": "이후활동에있을 위기...? 같은걸 알려주고",
          "original_form": "이후활동에있을 위기...? 같은걸 알려주고",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030155.1.29",
          "form": "아님 레티 스토리 나오는 분량부터만 읽을지도...",
          "original_form": "아님 레티 스토리 나오는 분량부터만 읽을지도...",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030155.1.30",
