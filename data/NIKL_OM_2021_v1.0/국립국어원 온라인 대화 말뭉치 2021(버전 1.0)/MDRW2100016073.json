      "utterance": [
        {
          "id": "MDRW2100016073.1.1",
          "form": "책 많이 읽으세요?",
          "original_form": "책 많이 읽으세요?",
          "speaker_id": "2",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100016073.1.2",
          "form": "시간 날 때 읽으려고 하는편이에요",
          "original_form": "시간 날 때 읽으려고 하는편이에요",
          "speaker_id": "1",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016073.1.3",
          "form": "책 자주 읽으시나요",
          "original_form": "책 자주 읽으시나요",
          "speaker_id": "1",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016073.1.4",
          "form": "저는 이북으로 읽어요",
          "original_form": "저는 이북으로 읽어요",
          "speaker_id": "2",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100016073.1.5",
          "form": "자주는 아니고 폰으로 틈날때 조금씩?",
          "original_form": "자주는 아니고 폰으로 틈날때 조금씩?",
          "speaker_id": "2",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100016073.1.6",
          "form": "이북이 아무래도 편한게 있어요",
          "original_form": "이북이 아무래도 편한게 있어요",
          "speaker_id": "1",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100016073.1.7",
          "form": "공간차지하지도 않고 좋더라구요",
          "original_form": "공간차지하지도 않고 좋더라구요",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100016073.1.8",
          "form": "최근 도서정가제로 이북가격도 오른다던데 어떻게 생각하세요",
          "original_form": "최근 도서정가제로 이북가격도 오른다던데 어떻게 생각하세요",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100016073.1.9",
          "form": "네 추석상여금받으면 이북리더기를 살까 생각중이에요",
          "original_form": "네 추석상여금받으면 이북리더기를 살까 생각중이에요",
          "speaker_id": "2",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100016073.1.10",
          "form": "그래서 밀리의서재랑 리디북스 정기결제해서 쓰도있어요",
          "original_form": "그래서 밀리의서재랑 리디북스 정기결제해서 쓰도있어요",
          "speaker_id": "2",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100016073.1.11",
          "form": "구독서브 괜찮더라구요",
          "original_form": "구독서브 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100016073.1.12",
          "form": "구독 서비스 괜찮더라고요",
          "original_form": "구독 서비스 괜찮더라고요",
          "speaker_id": "1",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100016073.1.13",
          "form": "최근에 뭐 읽으셨나요",
          "original_form": "최근에 뭐 읽으셨나요",
          "speaker_id": "1",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100016073.1.14",
          "form": "저 요새는 재테크 책 많이 읽어요",
          "original_form": "저 요새는 재테크 책 많이 읽어요",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016073.1.15",
          "form": "나는 주싣 대신 달러를 산다",
          "original_form": "나는 주싣 대신 달러를 산다",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016073.1.16",
          "form": "이거 읽고있어요 ㅎ",
          "original_form": "이거 읽고있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016073.1.17",
          "form": "재태크에 관심이 많으시구나",
          "original_form": "재태크에 관심이 많으시구나",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016073.1.18",
          "form": "어때요 읽으면 진짜 도움이 되나요?",
          "original_form": "어때요 읽으면 진짜 도움이 되나요?",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016073.1.19",
          "form": "주변에서 달러투자하라는 얘기를 들었너서 한 번 보고 있어요 ㅎ",
          "original_form": "주변에서 달러투자하라는 얘기를 들었너서 한 번 보고 있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016073.1.20",
          "form": "달러투자 전망은 어때보이나요",
          "original_form": "달러투자 전망은 어때보이나요",
          "speaker_id": "1",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016073.1.21",
          "form": "아무래도 가장 많이쓰는 화폐라",
          "original_form": "아무래도 가장 많이쓰는 화폐라",
          "speaker_id": "1",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016073.1.22",
          "form": "나쁘지않은거같더라고요",
          "original_form": "나쁘지않은거같더라고요",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016073.1.23",
          "form": "음 그러시구나",
          "original_form": "음 그러시구나",
          "speaker_id": "1",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016073.1.24",
          "form": "쌀때 사놨다 오르면 환차익금액만 투자금으로 쓰고 원금은 그냥 두고",
          "original_form": "쌀때 사놨다 오르면 환차익금액만 투자금으로 쓰고 원금은 그냥 두고",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016073.1.25",
          "form": "이런식으로 하라고 하더라고요",
          "original_form": "이런식으로 하라고 하더라고요",
          "speaker_id": "2",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100016073.1.26",
          "form": "아 맞아요 저도 들어본거같아요",
          "original_form": "아 맞아요 저도 들어본거같아요",
          "speaker_id": "1",
          "time": "20210826 11:42"
        }
      ]
    }
  ]
