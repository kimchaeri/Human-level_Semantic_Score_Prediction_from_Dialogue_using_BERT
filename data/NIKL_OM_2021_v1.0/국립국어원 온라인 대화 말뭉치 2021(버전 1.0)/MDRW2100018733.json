        },
        {
          "id": "MDRW2100018733.1.2",
          "form": "응모해서 다행이얍",
          "original_form": "응모해서 다행이얍",
          "speaker_id": "1",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018733.1.3",
          "form": "마자 히히",
          "original_form": "마자 히히",
          "speaker_id": "2",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018733.1.4",
          "form": "대박커~~",
          "original_form": "대박커~~",
          "speaker_id": "2",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018733.1.5",
          "form": "짱신나유~",
          "original_form": "짱신나유~",
          "speaker_id": "1",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018733.1.6",
          "form": "모자도 너무커서 못씌워",
          "original_form": "모자도 너무커서 못씌워",
          "speaker_id": "2",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018733.1.7",
          "form": "진짜귀여버 인형",
          "original_form": "진짜귀여버 인형",
          "speaker_id": "1",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018733.1.8",
          "form": "스너곰 또 사고싶어",
          "original_form": "스너곰 또 사고싶어",
          "speaker_id": "2",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018733.1.9",
          "form": "그 작년꺼?",
          "original_form": "그 작년꺼?",
          "speaker_id": "1",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018733.1.10",
