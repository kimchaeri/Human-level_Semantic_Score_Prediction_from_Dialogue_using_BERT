      "utterance": [
        {
          "id": "MDRW2100005352.1.1",
          "form": "악뮤 노래 듣는 거 있으신가요?",
          "original_form": "악뮤 노래 듣는 거 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005352.1.2",
          "form": "헉 좋아해요!",
          "original_form": "헉 좋아해요!",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005352.1.3",
          "form": "인공잔디랑 사람들이움직이는게",
          "original_form": "인공잔디랑 사람들이움직이는게",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005352.1.4",
          "form": "두개를 좋아해요",
          "original_form": "두개를 좋아해요",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005352.1.5",
          "form": "약간 활발한 노래 좋아하시는구나",
          "original_form": "약간 활발한 노래 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005352.1.6",
          "form": "악뮤 좋아하세요?",
          "original_form": "악뮤 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005352.1.7",
          "form": "사람들이 움직이는게 이거 되게 역동적인 느낌이라 좋은 거 같아요",
          "original_form": "사람들이 움직이는게 이거 되게 역동적인 느낌이라 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005352.1.8",
          "form": "맞아요 뮤비랑 같이봐도 재밌고",
          "original_form": "맞아요 뮤비랑 같이봐도 재밌고",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005352.1.9",
          "form": "네 수현이 목소리를 좋아해서 나오면 드는 편이에요",
          "original_form": "네 수현이 목소리를 좋아해서 나오면 드는 편이에요",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005352.1.10",
          "form": "노래가사가 귀여워서 좋아요",
          "original_form": "노래가사가 귀여워서 좋아요",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005352.1.11",
          "form": "맞아요 가사를 잘 쓰더라구요",
          "original_form": "맞아요 가사를 잘 쓰더라구요",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005352.1.12",
          "form": "목소리정말좋죠... 겨울왕국 커버인가 그것도 자주찾아봤어요",
          "original_form": "목소리정말좋죠... 겨울왕국 커버인가 그것도 자주찾아봤어요",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005352.1.13",
          "form": "사일런스인가... 알라딘거랑",
          "original_form": "사일런스인가... 알라딘거랑",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005352.1.14",
          "form": "맞아요 진짜 맑고 단단해서 잘어울리더라구요",
          "original_form": "맞아요 진짜 맑고 단단해서 잘어울리더라구요",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005352.1.15",
          "form": "커버하는거 자주찾아봐요",
          "original_form": "커버하는거 자주찾아봐요",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005352.1.16",
          "form": "디즈니의 인간화같아요",
          "original_form": "디즈니의 인간화같아요",
          "speaker_id": "1",
          "time": "20210821 21:57"
        }
      ]
