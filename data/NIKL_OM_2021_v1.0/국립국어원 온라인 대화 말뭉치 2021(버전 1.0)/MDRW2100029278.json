      "utterance": [
        {
          "id": "MDRW2100029278.1.1",
          "form": "호빵 무슨 맛 좋아하세요?",
          "original_form": "호빵 무슨 맛 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100029278.1.2",
          "form": "팥!!",
          "original_form": "팥!!",
          "speaker_id": "1",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100029278.1.3",
          "form": "저는 피자호빵이요 ㅎㅎ",
          "original_form": "저는 피자호빵이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100029278.1.4",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029278.1.5",
          "form": "맛있죠,,,",
          "original_form": "맛있죠,,,",
          "speaker_id": "1",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029278.1.6",
          "form": "팥은 달아서 ㅎㅎ",
          "original_form": "팥은 달아서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029278.1.7",
          "form": "팥붕어빵 슈크림붕어빵",
          "original_form": "팥붕어빵 슈크림붕어빵",
          "speaker_id": "1",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029278.1.8",
          "form": "저는 이것도 팥붕",
          "original_form": "저는 이것도 팥붕",
          "speaker_id": "1",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029278.1.9",
          "form": "저 이건 팥붕",
          "original_form": "저 이건 팥붕",
          "speaker_id": "2",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029278.1.10",
          "form": "팥붕은 좀 덜달아요 ㅋㅋ",
          "original_form": "팥붕은 좀 덜달아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029278.1.11",
          "form": "슈크림붕 안먹어봤어요 저",
          "original_form": "슈크림붕 안먹어봤어요 저",
          "speaker_id": "1",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029278.1.12",
          "form": "슈붕드셔보셧어요?",
          "original_form": "슈붕드셔보셧어요?",
          "speaker_id": "1",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029278.1.13",
          "form": "은근 맛있어요",
          "original_form": "은근 맛있어요",
          "speaker_id": "2",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029278.1.14",
          "form": "근데 파는곳마다 맛이 좀 달라요",
          "original_form": "근데 파는곳마다 맛이 좀 달라요",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029278.1.15",
          "form": "먹어볼게요",
          "original_form": "먹어볼게요",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029278.1.16",
          "form": "붕세권이세요?",
          "original_form": "붕세권이세요?",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029278.1.17",
          "form": "맛이 없는 곳도 있어요 약품 맛",
          "original_form": "맛이 없는 곳도 있어요 약품 맛",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029278.1.18",
          "form": "올해는 트럭이 올거 같아요 작년 겨울부터 오시더라고요",
          "original_form": "올해는 트럭이 올거 같아요 작년 겨울부터 오시더라고요",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029278.1.19",
          "form": "사실 저는겨울에 붕어빵보다 호떡파여서 ,,",
          "original_form": "사실 저는겨울에 붕어빵보다 호떡파여서 ,,",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029278.1.20",
          "form": "호떡도 맛있져",
          "original_form": "호떡도 맛있져",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029278.1.21",
          "form": "저는 만들어먹는게 맛있더라고요",
          "original_form": "저는 만들어먹는게 맛있더라고요",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029278.1.22",
          "form": "어 저그거만들어봤는데 생각보다 너무 못만들어서,,,",
          "original_form": "어 저그거만들어봤는데 생각보다 너무 못만들어서,,,",
          "speaker_id": "1",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100029278.1.23",
          "form": "시장표는 제 입맛에 기름져서요",
          "original_form": "시장표는 제 입맛에 기름져서요",
          "speaker_id": "2",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100029278.1.24",
          "form": "ㅋㄱㅋㄱㅋㅋㄱㅋㄱㅋㄱㅋㄱ",
          "original_form": "ㅋㄱㅋㄱㅋㅋㄱㅋㄱㅋㄱㅋㄱ",
          "speaker_id": "1",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100029278.1.25",
          "form": "몇번 만들어보시면 금방 만드실거에요ㅋㅋ",
          "original_form": "몇번 만들어보시면 금방 만드실거에요ㅋㅋ",
