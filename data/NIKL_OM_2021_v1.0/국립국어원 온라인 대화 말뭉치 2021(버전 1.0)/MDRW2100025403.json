      "utterance": [
        {
          "id": "MDRW2100025403.1.1",
          "form": "최근에 구매하신 물건 있으세요?",
          "original_form": "최근에 구매하신 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025403.1.2",
          "form": "네ㅋㅋ우유도 사고 장도봤어요ㅋ",
          "original_form": "네ㅋㅋ우유도 사고 장도봤어요ㅋ",
          "speaker_id": "2",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025403.1.3",
          "form": "이제 명절선물사려구요",
          "original_form": "이제 명절선물사려구요",
          "speaker_id": "2",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025403.1.4",
          "form": "최근에 뭐 구입한거있으세요?",
          "original_form": "최근에 뭐 구입한거있으세요?",
          "speaker_id": "2",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100025403.1.5",
          "form": "저는 주로 먹을거를 많이사요!",
          "original_form": "저는 주로 먹을거를 많이사요!",
          "speaker_id": "1",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100025403.1.6",
          "form": "명절 선물 어떤걸로 준비하세요?",
          "original_form": "명절 선물 어떤걸로 준비하세요?",
          "speaker_id": "1",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100025403.1.7",
          "form": "그거 LG선물세트로 샀어요.ㅋㅋ샴푸 비누ㅋㅋㅋ",
          "original_form": "그거 LG선물세트로 샀어요.ㅋㅋ샴푸 비누ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100025403.1.8",
          "form": "신랑 회사 직원분들꺼샀어요ㅋㅋ드리려구요",
          "original_form": "신랑 회사 직원분들꺼샀어요ㅋㅋ드리려구요",
          "speaker_id": "2",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100025403.1.9",
          "form": "오오 좋죠 !!!",
          "original_form": "오오 좋죠 !!!",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100025403.1.10",
          "form": "네네 비싸서 부담이었는데",
          "original_form": "네네 비싸서 부담이었는데",
          "speaker_id": "2",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100025403.1.11",
          "form": "지마켓에서 저렴하게샀어요ㅋ",
          "original_form": "지마켓에서 저렴하게샀어요ㅋ",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100025403.1.12",
          "form": "지금행사하고있어서요ㅋ",
          "original_form": "지금행사하고있어서요ㅋ",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100025403.1.13",
          "form": "우아 요즘 행사도 많이 하는군요!",
          "original_form": "우아 요즘 행사도 많이 하는군요!",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100025403.1.14",
          "form": "몇개 사셧어요?",
          "original_form": "몇개 사셧어요?",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100025403.1.15",
          "form": "일단 12개샀어요ㅋㅋ",
          "original_form": "일단 12개샀어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100025403.1.16",
          "form": "오오 name2님 중고거래 자주 하세요?",
          "original_form": "오오 &name2&님 중고거래 자주 하세요?",
          "speaker_id": "1",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100025403.1.17",
          "form": "네 많이해요ㅋㅋㅋ 당근이요ㅋ",
          "original_form": "네 많이해요ㅋㅋㅋ 당근이요ㅋ",
          "speaker_id": "2",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100025403.1.18",
          "form": "거래자주하세요?",
          "original_form": "거래자주하세요?",
          "speaker_id": "2",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100025403.1.19",
          "form": "저는 가끔 하는데 안팔려서 슬퍼요 ㅠ",
          "original_form": "저는 가끔 하는데 안팔려서 슬퍼요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100025403.1.20",
          "form": "네네ㅜㅋㅋ저두요. 애들한복이 그대로있네요ㅋ",
          "original_form": "네네ㅜㅋㅋ저두요. 애들한복이 그대로있네요ㅋ",
          "speaker_id": "2",
          "time": "20210903 08:26"
        },
        {
          "id": "MDRW2100025403.1.21",
          "form": "당근 뭐 내놓으셨어요?ㅋ",
          "original_form": "당근 뭐 내놓으셨어요?ㅋ",
          "speaker_id": "2",
          "time": "20210903 11:50"
