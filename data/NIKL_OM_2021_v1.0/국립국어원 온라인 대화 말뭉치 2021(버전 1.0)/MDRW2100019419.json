      "utterance": [
        {
          "id": "MDRW2100019419.1.1",
          "form": "저녁은 드셨나요",
          "original_form": "저녁은 드셨나요",
          "speaker_id": "1",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100019419.1.2",
          "form": "네 금방 먹었어요~",
          "original_form": "네 금방 먹었어요~",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019419.1.3",
          "form": "디저트로 마약옥수수를 먹고 있습니당",
          "original_form": "디저트로 마약옥수수를 먹고 있습니당",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019419.1.4",
          "form": "컥 마약옥수수... 어디서 사셨나요",
          "original_form": "컥 마약옥수수... 어디서 사셨나요",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019419.1.5",
          "form": "후후 만들어먹었어요",
          "original_form": "후후 만들어먹었어요",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019419.1.6",
          "form": "집에 옥수수가 너무 많아서 ㅠㅠ",
          "original_form": "집에 옥수수가 너무 많아서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019419.1.7",
          "form": "오...어케만들어요??",
          "original_form": "오...어케만들어요??",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100019419.1.8",
          "form": "요즘옥수수철인가 친구도 옥수수 엄청 많다던데 ㅋㅋㅋ",
          "original_form": "요즘옥수수철인가 친구도 옥수수 엄청 많다던데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100019419.1.9",
          "form": "버터를 발라서 오븐에 구운다음 양념감자가루를 뿌렸어요!ㅋㅋㅋ",
          "original_form": "버터를 발라서 오븐에 구운다음 양념감자가루를 뿌렸어요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100019419.1.10",
          "form": "name1님은 뭐 드셨나요",
          "original_form": "&name1&님은 뭐 드셨나요",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100019419.1.11",
          "form": "오 양념감자 가루는 파나요따로 ㅜㅜ 꿀맛이겟다",
          "original_form": "오 양념감자 가루는 파나요따로 ㅜㅜ 꿀맛이겟다",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100019419.1.12",
          "form": "저 운동에 방해될까봐 냉동 볶음밥만 조금 먹었어요",
          "original_form": "저 운동에 방해될까봐 냉동 볶음밥만 조금 먹었어요",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100019419.1.13",
          "form": "또 뭐 먹고싶네요ㅋㅋㅋ",
          "original_form": "또 뭐 먹고싶네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100019419.1.14",
          "form": "네네 요즘 인터넷에서 못구하는게 없답니다",
          "original_form": "네네 요즘 인터넷에서 못구하는게 없답니다",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100019419.1.15",
          "form": "냉동 볶음밥은 어디껀가요?",
          "original_form": "냉동 볶음밥은 어디껀가요?",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100019419.1.16",
          "form": "오호  ㅋㅋ 저도사봐야겟내요 움 전 꼬막볶음밥",
          "original_form": "오호  ㅋㅋ 저도사봐야겟내요 움 전 꼬막볶음밥",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100019419.1.17",
          "form": "fresh easy?",
          "original_form": "fresh easy?",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100019419.1.18",
          "form": "와 꼬막볶음밥도 나오다니 새롭네요",
          "original_form": "와 꼬막볶음밥도 나오다니 새롭네요",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100019419.1.19",
          "form": "그냥 야채볶음밥 정도일줄 알았는데ㅋㅋㅋ",
          "original_form": "그냥 야채볶음밥 정도일줄 알았는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100019419.1.20",
          "form": "세상이 좋아졌어요,+",
          "original_form": "세상이 좋아졌어요,+",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100019419.1.21",
          "form": "그죠 ㅋㅋㅋㅋㅋ 뭐 식당서 사먹는거만 못하지만...",
          "original_form": "그죠 ㅋㅋㅋㅋㅋ 뭐 식당서 사먹는거만 못하지만...",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100019419.1.22",
          "form": "맞아요.. 그래도 식당 매일갈순 없으니까!",
          "original_form": "맞아요.. 그래도 식당 매일갈순 없으니까!",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100019419.1.23",
          "form": "식당 자주 가면 필연적으로 살이 찌더라구요..",
          "original_form": "식당 자주 가면 필연적으로 살이 찌더라구요..",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100019419.1.24",
          "form": "마자용 가격도글코",
          "original_form": "마자용 가격도글코",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100019419.1.25",
          "form": "맞아요 집에서 먹으면 훨씬 싸죠!",
          "original_form": "맞아요 집에서 먹으면 훨씬 싸죠!",
          "speaker_id": "2",
          "time": "20210827 23:44"
        }
      ]
