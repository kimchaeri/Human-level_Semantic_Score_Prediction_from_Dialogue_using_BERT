      "utterance": [
        {
          "id": "MDRW2100019921.1.1",
          "form": "저녁식사 하셧나요?",
          "original_form": "저녁식사 하셧나요?",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019921.1.2",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019921.1.3",
          "form": "통햇네요",
          "original_form": "통햇네요",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019921.1.4",
          "form": "아 저 돈까스먹었어요",
          "original_form": "아 저 돈까스먹었어요",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019921.1.5",
          "form": "저는 스파게티 먹엇어요",
          "original_form": "저는 스파게티 먹엇어요",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019921.1.6",
          "form": "어 양식인것도 통했네요ㅎ",
          "original_form": "어 양식인것도 통했네요ㅎ",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019921.1.7",
          "form": "만들어드셨나요?",
          "original_form": "만들어드셨나요?",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019921.1.8",
          "form": "네 ㅎㅎ 스파게티가 제일 쉬운 것 같아요",
          "original_form": "네 ㅎㅎ 스파게티가 제일 쉬운 것 같아요",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019921.1.9",
          "form": "면 삶고 소스 붓고",
          "original_form": "면 삶고 소스 붓고",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019921.1.10",
          "form": "맞아요 요즘 시판소스도 잘나오더라구요",
          "original_form": "맞아요 요즘 시판소스도 잘나오더라구요",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019921.1.11",
          "form": "돈까스는 만드신건가요??",
          "original_form": "돈까스는 만드신건가요??",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019921.1.12",
          "form": "아니오 신포우리에서 사먹었어요ㅎ",
          "original_form": "아니오 신포우리에서 사먹었어요ㅎ",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019921.1.13",
          "form": "처음 먹어봤는데 음...ㅎ",
          "original_form": "처음 먹어봤는데 음...ㅎ",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019921.1.14",
          "form": "말을 아끼겠습니다",
          "original_form": "말을 아끼겠습니다",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019921.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 무슨 뜻인지 알겟습니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 무슨 뜻인지 알겟습니다",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019921.1.16",
          "form": "요리는 하시나요??",
          "original_form": "요리는 하시나요??",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019921.1.17",
          "form": "잘은 못하지만 간단한건 하는편이에요",
          "original_form": "잘은 못하지만 간단한건 하는편이에요",
