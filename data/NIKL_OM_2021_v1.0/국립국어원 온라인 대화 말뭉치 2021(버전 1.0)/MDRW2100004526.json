      "utterance": [
        {
          "id": "MDRW2100004526.1.1",
          "form": "오늘은 진짜 날씨가 요상하네요",
          "original_form": "오늘은 진짜 날씨가 요상하네요",
          "speaker_id": "1",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004526.1.2",
          "form": "태풍 올라오더라구요",
          "original_form": "태풍 올라오더라구요",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004526.1.3",
          "form": "비바람 막 들이치더니 지금은 또 해가 떴네요",
          "original_form": "비바람 막 들이치더니 지금은 또 해가 떴네요",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004526.1.4",
          "form": "저희 지역은 아직도 열심히 비가 내리고 있어요",
          "original_form": "저희 지역은 아직도 열심히 비가 내리고 있어요",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004526.1.5",
          "form": "비가 많이 오나요?",
          "original_form": "비가 많이 오나요?",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004526.1.6",
          "form": "네 비도 많이 오고 바람도 꽤 부는 것 같아요",
          "original_form": "네 비도 많이 오고 바람도 꽤 부는 것 같아요",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004526.1.7",
          "form": "티비 보니까 계속 밑에 속보 뜨더라구요",
          "original_form": "티비 보니까 계속 밑에 속보 뜨더라구요",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004526.1.8",
          "form": "계속 많이오나봐요ㅠㅠ",
          "original_form": "계속 많이오나봐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004526.1.9",
          "form": "여기도 남쪽에 가까운 지역이라 그런가봐요",
          "original_form": "여기도 남쪽에 가까운 지역이라 그런가봐요",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004526.1.10",
          "form": "이번 여름에 그쪽에 비가 많이오더라구요",
          "original_form": "이번 여름에 그쪽에 비가 많이오더라구요",
          "speaker_id": "1",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004526.1.11",
          "form": "좀 골고루 왔으면 좋았을텐데",
          "original_form": "좀 골고루 왔으면 좋았을텐데",
          "speaker_id": "1",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004526.1.12",
          "form": "맞아요 비 피해 많이 없었으면 좋겠네요",
          "original_form": "맞아요 비 피해 많이 없었으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004526.1.13",
          "form": "맞아요 이번에는 피해가 좀 덜하길ㅠㅠ",
          "original_form": "맞아요 이번에는 피해가 좀 덜하길ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004526.1.14",
          "form": "비오니까 날이 더 추워졌어요",
          "original_form": "비오니까 날이 더 추워졌어요",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004526.1.15",
          "form": "태풍 지나가도 가을장마라는거 같더라구요",
          "original_form": "태풍 지나가도 가을장마라는거 같더라구요",
          "speaker_id": "2",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004526.1.16",
          "form": "맞아요 이제 여름은 완전히 갈것같은",
          "original_form": "맞아요 이제 여름은 완전히 갈것같은",
          "speaker_id": "2",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004526.1.17",
          "form": "갑자기 여름이 끝나버린 느낌이에요",
          "original_form": "갑자기 여름이 끝나버린 느낌이에요",
          "speaker_id": "1",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004526.1.18",
          "form": "맞아요 저는 여름 좋아하는데 아쉬워요",
          "original_form": "맞아요 저는 여름 좋아하는데 아쉬워요",
          "speaker_id": "2",
          "time": "20210821 17:10"
        }
