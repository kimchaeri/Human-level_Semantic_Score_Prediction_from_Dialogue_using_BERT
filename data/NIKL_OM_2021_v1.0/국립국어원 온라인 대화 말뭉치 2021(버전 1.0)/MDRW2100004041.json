      "utterance": [
        {
          "id": "MDRW2100004041.1.1",
          "form": "아 요즘 날씨 갑자기 덥지 않냐",
          "original_form": "아 요즘 날씨 갑자기 덥지 않냐",
          "speaker_id": "1",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004041.1.2",
          "form": "요새 시원해지던데요 님 집밖에 안 나가시죠",
          "original_form": "요새 시원해지던데요 님 집밖에 안 나가시죠",
          "speaker_id": "2",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100004041.1.3",
          "form": "오늘 비오기 전까지는 더웠엉",
          "original_form": "오늘 비오기 전까지는 더웠엉",
          "speaker_id": "1",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100004041.1.4",
          "form": "사실 내가 집밖에 안 나가서 잘 몰라",
          "original_form": "사실 내가 집밖에 안 나가서 잘 몰라",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004041.1.5",
          "form": "무슨 날씨 좋아하세요 앵버님",
          "original_form": "무슨 날씨 좋아하세요 앵버님",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004041.1.6",
          "form": "나는 가을날씨!! 적당히 선선하고 습하지 않고 하늘도 맑은거",
          "original_form": "나는 가을날씨!! 적당히 선선하고 습하지 않고 하늘도 맑은거",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004041.1.7",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004041.1.8",
          "form": "난. 내가 집 안에 있으면 무조건 비 엄청 오는 날씨 좋아하구",
          "original_form": "난. 내가 집 안에 있으면 무조건 비 엄청 오는 날씨 좋아하구",
          "speaker_id": "2",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004041.1.9",
          "form": "밖이면 그냥 흐리고 시원한 날씨",
          "original_form": "밖이면 그냥 흐리고 시원한 날씨",
          "speaker_id": "2",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004041.1.10",
          "form": "맑은날 싫어 -.-",
          "original_form": "맑은날 싫어 -.-",
          "speaker_id": "2",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004041.1.11",
          "form": "헐 맑은날 왜 싫어ㅠㅠㅠ",
          "original_form": "헐 맑은날 왜 싫어ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004041.1.12",
          "form": "햇빛 있으면 덥자나",
          "original_form": "햇빛 있으면 덥자나",
          "speaker_id": "2",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004041.1.13",
          "form": "근데 저번처럼 소풍 가는 거면",
          "original_form": "근데 저번처럼 소풍 가는 거면",
          "speaker_id": "2",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004041.1.14",
          "form": "맑은날 용서가능",
          "original_form": "맑은날 용서가능",
          "speaker_id": "2",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004041.1.15",
          "form": "아 당연함 놀러갈때 날씨는 무조건 맑아야 함",
          "original_form": "아 당연함 놀러갈때 날씨는 무조건 맑아야 함",
          "speaker_id": "1",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004041.1.16",
          "form": "당연하지 비오는데 만난다? 약속깨기 마려움",
          "original_form": "당연하지 비오는데 만난다? 약속깨기 마려움",
          "speaker_id": "2",
          "time": "20210821 14:46"
        },
        {
          "id": "MDRW2100004041.1.17",
          "form": "그건 상대방도 동의하는 부분일 것임. 비오면 약속파토는 국룰",
          "original_form": "그건 상대방도 동의하는 부분일 것임. 비오면 약속파토는 국룰",
          "speaker_id": "1",
          "time": "20210821 14:46"
        },
        {
          "id": "MDRW2100004041.1.18",
          "form": "헷헷",
          "original_form": "헷헷",
          "speaker_id": "2",
          "time": "20210821 14:47"
        },
        {
          "id": "MDRW2100004041.1.19",
          "form": "계절과 옷차림 말해본다",
          "original_form": "계절과 옷차림 말해본다",
          "speaker_id": "2",
          "time": "20210821 14:47"
        },
        {
          "id": "MDRW2100004041.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ음 일단 나는 덥든 춥든 한벌옷 좋아함",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ음 일단 나는 덥든 춥든 한벌옷 좋아함",
          "speaker_id": "1",
          "time": "20210821 14:48"
        },
        {
          "id": "MDRW2100004041.1.21",
          "form": "점프슈트나 멜빵같은거><",
          "original_form": "점프슈트나 멜빵같은거><",
          "speaker_id": "1",
