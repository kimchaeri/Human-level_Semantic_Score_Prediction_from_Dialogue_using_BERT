      "utterance": [
        {
          "id": "MDRW2100031171.1.1",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031171.1.2",
          "form": "제가 못하는집에 갔었나",
          "original_form": "제가 못하는집에 갔었나",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031171.1.3",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031171.1.4",
          "form": "네 제가 맛있는데서 먹었나봐요",
          "original_form": "네 제가 맛있는데서 먹었나봐요",
          "speaker_id": "1",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031171.1.5",
          "form": "다른 게 하도 맛없어서 상대적으로 ㅋㅋ",
          "original_form": "다른 게 하도 맛없어서 상대적으로 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031171.1.6",
          "form": "그럴지도요 ㅋㅋㅋ",
          "original_form": "그럴지도요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031171.1.7",
          "form": "영국에서",
          "original_form": "영국에서",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031171.1.8",
          "form": "에어비앤비로 그냥 싸다고 방 예약했다가",
          "original_form": "에어비앤비로 그냥 싸다고 방 예약했다가",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031171.1.9",
          "form": "완전 지하 골방이어서",
          "original_form": "완전 지하 골방이어서",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031171.1.10",
          "form": "취소하고 밤새 방찾았던 기억이 ㅋㅋㅋ",
          "original_form": "취소하고 밤새 방찾았던 기억이 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031171.1.11",
          "form": "홀 에어비앤비로 개인집 예약하신 건가요?",
          "original_form": "홀 에어비앤비로 개인집 예약하신 건가요?",
          "speaker_id": "1",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031171.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031171.1.13",
          "form": "혼자갔거든요",
          "original_form": "혼자갔거든요",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031171.1.14",
          "form": "저도 혼자 갔는데 전 그냥 호스텔로 갔었어요 ㅋㅋ",
          "original_form": "저도 혼자 갔는데 전 그냥 호스텔로 갔었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031171.1.15",
          "form": "에어비앤비도 뭔가 로망이 있었는데..",
          "original_form": "에어비앤비도 뭔가 로망이 있었는데..",
          "speaker_id": "1",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031171.1.16",
          "form": "그땐 그게 제일 싸서요 ㅋㅋ",
          "original_form": "그땐 그게 제일 싸서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100031171.1.17",
          "form": "오 저렴했군요 의외네요",
          "original_form": "오 저렴했군요 의외네요",
          "speaker_id": "1",
          "time": "20210904 17:54"
        },
        {
          "id": "MDRW2100031171.1.18",
          "form": "근데 예약하고나면 쓰기는 편하겠어요 자유롭고",
          "original_form": "근데 예약하고나면 쓰기는 편하겠어요 자유롭고",
          "speaker_id": "1",
          "time": "20210904 17:54"
        },
        {
          "id": "MDRW2100031171.1.19",
          "form": "프랑스땐 완전 대성공이었거든요",
          "original_form": "프랑스땐 완전 대성공이었거든요",
          "speaker_id": "2",
          "time": "20210904 17:54"
        },
        {
          "id": "MDRW2100031171.1.20",
          "form": "오 방이 괜찮았나요",
          "original_form": "오 방이 괜찮았나요",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100031171.1.21",
          "form": "네 집 통째로 빌렸는데",
          "original_form": "네 집 통째로 빌렸는데",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100031171.1.22",
          "form": "완전 좋았고",
          "original_form": "완전 좋았고",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100031171.1.23",
          "form": "집에서 에펠탑이 보였어요",
          "original_form": "집에서 에펠탑이 보였어요",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100031171.1.24",
          "form": "와 대박이네요 ㅋㅋㅋ",
          "original_form": "와 대박이네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100031171.1.25",
          "form": "그것만으로도",
          "original_form": "그것만으로도",
          "speaker_id": "1",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100031171.1.26",
          "form": "그래서 에어비앤비 무한신뢰 ㅋㅋㅋㅋ",
          "original_form": "그래서 에어비앤비 무한신뢰 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:58"
        }
      ]
    }
  ]
