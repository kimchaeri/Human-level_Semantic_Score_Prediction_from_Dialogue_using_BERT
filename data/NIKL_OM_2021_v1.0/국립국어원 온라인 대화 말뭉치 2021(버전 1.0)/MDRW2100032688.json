      "utterance": [
        {
          "id": "MDRW2100032688.1.1",
          "form": "오늘의집인지 지름인지",
          "original_form": "오늘의집인지 지름인지",
          "speaker_id": "2",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100032688.1.2",
          "form": "보면 사고싶어지는 것 같아여",
          "original_form": "보면 사고싶어지는 것 같아여",
          "speaker_id": "2",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100032688.1.3",
          "form": "ㅋㅋㅋㅋㅋ아 너무 웃겨요 공감이요",
          "original_form": "ㅋㅋㅋㅋㅋ아 너무 웃겨요 공감이요",
          "speaker_id": "1",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100032688.1.4",
          "form": "아니 왤케 잘 꾸며놨는지 모르겠어요...",
          "original_form": "아니 왤케 잘 꾸며놨는지 모르겠어요...",
          "speaker_id": "1",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100032688.1.5",
          "form": "암튼 1인용 쇼파 리클라이너 사고싶네요",
          "original_form": "암튼 1인용 쇼파 리클라이너 사고싶네요",
          "speaker_id": "2",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100032688.1.6",
          "form": "아 곧 겨울이니 러그도",
          "original_form": "아 곧 겨울이니 러그도",
          "speaker_id": "2",
          "time": "20210905 22:21"
        },
        {
          "id": "MDRW2100032688.1.7",
          "form": "헉 저도 쇼파 사고 싶은데ㅋㅋㅋ",
          "original_form": "헉 저도 쇼파 사고 싶은데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:21"
        },
        {
          "id": "MDRW2100032688.1.8",
          "form": "저는 러그도 좋은데 카페트가 더 좋아요",
          "original_form": "저는 러그도 좋은데 카페트가 더 좋아요",
          "speaker_id": "1",
          "time": "20210905 22:21"
        },
        {
          "id": "MDRW2100032688.1.9",
          "form": "근데 부피 무게 배송료 때문에 ㅎㅎ",
          "original_form": "근데 부피 무게 배송료 때문에 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 22:21"
        },
        {
          "id": "MDRW2100032688.1.10",
          "form": "아 두개 차이가 뭐예요",
          "original_form": "아 두개 차이가 뭐예요",
          "speaker_id": "2",
          "time": "20210905 22:21"
        },
        {
          "id": "MDRW2100032688.1.11",
          "form": "음 러그는 약간 뽀송~? 카페트는 좀 꺼슬~?",
          "original_form": "음 러그는 약간 뽀송~? 카페트는 좀 꺼슬~?",
          "speaker_id": "1",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100032688.1.12",
          "form": "아 카페트는 왠지 자주 못빨지 않아요?",
          "original_form": "아 카페트는 왠지 자주 못빨지 않아요?",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032688.1.13",
          "form": "러그는 세탁가능한게 좀 있더라구요",
          "original_form": "러그는 세탁가능한게 좀 있더라구요",
          "speaker_id": "2",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032688.1.14",
          "form": "그쵸ㅠㅠ 저는 그래서 청소기로만 밀어주구 가끔만 빨래해요",
          "original_form": "그쵸ㅠㅠ 저는 그래서 청소기로만 밀어주구 가끔만 빨래해요",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032688.1.15",
          "form": "색깔은 어떤거 사셨어요?",
          "original_form": "색깔은 어떤거 사셨어요?",
          "speaker_id": "2",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100032688.1.16",
          "form": "약간 베이지랑 자주색이랑 짙은 남색 섞인~? 그런 카펫이에용",
          "original_form": "약간 베이지랑 자주색이랑 짙은 남색 섞인~? 그런 카펫이에용",
          "speaker_id": "1",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100032688.1.17",
          "form": "상상이 잘 안가요 ㅎㅎ",
          "original_form": "상상이 잘 안가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100032688.1.18",
          "form": "무늬가 있나요 전 산다면 그냥 단색으로 하려는데",
          "original_form": "무늬가 있나요 전 산다면 그냥 단색으로 하려는데",
          "speaker_id": "2",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100032688.1.19",
          "form": "베이지는 때 잘 타려마요",
          "original_form": "베이지는 때 잘 타려마요",
          "speaker_id": "2",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100032688.1.20",
          "form": "네네 무늬 있어요!!",
          "original_form": "네네 무늬 있어요!!",
          "speaker_id": "1",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100032688.1.21",
          "form": "이게 때가 잘 타는 재질이면 잘 타더라고요",
          "original_form": "이게 때가 잘 타는 재질이면 잘 타더라고요",
          "speaker_id": "1",
          "time": "20210905 22:30"
