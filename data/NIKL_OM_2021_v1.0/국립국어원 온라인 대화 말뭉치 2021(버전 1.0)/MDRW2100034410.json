      "utterance": [
        {
          "id": "MDRW2100034410.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210828 09:36"
        },
        {
          "id": "MDRW2100034410.1.2",
          "form": "안녕하세요 점심 드셨나요?",
          "original_form": "안녕하세요 점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100034410.1.3",
          "form": "점심으로 피자시켰어요 ㅎㅎ",
          "original_form": "점심으로 피자시켰어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034410.1.4",
          "form": "으아 피자 맛있겠네요 어디거 시키셨나요",
          "original_form": "으아 피자 맛있겠네요 어디거 시키셨나요",
          "speaker_id": "2",
          "time": "20210828 16:21"
        },
        {
          "id": "MDRW2100034410.1.5",
          "form": "저는 파파존스요~ 여기것만먹어요 ㅎㄹ",
          "original_form": "저는 파파존스요~ 여기것만먹어요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100034410.1.6",
          "form": "저희 집 근처에는 파파존스가 없어서 먹어본적이 없어요ㅠ",
          "original_form": "저희 집 근처에는 파파존스가 없어서 먹어본적이 없어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100034410.1.7",
          "form": "아하 ㅜㅜ체인인데 지점이 왤케 적나몰라요ㅜ",
          "original_form": "아하 ㅜㅜ체인인데 지점이 왤케 적나몰라요ㅜ",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100034410.1.8",
          "form": "하나 생겼으면 좋겠어요 근처에",
          "original_form": "하나 생겼으면 좋겠어요 근처에",
          "speaker_id": "2",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100034410.1.9",
          "form": "생기면 꼭드셔보세요!! 완전 미국맛!",
          "original_form": "생기면 꼭드셔보세요!! 완전 미국맛!",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100034410.1.10",
          "form": "추천 감사합니다",
          "original_form": "추천 감사합니다",
          "speaker_id": "2",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100034410.1.11",
          "form": "아녀요 좋은건 다같이공유해야져 ㅋㅋㅋ 점심뭐드셨어요~",
          "original_form": "아녀요 좋은건 다같이공유해야져 ㅋㅋㅋ 점심뭐드셨어요~",
          "speaker_id": "1",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100034410.1.12",
          "form": "어제 남은 된장 짜글이에 밥 비벼먹었어요",
          "original_form": "어제 남은 된장 짜글이에 밥 비벼먹었어요",
          "speaker_id": "2",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034410.1.13",
          "form": "오오 맛나겠당 짜글이! 안먹은지오래되서 맛도 가물가물하네욬ㅋㅋ",
          "original_form": "오오 맛나겠당 짜글이! 안먹은지오래되서 맛도 가물가물하네욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034410.1.14",
          "form": "저도 진짜 오랜만에 먹었어요ㅋㅋ",
          "original_form": "저도 진짜 오랜만에 먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100034410.1.15",
          "form": "든든하게 먹을거 찾아보다가 먹은거라",
          "original_form": "든든하게 먹을거 찾아보다가 먹은거라",
          "speaker_id": "2",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100034410.1.16",
          "form": "마쟈요 ㅋㅋ 특히 감자넣은 짜글이는 포만감 쩔죠",
          "original_form": "마쟈요 ㅋㅋ 특히 감자넣은 짜글이는 포만감 쩔죠",
          "speaker_id": "1",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100034410.1.17",
          "form": "맞아요 감자 넣으면 좋죠",
          "original_form": "맞아요 감자 넣으면 좋죠",
          "speaker_id": "2",
          "time": "20210828 19:13"
        }
      ]
