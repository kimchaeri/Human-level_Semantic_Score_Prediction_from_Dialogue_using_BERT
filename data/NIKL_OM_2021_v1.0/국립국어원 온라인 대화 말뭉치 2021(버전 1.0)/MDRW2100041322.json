      "utterance": [
        {
          "id": "MDRW2100041322.1.1",
          "form": "거기 목뼈는 그냥 씹어도 돼용",
          "original_form": "거기 목뼈는 그냥 씹어도 돼용",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100041322.1.2",
          "form": "사회적 지위와 체면이 있으니 그렇게는 안하지만ㅎ",
          "original_form": "사회적 지위와 체면이 있으니 그렇게는 안하지만ㅎ",
          "speaker_id": "1",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100041322.1.3",
          "form": "와 목뼈를 그냥 씹는다구용!!??대박!",
          "original_form": "와 목뼈를 그냥 씹는다구용!!??대박!",
          "speaker_id": "2",
          "time": "20210905 22:19"
        },
        {
          "id": "MDRW2100041322.1.4",
          "form": "교촌만..",
          "original_form": "교촌만..",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100041322.1.5",
          "form": "어린이는 따라하지 마세요!",
          "original_form": "어린이는 따라하지 마세요!",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100041322.1.6",
          "form": "근데 저 목뼈는 다 좋아하긴 해요ㅎ",
          "original_form": "근데 저 목뼈는 다 좋아하긴 해요ㅎ",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100041322.1.7",
          "form": "중국서 나름 국민간식? 중에 오리목뼈가 있는데",
          "original_form": "중국서 나름 국민간식? 중에 오리목뼈가 있는데",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100041322.1.8",
          "form": "한창 집에서 그걸로 혼술했죠",
          "original_form": "한창 집에서 그걸로 혼술했죠",
          "speaker_id": "1",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100041322.1.9",
          "form": "와....진짜 장난아닙니다...멋있으셔...",
          "original_form": "와....진짜 장난아닙니다...멋있으셔...",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041322.1.10",
          "form": "닭뼈보다 살이 많아요!",
          "original_form": "닭뼈보다 살이 많아요!",
          "speaker_id": "1",
          "time": "20210905 22:56"
        },
        {
          "id": "MDRW2100041322.1.11",
          "form": "염장? 돼 있어서 살짝 장조림 삘도 납니다",
          "original_form": "염장? 돼 있어서 살짝 장조림 삘도 납니다",
          "speaker_id": "1",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100041322.1.12",
          "form": "무엇보다 살이 안찔것 같은 느낌적인 느낌",
          "original_form": "무엇보다 살이 안찔것 같은 느낌적인 느낌",
          "speaker_id": "1",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100041322.1.13",
          "form": "뭔지 알쥬?",
          "original_form": "뭔지 알쥬?",
          "speaker_id": "1",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100041322.1.14",
          "form": "살이안찔거같은 느낌적인 느낌이든다면\n더더욱 손이가쥬",
          "original_form": "살이안찔거같은 느낌적인 느낌이든다면\n더더욱 손이가쥬",
          "speaker_id": "2",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100041322.1.15",
          "form": "ㅋㅋㅋㅋㅋㄱㄲ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㄲ",
          "speaker_id": "2",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100041322.1.16",
          "form": "광군제때 더 질렀어야 하는데ㅎ",
          "original_form": "광군제때 더 질렀어야 하는데ㅎ",
          "speaker_id": "1",
          "time": "20210906 01:03"
        },
        {
          "id": "MDRW2100041322.1.17",
          "form": "ㅋㅋㅋㅋㅋ저는 지마켓에서 펩시와 떡볶이지르고왔습니다..",
          "original_form": "ㅋㅋㅋㅋㅋ저는 지마켓에서 펩시와 떡볶이지르고왔습니다..",
          "speaker_id": "2",
          "time": "20210906 01:26"
        },
        {
          "id": "MDRW2100041322.1.18",
          "form": "아니ㅋ 이 시간에요ㅋㅋ?",
          "original_form": "아니ㅋ 이 시간에요ㅋㅋ?",
          "speaker_id": "1",
          "time": "20210906 01:33"
        },
        {
          "id": "MDRW2100041322.1.19",
          "form": "지금 허기진 거 아닙니꽈!",
          "original_form": "지금 허기진 거 아닙니꽈!",
          "speaker_id": "1",
          "time": "20210906 01:34"
        },
        {
          "id": "MDRW2100041322.1.20",
          "form": "이 시간에 지름신이 내리다니ㅇㅅㅇ",
          "original_form": "이 시간에 지름신이 내리다니ㅇㅅㅇ",
          "speaker_id": "1",
          "time": "20210906 01:34"
        },
        {
          "id": "MDRW2100041322.1.21",
          "form": "그런거같습니다ㅋㅋㄱ\n마침 특가 정보를 발견하고 지나칠수없었습니더..",
          "original_form": "그런거같습니다ㅋㅋㄱ\n마침 특가 정보를 발견하고 지나칠수없었습니더..",
          "speaker_id": "2",
          "time": "20210906 01:56"
        },
        {
          "id": "MDRW2100041322.1.22",
          "form": "ㅋㅋㄱㅈㅋ",
          "original_form": "ㅋㅋㄱㅈㅋ",
          "speaker_id": "2",
          "time": "20210906 01:56"
        },
        {
          "id": "MDRW2100041322.1.23",
          "form": "떡볶이 광인 혈육에게 알려줬어야 하는데ㅋㅋ",
          "original_form": "떡볶이 광인 혈육에게 알려줬어야 하는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 09:03"
        },
        {
          "id": "MDRW2100041322.1.24",
          "form": "떡볶이광ㅋㅋㅋㅋㅋ",
          "original_form": "떡볶이광ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 09:53"
        },
        {
          "id": "MDRW2100041322.1.25",
          "form": "님이랑 아주 잘 맞을 거예요ㅎ",
          "original_form": "님이랑 아주 잘 맞을 거예요ㅎ",
          "speaker_id": "1",
          "time": "20210906 09:55"
        },
        {
          "id": "MDRW2100041322.1.26",
          "form": "떡볶이 달콤한 거 좋아하고",
          "original_form": "떡볶이 달콤한 거 좋아하고",
          "speaker_id": "1",
          "time": "20210906 09:55"
        },
        {
          "id": "MDRW2100041322.1.27",
          "form": "오오오오  거기 영혼의단짝이  한명 더 있네...",
          "original_form": "오오오오  거기 영혼의단짝이  한명 더 있네...",
          "speaker_id": "2",
          "time": "20210906 09:56"
        },
        {
          "id": "MDRW2100041322.1.28",
          "form": "ㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210906 09:56"
        },
        {
          "id": "MDRW2100041322.1.29",
          "form": "제가 사랑하는 대부분을 극혐하쥬",
          "original_form": "제가 사랑하는 대부분을 극혐하쥬",
          "speaker_id": "1",
          "time": "20210906 09:59"
        },
        {
          "id": "MDRW2100041322.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ같이 메뉴정하기 힘들겠는데요\n어떠세요?ㅋㅋㅋ\n저는 동생이랑 배달 시킬때 안맞아서괴로운데..ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ같이 메뉴정하기 힘들겠는데요\n어떠세요?ㅋㅋㅋ\n저는 동생이랑 배달 시킬때 안맞아서괴로운데..ㅋㅋㅋ",
