      "utterance": [
        {
          "id": "MDRW2100027007.1.1",
          "form": "안녕하세요~ name2은 좋아하는 계절 있나요?",
          "original_form": "안녕하세요~ &name2&은 좋아하는 계절 있나요?",
          "speaker_id": "1",
          "time": "20210902 06:39"
        },
        {
          "id": "MDRW2100027007.1.2",
          "form": "겨울이요~",
          "original_form": "겨울이요~",
          "speaker_id": "2",
          "time": "20210902 07:09"
        },
        {
          "id": "MDRW2100027007.1.3",
          "form": "더운게 제일 싫어요",
          "original_form": "더운게 제일 싫어요",
          "speaker_id": "2",
          "time": "20210902 07:09"
        },
        {
          "id": "MDRW2100027007.1.4",
          "form": "더운게 제일 싫은건 저랑 같네요..",
          "original_form": "더운게 제일 싫은건 저랑 같네요..",
          "speaker_id": "1",
          "time": "20210902 07:11"
        },
        {
          "id": "MDRW2100027007.1.5",
          "form": "좋아하는 계절은 전 가을이지만요..ㅎㅎ",
          "original_form": "좋아하는 계절은 전 가을이지만요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 07:11"
        },
        {
          "id": "MDRW2100027007.1.6",
          "form": "겨울에 추위는 잘 안타나봐요",
          "original_form": "겨울에 추위는 잘 안타나봐요",
          "speaker_id": "1",
          "time": "20210902 07:11"
        },
        {
          "id": "MDRW2100027007.1.7",
          "form": "네 더위를 심하게 타서",
          "original_form": "네 더위를 심하게 타서",
          "speaker_id": "2",
          "time": "20210902 07:12"
        },
        {
          "id": "MDRW2100027007.1.8",
          "form": "겨울에도 땀 흘려요ㅎ",
          "original_form": "겨울에도 땀 흘려요ㅎ",
          "speaker_id": "2",
          "time": "20210902 07:12"
        },
        {
          "id": "MDRW2100027007.1.9",
          "form": "요즘 진짜 지구온난화때문인지",
          "original_form": "요즘 진짜 지구온난화때문인지",
          "speaker_id": "2",
          "time": "20210902 07:12"
        },
        {
          "id": "MDRW2100027007.1.10",
          "form": "기상이변때문에 여름은 극하게 덥도",
          "original_form": "기상이변때문에 여름은 극하게 덥도",
          "speaker_id": "2",
          "time": "20210902 07:12"
        },
        {
          "id": "MDRW2100027007.1.11",
          "form": "겨울은 극하게 춥고ㅎㅎ",
          "original_form": "겨울은 극하게 춥고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 07:12"
        },
        {
          "id": "MDRW2100027007.1.12",
          "form": "겨울에도 땀을 흘려요.....?",
          "original_form": "겨울에도 땀을 흘려요.....?",
          "speaker_id": "1",
          "time": "20210902 07:13"
        },
        {
          "id": "MDRW2100027007.1.13",
          "form": "와.. 정말 더위를 심하게 타는군요",
          "original_form": "와.. 정말 더위를 심하게 타는군요",
          "speaker_id": "1",
          "time": "20210902 07:14"
        },
        {
          "id": "MDRW2100027007.1.14",
          "form": "그리고 언젠가부터",
          "original_form": "그리고 언젠가부터",
          "speaker_id": "2",
          "time": "20210902 07:55"
        },
        {
          "id": "MDRW2100027007.1.15",
          "form": "봄과 가을이 사라지고 있지",
          "original_form": "봄과 가을이 사라지고 있지",
          "speaker_id": "2",
          "time": "20210902 07:55"
        },
        {
          "id": "MDRW2100027007.1.16",
          "form": "않나요ㅜ",
          "original_form": "않나요ㅜ",
          "speaker_id": "2",
          "time": "20210902 07:55"
        },
        {
          "id": "MDRW2100027007.1.17",
          "form": "그게 젤 아쉽죠",
          "original_form": "그게 젤 아쉽죠",
          "speaker_id": "1",
          "time": "20210902 07:58"
        },
        {
          "id": "MDRW2100027007.1.18",
          "form": "가을을 온전히 즐기기도 전에.. 금방 지나가요",
          "original_form": "가을을 온전히 즐기기도 전에.. 금방 지나가요",
          "speaker_id": "1",
          "time": "20210902 07:58"
        },
        {
          "id": "MDRW2100027007.1.19",
          "form": "가을은 왜 좋아하시는거예요?",
          "original_form": "가을은 왜 좋아하시는거예요?",
          "speaker_id": "2",
          "time": "20210902 08:05"
        },
        {
          "id": "MDRW2100027007.1.20",
          "form": "가을 단풍을 엄청 좋아해요~ ㅎㅎ",
          "original_form": "가을 단풍을 엄청 좋아해요~ ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 08:08"
        },
        {
          "id": "MDRW2100027007.1.21",
          "form": "아~~ 가을 좋죠",
          "original_form": "아~~ 가을 좋죠",
          "speaker_id": "2",
          "time": "20210902 08:21"
        },
        {
          "id": "MDRW2100027007.1.22",
          "form": "겨울 다음으로 좋아하는게 가을이에요",
          "original_form": "겨울 다음으로 좋아하는게 가을이에요",
          "speaker_id": "2",
          "time": "20210902 08:21"
        },
        {
          "id": "MDRW2100027007.1.23",
          "form": "선선하고 봄여름보다 벌레가 없어서ㅋㅋㅋ",
          "original_form": "선선하고 봄여름보다 벌레가 없어서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 08:21"
        },
        {
          "id": "MDRW2100027007.1.24",
          "form": "맞아요.. 가을은 선선하고 공기가 상쾌해요",
          "original_form": "맞아요.. 가을은 선선하고 공기가 상쾌해요",
          "speaker_id": "1",
          "time": "20210902 09:08"
        },
        {
          "id": "MDRW2100027007.1.25",
          "form": "기다리던 가을일텐데",
          "original_form": "기다리던 가을일텐데",
          "speaker_id": "2",
          "time": "20210902 12:32"
        },
        {
          "id": "MDRW2100027007.1.26",
          "form": "어디가기더 애매해서 어트케여",
          "original_form": "어디가기더 애매해서 어트케여",
          "speaker_id": "2",
          "time": "20210902 12:32"
        },
        {
          "id": "MDRW2100027007.1.27",
          "form": "지금 그래요.. 비도 자꾸 오고.. 아직 온전한 가을이 안됐어요",
          "original_form": "지금 그래요.. 비도 자꾸 오고.. 아직 온전한 가을이 안됐어요",
