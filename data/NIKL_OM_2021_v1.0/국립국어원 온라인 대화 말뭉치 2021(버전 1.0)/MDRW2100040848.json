      "utterance": [
        {
          "id": "MDRW2100040848.1.1",
          "form": "안타깝게도.. 아침은 패스하고 우유 한잔 마셨답니다",
          "original_form": "안타깝게도.. 아침은 패스하고 우유 한잔 마셨답니다",
          "speaker_id": "2",
          "time": "20210905 09:50"
        },
        {
          "id": "MDRW2100040848.1.2",
          "form": "아하.. 그냥 흰우유요?",
          "original_form": "아하.. 그냥 흰우유요?",
          "speaker_id": "1",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100040848.1.3",
          "form": "네네 ㅋㅋㅋ 정말 그냥 흰 우유요",
          "original_form": "네네 ㅋㅋㅋ 정말 그냥 흰 우유요",
          "speaker_id": "2",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100040848.1.4",
          "form": "유제품을 잘 드시나요?",
          "original_form": "유제품을 잘 드시나요?",
          "speaker_id": "2",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100040848.1.5",
          "form": "ㅋㅋㅋ 사실 잘 못먹어요 약간의 유당불내증 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 사실 잘 못먹어요 약간의 유당불내증 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100040848.1.6",
          "form": "그래서 집에있을때만 먹습니다 ㅋㅋ",
          "original_form": "그래서 집에있을때만 먹습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100040848.1.7",
          "form": "아 그러면 많이 드시게 힘들죠",
          "original_form": "아 그러면 많이 드시게 힘들죠",
          "speaker_id": "2",
          "time": "20210905 10:06"
        },
        {
          "id": "MDRW2100040848.1.8",
          "form": "저는 유제품을 입에 거의 달고 살아요 ㅋㅋㅋ",
          "original_form": "저는 유제품을 입에 거의 달고 살아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:07"
        },
        {
          "id": "MDRW2100040848.1.9",
          "form": "그래서 아침도 적당히 때운거구요",
          "original_form": "그래서 아침도 적당히 때운거구요",
          "speaker_id": "2",
          "time": "20210905 10:07"
        },
        {
          "id": "MDRW2100040848.1.10",
          "form": "부러워요 ㅜㅜ",
          "original_form": "부러워요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 10:07"
        },
        {
          "id": "MDRW2100040848.1.11",
          "form": "아점으로 어떤거 드실 예정이세요??",
          "original_form": "아점으로 어떤거 드실 예정이세요??",
          "speaker_id": "2",
          "time": "20210905 10:07"
        },
        {
          "id": "MDRW2100040848.1.12",
          "form": "전그냥 주는대로 ㅋㅋㅋㅋㅋ선택권이없어서요 ㅋㅋㅋ",
          "original_form": "전그냥 주는대로 ㅋㅋㅋㅋㅋ선택권이없어서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:08"
        },
        {
          "id": "MDRW2100040848.1.13",
          "form": "아핳ㅋㅋㅋㅋ 가족들과 함께 드시는군요",
          "original_form": "아핳ㅋㅋㅋㅋ 가족들과 함께 드시는군요",
          "speaker_id": "2",
          "time": "20210905 10:08"
        },
        {
          "id": "MDRW2100040848.1.14",
          "form": "네 ㅋㅋㅋㅋ 어디감히 제가 메뉴를 고를수가 없습니다 ㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ 어디감히 제가 메뉴를 고를수가 없습니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:10"
        },
        {
          "id": "MDRW2100040848.1.15",
          "form": "유제품 어떤거 주로 드세요?!",
          "original_form": "유제품 어떤거 주로 드세요?!",
          "speaker_id": "1",
          "time": "20210905 10:10"
        },
        {
          "id": "MDRW2100040848.1.16",
          "form": "어유 그렇죠 ㅋㅋㅋ",
          "original_form": "어유 그렇죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:11"
        },
        {
          "id": "MDRW2100040848.1.17",
          "form": "치즈 엄청 좋아하구요",
          "original_form": "치즈 엄청 좋아하구요",
          "speaker_id": "2",
          "time": "20210905 10:11"
        },
        {
          "id": "MDRW2100040848.1.18",
          "form": "요구르트도 좋아해요",
          "original_form": "요구르트도 좋아해요",
          "speaker_id": "2",
          "time": "20210905 10:11"
        },
        {
          "id": "MDRW2100040848.1.19",
          "form": "치즈! 좋지요!",
          "original_form": "치즈! 좋지요!",
          "speaker_id": "1",
          "time": "20210905 10:11"
        },
        {
          "id": "MDRW2100040848.1.20",
          "form": "우유 버터 요거트",
          "original_form": "우유 버터 요거트",
          "speaker_id": "2",
          "time": "20210905 10:11"
        },
        {
          "id": "MDRW2100040848.1.21",
          "form": "전부 다 좋아하는데",
          "original_form": "전부 다 좋아하는데",
          "speaker_id": "2",
          "time": "20210905 10:11"
        },
        {
          "id": "MDRW2100040848.1.22",
          "form": "어제 요구르트 샀는데! ㅋㅋ",
          "original_form": "어제 요구르트 샀는데! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:11"
        },
        {
          "id": "MDRW2100040848.1.23",
          "form": "어제 요구르트를 샀어요 ㅋㅋㅋ 마침 ㅋㅋㅋ",
          "original_form": "어제 요구르트를 샀어요 ㅋㅋㅋ 마침 ㅋㅋㅋ",
