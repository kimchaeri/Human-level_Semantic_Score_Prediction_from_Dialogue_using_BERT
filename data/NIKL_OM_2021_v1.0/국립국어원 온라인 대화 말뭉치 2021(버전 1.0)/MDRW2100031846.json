        {
          "id": "MDRW2100031846.1.1",
          "form": "근데 왜 바꾸셨어요?",
          "original_form": "근데 왜 바꾸셨어요?",
          "speaker_id": "2",
          "time": "20210905 02:47"
        },
        {
          "id": "MDRW2100031846.1.2",
          "form": "엄기준이 요즘",
          "original_form": "엄기준이 요즘",
          "speaker_id": "1",
          "time": "20210905 02:47"
        },
        {
          "id": "MDRW2100031846.1.3",
          "form": "노래를 잘 못한대서요 ㅋㅋ",
          "original_form": "노래를 잘 못한대서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 02:47"
        },
        {
          "id": "MDRW2100031846.1.4",
          "form": "펜하 때문일까요?",
          "original_form": "펜하 때문일까요?",
          "speaker_id": "2",
          "time": "20210905 02:48"
        },
        {
          "id": "MDRW2100031846.1.5",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20210905 02:48"
        },
        {
          "id": "MDRW2100031846.1.6",
          "form": "후회 안되세요?",
          "original_form": "후회 안되세요?",
          "speaker_id": "2",
          "time": "20210905 02:48"
        },
        {
          "id": "MDRW2100031846.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210905 02:48"
        },
        {
          "id": "MDRW2100031846.1.8",
          "form": "오히려 잘한 선택 같아요",
          "original_form": "오히려 잘한 선택 같아요",
          "speaker_id": "1",
          "time": "20210905 02:48"
        },
        {
          "id": "MDRW2100031846.1.9",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210905 02:48"
        },
        {
          "id": "MDRW2100031846.1.10",
          "form": "윤도현 노래 넘 잘해서요 ㅎㅎ",
          "original_form": "윤도현 노래 넘 잘해서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 02:48"
        },
        {
          "id": "MDRW2100031846.1.11",
          "form": "노래가 많아요?",
          "original_form": "노래가 많아요?",
          "speaker_id": "2",
          "time": "20210905 02:48"
        },
        {
          "id": "MDRW2100031846.1.12",
          "form": "네 완전",
          "original_form": "네 완전",
          "speaker_id": "1",
          "time": "20210905 02:48"
        },
        {
          "id": "MDRW2100031846.1.13",
          "form": "연기는 별로 안 중요한거 같아요",
          "original_form": "연기는 별로 안 중요한거 같아요",
          "speaker_id": "1",
          "time": "20210905 02:48"
        },
        {
          "id": "MDRW2100031846.1.14",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210905 02:49"
        },
        {
          "id": "MDRW2100031846.1.15",
          "form": "네 볼만 했엉ㅅ",
          "original_form": "네 볼만 했엉ㅅ",
          "speaker_id": "1",
          "time": "20210905 02:49"
        },
        {
          "id": "MDRW2100031846.1.16",
          "form": "했어요",
          "original_form": "했어요",
          "speaker_id": "1",
          "time": "20210905 02:49"
        },
        {
          "id": "MDRW2100031846.1.17",
          "form": "저도 보고싶네요",
