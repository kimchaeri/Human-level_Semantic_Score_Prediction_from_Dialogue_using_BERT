        },
        {
          "id": "MDRW2100033946.1.4",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100033946.1.5",
          "form": "강아지랑 고양이",
          "original_form": "강아지랑 고양이",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100033946.1.6",
          "form": "너도 좋아하잖아 대형견",
          "original_form": "너도 좋아하잖아 대형견",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100033946.1.7",
          "form": "맞지",
          "original_form": "맞지",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100033946.1.8",
          "form": "알레르기 때문에 도망다니지만",
          "original_form": "알레르기 때문에 도망다니지만",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100033946.1.9",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100033946.1.10",
          "form": "랜선 집사 하자",
          "original_form": "랜선 집사 하자",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100033946.1.11",
          "form": "맞지 유튜브 보면서 힐링...",
          "original_form": "맞지 유튜브 보면서 힐링...",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100033946.1.12",
          "form": "너무 귀여워",
          "original_form": "너무 귀여워",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100033946.1.13",
          "form": "너는 강아지랑 고양이 중에 뭐가 좋아?",
          "original_form": "너는 강아지랑 고양이 중에 뭐가 좋아?",
          "speaker_id": "1",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100033946.1.14",
          "form": "나는 고양이",
          "original_form": "나는 고양이",
          "speaker_id": "2",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100033946.1.15",
          "form": "너는 개?",
          "original_form": "너는 개?",
          "speaker_id": "2",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100033946.1.16",
