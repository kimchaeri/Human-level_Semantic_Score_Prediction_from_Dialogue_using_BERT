      "utterance": [
        {
          "id": "MDRW2100035768.1.1",
          "form": "안좋아하는 음식 있으세요?",
          "original_form": "안좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100035768.1.2",
          "form": "저는 개인적으로 마라요",
          "original_form": "저는 개인적으로 마라요",
          "speaker_id": "2",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100035768.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100035768.1.4",
          "form": "헐 전 마라 좋아하는데 !",
          "original_form": "헐 전 마라 좋아하는데 !",
          "speaker_id": "1",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100035768.1.5",
          "form": "전 버섯을 싫어해요 ㅎㅎ",
          "original_form": "전 버섯을 싫어해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100035768.1.6",
          "form": "저는 야채중에서는 파프리카랑 버섯도 싫어해요",
          "original_form": "저는 야채중에서는 파프리카랑 버섯도 싫어해요",
          "speaker_id": "2",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100035768.1.7",
          "form": "아 저도 파프리카 싫어해요 ㅋㅋ !",
          "original_form": "아 저도 파프리카 싫어해요 ㅋㅋ !",
          "speaker_id": "1",
          "time": "20210830 01:14"
        },
        {
          "id": "MDRW2100035768.1.8",
          "form": "향 나는걸 별로 안좋아해요 ㅎㅎ",
          "original_form": "향 나는걸 별로 안좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:14"
        },
        {
          "id": "MDRW2100035768.1.9",
          "form": "그러시구나 저는 사실 그냥 야채는 몇몇개빼고 다 싫어하는 것 같아요",
          "original_form": "그러시구나 저는 사실 그냥 야채는 몇몇개빼고 다 싫어하는 것 같아요",
          "speaker_id": "2",
          "time": "20210830 01:14"
        },
        {
          "id": "MDRW2100035768.1.10",
          "form": "맞아요 ㅋㅋ 저도 좋아하는 야채가 별로 없네요",
          "original_form": "맞아요 ㅋㅋ 저도 좋아하는 야채가 별로 없네요",
          "speaker_id": "1",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100035768.1.11",
          "form": "양파 콩나물 이런거빼곤 ..",
          "original_form": "양파 콩나물 이런거빼곤 ..",
          "speaker_id": "1",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100035768.1.12",
          "form": "저도 깻잎이나 콩나물 이런거 빼고는 왜인지 채소가 싫더라고요",
          "original_form": "저도 깻잎이나 콩나물 이런거 빼고는 왜인지 채소가 싫더라고요",
          "speaker_id": "2",
          "time": "20210830 01:17"
        },
        {
          "id": "MDRW2100035768.1.13",
          "form": "맞아요 ㅋㅋ 좋아하는 사람이 드물겠지만",
          "original_form": "맞아요 ㅋㅋ 좋아하는 사람이 드물겠지만",
          "speaker_id": "1",
          "time": "20210830 01:18"
        },
        {
          "id": "MDRW2100035768.1.14",
          "form": "마라는 왜 안좋아하세요?",
          "original_form": "마라는 왜 안좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 01:18"
        },
        {
          "id": "MDRW2100035768.1.15",
          "form": "몇 번 먹어봤는데 약간 비누맛인가? 뭔가 제 입맛에는 잘 안맞더라고요",
          "original_form": "몇 번 먹어봤는데 약간 비누맛인가? 뭔가 제 입맛에는 잘 안맞더라고요",
          "speaker_id": "2",
          "time": "20210830 01:20"
        },
        {
          "id": "MDRW2100035768.1.16",
          "form": "아아 ㅎㅎ 비누맛 뭔지 알거같네요 ㅋㅋ",
          "original_form": "아아 ㅎㅎ 비누맛 뭔지 알거같네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100035768.1.17",
          "form": "그 마라 향이 안맞으시나봐요",
          "original_form": "그 마라 향이 안맞으시나봐요",
          "speaker_id": "1",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100035768.1.18",
          "form": "맞아여 저한텐 그 향이 안맞는 것 같아요",
          "original_form": "맞아여 저한텐 그 향이 안맞는 것 같아요",
          "speaker_id": "2",
          "time": "20210830 01:27"
        },
        {
          "id": "MDRW2100035768.1.19",
          "form": "저는 고수도 안좋아하는데",
          "original_form": "저는 고수도 안좋아하는데",
          "speaker_id": "1",
          "time": "20210830 01:31"
        }
      ]
