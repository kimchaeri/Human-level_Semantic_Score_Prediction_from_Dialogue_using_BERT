        },
        {
          "id": "MDRW2100025641.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 15:03"
        },
        {
          "id": "MDRW2100025641.1.3",
          "form": "요즘 공연 전시 보신 거 있으신가여",
          "original_form": "요즘 공연 전시 보신 거 있으신가여",
          "speaker_id": "1",
          "time": "20210901 15:06"
        },
        {
          "id": "MDRW2100025641.1.4",
          "form": "온라인으로 봤어요 트로이의 여인들",
          "original_form": "온라인으로 봤어요 트로이의 여인들",
          "speaker_id": "2",
          "time": "20210901 15:21"
        },
        {
          "id": "MDRW2100025641.1.5",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "1",
          "time": "20210901 15:22"
        },
        {
          "id": "MDRW2100025641.1.6",
          "form": "name1님께서는 전시회나 공연 좋아하세요?",
          "original_form": "&name1&님께서는 전시회나 공연 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 15:24"
        },
        {
          "id": "MDRW2100025641.1.7",
          "form": "네 2주전에 제주 내려왔는데 그때도 김보희 작가님 제주 풍경 그림 전시를 서울에 봤었어요",
          "original_form": "네 2주전에 제주 내려왔는데 그때도 김보희 작가님 제주 풍경 그림 전시를 서울에 봤었어요",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025641.1.8",
          "form": "그림보는거 좋아해여",
          "original_form": "그림보는거 좋아해여",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025641.1.9",
          "form": "그림을 좋아하시는군요 멋져요~",
          "original_form": "그림을 좋아하시는군요 멋져요~",
          "speaker_id": "2",
          "time": "20210901 15:59"
        },
        {
          "id": "MDRW2100025641.1.10",
          "form": "최근에 영화는 뭐 보셨어요?",
          "original_form": "최근에 영화는 뭐 보셨어요?",
          "speaker_id": "1",
          "time": "20210901 16:14"
        },
        {
          "id": "MDRW2100025641.1.11",
          "form": "\"벌룬\" 봤어요",
          "original_form": "\"벌룬\" 봤어요",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025641.1.12",
          "form": "열기구타고 동독을 탈출하는 가족이야기인데",
          "original_form": "열기구타고 동독을 탈출하는 가족이야기인데",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025641.1.13",
          "form": "잘만들었더라구요",
          "original_form": "잘만들었더라구요",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025641.1.14",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100025641.1.15",
          "form": "영화관 아님 넷플릭스?",
          "original_form": "영화관 아님 넷플릭스?",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100025641.1.16",
          "form": "웨이브에서 봤어요 극장에서 영화보고싶은데",
          "original_form": "웨이브에서 봤어요 극장에서 영화보고싶은데",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025641.1.17",
          "form": "못 가고 있네요 팝콘이 그립고 영화관이 그리워요",
          "original_form": "못 가고 있네요 팝콘이 그립고 영화관이 그리워요",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025641.1.18",
          "form": "오 저두 모가디슈 살금살금가서 봤눈데",
          "original_form": "오 저두 모가디슈 살금살금가서 봤눈데",
          "speaker_id": "1",
          "time": "20210901 18:42"
        }
