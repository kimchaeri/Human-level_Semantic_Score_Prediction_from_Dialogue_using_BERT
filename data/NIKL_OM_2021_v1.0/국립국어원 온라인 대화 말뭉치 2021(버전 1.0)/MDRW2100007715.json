        {
          "id": "MDRW2100007715.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007715.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007715.1.3",
          "form": "강이지 키우고 싶어요",
          "original_form": "강이지 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007715.1.4",
          "form": "애동 키우시나요?",
          "original_form": "애동 키우시나요?",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007715.1.5",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210822 09:53"
        },
        {
          "id": "MDRW2100007715.1.6",
          "form": "뭐 키우고 싶은 거 있으세요?",
          "original_form": "뭐 키우고 싶은 거 있으세요?",
          "speaker_id": "2",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007715.1.7",
          "form": "저도 강아지",
          "original_form": "저도 강아지",
          "speaker_id": "1",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007715.1.8",
          "form": "무슨 종이요?",
          "original_form": "무슨 종이요?",
          "speaker_id": "2",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007715.1.9",
          "form": "종은 잘 몰라서",
          "original_form": "종은 잘 몰라서",
          "speaker_id": "1",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007715.1.10",
          "form": "전 리트리버요. 리트리버는 아시죠?",
          "original_form": "전 리트리버요. 리트리버는 아시죠?",
          "speaker_id": "2",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007715.1.11",
          "form": "강아지 잘 모르는 사람들이 로망만 같는다는ㅋㅋㅋㅋ",
          "original_form": "강아지 잘 모르는 사람들이 로망만 같는다는ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007715.1.12",
          "form": "대형견은 키우기 정말 어렵대요",
          "original_form": "대형견은 키우기 정말 어렵대요",
          "speaker_id": "2",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007715.1.13",
          "form": "마당있나요?",
          "original_form": "마당있나요?",
          "speaker_id": "1",
          "time": "20210822 09:58"
        },
        {
          "id": "MDRW2100007715.1.14",
          "form": "저희 집요? 아니요ㅋㅋㅋ",
          "original_form": "저희 집요? 아니요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:58"
        },
        {
          "id": "MDRW2100007715.1.15",
          "form": "것도 걍 로망이에요. 마당 있는 집에서 강아지 뛰어 노는 거",
          "original_form": "것도 걍 로망이에요. 마당 있는 집에서 강아지 뛰어 노는 거",
          "speaker_id": "2",
          "time": "20210822 09:58"
        },
        {
          "id": "MDRW2100007715.1.16",
          "form": "박세리 마당 넓은데",
          "original_form": "박세리 마당 넓은데",
          "speaker_id": "1",
          "time": "20210822 09:59"
        },
        {
          "id": "MDRW2100007715.1.17",
          "form": "그 집 강아지도 예쁘더라구요",
          "original_form": "그 집 강아지도 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210822 09:59"
        },
