        },
        {
          "id": "MDRW2100016870.1.2",
          "form": "아들 말리다 제가해요",
          "original_form": "아들 말리다 제가해요",
          "speaker_id": "1",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016870.1.3",
          "form": "ㅋㅋㅋ어떤 게임 하셨어요?",
          "original_form": "ㅋㅋㅋ어떤 게임 하셨어요?",
          "speaker_id": "2",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016870.1.4",
          "form": "브롤이요ㅋㅋ",
          "original_form": "브롤이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016870.1.5",
          "form": "브롤~",
          "original_form": "브롤~",
          "speaker_id": "2",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016870.1.6",
          "form": "아이가 몇살이에요?",
          "original_form": "아이가 몇살이에요?",
          "speaker_id": "2",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016870.1.7",
          "form": "10살이요",
          "original_form": "10살이요",
          "speaker_id": "1",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016870.1.8",
          "form": "pc방 가는게 꿈이래요",
          "original_form": "pc방 가는게 꿈이래요",
          "speaker_id": "1",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016870.1.9",
          "form": "아직 안 가봤군요! 하긴..담배냄새나고 안 좋죠",
          "original_form": "아직 안 가봤군요! 하긴..담배냄새나고 안 좋죠",
          "speaker_id": "2",
          "time": "20210826 16:04"
        },
        {
          "id": "MDRW2100016870.1.10",
          "form": "이제 눈뜨나봐요",
          "original_form": "이제 눈뜨나봐요",
          "speaker_id": "1",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016870.1.11",
          "form": "아들이라 더 빨리 받아들일 것 같아요;;",
          "original_form": "아들이라 더 빨리 받아들일 것 같아요;;",
          "speaker_id": "2",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016870.1.12",
          "form": "게임을 너무 좋아하네요",
          "original_form": "게임을 너무 좋아하네요",
          "speaker_id": "1",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016870.1.13",
          "form": "우리 아이는 어려서 아직 게임 잘 모르는데 한번 빠지면 걷잡을 수 없을 것 같긴해요ㅋㅋㅋ",
          "original_form": "우리 아이는 어려서 아직 게임 잘 모르는데 한번 빠지면 걷잡을 수 없을 것 같긴해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016870.1.14",
          "form": "저랑 신랑도 게임 좋아해서ㅋㅋ",
          "original_form": "저랑 신랑도 게임 좋아해서ㅋㅋ",
