      "utterance": [
        {
          "id": "MDRW2100039525.1.1",
          "form": "저는 요즘 자취하고 싶어요",
          "original_form": "저는 요즘 자취하고 싶어요",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100039525.1.2",
          "form": "헛 저도 꿈꾸고 있습니다",
          "original_form": "헛 저도 꿈꾸고 있습니다",
          "speaker_id": "2",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100039525.1.3",
          "form": "돈만 있다면...",
          "original_form": "돈만 있다면...",
          "speaker_id": "2",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100039525.1.4",
          "form": "만약 한다면 혼자 살고 싶으신가요",
          "original_form": "만약 한다면 혼자 살고 싶으신가요",
          "speaker_id": "2",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100039525.1.5",
          "form": "네 당연!!!",
          "original_form": "네 당연!!!",
          "speaker_id": "1",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100039525.1.6",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100039525.1.7",
          "form": "전 약간 친구랑 동거 하고 싶은데",
          "original_form": "전 약간 친구랑 동거 하고 싶은데",
          "speaker_id": "2",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039525.1.8",
          "form": "그렇게 잘 맞는 친구를 찾을수 있을까 싶어요 ㅠ",
          "original_form": "그렇게 잘 맞는 친구를 찾을수 있을까 싶어요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039525.1.9",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039525.1.10",
          "form": "혼자 사는거 안좋아하세요?",
          "original_form": "혼자 사는거 안좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039525.1.11",
          "form": "혼자 살아본 적이 없어서 살짝 겁나요",
          "original_form": "혼자 살아본 적이 없어서 살짝 겁나요",
          "speaker_id": "2",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100039525.1.12",
          "form": "외로울 것 같기도 하고",
          "original_form": "외로울 것 같기도 하고",
          "speaker_id": "2",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100039525.1.13",
          "form": "친구 둘이서 재밌게 사는 거 보면 살짝 부럽기도 하구요",
          "original_form": "친구 둘이서 재밌게 사는 거 보면 살짝 부럽기도 하구요",
          "speaker_id": "2",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100039525.1.14",
          "form": "오 아무리 친해도 같이살면 싸운대요ㅠㅠ",
          "original_form": "오 아무리 친해도 같이살면 싸운대요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100039525.1.15",
          "form": "전 혼자 사는게 꿈이에요..",
          "original_form": "전 혼자 사는게 꿈이에요..",
          "speaker_id": "1",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100039525.1.16",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100039525.1.17",
          "form": "다들 그렇게 말하긴 하죠..ㅠㅠ",
          "original_form": "다들 그렇게 말하긴 하죠..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 18:46"
        },
        {
          "id": "MDRW2100039525.1.18",
          "form": "혹시 기숙사 생활 그런건 해본적 있으세요?",
          "original_form": "혹시 기숙사 생활 그런건 해본적 있으세요?",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100039525.1.19",
          "form": "아니요 그냥 일년 자취한적은 있는데",
          "original_form": "아니요 그냥 일년 자취한적은 있는데",
          "speaker_id": "1",
          "time": "20210903 18:55"
        },
        {
          "id": "MDRW2100039525.1.20",
          "form": "다시 돌아오니 그리워요ㅜㅜ",
          "original_form": "다시 돌아오니 그리워요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 18:55"
        },
        {
          "id": "MDRW2100039525.1.21",
          "form": "맞는 친구있우면 정말 좋긴할텐데",
          "original_form": "맞는 친구있우면 정말 좋긴할텐데",
          "speaker_id": "1",
          "time": "20210903 18:55"
        },
        {
          "id": "MDRW2100039525.1.22",
          "form": "그런 친구는 정말..한명만 있어도 인생 성공한 것 같아요",
          "original_form": "그런 친구는 정말..한명만 있어도 인생 성공한 것 같아요",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100039525.1.23",
          "form": "일년 자취하셨었어요?",
          "original_form": "일년 자취하셨었어요?",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100039525.1.24",
          "form": "원래 한번 맛보면 더 그리운 법인데 ㅠ",
          "original_form": "원래 한번 맛보면 더 그리운 법인데 ㅠ",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100039525.1.25",
          "form": "그니까요..",
          "original_form": "그니까요..",
          "speaker_id": "1",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100039525.1.26",
          "form": "그래서 더 자취하고 싶은가봐요",
          "original_form": "그래서 더 자취하고 싶은가봐요",
          "speaker_id": "1",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100039525.1.27",
          "form": "자취하다 같이 있으니 싸움도 잦고..",
          "original_form": "자취하다 같이 있으니 싸움도 잦고..",
          "speaker_id": "1",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100039525.1.28",
          "form": "아구..주로 어떤 것 때문에 부딪히시나요?",
          "original_form": "아구..주로 어떤 것 때문에 부딪히시나요?",
          "speaker_id": "2",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100039525.1.29",
          "form": "아무래도 집안일..ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아무래도 집안일..ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100039525.1.30",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100039525.1.31",
          "form": "아,.약간 가사 분담 그런걸루요?",
          "original_form": "아,.약간 가사 분담 그런걸루요?",
          "speaker_id": "2",
          "time": "20210904 13:44"
        },
        {
          "id": "MDRW2100039525.1.32",
          "form": "혼자 자유롭게 하다가 맞춰야 될 규칙들이 생겨서 스트레스 받을것 같긴 하네요..ㅎㅎㅠ",
          "original_form": "혼자 자유롭게 하다가 맞춰야 될 규칙들이 생겨서 스트레스 받을것 같긴 하네요..ㅎㅎㅠ",
          "speaker_id": "2",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100039525.1.33",
          "form": "마자요 그런것때문에 자꾸 부딪혀요ㅠㅠ",
          "original_form": "마자요 그런것때문에 자꾸 부딪혀요ㅠㅠ",
