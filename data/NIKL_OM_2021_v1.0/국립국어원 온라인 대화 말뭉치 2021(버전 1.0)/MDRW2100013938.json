      "utterance": [
        {
          "id": "MDRW2100013938.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013938.1.2",
          "form": "안녕하세요 반려동물키우시나요",
          "original_form": "안녕하세요 반려동물키우시나요",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013938.1.3",
          "form": "아니요ㅠ",
          "original_form": "아니요ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013938.1.4",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013938.1.5",
          "form": "네 전 고양이한마리키우고있어요",
          "original_form": "네 전 고양이한마리키우고있어요",
          "speaker_id": "2",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013938.1.6",
          "form": "함께하고픈 동물있으신가요",
          "original_form": "함께하고픈 동물있으신가요",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013938.1.7",
          "form": "오 부러워요",
          "original_form": "오 부러워요",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013938.1.8",
          "form": "전 강아지 키우고 싶네요ㅠ",
          "original_form": "전 강아지 키우고 싶네요ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013938.1.9",
          "form": "어릴때 강아지맡아본적 잠시있었는데 강쥐들은 고양이랑 또 엄청 다르더라구여",
          "original_form": "어릴때 강아지맡아본적 잠시있었는데 강쥐들은 고양이랑 또 엄청 다르더라구여",
          "speaker_id": "2",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013938.1.10",
          "form": "혹시 고양이 이름은 뭔가여?",
          "original_form": "혹시 고양이 이름은 뭔가여?",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013938.1.11",
          "form": "name3에요 애기때 아팠어서 잘자랐으면하는맘에지었는데",
          "original_form": "&name3&에요 애기때 아팠어서 잘자랐으면하는맘에지었는데",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013938.1.12",
          "form": "엄청 잘자라는걸 넘어서서 돼지가됐어요",
          "original_form": "엄청 잘자라는걸 넘어서서 돼지가됐어요",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013938.1.13",
          "form": "ㅎㅎ그래도 귀여울 것 같아요",
          "original_form": "ㅎㅎ그래도 귀여울 것 같아요",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013938.1.14",
          "form": "네 넘귀여워서 못참을때 많아요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 넘귀여워서 못참을때 많아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013938.1.15",
          "form": "고양이 궁금하신거있나여",
          "original_form": "고양이 궁금하신거있나여",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013938.1.16",
          "form": "실례지만 고양이 변 냄새 심한가요..?",
          "original_form": "실례지만 고양이 변 냄새 심한가요..?",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013938.1.17",
          "form": "네 완전완전 심해요 ㅋㅋㅋ",
          "original_form": "네 완전완전 심해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013938.1.18",
          "form": "그래서 빨리빨리 치울수있게돼요",
          "original_form": "그래서 빨리빨리 치울수있게돼요",
          "speaker_id": "2",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013938.1.19",
          "form": "누고나면 냄새가 코를찌릅니다 ㅋㅋㅋ",
          "original_form": "누고나면 냄새가 코를찌릅니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013938.1.20",
          "form": "앗 장점도 있군요ㅎ",
          "original_form": "앗 장점도 있군요ㅎ",
