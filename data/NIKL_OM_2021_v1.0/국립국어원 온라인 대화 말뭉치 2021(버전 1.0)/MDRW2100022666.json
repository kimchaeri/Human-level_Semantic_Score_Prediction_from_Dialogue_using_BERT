      "utterance": [
        {
          "id": "MDRW2100022666.1.1",
          "form": "카페 자주가세요?",
          "original_form": "카페 자주가세요?",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022666.1.2",
          "form": "저 커피를 안 마셔서 자주는 안 가요...ㅎㅎ",
          "original_form": "저 커피를 안 마셔서 자주는 안 가요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022666.1.3",
          "form": "그럼 음식 중에 최애는 뭐예요?",
          "original_form": "그럼 음식 중에 최애는 뭐예요?",
          "speaker_id": "1",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022666.1.4",
          "form": "저는 고기류 좋아하는데 최근 1-2년 사이엔 곱창이 제일 좋아요",
          "original_form": "저는 고기류 좋아하는데 최근 1-2년 사이엔 곱창이 제일 좋아요",
          "speaker_id": "1",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022666.1.5",
          "form": "저는 파스타 정말정말 좋아합니다ㅠㅠ",
          "original_form": "저는 파스타 정말정말 좋아합니다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022666.1.6",
          "form": "어떤거요?!",
          "original_form": "어떤거요?!",
          "speaker_id": "1",
          "time": "20210829 17:02"
        },
        {
          "id": "MDRW2100022666.1.7",
          "form": "동생이 알리오올리오 좋아해서 일주일에 두번씩 먹어요...ㅎ",
          "original_form": "동생이 알리오올리오 좋아해서 일주일에 두번씩 먹어요...ㅎ",
          "speaker_id": "1",
          "time": "20210829 17:02"
        },
        {
          "id": "MDRW2100022666.1.8",
          "form": "파스타 종류별로 다 먹긴 하는데 자주 먹는 거라면 로제나 알리오 올리오?!",
          "original_form": "파스타 종류별로 다 먹긴 하는데 자주 먹는 거라면 로제나 알리오 올리오?!",
          "speaker_id": "2",
          "time": "20210829 17:02"
        },
        {
          "id": "MDRW2100022666.1.9",
          "form": "진짜 알리오올리오 마늘향이 너무 좋아요ㅠㅠ",
          "original_form": "진짜 알리오올리오 마늘향이 너무 좋아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:02"
        },
        {
          "id": "MDRW2100022666.1.10",
          "form": "먹을 때마다 마늘 왕창 넣어서 먹습니다",
          "original_form": "먹을 때마다 마늘 왕창 넣어서 먹습니다",
          "speaker_id": "2",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022666.1.11",
          "form": "알리오올리오는 마늘맛 아니겠습니까ㅋㅋㅋ",
          "original_form": "알리오올리오는 마늘맛 아니겠습니까ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100022666.1.12",
          "form": "맞아요 마늘의 양이 맛을 좌우하죠ᅲᅲ",
          "original_form": "맞아요 마늘의 양이 맛을 좌우하죠ᅲᅲ",
          "speaker_id": "2",
          "time": "20210829 17:07"
        },
        {
          "id": "MDRW2100022666.1.13",
          "form": "곱창은 주로 어떻게 드세요?",
          "original_form": "곱창은 주로 어떻게 드세요?",
          "speaker_id": "2",
          "time": "20210829 17:08"
        },
        {
          "id": "MDRW2100022666.1.14",
          "form": "배달? 아니면 직접 구워서?",
          "original_form": "배달? 아니면 직접 구워서?",
          "speaker_id": "2",
          "time": "20210829 17:08"
        },
        {
          "id": "MDRW2100022666.1.15",
          "form": "맛집 가서요!",
          "original_form": "맛집 가서요!",
          "speaker_id": "1",
          "time": "20210829 17:08"
        },
        {
          "id": "MDRW2100022666.1.16",
          "form": "아 직접 가서 드시는구나",
          "original_form": "아 직접 가서 드시는구나",
          "speaker_id": "2",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022666.1.17",
          "form": "소곱창을 좋아하는데",
          "original_form": "소곱창을 좋아하는데",
          "speaker_id": "1",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022666.1.18",
          "form": "배달후기보니까 양이 너무 적은거같고....",
          "original_form": "배달후기보니까 양이 너무 적은거같고....",
          "speaker_id": "1",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022666.1.19",
          "form": "바로 구워먹는게 맛있어요ㅠ",
          "original_form": "바로 구워먹는게 맛있어요ㅠ",
          "speaker_id": "1",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022666.1.20",
          "form": "아 그렇긴 하죠ㅠㅠ",
          "original_form": "아 그렇긴 하죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022666.1.21",
          "form": "사실 저는 곱창을 아직 제대로 먹어본 적이 없어서 곱창 맛을 잘 몰라요...ㅠㅠ",
          "original_form": "사실 저는 곱창을 아직 제대로 먹어본 적이 없어서 곱창 맛을 잘 몰라요...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022666.1.22",
          "form": "그런데 편의점에서 안주로 한 번 사 먹어봤는데 진짜 맛있더라구요",
          "original_form": "그런데 편의점에서 안주로 한 번 사 먹어봤는데 진짜 맛있더라구요",
          "speaker_id": "2",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022666.1.23",
          "form": "노ㅜㅜㅜㅜ",
          "original_form": "노ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022666.1.24",
          "form": "바로 구워서 드셔보세요 제발 ㅜㅜㅜ",
          "original_form": "바로 구워서 드셔보세요 제발 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022666.1.25",
          "form": "내장류 안 좋아해서 안드셔본거예요?",
          "original_form": "내장류 안 좋아해서 안드셔본거예요?",
          "speaker_id": "1",
          "time": "20210829 17:10"
        }
      ]
    }
