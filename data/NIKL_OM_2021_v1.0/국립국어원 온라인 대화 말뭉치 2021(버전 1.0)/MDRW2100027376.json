      "utterance": [
        {
          "id": "MDRW2100027376.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100027376.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210902 12:01"
        },
        {
          "id": "MDRW2100027376.1.3",
          "form": "지금 계시는 곳 날씨는 어때요?",
          "original_form": "지금 계시는 곳 날씨는 어때요?",
          "speaker_id": "1",
          "time": "20210902 12:02"
        },
        {
          "id": "MDRW2100027376.1.4",
          "form": "흐릿흐릿 하네요 비가 아침엔 좀 오더니 ㅎㅎㅎ",
          "original_form": "흐릿흐릿 하네요 비가 아침엔 좀 오더니 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100027376.1.5",
          "form": "그곳은 어때요",
          "original_form": "그곳은 어때요",
          "speaker_id": "2",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100027376.1.6",
          "form": "이쪽도 흐려요 ㅠ",
          "original_form": "이쪽도 흐려요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 12:05"
        },
        {
          "id": "MDRW2100027376.1.7",
          "form": "오늘 추워서 긴팔자켓 꺼내입었습니다..ㅎㅎ",
          "original_form": "오늘 추워서 긴팔자켓 꺼내입었습니다..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:05"
        },
        {
          "id": "MDRW2100027376.1.8",
          "form": "저두요 오늘 직장동료들이 가을가을하다고",
          "original_form": "저두요 오늘 직장동료들이 가을가을하다고",
          "speaker_id": "2",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100027376.1.9",
          "form": "제일 좋아하는 계절 있으신가여",
          "original_form": "제일 좋아하는 계절 있으신가여",
          "speaker_id": "1",
          "time": "20210902 12:08"
        },
        {
          "id": "MDRW2100027376.1.10",
          "form": "저는 여름 좋아해옷",
          "original_form": "저는 여름 좋아해옷",
          "speaker_id": "2",
          "time": "20210902 12:10"
        },
        {
          "id": "MDRW2100027376.1.11",
          "form": "저는 가을 좋아하는데",
          "original_form": "저는 가을 좋아하는데",
          "speaker_id": "1",
          "time": "20210902 12:11"
        },
        {
          "id": "MDRW2100027376.1.12",
          "form": "더위 많이 안타시나봐요!",
          "original_form": "더위 많이 안타시나봐요!",
          "speaker_id": "1",
          "time": "20210902 12:11"
        },
        {
          "id": "MDRW2100027376.1.13",
          "form": "가을이 젤 살기 좋긴한데",
          "original_form": "가을이 젤 살기 좋긴한데",
          "speaker_id": "2",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027376.1.14",
          "form": "더위도 많이 타요",
          "original_form": "더위도 많이 타요",
          "speaker_id": "2",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027376.1.15",
          "form": "근데 여름밤이 너무 좋아요",
          "original_form": "근데 여름밤이 너무 좋아요",
          "speaker_id": "2",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027376.1.16",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027376.1.17",
          "form": "뭔가 에너지 뿜뿜",
          "original_form": "뭔가 에너지 뿜뿜",
          "speaker_id": "2",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027376.1.18",
          "form": "ㅋㅋㅋ아하 그 특유의 느낌 있죠",
          "original_form": "ㅋㅋㅋ아하 그 특유의 느낌 있죠",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100027376.1.19",
          "form": "저도 더위 많이 안타서 여름 좋아하는 편이에요.ㅎㅎ",
          "original_form": "저도 더위 많이 안타서 여름 좋아하는 편이에요.ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100027376.1.20",
          "form": "넹 무더워서 인생 의욕이 없어지지만",
          "original_form": "넹 무더워서 인생 의욕이 없어지지만",
          "speaker_id": "2",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100027376.1.21",
          "form": "전 원피스 입는거 좋아하는데 여름이 원피스의 계절이라 사실 그것도 좋아요 ㅋㅋ",
          "original_form": "전 원피스 입는거 좋아하는데 여름이 원피스의 계절이라 사실 그것도 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027376.1.22",
          "form": "오 맞아요 맞아요",
          "original_form": "오 맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027376.1.23",
          "form": "여름 원피스 입는거 너무 좋아요",
          "original_form": "여름 원피스 입는거 너무 좋아요",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027376.1.24",
          "form": "하지만 곧 롱패딩의 계절이 다가오네요 ㅠ",
          "original_form": "하지만 곧 롱패딩의 계절이 다가오네요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 12:29"
        }
      ]
