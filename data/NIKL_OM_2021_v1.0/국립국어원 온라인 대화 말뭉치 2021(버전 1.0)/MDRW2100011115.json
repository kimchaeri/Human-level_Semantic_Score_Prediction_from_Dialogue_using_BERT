      "utterance": [
        {
          "id": "MDRW2100011115.1.1",
          "form": "태풍오고있다는데 name2님 지역은 괜찮으세요?",
          "original_form": "태풍오고있다는데 &name2&님 지역은 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210824 01:10"
        },
        {
          "id": "MDRW2100011115.1.2",
          "form": "서울이라 아직 조용하네요!",
          "original_form": "서울이라 아직 조용하네요!",
          "speaker_id": "2",
          "time": "20210824 01:11"
        },
        {
          "id": "MDRW2100011115.1.3",
          "form": "아 같은 지역이군요ㅎ",
          "original_form": "아 같은 지역이군요ㅎ",
          "speaker_id": "1",
          "time": "20210824 01:13"
        },
        {
          "id": "MDRW2100011115.1.4",
          "form": "제주도는 400미리온다던데 수도권은 괜찮은가봐요",
          "original_form": "제주도는 400미리온다던데 수도권은 괜찮은가봐요",
          "speaker_id": "1",
          "time": "20210824 01:13"
        },
        {
          "id": "MDRW2100011115.1.5",
          "form": "헐 400미리요..?",
          "original_form": "헐 400미리요..?",
          "speaker_id": "2",
          "time": "20210824 01:13"
        },
        {
          "id": "MDRW2100011115.1.6",
          "form": "네 엄청오나봐요",
          "original_form": "네 엄청오나봐요",
          "speaker_id": "1",
          "time": "20210824 01:14"
        },
        {
          "id": "MDRW2100011115.1.7",
          "form": "남부지방은",
          "original_form": "남부지방은",
          "speaker_id": "1",
          "time": "20210824 01:14"
        },
        {
          "id": "MDRW2100011115.1.8",
          "form": "서울은 아직 평온하네요ㅎㅎ",
          "original_form": "서울은 아직 평온하네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100011115.1.9",
          "form": "퇴근길에 좀 오긴했는데",
          "original_form": "퇴근길에 좀 오긴했는데",
          "speaker_id": "1",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100011115.1.10",
          "form": "비그치면 가을오겠죠?",
          "original_form": "비그치면 가을오겠죠?",
          "speaker_id": "1",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100011115.1.11",
          "form": "그러게요 이번여름 너무 더웠어요",
          "original_form": "그러게요 이번여름 너무 더웠어요",
          "speaker_id": "2",
          "time": "20210824 01:19"
        },
        {
          "id": "MDRW2100011115.1.12",
          "form": "맞아요 전 여름 넘 싫어해서",
          "original_form": "맞아요 전 여름 넘 싫어해서",
          "speaker_id": "1",
          "time": "20210824 01:20"
        },
        {
          "id": "MDRW2100011115.1.13",
          "form": "빨리가을 왔음 좋겠네요",
          "original_form": "빨리가을 왔음 좋겠네요",
          "speaker_id": "1",
          "time": "20210824 01:20"
        },
        {
          "id": "MDRW2100011115.1.14",
          "form": "가을이 젤좋죠 진짜ㅠ",
          "original_form": "가을이 젤좋죠 진짜ㅠ",
          "speaker_id": "2",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011115.1.15",
          "form": "넘 짧아서 아쉽죠ㅠㅠ",
          "original_form": "넘 짧아서 아쉽죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011115.1.16",
          "form": "마자요 봄이랑 가을이 제일 좋아요",
          "original_form": "마자요 봄이랑 가을이 제일 좋아요",
          "speaker_id": "2",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011115.1.17",
