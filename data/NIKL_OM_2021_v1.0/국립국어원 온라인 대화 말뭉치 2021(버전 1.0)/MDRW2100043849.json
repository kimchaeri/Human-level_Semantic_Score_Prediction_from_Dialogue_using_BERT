      "utterance": [
        {
          "id": "MDRW2100043849.1.1",
          "form": "식빵이나 치킨 한두조각이랑 같이 먹었어요",
          "original_form": "식빵이나 치킨 한두조각이랑 같이 먹었어요",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043849.1.2",
          "form": "깐부치킨이라는 치킨 프랜차이즈가 있는데",
          "original_form": "깐부치킨이라는 치킨 프랜차이즈가 있는데",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043849.1.3",
          "form": "거기 코우슬로가 맛있었어요",
          "original_form": "거기 코우슬로가 맛있었어요",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043849.1.4",
          "form": "물론 치킨도 맛있는데",
          "original_form": "물론 치킨도 맛있는데",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043849.1.5",
          "form": "매장가서 먹었을때 진짜 맛있었어서 추천이에요!",
          "original_form": "매장가서 먹었을때 진짜 맛있었어서 추천이에요!",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043849.1.6",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043849.1.7",
          "form": "다음에 먹을때 참고할게요!!",
          "original_form": "다음에 먹을때 참고할게요!!",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043849.1.8",
          "form": "매장가서 먹은 치킨집 중엔 최고였던 것 같아요 ㅋㅋ",
          "original_form": "매장가서 먹은 치킨집 중엔 최고였던 것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043849.1.9",
          "form": "오 기대되네요",
          "original_form": "오 기대되네요",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043849.1.10",
          "form": "가보신 맛집중에 괜찮은 집이 있다면 추천해주세용",
          "original_form": "가보신 맛집중에 괜찮은 집이 있다면 추천해주세용",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043849.1.11",
          "form": "아 어떤 종류의 음식을 좋아하세요?ㅎㅎ",
          "original_form": "아 어떤 종류의 음식을 좋아하세요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043849.1.12",
          "form": "음.. 저는 거의 음식은 가리지 않는데",
          "original_form": "음.. 저는 거의 음식은 가리지 않는데",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043849.1.13",
          "form": "딱 거기에만 있는 그런 맛집들에 있다면 추천해주세요!",
          "original_form": "딱 거기에만 있는 그런 맛집들에 있다면 추천해주세요!",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043849.1.14",
          "form": "음 최근엔 연남동에 종종 갔는데",
          "original_form": "음 최근엔 연남동에 종종 갔는데",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043849.1.15",
          "form": "미쁘동이란 일식덮밥집",
          "original_form": "미쁘동이란 일식덮밥집",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043849.1.16",
          "form": "그리고 솥솥이랑 솥밥집",
          "original_form": "그리고 솥솥이랑 솥밥집",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043849.1.17",
          "form": "맛있었어요",
          "original_form": "맛있었어요",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043849.1.18",
          "form": "연남동 맛집 많죠!",
          "original_form": "연남동 맛집 많죠!",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043849.1.19",
          "form": "추천메뉴 혹시 있으세요?",
          "original_form": "추천메뉴 혹시 있으세요?",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043849.1.20",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043849.1.21",
          "form": "케이크 좋아하시나요",
          "original_form": "케이크 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043849.1.22",
          "form": "디저트는 제 사랑이죠",
          "original_form": "디저트는 제 사랑이죠",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043849.1.23",
          "form": "완전 좋아해요!",
          "original_form": "완전 좋아해요!",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043849.1.24",
          "form": "아 그럼 아레아레아라는 케이크집 있어요",
          "original_form": "아 그럼 아레아레아라는 케이크집 있어요",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043849.1.25",
          "form": "거기 케이크가 맛있어요",
          "original_form": "거기 케이크가 맛있어요",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043849.1.26",
          "form": "재료도 좋고",
          "original_form": "재료도 좋고",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043849.1.27",
          "form": "지금 살짝 검색해봤는데",
          "original_form": "지금 살짝 검색해봤는데",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043849.1.28",
          "form": "어머 케이크들 다 너무 예뻐요!!",
          "original_form": "어머 케이크들 다 너무 예뻐요!!",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043849.1.29",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043849.1.30",
          "form": "추천합니다!!",
          "original_form": "추천합니다!!",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043849.1.31",
          "form": "저는 케이크 맛있게 먹었던 집은 숙대 근처에 비나 케이크라는 곳이 있어요",
          "original_form": "저는 케이크 맛있게 먹었던 집은 숙대 근처에 비나 케이크라는 곳이 있어요",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043849.1.32",
          "form": "여기 재료 좋고 맛도 괜찮아요!",
          "original_form": "여기 재료 좋고 맛도 괜찮아요!",
          "speaker_id": "2",
          "time": "20210907 21:14"
        }
      ]
