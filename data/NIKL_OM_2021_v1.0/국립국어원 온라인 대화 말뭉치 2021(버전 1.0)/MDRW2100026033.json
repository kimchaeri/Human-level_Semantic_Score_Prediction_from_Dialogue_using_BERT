      "utterance": [
        {
          "id": "MDRW2100026033.1.1",
          "form": "제주는 날씨가 참 변덕스러워요",
          "original_form": "제주는 날씨가 참 변덕스러워요",
          "speaker_id": "2",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100026033.1.2",
          "form": "사시는 곳은 어떤가요",
          "original_form": "사시는 곳은 어떤가요",
          "speaker_id": "2",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100026033.1.3",
          "form": "여기도 변덕스러워요",
          "original_form": "여기도 변덕스러워요",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100026033.1.4",
          "form": "아까전에는",
          "original_form": "아까전에는",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100026033.1.5",
          "form": "해가나더니",
          "original_form": "해가나더니",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100026033.1.6",
          "form": "또 비가 내려요.ㅜ",
          "original_form": "또 비가 내려요.ㅜ",
          "speaker_id": "1",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100026033.1.7",
          "form": "곧 건조해질텐데 추운거 넘 싫어요",
          "original_form": "곧 건조해질텐데 추운거 넘 싫어요",
          "speaker_id": "2",
          "time": "20210901 19:06"
        },
        {
          "id": "MDRW2100026033.1.8",
          "form": "맞아요ㅠ저두요.",
          "original_form": "맞아요ㅠ저두요.",
          "speaker_id": "1",
          "time": "20210901 19:07"
        },
        {
          "id": "MDRW2100026033.1.9",
          "form": "습한거는 사실 더싫긴하지만 ㅜㅜㅋㅋ",
          "original_form": "습한거는 사실 더싫긴하지만 ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:07"
        },
        {
          "id": "MDRW2100026033.1.10",
          "form": "코로나신경쓰느라 미세먼저 정보는 상대적으로 체크안하게되네요",
          "original_form": "코로나신경쓰느라 미세먼저 정보는 상대적으로 체크안하게되네요",
          "speaker_id": "2",
          "time": "20210901 19:07"
        },
        {
          "id": "MDRW2100026033.1.11",
          "form": "맞아요. 미세먼지는",
          "original_form": "맞아요. 미세먼지는",
          "speaker_id": "1",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100026033.1.12",
          "form": "체크안한지 꽤됐어요, ㅜㅜ",
          "original_form": "체크안한지 꽤됐어요, ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100026033.1.13",
          "form": "담주도 비가 많이 온다고 하나요?",
          "original_form": "담주도 비가 많이 온다고 하나요?",
          "speaker_id": "2",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100026033.1.14",
          "form": "다음주는 흐림이던데ㅜ",
          "original_form": "다음주는 흐림이던데ㅜ",
          "speaker_id": "1",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100026033.1.15",
          "form": "설마 또 비가올까요ㅜㅜ",
          "original_form": "설마 또 비가올까요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100026033.1.16",
          "form": "일기예보가 요즘 믿을게 못되서요",
          "original_form": "일기예보가 요즘 믿을게 못되서요",
          "speaker_id": "2",
          "time": "20210901 19:21"
        },
        {
          "id": "MDRW2100026033.1.17",
          "form": "지금 제주는 갑자기 번개가.. @@",
          "original_form": "지금 제주는 갑자기 번개가.. @@",
          "speaker_id": "2",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026033.1.18",
          "form": "맞아요.ㅜㅜ",
          "original_form": "맞아요.ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100026033.1.19",
          "form": "아구 거기는 번개도쳤군요.ㅜ",
          "original_form": "아구 거기는 번개도쳤군요.ㅜ",
          "speaker_id": "1",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100026033.1.20",
          "form": "제주는 진짜 예상하기가 어렵네요",
          "original_form": "제주는 진짜 예상하기가 어렵네요",
          "speaker_id": "2",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100026033.1.21",
          "form": "그렇네요. 여기보다 더하겠어요.",
          "original_form": "그렇네요. 여기보다 더하겠어요.",
          "speaker_id": "1",
          "time": "20210901 20:00"
        },
        {
          "id": "MDRW2100026033.1.22",
          "form": "내일은 비안올것같던데ㅜ",
          "original_form": "내일은 비안올것같던데ㅜ",
          "speaker_id": "1",
          "time": "20210901 20:00"
        },
        {
          "id": "MDRW2100026033.1.23",
          "form": "제주도는 오려나요?ㅜ",
          "original_form": "제주도는 오려나요?ㅜ",
          "speaker_id": "1",
          "time": "20210901 20:00"
        },
        {
          "id": "MDRW2100026033.1.24",
          "form": "제주는 하루에 2-3가지 날씨가 복합적으로 겹쳐서 어떤날 하기가 어려워요",
          "original_form": "제주는 하루에 2-3가지 날씨가 복합적으로 겹쳐서 어떤날 하기가 어려워요",
          "speaker_id": "2",
          "time": "20210901 20:01"
        }
      ]
    }
