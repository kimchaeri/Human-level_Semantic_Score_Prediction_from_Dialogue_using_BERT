        {
          "id": "MDRW2100031678.1.1",
          "form": "안경 사셨나요?",
          "original_form": "안경 사셨나요?",
          "speaker_id": "1",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100031678.1.2",
          "form": "ㅋㅋㅋ네",
          "original_form": "ㅋㅋㅋ네",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100031678.1.3",
          "form": "니콘렌즈",
          "original_form": "니콘렌즈",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100031678.1.4",
          "form": "했어요",
          "original_form": "했어요",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100031678.1.5",
          "form": "니콘렌즈는 좋은건가요?",
          "original_form": "니콘렌즈는 좋은건가요?",
          "speaker_id": "1",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031678.1.6",
          "form": "대명렌즈 하려고 했는데 내일 찾으러 오래서",
          "original_form": "대명렌즈 하려고 했는데 내일 찾으러 오래서",
          "speaker_id": "2",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031678.1.7",
          "form": "니콘 선택하고",
          "original_form": "니콘 선택하고",
          "speaker_id": "2",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031678.1.8",
          "form": "그럼 주문한건 취소된건가요",
          "original_form": "그럼 주문한건 취소된건가요",
          "speaker_id": "1",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031678.1.9",
          "form": "검색해보니 좋긴하네요",
          "original_form": "검색해보니 좋긴하네요",
          "speaker_id": "2",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031678.1.10",
          "form": "안경 잘모르는ㄷ0 전문가시네용",
          "original_form": "안경 잘모르는ㄷ0 전문가시네용",
          "speaker_id": "1",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031678.1.11",
          "form": "아뇨 대명꺼 없다해서 걍 니콘으로 그 자리에서 바로 했어요",
          "original_form": "아뇨 대명꺼 없다해서 걍 니콘으로 그 자리에서 바로 했어요",
          "speaker_id": "2",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100031678.1.12",
          "form": "엄마 안경렌즈는 최소 안돼서 걍 쓰려고요",
          "original_form": "엄마 안경렌즈는 최소 안돼서 걍 쓰려고요",
          "speaker_id": "2",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100031678.1.13",
          "form": "잘했네요 얼만가요",
          "original_form": "잘했네요 얼만가요",
          "speaker_id": "1",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100031678.1.14",
          "form": "1.56에 6만원 줬어요",
          "original_form": "1.56에 6만원 줬어요",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100031678.1.15",
          "form": "아아 안경을 각자 산거군요",
          "original_form": "아아 안경을 각자 산거군요",
          "speaker_id": "1",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100031678.1.16",
          "form": "1번압축",
          "original_form": "1번압축",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100031678.1.17",
