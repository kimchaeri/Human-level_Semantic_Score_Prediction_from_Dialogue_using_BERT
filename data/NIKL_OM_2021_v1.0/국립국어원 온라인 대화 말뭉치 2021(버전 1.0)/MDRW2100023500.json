      "utterance": [
        {
          "id": "MDRW2100023500.1.1",
          "form": "웹툰은이거아니야?",
          "original_form": "웹툰은이거아니야?",
          "speaker_id": "1",
          "time": "20210830 09:54"
        },
        {
          "id": "MDRW2100023500.1.2",
          "form": "오오오오 천재네",
          "original_form": "오오오오 천재네",
          "speaker_id": "2",
          "time": "20210830 10:58"
        },
        {
          "id": "MDRW2100023500.1.3",
          "form": "우사이라는 웹툰 알아?",
          "original_form": "우사이라는 웹툰 알아?",
          "speaker_id": "2",
          "time": "20210830 10:58"
        },
        {
          "id": "MDRW2100023500.1.4",
          "form": "우리 카페 사장님이 이상하다라는 네이버 베도 웹툰인데",
          "original_form": "우리 카페 사장님이 이상하다라는 네이버 베도 웹툰인데",
          "speaker_id": "2",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100023500.1.5",
          "form": "앎?",
          "original_form": "앎?",
          "speaker_id": "2",
          "time": "20210830 11:00"
        },
        {
          "id": "MDRW2100023500.1.6",
          "form": "몰라",
          "original_form": "몰라",
          "speaker_id": "1",
          "time": "20210830 11:54"
        },
        {
          "id": "MDRW2100023500.1.7",
          "form": "처음들어봤어?",
          "original_form": "처음들어봤어?",
          "speaker_id": "1",
          "time": "20210830 11:54"
        },
        {
          "id": "MDRW2100023500.1.8",
          "form": "어",
          "original_form": "어",
          "speaker_id": "1",
          "time": "20210830 11:54"
        },
        {
          "id": "MDRW2100023500.1.9",
          "form": "어ㅐ?",
          "original_form": "어ㅐ?",
          "speaker_id": "1",
          "time": "20210830 11:54"
        },
        {
          "id": "MDRW2100023500.1.10",
          "form": "그거 잔잔하게 재밌음",
          "original_form": "그거 잔잔하게 재밌음",
          "speaker_id": "2",
          "time": "20210830 11:56"
        },
        {
          "id": "MDRW2100023500.1.11",
          "form": "그냥 카페 이야기인데 인외 캐릭터가 많이 나옴",
          "original_form": "그냥 카페 이야기인데 인외 캐릭터가 많이 나옴",
          "speaker_id": "2",
          "time": "20210830 11:56"
        },
        {
          "id": "MDRW2100023500.1.12",
          "form": "그리고 주인공 아빠빼고 다 여성임",
          "original_form": "그리고 주인공 아빠빼고 다 여성임",
          "speaker_id": "2",
          "time": "20210830 11:56"
        },
        {
          "id": "MDRW2100023500.1.13",
          "form": "그리고 그림체가 깔끔함",
          "original_form": "그리고 그림체가 깔끔함",
          "speaker_id": "2",
          "time": "20210830 11:58"
        },
        {
          "id": "MDRW2100023500.1.14",
          "form": "그냥카페에서",
          "original_form": "그냥카페에서",
          "speaker_id": "1",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100023500.1.15",
          "form": "일어나는일들",
          "original_form": "일어나는일들",
          "speaker_id": "1",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100023500.1.16",
          "form": "야기임?",
          "original_form": "야기임?",
          "speaker_id": "1",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100023500.1.17",
          "form": "일상?",
          "original_form": "일상?",
          "speaker_id": "1",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100023500.1.18",
          "form": "ㅇㅇ 근데 약간의 오컬트가 섞여있는",
          "original_form": "ㅇㅇ 근데 약간의 오컬트가 섞여있는",
          "speaker_id": "2",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100023500.1.19",
          "form": "그런 느낌",
          "original_form": "그런 느낌",
          "speaker_id": "2",
          "time": "20210830 12:01"
        },
        {
          "id": "MDRW2100023500.1.20",
          "form": "오컬트는어떤거임?",
          "original_form": "오컬트는어떤거임?",
          "speaker_id": "1",
          "time": "20210830 12:03"
        },
        {
          "id": "MDRW2100023500.1.21",
          "form": "약간 요상꾸리 뭐라 그러냐",
          "original_form": "약간 요상꾸리 뭐라 그러냐",
          "speaker_id": "2",
          "time": "20210830 12:28"
        },
        {
          "id": "MDRW2100023500.1.22",
          "form": "과학적으로 증명 되지 않는 그런 존재들에 대한 이야기?",
          "original_form": "과학적으로 증명 되지 않는 그런 존재들에 대한 이야기?",
          "speaker_id": "2",
          "time": "20210830 12:28"
        },
        {
          "id": "MDRW2100023500.1.23",
          "form": "퇴마나 악령 요상꾸리 나오는 것들 그런거",
          "original_form": "퇴마나 악령 요상꾸리 나오는 것들 그런거",
          "speaker_id": "2",
          "time": "20210830 12:30"
        },
        {
          "id": "MDRW2100023500.1.24",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210830 12:43"
        },
        {
          "id": "MDRW2100023500.1.25",
          "form": "분신사바같은거임?",
          "original_form": "분신사바같은거임?",
          "speaker_id": "1",
          "time": "20210830 12:43"
        },
        {
          "id": "MDRW2100023500.1.26",
          "form": "오 그것도 오컬트 장르 중에 한 갈래 겠지?",
          "original_form": "오 그것도 오컬트 장르 중에 한 갈래 겠지?",
          "speaker_id": "2",
          "time": "20210830 12:47"
        },
        {
          "id": "MDRW2100023500.1.27",
          "form": "근데 우사이는 그렇게 막 공포 음산 이 분위기는 아님",
          "original_form": "근데 우사이는 그렇게 막 공포 음산 이 분위기는 아님",
          "speaker_id": "2",
          "time": "20210830 12:47"
        },
        {
          "id": "MDRW2100023500.1.28",
          "form": "일상카페물에 인외캐릭터가 많은 그런 느낌",
          "original_form": "일상카페물에 인외캐릭터가 많은 그런 느낌",
          "speaker_id": "2",
          "time": "20210830 12:47"
        },
        {
          "id": "MDRW2100023500.1.29",
          "form": "인외캐릭터는어떤거임?",
          "original_form": "인외캐릭터는어떤거임?",
          "speaker_id": "1",
          "time": "20210830 12:49"
        },
        {
          "id": "MDRW2100023500.1.30",
          "form": "인간성외외",
          "original_form": "인간성외외",
          "speaker_id": "1",
          "time": "20210830 12:49"
        },
        {
          "id": "MDRW2100023500.1.31",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210830 12:49"
        },
        {
          "id": "MDRW2100023500.1.32",
          "form": "인간 외의 종?",
          "original_form": "인간 외의 종?",
          "speaker_id": "2",
