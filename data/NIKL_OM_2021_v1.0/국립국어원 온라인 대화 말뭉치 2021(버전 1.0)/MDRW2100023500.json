        },
        {
          "id": "MDRW2100023500.1.33",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100023500.1.34",
          "form": "외계인이라거나 해골 이라던가 그런거",
          "original_form": "외계인이라거나 해골 이라던가 그런거",
          "speaker_id": "2",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100023500.1.35",
          "form": "아ㅏ",
          "original_form": "아ㅏ",
          "speaker_id": "1",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100023500.1.36",
