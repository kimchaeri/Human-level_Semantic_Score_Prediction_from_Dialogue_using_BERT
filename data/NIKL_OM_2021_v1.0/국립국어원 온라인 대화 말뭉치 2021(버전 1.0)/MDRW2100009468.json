      "utterance": [
        {
          "id": "MDRW2100009468.1.1",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009468.1.2",
          "form": "날씨 어제가 대박이었죠",
          "original_form": "날씨 어제가 대박이었죠",
          "speaker_id": "2",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100009468.1.3",
          "form": "네 저 물에 떠내려 가는줄 알았어요",
          "original_form": "네 저 물에 떠내려 가는줄 알았어요",
          "speaker_id": "1",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009468.1.4",
          "form": "저는 우산을 썼는데도 우산 안으로 빗물이 떨어지더라고요",
          "original_form": "저는 우산을 썼는데도 우산 안으로 빗물이 떨어지더라고요",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009468.1.5",
          "form": "그래서 매우 짜증이 ㅋㅋ",
          "original_form": "그래서 매우 짜증이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009468.1.6",
          "form": "저런 안 다치셨나요",
          "original_form": "저런 안 다치셨나요",
          "speaker_id": "2",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009468.1.7",
          "form": "우산 공프 우선정도 되야해요 ㅋㅋ",
          "original_form": "우산 공프 우선정도 되야해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009468.1.8",
          "form": "치과가느라",
          "original_form": "치과가느라",
          "speaker_id": "1",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009468.1.9",
          "form": "빗속을 달렸어요",
          "original_form": "빗속을 달렸어요",
          "speaker_id": "1",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009468.1.10",
          "form": "저는 그거 세번이나 당했어요",
          "original_form": "저는 그거 세번이나 당했어요",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009468.1.11",
          "form": "차에서 촥 빗물 맞는거",
          "original_form": "차에서 촥 빗물 맞는거",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009468.1.12",
          "form": "드라마에서나 보던걸 직접당하다니 ㄷㄷ",
          "original_form": "드라마에서나 보던걸 직접당하다니 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009468.1.13",
          "form": "ㅋㅋ 그러게요",
          "original_form": "ㅋㅋ 그러게요",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009468.1.14",
          "form": "드라마가 현실에서 일어나요",
          "original_form": "드라마가 현실에서 일어나요",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009468.1.15",
          "form": "오늘은 비온다더니 안오고",
          "original_form": "오늘은 비온다더니 안오고",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009468.1.16",
          "form": "날씨가 알수가 없네요",
          "original_form": "날씨가 알수가 없네요",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009468.1.17",
          "form": "저도 촥 당했었어요",
          "original_form": "저도 촥 당했었어요",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009468.1.18",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009468.1.19",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009468.1.20",
          "form": "이미 비가 옆으로 와서 다젖어서 화도 안나더라고요",
          "original_form": "이미 비가 옆으로 와서 다젖어서 화도 안나더라고요",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009468.1.21",
          "form": "헛웃음 나더라는 ㅋㅋ",
          "original_form": "헛웃음 나더라는 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009468.1.22",
          "form": "특히 세번 당하니까",
          "original_form": "특히 세번 당하니까",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009468.1.23",
          "form": "저도요 저 잘당해요",
          "original_form": "저도요 저 잘당해요",
          "speaker_id": "1",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009468.1.24",
          "form": "그러고 그냥가요",
          "original_form": "그러고 그냥가요",
          "speaker_id": "1",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009468.1.25",
          "form": "세탁비 받은적도 없어요",
          "original_form": "세탁비 받은적도 없어요",
          "speaker_id": "1",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009468.1.26",
          "form": "아 비싫어요",
          "original_form": "아 비싫어요",
          "speaker_id": "1",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009468.1.27",
          "form": "집에와서 근데 우산검색함",
          "original_form": "집에와서 근데 우산검색함",
          "speaker_id": "2",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009468.1.28",
          "form": "어떤 우산요",
          "original_form": "어떤 우산요",
          "speaker_id": "1",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009468.1.29",
          "form": "우산 안으로 비떨어지는거에 빡쳐서요 ㅋㅋㅋ",
          "original_form": "우산 안으로 비떨어지는거에 빡쳐서요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009468.1.30",
          "form": "고급우산?",
          "original_form": "고급우산?",
          "speaker_id": "2",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009468.1.31",
          "form": "비가안새는 ㅋㅋ",
          "original_form": "비가안새는 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009468.1.32",
          "form": "아 저도 그래요",
          "original_form": "아 저도 그래요",
          "speaker_id": "1",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009468.1.33",
          "form": "어깨가 다젖고ㅠㅜ",
          "original_form": "어깨가 다젖고ㅠㅜ",
          "speaker_id": "1",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009468.1.34",
          "form": "고급은 안그래요?",
          "original_form": "고급은 안그래요?",
          "speaker_id": "1",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009468.1.35",
          "form": "막 폭포아래서 실험한 것도 있더라고요",
          "original_form": "막 폭포아래서 실험한 것도 있더라고요",
          "speaker_id": "2",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100009468.1.36",
          "form": "오 뭐예요?",
          "original_form": "오 뭐예요?",
          "speaker_id": "1",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100009468.1.37",
          "form": "평생 as해준다고도히고",
          "original_form": "평생 as해준다고도히고",
          "speaker_id": "2",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100009468.1.38",
          "form": "근데 가격이 거진 10만원  ㅋㅋ",
          "original_form": "근데 가격이 거진 10만원  ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009468.1.39",
          "form": "엥 넘비싸요",
          "original_form": "엥 넘비싸요",
          "speaker_id": "1",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100009468.1.40",
          "form": "해줄만하네요 ㅎㅎ10만원",
          "original_form": "해줄만하네요 ㅎㅎ10만원",
          "speaker_id": "1",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100009468.1.41",
          "form": "그죠 저도 곱게 창닫았어요",
          "original_form": "그죠 저도 곱게 창닫았어요",
          "speaker_id": "2",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100009468.1.42",
          "form": "망가지는건 그렇다쳐도",
          "original_form": "망가지는건 그렇다쳐도",
          "speaker_id": "2",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100009468.1.43",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100009468.1.44",
          "form": "잃어버리거나 도난당하면? 생각하니 ㅋ",
          "original_form": "잃어버리거나 도난당하면? 생각하니 ㅋ",
          "speaker_id": "2",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100009468.1.45",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100009468.1.46",
          "form": "그게 문제예요",
          "original_form": "그게 문제예요",
          "speaker_id": "1",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100009468.1.47",
          "form": "조도 그 가격은 창닫아요 ㅋㅋ",
          "original_form": "조도 그 가격은 창닫아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:21"
        },
        {
          "id": "MDRW2100009468.1.48",
          "form": "네 그러세요",
          "original_form": "네 그러세요",
          "speaker_id": "1",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100009468.1.49",
          "form": "오늘 넘잼났어요 ㅎㅎ",
          "original_form": "오늘 넘잼났어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100009468.1.50",
          "form": "자려고요",
          "original_form": "자려고요",
          "speaker_id": "1",
          "time": "20210824 00:04"
        }
      ]
