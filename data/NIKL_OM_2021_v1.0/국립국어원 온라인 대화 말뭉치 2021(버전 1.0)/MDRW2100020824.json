      "utterance": [
        {
          "id": "MDRW2100020824.1.1",
          "form": "오 직접자르시나요?",
          "original_form": "오 직접자르시나요?",
          "speaker_id": "2",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020824.1.2",
          "form": "아뇨 무한정 기르고 있어요 ㅋㅋ",
          "original_form": "아뇨 무한정 기르고 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020824.1.3",
          "form": "앗 그것도 좋은 방법이네요",
          "original_form": "앗 그것도 좋은 방법이네요",
          "speaker_id": "2",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020824.1.4",
          "form": "님은 헤어스타일이 어떠세요?",
          "original_form": "님은 헤어스타일이 어떠세요?",
          "speaker_id": "1",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020824.1.5",
          "form": "저는 짧은머리를 유지중이에요 관리가 편해서",
          "original_form": "저는 짧은머리를 유지중이에요 관리가 편해서",
          "speaker_id": "2",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020824.1.6",
          "form": "아 확실히 머리감고 말리고는 편하더라고요!",
          "original_form": "아 확실히 머리감고 말리고는 편하더라고요!",
          "speaker_id": "1",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020824.1.7",
          "form": "아무래도 그렇죠?",
          "original_form": "아무래도 그렇죠?",
          "speaker_id": "2",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020824.1.8",
          "form": "근데 미용실을 자주가야 하지 않나요ㅠㅠ",
          "original_form": "근데 미용실을 자주가야 하지 않나요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020824.1.9",
          "form": "최소 한달에 한번은 가는 것 같아요",
          "original_form": "최소 한달에 한번은 가는 것 같아요",
          "speaker_id": "2",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020824.1.10",
          "form": "헉 엄청 자주가시네요",
          "original_form": "헉 엄청 자주가시네요",
          "speaker_id": "1",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020824.1.11",
          "form": "그러지 않으면 너무 지저분해져서요",
          "original_form": "그러지 않으면 너무 지저분해져서요",
          "speaker_id": "2",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020824.1.12",
          "form": "저는 그냥 기른머리예요 ㅎㅎ",
          "original_form": "저는 그냥 기른머리예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020824.1.13",
          "form": "그럼 머리가 엄청 기시겠어요",
          "original_form": "그럼 머리가 엄청 기시겠어요",
          "speaker_id": "2",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020824.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020824.1.15",
          "form": "저는 요즘 다이어트 중이에요",
