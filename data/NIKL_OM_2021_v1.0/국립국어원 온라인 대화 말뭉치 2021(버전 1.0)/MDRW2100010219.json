      "utterance": [
        {
          "id": "MDRW2100010219.1.1",
          "form": "귀여운 잡지는 잘 모르겠는데",
          "original_form": "귀여운 잡지는 잘 모르겠는데",
          "speaker_id": "1",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010219.1.2",
          "form": "하퍼스바자 이런거 보니?",
          "original_form": "하퍼스바자 이런거 보니?",
          "speaker_id": "1",
          "time": "20210823 15:47"
        },
        {
          "id": "MDRW2100010219.1.3",
          "form": "그게 뭐야",
          "original_form": "그게 뭐야",
          "speaker_id": "2",
          "time": "20210823 15:47"
        },
        {
          "id": "MDRW2100010219.1.4",
          "form": "잡지 이름이야.",
          "original_form": "잡지 이름이야.",
          "speaker_id": "1",
          "time": "20210823 15:47"
        },
        {
          "id": "MDRW2100010219.1.5",
          "form": "생전 처음들어보는데",
          "original_form": "생전 처음들어보는데",
          "speaker_id": "2",
          "time": "20210823 15:47"
        },
        {
          "id": "MDRW2100010219.1.6",
          "form": "너눈 어떤 잡지를 본 적 있니?",
          "original_form": "너눈 어떤 잡지를 본 적 있니?",
          "speaker_id": "1",
          "time": "20210823 15:47"
        },
        {
          "id": "MDRW2100010219.1.7",
          "form": "혹시 잡지 이룸이야?",
          "original_form": "혹시 잡지 이룸이야?",
          "speaker_id": "2",
          "time": "20210823 15:47"
        },
        {
          "id": "MDRW2100010219.1.8",
          "form": "맞아!",
          "original_form": "맞아!",
          "speaker_id": "1",
          "time": "20210823 15:47"
        },
        {
          "id": "MDRW2100010219.1.9",
          "form": "나는 코스모폴리탄 본적있어",
          "original_form": "나는 코스모폴리탄 본적있어",
          "speaker_id": "2",
          "time": "20210823 15:48"
        },
        {
          "id": "MDRW2100010219.1.10",
          "form": "미용실에서 읽었었지",
          "original_form": "미용실에서 읽었었지",
          "speaker_id": "2",
          "time": "20210823 15:48"
        },
        {
          "id": "MDRW2100010219.1.11",
          "form": "예전에 문구점에서도 팔던 잡지 스타일은 기억나니?",
          "original_form": "예전에 문구점에서도 팔던 잡지 스타일은 기억나니?",
          "speaker_id": "1",
          "time": "20210823 15:48"
        },
        {
          "id": "MDRW2100010219.1.12",
          "form": "알지 그거 한 천원했나",
          "original_form": "알지 그거 한 천원했나",
          "speaker_id": "2",
          "time": "20210823 15:48"
        },
        {
          "id": "MDRW2100010219.1.13",
          "form": "그거사면 스티커도 줬잖아!",
          "original_form": "그거사면 스티커도 줬잖아!",
          "speaker_id": "2",
          "time": "20210823 15:48"
        },
        {
          "id": "MDRW2100010219.1.14",
          "form": "그거보단 비싸지 않았을까",
          "original_form": "그거보단 비싸지 않았을까",
          "speaker_id": "1",
          "time": "20210823 15:48"
        },
        {
          "id": "MDRW2100010219.1.15",
          "form": "맞아!",
          "original_form": "맞아!",
          "speaker_id": "1",
          "time": "20210823 15:48"
        },
        {
          "id": "MDRW2100010219.1.16",
          "form": "내가 좋아하는 아이돌이 나왔을때 사본적있어",
          "original_form": "내가 좋아하는 아이돌이 나왔을때 사본적있어",
          "speaker_id": "2",
          "time": "20210823 15:49"
        },
        {
          "id": "MDRW2100010219.1.17",
          "form": "유명 아이돌들이 대거출연해서 모아놨던 기억이 있어",
          "original_form": "유명 아이돌들이 대거출연해서 모아놨던 기억이 있어",
          "speaker_id": "1",
          "time": "20210823 15:49"
        },
        {
          "id": "MDRW2100010219.1.18",
          "form": "나도.",
          "original_form": "나도.",
          "speaker_id": "1",
          "time": "20210823 15:49"
        },
        {
          "id": "MDRW2100010219.1.19",
          "form": "너는 사본적있니?",
          "original_form": "너는 사본적있니?",
          "speaker_id": "2",
          "time": "20210823 15:49"
        },
        {
          "id": "MDRW2100010219.1.20",
          "form": "그럼.",
          "original_form": "그럼.",
          "speaker_id": "1",
          "time": "20210823 15:49"
        },
        {
          "id": "MDRW2100010219.1.21",
          "form": "사서 숨겨뒀단다",
          "original_form": "사서 숨겨뒀단다",
          "speaker_id": "1",
          "time": "20210823 15:49"
        },
        {
          "id": "MDRW2100010219.1.22",
          "form": "지금은 흑역사야.",
          "original_form": "지금은 흑역사야.",
          "speaker_id": "1",
          "time": "20210823 15:49"
        },
        {
          "id": "MDRW2100010219.1.23",
          "form": "어디있는지조차 모르겠어.",
          "original_form": "어디있는지조차 모르겠어.",
          "speaker_id": "1",
          "time": "20210823 15:49"
        }
      ]
