      "utterance": [
        {
          "id": "MDRW2100035877.1.1",
          "form": "우어ㅏ..굴이..들어가나요?",
          "original_form": "우어ㅏ..굴이..들어가나요?",
          "speaker_id": "2",
          "time": "20210830 10:09"
        },
        {
          "id": "MDRW2100035877.1.2",
          "form": "두려워요 ㅣㅋㅋㅋㅋ",
          "original_form": "두려워요 ㅣㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:09"
        },
        {
          "id": "MDRW2100035877.1.3",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210830 10:09"
        },
        {
          "id": "MDRW2100035877.1.4",
          "form": "저번에는 김치에 생선 넣는거보고",
          "original_form": "저번에는 김치에 생선 넣는거보고",
          "speaker_id": "1",
          "time": "20210830 10:09"
        },
        {
          "id": "MDRW2100035877.1.5",
          "form": "기겁했어요",
          "original_form": "기겁했어요",
          "speaker_id": "1",
          "time": "20210830 10:09"
        },
        {
          "id": "MDRW2100035877.1.6",
          "form": "저희는 친가는 아예 젓갈을 안 넣고 외가쪽만 조금 들어가요 ㅋㅋㅋㅋㅎㅎ어?",
          "original_form": "저희는 친가는 아예 젓갈을 안 넣고 외가쪽만 조금 들어가요 ㅋㅋㅋㅋㅎㅎ어?",
          "speaker_id": "2",
          "time": "20210830 10:09"
        },
        {
          "id": "MDRW2100035877.1.7",
          "form": "어....그...생선이요?",
          "original_form": "어....그...생선이요?",
          "speaker_id": "2",
          "time": "20210830 10:09"
        },
        {
          "id": "MDRW2100035877.1.8",
          "form": "와 너무 부럽다ㅠㅠ",
          "original_form": "와 너무 부럽다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 10:09"
        },
        {
          "id": "MDRW2100035877.1.9",
          "form": "넹냉",
          "original_form": "넹냉",
          "speaker_id": "1",
          "time": "20210830 10:09"
        },
        {
          "id": "MDRW2100035877.1.10",
          "form": "상상하시는 그 생선이!",
          "original_form": "상상하시는 그 생선이!",
          "speaker_id": "1",
          "time": "20210830 10:09"
        },
        {
          "id": "MDRW2100035877.1.11",
          "form": "저 한번도 안 먹어서 맛이 궁금해요 ㅋㅋㅋㅋㅋ신기하다...",
          "original_form": "저 한번도 안 먹어서 맛이 궁금해요 ㅋㅋㅋㅋㅋ신기하다...",
          "speaker_id": "2",
          "time": "20210830 10:09"
        },
        {
          "id": "MDRW2100035877.1.12",
          "form": "통째로 들어갑니다",
          "original_form": "통째로 들어갑니다",
          "speaker_id": "1",
          "time": "20210830 10:09"
        },
        {
          "id": "MDRW2100035877.1.13",
          "form": "고등어......그런....애들이요?",
          "original_form": "고등어......그런....애들이요?",
          "speaker_id": "2",
          "time": "20210830 10:10"
        },
        {
          "id": "MDRW2100035877.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ 넹넹",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 넹넹",
          "speaker_id": "1",
          "time": "20210830 10:10"
        },
        {
          "id": "MDRW2100035877.1.15",
          "form": "제가 생선은 비려서 잘 안먹어서",
          "original_form": "제가 생선은 비려서 잘 안먹어서",
          "speaker_id": "1",
          "time": "20210830 10:10"
        },
        {
          "id": "MDRW2100035877.1.16",
          "form": "종류는 모르겠는데",
          "original_form": "종류는 모르겠는데",
          "speaker_id": "1",
          "time": "20210830 10:10"
        },
        {
          "id": "MDRW2100035877.1.17",
          "form": "우와 전라도 김치의 특색이겠죠? 신기해요!",
          "original_form": "우와 전라도 김치의 특색이겠죠? 신기해요!",
          "speaker_id": "2",
          "time": "20210830 10:10"
        },
        {
          "id": "MDRW2100035877.1.18",
          "form": "갈치는 확실한것같구",
          "original_form": "갈치는 확실한것같구",
          "speaker_id": "1",
          "time": "20210830 10:10"
        },
        {
          "id": "MDRW2100035877.1.19",
          "form": "그런것같아용ㅋㅋㅋㅋ",
          "original_form": "그런것같아용ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:10"
        },
        {
          "id": "MDRW2100035877.1.20",
          "form": "굴들어간건 식당에서 종종봤었거든요 아 ㅋㅋㅋㅋㅋ",
          "original_form": "굴들어간건 식당에서 종종봤었거든요 아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 10:10"
        },
        {
          "id": "MDRW2100035877.1.21",
          "form": "아 갈치!",
          "original_form": "아 갈치!",
          "speaker_id": "2",
          "time": "20210830 10:10"
        },
        {
          "id": "MDRW2100035877.1.22",
          "form": "앜ㅋㅋㅋㅋ 실제로보면 식은땀나요ㅠㅠ",
          "original_form": "앜ㅋㅋㅋㅋ 실제로보면 식은땀나요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 10:10"
        },
        {
          "id": "MDRW2100035877.1.23",
          "form": "갈치 들어간 거 들어봤어요!ㅋㅋㅋㅋㅋㅋ",
          "original_form": "갈치 들어간 거 들어봤어요!ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 10:10"
        }
      ]
    }
