      "utterance": [
        {
          "id": "MDRW2100046155.1.1",
          "form": "사고 싶은거? 동생 산 그 이어폰",
          "original_form": "사고 싶은거? 동생 산 그 이어폰",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046155.1.2",
          "form": "좀 한번 보고 싶긴 하더라",
          "original_form": "좀 한번 보고 싶긴 하더라",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046155.1.3",
          "form": "너두 구매 하려고",
          "original_form": "너두 구매 하려고",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046155.1.4",
          "form": "음 한번 관심이 간다 이거지!",
          "original_form": "음 한번 관심이 간다 이거지!",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046155.1.5",
          "form": "성능이 좋긴 한거니",
          "original_form": "성능이 좋긴 한거니",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046155.1.6",
          "form": "내가 저번에 동생 온거 써보니까",
          "original_form": "내가 저번에 동생 온거 써보니까",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046155.1.7",
          "form": "좋던뎅?? 엄마는 안봤어?",
          "original_form": "좋던뎅?? 엄마는 안봤어?",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046155.1.8",
          "form": "보긴 했는데 성능은 모르겠어",
          "original_form": "보긴 했는데 성능은 모르겠어",
          "speaker_id": "1",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100046155.1.9",
          "form": "전화도 해보지 않았나? 전화 어땠는뎅",
          "original_form": "전화도 해보지 않았나? 전화 어땠는뎅",
          "speaker_id": "2",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100046155.1.10",
          "form": "잘들리긴 했어",
          "original_form": "잘들리긴 했어",
          "speaker_id": "1",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100046155.1.11",
          "form": "응응 거봐 성능 괜찮아보여",
          "original_form": "응응 거봐 성능 괜찮아보여",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100046155.1.12",
          "form": "삼성 오프라인 매장 가서 한번 더 볼까 하지 뭐",
          "original_form": "삼성 오프라인 매장 가서 한번 더 볼까 하지 뭐",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100046155.1.13",
          "form": "가격 비싼것 아닌가",
          "original_form": "가격 비싼것 아닌가",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046155.1.14",
          "form": "한 십만원 하지?",
          "original_form": "한 십만원 하지?",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046155.1.15",
          "form": "온라인은 얼마였는데",
          "original_form": "온라인은 얼마였는데",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046155.1.16",
          "form": "온라인도 그정도 해 아 좀더 하나?",
          "original_form": "온라인도 그정도 해 아 좀더 하나?",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046155.1.17",
          "form": "아 당근은 어때",
          "original_form": "아 당근은 어때",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046155.1.18",
          "form": "한번 고려해봐야겠다 당근마켓!!",
          "original_form": "한번 고려해봐야겠다 당근마켓!!",
