      "utterance": [
        {
          "id": "MDRW2100024458.1.1",
          "form": "커피 자주드시나요?",
          "original_form": "커피 자주드시나요?",
          "speaker_id": "2",
          "time": "20210831 12:04"
        },
        {
          "id": "MDRW2100024458.1.2",
          "form": "커피 알못인데 어찌어찌 하루 두잔은 마시는것 같아요",
          "original_form": "커피 알못인데 어찌어찌 하루 두잔은 마시는것 같아요",
          "speaker_id": "1",
          "time": "20210831 12:06"
        },
        {
          "id": "MDRW2100024458.1.3",
          "form": "name2님은 자주 드시나요?",
          "original_form": "&name2&님은 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210831 12:06"
        },
        {
          "id": "MDRW2100024458.1.4",
          "form": "카페인 마니 먹으면 잠안와서",
          "original_form": "카페인 마니 먹으면 잠안와서",
          "speaker_id": "2",
          "time": "20210831 13:22"
        },
        {
          "id": "MDRW2100024458.1.5",
          "form": "디카페인으로 한잔씩 먹어요",
          "original_form": "디카페인으로 한잔씩 먹어요",
          "speaker_id": "2",
          "time": "20210831 13:22"
        },
        {
          "id": "MDRW2100024458.1.6",
          "form": "미숫가루 선호합니다",
          "original_form": "미숫가루 선호합니다",
          "speaker_id": "2",
          "time": "20210831 13:22"
        },
        {
          "id": "MDRW2100024458.1.7",
          "form": "아 카페인에 민감하시군요",
          "original_form": "아 카페인에 민감하시군요",
          "speaker_id": "1",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024458.1.8",
          "form": "미숫가루 맛나죠ㅎㅎ 간만에 마시고 싶네요",
          "original_form": "미숫가루 맛나죠ㅎㅎ 간만에 마시고 싶네요",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024458.1.9",
          "form": "전 경상도사는데 전라도 김제에 우리건강원이라고 미숫가루 정말 좋아요 추천합니다",
          "original_form": "전 경상도사는데 전라도 김제에 우리건강원이라고 미숫가루 정말 좋아요 추천합니다",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100024458.1.10",
          "form": "오오 배달도 되는거죠?",
          "original_form": "오오 배달도 되는거죠?",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100024458.1.11",
          "form": "미숫가루 맛 차이는 신경 안쓰고 살았는데",
          "original_form": "미숫가루 맛 차이는 신경 안쓰고 살았는데",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100024458.1.12",
          "form": "고소한 풍미가 남다른가요?",
          "original_form": "고소한 풍미가 남다른가요?",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100024458.1.13",
          "form": "깜놀하실겁니다",
          "original_form": "깜놀하실겁니다",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100024458.1.14",
          "form": "그리고 우리나라 최대의 곡창지대에서 생산되는 곡물로만 만드세요",
          "original_form": "그리고 우리나라 최대의 곡창지대에서 생산되는 곡물로만 만드세요",
          "speaker_id": "2",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100024458.1.15",
          "form": "네이버 검색가능합니다",
          "original_form": "네이버 검색가능합니다",
          "speaker_id": "2",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100024458.1.16",
          "form": "오오 신뢰가ㅎ",
          "original_form": "오오 신뢰가ㅎ",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100024458.1.17",
          "form": "거기는 어떻게 아시게 됐어요?",
          "original_form": "거기는 어떻게 아시게 됐어요?",
          "speaker_id": "1",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100024458.1.18",
          "form": "맘카페에서",
          "original_form": "맘카페에서",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100024458.1.19",
          "form": "겟했습니다",
          "original_form": "겟했습니다",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100024458.1.20",
          "form": "아 저는 맘은 아닌데 어쩌다가 찾게되었어요 ㅋㅋ",
          "original_form": "아 저는 맘은 아닌데 어쩌다가 찾게되었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100024458.1.21",
          "form": "아ㅋㅋ",
          "original_form": "아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100024458.1.22",
          "form": "맘카페 인증이면 믿을만하죠ㅎㅎ",
          "original_form": "맘카페 인증이면 믿을만하죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100024458.1.23",
          "form": "오늘 하루 맛난거 드셨나요?",
          "original_form": "오늘 하루 맛난거 드셨나요?",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100024458.1.24",
          "form": "저는 지금 퇴근해서 부대찌개를 먹고 있습니다ㅎ",
          "original_form": "저는 지금 퇴근해서 부대찌개를 먹고 있습니다ㅎ",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100024458.1.25",
          "form": "님은 저녁 뭐 드셨어요?",
          "original_form": "님은 저녁 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100024458.1.26",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100024458.1.27",
          "form": "반칙!",
          "original_form": "반칙!",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100024458.1.28",
          "form": "연어솥밥먹엇구요 지금은 저 팀탐 구해서 우유에 찍어먹어봅니다 ㅋㅋ",
          "original_form": "연어솥밥먹엇구요 지금은 저 팀탐 구해서 우유에 찍어먹어봅니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100024458.1.29",
          "form": "아니...뭐가 반칙이죠",
          "original_form": "아니...뭐가 반칙이죠",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100024458.1.30",
          "form": "심지어 연어솥밥은 어떻게 생긴 겁니꽈!!",
          "original_form": "심지어 연어솥밥은 어떻게 생긴 겁니꽈!!",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100024458.1.31",
          "form": "이시간에 부대찌게는 너무 부럽습니다",
          "original_form": "이시간에 부대찌게는 너무 부럽습니다",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100024458.1.32",
          "form": "솥을 하나 삿어요( 20만원 )",
          "original_form": "솥을 하나 삿어요( 20만원 )",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100024458.1.33",
          "form": "전 연어솥밥이 더 그런데요",
          "original_form": "전 연어솥밥이 더 그런데요",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100024458.1.34",
          "form": "밥짓는 솥",
          "original_form": "밥짓는 솥",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100024458.1.35",
          "form": "심지어 직접 해드신 거?",
          "original_form": "심지어 직접 해드신 거?",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100024458.1.36",
          "form": "해주시더라구요 ㅋㅋ",
          "original_form": "해주시더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100024458.1.37",
          "form": "ㅂㄷㅂㄷ 20만원짜리 솥을 사는 재력과 밥력이 부럽습니다ㄷㄷ",
          "original_form": "ㅂㄷㅂㄷ 20만원짜리 솥을 사는 재력과 밥력이 부럽습니다ㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100024458.1.38",
          "form": "거기 밥을 하고",
          "original_form": "거기 밥을 하고",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100024458.1.39",
          "form": "아니ㅜ 결혼 바이럴 물러가라 물러가라",
          "original_form": "아니ㅜ 결혼 바이럴 물러가라 물러가라",
