      "utterance": [
        {
          "id": "MDRW2100019517.1.1",
          "form": "초등학생 때 같이 유럽간 거 기억나?",
          "original_form": "초등학생 때 같이 유럽간 거 기억나?",
          "speaker_id": "1",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019517.1.2",
          "form": "스페인 갔던 거 말하지?",
          "original_form": "스페인 갔던 거 말하지?",
          "speaker_id": "2",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019517.1.3",
          "form": "바르셀로나에서 우리 같이 있었잖아",
          "original_form": "바르셀로나에서 우리 같이 있었잖아",
          "speaker_id": "1",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019517.1.4",
          "form": "맞아 그때 진짜 재밌었지",
          "original_form": "맞아 그때 진짜 재밌었지",
          "speaker_id": "2",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019517.1.5",
          "form": "전시회 갔던게 기억에 남아",
          "original_form": "전시회 갔던게 기억에 남아",
          "speaker_id": "1",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019517.1.6",
          "form": "피카소?",
          "original_form": "피카소?",
          "speaker_id": "2",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019517.1.7",
          "form": "응 피카소의 어린 시절 그림이 기억에 남아",
          "original_form": "응 피카소의 어린 시절 그림이 기억에 남아",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019517.1.8",
          "form": "지금과 다른 화풍이지만 참 잘그렸지",
          "original_form": "지금과 다른 화풍이지만 참 잘그렸지",
          "speaker_id": "2",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019517.1.9",
          "form": "재능이라는게 느껴져",
          "original_form": "재능이라는게 느껴져",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019517.1.10",
          "form": "츄러스도 먹었지",
          "original_form": "츄러스도 먹었지",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019517.1.11",
          "form": "코코아 찍어먹는게 맛있었어!",
          "original_form": "코코아 찍어먹는게 맛있었어!",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019517.1.12",
          "form": "맞아 설탕뿌려서",
          "original_form": "맞아 설탕뿌려서",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019517.1.13",
          "form": "시나몬도 뿌려야지",
          "original_form": "시나몬도 뿌려야지",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019517.1.14",
          "form": "날씨도 좋았어",
          "original_form": "날씨도 좋았어",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019517.1.15",
          "form": "맞아 덕분에 해변서 놀았지",
          "original_form": "맞아 덕분에 해변서 놀았지",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019517.1.16",
          "form": "바르셀로네타 해변!",
          "original_form": "바르셀로네타 해변!",
          "speaker_id": "2",
          "time": "20210827 20:05"
        }
      ]
