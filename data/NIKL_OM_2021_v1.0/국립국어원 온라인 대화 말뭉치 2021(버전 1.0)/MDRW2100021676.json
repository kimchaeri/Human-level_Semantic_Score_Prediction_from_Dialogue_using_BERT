      "utterance": [
        {
          "id": "MDRW2100021676.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021676.1.2",
          "form": "오늘 비가 왔나요?",
          "original_form": "오늘 비가 왔나요?",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021676.1.3",
          "form": "오늘 너무 맑았어요!!",
          "original_form": "오늘 너무 맑았어요!!",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021676.1.4",
          "form": "제가 사는 곳도 오늘 맑았어요",
          "original_form": "제가 사는 곳도 오늘 맑았어요",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021676.1.5",
          "form": "낮에는 조금 더웠는데 해지니까 시원하더라구요",
          "original_form": "낮에는 조금 더웠는데 해지니까 시원하더라구요",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021676.1.6",
          "form": "약간 여름이 가기전 더위가 힘을 쥐어짜는 느낌이에요 ㅋㅋㅋ",
          "original_form": "약간 여름이 가기전 더위가 힘을 쥐어짜는 느낌이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021676.1.7",
          "form": "그래도 바람이 선선해요 ㅋㅋ",
          "original_form": "그래도 바람이 선선해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021676.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021676.1.9",
          "form": "확실히 한참 더웠을 때랑은 다르네요",
          "original_form": "확실히 한참 더웠을 때랑은 다르네요",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021676.1.10",
          "form": "확실히 가을이다 싶어요!! 가을 좋아하세요?",
          "original_form": "확실히 가을이다 싶어요!! 가을 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100021676.1.11",
          "form": "가을 좋죠",
          "original_form": "가을 좋죠",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100021676.1.12",
          "form": "덥지도 춥지도 않은 날씨를 좋아합니다",
          "original_form": "덥지도 춥지도 않은 날씨를 좋아합니다",
          "speaker_id": "1",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021676.1.13",
          "form": "단풍도 예쁘고요",
          "original_form": "단풍도 예쁘고요",
          "speaker_id": "1",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021676.1.14",
          "form": "봄가을 너무 좋은데 알레르기 때문에 여름이 좋아졌어요 ㅋㅋ",
          "original_form": "봄가을 너무 좋은데 알레르기 때문에 여름이 좋아졌어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021676.1.15",
          "form": "혹시 알레르기성 비염 있으신가요?",
          "original_form": "혹시 알레르기성 비염 있으신가요?",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021676.1.16",
          "form": "네네 ㅜㅜ 꽃가루 ㅜㅜ",
          "original_form": "네네 ㅜㅜ 꽃가루 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021676.1.17",
          "form": "저두 심해요ㅜㅜ",
          "original_form": "저두 심해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021676.1.18",
          "form": "여름이 숨쉬기 좋은 계절이잖아요",
          "original_form": "여름이 숨쉬기 좋은 계절이잖아요",
          "speaker_id": "1",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021676.1.19",
          "form": "근데 또 요즘은 여름도 선풍기나 에어컨 때문에..ㅜㅜ",
          "original_form": "근데 또 요즘은 여름도 선풍기나 에어컨 때문에..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021676.1.20",
          "form": "맞아요맞아요 ㅜㅜ 자유롭게 숨쉴수있는시기가 많지도 않아요 ㅜㅜ ㅋㅋ",
          "original_form": "맞아요맞아요 ㅜㅜ 자유롭게 숨쉴수있는시기가 많지도 않아요 ㅜㅜ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021676.1.21",
          "form": "가을 오는 것도 코가 먼저 알게 되지 않나요?ㅎㅎ",
          "original_form": "가을 오는 것도 코가 먼저 알게 되지 않나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021676.1.22",
          "form": "정말그래요 ㅜㅜ벌써 훌쩍이기 시작해요",
