      "utterance": [
        {
          "id": "MDRW2100028561.1.1",
          "form": "전 부산이 너무 그리워요",
          "original_form": "전 부산이 너무 그리워요",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028561.1.2",
          "form": "아름다운바다",
          "original_form": "아름다운바다",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028561.1.3",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028561.1.4",
          "form": "헉 진짜 부산 가고 싶어 ㅜㅜ",
          "original_form": "헉 진짜 부산 가고 싶어 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028561.1.5",
          "form": "그 맨 마지막에 묵었던 에어비앤비",
          "original_form": "그 맨 마지막에 묵었던 에어비앤비",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028561.1.6",
          "form": "계속 생각나 요즘도",
          "original_form": "계속 생각나 요즘도",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028561.1.7",
          "form": "위에 잘 곳은 조금 좁긴 했지만 정말 아늑하고 좋았다",
          "original_form": "위에 잘 곳은 조금 좁긴 했지만 정말 아늑하고 좋았다",
          "speaker_id": "2",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100028561.1.8",
          "form": "우리 대화하는 거 여기서만 하면 금방 모으지 않을까 스타벅스 정도는",
          "original_form": "우리 대화하는 거 여기서만 하면 금방 모으지 않을까 스타벅스 정도는",
          "speaker_id": "2",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100028561.1.9",
          "form": "스타벅스 하니까 생각났는데 사천에 카페 예쁜 데 많아 놀러와...",
          "original_form": "스타벅스 하니까 생각났는데 사천에 카페 예쁜 데 많아 놀러와...",
          "speaker_id": "2",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028561.1.10",
          "form": "헉 전에 갔던데 말고 또 아름다운 카페를 찾앗나요 궁금하다",
          "original_form": "헉 전에 갔던데 말고 또 아름다운 카페를 찾앗나요 궁금하다",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028561.1.11",
          "form": "난 사실 카ㅍㅔ.... 음료가 맛잇으면 좋지만",
          "original_form": "난 사실 카ㅍㅔ.... 음료가 맛잇으면 좋지만",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028561.1.12",
          "form": "여기가 지방이라서 그런 거 같은데 카페들이 다 널찍하고 베이커리도 맛있어",
          "original_form": "여기가 지방이라서 그런 거 같은데 카페들이 다 널찍하고 베이커리도 맛있어",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028561.1.13",
          "form": "예쁜카페가좋아",
          "original_form": "예쁜카페가좋아",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028561.1.14",
          "form": "헐 베이커리 궁금하다",
          "original_form": "헐 베이커리 궁금하다",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028561.1.15",
          "form": "나두 좋은 공간에서 행복한 기분을 느끼는 게 좋아",
          "original_form": "나두 좋은 공간에서 행복한 기분을 느끼는 게 좋아",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028561.1.16",
          "form": "우리 부산갓을때도",
          "original_form": "우리 부산갓을때도",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028561.1.17",
          "form": "그 해운대 앞에 그 카페",
          "original_form": "그 해운대 앞에 그 카페",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028561.1.18",
          "form": "좋았는데",
          "original_form": "좋았는데",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028561.1.19",
          "form": "아! 그 별카페",
          "original_form": "아! 그 별카페",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028561.1.20",
          "form": "였나...? 별침대? 였나",
          "original_form": "였나...? 별침대? 였나",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028561.1.21",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋ 거기말고 거기 가기 전에 머물렀던...",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋ 거기말고 거기 가기 전에 머물렀던...",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028561.1.22",
          "form": "콘센트가 되지 않는곳",
          "original_form": "콘센트가 되지 않는곳",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028561.1.23",
          "form": "해운대도 그렇고 다너무재밌고... 다시 너랑 부산 가고 싶어 슬퍼하는 중",
          "original_form": "해운대도 그렇고 다너무재밌고... 다시 너랑 부산 가고 싶어 슬퍼하는 중",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028561.1.24",
          "form": "아아아 하얀카페",
          "original_form": "아아아 하얀카페",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028561.1.25",
          "form": "별침대 거기는 다좋은데 노키즈존이라 약간 그림의떡",
          "original_form": "별침대 거기는 다좋은데 노키즈존이라 약간 그림의떡",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028561.1.26",
          "form": "ㅁㅈㅁㅈㅁㅈ하얀카페",
          "original_form": "ㅁㅈㅁㅈㅁㅈ하얀카페",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028561.1.27",
          "form": "ㅋㅋㅋㅋ 맞아 거긴 그림의떡",
          "original_form": "ㅋㅋㅋㅋ 맞아 거긴 그림의떡",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028561.1.28",
          "form": "나도 부산진짜 가고싶어ㅜㅠ 여행의 기억으로 살아가고잇기",
          "original_form": "나도 부산진짜 가고싶어ㅜㅠ 여행의 기억으로 살아가고잇기",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028561.1.29",
          "form": "ㅜㅜ 답답하지 진짜 요즘 코로나라서",
          "original_form": "ㅜㅜ 답답하지 진짜 요즘 코로나라서",
          "speaker_id": "2",
