      "utterance": [
        {
          "id": "MDRW2100024745.1.1",
          "form": "저녁먹으러가니",
          "original_form": "저녁먹으러가니",
          "speaker_id": "2",
          "time": "20210831 18:03"
        },
        {
          "id": "MDRW2100024745.1.2",
          "form": "지금은 아니고 한 시간쯤 후에 먹을 것 같은데",
          "original_form": "지금은 아니고 한 시간쯤 후에 먹을 것 같은데",
          "speaker_id": "1",
          "time": "20210831 18:03"
        },
        {
          "id": "MDRW2100024745.1.3",
          "form": "넌 지금 먹니?",
          "original_form": "넌 지금 먹니?",
          "speaker_id": "1",
          "time": "20210831 18:03"
        },
        {
          "id": "MDRW2100024745.1.4",
          "form": "난 오늘 샤브뷔페먹어서",
          "original_form": "난 오늘 샤브뷔페먹어서",
          "speaker_id": "2",
          "time": "20210831 18:04"
        },
        {
          "id": "MDRW2100024745.1.5",
          "form": "저녁은 안먹게",
          "original_form": "저녁은 안먹게",
          "speaker_id": "2",
          "time": "20210831 18:04"
        },
        {
          "id": "MDRW2100024745.1.6",
          "form": "헐 진짜 맛있었겠다",
          "original_form": "헐 진짜 맛있었겠다",
          "speaker_id": "1",
          "time": "20210831 18:04"
        },
        {
          "id": "MDRW2100024745.1.7",
          "form": "나도 샤브샤브 좋아하는데",
          "original_form": "나도 샤브샤브 좋아하는데",
          "speaker_id": "1",
          "time": "20210831 18:04"
        },
        {
          "id": "MDRW2100024745.1.8",
          "form": "죽도 먹었어?",
          "original_form": "죽도 먹었어?",
          "speaker_id": "1",
          "time": "20210831 18:05"
        },
        {
          "id": "MDRW2100024745.1.9",
          "form": "응 칼국수에 죽도먹었어",
          "original_form": "응 칼국수에 죽도먹었어",
          "speaker_id": "2",
          "time": "20210831 18:05"
        },
        {
          "id": "MDRW2100024745.1.10",
          "form": "거기다 후식도",
          "original_form": "거기다 후식도",
          "speaker_id": "2",
          "time": "20210831 18:05"
        },
        {
          "id": "MDRW2100024745.1.11",
          "form": "오메 맛있었겠다ㅋㅋ 후식 뭐 먹었니",
          "original_form": "오메 맛있었겠다ㅋㅋ 후식 뭐 먹었니",
          "speaker_id": "1",
          "time": "20210831 18:05"
        },
        {
          "id": "MDRW2100024745.1.12",
          "form": "후식으로 거기에",
          "original_form": "후식으로 거기에",
          "speaker_id": "2",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024745.1.13",
          "form": "초코퐁듀있더라",
          "original_form": "초코퐁듀있더라",
          "speaker_id": "2",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024745.1.14",
          "form": "그래서 바나나랑 마쉬멜로 찍어먹었어",
          "original_form": "그래서 바나나랑 마쉬멜로 찍어먹었어",
          "speaker_id": "2",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024745.1.15",
          "form": "헐 대박이잖아",
          "original_form": "헐 대박이잖아",
          "speaker_id": "1",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024745.1.16",
          "form": "신기하지",
          "original_form": "신기하지",
          "speaker_id": "2",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024745.1.17",
          "form": "환상적이었겠는데ㅋㅋㅋㅋㅋㅋ",
          "original_form": "환상적이었겠는데ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024745.1.18",
          "form": "샤브샤브집인데",
          "original_form": "샤브샤브집인데",
          "speaker_id": "2",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024745.1.19",
          "form": "그니까 ㅋㅋㅋㅋ 샤브샤브랑 퐁듀라니",
          "original_form": "그니까 ㅋㅋㅋㅋ 샤브샤브랑 퐁듀라니",
          "speaker_id": "1",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024745.1.20",
          "form": "그리고 샤브샤브 고기자체가",
          "original_form": "그리고 샤브샤브 고기자체가",
          "speaker_id": "2",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024745.1.21",
          "form": "보통 그 둘이 같이 있는 가게는 잘 못 본 것 같은데",
          "original_form": "보통 그 둘이 같이 있는 가게는 잘 못 본 것 같은데",
          "speaker_id": "1",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024745.1.22",
          "form": "무슨 저민고기처럼 나왔어",
          "original_form": "무슨 저민고기처럼 나왔어",
          "speaker_id": "2",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024745.1.23",
          "form": "맛있는 거에 맛있는 거 먹었네",
          "original_form": "맛있는 거에 맛있는 거 먹었네",
          "speaker_id": "1",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024745.1.24",
          "form": "일부러 점심특선 맞춰서",
          "original_form": "일부러 점심특선 맞춰서",
          "speaker_id": "2",
          "time": "20210831 18:08"
        },
        {
          "id": "MDRW2100024745.1.25",
          "form": "싸게 다녀왔어",
          "original_form": "싸게 다녀왔어",
          "speaker_id": "2",
          "time": "20210831 18:08"
        }
