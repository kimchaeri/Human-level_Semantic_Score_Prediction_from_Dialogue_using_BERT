      "utterance": [
        {
          "id": "MDRW2100023569.1.1",
          "form": "노량진회랑 차이 많이나?",
          "original_form": "노량진회랑 차이 많이나?",
          "speaker_id": "1",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100023569.1.2",
          "form": "숙성회는 부드러워",
          "original_form": "숙성회는 부드러워",
          "speaker_id": "2",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100023569.1.3",
          "form": "원래 나 숙성회 안좋아했거든 물컹해서",
          "original_form": "원래 나 숙성회 안좋아했거든 물컹해서",
          "speaker_id": "2",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100023569.1.4",
          "form": "아하 활어는 쫄깃하고 숙성은 부드럽구나",
          "original_form": "아하 활어는 쫄깃하고 숙성은 부드럽구나",
          "speaker_id": "1",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100023569.1.5",
          "form": "활어가 씹는맛이 있고",
          "original_form": "활어가 씹는맛이 있고",
          "speaker_id": "2",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100023569.1.6",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100023569.1.7",
          "form": "마자 그치",
          "original_form": "마자 그치",
          "speaker_id": "1",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100023569.1.8",
          "form": "근데 흰살생선에 소금찍어먹음 맛있더라고",
          "original_form": "근데 흰살생선에 소금찍어먹음 맛있더라고",
          "speaker_id": "2",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100023569.1.9",
          "form": "오~회 소금에도 찍어먹는구나?",
          "original_form": "오~회 소금에도 찍어먹는구나?",
          "speaker_id": "1",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100023569.1.10",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "1",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100023569.1.11",
          "form": "응! 나중에 먹어봐 꼬ㅑ 괜찮아",
          "original_form": "응! 나중에 먹어봐 꼬ㅑ 괜찮아",
          "speaker_id": "2",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100023569.1.12",
          "form": "간장이랑 초장에 먹었을때랑 또 다른맛이니?",
          "original_form": "간장이랑 초장에 먹었을때랑 또 다른맛이니?",
          "speaker_id": "1",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100023569.1.13",
          "form": "난 이제 거의 초장을 안먹고 간장아니면 소금! 소금은 이제 간장보다 더 담백한 느낌?",
          "original_form": "난 이제 거의 초장을 안먹고 간장아니면 소금! 소금은 이제 간장보다 더 담백한 느낌?",
          "speaker_id": "2",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100023569.1.14",
          "form": "그니까 한번 소금찍어서 먹어봐야겠다\n오~뭔가 신선해ㅋㅋㅋㅋ",
          "original_form": "그니까 한번 소금찍어서 먹어봐야겠다\n오~뭔가 신선해ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100023569.1.15",
          "form": "저기가 또 소금을 줘서ㅋㅋ 집엔 굵은소금만 있음ㅠㅠㅋㅋ",
          "original_form": "저기가 또 소금을 줘서ㅋㅋ 집엔 굵은소금만 있음ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100023569.1.16",
          "form": "오 이제 완전 회 고수 된거같네ㅋㅋㅋㅋㅋ",
          "original_form": "오 이제 완전 회 고수 된거같네ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100023569.1.17",
          "form": "아 소금을 아예 횟집에서 주는구나",
          "original_form": "아 소금을 아예 횟집에서 주는구나",
          "speaker_id": "1",
          "time": "20210830 11:44"
        }
      ]
