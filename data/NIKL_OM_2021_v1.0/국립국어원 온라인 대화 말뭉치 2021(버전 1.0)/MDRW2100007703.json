      "utterance": [
        {
          "id": "MDRW2100007703.1.1",
          "form": "내일 가을장마 오는데 외출할건가",
          "original_form": "내일 가을장마 오는데 외출할건가",
          "speaker_id": "2",
          "time": "20210822 09:48"
        },
        {
          "id": "MDRW2100007703.1.2",
          "form": "내일 가을이야?",
          "original_form": "내일 가을이야?",
          "speaker_id": "1",
          "time": "20210822 09:48"
        },
        {
          "id": "MDRW2100007703.1.3",
          "form": "외출하지",
          "original_form": "외출하지",
          "speaker_id": "1",
          "time": "20210822 09:48"
        },
        {
          "id": "MDRW2100007703.1.4",
          "form": "그럼 말복지나면 가을이지",
          "original_form": "그럼 말복지나면 가을이지",
          "speaker_id": "2",
          "time": "20210822 09:49"
        },
        {
          "id": "MDRW2100007703.1.5",
          "form": "말복이 언젠데?ㅋㅋㅋ",
          "original_form": "말복이 언젠데?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:50"
        },
        {
          "id": "MDRW2100007703.1.6",
          "form": "헉 8월10일 번써 한참지났지",
          "original_form": "헉 8월10일 번써 한참지났지",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007703.1.7",
          "form": "헉!",
          "original_form": "헉!",
          "speaker_id": "1",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007703.1.8",
          "form": "우리 뭐 먹었나?",
          "original_form": "우리 뭐 먹었나?",
          "speaker_id": "1",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007703.1.9",
          "form": "말복때 난 엄마랑 염소탕 먹었지",
          "original_form": "말복때 난 엄마랑 염소탕 먹었지",
          "speaker_id": "2",
          "time": "20210822 09:53"
        },
        {
          "id": "MDRW2100007703.1.10",
          "form": "ㅋㅋㅋ헠",
          "original_form": "ㅋㅋㅋ헠",
          "speaker_id": "1",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007703.1.11",
          "form": "하여튼",
          "original_form": "하여튼",
          "speaker_id": "1",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007703.1.12",
          "form": "낼 비온다구?",
          "original_form": "낼 비온다구?",
          "speaker_id": "1",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007703.1.13",
          "form": "일기예보에서 태풍 온다고 했는데",
          "original_form": "일기예보에서 태풍 온다고 했는데",
          "speaker_id": "2",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007703.1.14",
          "form": "헐?!",
          "original_form": "헐?!",
          "speaker_id": "1",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007703.1.15",
          "form": "무슨 태풍!!",
          "original_form": "무슨 태풍!!",
          "speaker_id": "1",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007703.1.16",
          "form": "진짜야?",
          "original_form": "진짜야?",
          "speaker_id": "1",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007703.1.17",
          "form": "오마이스 라고해",
          "original_form": "오마이스 라고해",
          "speaker_id": "2",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007703.1.18",
          "form": "이름이 특이하네",
          "original_form": "이름이 특이하네",
          "speaker_id": "1",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007703.1.19",
          "form": "춥겠다ㅠ",
          "original_form": "춥겠다ㅠ",
          "speaker_id": "1",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007703.1.20",
