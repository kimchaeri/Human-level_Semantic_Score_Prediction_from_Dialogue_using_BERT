      "utterance": [
        {
          "id": "MDRW2100030148.1.1",
          "form": "저는 평생 비혼으로 살고싶어서..",
          "original_form": "저는 평생 비혼으로 살고싶어서..",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030148.1.2",
          "form": "전 그건 모르겠는데 여자 만나고 싶어요... ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 그건 모르겠는데 여자 만나고 싶어요... ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030148.1.3",
          "form": "제발 코로나 좀 끝났으면 좋겠어요,,,",
          "original_form": "제발 코로나 좀 끝났으면 좋겠어요,,,",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030148.1.4",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030148.1.5",
          "form": "오잉",
          "original_form": "오잉",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030148.1.6",
          "form": "여성분ㅇㅏ니세요??",
          "original_form": "여성분ㅇㅏ니세요??",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030148.1.7",
          "form": "마자용 전 근데 남자보다 여자 좋아해서",
          "original_form": "마자용 전 근데 남자보다 여자 좋아해서",
          "speaker_id": "2",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030148.1.8",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030148.1.9",
          "form": "저두ㅋㅋㅋㅋ",
          "original_form": "저두ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030148.1.10",
          "form": "예쁜 언니랑 만나고 싶어요 진자,,,",
          "original_form": "예쁜 언니랑 만나고 싶어요 진자,,,",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030148.1.11",
          "form": "허얼 정말요? ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "허얼 정말요? ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030148.1.12",
          "form": "네ㅜㅠ 저는 연상 좋아해요 언니 최고,,,",
          "original_form": "네ㅜㅠ 저는 연상 좋아해요 언니 최고,,,",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030148.1.13",
          "form": "다들 연상 좋아하죠,,,",
          "original_form": "다들 연상 좋아하죠,,,{emoji:☆}",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030148.1.14",
          "form": "으 진짜 더러워요ㅜ",
          "original_form": "으 진짜 더러워요ㅜ",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030148.1.15",
          "form": "저는 진짜...",
          "original_form": "저는 진짜...",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030148.1.16",
          "form": "연상언니 정말 좋아하는데 아직 마음 맞는분을 못찾앗어오ㅜㅜ",
          "original_form": "연상언니 정말 좋아하는데 아직 마음 맞는분을 못찾앗어오ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030148.1.17",
          "form": "쉽지 않죠...",
          "original_form": "쉽지 않죠...",
          "speaker_id": "2",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030148.1.18",
          "form": "그래도 언젠간 만날 거예요 화이팅합시당 ㅎㅎㅎㅎ",
          "original_form": "그래도 언젠간 만날 거예요 화이팅합시당 ㅎㅎㅎㅎ",
