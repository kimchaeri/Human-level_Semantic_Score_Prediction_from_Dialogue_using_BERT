      "utterance": [
        {
          "id": "MDRW2100041449.1.1",
          "form": "안녕 이번후 식스센스 봤니",
          "original_form": "안녕 이번후 식스센스 봤니",
          "speaker_id": "2",
          "time": "20210906 00:15"
        },
        {
          "id": "MDRW2100041449.1.2",
          "form": "이번화당연히 봤지!",
          "original_form": "이번화당연히 봤지!",
          "speaker_id": "1",
          "time": "20210906 00:16"
        },
        {
          "id": "MDRW2100041449.1.3",
          "form": "무슨내용이였더라",
          "original_form": "무슨내용이였더라",
          "speaker_id": "2",
          "time": "20210906 00:17"
        },
        {
          "id": "MDRW2100041449.1.4",
          "form": "이번 주에 가짜 상담가를 찾는거",
          "original_form": "이번 주에 가짜 상담가를 찾는거",
          "speaker_id": "1",
          "time": "20210906 00:17"
        },
        {
          "id": "MDRW2100041449.1.5",
          "form": "아 맞다 재밌었어",
          "original_form": "아 맞다 재밌었어",
          "speaker_id": "2",
          "time": "20210906 00:17"
        },
        {
          "id": "MDRW2100041449.1.6",
          "form": "다 맞췄어 답?",
          "original_form": "다 맞췄어 답?",
          "speaker_id": "1",
          "time": "20210906 00:18"
        },
        {
          "id": "MDRW2100041449.1.7",
          "form": "나는 틀렸어 너는",
          "original_form": "나는 틀렸어 너는",
          "speaker_id": "2",
          "time": "20210906 00:18"
        },
        {
          "id": "MDRW2100041449.1.8",
          "form": "나는 이번 거 맞췄어",
          "original_form": "나는 이번 거 맞췄어",
          "speaker_id": "1",
          "time": "20210906 00:18"
        },
        {
          "id": "MDRW2100041449.1.9",
          "form": "나는 삼번이 가짜인줄 알았어",
          "original_form": "나는 삼번이 가짜인줄 알았어",
          "speaker_id": "2",
          "time": "20210906 00:18"
        },
        {
          "id": "MDRW2100041449.1.10",
          "form": "삼번 이 좀 의심되긴 했는데 일번이 확 가짜 더라고",
          "original_form": "삼번 이 좀 의심되긴 했는데 일번이 확 가짜 더라고",
          "speaker_id": "1",
          "time": "20210906 00:18"
        },
        {
          "id": "MDRW2100041449.1.11",
          "form": "오 똑똑한데",
          "original_form": "오 똑똑한데",
          "speaker_id": "2",
          "time": "20210906 00:18"
        },
        {
          "id": "MDRW2100041449.1.12",
          "form": "다음 주 게스트는 이상이래",
          "original_form": "다음 주 게스트는 이상이래",
          "speaker_id": "1",
          "time": "20210906 00:19"
        },
        {
          "id": "MDRW2100041449.1.13",
          "form": "이번 주 게스트는 로꼬랑 그레이 였지",
          "original_form": "이번 주 게스트는 로꼬랑 그레이 였지",
          "speaker_id": "2",
          "time": "20210906 00:19"
        },
        {
          "id": "MDRW2100041449.1.14",
          "form": "응 맞아 재미있더라",
          "original_form": "응 맞아 재미있더라",
          "speaker_id": "1",
          "time": "20210906 00:19"
        },
        {
          "id": "MDRW2100041449.1.15",
          "form": "식스센스 이제 곧 끝난데",
          "original_form": "식스센스 이제 곧 끝난데",
          "speaker_id": "2",
          "time": "20210906 00:19"
        },
        {
          "id": "MDRW2100041449.1.16",
          "form": "안돼 금요일 마다 재미있게 보고 있는데",
          "original_form": "안돼 금요일 마다 재미있게 보고 있는데",
          "speaker_id": "1",
          "time": "20210906 00:19"
        },
        {
          "id": "MDRW2100041449.1.17",
          "form": "드니까 얼른 시즌 쓰리로 나오면 좋겠다",
          "original_form": "드니까 얼른 시즌 쓰리로 나오면 좋겠다",
          "speaker_id": "2",
          "time": "20210906 00:19"
        }
      ]
