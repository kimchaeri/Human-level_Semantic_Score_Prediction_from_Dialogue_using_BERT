      "utterance": [
        {
          "id": "MDRW2100033462.1.1",
          "form": "영화도 갑자기 다시 보고싶어지고.. 볼거가 더 쌓여가네요 ㅎㅎ",
          "original_form": "영화도 갑자기 다시 보고싶어지고.. 볼거가 더 쌓여가네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100033462.1.2",
          "form": "네 가끔 다시 보고 그래요",
          "original_form": "네 가끔 다시 보고 그래요",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100033462.1.3",
          "form": "영화는 어떤 장르 좋아하세ㅔ요?",
          "original_form": "영화는 어떤 장르 좋아하세ㅔ요?",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100033462.1.4",
          "form": "액션이요",
          "original_form": "액션이요",
          "speaker_id": "2",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100033462.1.5",
          "form": "스릴러나요",
          "original_form": "스릴러나요",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100033462.1.6",
          "form": "마블 좋아 했는데 ㅜㅜ",
          "original_form": "마블 좋아 했는데 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100033462.1.7",
          "form": "요새 극장도 못가고 봐도 흥이 안나요",
          "original_form": "요새 극장도 못가고 봐도 흥이 안나요",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100033462.1.8",
          "form": "그쵸.. 아 9월1일날에 마블 영화 나오더라구요",
          "original_form": "그쵸.. 아 9월1일날에 마블 영화 나오더라구요",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100033462.1.9",
          "form": "이터널스랑 상치인가요",
          "original_form": "이터널스랑 상치인가요",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100033462.1.10",
          "form": "요새 이름이 생각 안나요",
          "original_form": "요새 이름이 생각 안나요",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100033462.1.11",
          "form": "보려구요",
          "original_form": "보려구요",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100033462.1.12",
          "form": "유플에 금방뜨겠죠 ㅋㅋ",
          "original_form": "유플에 금방뜨겠죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100033462.1.13",
          "form": "저도.. 제목이 기억이 안나네요 ㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "original_form": "저도.. 제목이 기억이 안나네요 ㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100033462.1.14",
          "form": "ㅋㅋ뭐 몰라도 대충아니 보면되죠 뭐",
          "original_form": "ㅋㅋ뭐 몰라도 대충아니 보면되죠 뭐",
          "speaker_id": "2",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100033462.1.15",
          "form": "저도 맨날 그거 그거 그래요 ㅜㅜ",
          "original_form": "저도 맨날 그거 그거 그래요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100033462.1.16",
          "form": "아.. ㅠㅠㅠ 너무 슬프네요 ㅠㅠㅠ\r\n그래도 사무실도 한명빼곤 아줌마라서 그거 있자나 그거 하면... 다 알아듣더가구요..ㅎㅎㅎ",
          "original_form": "아.. ㅠㅠㅠ 너무 슬프네요 ㅠㅠㅠ\r\n그래도 사무실도 한명빼곤 아줌마라서 그거 있자나 그거 하면... 다 알아듣더가구요..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100033462.1.17",
          "form": "맞아요 남편만 몰라요 ㅡㅡ",
          "original_form": "맞아요 남편만 몰라요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100033462.1.18",
          "form": "그럼제가 그래요 그거 그거",
          "original_form": "그럼제가 그래요 그거 그거",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100033462.1.19",
          "form": "다안다고",
          "original_form": "다안다고",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100033462.1.20",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100033462.1.21",
          "form": "전 마블 나옴 보려고요",
          "original_form": "전 마블 나옴 보려고요",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100033462.1.22",
          "form": "영화 뭐좋아하세요?",
          "original_form": "영화 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100033462.1.23",
          "form": "드라마 장르랑 비슷해요,\r\n공포, 스릴러, 미스터리, 범죄 종류 좋아해요",
          "original_form": "드라마 장르랑 비슷해요,\r\n공포, 스릴러, 미스터리, 범죄 종류 좋아해요",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100033462.1.24",
          "form": "해리포터같은 마법판타지도 좋아하구요",
          "original_form": "해리포터같은 마법판타지도 좋아하구요",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100033462.1.25",
          "form": "저도 해리포터 극장서 다봤어요",
          "original_form": "저도 해리포터 극장서 다봤어요",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100033462.1.26",
          "form": "저도 그래요 ㅋㅋ",
          "original_form": "저도 그래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100033462.1.27",
          "form": "셜록홈즈 영화도 다보고",
          "original_form": "셜록홈즈 영화도 다보고",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100033462.1.28",
          "form": "잼났어요",
          "original_form": "잼났어요",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100033462.1.29",
          "form": "저두요!!!!",
          "original_form": "저두요!!!!",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100033462.1.30",
          "form": "셜록은.. 이름이..너무 어려우니 헷갈려서.. ㅠㅠ",
          "original_form": "셜록은.. 이름이..너무 어려우니 헷갈려서.. ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100033462.1.31",
