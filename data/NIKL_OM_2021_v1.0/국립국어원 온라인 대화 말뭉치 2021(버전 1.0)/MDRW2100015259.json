      "utterance": [
        {
          "id": "MDRW2100015259.1.1",
          "form": "운동 직관 좋아하세요?",
          "original_form": "운동 직관 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015259.1.2",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015259.1.3",
          "form": "아직 프로경기 직관은 못가봤지만 관심이 많이 생겨서요ㅎㅎ",
          "original_form": "아직 프로경기 직관은 못가봤지만 관심이 많이 생겨서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015259.1.4",
          "form": "어떤 종목이요?",
          "original_form": "어떤 종목이요?",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015259.1.5",
          "form": "여자배구요!! 올림픽에서 보고 정말",
          "original_form": "여자배구요!! 올림픽에서 보고 정말",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015259.1.6",
          "form": "직관가보고싶다 생각했는데",
          "original_form": "직관가보고싶다 생각했는데",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015259.1.7",
          "form": "무관중이라ㅜㅜ 안받는거같더라구용",
          "original_form": "무관중이라ㅜㅜ 안받는거같더라구용",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015259.1.8",
          "form": "아.. 배구가 인기 많아졌죠..",
          "original_form": "아.. 배구가 인기 많아졌죠..",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015259.1.9",
          "form": "김연경은 진짜 신인듯..",
          "original_form": "김연경은 진짜 신인듯..",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015259.1.10",
          "form": "직관 가보신적 있으세요?.?",
          "original_form": "직관 가보신적 있으세요?.?",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015259.1.11",
          "form": "야구랑 축구국대랑.. 몇번정도요..",
          "original_form": "야구랑 축구국대랑.. 몇번정도요..",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015259.1.12",
          "form": "근데 테레비로 보는게 더 좋더라구요",
          "original_form": "근데 테레비로 보는게 더 좋더라구요",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015259.1.13",
          "form": "아하 야구도 보러 가고싶었는데",
          "original_form": "아하 야구도 보러 가고싶었는데",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015259.1.14",
          "form": "오오 티비가 더 잘보여서요??",
          "original_form": "오오 티비가 더 잘보여서요??",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015259.1.15",
          "form": "직관가면 선수들 코딱지만하게 보여요..",
          "original_form": "직관가면 선수들 코딱지만하게 보여요..",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015259.1.16",
          "form": "낮경기는 햇볕 엄청 덥기도하고..",
          "original_form": "낮경기는 햇볕 엄청 덥기도하고..",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015259.1.17",
          "form": "아하... 역시 야구장은 넘 넓어서 ㅎㅎㅎ",
          "original_form": "아하... 역시 야구장은 넘 넓어서 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015259.1.18",
          "form": "잠실주경기장도 트랙땜에 경기장 멀어서..",
          "original_form": "잠실주경기장도 트랙땜에 경기장 멀어서..",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015259.1.19",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015259.1.20",
          "form": "저는 원래 스포츠 경기를 거의 안봤는데",
          "original_form": "저는 원래 스포츠 경기를 거의 안봤는데",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015259.1.21",
          "form": "요즘은 여자배구 챙겨보고있어요 ㅎㅎ",
          "original_form": "요즘은 여자배구 챙겨보고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015259.1.22",
          "form": "리그 지금하나요?",
          "original_form": "리그 지금하나요?",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015259.1.23",
          "form": "넹 23일부터 시작했어용",
          "original_form": "넹 23일부터 시작했어용",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015259.1.24",
          "form": "엊그제네여...",
          "original_form": "엊그제네여...",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015259.1.25",
          "form": "응원팀 있어요?",
          "original_form": "응원팀 있어요?",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015259.1.26",
          "form": "국대로 나갔던 선수들 다좋아해서 넘 고르긴 어렵지만...",
          "original_form": "국대로 나갔던 선수들 다좋아해서 넘 고르긴 어렵지만...",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015259.1.27",
          "form": "IBK기업은행 응원합니당 ㅋㅋㅋ",
          "original_form": "IBK기업은행 응원합니당 ㅋㅋㅋ",
