      "utterance": [
        {
          "id": "MDRW2100018398.1.1",
          "form": "안녕하세요. 게임 좋아하세요?",
          "original_form": "안녕하세요. 게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 01:04"
        },
        {
          "id": "MDRW2100018398.1.2",
          "form": "안녕하세요:)\n예전에는 좋아했었는데\n지금 하고 있는건 없어요",
          "original_form": "안녕하세요:)\n예전에는 좋아했었는데\n지금 하고 있는건 없어요",
          "speaker_id": "2",
          "time": "20210827 12:02"
        },
        {
          "id": "MDRW2100018398.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210827 12:02"
        },
        {
          "id": "MDRW2100018398.1.4",
          "form": "저는 예전에 쿠키런 킹덤했었어요ㅋㅋㅋ",
          "original_form": "저는 예전에 쿠키런 킹덤했었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018398.1.5",
          "form": "쿠키들이 귀여워서...",
          "original_form": "쿠키들이 귀여워서...",
          "speaker_id": "1",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018398.1.6",
          "form": "앗!  저도 쿠키런이\n제가 했던 마지막 게임 같아요",
          "original_form": "앗!  저도 쿠키런이\n제가 했던 마지막 게임 같아요",
          "speaker_id": "2",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018398.1.7",
          "form": "유료 결제도 하셨었나요?",
          "original_form": "유료 결제도 하셨었나요?",
          "speaker_id": "2",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018398.1.8",
          "form": "저 열심히 무료로 했습니다ㅋㅋㅋ!",
          "original_form": "저 열심히 무료로 했습니다ㅋㅋㅋ!",
          "speaker_id": "1",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018398.1.9",
          "form": "무료로도 어느정도 진행이 잘되는 게임이라 좋았어요",
          "original_form": "무료로도 어느정도 진행이 잘되는 게임이라 좋았어요",
          "speaker_id": "1",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018398.1.10",
          "form": "그쵸:) 저도 무료 재미까지만 느끼고\n현질까지는 안하는거 같아요ㅋ",
          "original_form": "그쵸:) 저도 무료 재미까지만 느끼고\n현질까지는 안하는거 같아요ㅋ",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018398.1.11",
          "form": "예전에 애니팡도 하셨었나요?",
          "original_form": "예전에 애니팡도 하셨었나요?",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018398.1.12",
          "form": "넵ㅋㅋㅋ 애니팡 첨 나왔을 때 열풍이었죠",
          "original_form": "넵ㅋㅋㅋ 애니팡 첨 나왔을 때 열풍이었죠",
          "speaker_id": "1",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100018398.1.13",
          "form": "저희 엄마는 아직도 하세요",
          "original_form": "저희 엄마는 아직도 하세요",
          "speaker_id": "1",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100018398.1.14",
          "form": "ㅋㅋ앗ㅋㅋ역시 남녀노소에게\n사랑받던 게임이죠",
          "original_form": "ㅋㅋ앗ㅋㅋ역시 남녀노소에게\n사랑받던 게임이죠{emoji:♡}",
          "speaker_id": "2",
          "time": "20210827 13:03"
        },
        {
          "id": "MDRW2100018398.1.15",
          "form": "저희 엄마도 엄청 좋아하셨어요",
          "original_form": "저희 엄마도 엄청 좋아하셨어요",
          "speaker_id": "2",
          "time": "20210827 13:03"
        },
        {
          "id": "MDRW2100018398.1.16",
          "form": "ㅋㅋㅋ그쵸 애니팡 덕에 엄마가 스마트폰에 적응을 좀 많이한듯!",
          "original_form": "ㅋㅋㅋ그쵸 애니팡 덕에 엄마가 스마트폰에 적응을 좀 많이한듯!",
          "speaker_id": "1",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018398.1.17",
          "form": "애니팡 말고는 또 뭐하셨어요?",
          "original_form": "애니팡 말고는 또 뭐하셨어요?",
          "speaker_id": "1",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018398.1.18",
          "form": "폰게임은\n애니팡이랑 쿠키런 했고\n예전에 한게임 테트리스\n좋아했었어요 ㅋㅋ",
          "original_form": "폰게임은\n애니팡이랑 쿠키런 했고\n예전에 한게임 테트리스\n좋아했었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018398.1.19",
          "form": "앗 저랑 잘맞으시네요ㅋㅋㅋㅋ",
          "original_form": "앗 저랑 잘맞으시네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100018398.1.20",
          "form": "저도 게임하면 테트리스부터 떠오르더라구요",
          "original_form": "저도 게임하면 테트리스부터 떠오르더라구요",
          "speaker_id": "1",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100018398.1.21",
          "form": "한게임 요즘도 있나요..? 자주했는데...",
          "original_form": "한게임 요즘도 있나요..? 자주했는데...",
          "speaker_id": "1",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100018398.1.22",
          "form": "저도 최근에는 안해봐서\n잘 모르겠어요:)",
          "original_form": "저도 최근에는 안해봐서\n잘 모르겠어요:)",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018398.1.23",
          "form": "테트리스 진짜 재밌었죠ㅋㅋ",
          "original_form": "테트리스 진짜 재밌었죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018398.1.24",
          "form": "맞아요 순발력이랑 머리가 동시에 필요한 게임!",
          "original_form": "맞아요 순발력이랑 머리가 동시에 필요한 게임!",
          "speaker_id": "1",
          "time": "20210827 13:15"
        }
      ]
    }
