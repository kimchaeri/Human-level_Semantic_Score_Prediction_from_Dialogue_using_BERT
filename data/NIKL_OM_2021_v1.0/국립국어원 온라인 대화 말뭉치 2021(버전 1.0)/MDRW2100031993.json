      "utterance": [
        {
          "id": "MDRW2100031993.1.1",
          "form": "아침은 드셨나요??",
          "original_form": "아침은 드셨나요??",
          "speaker_id": "1",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100031993.1.2",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031993.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031993.1.4",
          "form": "저는 김치찌개 먹었어요",
          "original_form": "저는 김치찌개 먹었어요",
          "speaker_id": "1",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031993.1.5",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031993.1.6",
          "form": "맛있겠어요!!",
          "original_form": "맛있겠어요!!",
          "speaker_id": "2",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031993.1.7",
          "form": "저는 곰탕이요",
          "original_form": "저는 곰탕이요",
          "speaker_id": "2",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031993.1.8",
          "form": "계란후라이도 드셨어요? ㅋㅋ",
          "original_form": "계란후라이도 드셨어요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031993.1.9",
          "form": "외식이라 계란후라이는 안타깝게도... ",
          "original_form": "외식이라 계란후라이는 안타깝게도... ",
          "speaker_id": "1",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100031993.1.10",
          "form": "어묵사리랑 라면사리 추가하려구요",
          "original_form": "어묵사리랑 라면사리 추가하려구요",
          "speaker_id": "1",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100031993.1.11",
          "form": "드시고계셔요?",
          "original_form": "드시고계셔요?",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031993.1.12",
          "form": "캬 사리까지",
          "original_form": "캬 사리까지",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031993.1.13",
          "form": "맛나게 드세요",
          "original_form": "맛나게 드세요",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031993.1.14",
          "form": "으아, 어묵 추가하니 라면까진 무리예요",
          "original_form": "으아, 어묵 추가하니 라면까진 무리예요",
          "speaker_id": "1",
          "time": "20210905 10:53"
        },
        {
          "id": "MDRW2100031993.1.15",
          "form": "먹고 왔습니다",
          "original_form": "먹고 왔습니다",
          "speaker_id": "1",
          "time": "20210905 10:53"
        },
        {
          "id": "MDRW2100031993.1.16",
          "form": "ㅋㅋㅋ일요일 시작이 맛나겠어요",
          "original_form": "ㅋㅋㅋ일요일 시작이 맛나겠어요",
          "speaker_id": "2",
          "time": "20210905 10:56"
        },
        {
          "id": "MDRW2100031993.1.17",
          "form": "이제 짠->단",
          "original_form": "이제 짠->단",
          "speaker_id": "2",
          "time": "20210905 10:56"
        },
        {
          "id": "MDRW2100031993.1.18",
          "form": "차례인가욯ㅎ",
          "original_form": "차례인가욯ㅎ",
          "speaker_id": "2",
          "time": "20210905 10:56"
        },
        {
          "id": "MDRW2100031993.1.19",
          "form": "하, 쩝쩝박사님이시네",
          "original_form": "하, 쩝쩝박사님이시네",
          "speaker_id": "1",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100031993.1.20",
          "form": "다니다가 괜찮은 카페 있으면 한잔 때리려구요",
          "original_form": "다니다가 괜찮은 카페 있으면 한잔 때리려구요",
          "speaker_id": "1",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100031993.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋ살살 때려주세오",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋ살살 때려주세오",
          "speaker_id": "2",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100031993.1.22",
          "form": "홍차라떼 한잔 때렸습니다",
          "original_form": "홍차라떼 한잔 때렸습니다",
          "speaker_id": "1",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100031993.1.23",
          "form": "라떼를 마시니 소다나 에이드가 땡기네요",
          "original_form": "라떼를 마시니 소다나 에이드가 땡기네요",
          "speaker_id": "1",
          "time": "20210905 13:14"
        },
        {
          "id": "MDRW2100031993.1.24",
          "form": "잘하셨어요 ㅋㅋㅋ",
          "original_form": "잘하셨어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:44"
        },
        {
          "id": "MDRW2100031993.1.25",
          "form": "음료 무한굴레인가요",
          "original_form": "음료 무한굴레인가요",
          "speaker_id": "2",
          "time": "20210905 16:44"
        },
        {
          "id": "MDRW2100031993.1.26",
          "form": "ㅋㅋㅋ 아니에요, 인내했습니다",
          "original_form": "ㅋㅋㅋ 아니에요, 인내했습니다",
          "speaker_id": "1",
          "time": "20210905 16:46"
        },
        {
          "id": "MDRW2100031993.1.27",
          "form": "이제 저녁 메뉴 연구중이에요",
          "original_form": "이제 저녁 메뉴 연구중이에요",
          "speaker_id": "1",
          "time": "20210905 16:46"
        },
        {
          "id": "MDRW2100031993.1.28",
          "form": "ㅋㅋㅋ저두요",
          "original_form": "ㅋㅋㅋ저두요",
          "speaker_id": "2",
          "time": "20210905 16:46"
        }
      ]
    }
  ]
