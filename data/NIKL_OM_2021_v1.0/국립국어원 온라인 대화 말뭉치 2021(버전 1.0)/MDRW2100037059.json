      "utterance": [
        {
          "id": "MDRW2100037059.1.1",
          "form": "안녕하세요 저녁은 드셨나요?",
          "original_form": "안녕하세요 저녁은 드셨나요?",
          "speaker_id": "1",
          "time": "20210831 18:30"
        },
        {
          "id": "MDRW2100037059.1.2",
          "form": "으어 디톡스중이에요ㅜ",
          "original_form": "으어 디톡스중이에요ㅜ",
          "speaker_id": "2",
          "time": "20210831 18:32"
        },
        {
          "id": "MDRW2100037059.1.3",
          "form": "주스 마셨네요ㅜ",
          "original_form": "주스 마셨네요ㅜ",
          "speaker_id": "2",
          "time": "20210831 18:33"
        },
        {
          "id": "MDRW2100037059.1.4",
          "form": "디톡스요?",
          "original_form": "디톡스요?",
          "speaker_id": "1",
          "time": "20210831 18:33"
        },
        {
          "id": "MDRW2100037059.1.5",
          "form": "레몬수만 드셔요?",
          "original_form": "레몬수만 드셔요?",
          "speaker_id": "1",
          "time": "20210831 18:33"
        },
        {
          "id": "MDRW2100037059.1.6",
          "form": "네이버에 레디톡치면 나오는 제품 이거 먹고있어요",
          "original_form": "네이버에 레디톡치면 나오는 제품 이거 먹고있어요",
          "speaker_id": "2",
          "time": "20210831 18:36"
        },
        {
          "id": "MDRW2100037059.1.7",
          "form": "아 아예 완제품 사드시는군요 맛은 있어요???",
          "original_form": "아 아예 완제품 사드시는군요 맛은 있어요???",
          "speaker_id": "1",
          "time": "20210831 18:36"
        },
        {
          "id": "MDRW2100037059.1.8",
          "form": "맛은 좋아요",
          "original_form": "맛은 좋아요",
          "speaker_id": "2",
          "time": "20210831 18:36"
        },
        {
          "id": "MDRW2100037059.1.9",
          "form": "새콤달콤한 자몽맛이요",
          "original_form": "새콤달콤한 자몽맛이요",
          "speaker_id": "2",
          "time": "20210831 18:36"
        },
        {
          "id": "MDRW2100037059.1.10",
          "form": "ㅋㅋ허기짐은요? ㅋㅋ",
          "original_form": "ㅋㅋ허기짐은요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100037059.1.11",
          "form": "가격은 안착한거같아요 ㅋㅋㅋ",
          "original_form": "가격은 안착한거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100037059.1.12",
          "form": "아! ㅋㅋ 치명적이네요ㅋㅋ",
          "original_form": "아! ㅋㅋ 치명적이네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100037059.1.13",
          "form": "허기져요 괴로워요ㅜㅜ",
          "original_form": "허기져요 괴로워요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100037059.1.14",
          "form": "한달치예요??",
          "original_form": "한달치예요??",
          "speaker_id": "1",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100037059.1.15",
          "form": "내가 왜 이러고있나 싶은",
          "original_form": "내가 왜 이러고있나 싶은",
          "speaker_id": "2",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100037059.1.16",
          "form": "이거한달하면ㅋㅋ",
          "original_form": "이거한달하면ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100037059.1.17",
          "form": "아...ㅜㅠ 식단하는거 너무 어려워요",
          "original_form": "아...ㅜㅠ 식단하는거 너무 어려워요",
          "speaker_id": "1",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100037059.1.18",
          "form": "건강해칠듯",
          "original_form": "건강해칠듯",
          "speaker_id": "2",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100037059.1.19",
          "form": "3일인가요?",
          "original_form": "3일인가요?",
          "speaker_id": "1",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100037059.1.20",
          "form": "48시간하는거에요 ㅎ",
          "original_form": "48시간하는거에요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100037059.1.21",
          "form": "아 근데 보통 3일도하더라고요 전 48시간이요ㅜㅜ",
          "original_form": "아 근데 보통 3일도하더라고요 전 48시간이요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 18:38"
        }
      ]
