      "utterance": [
        {
          "id": "MDRW2100021343.1.1",
          "form": "운동하시는거있나요",
          "original_form": "운동하시는거있나요",
          "speaker_id": "1",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100021343.1.2",
          "form": "아뇨 산책정도만요",
          "original_form": "아뇨 산책정도만요",
          "speaker_id": "2",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100021343.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100021343.1.4",
          "form": "홈트 해보려구요",
          "original_form": "홈트 해보려구요",
          "speaker_id": "1",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100021343.1.5",
          "form": "헬시는 다니셨나요?",
          "original_form": "헬시는 다니셨나요?",
          "speaker_id": "2",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100021343.1.6",
          "form": "헬스는",
          "original_form": "헬스는",
          "speaker_id": "2",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100021343.1.7",
          "form": "코로나전에요",
          "original_form": "코로나전에요",
          "speaker_id": "2",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100021343.1.8",
          "form": "아뇨 안해봤어요ㅠ",
          "original_form": "아뇨 안해봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100021343.1.9",
          "form": "저는 잠깐 다녔는데 꾸준히 다니기가 힘들어요 ㅋㅋ",
          "original_form": "저는 잠깐 다녔는데 꾸준히 다니기가 힘들어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100021343.1.10",
          "form": "마자요 pt도받으셨나요?",
          "original_form": "마자요 pt도받으셨나요?",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100021343.1.11",
          "form": "아뇨 피티까지 하면 거의 백마원돈이 금방나가서,,",
          "original_form": "아뇨 피티까지 하면 거의 백마원돈이 금방나가서,,",
          "speaker_id": "2",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100021343.1.12",
          "form": "헬스는 안가도 출장피티 받는 사람도있던데",
          "original_form": "헬스는 안가도 출장피티 받는 사람도있던데",
          "speaker_id": "2",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100021343.1.13",
          "form": "님은 피티받으셨나요?",
          "original_form": "님은 피티받으셨나요?",
          "speaker_id": "2",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100021343.1.14",
          "form": "나중에 받아보고싶네요 지금은 너무초보라ㅠ",
          "original_form": "나중에 받아보고싶네요 지금은 너무초보라ㅠ",
          "speaker_id": "1",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100021343.1.15",
          "form": "운동 목젂따라 다른데 제 동생은 스쿼트 자세잡아주는? 센터같은데가서 자세잡았었어요",
          "original_form": "운동 목젂따라 다른데 제 동생은 스쿼트 자세잡아주는? 센터같은데가서 자세잡았었어요",
          "speaker_id": "2",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100021343.1.16",
          "form": "오오 많이저렴한가요 그런데는",
          "original_form": "오오 많이저렴한가요 그런데는",
          "speaker_id": "1",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100021343.1.17",
          "form": "아뇨 운동처방하는데라 비쌌어요,,",
          "original_form": "아뇨 운동처방하는데라 비쌌어요,,",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100021343.1.18",
          "form": "거기도 한번 가는데 일이십했나",
          "original_form": "거기도 한번 가는데 일이십했나",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100021343.1.19",
          "form": "근데 부상입었음 그런거 받을만해요 고질적인 디스크같은거 있으신가요?",
          "original_form": "근데 부상입었음 그런거 받을만해요 고질적인 디스크같은거 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100021343.1.20",
