      "utterance": [
        {
          "id": "MDRW2100008118.1.1",
          "form": "앜ㅋㅋㅋㅋㅋㅋ갑분 연애와 결혼ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ갑분 연애와 결혼ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008118.1.2",
          "form": "나 근데 요즘 갑자기 주변에서",
          "original_form": "나 근데 요즘 갑자기 주변에서",
          "speaker_id": "2",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008118.1.3",
          "form": "커밍아웃 많이 들어... ㅇㅅㅇ...",
          "original_form": "커밍아웃 많이 들어... ㅇㅅㅇ...",
          "speaker_id": "2",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008118.1.4",
          "form": "ㅋㅋㅋㅋㅋ방금 주제를 이어나가기엔",
          "original_form": "ㅋㅋㅋㅋㅋ방금 주제를 이어나가기엔",
          "speaker_id": "1",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008118.1.5",
          "form": "이 주제가 더 좋을거같다는 생각이 들었어...ㅎㅎ",
          "original_form": "이 주제가 더 좋을거같다는 생각이 들었어...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008118.1.6",
          "form": "헉.... 커밍아웃??",
          "original_form": "헉.... 커밍아웃??",
          "speaker_id": "1",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008118.1.7",
          "form": "응... 뭐 상관없기는 한데 ㅋㅋㅋㅋㅋ",
          "original_form": "응... 뭐 상관없기는 한데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008118.1.8",
          "form": "엥 머야 사귀는 사람이 있었다고??",
          "original_form": "엥 머야 사귀는 사람이 있었다고??",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008118.1.9",
          "form": "아 같은 성별이라 안알렸던거구나 요런 느낌",
          "original_form": "아 같은 성별이라 안알렸던거구나 요런 느낌",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008118.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ헉 맞아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ헉 맞아",
          "speaker_id": "1",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008118.1.11",
          "form": "약간 커밍아웃은",
          "original_form": "약간 커밍아웃은",
          "speaker_id": "1",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008118.1.12",
          "form": "서로간에 신뢰가 따라줘야 할수있으니까....",
          "original_form": "서로간에 신뢰가 따라줘야 할수있으니까....",
          "speaker_id": "1",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008118.1.13",
          "form": "근데 생각보다 주변에 그런 연애 하는 사람들이 있었다는걸",
          "original_form": "근데 생각보다 주변에 그런 연애 하는 사람들이 있었다는걸",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008118.1.14",
          "form": "요즘들어서 깨닫고 있어",
          "original_form": "요즘들어서 깨닫고 있어",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008118.1.15",
          "form": "다들 연애 안하는게 아니었어 안알려준거지 ㅠ",
          "original_form": "다들 연애 안하는게 아니었어 안알려준거지 ㅠ",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008118.1.16",
          "form": "아 진짜 주변에 그렇게 많이??",
          "original_form": "아 진짜 주변에 그렇게 많이??",
          "speaker_id": "1",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008118.1.17",
          "form": "앜ㅋㅋㅋㅋㅋ 안하는게 아니라 안알려준거였다니",
          "original_form": "앜ㅋㅋㅋㅋㅋ 안하는게 아니라 안알려준거였다니",
          "speaker_id": "1",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008118.1.18",
          "form": "너무해ㅠㅠㅠㅠ",
          "original_form": "너무해ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008118.1.19",
          "form": "난 다들 모쏠이나 솔로들인줄 알았지...",
          "original_form": "난 다들 모쏠이나 솔로들인줄 알았지...",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008118.1.20",
          "form": "나만 몰랐던 이야기...",
          "original_form": "나만 몰랐던 이야기...",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008118.1.21",
          "form": "너는 뭐 소개팅이나 이런 얘기 안들어와?_?",
          "original_form": "너는 뭐 소개팅이나 이런 얘기 안들어와?_?",
          "speaker_id": "2",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008118.1.22",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ 그게아니라 다들 몰래몰래(?) 연애중이었구나",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ 그게아니라 다들 몰래몰래(?) 연애중이었구나",
          "speaker_id": "1",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008118.1.23",
          "form": "음 뭔가 정식으로 들어온다는 느낌보다는 소개시켜줄까유? 이런 느낌??ㅋㅋㅋㅋ",
          "original_form": "음 뭔가 정식으로 들어온다는 느낌보다는 소개시켜줄까유? 이런 느낌??ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008118.1.24",
          "form": "넌 어때??ㅎ.ㅎ",
          "original_form": "넌 어때??ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008118.1.25",
          "form": "난 머 직장 이번에 새로 옮겼고 ㅋㅋㅋㅋㅋ",
          "original_form": "난 머 직장 이번에 새로 옮겼고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008118.1.26",
          "form": "너도 알다시피 그런 얘기 들어올 자리에서 일한 적 없어서 딱히?",
          "original_form": "너도 알다시피 그런 얘기 들어올 자리에서 일한 적 없어서 딱히?",
          "speaker_id": "2",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008118.1.27",
          "form": "내주변은 나한테 그런 얘기 꺼낼 사람 없고 ㅋㅋㅋㅋㅋㅋㅋ 오히려 소개시켜달라는 뉘앙스는 들어봤지만 절대 안해줌 ㅎㅎ",
          "original_form": "내주변은 나한테 그런 얘기 꺼낼 사람 없고 ㅋㅋㅋㅋㅋㅋㅋ 오히려 소개시켜달라는 뉘앙스는 들어봤지만 절대 안해줌 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008118.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ원장이 그런얘기는 다행히(?)안했나바?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ원장이 그런얘기는 다행히(?)안했나바?",
          "speaker_id": "1",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008118.1.29",
          "form": "헉 소개시켜달라는 얘기는 누가해써?",
          "original_form": "헉 소개시켜달라는 얘기는 누가해써?",
          "speaker_id": "1",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100008118.1.30",
          "form": "소개팅 받고싶엇나바",
          "original_form": "소개팅 받고싶엇나바",
          "speaker_id": "1",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100008118.1.31",
          "form": "원장이 나이가 있는데 나를 어케 소개해줘 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "원장이 나이가 있는데 나를 어케 소개해줘 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100008118.1.32",
          "form": "소개시켜달란 얘긴 주로 남자..들..이..",
          "original_form": "소개시켜달란 얘긴 주로 남자..들..이..",
          "speaker_id": "2",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100008118.1.33",
          "form": "그치만 절대 안해주지 ㅎㅎ... 내 친구들 지켜...",
          "original_form": "그치만 절대 안해주지 ㅎㅎ... 내 친구들 지켜...",
          "speaker_id": "2",
          "time": "20210822 13:43"
        },
        {
          "id": "MDRW2100008118.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 맞네... 본인 나이가 있어서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 맞네... 본인 나이가 있어서",
          "speaker_id": "1",
