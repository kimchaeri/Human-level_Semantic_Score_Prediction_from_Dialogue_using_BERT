      "utterance": [
        {
          "id": "MDRW2100044257.1.1",
          "form": "누룽지 ....꼭 말아드셔야해요",
          "original_form": "누룽지 ....꼭 말아드셔야해요",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044257.1.2",
          "form": "헉 맞아요",
          "original_form": "헉 맞아요",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044257.1.3",
          "form": "사람들이 봉추인가 가는 이유가",
          "original_form": "사람들이 봉추인가 가는 이유가",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044257.1.4",
          "form": "누룽지랑 비벼먹는 거 때문이라던데!",
          "original_form": "누룽지랑 비벼먹는 거 때문이라던데!",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044257.1.5",
          "form": "맞아여ㅋㅋㅋㅋㅋㄱ진짜누룽지",
          "original_form": "맞아여ㅋㅋㅋㅋㅋㄱ진짜누룽지",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044257.1.6",
          "form": "와. , ,. .",
          "original_form": "와. , ,. .",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044257.1.7",
          "form": "미쳤어요",
          "original_form": "미쳤어요",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044257.1.8",
          "form": "찐누룽지를 주는 건가여?",
          "original_form": "찐누룽지를 주는 건가여?",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044257.1.9",
          "form": "처음에 돈까스비쥬얼의 누렁누렁한 누룽지를주는데",
          "original_form": "처음에 돈까스비쥬얼의 누렁누렁한 누룽지를주는데",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044257.1.10",
          "form": "그걸 양념에 찢어?숟가락으로",
          "original_form": "그걸 양념에 찢어?숟가락으로",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044257.1.11",
          "form": "미쳐버렸네여",
          "original_form": "미쳐버렸네여",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044257.1.12",
          "form": "돈까스비쥬얼이라니",
          "original_form": "돈까스비쥬얼이라니",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044257.1.13",
          "form": "눌러 펴가면서 양념적셔서드시면돼요",
          "original_form": "눌러 펴가면서 양념적셔서드시면돼요",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044257.1.14",
          "form": "진짜맛있어요!!!!!!",
          "original_form": "진짜맛있어요!!!!!!",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044257.1.15",
          "form": "ㅜㅜㅜㅜ포장해오는거말고",
          "original_form": "ㅜㅜㅜㅜ포장해오는거말고",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044257.1.16",
          "form": "가게에서먹는게 제맛인데ㅜㅜ",
          "original_form": "가게에서먹는게 제맛인데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044257.1.17",
          "form": "코로나 미워요",
          "original_form": "코로나 미워요",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044257.1.18",
          "form": "가게에서는 어느정도먹다가 누룽지시키면",
          "original_form": "가게에서는 어느정도먹다가 누룽지시키면",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044257.1.19",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044257.1.20",
          "form": "뜨끈뜨끈하게 바로주거든요ㅜㅜ",
          "original_form": "뜨끈뜨끈하게 바로주거든요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044257.1.21",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044257.1.22",
          "form": "코로나 이후로",
          "original_form": "코로나 이후로",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044257.1.23",
          "form": "외식 아예 안 하시나요?ㅠㅠ",
          "original_form": "외식 아예 안 하시나요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044257.1.24",
          "form": "아근데 유튜브보니까 찬밥이 양념이 더잘스며든다고하더라구요",
          "original_form": "아근데 유튜브보니까 찬밥이 양념이 더잘스며든다고하더라구요",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044257.1.25",
          "form": "저는 고기 같이 먹기힘든거만",
          "original_form": "저는 고기 같이 먹기힘든거만",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044257.1.26",
          "form": "외식하고ㅜㅜ",
          "original_form": "외식하고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044257.1.27",
          "form": "치킨.돈까스같은 배단 시키거나",
          "original_form": "치킨.돈까스같은 배단 시키거나",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044257.1.28",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044257.1.29",
          "form": "아까얘기한 다담소스같은거 밀키트같은거 사서 먹고있어요ㅜㅜ",
          "original_form": "아까얘기한 다담소스같은거 밀키트같은거 사서 먹고있어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044257.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ앜ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ앜ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044257.1.31",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044257.1.32",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044257.1.33",
          "form": "요새 밀키트 엄청 먹어요 작년부터 ㅠㅠ",
