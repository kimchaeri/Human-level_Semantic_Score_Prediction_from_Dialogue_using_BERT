      "utterance": [
        {
          "id": "MDRW2100002346.1.1",
          "form": "전 요즘에 포토프린터가 갖고 싶더라고요~",
          "original_form": "전 요즘에 포토프린터가 갖고 싶더라고요~",
          "speaker_id": "1",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100002346.1.2",
          "form": "집에서 뭐하고 시간보내나요?",
          "original_form": "집에서 뭐하고 시간보내나요?",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100002346.1.3",
          "form": "요즘엔 책 읽고 있어요~",
          "original_form": "요즘엔 책 읽고 있어요~",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100002346.1.4",
          "form": "가을와서 그런가 급 책이 땡기더라고요~",
          "original_form": "가을와서 그런가 급 책이 땡기더라고요~",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100002346.1.5",
          "form": "집안일 끝내고 느긋하게 책읽기~",
          "original_form": "집안일 끝내고 느긋하게 책읽기~",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100002346.1.6",
          "form": "좋을 것 같네요!",
          "original_form": "좋을 것 같네요!",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100002346.1.7",
          "form": "집안일 주로 누가 담당하세요?",
          "original_form": "집안일 주로 누가 담당하세요?",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100002346.1.8",
          "form": "부모님이랑 분담해서 하고 있어요~ㅎ",
          "original_form": "부모님이랑 분담해서 하고 있어요~ㅎ",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100002346.1.9",
          "form": "저흰 거의 엄마가 다 하시는것 같아요",
          "original_form": "저흰 거의 엄마가 다 하시는것 같아요",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100002346.1.10",
          "form": "저희도 주로 엄마가 총괄하시는데",
          "original_form": "저희도 주로 엄마가 총괄하시는데",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100002346.1.11",
          "form": "각자 할수 있는 부분들은 알아서 하는 거 같아요~",
          "original_form": "각자 할수 있는 부분들은 알아서 하는 거 같아요~",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100002346.1.12",
          "form": "맞아요~각자 할 수 있는 일 하고~",
          "original_form": "맞아요~각자 할 수 있는 일 하고~",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100002346.1.13",
          "form": "식사하고는 바로 설거지하려고 노력하고 있어요~",
          "original_form": "식사하고는 바로 설거지하려고 노력하고 있어요~",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100002346.1.14",
          "form": "밀리면 끝이 없더라구요ㅋㅋ",
          "original_form": "밀리면 끝이 없더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100002346.1.15",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "1",
          "time": "20210826 06:45"
        },
        {
          "id": "MDRW2100002346.1.16",
          "form": "해도 티 안나고 해도 끝이 없는게 집안일 같아요~",
          "original_form": "해도 티 안나고 해도 끝이 없는게 집안일 같아요~",
          "speaker_id": "1",
          "time": "20210826 06:46"
        },
        {
          "id": "MDRW2100002346.1.17",
          "form": "그러니깐요~",
          "original_form": "그러니깐요~",
          "speaker_id": "2",
          "time": "20210826 08:22"
        },
        {
          "id": "MDRW2100002346.1.18",
          "form": "안할수도 없고",
          "original_form": "안할수도 없고",
          "speaker_id": "2",
          "time": "20210826 08:22"
        },
        {
          "id": "MDRW2100002346.1.19",
          "form": "근데 또 미리미리 해놓으면 나중이 편해서~",
          "original_form": "근데 또 미리미리 해놓으면 나중이 편해서~",
          "speaker_id": "1",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100002346.1.20",
          "form": "조금씩조금씩 미리하게 되는 거 같아요~~",
          "original_form": "조금씩조금씩 미리하게 되는 거 같아요~~",
          "speaker_id": "1",
          "time": "20210826 10:31"
        }
      ]
