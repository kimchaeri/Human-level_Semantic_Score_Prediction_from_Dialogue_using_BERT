      "utterance": [
        {
          "id": "MDRW2100000593.1.1",
          "form": "운동하시는거 있으세요?",
          "original_form": "운동하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000593.1.2",
          "form": "러닝이요! 꾸준히 하다가 여름엔 넘 더워서 못 하고 있었어요~",
          "original_form": "러닝이요! 꾸준히 하다가 여름엔 넘 더워서 못 하고 있었어요~",
          "speaker_id": "1",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000593.1.3",
          "form": "야외 러닝이요? 조깅? 대단하세요",
          "original_form": "야외 러닝이요? 조깅? 대단하세요",
          "speaker_id": "2",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000593.1.4",
          "form": "저 달리기 진짜 싫어했는데! 하니까 재밌더라구요~",
          "original_form": "저 달리기 진짜 싫어했는데! 하니까 재밌더라구요~",
          "speaker_id": "1",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000593.1.5",
          "form": "야외에서 해야 더 기분좋아요!",
          "original_form": "야외에서 해야 더 기분좋아요!",
          "speaker_id": "1",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000593.1.6",
          "form": "친구도 잼있다고 추천하더라구요 근데 저는 자신이 없어요 헉헉 거리는게 힘들것같아서요",
          "original_form": "친구도 잼있다고 추천하더라구요 근데 저는 자신이 없어요 헉헉 거리는게 힘들것같아서요",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000593.1.7",
          "form": "ㅠㅠ마자요 그게 젤힘든데,, 하다보면 조금씩 늘더라구요!",
          "original_form": "ㅠㅠ마자요 그게 젤힘든데,, 하다보면 조금씩 늘더라구요!",
          "speaker_id": "1",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000593.1.8",
          "form": "하신지 오래되셨어요?",
          "original_form": "하신지 오래되셨어요?",
          "speaker_id": "2",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000593.1.9",
          "form": "런데이 라는 어플로 도움 받으면서 하니까 수월했어요~",
          "original_form": "런데이 라는 어플로 도움 받으면서 하니까 수월했어요~",
          "speaker_id": "1",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000593.1.10",
          "form": "아니요 작년에 처음 제대로 도전 ㅋㅋㅋ",
          "original_form": "아니요 작년에 처음 제대로 도전 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000593.1.11",
          "form": "친구 무슨 어플이야기하던데 그건가봐요",
          "original_form": "친구 무슨 어플이야기하던데 그건가봐요",
          "speaker_id": "2",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000593.1.12",
          "form": "무릎 아프거나하지않으세요? ㅎㅎ",
          "original_form": "무릎 아프거나하지않으세요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000593.1.13",
          "form": "넘 무리하면 조금 아프긴해요! 그럴땐 쉬면 낫더라구요~ㅎ",
          "original_form": "넘 무리하면 조금 아프긴해요! 그럴땐 쉬면 낫더라구요~ㅎ",
          "speaker_id": "1",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000593.1.14",
          "form": "뛰러나가는게 귀찮지는 않으세요?",
          "original_form": "뛰러나가는게 귀찮지는 않으세요?",
          "speaker_id": "2",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000593.1.15",
          "form": "귀찮아요 ㅠ진짜 가기싫을때도있고~",
          "original_form": "귀찮아요 ㅠ진짜 가기싫을때도있고~",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000593.1.16",
          "form": "그래도 억지로 나가도 뛰고나면 좋더라구용 ㅎ",
          "original_form": "그래도 억지로 나가도 뛰고나면 좋더라구용 ㅎ",
          "speaker_id": "1",
          "time": "20210819 17:20"
        }
      ]
