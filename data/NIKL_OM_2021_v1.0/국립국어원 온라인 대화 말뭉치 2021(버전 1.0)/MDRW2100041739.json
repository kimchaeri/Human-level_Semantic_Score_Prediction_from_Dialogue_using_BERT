      "utterance": [
        {
          "id": "MDRW2100041739.1.1",
          "form": "뭐냐면 그게",
          "original_form": "뭐냐면 그게",
          "speaker_id": "2",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041739.1.2",
          "form": "갈변현상 이용한거라서",
          "original_form": "갈변현상 이용한거라서",
          "speaker_id": "2",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041739.1.3",
          "form": "안되겠지",
          "original_form": "안되겠지",
          "speaker_id": "1",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041739.1.4",
          "form": "그 사과 갈변하는 갈변현상이에요 사진도 봤는데 괜찮더라구요",
          "original_form": "그 사과 갈변하는 갈변현상이에요 사진도 봤는데 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041739.1.5",
          "form": "샴푸로 어떻게 염색을 해",
          "original_form": "샴푸로 어떻게 염색을 해",
          "speaker_id": "1",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041739.1.6",
          "form": "돼요 감기만 하면 ㅋㅋㅋㅋ",
          "original_form": "돼요 감기만 하면 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041739.1.7",
          "form": "얼만데??",
          "original_form": "얼만데??",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041739.1.8",
          "form": "좀 비싸긴 한데...",
          "original_form": "좀 비싸긴 한데...",
          "speaker_id": "2",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041739.1.9",
          "form": "얼만데",
          "original_form": "얼만데",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041739.1.10",
          "form": "한통에 삼만원정도??",
          "original_form": "한통에 삼만원정도??",
          "speaker_id": "2",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041739.1.11",
          "form": "비싸다 안되겠다",
          "original_form": "비싸다 안되겠다",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041739.1.12",
          "form": "ㅠㅠ 그래도 머리 신경쓰시니까",
          "original_form": "ㅠㅠ 그래도 머리 신경쓰시니까",
          "speaker_id": "2",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041739.1.13",
          "form": "오래걸릴거아냐 한번만 쓰면 염색돼?",
          "original_form": "오래걸릴거아냐 한번만 쓰면 염색돼?",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041739.1.14",
          "form": "아뇨 그건 아니지만",
          "original_form": "아뇨 그건 아니지만",
          "speaker_id": "2",
          "time": "20210906 15:18"
        },
        {
          "id": "MDRW2100041739.1.15",
          "form": "한달정도 쓰면 된대요",
          "original_form": "한달정도 쓰면 된대요",
          "speaker_id": "2",
          "time": "20210906 15:18"
        },
        {
          "id": "MDRW2100041739.1.16",
          "form": "돈 많이 들어 레티놀도 돈인데",
          "original_form": "돈 많이 들어 레티놀도 돈인데",
          "speaker_id": "1",
          "time": "20210906 15:18"
        }
