      "utterance": [
        {
          "id": "MDRW2100010343.1.1",
          "form": "트렌치코트 몇번입으면 패딩꺼내야되더라구요",
          "original_form": "트렌치코트 몇번입으면 패딩꺼내야되더라구요",
          "speaker_id": "2",
          "time": "20210823 17:04"
        },
        {
          "id": "MDRW2100010343.1.2",
          "form": "그래서 트렌치코트가 장바구니에만 있어요ㅋㅋㅠㅜㅜ",
          "original_form": "그래서 트렌치코트가 장바구니에만 있어요ㅋㅋㅠㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 17:05"
        },
        {
          "id": "MDRW2100010343.1.3",
          "form": "간절기 옷사봐야 입지도 못해요",
          "original_form": "간절기 옷사봐야 입지도 못해요",
          "speaker_id": "2",
          "time": "20210823 17:06"
        },
        {
          "id": "MDRW2100010343.1.4",
          "form": "맞아요 잠깐입으면 아까워요",
          "original_form": "맞아요 잠깐입으면 아까워요",
          "speaker_id": "1",
          "time": "20210823 17:07"
        },
        {
          "id": "MDRW2100010343.1.5",
          "form": "그냥 긴팔 남방이나 가디건으로 잠깐 버티는거죠ㅎㅎ",
          "original_form": "그냥 긴팔 남방이나 가디건으로 잠깐 버티는거죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 17:08"
        },
        {
          "id": "MDRW2100010343.1.6",
          "form": "역시 기본템이 좋아요ㅋㅋㅋ",
          "original_form": "역시 기본템이 좋아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:09"
        },
        {
          "id": "MDRW2100010343.1.7",
          "form": "맞아요ㅎㅎ  올해는 그래도 옷을좀 사야겠어요",
          "original_form": "맞아요ㅎㅎ  올해는 그래도 옷을좀 사야겠어요",
          "speaker_id": "2",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010343.1.8",
          "form": "비그치고남 갑자기 추워질려나요?",
          "original_form": "비그치고남 갑자기 추워질려나요?",
          "speaker_id": "2",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010343.1.9",
          "form": "오늘부터 부쩍 쌀쌀해져서 그럴것같죠?!",
          "original_form": "오늘부터 부쩍 쌀쌀해져서 그럴것같죠?!",
          "speaker_id": "1",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010343.1.10",
          "form": "비오기 며칠전에 날씨 넘 좋았는데",
          "original_form": "비오기 며칠전에 날씨 넘 좋았는데",
          "speaker_id": "2",
          "time": "20210823 17:12"
        },
        {
          "id": "MDRW2100010343.1.11",
          "form": "구름도 이쁘고",
          "original_form": "구름도 이쁘고",
          "speaker_id": "2",
          "time": "20210823 17:12"
        },
        {
          "id": "MDRW2100010343.1.12",
          "form": "맞아요! 노을구름 대박이었어요",
          "original_form": "맞아요! 노을구름 대박이었어요",
          "speaker_id": "1",
          "time": "20210823 17:14"
        },
        {
          "id": "MDRW2100010343.1.13",
          "form": "저 그래서 사진찍었어요 카톡프사로ㅎㅎ",
          "original_form": "저 그래서 사진찍었어요 카톡프사로ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 17:14"
        },
        {
          "id": "MDRW2100010343.1.14",
          "form": "저도 바로 배경했어요ㅋㅋㅋ구름맛집",
          "original_form": "저도 바로 배경했어요ㅋㅋㅋ구름맛집",
          "speaker_id": "1",
          "time": "20210823 17:15"
        },
        {
          "id": "MDRW2100010343.1.15",
          "form": "딱 여행가기 좋은 날씨죠",
          "original_form": "딱 여행가기 좋은 날씨죠",
          "speaker_id": "2",
