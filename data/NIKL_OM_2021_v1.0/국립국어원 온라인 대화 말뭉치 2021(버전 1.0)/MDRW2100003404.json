      "utterance": [
        {
          "id": "MDRW2100003404.1.1",
          "form": "요즘은 거의 안하고있어요",
          "original_form": "요즘은 거의 안하고있어요",
          "speaker_id": "2",
          "time": "20210820 22:39"
        },
        {
          "id": "MDRW2100003404.1.2",
          "form": "집콕중이라 맨날 먹고 뒹구니 살만찌네요",
          "original_form": "집콕중이라 맨날 먹고 뒹구니 살만찌네요",
          "speaker_id": "2",
          "time": "20210820 22:39"
        },
        {
          "id": "MDRW2100003404.1.3",
          "form": "저도 배달음식 자주시켜먹어서 작년에비해 살이계속 찌네요",
          "original_form": "저도 배달음식 자주시켜먹어서 작년에비해 살이계속 찌네요",
          "speaker_id": "1",
          "time": "20210820 22:40"
        },
        {
          "id": "MDRW2100003404.1.4",
          "form": "저도요  최고점 찍은거같아요",
          "original_form": "저도요  최고점 찍은거같아요",
          "speaker_id": "2",
          "time": "20210820 22:41"
        },
        {
          "id": "MDRW2100003404.1.5",
          "form": "빼야되는데 한번찌니 쉽지않네요",
          "original_form": "빼야되는데 한번찌니 쉽지않네요",
          "speaker_id": "2",
          "time": "20210820 22:42"
        },
        {
          "id": "MDRW2100003404.1.6",
          "form": "다이어트는 안하세요?",
          "original_form": "다이어트는 안하세요?",
          "speaker_id": "1",
          "time": "20210820 22:42"
        },
        {
          "id": "MDRW2100003404.1.7",
          "form": "먹는 즐거움을 포기할수없어서   다이어트는 여지껏 안해봤어요",
          "original_form": "먹는 즐거움을 포기할수없어서   다이어트는 여지껏 안해봤어요",
          "speaker_id": "2",
          "time": "20210820 22:43"
        },
        {
          "id": "MDRW2100003404.1.8",
          "form": "옳은 선택이네요",
          "original_form": "옳은 선택이네요",
          "speaker_id": "1",
          "time": "20210820 22:44"
        },
        {
          "id": "MDRW2100003404.1.9",
          "form": "아픈곳은 없어요?",
          "original_form": "아픈곳은 없어요?",
          "speaker_id": "1",
          "time": "20210820 22:44"
        },
        {
          "id": "MDRW2100003404.1.10",
          "form": "네 가족이 다들 건강체질이라 몇년간 감기한번 안걸렸어요",
          "original_form": "네 가족이 다들 건강체질이라 몇년간 감기한번 안걸렸어요",
          "speaker_id": "2",
          "time": "20210820 22:45"
        },
        {
          "id": "MDRW2100003404.1.11",
          "form": "지인님은요?",
          "original_form": "지인님은요?",
          "speaker_id": "2",
          "time": "20210820 22:45"
        },
        {
          "id": "MDRW2100003404.1.12",
          "form": "저도 요즘 아픈곳은 없는데",
          "original_form": "저도 요즘 아픈곳은 없는데",
          "speaker_id": "1",
          "time": "20210820 22:47"
        },
        {
          "id": "MDRW2100003404.1.13",
          "form": "오늘 교정 시작했네요..",
          "original_form": "오늘 교정 시작했네요..",
          "speaker_id": "1",
          "time": "20210820 22:47"
        },
        {
          "id": "MDRW2100003404.1.14",
          "form": "저도 부정교합이라 교정하고싶은데 무서워서 못하겠어요",
          "original_form": "저도 부정교합이라 교정하고싶은데 무서워서 못하겠어요",
          "speaker_id": "2",
          "time": "20210820 22:49"
        },
        {
          "id": "MDRW2100003404.1.15",
          "form": "그냥 발치하고 조이는 교정이죠?",
          "original_form": "그냥 발치하고 조이는 교정이죠?",
          "speaker_id": "2",
          "time": "20210820 22:49"
        },
        {
          "id": "MDRW2100003404.1.16",
          "form": "저는 비발치로 결정했는데",
          "original_form": "저는 비발치로 결정했는데",
          "speaker_id": "1",
          "time": "20210820 22:51"
        },
        {
          "id": "MDRW2100003404.1.17",
          "form": "다음주나 다다음주에 사랑니는 뺄거같네요..",
          "original_form": "다음주나 다다음주에 사랑니는 뺄거같네요..",
          "speaker_id": "1",
          "time": "20210820 22:51"
        },
        {
          "id": "MDRW2100003404.1.18",
          "form": "사랑니 똑바로 나온거면 순식간에 뽑으니 걱정안하셔도 될듯요",
          "original_form": "사랑니 똑바로 나온거면 순식간에 뽑으니 걱정안하셔도 될듯요",
          "speaker_id": "2",
          "time": "20210820 22:52"
        },
        {
          "id": "MDRW2100003404.1.19",
          "form": "저는 사랑니 매복수평이라",
          "original_form": "저는 사랑니 매복수평이라",
          "speaker_id": "1",
          "time": "20210820 22:53"
        },
        {
          "id": "MDRW2100003404.1.20",
          "form": "그냥 뽑는곳은 안되고 대학병원이나 그런 전문병원 가야한다네요..",
          "original_form": "그냥 뽑는곳은 안되고 대학병원이나 그런 전문병원 가야한다네요..",
          "speaker_id": "1",
          "time": "20210820 22:53"
        },
        {
          "id": "MDRW2100003404.1.21",
          "form": "무서워요",
          "original_form": "무서워요",
          "speaker_id": "1",
          "time": "20210820 22:53"
        },
        {
          "id": "MDRW2100003404.1.22",
          "form": "앗 매복이면 좀 아프겠어요",
          "original_form": "앗 매복이면 좀 아프겠어요",
          "speaker_id": "2",
          "time": "20210820 22:53"
        }
      ]
    }
