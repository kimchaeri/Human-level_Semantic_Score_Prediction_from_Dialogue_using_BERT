      "utterance": [
        {
          "id": "MDRW2100032482.1.1",
          "form": "저 피나치공 완전 좋아해요",
          "original_form": "저 피나치공 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210905 19:35"
        },
        {
          "id": "MDRW2100032482.1.2",
          "form": "피나치공에선 더블포테이토",
          "original_form": "피나치공에선 더블포테이토",
          "speaker_id": "2",
          "time": "20210905 19:35"
        },
        {
          "id": "MDRW2100032482.1.3",
          "form": "무조건 더블포테이토 먹습니다",
          "original_form": "무조건 더블포테이토 먹습니다",
          "speaker_id": "2",
          "time": "20210905 19:36"
        },
        {
          "id": "MDRW2100032482.1.4",
          "form": "오오 궁금했는데 더블포테이토 기억할게요!!ㅋㅋㅋ",
          "original_form": "오오 궁금했는데 더블포테이토 기억할게요!!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100032482.1.5",
          "form": "치킨도 맛있는거 있나요??",
          "original_form": "치킨도 맛있는거 있나요??",
          "speaker_id": "1",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100032482.1.6",
          "form": "치킨은 그냥 매번 반반만 먹어요!",
          "original_form": "치킨은 그냥 매번 반반만 먹어요!",
          "speaker_id": "2",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100032482.1.7",
          "form": "저희집 근처는 반반 완전 바삭하고 맛있게 잘하더라구요",
          "original_form": "저희집 근처는 반반 완전 바삭하고 맛있게 잘하더라구요",
          "speaker_id": "2",
          "time": "20210905 20:30"
        },
        {
          "id": "MDRW2100032482.1.8",
          "form": "옼ㅋㅋㅋㅋ 완전 럭키세요!! 근처에 맛집이 있는게 얼마나 좋은일인지ㅜㅜ",
          "original_form": "옼ㅋㅋㅋㅋ 완전 럭키세요!! 근처에 맛집이 있는게 얼마나 좋은일인지ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100032482.1.9",
          "form": "집근처에 피나치공있으신가요?",
          "original_form": "집근처에 피나치공있으신가요?",
          "speaker_id": "2",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032482.1.10",
          "form": "아니요ㅜㅜ 멀리있어서 배달팁도 어마어마하더라구요.. 그래서 못먹어보고있어요ㅠ",
          "original_form": "아니요ㅜㅜ 멀리있어서 배달팁도 어마어마하더라구요.. 그래서 못먹어보고있어요ㅠ",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032482.1.11",
          "form": "오노 넘 아쉬워요 ㅠㅠ 느끼한거 좋아하시면",
          "original_form": "오노 넘 아쉬워요 ㅠㅠ 느끼한거 좋아하시면",
          "speaker_id": "2",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100032482.1.12",
          "form": "더블포테이토 진짜 최고예요",
          "original_form": "더블포테이토 진짜 최고예요",
          "speaker_id": "2",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100032482.1.13",
          "form": "마요네즈 대박입니다",
          "original_form": "마요네즈 대박입니다",
          "speaker_id": "2",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100032482.1.14",
          "form": "감자에 마요네즈면 완전 반칙수준이죠ㅜㅜㅜ",
          "original_form": "감자에 마요네즈면 완전 반칙수준이죠ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100032482.1.15",
          "form": "도미노 포테이토피자 드셔보셨어요??",
          "original_form": "도미노 포테이토피자 드셔보셨어요??",
          "speaker_id": "1",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100032482.1.16",
          "form": "앗 네!! 저 진짜 좋아래요",
          "original_form": "앗 네!! 저 진짜 좋아래요",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100032482.1.17",
          "form": "씬도우로 드셔보셨어요?",
          "original_form": "씬도우로 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100032482.1.18",
          "form": "아니요ㅜㅠ 처음먹어서 그냥 도우로 먹었어요!",
          "original_form": "아니요ㅜㅠ 처음먹어서 그냥 도우로 먹었어요!",
          "speaker_id": "1",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100032482.1.19",
          "form": "씬도우가 진리에요???",
          "original_form": "씬도우가 진리에요???",
          "speaker_id": "1",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100032482.1.20",
          "form": "네.. 제발 다음에 먹어주세요 완전 바삭하고 느끼하고 난리나요",
          "original_form": "네.. 제발 다음에 먹어주세요 완전 바삭하고 느끼하고 난리나요",
          "speaker_id": "2",
          "time": "20210905 21:32"
        },
        {
          "id": "MDRW2100032482.1.21",
          "form": "오오오오오 대박 완전 맛있을것 같아요 꼭 먹을게요ㅋㅋㅋㅋ",
          "original_form": "오오오오오 대박 완전 맛있을것 같아요 꼭 먹을게요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100032482.1.22",
          "form": "마요네즈도 추가해서 드세요??",
          "original_form": "마요네즈도 추가해서 드세요??",
          "speaker_id": "1",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100032482.1.23",
          "form": "네 진짜 그럼 극락가요..",
          "original_form": "네 진짜 그럼 극락가요..",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032482.1.24",
          "form": "강추할게요",
          "original_form": "강추할게요",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032482.1.25",
          "form": "씬도우+마요네즈 추가해주세요",
          "original_form": "씬도우+마요네즈 추가해주세요",
          "speaker_id": "2",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032482.1.26",
          "form": "앜ㅋㅋㅋ넼ㅋㅋㅋㅋ 뇌.에 새.기겠습니다",
          "original_form": "앜ㅋㅋㅋ넼ㅋㅋㅋㅋ 뇌.에 새.기겠습니다",
          "speaker_id": "1",
          "time": "20210905 22:38"
        },
        {
          "id": "MDRW2100032482.1.27",
          "form": "나폴리도우인가 그것도 드셔보셨나요??",
          "original_form": "나폴리도우인가 그것도 드셔보셨나요??",
