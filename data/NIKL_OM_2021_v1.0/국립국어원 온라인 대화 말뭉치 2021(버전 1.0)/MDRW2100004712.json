      "utterance": [
        {
          "id": "MDRW2100004712.1.1",
          "form": "무슨 일 하고 계세요?ㅎㅎ",
          "original_form": "무슨 일 하고 계세요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004712.1.2",
          "form": "저는 아직 취준생이예요 ㅠㅠ",
          "original_form": "저는 아직 취준생이예요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:00"
        },
        {
          "id": "MDRW2100004712.1.3",
          "form": "앗 저도 알바하면서 취업준비중이에요 ㅎㅎ",
          "original_form": "앗 저도 알바하면서 취업준비중이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004712.1.4",
          "form": "취업 진짜 힘들죠ㅠ 전공쪽으로 준비하시나요?",
          "original_form": "취업 진짜 힘들죠ㅠ 전공쪽으로 준비하시나요?",
          "speaker_id": "2",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004712.1.5",
          "form": "넵ㅜㅜ 일단 자격증부터 따고 있는데 스펙쌓기가 정말 힘드네요",
          "original_form": "넵ㅜㅜ 일단 자격증부터 따고 있는데 스펙쌓기가 정말 힘드네요",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100004712.1.6",
          "form": "저는 회계전공이에용",
          "original_form": "저는 회계전공이에용",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100004712.1.7",
          "form": "아 cpa? 였가요 회계사시험이",
          "original_form": "아 cpa? 였가요 회계사시험이",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100004712.1.8",
          "form": "되게 어렵다고 들은거같아요",
          "original_form": "되게 어렵다고 들은거같아요",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100004712.1.9",
          "form": "전 화학전공인데 공시생이에요..ㅋㅋㅋ",
          "original_form": "전 화학전공인데 공시생이에요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100004712.1.10",
          "form": "넵ㅎㅎ 근데 회계사시험까지는 못 볼것 같구 세무사 준비하고 있어용 ㅎㅎ",
          "original_form": "넵ㅎㅎ 근데 회계사시험까지는 못 볼것 같구 세무사 준비하고 있어용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100004712.1.11",
          "form": "화학전공인데 공시생이면 큰결심 하신거네요",
          "original_form": "화학전공인데 공시생이면 큰결심 하신거네요",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100004712.1.12",
          "form": "그럼 지금 공부하고 있는건가요?",
          "original_form": "그럼 지금 공부하고 있는건가요?",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100004712.1.13",
          "form": "올해 다행히 면접까지 가서 지금은 결과기다리구 있어요..ㅎ",
          "original_form": "올해 다행히 면접까지 가서 지금은 결과기다리구 있어요..ㅎ",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100004712.1.14",
          "form": "딱 알바해서 사고싶은거 사기 좋은 시기인데",
          "original_form": "딱 알바해서 사고싶은거 사기 좋은 시기인데",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100004712.1.15",
          "form": "알바가 안구해지네요ㅠㅡㅠ",
          "original_form": "알바가 안구해지네요ㅠㅡㅠ",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100004712.1.16",
          "form": "요즘 알바자리 정말 없죠ㅜㅜ",
          "original_form": "요즘 알바자리 정말 없죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100004712.1.17",
          "form": "결과 기다릴때가 제일 힘들던데 괜찮으세요?",
          "original_form": "결과 기다릴때가 제일 힘들던데 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100004712.1.18",
          "form": "그러니까요 공부도 일도 안하는데 정신상태는 더 안 좋네요..ㅋㅋㅋ",
          "original_form": "그러니까요 공부도 일도 안하는데 정신상태는 더 안 좋네요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100004712.1.19",
          "form": "뭔가 해야하는 일이 주어져야 편한 성격인가봐요 지금은 너무 잉여..",
          "original_form": "뭔가 해야하는 일이 주어져야 편한 성격인가봐요 지금은 너무 잉여..",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100004712.1.20",
          "form": "알바는 어떤거 하세요??",
          "original_form": "알바는 어떤거 하세요??",
          "speaker_id": "2",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100004712.1.21",
          "form": "저는 재택알바하구있어요ㅎㅎ",
          "original_form": "저는 재택알바하구있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:14"
        },
        {
          "id": "MDRW2100004712.1.22",
          "form": "문서작성하는 알바에요 ㅎㅎ",
          "original_form": "문서작성하는 알바에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:14"
        },
        {
          "id": "MDRW2100004712.1.23",
          "form": "우와 좋네요! 코로나 걱정도없구",
          "original_form": "우와 좋네요! 코로나 걱정도없구",
          "speaker_id": "2",
          "time": "20210822 21:15"
        },
        {
          "id": "MDRW2100004712.1.24",
          "form": "딱히 사람들이랑 부딪힐필요됴 없구 ㅎㅎ",
          "original_form": "딱히 사람들이랑 부딪힐필요됴 없구 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100004712.1.25",
          "form": "넵ㅎㅎ 진짜 좋은데 재택알바는 거의 단기라 다시구하기가 힘들어용ㅜㅜ",
