      "utterance": [
        {
          "id": "MDRW2100006000.1.1",
          "form": "최근 책 읽으시는 거 있으신가요",
          "original_form": "최근 책 읽으시는 거 있으신가요",
          "speaker_id": "1",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100006000.1.2",
          "form": "네 저 그 우리가 빛의속도로 갈 수 있다면",
          "original_form": "네 저 그 우리가 빛의속도로 갈 수 있다면",
          "speaker_id": "2",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100006000.1.3",
          "form": "소설책  읽었어요",
          "original_form": "소설책  읽었어요",
          "speaker_id": "2",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100006000.1.4",
          "form": "작가가 누구시죠",
          "original_form": "작가가 누구시죠",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100006000.1.5",
          "form": "아차차 있다몀이 아니라 없다면이군요",
          "original_form": "아차차 있다몀이 아니라 없다면이군요",
          "speaker_id": "2",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100006000.1.6",
          "form": "김초엽 작가님이에요",
          "original_form": "김초엽 작가님이에요",
          "speaker_id": "2",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100006000.1.7",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "1",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100006000.1.8",
          "form": "전 떨림과울림이요",
          "original_form": "전 떨림과울림이요",
          "speaker_id": "1",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100006000.1.9",
          "form": "소설인가요?",
          "original_form": "소설인가요?",
          "speaker_id": "2",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100006000.1.10",
          "form": "아 김상욱교수라고",
          "original_form": "아 김상욱교수라고",
          "speaker_id": "1",
          "time": "20210907 18:42"
        },
        {
          "id": "MDRW2100006000.1.11",
          "form": "과학책에에요",
          "original_form": "과학책에에요",
          "speaker_id": "1",
          "time": "20210907 18:43"
        },
        {
          "id": "MDRW2100006000.1.12",
          "form": "쉬운갸",
          "original_form": "쉬운갸",
          "speaker_id": "1",
          "time": "20210907 18:43"
        },
        {
          "id": "MDRW2100006000.1.13",
          "form": "ㅋㅋㅋㅋㅋ과학책...이요?(동공지진",
          "original_form": "ㅋㅋㅋㅋㅋ과학책...이요?(동공지진",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100006000.1.14",
          "form": "이과신가요",
          "original_form": "이과신가요",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100006000.1.15",
