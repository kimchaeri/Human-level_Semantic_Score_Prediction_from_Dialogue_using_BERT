      "utterance": [
        {
          "id": "MDRW2100031994.1.1",
          "form": "오 동남아음식 대부분 향신료가 강하지 않나요?",
          "original_form": "오 동남아음식 대부분 향신료가 강하지 않나요?",
          "speaker_id": "2",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031994.1.2",
          "form": "그런건 또 먹을만하던데요 ㅎㅎ 고수가 안맞는듯요",
          "original_form": "그런건 또 먹을만하던데요 ㅎㅎ 고수가 안맞는듯요",
          "speaker_id": "1",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100031994.1.3",
          "form": "알러지있는음식은없으세요?",
          "original_form": "알러지있는음식은없으세요?",
          "speaker_id": "2",
          "time": "20210905 10:36"
        },
        {
          "id": "MDRW2100031994.1.4",
          "form": "네 크게 없는듯요 ㅎㅎ",
          "original_form": "네 크게 없는듯요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031994.1.5",
          "form": "전 멜론을 못먹어요 ㅎㅎㅎㅎ",
          "original_form": "전 멜론을 못먹어요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031994.1.6",
          "form": "거 못먹는게",
          "original_form": "거 못먹는게",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100031994.1.7",
          "form": "꽤 있는 여자셨군요",
          "original_form": "꽤 있는 여자셨군요",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100031994.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ 맞아요 은근 가립니다 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 맞아요 은근 가립니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:59"
        },
        {
          "id": "MDRW2100031994.1.9",
          "form": "멜론은 알러지가있어서요 ㅋㅋ",
          "original_form": "멜론은 알러지가있어서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:59"
        },
        {
          "id": "MDRW2100031994.1.10",
          "form": "까다로운 여자셨군요",
          "original_form": "까다로운 여자셨군요",
          "speaker_id": "1",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100031994.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100031994.1.12",
          "form": "목이 간질간질거리고 부어요 ㅋㅋㅋㅋ 꽃가루 알레르기가 있다면 교차반응합니다 ㅋㅋ",
          "original_form": "목이 간질간질거리고 부어요 ㅋㅋㅋㅋ 꽃가루 알레르기가 있다면 교차반응합니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100031994.1.13",
          "form": "어우 불편하시겠어요",
          "original_form": "어우 불편하시겠어요",
          "speaker_id": "1",
          "time": "20210905 11:15"
        },
        {
          "id": "MDRW2100031994.1.14",
          "form": "와이프는 갑각류 있던데",
          "original_form": "와이프는 갑각류 있던데",
          "speaker_id": "1",
          "time": "20210905 11:15"
        },
        {
          "id": "MDRW2100031994.1.15",
          "form": "갑각류는 음식중에서도 심하게 반응하던데 모든 갑각류 다그러세요? ㅜㅜ",
          "original_form": "갑각류는 음식중에서도 심하게 반응하던데 모든 갑각류 다그러세요? ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 13:27"
        },
        {
          "id": "MDRW2100031994.1.16",
          "form": "그 익힌거는 그래도 괜찮아서",
          "original_form": "그 익힌거는 그래도 괜찮아서",
          "speaker_id": "1",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100031994.1.17",
          "form": "간장게장을 못먹다니!!!",
          "original_form": "간장게장을 못먹다니!!!",
          "speaker_id": "1",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100031994.1.18",
          "form": "못먹는분 굉장히 많으시더라구요 입술이 붓고 그러던데요!",
          "original_form": "못먹는분 굉장히 많으시더라구요 입술이 붓고 그러던데요!",
          "speaker_id": "2",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100031994.1.19",
          "form": "저는 잘먹어서요",
          "original_form": "저는 잘먹어서요",
          "speaker_id": "1",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100031994.1.20",
          "form": "애기야 아빠 닮아라",
          "original_form": "애기야 아빠 닮아라",
          "speaker_id": "1",
          "time": "20210905 13:54"
