      "utterance": [
        {
          "id": "MDRW2100027625.1.1",
          "form": "식사 하셨나요?",
          "original_form": "식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027625.1.2",
          "form": "네 오늘은 바빠서 컵라면 먹었어요",
          "original_form": "네 오늘은 바빠서 컵라면 먹었어요",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027625.1.3",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027625.1.4",
          "form": "헉 그러셨군요 ㅠㅠ",
          "original_form": "헉 그러셨군요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027625.1.5",
          "form": "저는 등갈비찜을 먹었습니다!",
          "original_form": "저는 등갈비찜을 먹었습니다!",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027625.1.6",
          "form": "저녁은 뭐드실건가요?",
          "original_form": "저녁은 뭐드실건가요?",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027625.1.7",
          "form": "저녁은 고기구워서 먹을거 같아요!",
          "original_form": "저녁은 고기구워서 먹을거 같아요!",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100027625.1.8",
          "form": "등갈비찜! 맛있으셨겠다!",
          "original_form": "등갈비찜! 맛있으셨겠다!",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027625.1.9",
          "form": "네 ㅎㅎ 정말 맛있었어요 ㅠㅠ",
          "original_form": "네 ㅎㅎ 정말 맛있었어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100027625.1.10",
          "form": "고기 너무 맛있겠어요 어느부위로 드시나요?~",
          "original_form": "고기 너무 맛있겠어요 어느부위로 드시나요?~",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100027625.1.11",
          "form": "정육점 사장님이 오늘 돼지 앞다리살이 좋다고하셔서",
          "original_form": "정육점 사장님이 오늘 돼지 앞다리살이 좋다고하셔서",
          "speaker_id": "1",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100027625.1.12",
          "form": "돼지 앞다리살 사왔어요",
          "original_form": "돼지 앞다리살 사왔어요",
          "speaker_id": "1",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100027625.1.13",
          "form": "무슨 요리 해드셨나요? 맛있는 저녁이겠어요!!",
          "original_form": "무슨 요리 해드셨나요? 맛있는 저녁이겠어요!!",
          "speaker_id": "2",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100027625.1.14",
          "form": "소금구이랑 파채랑 양파절임 먹을거예요",
          "original_form": "소금구이랑 파채랑 양파절임 먹을거예요",
          "speaker_id": "1",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100027625.1.15",
          "form": "와 ㅠㅠ너무 맛있는 식사겠어요",
          "original_form": "와 ㅠㅠ너무 맛있는 식사겠어요",
          "speaker_id": "2",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100027625.1.16",
          "form": "즐거운 식사하세요!!",
          "original_form": "즐거운 식사하세요!!",
          "speaker_id": "2",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100027625.1.17",
          "form": "저녁 뭐드시나요?",
          "original_form": "저녁 뭐드시나요?",
          "speaker_id": "1",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100027625.1.18",
          "form": "음료는 어떤걸로 하실건가요?",
          "original_form": "음료는 어떤걸로 하실건가요?",
          "speaker_id": "2",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100027625.1.19",
          "form": "저는 삼계탕먹었어요!",
          "original_form": "저는 삼계탕먹었어요!",
          "speaker_id": "2",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100027625.1.20",
          "form": "물 이요",
          "original_form": "물 이요",
          "speaker_id": "1",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100027625.1.21",
          "form": "삼계탕!",
          "original_form": "삼계탕!",
          "speaker_id": "1",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100027625.1.22",
          "form": "든든하게드시네요",
          "original_form": "든든하게드시네요",
          "speaker_id": "1",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100027625.1.23",
          "form": "네!! 정말 맛있었어요",
          "original_form": "네!! 정말 맛있었어요",
          "speaker_id": "2",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100027625.1.24",
          "form": "닭이 청계였는데 정말 맛있었어요!",
          "original_form": "닭이 청계였는데 정말 맛있었어요!",
          "speaker_id": "2",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100027625.1.25",
          "form": "우와...진짜 맛있었겠어요ㅠㅠ",
