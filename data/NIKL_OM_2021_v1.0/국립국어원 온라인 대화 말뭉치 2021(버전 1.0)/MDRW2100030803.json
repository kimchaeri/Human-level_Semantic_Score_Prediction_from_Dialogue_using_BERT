      "utterance": [
        {
          "id": "MDRW2100030803.1.1",
          "form": "안녕!",
          "original_form": "안녕!",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030803.1.2",
          "form": "주제가 식음료네... name2은 점심 먹었어?",
          "original_form": "주제가 식음료네... &name2&은 점심 먹었어?",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030803.1.3",
          "form": "하이하이",
          "original_form": "하이하이",
          "speaker_id": "2",
          "time": "20210904 12:47"
        },
        {
          "id": "MDRW2100030803.1.4",
          "form": "나 지금 점심먹으면서 키보드 두드리는중",
          "original_form": "나 지금 점심먹으면서 키보드 두드리는중",
          "speaker_id": "2",
          "time": "20210904 12:47"
        },
        {
          "id": "MDRW2100030803.1.5",
          "form": "롯데리아 더블2X버거인가 먼가",
          "original_form": "롯데리아 더블2X버거인가 먼가",
          "speaker_id": "2",
          "time": "20210904 12:47"
        },
        {
          "id": "MDRW2100030803.1.6",
          "form": "그거 먹고 있음",
          "original_form": "그거 먹고 있음",
          "speaker_id": "2",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030803.1.7",
          "form": "나 방금 점심 먹고 왔다",
          "original_form": "나 방금 점심 먹고 왔다",
          "speaker_id": "1",
          "time": "20210904 13:00"
        },
        {
          "id": "MDRW2100030803.1.8",
          "form": "라면",
          "original_form": "라면",
          "speaker_id": "1",
          "time": "20210904 13:00"
        },
        {
          "id": "MDRW2100030803.1.9",
          "form": "라면이라 함은 무슨 라면인가!",
          "original_form": "라면이라 함은 무슨 라면인가!",
          "speaker_id": "2",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030803.1.10",
          "form": "왕뚜껑이랑 건면 섞어서",
          "original_form": "왕뚜껑이랑 건면 섞어서",
          "speaker_id": "1",
          "time": "20210904 13:15"
        },
        {
          "id": "MDRW2100030803.1.11",
          "form": "파 계란 만두 등등 넣고",
          "original_form": "파 계란 만두 등등 넣고",
          "speaker_id": "1",
          "time": "20210904 13:15"
        },
        {
          "id": "MDRW2100030803.1.12",
          "form": "지금은 베라 먹는중 ㅋㅋㅋ",
          "original_form": "지금은 베라 먹는중 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:15"
        },
        {
          "id": "MDRW2100030803.1.13",
          "form": "아이스꼬북칩인가 이거 맛있네",
          "original_form": "아이스꼬북칩인가 이거 맛있네",
          "speaker_id": "1",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100030803.1.14",
          "form": "ㅁㅈㅁㅈ 그거 신메뉴지? 묘한 콘소메 맛이 있어",
          "original_form": "ㅁㅈㅁㅈ 그거 신메뉴지? 묘한 콘소메 맛이 있어",
          "speaker_id": "2",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100030803.1.15",
          "form": "왕뚜껑에 건면 섞는건 뭐야?",
          "original_form": "왕뚜껑에 건면 섞는건 뭐야?",
          "speaker_id": "2",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100030803.1.16",
          "form": "그냥 집에 왕뚜껑이랑 건면 있어서",
          "original_form": "그냥 집에 왕뚜껑이랑 건면 있어서",
          "speaker_id": "1",
          "time": "20210904 13:17"
        },
        {
          "id": "MDRW2100030803.1.17",
          "form": "두 개씩 섞어서 나눠 먹었는데",
          "original_form": "두 개씩 섞어서 나눠 먹었는데",
          "speaker_id": "1",
          "time": "20210904 13:17"
        },
        {
          "id": "MDRW2100030803.1.18",
          "form": "우리집은 원래 그래 ㅋㅋㅋㅋ",
          "original_form": "우리집은 원래 그래 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:17"
        },
        {
          "id": "MDRW2100030803.1.19",
          "form": "짱이다",
          "original_form": "짱이다",
          "speaker_id": "2",
          "time": "20210904 13:18"
        },
        {
          "id": "MDRW2100030803.1.20",
          "form": "왕뚜껑 건면...",
          "original_form": "왕뚜껑 건면...",
          "speaker_id": "2",
          "time": "20210904 13:18"
        },
        {
          "id": "MDRW2100030803.1.21",
          "form": "나도 이제 다 먹었으니 집정리좀 해야지",
          "original_form": "나도 이제 다 먹었으니 집정리좀 해야지",
          "speaker_id": "2",
          "time": "20210904 13:18"
        },
        {
          "id": "MDRW2100030803.1.22",
          "form": "식음료... 우허는 왠지 그런거 좋아할 것  같음",
          "original_form": "식음료... 우허는 왠지 그런거 좋아할 것  같음",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030803.1.23",
          "form": "1일 권장 칼로리 다 충족시키는 캡슐 나오면",
          "original_form": "1일 권장 칼로리 다 충족시키는 캡슐 나오면",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030803.1.24",
          "form": "식사를 포기한다 vs 안한다",
          "original_form": "식사를 포기한다 vs 안한다",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030803.1.25",
          "form": "절대 안 하지",
          "original_form": "절대 안 하지",
          "speaker_id": "1",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030803.1.26",
          "form": "나 먹는거 엄청 좋아해 ㅋㅋㅋ",
          "original_form": "나 먹는거 엄청 좋아해 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030803.1.27",
          "form": "아 고럼고럼 먹는거 좋지. 나는 여행가는걸 먹는거 위주로 가는 편",
          "original_form": "아 고럼고럼 먹는거 좋지. 나는 여행가는걸 먹는거 위주로 가는 편",
          "speaker_id": "2",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100030803.1.28",
          "form": "식사란 좋은것",
          "original_form": "식사란 좋은것",
          "speaker_id": "2",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100030803.1.29",
          "form": "먹는거 가리는거 있니?",
          "original_form": "먹는거 가리는거 있니?",
          "speaker_id": "2",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100030803.1.30",
          "form": "딱히 없을걸",
          "original_form": "딱히 없을걸",
          "speaker_id": "1",
          "time": "20210904 13:23"
        },
        {
          "id": "MDRW2100030803.1.31",
          "form": "아 근데 오일파스타 먹을때마다 목이 아프더라...",
          "original_form": "아 근데 오일파스타 먹을때마다 목이 아프더라...",
          "speaker_id": "1",
          "time": "20210904 13:23"
        },
        {
          "id": "MDRW2100030803.1.32",
          "form": "무슨 알레르기인지",
          "original_form": "무슨 알레르기인지",
          "speaker_id": "1",
          "time": "20210904 13:23"
        },
        {
          "id": "MDRW2100030803.1.33",
          "form": "피검사 한번 해봐",
          "original_form": "피검사 한번 해봐",
          "speaker_id": "2",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100030803.1.34",
          "form": "피검사 비용 실비로 청구 가능",
          "original_form": "피검사 비용 실비로 청구 가능",
