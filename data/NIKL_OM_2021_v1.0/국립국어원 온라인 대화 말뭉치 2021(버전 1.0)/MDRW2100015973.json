        {
          "id": "MDRW2100015973.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100015973.1.2",
          "form": "안녕하세용~",
          "original_form": "안녕하세용~",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100015973.1.3",
          "form": "여행 좋아해요?",
          "original_form": "여행 좋아해요?",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100015973.1.4",
          "form": "여행 원래는 좋아하지않았는데",
          "original_form": "여행 원래는 좋아하지않았는데",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100015973.1.5",
          "form": "코로나 지나면서부터 가고싶어졌어요",
          "original_form": "코로나 지나면서부터 가고싶어졌어요",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100015973.1.6",
          "form": "사람이 얽매이니까 자꾸 다른 곳을 갈망하게되더라고여 ㅋㅋㅋ",
          "original_form": "사람이 얽매이니까 자꾸 다른 곳을 갈망하게되더라고여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100015973.1.7",
          "form": "저도 사람 없는곳으로 떠나고싶네요",
          "original_form": "저도 사람 없는곳으로 떠나고싶네요",
          "speaker_id": "2",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100015973.1.8",
          "form": "여행할때 힐링하는 스타일이신가봐요!",
          "original_form": "여행할때 힐링하는 스타일이신가봐요!",
          "speaker_id": "1",
          "time": "20210826 14:07"
        },
        {
          "id": "MDRW2100015973.1.9",
          "form": "관광하는 여행이랑 힐링하는 여행이랑 좀 다르잖아여",
          "original_form": "관광하는 여행이랑 힐링하는 여행이랑 좀 다르잖아여",
          "speaker_id": "1",
          "time": "20210826 14:07"
        },
        {
          "id": "MDRW2100015973.1.10",
          "form": "네네 어디다녀오셨나요?",
          "original_form": "네네 어디다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210826 14:09"
        },
        {
          "id": "MDRW2100015973.1.11",
          "form": "해외여행은 일본만 다녀왔구여 국내여행은 얼마전에 여수랑 제주도 다녀왔어요!",
          "original_form": "해외여행은 일본만 다녀왔구여 국내여행은 얼마전에 여수랑 제주도 다녀왔어요!",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100015973.1.12",
          "form": "제주도 좋죠~",
          "original_form": "제주도 좋죠~",
          "speaker_id": "2",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100015973.1.13",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100015973.1.14",
          "form": "저 제주도 가서 남문시장에서 전복김밥이랑 회 포장해와서 먹었어요!",
          "original_form": "저 제주도 가서 남문시장에서 전복김밥이랑 회 포장해와서 먹었어요!",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100015973.1.15",
          "form": "흑돼지말이구이? 이것두 포장해오고",
          "original_form": "흑돼지말이구이? 이것두 포장해오고",
          "speaker_id": "1",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100015973.1.16",
          "form": "맛있는게 많더라구여 ㅎㅎ",
          "original_form": "맛있는게 많더라구여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100015973.1.17",
          "form": "와 전복김밥 맛있을거같아요",
          "original_form": "와 전복김밥 맛있을거같아요",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100015973.1.18",
          "form": "회로 들어있나요?",
          "original_form": "회로 들어있나요?",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100015973.1.19",
          "form": "익힌건가요?",
          "original_form": "익힌건가요?",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100015973.1.20",
          "form": "회는 아니고 전복이랑 밥을 같이 넣고 익혀서 미리 김밥을 말아놓았더라구여",
          "original_form": "회는 아니고 전복이랑 밥을 같이 넣고 익혀서 미리 김밥을 말아놓았더라구여",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100015973.1.21",
          "form": "거기에 계란을 겉에 입혀서 주세여",
          "original_form": "거기에 계란을 겉에 입혀서 주세여",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100015973.1.22",
          "form": "맛있었어용 ㅋㅋㅋ",
          "original_form": "맛있었어용 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100015973.1.23",
          "form": "계란까지.. 맛이 없을수가 없네요ㅋㅋ",
          "original_form": "계란까지.. 맛이 없을수가 없네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:19"
        }
      ]
    }
