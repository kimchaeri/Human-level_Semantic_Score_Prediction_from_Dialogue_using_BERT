      "utterance": [
        {
          "id": "MDRW2100047588.1.1",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100047588.1.2",
          "form": "저는 아직이요 9월예정이에요",
          "original_form": "저는 아직이요 9월예정이에요",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100047588.1.3",
          "form": "룸메이트가 오늘 맞고 왔는데 맞고나면",
          "original_form": "룸메이트가 오늘 맞고 왔는데 맞고나면",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100047588.1.4",
          "form": "좀 피곤하고 팔쪽이 아픈가봐요",
          "original_form": "좀 피곤하고 팔쪽이 아픈가봐요",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100047588.1.5",
          "form": "아 저희 가족도 그렇다더라구요",
          "original_form": "아 저희 가족도 그렇다더라구요",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100047588.1.6",
          "form": "가족분 2차도 맞으셨나요?",
          "original_form": "가족분 2차도 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100047588.1.7",
          "form": "네 가족중에서 저만 남았어요 ㅋㅋㅋ",
          "original_form": "네 가족중에서 저만 남았어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100047588.1.8",
          "form": "앜ㅋㅋㅋㅋ2차는 맞고나면 어때요?",
          "original_form": "앜ㅋㅋㅋㅋ2차는 맞고나면 어때요?",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100047588.1.9",
          "form": "가족분 반응이 어떠셨나요",
          "original_form": "가족분 반응이 어떠셨나요",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100047588.1.10",
          "form": "1차때보다 훨씬 괜찮아보이던데요 ㅋㅋㅋ 열도 없었어요",
          "original_form": "1차때보다 훨씬 괜찮아보이던데요 ㅋㅋㅋ 열도 없었어요",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100047588.1.11",
          "form": "아 진짜요? 2차가 더 아프대서 일정조절고민이 컸는데",
          "original_form": "아 진짜요? 2차가 더 아프대서 일정조절고민이 컸는데",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100047588.1.12",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100047588.1.13",
          "form": "2차는 1차예약하니 시간도 같은 시간으로 예약되더라구요",
          "original_form": "2차는 1차예약하니 시간도 같은 시간으로 예약되더라구요",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100047588.1.14",
          "form": "네 그렇게 자동으로 되어버려서",
          "original_form": "네 그렇게 자동으로 되어버려서",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100047588.1.15",
          "form": "전화로 조절할수 있대요",
          "original_form": "전화로 조절할수 있대요",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100047588.1.16",
          "form": "당황했어요 ㅋㅋㅋ",
          "original_form": "당황했어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100047588.1.17",
          "form": "오 전화로도 되는군여",
          "original_form": "오 전화로도 되는군여",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100047588.1.18",
          "form": "지역별로 예방접종 전화 예약번호가 있대요",
          "original_form": "지역별로 예방접종 전화 예약번호가 있대요",
          "speaker_id": "1",
          "time": "20210826 21:38"
        }
      ]
