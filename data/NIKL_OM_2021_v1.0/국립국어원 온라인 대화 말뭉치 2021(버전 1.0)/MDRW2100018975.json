      "utterance": [
        {
          "id": "MDRW2100018975.1.1",
          "form": "무슨책읽고있니",
          "original_form": "무슨책읽고있니",
          "speaker_id": "1",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018975.1.2",
          "form": "꿈꾸는 책들의 도시 1권 다 읽고",
          "original_form": "꿈꾸는 책들의 도시 1권 다 읽고",
          "speaker_id": "2",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100018975.1.3",
          "form": "이제 2권 읽을 차례야",
          "original_form": "이제 2권 읽을 차례야",
          "speaker_id": "2",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100018975.1.4",
          "form": "와 읽는속도 빠른가봐",
          "original_form": "와 읽는속도 빠른가봐",
          "speaker_id": "1",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100018975.1.5",
          "form": "출퇴근시간동안 읽는거지?",
          "original_form": "출퇴근시간동안 읽는거지?",
          "speaker_id": "1",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100018975.1.6",
          "form": "ㅇㅇㅇㅇ 안읽는날도 있고",
          "original_form": "ㅇㅇㅇㅇ 안읽는날도 있고",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100018975.1.7",
          "form": "보통 퇴근때보단 출근때 잘 읽히더라",
          "original_form": "보통 퇴근때보단 출근때 잘 읽히더라",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100018975.1.8",
          "form": "책도 체력이돼야읽지",
          "original_form": "책도 체력이돼야읽지",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100018975.1.9",
          "form": "오늘은 도서관에서 세계를 물들인 색이란 책이랑",
          "original_form": "오늘은 도서관에서 세계를 물들인 색이란 책이랑",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100018975.1.10",
          "form": "들고다니기무겁진않아?",
          "original_form": "들고다니기무겁진않아?",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100018975.1.11",
          "form": "타임 트래블러란 소설 빌려옴",
          "original_form": "타임 트래블러란 소설 빌려옴",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100018975.1.12",
          "form": "ㅇㅇ 한권이라 그냥 적당",
          "original_form": "ㅇㅇ 한권이라 그냥 적당",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100018975.1.13",
          "form": "책제목들이 다좋다",
          "original_form": "책제목들이 다좋다",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100018975.1.14",
          "form": "난 한2년존버했던",
          "original_form": "난 한2년존버했던",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100018975.1.15",
          "form": "보태니컬 셰익스피어",
          "original_form": "보태니컬 셰익스피어",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100018975.1.16",
          "form": "이책을 샀어",
          "original_form": "이책을 샀어",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100018975.1.17",
          "form": "헐 검색해봤는데 재밌어보여",
          "original_form": "헐 검색해봤는데 재밌어보여",
          "speaker_id": "2",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100018975.1.18",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "1",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100018975.1.19",
          "form": "나중에 빌려줄게",
          "original_form": "나중에 빌려줄게",
          "speaker_id": "1",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100018975.1.20",
          "form": "옹 좋아 ㅋㅋㅋㅋ 전에 읽던건 다 읽음?",
          "original_form": "옹 좋아 ㅋㅋㅋㅋ 전에 읽던건 다 읽음?",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100018975.1.21",
          "form": "채식주의자?",
          "original_form": "채식주의자?",
          "speaker_id": "1",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100018975.1.22",
          "form": "아그거도 안읽었댔지",
          "original_form": "아그거도 안읽었댔지",
          "speaker_id": "1",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100018975.1.23",
          "form": "그거도 담에보면 빌려줄게",
          "original_form": "그거도 담에보면 빌려줄게",
          "speaker_id": "1",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100018975.1.24",
          "form": "재밌어?",
          "original_form": "재밌어?",
          "speaker_id": "2",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100018975.1.25",
          "form": "ㅋㅋㅋㅋ저번에말했던대로",
          "original_form": "ㅋㅋㅋㅋ저번에말했던대로",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100018975.1.26",
          "form": "좀 외설적이야",
          "original_form": "좀 외설적이야",
          "speaker_id": "1",
          "time": "20210827 15:45"
        }
      ]
    }
