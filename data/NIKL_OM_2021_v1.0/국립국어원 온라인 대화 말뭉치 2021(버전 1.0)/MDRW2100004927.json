      "utterance": [
        {
          "id": "MDRW2100004927.1.1",
          "form": "여행!!",
          "original_form": "여행!!",
          "speaker_id": "1",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004927.1.2",
          "form": "여행가고싶다",
          "original_form": "여행가고싶다",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004927.1.3",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004927.1.4",
          "form": "요즘 날씨 좋은데",
          "original_form": "요즘 날씨 좋은데",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004927.1.5",
          "form": "맨날맨날",
          "original_form": "맨날맨날",
          "speaker_id": "1",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004927.1.6",
          "form": "여행다닐거야 담에",
          "original_form": "여행다닐거야 담에",
          "speaker_id": "1",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004927.1.7",
          "form": "맞아ㅜㅜㅜ해외도 가보고 그럴거야",
          "original_form": "맞아ㅜㅜㅜ해외도 가보고 그럴거야",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004927.1.8",
          "form": "돈 모아놨다가",
          "original_form": "돈 모아놨다가",
          "speaker_id": "1",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004927.1.9",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004927.1.10",
          "form": "해외어디?",
          "original_form": "해외어디?",
          "speaker_id": "1",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004927.1.11",
          "form": "음 파리~",
          "original_form": "음 파리~",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004927.1.12",
          "form": "나는 캐나다",
          "original_form": "나는 캐나다",
          "speaker_id": "1",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004927.1.13",
          "form": "파리 왜?",
          "original_form": "파리 왜?",
          "speaker_id": "1",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004927.1.14",
          "form": "호주",
          "original_form": "호주",
          "speaker_id": "2",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004927.1.15",
          "form": "호주좋다",
          "original_form": "호주좋다",
          "speaker_id": "1",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004927.1.16",
          "form": "파리 거리가 완전 내스타일이야",
          "original_form": "파리 거리가 완전 내스타일이야",
          "speaker_id": "2",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004927.1.17",
          "form": "에펠탑앞에서",
          "original_form": "에펠탑앞에서",
          "speaker_id": "1",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004927.1.18",
          "form": "에펠탑흉내내게?",
          "original_form": "에펠탑흉내내게?",
          "speaker_id": "1",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004927.1.19",
          "form": "응 맞아! 완전 재밌겠다",
          "original_form": "응 맞아! 완전 재밌겠다",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004927.1.20",
          "form": "잘어울리겠다",
          "original_form": "잘어울리겠다",
          "speaker_id": "1",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004927.1.21",
          "form": "호주는 왠지 공기가 좋을거같아서 가보고싶어",
          "original_form": "호주는 왠지 공기가 좋을거같아서 가보고싶어",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004927.1.22",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004927.1.23",
          "form": "근데 캥거루 만날지도",
          "original_form": "근데 캥거루 만날지도",
          "speaker_id": "1",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004927.1.24",
          "form": "그리고 미국도 한번쯤 가보고싶어",
          "original_form": "그리고 미국도 한번쯤 가보고싶어",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004927.1.25",
          "form": "영어공부하자",
          "original_form": "영어공부하자",
          "speaker_id": "1",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004927.1.26",
          "form": "저녁에 나가면 완전 힙할거같아",
          "original_form": "저녁에 나가면 완전 힙할거같아",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004927.1.27",
          "form": "너가 가이드해줘",
          "original_form": "너가 가이드해줘",
          "speaker_id": "1",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004927.1.28",
          "form": "흑흑 두유 해브 어 플랜?",
          "original_form": "흑흑 두유 해브 어 플랜?",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004927.1.29",
          "form": "징글징글허다",
          "original_form": "징글징글허다",
          "speaker_id": "1",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004927.1.30",
          "form": "여행가고싶어",
          "original_form": "여행가고싶어",
          "speaker_id": "1",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100004927.1.31",
          "form": "난 그거 탈거야",
          "original_form": "난 그거 탈거야",
          "speaker_id": "1",
          "time": "20210825 12:21"
        }
      ]
    }
  ]
