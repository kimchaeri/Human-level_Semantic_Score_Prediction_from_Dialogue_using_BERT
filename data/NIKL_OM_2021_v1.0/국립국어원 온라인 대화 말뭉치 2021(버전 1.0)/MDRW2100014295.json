      "utterance": [
        {
          "id": "MDRW2100014295.1.1",
          "form": "저녁뭐먹을껴?",
          "original_form": "저녁뭐먹을껴?",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014295.1.2",
          "form": "ㅋㅋㅋㅋ아직 고민중이야ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ아직 고민중이야ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014295.1.3",
          "form": "일단 아이스크림 먹는중ㅋㅋ",
          "original_form": "일단 아이스크림 먹는중ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014295.1.4",
          "form": "너는 뭐먹어?",
          "original_form": "너는 뭐먹어?",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014295.1.5",
          "form": "아이스크림 좋아하는구나ㅋㅋ",
          "original_form": "아이스크림 좋아하는구나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014295.1.6",
          "form": "나 회먹으려고 했는데 먹고싶은데가 안열어..",
          "original_form": "나 회먹으려고 했는데 먹고싶은데가 안열어..",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014295.1.7",
          "form": "그럼 나 아이스크림  완전 좋아하지",
          "original_form": "그럼 나 아이스크림  완전 좋아하지",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014295.1.8",
          "form": "노량진말고 또 있어?",
          "original_form": "노량진말고 또 있어?",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014295.1.9",
          "form": "무슨아이스크림먹어?",
          "original_form": "무슨아이스크림먹어?",
          "speaker_id": "1",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014295.1.10",
          "form": "응 저번에 숙성회먹었는데 맛있었던데가 있었거든",
          "original_form": "응 저번에 숙성회먹었는데 맛있었던데가 있었거든",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014295.1.11",
          "form": "오늘 먹어야하는데.. 쿠폰써야하는데..",
          "original_form": "오늘 먹어야하는데.. 쿠폰써야하는데..",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014295.1.12",
          "form": "나 벤앤제리ㅋㅋ짱맛있어ㅋㅋ",
          "original_form": "나 벤앤제리ㅋㅋ짱맛있어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014295.1.13",
          "form": "오~숙성회 맛있겠네",
          "original_form": "오~숙성회 맛있겠네",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014295.1.14",
          "form": "아 그거 쿠키도우? 꽂혔어?ㅋㅋㅋ",
          "original_form": "아 그거 쿠키도우? 꽂혔어?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014295.1.15",
          "form": "왠지 안열거같아ㅠㅠ젠장ㅠㅠ",
          "original_form": "왠지 안열거같아ㅠㅠ젠장ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014295.1.16",
          "form": "아쉬운대로 노량진이라도 먹어ㅠ",
          "original_form": "아쉬운대로 노량진이라도 먹어ㅠ",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014295.1.17",
          "form": "아니 이번에 다른맛 샀어",
          "original_form": "아니 이번에 다른맛 샀어",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014295.1.18",
          "form": "저번에 할인한다해서 여러가지 사봤지",
          "original_form": "저번에 할인한다해서 여러가지 사봤지",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014295.1.19",
          "form": "노량진도 좋긴한데.. 고민이군",
          "original_form": "노량진도 좋긴한데.. 고민이군",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014295.1.20",
          "form": "무슨맛??",
          "original_form": "무슨맛??",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014295.1.21",
          "form": "체리뭐시기랑 카라멜어쩌구, 커피, 초코",
          "original_form": "체리뭐시기랑 카라멜어쩌구, 커피, 초코",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014295.1.22",
          "form": "여러가지 샀어",
          "original_form": "여러가지 샀어",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014295.1.23",
          "form": "그 횟집꺼가 꽂히는구나? ㅋㅋ",
          "original_form": "그 횟집꺼가 꽂히는구나? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014295.1.24",
          "form": "진짜 여러가지 샀네ㅋㅋ\n지금은 무슨 맛 먹늠중?",
          "original_form": "진짜 여러가지 샀네ㅋㅋ\n지금은 무슨 맛 먹늠중?",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014295.1.25",
          "form": "응 맞아 그거를 먹고싶어ㅠㅠㅋㅋ",
          "original_form": "응 맞아 그거를 먹고싶어ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014295.1.26",
          "form": "체리랑 카라멜 먹어봤고 지금 초코 먹는중\n체리는 별로야ㅠ",
          "original_form": "체리랑 카라멜 먹어봤고 지금 초코 먹는중\n체리는 별로야ㅠ",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014295.1.27",
          "form": "맞아ㅋㅋ회라고 같은회가 아니지ㅋㅋ",
          "original_form": "맞아ㅋㅋ회라고 같은회가 아니지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014295.1.28",
          "form": "원래 체리좋아해?",
          "original_form": "원래 체리좋아해?",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014295.1.29",
          "form": "숙성횐데ㅠㅠ흑흑",
          "original_form": "숙성횐데ㅠㅠ흑흑",
          "speaker_id": "1",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014295.1.30",
          "form": "응 체리 좋아하는데 \n저건 별로네..",
          "original_form": "응 체리 좋아하는데 \n저건 별로네..",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014295.1.31",
          "form": "어서 다른 메뉴 생각해보렴ㅋㅋㅋ",
          "original_form": "어서 다른 메뉴 생각해보렴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014295.1.32",
          "form": "으아아아 먹고싶은거 못먹으면 너무 슬퍼",
          "original_form": "으아아아 먹고싶은거 못먹으면 너무 슬퍼",
          "speaker_id": "1",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014295.1.33",
          "form": "마자ㅋㅋㅋ\n꼭 내가 먹고싶은날 문닫더라",
          "original_form": "마자ㅋㅋㅋ\n꼭 내가 먹고싶은날 문닫더라",
          "speaker_id": "2",
          "time": "20210825 18:42"
        }
      ]
