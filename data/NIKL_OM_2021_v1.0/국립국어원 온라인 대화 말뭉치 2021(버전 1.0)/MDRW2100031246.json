      "utterance": [
        {
          "id": "MDRW2100031246.1.1",
          "form": "뭔가 서로 되게 귀엽겠어요 ㅋㅋㅋㅋ",
          "original_form": "뭔가 서로 되게 귀엽겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:02"
        },
        {
          "id": "MDRW2100031246.1.2",
          "form": "주로 우동이들아~ 이렇게 많이 불러요 ㅋㅋㅋ",
          "original_form": "주로 우동이들아~ 이렇게 많이 불러요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:02"
        },
        {
          "id": "MDRW2100031246.1.3",
          "form": "ㅋㅋㅋㅋ 귀여워요",
          "original_form": "ㅋㅋㅋㅋ 귀여워요",
          "speaker_id": "2",
          "time": "20210904 19:03"
        },
        {
          "id": "MDRW2100031246.1.4",
          "form": "더쿠는 호칭이 뭔가요??",
          "original_form": "더쿠는 호칭이 뭔가요??",
          "speaker_id": "1",
          "time": "20210904 19:03"
        },
        {
          "id": "MDRW2100031246.1.5",
          "form": "더쿠는 덬아 이렇게 불러요",
          "original_form": "더쿠는 덬아 이렇게 불러요",
          "speaker_id": "2",
          "time": "20210904 19:03"
        },
        {
          "id": "MDRW2100031246.1.6",
          "form": "아아 맞다맞다",
          "original_form": "아아 맞다맞다",
          "speaker_id": "1",
          "time": "20210904 19:03"
        },
        {
          "id": "MDRW2100031246.1.7",
          "form": "글쓴 사람은 원덬",
          "original_form": "글쓴 사람은 원덬",
          "speaker_id": "2",
          "time": "20210904 19:03"
        },
        {
          "id": "MDRW2100031246.1.8",
          "form": "들어봤어요 ㅋㅋㅋㅋ",
          "original_form": "들어봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:03"
        },
        {
          "id": "MDRW2100031246.1.9",
          "form": "오 호칭도 아시네요",
          "original_form": "오 호칭도 아시네요",
          "speaker_id": "2",
          "time": "20210904 19:03"
        },
        {
          "id": "MDRW2100031246.1.10",
          "form": "카페 많이 해서",
          "original_form": "카페 많이 해서",
          "speaker_id": "1",
          "time": "20210904 19:03"
        },
        {
          "id": "MDRW2100031246.1.11",
          "form": "더쿠 글도 많이 봤었거든요 ㅋㅋㅋㅋ",
          "original_form": "더쿠 글도 많이 봤었거든요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:03"
        },
        {
          "id": "MDRW2100031246.1.12",
          "form": "이슈게시판 이런곳에서요",
          "original_form": "이슈게시판 이런곳에서요",
          "speaker_id": "1",
          "time": "20210904 19:04"
        },
        {
          "id": "MDRW2100031246.1.13",
          "form": "아 다른 커뮤니티 글같은것도 올라오죠",
          "original_form": "아 다른 커뮤니티 글같은것도 올라오죠",
          "speaker_id": "2",
          "time": "20210904 19:05"
        },
        {
          "id": "MDRW2100031246.1.14",
          "form": "더쿠에도 종종 올라와요",
          "original_form": "더쿠에도 종종 올라와요",
          "speaker_id": "2",
          "time": "20210904 19:05"
        },
        {
          "id": "MDRW2100031246.1.15",
          "form": "인티 종종 봤어요ㅋㅋㅋㅋ 약간 허언증 처럼..ㅋㅋㅋ",
          "original_form": "인티 종종 봤어요ㅋㅋㅋㅋ 약간 허언증 처럼..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:05"
        },
        {
          "id": "MDRW2100031246.1.16",
          "form": "한남더힐 산다 이런 종류 글이요",
          "original_form": "한남더힐 산다 이런 종류 글이요",
          "speaker_id": "2",
          "time": "20210904 19:05"
        },
        {
          "id": "MDRW2100031246.1.17",
          "form": "앜ㅋㅋㅋㅋㅋ'ㅋㅋㅋㅋㅋㅋ 한남더힐 유명하죸ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ'ㅋㅋㅋㅋㅋㅋ 한남더힐 유명하죸ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:08"
        },
        {
          "id": "MDRW2100031246.1.18",
          "form": "한남더힐 사는사람 머리위로 박수 짝~",
          "original_form": "한남더힐 사는사람 머리위로 박수 짝~",
          "speaker_id": "1",
          "time": "20210904 19:09"
        },
        {
          "id": "MDRW2100031246.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 진짜로 컨셉을 잡고 그러는건지 다같이 노는건지 모르겠지만 재밌어보여요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 진짜로 컨셉을 잡고 그러는건지 다같이 노는건지 모르겠지만 재밌어보여요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:10"
        },
        {
          "id": "MDRW2100031246.1.20",
          "form": "컨셉이 아니라 그냥 진심으로 다들 거짓말하는 것 같더라구요 ㅋㅋㅋ",
          "original_form": "컨셉이 아니라 그냥 진심으로 다들 거짓말하는 것 같더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:14"
        },
        {
          "id": "MDRW2100031246.1.21",
          "form": "왜 그런 거짓말을 하는건지 ㅎ..",
          "original_form": "왜 그런 거짓말을 하는건지 ㅎ..",
          "speaker_id": "1",
          "time": "20210904 19:14"
        },
        {
          "id": "MDRW2100031246.1.22",
          "form": "암튼 제 3자 입장에선 어이없어서 웃기긴 해요",
          "original_form": "암튼 제 3자 입장에선 어이없어서 웃기긴 해요",
          "speaker_id": "1",
          "time": "20210904 19:14"
        },
        {
          "id": "MDRW2100031246.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:15"
        },
        {
          "id": "MDRW2100031246.1.24",
          "form": "아 저는 커뮤니티 하면서 기사를 안보게됐어요",
          "original_form": "아 저는 커뮤니티 하면서 기사를 안보게됐어요",
          "speaker_id": "2",
          "time": "20210904 19:15"
        },
        {
          "id": "MDRW2100031246.1.25",
          "form": "스퀘어라고 있는데 거기에 좀 중요한 기사같은건 다 올라와서",
          "original_form": "스퀘어라고 있는데 거기에 좀 중요한 기사같은건 다 올라와서",
          "speaker_id": "2",
          "time": "20210904 19:16"
        },
        {
          "id": "MDRW2100031246.1.26",
          "form": "그냥 커뮤니티에서 봐요",
          "original_form": "그냥 커뮤니티에서 봐요",
          "speaker_id": "2",
          "time": "20210904 19:16"
        },
        {
          "id": "MDRW2100031246.1.27",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210904 19:25"
        },
        {
          "id": "MDRW2100031246.1.28",
          "form": "저도.. 커뮤니티만 해도",
          "original_form": "저도.. 커뮤니티만 해도",
          "speaker_id": "1",
          "time": "20210904 19:25"
        },
        {
          "id": "MDRW2100031246.1.29",
          "form": "세상 돌아가는 거 어느정도 다 알게 돼서 ㅋㅋㅋ",
          "original_form": "세상 돌아가는 거 어느정도 다 알게 돼서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:26"
        },
        {
          "id": "MDRW2100031246.1.30",
          "form": "그냥 커뮤니티에서 살아요 ㅎㅎ",
          "original_form": "그냥 커뮤니티에서 살아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 19:26"
        }
      ]
