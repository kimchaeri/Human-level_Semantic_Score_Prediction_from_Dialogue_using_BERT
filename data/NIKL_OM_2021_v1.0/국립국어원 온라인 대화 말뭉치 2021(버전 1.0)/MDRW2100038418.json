      "utterance": [
        {
          "id": "MDRW2100038418.1.1",
          "form": "토르 가장 좋아해요 ㅎㅎ",
          "original_form": "토르 가장 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:47"
        },
        {
          "id": "MDRW2100038418.1.2",
          "form": "오 토르 좋아하시는구나",
          "original_form": "오 토르 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210902 12:48"
        },
        {
          "id": "MDRW2100038418.1.3",
          "form": "ㅋㅋㅋㅋㅋ라그나로크 보고 넘 존멋이었는데",
          "original_form": "ㅋㅋㅋㅋㅋ라그나로크 보고 넘 존멋이었는데",
          "speaker_id": "1",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100038418.1.4",
          "form": "돼지가 되어서 실망 ㅠㅠㅜㅜ",
          "original_form": "돼지가 되어서 실망 ㅠㅠㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100038418.1.5",
          "form": "캐릭터가 너무 망가졌었죠",
          "original_form": "캐릭터가 너무 망가졌었죠",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100038418.1.6",
          "form": "안대 쓴 것도 좋았는데ㅠ",
          "original_form": "안대 쓴 것도 좋았는데ㅠ",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100038418.1.7",
          "form": "아그니까요 ㅠㅠㅠ 토르 ㅠㅠㅠㅠ",
          "original_form": "아그니까요 ㅠㅠㅠ 토르 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100038418.1.8",
          "form": "배우 본인이 원한거라하지만 ㅠㅠㅠ",
          "original_form": "배우 본인이 원한거라하지만 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100038418.1.9",
          "form": "본인이 원한거라는건 좀 충격이네요",
          "original_form": "본인이 원한거라는건 좀 충격이네요",
          "speaker_id": "2",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100038418.1.10",
          "form": "돼지가 하고싶었나봐요 ㅠㅠ",
          "original_form": "돼지가 하고싶었나봐요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100038418.1.11",
          "form": "분장하는것두 힘든데",
          "original_form": "분장하는것두 힘든데",
          "speaker_id": "1",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100038418.1.12",
          "form": "분장을 당하고 싶었던걸까요ㅋㅋ",
          "original_form": "분장을 당하고 싶었던걸까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100038418.1.13",
          "form": "ㅋㅋㅋㅋㅋ그런가봐요 맨날 근육맨만하니",
          "original_form": "ㅋㅋㅋㅋㅋ그런가봐요 맨날 근육맨만하니",
          "speaker_id": "1",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100038418.1.14",
          "form": "돼지하고싶었던듯",
          "original_form": "돼지하고싶었던듯",
          "speaker_id": "1",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100038418.1.15",
          "form": "ㅋㅋㅋㅋ 그렇게 말하시니 웃기네요",
          "original_form": "ㅋㅋㅋㅋ 그렇게 말하시니 웃기네요",
          "speaker_id": "2",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100038418.1.16",
          "form": "다른 사람들은 다 싫어할텐데",
          "original_form": "다른 사람들은 다 싫어할텐데",
          "speaker_id": "2",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100038418.1.17",
          "form": "뭐...실제로 돼지가 될수없으니?ㅋㅋㅋㅋ",
          "original_form": "뭐...실제로 돼지가 될수없으니?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038418.1.18",
          "form": "저는 근육맨이조아요...",
          "original_form": "저는 근육맨이조아요...",
          "speaker_id": "1",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038418.1.19",
          "form": "저두요... 돼지는 아무래도...ㅎㅎ",
          "original_form": "저두요... 돼지는 아무래도...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100038418.1.20",
