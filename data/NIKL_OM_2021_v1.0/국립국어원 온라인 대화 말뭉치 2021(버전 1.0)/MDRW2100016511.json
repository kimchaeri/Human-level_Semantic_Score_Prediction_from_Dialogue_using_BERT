      "utterance": [
        {
          "id": "MDRW2100016511.1.1",
          "form": "님 백신맞았다면서여",
          "original_form": "님 백신맞았다면서여",
          "speaker_id": "1",
          "time": "20210826 13:25"
        },
        {
          "id": "MDRW2100016511.1.2",
          "form": "몸은 괜찮나여",
          "original_form": "몸은 괜찮나여",
          "speaker_id": "1",
          "time": "20210826 13:25"
        },
        {
          "id": "MDRW2100016511.1.3",
          "form": "건강ㅎㅏ려고 맞은걸요",
          "original_form": "건강ㅎㅏ려고 맞은걸요",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016511.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016511.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016511.1.6",
          "form": "저야머..2개월전에 맞아서..",
          "original_form": "저야머..2개월전에 맞아서..",
          "speaker_id": "1",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016511.1.7",
          "form": "가끔 비오면 맞은데 쑤셔요",
          "original_form": "가끔 비오면 맞은데 쑤셔요",
          "speaker_id": "1",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100016511.1.8",
          "form": "저는 아까 약먹어서 그런지 하나도 모르겠네요",
          "original_form": "저는 아까 약먹어서 그런지 하나도 모르겠네요",
          "speaker_id": "2",
          "time": "20210826 13:34"
        },
        {
          "id": "MDRW2100016511.1.9",
          "form": "코로나 사라졌으면..",
          "original_form": "코로나 사라졌으면..",
          "speaker_id": "2",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016511.1.10",
          "form": "그러겤ㅋㅋㅋ",
          "original_form": "그러겤ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016511.1.11",
          "form": "근데 오늘저녁이나 새벽에 피크고 나머지는 평온할거가태",
          "original_form": "근데 오늘저녁이나 새벽에 피크고 나머지는 평온할거가태",
          "speaker_id": "1",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016511.1.12",
          "form": "다음날이 제일 아프지않아?",
          "original_form": "다음날이 제일 아프지않아?",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016511.1.13",
          "form": "1차때아프면 코로나걸렸던거라던데",
          "original_form": "1차때아프면 코로나걸렸던거라던데",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016511.1.14",
          "form": "엥 아닐걸???",
          "original_form": "엥 아닐걸???",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100016511.1.15",
          "form": "면역이없어서 아픈거아냐??",
          "original_form": "면역이없어서 아픈거아냐??",
          "speaker_id": "1",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016511.1.16",
          "form": "라는 기사가.있더라곸ㅋㅋㅋ",
          "original_form": "라는 기사가.있더라곸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016511.1.17",
          "form": "휴 저녁에 힘내기 위해서 수육먹을거야",
          "original_form": "휴 저녁에 힘내기 위해서 수육먹을거야",
          "speaker_id": "2",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016511.1.18",
          "form": "바이코로나",
          "original_form": "바이코로나",
          "speaker_id": "2",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016511.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016511.1.20",
          "form": "다리아픈건여",
          "original_form": "다리아픈건여",
          "speaker_id": "1",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100016511.1.21",
          "form": "저리진않아여?",
          "original_form": "저리진않아여?",
          "speaker_id": "1",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100016511.1.22",
          "form": "무릎 또 아파 ㅜㅜ",
          "original_form": "무릎 또 아파 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100016511.1.23",
          "form": "생리때라그런지",
          "original_form": "생리때라그런지",
          "speaker_id": "2",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100016511.1.24",
          "form": "정형외과 가볼까봐",
          "original_form": "정형외과 가볼까봐",
          "speaker_id": "2",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100016511.1.25",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210826 14:07"
        },
        {
          "id": "MDRW2100016511.1.26",
          "form": "물리치료각인데",
          "original_form": "물리치료각인데",
          "speaker_id": "1",
          "time": "20210826 14:07"
        },
        {
          "id": "MDRW2100016511.1.27",
          "form": "한의원은 별로였던걸로...",
          "original_form": "한의원은 별로였던걸로...",
          "speaker_id": "2",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100016511.1.28",
          "form": "운동 언제가냐 ㅠㅠ 가고싶은데",
          "original_form": "운동 언제가냐 ㅠㅠ 가고싶은데",
          "speaker_id": "2",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100016511.1.29",
          "form": "양의보고 한의보는게 답인가바..",
          "original_form": "양의보고 한의보는게 답인가바..",
          "speaker_id": "1",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100016511.1.30",
          "form": "ㅠㅠ 나아야가지...화이팅..",
          "original_form": "ㅠㅠ 나아야가지...화이팅..",
          "speaker_id": "1",
          "time": "20210826 14:11"
        }
      ]
    }
  ]
