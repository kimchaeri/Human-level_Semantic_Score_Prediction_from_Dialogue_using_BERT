      "utterance": [
        {
          "id": "MDRW2100043528.1.1",
          "form": "사고싶으신 물건있나요",
          "original_form": "사고싶으신 물건있나요",
          "speaker_id": "1",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043528.1.2",
          "form": "저 갤럭시워치4랑 플립사고싶어요ㅜ",
          "original_form": "저 갤럭시워치4랑 플립사고싶어요ㅜ",
          "speaker_id": "2",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043528.1.3",
          "form": "와 이번에 재난지원금으로 편의점에서살수있대요",
          "original_form": "와 이번에 재난지원금으로 편의점에서살수있대요",
          "speaker_id": "1",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043528.1.4",
          "form": "갤럭시워치",
          "original_form": "갤럭시워치",
          "speaker_id": "1",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043528.1.5",
          "form": "오 신기하네요",
          "original_form": "오 신기하네요",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043528.1.6",
          "form": "넘한방에 사버리는거같아서 고민이좀들긴해요 ㅋㅋㅋㅋ",
          "original_form": "넘한방에 사버리는거같아서 고민이좀들긴해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043528.1.7",
          "form": "저도 동네 필라테스끊을까 고민중인데",
          "original_form": "저도 동네 필라테스끊을까 고민중인데",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043528.1.8",
          "form": "막상 유툽에 좋은 영상들도많아서",
          "original_form": "막상 유툽에 좋은 영상들도많아서",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043528.1.9",
          "form": "필테끊으려니 돈아깝네요",
          "original_form": "필테끊으려니 돈아깝네요",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043528.1.10",
          "form": "그쳐 동네 필테 가격은 어때요",
          "original_form": "그쳐 동네 필테 가격은 어때요",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043528.1.11",
          "form": "아 저렴해요",
          "original_form": "아 저렴해요",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043528.1.12",
          "form": "회당 만원",
          "original_form": "회당 만원",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043528.1.13",
          "form": "그래서 더고민되네요",
          "original_form": "그래서 더고민되네요",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043528.1.14",
          "form": "우와 진짜 괜찮네요",
          "original_form": "우와 진짜 괜찮네요",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043528.1.15",
          "form": "그룹인가여",
          "original_form": "그룹인가여",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043528.1.16",
          "form": "그쵸ㅋㅋ 선착순이라는데",
          "original_form": "그쵸ㅋㅋ 선착순이라는데",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043528.1.17",
          "form": "재난지원금 받을때까지",
          "original_form": "재난지원금 받을때까지",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043528.1.18",
          "form": "어떻게할까고민이네요",
          "original_form": "어떻게할까고민이네요",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043528.1.19",
          "form": "그룹이요",
          "original_form": "그룹이요",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043528.1.20",
          "form": "6대1",
          "original_form": "6대1",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043528.1.21",
          "form": "아하 그래도 진짜 괜찮네요",
          "original_form": "아하 그래도 진짜 괜찮네요",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043528.1.22",
          "form": "자세교정에 좋다면서요",
          "original_form": "자세교정에 좋다면서요",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043528.1.23",
          "form": "네 제가 자세가 안좋아서",
          "original_form": "네 제가 자세가 안좋아서",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043528.1.24",
          "form": "요가랑 필라테스로 많이 고쳤어요",
          "original_form": "요가랑 필라테스로 많이 고쳤어요",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043528.1.25",
          "form": "근데 너무키큰분들은",
          "original_form": "근데 너무키큰분들은",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043528.1.26",
          "form": "키클까봐 안한대요",
          "original_form": "키클까봐 안한대요",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043528.1.27",
          "form": "오 저도 자세가 진짜안좋은데 구미가당기네여ㅜ",
          "original_form": "오 저도 자세가 진짜안좋은데 구미가당기네여ㅜ",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043528.1.28",
          "form": "키도 크면 완전감사합니다",
          "original_form": "키도 크면 완전감사합니다",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043528.1.29",
          "form": "네네 키 조금 커용",
          "original_form": "네네 키 조금 커용",
