      "utterance": [
        {
          "id": "MDRW2100007798.1.1",
          "form": "청소해야되는데 언제한담",
          "original_form": "청소해야되는데 언제한담",
          "speaker_id": "1",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100007798.1.2",
          "form": "청소.. 대청소도해야하는데",
          "original_form": "청소.. 대청소도해야하는데",
          "speaker_id": "2",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007798.1.3",
          "form": "요즘 청소기만 간신히 돌림ㅋㅋ귀찮음병",
          "original_form": "요즘 청소기만 간신히 돌림ㅋㅋ귀찮음병",
          "speaker_id": "2",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007798.1.4",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007798.1.5",
          "form": "귀찮",
          "original_form": "귀찮",
          "speaker_id": "2",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007798.1.6",
          "form": "설거지도 산을 이루고 있는데 어쩐담",
          "original_form": "설거지도 산을 이루고 있는데 어쩐담",
          "speaker_id": "1",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007798.1.7",
          "form": "몹시요",
          "original_form": "몹시요",
          "speaker_id": "2",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007798.1.8",
          "form": "겨우겨우 빨래만 돌리는중",
          "original_form": "겨우겨우 빨래만 돌리는중",
          "speaker_id": "1",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007798.1.9",
          "form": "산타야하겠다",
          "original_form": "산타야하겠다",
          "speaker_id": "2",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007798.1.10",
          "form": "ㅋㅋ저 어제 3번돌림",
          "original_form": "ㅋㅋ저 어제 3번돌림",
          "speaker_id": "2",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007798.1.11",
          "form": "애기처럼 낮잠이 자꾸 들어",
          "original_form": "애기처럼 낮잠이 자꾸 들어",
          "speaker_id": "1",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007798.1.12",
          "form": "방금도 잠들다 깸",
          "original_form": "방금도 잠들다 깸",
          "speaker_id": "1",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007798.1.13",
          "form": "나도 지금 3번째 빨래돌리는중",
          "original_form": "나도 지금 3번째 빨래돌리는중",
          "speaker_id": "1",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007798.1.14",
          "form": "ㅋㅋ애기회귀",
          "original_form": "ㅋㅋ애기회귀",
          "speaker_id": "2",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007798.1.15",
          "form": "어제밤부터 ㅋㅋㅋ",
          "original_form": "어제밤부터 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007798.1.16",
          "form": "실상 우리애기는 안자여",
          "original_form": "실상 우리애기는 안자여",
          "speaker_id": "2",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007798.1.17",
          "form": "이런 ㅋㅋ",
          "original_form": "이런 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007798.1.18",
          "form": "어제드 12시주무심",
          "original_form": "어제드 12시주무심",
          "speaker_id": "2",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007798.1.19",
          "form": "나랑 바꾸자",
          "original_form": "나랑 바꾸자",
          "speaker_id": "1",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007798.1.20",
          "form": "어른처럼 자네?",
          "original_form": "어른처럼 자네?",
          "speaker_id": "2",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007798.1.21",
          "form": "신랑이 더 빨리자",
          "original_form": "신랑이 더 빨리자",
          "speaker_id": "2",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007798.1.22",
          "form": "그 집 애기 ㅋㅋ",
          "original_form": "그 집 애기 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100007798.1.23",
          "form": "엊그제 11시부터 어제 10시까지자고",
          "original_form": "엊그제 11시부터 어제 10시까지자고",
          "speaker_id": "2",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100007798.1.24",
          "form": "낮잠도 2시부터 4시반",
          "original_form": "낮잠도 2시부터 4시반",
          "speaker_id": "2",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100007798.1.25",
          "form": "리얼 애기",
          "original_form": "리얼 애기",
          "speaker_id": "1",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100007798.1.26",
          "form": "근데도 또 졸리대!!",
          "original_form": "근데도 또 졸리대!!",
          "speaker_id": "2",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100007798.1.27",
          "form": "ㅋㅋㅋㅋ아프냐고물어봄",
          "original_form": "ㅋㅋㅋㅋ아프냐고물어봄",
          "speaker_id": "2",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100007798.1.28",
          "form": "울 남편은 화장실 청소한다더니 컴퓨터 킴",
          "original_form": "울 남편은 화장실 청소한다더니 컴퓨터 킴",
          "speaker_id": "1",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100007798.1.29",
          "form": "ㅋㅋ원격으로 할건가봐",
          "original_form": "ㅋㅋ원격으로 할건가봐",
          "speaker_id": "2",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007798.1.30",
          "form": "왜 화장실만 하려하고 딴데는 안해 ㅋㅋㅋ",
          "original_form": "왜 화장실만 하려하고 딴데는 안해 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007798.1.31",
          "form": "왜 나머지는 내몫이지?",
          "original_form": "왜 나머지는 내몫이지?",
          "speaker_id": "1",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007798.1.32",
          "form": "ㅋㅋㅋ그처?",
          "original_form": "ㅋㅋㅋ그처?",
          "speaker_id": "2",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007798.1.33",
          "form": "이상하게 울남편도",
          "original_form": "이상하게 울남편도",
          "speaker_id": "2",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007798.1.34",
          "form": "근데 가뭄에 콩나듯 딴데하면 맘에 안들지",
          "original_form": "근데 가뭄에 콩나듯 딴데하면 맘에 안들지",
          "speaker_id": "1",
          "time": "20210822 11:30"
        },
        {
          "id": "MDRW2100007798.1.35",
          "form": "애기는 봐줄사람이 많겠다며",
          "original_form": "애기는 봐줄사람이 많겠다며",
          "speaker_id": "1",
          "time": "20210828 14:22"
        }
      ]
    }
