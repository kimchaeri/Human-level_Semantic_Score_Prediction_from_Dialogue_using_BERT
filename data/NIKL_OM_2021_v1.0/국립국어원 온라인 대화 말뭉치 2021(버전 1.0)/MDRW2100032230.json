      "utterance": [
        {
          "id": "MDRW2100032230.1.1",
          "form": "기억해 놓도록 하지요",
          "original_form": "기억해 놓도록 하지요",
          "speaker_id": "1",
          "time": "20210905 14:16"
        },
        {
          "id": "MDRW2100032230.1.2",
          "form": "게임은 하루에 몇 시간 하시나요",
          "original_form": "게임은 하루에 몇 시간 하시나요",
          "speaker_id": "2",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100032230.1.3",
          "form": "혹시 페이커 아시나요",
          "original_form": "혹시 페이커 아시나요",
          "speaker_id": "1",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100032230.1.4",
          "form": "시간 될 때마다 계속 합니다",
          "original_form": "시간 될 때마다 계속 합니다",
          "speaker_id": "1",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100032230.1.5",
          "form": "저는 오전에 1시간 쥐서 1시간 했어요",
          "original_form": "저는 오전에 1시간 쥐서 1시간 했어요",
          "speaker_id": "2",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100032230.1.6",
          "form": "원래 주말에는 이틀 내내 하는데",
          "original_form": "원래 주말에는 이틀 내내 하는데",
          "speaker_id": "1",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100032230.1.7",
          "form": "최근에는 게임 컴퓨터로 못 하고 있었어요",
          "original_form": "최근에는 게임 컴퓨터로 못 하고 있었어요",
          "speaker_id": "1",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100032230.1.8",
          "form": "페이커 알지요 세계 탑",
          "original_form": "페이커 알지요 세계 탑",
          "speaker_id": "2",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100032230.1.9",
          "form": "휴대폰 게임만 주구장창 합니다",
          "original_form": "휴대폰 게임만 주구장창 합니다",
          "speaker_id": "1",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100032230.1.10",
          "form": "PC 방을 못 가서 그런가요",
          "original_form": "PC 방을 못 가서 그런가요",
          "speaker_id": "2",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100032230.1.11",
          "form": "저도 pc 게임 안 한지 오래되었어요",
          "original_form": "저도 pc 게임 안 한지 오래되었어요",
          "speaker_id": "2",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100032230.1.12",
          "form": "PC로 어떤 게임 하셨나요",
          "original_form": "PC로 어떤 게임 하셨나요",
          "speaker_id": "1",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100032230.1.13",
          "form": "있으니까요 갤탭",
          "original_form": "있으니까요 갤탭",
          "speaker_id": "2",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100032230.1.14",
          "form": "꽃을 키웠지요",
          "original_form": "꽃을 키웠지요",
          "speaker_id": "2",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100032230.1.15",
          "form": "갤럭시 탭 정말 편하죠",
          "original_form": "갤럭시 탭 정말 편하죠",
          "speaker_id": "1",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100032230.1.16",
          "form": "저도 갤탭으로 꽃 한번씩 키웁니다 어제도 키워 봤네요",
          "original_form": "저도 갤탭으로 꽃 한번씩 키웁니다 어제도 키워 봤네요",
          "speaker_id": "1",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100032230.1.17",
          "form": "맞아요 키우기가 플래시 없이 할 수 있어서 감사합니다",
          "original_form": "맞아요 키우기가 플래시 없이 할 수 있어서 감사합니다",
          "speaker_id": "2",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100032230.1.18",
          "form": "최고점이 400점 겨우 넘은 점수 여서 안타깝습니다",
          "original_form": "최고점이 400점 겨우 넘은 점수 여서 안타깝습니다",
          "speaker_id": "1",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100032230.1.19",
          "form": "근데 실력은 꽝이라 합니다",
          "original_form": "근데 실력은 꽝이라 합니다",
          "speaker_id": "2",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100032230.1.20",
          "form": "저보다는 훨씬 잘 하시잖아요",
          "original_form": "저보다는 훨씬 잘 하시잖아요",
          "speaker_id": "1",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100032230.1.21",
          "form": "그래서 요새 정이 안 가요",
          "original_form": "그래서 요새 정이 안 가요",
          "speaker_id": "2",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100032230.1.22",
          "form": "저는 요령이 없습니다",
          "original_form": "저는 요령이 없습니다",
          "speaker_id": "1",
          "time": "20210905 14:19"
        }
