      "utterance": [
        {
          "id": "MDRW2100027299.1.1",
          "form": "작년에 비해 10키로쪄서",
          "original_form": "작년에 비해 10키로쪄서",
          "speaker_id": "2",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100027299.1.2",
          "form": "바지가 안맞더라구요 ㅋㅋㅋ",
          "original_form": "바지가 안맞더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100027299.1.3",
          "form": "아 옷입을때 너무 확 와닿죠? ㅠㅠㅠ 저두요",
          "original_form": "아 옷입을때 너무 확 와닿죠? ㅠㅠㅠ 저두요",
          "speaker_id": "1",
          "time": "20210902 11:34"
        },
        {
          "id": "MDRW2100027299.1.4",
          "form": "살이 배 위주로 찌니까 음식을 좀 다이어트식으로 먹을까 계획중이에요ㅠㅠ",
          "original_form": "살이 배 위주로 찌니까 음식을 좀 다이어트식으로 먹을까 계획중이에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027299.1.5",
          "form": "저녁식사만이라두요",
          "original_form": "저녁식사만이라두요",
          "speaker_id": "1",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027299.1.6",
          "form": "저도 올챙이배에요",
          "original_form": "저도 올챙이배에요",
          "speaker_id": "2",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027299.1.7",
          "form": "ㅠㅠ 뱃살 빼기 넘 힘들어요",
          "original_form": "ㅠㅠ 뱃살 빼기 넘 힘들어요",
          "speaker_id": "2",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027299.1.8",
          "form": "맞아요 ㅠㅠ 진짜 다른 곳보다 배가 확 와닿아요 ㅠㅠ 배는 탄수화물만 줄여도 들어간다는데",
          "original_form": "맞아요 ㅠㅠ 진짜 다른 곳보다 배가 확 와닿아요 ㅠㅠ 배는 탄수화물만 줄여도 들어간다는데",
          "speaker_id": "1",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027299.1.9",
          "form": "탄수화물 줄이는게 너무 힘드네요 ㅠㅠ",
          "original_form": "탄수화물 줄이는게 너무 힘드네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027299.1.10",
          "form": "저는 치킨 먹을때도",
          "original_form": "저는 치킨 먹을때도",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027299.1.11",
          "form": "밥 먹어요 ㅋㅋㅋ",
          "original_form": "밥 먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027299.1.12",
          "form": "치킨으로는 밥이 안되잖아요 ㅠㅠ",
          "original_form": "치킨으로는 밥이 안되잖아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027299.1.13",
          "form": "식단은 저녁 조절이 그나마 가장 쉬울 것 같은데 밥 줄이면 밤에 너무 배고플 것 같아요",
          "original_form": "식단은 저녁 조절이 그나마 가장 쉬울 것 같은데 밥 줄이면 밤에 너무 배고플 것 같아요",
          "speaker_id": "1",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027299.1.14",
          "form": "저는 집가면 6시라",
          "original_form": "저는 집가면 6시라",
          "speaker_id": "2",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027299.1.15",
          "form": "늦게 먹게 돼요 ㅠㅠㅠㅠ",
          "original_form": "늦게 먹게 돼요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027299.1.16",
          "form": "몇시쯤 드세요 저녁을",
          "original_form": "몇시쯤 드세요 저녁을",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027299.1.17",
          "form": "저는 한 7시 반이후에 먹어요 도착하면 그쯤 되더라구요 ㅠㅠ",
          "original_form": "저는 한 7시 반이후에 먹어요 도착하면 그쯤 되더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100027299.1.18",
          "form": "그쵸 늦게 먹게돼요 ㅜㅠ",
          "original_form": "그쵸 늦게 먹게돼요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027299.1.19",
          "form": "다이어트 할려면",
          "original_form": "다이어트 할려면",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027299.1.20",
          "form": "6시전엔가 먹으라는데",
          "original_form": "6시전엔가 먹으라는데",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027299.1.21",
          "form": "맞아요.. 근데 전 7시반 혹은 8시에 밥먹고 거의 움직이지도 않고 3시간지나면 자버려서 더 배가 나오나봐요",
          "original_form": "맞아요.. 근데 전 7시반 혹은 8시에 밥먹고 거의 움직이지도 않고 3시간지나면 자버려서 더 배가 나오나봐요",
          "speaker_id": "1",
          "time": "20210902 12:32"
        },
        {
          "id": "MDRW2100027299.1.22",
          "form": "6시 전에 먹기는 힘들어요 ㅠㅠ",
          "original_form": "6시 전에 먹기는 힘들어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027299.1.23",
          "form": "저두요 먹고 피곤해서",
          "original_form": "저두요 먹고 피곤해서",
          "speaker_id": "2",
          "time": "20210902 12:43"
        },
        {
          "id": "MDRW2100027299.1.24",
          "form": "바로 누워버림 ㅋㅋㅋ",
          "original_form": "바로 누워버림 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:43"
        },
        {
          "id": "MDRW2100027299.1.25",
          "form": "마쟈욬ㅋㅋㅋ 조심하세요 저 그러다가 역류성 식도염 왔어요 ㅠㅠㅠㅠ",
          "original_form": "마쟈욬ㅋㅋㅋ 조심하세요 저 그러다가 역류성 식도염 왔어요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027299.1.26",
          "form": "그래서 강제로 3시간동안 앉아있어요 ㅠㅠ",
          "original_form": "그래서 강제로 3시간동안 앉아있어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027299.1.27",
          "form": "헉 위내시경 해보셨나요?",
          "original_form": "헉 위내시경 해보셨나요?",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027299.1.28",
          "form": "위내시경 해보셨어요?",
          "original_form": "위내시경 해보셨어요?",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027299.1.29",
          "form": "식사하고 바로 누움 안좋긴하네요 ㅜㅜ",
          "original_form": "식사하고 바로 누움 안좋긴하네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 14:13"
        }
      ]
