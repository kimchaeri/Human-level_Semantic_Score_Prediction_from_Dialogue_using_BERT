      "utterance": [
        {
          "id": "MDRW2100043405.1.1",
          "form": "저녁 식사 하셨나요?",
          "original_form": "저녁 식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100043405.1.2",
          "form": "김치볶음밥",
          "original_form": "김치볶음밥",
          "speaker_id": "2",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100043405.1.3",
          "form": "오뎅탕 먹엇어용",
          "original_form": "오뎅탕 먹엇어용",
          "speaker_id": "2",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100043405.1.4",
          "form": "오 오늘 날씨에 어울리는 메뉴네요",
          "original_form": "오 오늘 날씨에 어울리는 메뉴네요",
          "speaker_id": "1",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100043405.1.5",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100043405.1.6",
          "form": "식사하셧나여",
          "original_form": "식사하셧나여",
          "speaker_id": "2",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100043405.1.7",
          "form": "네 저는 낙지볶음밥이랑 계란국",
          "original_form": "네 저는 낙지볶음밥이랑 계란국",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043405.1.8",
          "form": "이야",
          "original_form": "이야",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043405.1.9",
          "form": "맛잇는거드셧네여ㅜㅜ",
          "original_form": "맛잇는거드셧네여ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043405.1.10",
          "form": "직접요리하시나요",
          "original_form": "직접요리하시나요",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043405.1.11",
          "form": "가족들이 해줘요 보통",
          "original_form": "가족들이 해줘요 보통",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043405.1.12",
          "form": "주말엔 제가 좀 더 하고요",
          "original_form": "주말엔 제가 좀 더 하고요",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043405.1.13",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043405.1.14",
          "form": "저는 남자친구가 장금이라서",
          "original_form": "저는 남자친구가 장금이라서",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043405.1.15",
          "form": "와 좋네요",
          "original_form": "와 좋네요",
          "speaker_id": "1",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043405.1.16",
          "form": "저는 맛나게 먹어요",
          "original_form": "저는 맛나게 먹어요",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043405.1.17",
          "form": "후식은",
          "original_form": "후식은",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043405.1.18",
          "form": "비요뜨먹엇어요",
          "original_form": "비요뜨먹엇어요",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043405.1.19",
          "form": "비요뜨 맛있죠",
          "original_form": "비요뜨 맛있죠",
          "speaker_id": "1",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043405.1.20",
          "form": "한때 진짜 좋아했는데",
          "original_form": "한때 진짜 좋아했는데",
          "speaker_id": "1",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043405.1.21",
          "form": "아이스크림 먹고싶은데",
          "original_form": "아이스크림 먹고싶은데",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043405.1.22",
          "form": "참고잇어요",
          "original_form": "참고잇어요",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043405.1.23",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043405.1.24",
          "form": "왜 참으시나요?ㅜ",
          "original_form": "왜 참으시나요?ㅜ",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043405.1.25",
          "form": "ㅜㅜ나름 다이어터라서",
          "original_form": "ㅜㅜ나름 다이어터라서",
          "speaker_id": "2",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043405.1.26",
          "form": "참아야해욤ㅜㅜ",
          "original_form": "참아야해욤ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043405.1.27",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "1",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043405.1.28",
          "form": "저는 다이어트 한다고 나름 적게 먹었는데",
          "original_form": "저는 다이어트 한다고 나름 적게 먹었는데",
          "speaker_id": "1",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100043405.1.29",
          "form": "요즘 살이 쪄요ㅜ",
          "original_form": "요즘 살이 쪄요ㅜ",
          "speaker_id": "1",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100043405.1.30",
          "form": "저두여ㅜㅜㅜㅜㅜ",
          "original_form": "저두여ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100043405.1.31",
          "form": "그래서 오늘 아이스크림은 진짜 참아야해요",
          "original_form": "그래서 오늘 아이스크림은 진짜 참아야해요",
          "speaker_id": "2",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100043405.1.32",
          "form": "네 내일 낮에 드셔요ㅜ",
          "original_form": "네 내일 낮에 드셔요ㅜ",
          "speaker_id": "1",
          "time": "20210907 19:00"
        }
      ]
