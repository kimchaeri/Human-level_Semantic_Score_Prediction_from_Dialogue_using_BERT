      "utterance": [
        {
          "id": "MDRW2100040491.1.1",
          "form": "뭐사실거에요",
          "original_form": "뭐사실거에요",
          "speaker_id": "2",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100040491.1.2",
          "form": "나이제뭘로떠들어야할지고민돼요",
          "original_form": "나이제뭘로떠들어야할지고민돼요",
          "speaker_id": "1",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100040491.1.3",
          "form": "저번 지원금 꽃 하지 않았나요",
          "original_form": "저번 지원금 꽃 하지 않았나요",
          "speaker_id": "2",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100040491.1.4",
          "form": "먹을거나다사지않을까요",
          "original_form": "먹을거나다사지않을까요",
          "speaker_id": "1",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100040491.1.5",
          "form": "저번에뭐했더라..",
          "original_form": "저번에뭐했더라..",
          "speaker_id": "1",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100040491.1.6",
          "form": "내가 유퀴즈 진행자가 되었다고 생각하고 질문을 던져보는거에여",
          "original_form": "내가 유퀴즈 진행자가 되었다고 생각하고 질문을 던져보는거에여",
          "speaker_id": "2",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100040491.1.7",
          "form": "꽃 포장지 사셨던거 같아요",
          "original_form": "꽃 포장지 사셨던거 같아요",
          "speaker_id": "2",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100040491.1.8",
          "form": "아 유퀴즈진행자ㅋㄱㄲㄱㄱ",
          "original_form": "아 유퀴즈진행자ㅋㄱㄲㄱㄱ",
          "speaker_id": "1",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100040491.1.9",
          "form": "같이 연산동 문구점 갔던 기억",
          "original_form": "같이 연산동 문구점 갔던 기억",
          "speaker_id": "2",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100040491.1.10",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100040491.1.11",
          "form": "그건 지역 지원금이었나",
          "original_form": "그건 지역 지원금이었나",
          "speaker_id": "2",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100040491.1.12",
          "form": "우리그때갔다가안사고나온거기억해요?",
          "original_form": "우리그때갔다가안사고나온거기억해요?",
          "speaker_id": "1",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100040491.1.13",
          "form": "ㄱㅈㅋㅈㅋ",
          "original_form": "ㄱㅈㅋㅈㅋ",
          "speaker_id": "1",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100040491.1.14",
          "form": "네그거5만원",
          "original_form": "네그거5만원",
          "speaker_id": "1",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.15",
          "form": "네넼ㅋㅋㅋㅋㅋ저만 샀어요",
          "original_form": "네넼ㅋㅋㅋㅋㅋ저만 샀어요",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.16",
          "form": "막대기",
          "original_form": "막대기",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.17",
          "form": "ㅋㅋㅋㅋㄱㅈㅋㅈㅋㅈㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅈㅋㅈㅋㅈㅋ",
          "speaker_id": "1",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.18",
          "form": "아직도 서랍안에 있어요",
          "original_form": "아직도 서랍안에 있어요",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.19",
          "form": "막대기맞닥ㄱㄱㅋㅈㄱㅈㄱ",
          "original_form": "막대기맞닥ㄱㄱㅋㅈㄱㅈㄱ",
          "speaker_id": "1",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.20",
          "form": "에?",
          "original_form": "에?",
          "speaker_id": "1",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.22",
          "form": "잘쓰시나요ㅡ적고있었는데",
          "original_form": "잘쓰시나요ㅡ적고있었는데",
          "speaker_id": "1",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.23",
          "form": "언젠가 쓰겠지 하면서",
          "original_form": "언젠가 쓰겠지 하면서",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.25",
          "form": "아직서랍에있다닞ㅋㅈㄱㅈㄱㅈㅋ",
          "original_form": "아직서랍에있다닞ㅋㅈㄱㅈㄱㅈㅋ",
          "speaker_id": "1",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.26",
          "form": "반전",
          "original_form": "반전",
          "speaker_id": "1",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.27",
          "form": "벌써1년전아닌가요",
          "original_form": "벌써1년전아닌가요",
          "speaker_id": "1",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.28",
          "form": "코로나땜에 사실 뭘 할 수가 없으니",
          "original_form": "코로나땜에 사실 뭘 할 수가 없으니",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.30",
          "form": "ㅋㅋㄱㅋㅈㅋㅈㄱ",
          "original_form": "ㅋㅋㄱㅋㅈㅋㅈㄱ",
          "speaker_id": "1",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.31",
          "form": "그러네여 1년전",
          "original_form": "그러네여 1년전",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.32",
          "form": "시간가는거",
          "original_form": "시간가는거",
          "speaker_id": "1",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040491.1.33",
          "form": "미 쳤  다",
          "original_form": "미 쳤  다",
          "speaker_id": "1",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100040491.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100040491.1.35",
          "form": "추석 선물 사시나여",
          "original_form": "추석 선물 사시나여",
          "speaker_id": "2",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100040491.1.36",
          "form": "저는선물안보내여",
          "original_form": "저는선물안보내여",
          "speaker_id": "1",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100040491.1.37",
          "form": "사시나요",
          "original_form": "사시나요",
          "speaker_id": "1",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100040491.1.38",
          "form": "심심이 친구한테 잼 추천 받앗는데 맛있어보여서 인스타 추가했어여",
          "original_form": "심심이 친구한테 잼 추천 받앗는데 맛있어보여서 인스타 추가했어여",
          "speaker_id": "2",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100040491.1.39",
          "form": "복음자리말구?",
          "original_form": "복음자리말구?",
          "speaker_id": "1",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100040491.1.40",
          "form": "저희집도 추석선물 잘 안해여 받기만 해여",
          "original_form": "저희집도 추석선물 잘 안해여 받기만 해여",
          "speaker_id": "2",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100040491.1.41",
          "form": "무슨 독일 브랜드였어여",
          "original_form": "무슨 독일 브랜드였어여",
          "speaker_id": "2",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100040491.1.42",
          "form": "오 고급..",
          "original_form": "오 고급..",
          "speaker_id": "1",
          "time": "20210904 21:56"
        },
        {
          "id": "MDRW2100040491.1.43",
          "form": "결혼을하면",
          "original_form": "결혼을하면",
          "speaker_id": "1",
          "time": "20210904 21:56"
        },
        {
          "id": "MDRW2100040491.1.44",
          "form": "저런걸다챙겨보내더라구요",
          "original_form": "저런걸다챙겨보내더라구요",
          "speaker_id": "1",
          "time": "20210904 21:56"
        },
        {
          "id": "MDRW2100040491.1.45",
          "form": "그러게 말이에요",
          "original_form": "그러게 말이에요",
          "speaker_id": "2",
          "time": "20210904 21:56"
        },
        {
          "id": "MDRW2100040491.1.46",
          "form": "그런거너무부러워요",
          "original_form": "그런거너무부러워요",
          "speaker_id": "1",
          "time": "20210904 21:59"
        },
        {
          "id": "MDRW2100040491.1.47",
          "form": "안돼요그게",
          "original_form": "안돼요그게",
          "speaker_id": "1",
