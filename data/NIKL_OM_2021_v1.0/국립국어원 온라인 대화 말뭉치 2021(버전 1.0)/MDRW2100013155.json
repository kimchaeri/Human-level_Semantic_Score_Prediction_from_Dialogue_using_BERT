      "utterance": [
        {
          "id": "MDRW2100013155.1.1",
          "form": "아니용",
          "original_form": "아니용",
          "speaker_id": "2",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100013155.1.2",
          "form": "늑장부려서 못갔어요 ㅠㅠ",
          "original_form": "늑장부려서 못갔어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100013155.1.3",
          "form": "헉 아쉽네요",
          "original_form": "헉 아쉽네요",
          "speaker_id": "1",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100013155.1.4",
          "form": "네 ㅜㅜ 오늘 저녁에도 차지연 캐스팅이긴 한데",
          "original_form": "네 ㅜㅜ 오늘 저녁에도 차지연 캐스팅이긴 한데",
          "speaker_id": "2",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100013155.1.5",
          "form": "그냥 포기하려구요",
          "original_form": "그냥 포기하려구요",
          "speaker_id": "2",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100013155.1.6",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100013155.1.7",
          "form": "저녁에 갔다오기엔 너무 귀찮아서..",
          "original_form": "저녁에 갔다오기엔 너무 귀찮아서..",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013155.1.8",
          "form": "차지연씨는 2번이나 공연하는거에요?",
          "original_form": "차지연씨는 2번이나 공연하는거에요?",
          "speaker_id": "1",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013155.1.9",
          "form": "네 오늘은 수요일 마티네라",
          "original_form": "네 오늘은 수요일 마티네라",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013155.1.10",
          "form": "아쉽네요 ㅠㅠ",
          "original_form": "아쉽네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013155.1.11",
          "form": "아무래도 예술의 전당이 너무 멀긴 하죠",
          "original_form": "아무래도 예술의 전당이 너무 멀긴 하죠",
          "speaker_id": "1",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013155.1.12",
          "form": "네 교통도 넘 안좋고",
          "original_form": "네 교통도 넘 안좋고",
          "speaker_id": "2",
          "time": "20210825 13:10"
        },
        {
          "id": "MDRW2100013155.1.13",
          "form": "역에서 멀고 ㅜ",
          "original_form": "역에서 멀고 ㅜ",
          "speaker_id": "2",
          "time": "20210825 13:10"
        },
        {
          "id": "MDRW2100013155.1.14",
          "form": "주변에 아무것도 없잖아요",
          "original_form": "주변에 아무것도 없잖아요",
          "speaker_id": "1",
          "time": "20210825 13:10"
        },
        {
          "id": "MDRW2100013155.1.15",
          "form": "네 대학로나 광화문만 됐어도 갔을텐데 ㅜㅜ",
          "original_form": "네 대학로나 광화문만 됐어도 갔을텐데 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 13:10"
        },
        {
          "id": "MDRW2100013155.1.16",
          "form": "언제까지에요?",
          "original_form": "언제까지에요?",
          "speaker_id": "1",
          "time": "20210825 13:10"
        },
        {
          "id": "MDRW2100013155.1.17",
          "form": "음 아마 9월초까지하고",
          "original_form": "음 아마 9월초까지하고",
          "speaker_id": "2",
          "time": "20210825 13:10"
        },
        {
          "id": "MDRW2100013155.1.18",
          "form": "지방으로 내려가는 거 같아요 ㅠㅠ",
          "original_form": "지방으로 내려가는 거 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 13:10"
        },
        {
          "id": "MDRW2100013155.1.19",
          "form": "ㅎㅎ 보실거에요?",
          "original_form": "ㅎㅎ 보실거에요?",
          "speaker_id": "1",
          "time": "20210825 13:10"
        },
        {
          "id": "MDRW2100013155.1.20",
          "form": "보고싶긴한데",
          "original_form": "보고싶긴한데",
          "speaker_id": "2",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100013155.1.21",
          "form": "넘 귀찮아서",
          "original_form": "넘 귀찮아서",
          "speaker_id": "2",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100013155.1.22",
          "form": "그냥 포기할까도 생각해요",
          "original_form": "그냥 포기할까도 생각해요",
          "speaker_id": "2",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100013155.1.23",
          "form": "저는 진작에 포기했어요",
          "original_form": "저는 진작에 포기했어요",
          "speaker_id": "1",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100013155.1.24",
          "form": "내일 광화문 연가 보러갈까말까 고민이에요 ㅠ",
          "original_form": "내일 광화문 연가 보러갈까말까 고민이에요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 02:32"
        }
      ]
