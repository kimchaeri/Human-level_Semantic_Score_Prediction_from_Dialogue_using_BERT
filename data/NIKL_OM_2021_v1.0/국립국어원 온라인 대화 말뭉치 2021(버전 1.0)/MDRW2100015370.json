      "utterance": [
        {
          "id": "MDRW2100015370.1.1",
          "form": "좋아하는 tv프로그램 있으세여?",
          "original_form": "좋아하는 tv프로그램 있으세여?",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015370.1.2",
          "form": "전참시 펜트 하우스요",
          "original_form": "전참시 펜트 하우스요",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015370.1.3",
          "form": "나혼산이랑요",
          "original_form": "나혼산이랑요",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015370.1.4",
          "form": "저도 나혼산 즐겨봐요 ㅋㅋㅋ최근에는 챙겨보지 못햇지만 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 나혼산 즐겨봐요 ㅋㅋㅋ최근에는 챙겨보지 못햇지만 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015370.1.5",
          "form": "저도 가끔 챙겨봐요",
          "original_form": "저도 가끔 챙겨봐요",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015370.1.6",
          "form": "요새 유튜브도 보고 이것저것봐서",
          "original_form": "요새 유튜브도 보고 이것저것봐서",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015370.1.7",
          "form": "딱히 많이 보진 않아요",
          "original_form": "딱히 많이 보진 않아요",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015370.1.8",
          "form": "유튜브 영화소개 주식 내성발톱 피지 관리 ㅋㅋ봐요",
          "original_form": "유튜브 영화소개 주식 내성발톱 피지 관리 ㅋㅋ봐요",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015370.1.9",
          "form": "맞아요 아무래도 유투브가 잇으니 tv를 덜보게되더라구요",
          "original_form": "맞아요 아무래도 유투브가 잇으니 tv를 덜보게되더라구요",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015370.1.10",
          "form": "주식부터 내성발톱까지 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "주식부터 내성발톱까지 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015370.1.11",
          "form": "엄청난 스펙트럼...!",
          "original_form": "엄청난 스펙트럼...!",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015370.1.12",
          "form": "ㅋㅋ그게 내성발톱은 좀 희열이 느껴져요",
          "original_form": "ㅋㅋ그게 내성발톱은 좀 희열이 느껴져요",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015370.1.13",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015370.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ전 내성발톱이랑 피지는 본적 없는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ전 내성발톱이랑 피지는 본적 없는데",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015370.1.15",
          "form": "귀지제거영상은 유투브에서 본적잇어요",
          "original_form": "귀지제거영상은 유투브에서 본적잇어요",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015370.1.16",
          "form": "오저도요 ㅋㅋ",
          "original_form": "오저도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015370.1.17",
          "form": "귀에서 괴생명체가 나오는 줄 알앗어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "귀에서 괴생명체가 나오는 줄 알앗어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015370.1.18",
          "form": "너무커섴ㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무커섴ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015370.1.19",
          "form": "근데 장비 사봤는데 드릴요 ㅋㅋ",
          "original_form": "근데 장비 사봤는데 드릴요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015370.1.20",
          "form": "안돼요 피나요 ㅋㅋ",
          "original_form": "안돼요 피나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015370.1.21",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015370.1.22",
          "form": "아 쾌감있죠",
          "original_form": "아 쾌감있죠",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015370.1.23",
          "form": "유투브가 진짜 장르도 다양하고 볼게 많긴 해요 ㅋㅋㅋㅋㅋ",
          "original_form": "유투브가 진짜 장르도 다양하고 볼게 많긴 해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015370.1.24",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015370.1.25",
          "form": "요새 티비에 연결되서 크게보곺좋아요",
          "original_form": "요새 티비에 연결되서 크게보곺좋아요",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015370.1.26",
          "form": "기술이 점점 좋아져요",
          "original_form": "기술이 점점 좋아져요",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015370.1.27",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015370.1.28",
          "form": "그런데 내성발톱제거영상을 티비로 크게보면 좀 무서울것같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "그런데 내성발톱제거영상을 티비로 크게보면 좀 무서울것같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015370.1.29",
          "form": "리모컨으로 딱",
          "original_form": "리모컨으로 딱",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015370.1.30",
          "form": "주식만 크게봐요 ㅋㅋ",
          "original_form": "주식만 크게봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015370.1.31",
          "form": "음악하고요 ㅋㅋ",
          "original_form": "음악하고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015370.1.32",
          "form": "고건 몰래",
          "original_form": "고건 몰래",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015370.1.33",
          "form": "애니도 좋아해요",
          "original_form": "애니도 좋아해요",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015370.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ주식만크게보기 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ주식만크게보기 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015370.1.35",
          "form": "애니도 좋아하세요? 다양하게 즐겨보시네요",
          "original_form": "애니도 좋아하세요? 다양하게 즐겨보시네요",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015370.1.36",
          "form": "내 소리 엄청크게",
          "original_form": "내 소리 엄청크게",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015370.1.37",
          "form": "다 그런거죠",
          "original_form": "다 그런거죠",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015370.1.38",
          "form": "네 주술회전 보려고",
          "original_form": "네 주술회전 보려고",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015370.1.39",
          "form": "왓챠 보고있네요",
          "original_form": "왓챠 보고있네요",
          "speaker_id": "1",
          "time": "20210825 23:06"
        }
      ]
    }
