      "utterance": [
        {
          "id": "MDRW2100028642.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028642.1.2",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028642.1.3",
          "form": "키우시나요?",
          "original_form": "키우시나요?",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028642.1.4",
          "form": "이런 저도 안 키우고 있어요 예전에 햄르터 키워봤어요",
          "original_form": "이런 저도 안 키우고 있어요 예전에 햄르터 키워봤어요",
          "speaker_id": "2",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028642.1.5",
          "form": "햄스터",
          "original_form": "햄스터",
          "speaker_id": "2",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028642.1.6",
          "form": "햄스터 너무 귀엽죠!!",
          "original_form": "햄스터 너무 귀엽죠!!",
          "speaker_id": "1",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028642.1.7",
          "form": "저도 옛날에 사촌 언니가 키워서 자주 보러 갔어요ㅋㅋㅋ",
          "original_form": "저도 옛날에 사촌 언니가 키워서 자주 보러 갔어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028642.1.8",
          "form": "귀엽죠ㅎㄹ 근데 햄스터는 수명이 짧더라고요 ㅠㅠ",
          "original_form": "귀엽죠ㅎㄹ 근데 햄스터는 수명이 짧더라고요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028642.1.9",
          "form": "반려동물을 키우면 뭐 키우고 싶으세요?",
          "original_form": "반려동물을 키우면 뭐 키우고 싶으세요?",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028642.1.10",
          "form": "저는 먼저 보내는 게 너무 무서워서 못 키우겠어요ㅠㅠ",
          "original_form": "저는 먼저 보내는 게 너무 무서워서 못 키우겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028642.1.11",
          "form": "예전에는 고양이 키우고 싶어했어요ㅎㅎ",
          "original_form": "예전에는 고양이 키우고 싶어했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028642.1.12",
          "form": "친구가 고양이 키우는데 생각보다 애교도 많고 혼자서 잘 지내서 좋더라고요",
          "original_form": "친구가 고양이 키우는데 생각보다 애교도 많고 혼자서 잘 지내서 좋더라고요",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028642.1.13",
          "form": "나중에 자취하면 키워 보세요!",
          "original_form": "나중에 자취하면 키워 보세요!",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028642.1.14",
          "form": "기회가 되면 그래야겠어요",
          "original_form": "기회가 되면 그래야겠어요",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028642.1.15",
          "form": "name2 님은 키우고 싶은 동물 있으세요?",
          "original_form": "&name2& 님은 키우고 싶은 동물 있으세요?",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028642.1.16",
          "form": "저는 강아지 키워보고 싶어요. 애교쟁이",
          "original_form": "저는 강아지 키워보고 싶어요. 애교쟁이",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028642.1.17",
          "form": "애교 진짜 많죠ㅎㅎ",
          "original_form": "애교 진짜 많죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028642.1.18",
          "form": "햄스터는 귀여운데 소통이 안 되어 아쉽더라고요",
          "original_form": "햄스터는 귀여운데 소통이 안 되어 아쉽더라고요",
          "speaker_id": "2",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028642.1.19",
          "form": "그랬군요ㅜㅜ",
          "original_form": "그랬군요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028642.1.20",
          "form": "돈이 정말 많이 들더라고요 열심히 벌어야겠어요",
          "original_form": "돈이 정말 많이 들더라고요 열심히 벌어야겠어요",
          "speaker_id": "1",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100028642.1.21",
          "form": "반려 동물은 한 번 다치면 치료비가 꽤 들어서 문제예요",
          "original_form": "반려 동물은 한 번 다치면 치료비가 꽤 들어서 문제예요",
          "speaker_id": "2",
