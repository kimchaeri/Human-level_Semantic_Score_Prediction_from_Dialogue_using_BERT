      "utterance": [
        {
          "id": "MDRW2100016916.1.1",
          "form": "고기좋지",
          "original_form": "고기좋지",
          "speaker_id": "1",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016916.1.2",
          "form": "크라이치즈버거맛있대",
          "original_form": "크라이치즈버거맛있대",
          "speaker_id": "1",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016916.1.3",
          "form": "그냥 코엑스몰 들어가서 머거도 되겠다",
          "original_form": "그냥 코엑스몰 들어가서 머거도 되겠다",
          "speaker_id": "1",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016916.1.4",
          "form": "찾아봣는데 삼성역엔 엉생이 없는듯..?점심 안먹는댓나 너",
          "original_form": "찾아봣는데 삼성역엔 엉생이 없는듯..?점심 안먹는댓나 너",
          "speaker_id": "1",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016916.1.5",
          "form": "크라이치즈버거는 어디꺼야?",
          "original_form": "크라이치즈버거는 어디꺼야?",
          "speaker_id": "2",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016916.1.6",
          "form": "엉 점심 안먹는댕",
          "original_form": "엉 점심 안먹는댕",
          "speaker_id": "2",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016916.1.7",
          "form": "아님 쉑쉑?",
          "original_form": "아님 쉑쉑?",
          "speaker_id": "2",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016916.1.8",
          "form": "그러면 점심 코엑스에서 먹고 저녁 엉생드쉴?^^",
          "original_form": "그러면 점심 코엑스에서 먹고 저녁 엉생드쉴?^^",
          "speaker_id": "1",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016916.1.9",
          "form": "방금 주제 머엿는지 ㄹㅇ생각안남",
          "original_form": "방금 주제 머엿는지 ㄹㅇ생각안남",
          "speaker_id": "2",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016916.1.10",
          "form": "크라이치즈버거는삼성역 버거맛집인가바",
          "original_form": "크라이치즈버거는삼성역 버거맛집인가바",
          "speaker_id": "1",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016916.1.11",
          "form": "주제관련 아닌거같은데 걍 피부과 키워드 입력함",
          "original_form": "주제관련 아닌거같은데 걍 피부과 키워드 입력함",
          "speaker_id": "2",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016916.1.12",
          "form": "다들 여기 엄청 추천하네",
          "original_form": "다들 여기 엄청 추천하네",
          "speaker_id": "1",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016916.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016916.1.14",
          "form": "인정안해주는건 아니겟지?",
          "original_form": "인정안해주는건 아니겟지?",
          "speaker_id": "2",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016916.1.15",
          "form": "코엑스몰 안에 훠궈집잇는데",
          "original_form": "코엑스몰 안에 훠궈집잇는데",
          "speaker_id": "1",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016916.1.16",
          "form": "호오 버거맛집",
          "original_form": "호오 버거맛집",
          "speaker_id": "2",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016916.1.17",
          "form": "저녁 엉생까지도 조와",
          "original_form": "저녁 엉생까지도 조와",
          "speaker_id": "2",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016916.1.18",
          "form": "그거 먹고싶었는데",
          "original_form": "그거 먹고싶었는데",
          "speaker_id": "1",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016916.1.19",
          "form": "훠궈...",
          "original_form": "훠궈...",
          "speaker_id": "1",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016916.1.20",
          "form": "는 너 안먹어봣지",
          "original_form": "는 너 안먹어봣지",
          "speaker_id": "1",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016916.1.21",
          "form": "훠궈는 어때",
          "original_form": "훠궈는 어때",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016916.1.22",
          "form": "새로운 도전과 엉생의 콜라보",
          "original_form": "새로운 도전과 엉생의 콜라보",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016916.1.23",
          "form": "념념긋",
          "original_form": "념념긋",
          "speaker_id": "2",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016916.1.24",
          "form": "훠궈 좋아해?",
          "original_form": "훠궈 좋아해?",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016916.1.25",
          "form": "머거보고싶어",
          "original_form": "머거보고싶어",
          "speaker_id": "1",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016916.1.26",
          "form": "아 안머거본겨?",
          "original_form": "아 안머거본겨?",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016916.1.27",
          "form": "버거..도 머거보고 싶긴한데",
          "original_form": "버거..도 머거보고 싶긴한데",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016916.1.28",
          "form": "가족이랑 먹어봄",
          "original_form": "가족이랑 먹어봄",
          "speaker_id": "1",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016916.1.29",
          "form": "저녁이 엉생이라믄",
          "original_form": "저녁이 엉생이라믄",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016916.1.30",
          "form": "점심은 훠궈",
          "original_form": "점심은 훠궈",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016916.1.31",
          "form": "끄덕",
          "original_form": "끄덕",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016916.1.32",
          "form": "와중에 파리바게트",
          "original_form": "와중에 파리바게트",
