      "utterance": [
        {
          "id": "MDRW2100037141.1.1",
          "form": "ㅋㅋ작은 양심이죵!",
          "original_form": "ㅋㅋ작은 양심이죵!",
          "speaker_id": "2",
          "time": "20210831 20:37"
        },
        {
          "id": "MDRW2100037141.1.2",
          "form": "먹을거 얘기하면 먹고싶어져요ㅋㅋㅋ",
          "original_form": "먹을거 얘기하면 먹고싶어져요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:37"
        },
        {
          "id": "MDRW2100037141.1.3",
          "form": "ㅋㅋㅋㅋㅋ저두에요",
          "original_form": "ㅋㅋㅋㅋㅋ저두에요",
          "speaker_id": "1",
          "time": "20210831 20:39"
        },
        {
          "id": "MDRW2100037141.1.4",
          "form": "커피는 조아하세요?",
          "original_form": "커피는 조아하세요?",
          "speaker_id": "1",
          "time": "20210831 20:39"
        },
        {
          "id": "MDRW2100037141.1.5",
          "form": "커피는 못마셔용ㅠ",
          "original_form": "커피는 못마셔용ㅠ",
          "speaker_id": "2",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100037141.1.6",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100037141.1.7",
          "form": "그렇군요 ㅠ 저는 좋아해요 매일마셔요 ㅋㅋㅋㅋ",
          "original_form": "그렇군요 ㅠ 저는 좋아해요 매일마셔요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:43"
        },
        {
          "id": "MDRW2100037141.1.8",
          "form": "카페인에 약하세요!?",
          "original_form": "카페인에 약하세요!?",
          "speaker_id": "1",
          "time": "20210831 20:43"
        },
        {
          "id": "MDRW2100037141.1.9",
          "form": "너무 써서 잘 안마셔용ㅋㅋ",
          "original_form": "너무 써서 잘 안마셔용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100037141.1.10",
          "form": "어떤커피 좋아하세요?",
          "original_form": "어떤커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100037141.1.11",
          "form": "아메리카노나 라떼 주로 먹어요!",
          "original_form": "아메리카노나 라떼 주로 먹어요!",
          "speaker_id": "1",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100037141.1.12",
          "form": "그럼 카페가면 티 종류 드세여?",
          "original_form": "그럼 카페가면 티 종류 드세여?",
          "speaker_id": "1",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100037141.1.13",
          "form": "티나 레몬에이드나 녹차라떼요!",
          "original_form": "티나 레몬에이드나 녹차라떼요!",
          "speaker_id": "2",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100037141.1.14",
          "form": "옹 그렇군요 녹차라떼 저두 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "옹 그렇군요 녹차라떼 저두 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100037141.1.15",
          "form": "혹시 민초 조아하세여?",
          "original_form": "혹시 민초 조아하세여?",
          "speaker_id": "1",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100037141.1.16",
          "form": "넹! 민트 돟아해요",
          "original_form": "넹! 민트 돟아해요",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037141.1.17",
          "form": "좋아하세요?!",
          "original_form": "좋아하세요?!",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037141.1.18",
          "form": "네 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100037141.1.19",
          "form": "배라가면 민초는 꼭 담아요",
          "original_form": "배라가면 민초는 꼭 담아요",
          "speaker_id": "1",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100037141.1.20",
          "form": "크으 저 녹차도 넣어용",
          "original_form": "크으 저 녹차도 넣어용",
          "speaker_id": "2",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100037141.1.21",
          "form": "맛나욤",
          "original_form": "맛나욤",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037141.1.22",
          "form": "베라 민초는 특별해요",
          "original_form": "베라 민초는 특별해요",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037141.1.23",
          "form": "저두 좋아해요 ㅋㅋㅋㅋ근데 배라 녹차는 안먹어본거같아요",
          "original_form": "저두 좋아해요 ㅋㅋㅋㅋ근데 배라 녹차는 안먹어본거같아요",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037141.1.24",
          "form": "그쵸 민초중에 배라가 진짜 최고",
          "original_form": "그쵸 민초중에 배라가 진짜 최고",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037141.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100037141.1.26",
          "form": "베라 안갈수가 없다니까요",
          "original_form": "베라 안갈수가 없다니까요",
          "speaker_id": "2",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100037141.1.27",
          "form": "저 맛 나는게 없어요",
          "original_form": "저 맛 나는게 없어요",
          "speaker_id": "2",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100037141.1.28",
          "form": "맞아요 ㅠㅠㅠ",
          "original_form": "맞아요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100037141.1.29",
          "form": "요즘 민초제품 많이나오던데",
          "original_form": "요즘 민초제품 많이나오던데",
          "speaker_id": "1",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100037141.1.30",
          "form": "막 엄청 맛잇는건 없더라구요",
          "original_form": "막 엄청 맛잇는건 없더라구요",
          "speaker_id": "1",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100037141.1.31",
          "form": "맞아요ㅠㅜ 많은데 먹을게 없어요ㅠ",
          "original_form": "맞아요ㅠㅜ 많은데 먹을게 없어요ㅠ",
          "speaker_id": "2",
          "time": "20210831 21:51"
        }
