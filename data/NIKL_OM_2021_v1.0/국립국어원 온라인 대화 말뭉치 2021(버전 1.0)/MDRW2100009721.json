      "utterance": [
        {
          "id": "MDRW2100009721.1.1",
          "form": "좋아하는 종목 있어?",
          "original_form": "좋아하는 종목 있어?",
          "speaker_id": "1",
          "time": "20210906 02:46"
        },
        {
          "id": "MDRW2100009721.1.2",
          "form": "나는 양궁!",
          "original_form": "나는 양궁!",
          "speaker_id": "1",
          "time": "20210906 02:46"
        },
        {
          "id": "MDRW2100009721.1.3",
          "form": "캬",
          "original_form": "캬",
          "speaker_id": "2",
          "time": "20210906 02:46"
        },
        {
          "id": "MDRW2100009721.1.4",
          "form": "양궁 너무 좋지 안산선수로 입덕함",
          "original_form": "양궁 너무 좋지{emoji:♡}{emoji:♡}{emoji:♡}{emoji:♡} 안산선수로 입덕함",
          "speaker_id": "2",
          "time": "20210906 02:46"
        },
        {
          "id": "MDRW2100009721.1.5",
          "form": "난 사격도 좋아해",
          "original_form": "난 사격도 좋아해",
          "speaker_id": "2",
          "time": "20210906 02:46"
        },
        {
          "id": "MDRW2100009721.1.6",
          "form": "헐 나도",
          "original_form": "헐 나도",
          "speaker_id": "1",
          "time": "20210906 02:46"
        },
        {
          "id": "MDRW2100009721.1.7",
          "form": "스케이트도 좋아",
          "original_form": "스케이트도 좋아",
          "speaker_id": "2",
          "time": "20210906 02:47"
        },
        {
          "id": "MDRW2100009721.1.8",
          "form": "우리 좀 잘 맞는다 ㅋ",
          "original_form": "우리 좀 잘 맞는다 ㅋ",
          "speaker_id": "2",
          "time": "20210906 02:47"
        },
        {
          "id": "MDRW2100009721.1.9",
          "form": "와 나 방금 좀 소름돋았어 좋아하는 종목이 드 똑같다ㅋㅋㅋ",
          "original_form": "와 나 방금 좀 소름돋았어 좋아하는 종목이 드 똑같다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 02:47"
        },
        {
          "id": "MDRW2100009721.1.10",
          "form": "드 뭐야 다ㅠ",
          "original_form": "드 뭐야 다ㅠ",
          "speaker_id": "1",
          "time": "20210906 02:47"
        },
        {
          "id": "MDRW2100009721.1.11",
          "form": "너 혹이 내 ae...?",
          "original_form": "너 혹이 내 ae...?",
          "speaker_id": "2",
          "time": "20210906 02:48"
        },
        {
          "id": "MDRW2100009721.1.12",
          "form": "익명은 나야 둘이 될 수 없어",
          "original_form": "익명은 나야 둘이 될 수 없어",
          "speaker_id": "2",
          "time": "20210906 02:48"
        },
        {
          "id": "MDRW2100009721.1.13",
          "form": "ㅋㅋㅋㅋ스케이트 타봤으?",
          "original_form": "ㅋㅋㅋㅋ스케이트 타봤으?",
          "speaker_id": "2",
          "time": "20210906 02:48"
        },
        {
          "id": "MDRW2100009721.1.14",
          "form": "예전에 타본 적 있어",
          "original_form": "예전에 타본 적 있어",
          "speaker_id": "1",
          "time": "20210906 02:50"
        },
        {
          "id": "MDRW2100009721.1.15",
          "form": "갓 태어난 새끼사슴마냥 발발대면서 기어다님ㅠ",
          "original_form": "갓 태어난 새끼사슴마냥 발발대면서 기어다님ㅠ",
          "speaker_id": "1",
          "time": "20210906 02:50"
        },
        {
          "id": "MDRW2100009721.1.16",
          "form": "재밌쥐",
          "original_form": "재밌쥐",
          "speaker_id": "2",
          "time": "20210906 02:50"
        },
        {
          "id": "MDRW2100009721.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ처음탈 땐 그렇지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ처음탈 땐 그렇지",
          "speaker_id": "2",
          "time": "20210906 02:50"
        },
        {
          "id": "MDRW2100009721.1.18",
          "form": "나도 갓태어난 기린처럼 벽 잡고 기었어",
          "original_form": "나도 갓태어난 기린처럼 벽 잡고 기었어",
          "speaker_id": "2",
          "time": "20210906 02:50"
        },
        {
          "id": "MDRW2100009721.1.19",
          "form": "경기로 볼 때는 쉬워보였는데 타니까 벽 잡고 빌빌대더라...",
          "original_form": "경기로 볼 때는 쉬워보였는데 타니까 벽 잡고 빌빌대더라...",
          "speaker_id": "1",
          "time": "20210906 02:50"
        },
        {
          "id": "MDRW2100009721.1.20",
          "form": "ㄹㅇ....특히 속도 안 나면...",
          "original_form": "ㄹㅇ....특히 속도 안 나면...",
          "speaker_id": "2",
          "time": "20210906 02:51"
        },
        {
          "id": "MDRW2100009721.1.21",
          "form": "난 달리는 것보다 가만히 서서 중심잡는 게 더 대단해보여",
          "original_form": "난 달리는 것보다 가만히 서서 중심잡는 게 더 대단해보여",
          "speaker_id": "2",
          "time": "20210906 02:51"
        },
        {
          "id": "MDRW2100009721.1.22",
          "form": "ㄹㅇㄹㅇ 어케 가만히 서 있을 수가 있지",
          "original_form": "ㄹㅇㄹㅇ 어케 가만히 서 있을 수가 있지",
          "speaker_id": "1",
          "time": "20210906 02:51"
        },
        {
          "id": "MDRW2100009721.1.23",
          "form": "나: 1초컷",
          "original_form": "나: 1초컷",
          "speaker_id": "2",
          "time": "20210906 02:51"
        },
        {
          "id": "MDRW2100009721.1.24",
          "form": "아 나도ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 나도ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 02:51"
        },
        {
          "id": "MDRW2100009721.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ자전거같아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ자전거같아",
          "speaker_id": "2",
          "time": "20210906 02:51"
        },
        {
          "id": "MDRW2100009721.1.26",
          "form": "움직이지 않는 순간 벌렁",
          "original_form": "움직이지 않는 순간 벌렁",
          "speaker_id": "2",
          "time": "20210906 02:52"
        },
        {
          "id": "MDRW2100009721.1.27",
          "form": "벽에서 손 떼고 있을 수 있다는 근자감이 생겨서 놓았더니 그대로 바닥에 쓰러짐",
          "original_form": "벽에서 손 떼고 있을 수 있다는 근자감이 생겨서 놓았더니 그대로 바닥에 쓰러짐",
          "speaker_id": "1",
          "time": "20210906 02:52"
        },
        {
          "id": "MDRW2100009721.1.28",
          "form": "선생님이 지나가다 쳐다보시는데 진짜 쪽팔렸다",
          "original_form": "선생님이 지나가다 쳐다보시는데 진짜 쪽팔렸다",
          "speaker_id": "1",
          "time": "20210906 02:52"
        },
        {
          "id": "MDRW2100009721.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋㄱㅋㄱㅋㄱㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋㄱㅋㄱㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 02:52"
        },
        {
          "id": "MDRW2100009721.1.30",
          "form": "학원까지 다닌 거?",
          "original_form": "학원까지 다닌 거?",
          "speaker_id": "2",
          "time": "20210906 02:53"
        },
        {
          "id": "MDRW2100009721.1.31",
          "form": "부럽당...",
          "original_form": "부럽당...",
          "speaker_id": "2",
          "time": "20210906 02:53"
        },
        {
          "id": "MDRW2100009721.1.32",
          "form": "아냐 학교에서 애들 랜덤으로 뽑아서 데려갔어",
          "original_form": "아냐 학교에서 애들 랜덤으로 뽑아서 데려갔어",
          "speaker_id": "1",
