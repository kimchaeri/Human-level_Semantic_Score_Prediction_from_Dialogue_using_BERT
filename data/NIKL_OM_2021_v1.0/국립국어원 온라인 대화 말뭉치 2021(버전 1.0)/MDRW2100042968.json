      "utterance": [
        {
          "id": "MDRW2100042968.1.1",
          "form": "와 오늘 갑자기 싸늘하네 ㅠ",
          "original_form": "와 오늘 갑자기 싸늘하네 ㅠ",
          "speaker_id": "1",
          "time": "20210907 12:14"
        },
        {
          "id": "MDRW2100042968.1.2",
          "form": "ㅇㅇ 약간 서늘해",
          "original_form": "ㅇㅇ 약간 서늘해",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042968.1.3",
          "form": "걸으면 딱 열나서 괜찮은?",
          "original_form": "걸으면 딱 열나서 괜찮은?",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042968.1.4",
          "form": "땀 많은 난 너무 싫군",
          "original_form": "땀 많은 난 너무 싫군",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042968.1.5",
          "form": "여긴 비와 ㅠ",
          "original_form": "여긴 비와 ㅠ",
          "speaker_id": "1",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042968.1.6",
          "form": "에궁..여긴 막 그쳤어",
          "original_form": "에궁..여긴 막 그쳤어",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042968.1.7",
          "form": "내내 오다가",
          "original_form": "내내 오다가",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042968.1.8",
          "form": "여긴 아직도",
          "original_form": "여긴 아직도",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042968.1.9",
          "form": "약하게 비오면서 바람 부는데",
          "original_form": "약하게 비오면서 바람 부는데",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042968.1.10",
          "form": "급 싸늘해졋다",
          "original_form": "급 싸늘해졋다",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042968.1.11",
          "form": "에구",
          "original_form": "에구",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042968.1.12",
          "form": "이대로면 우리 만날땐 니트 입어야할듯..",
          "original_form": "이대로면 우리 만날땐 니트 입어야할듯..",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042968.1.13",
          "form": "가디건같은거 이ㅔ 슬슬 챙길 날씨야",
          "original_form": "가디건같은거 이ㅔ 슬슬 챙길 날씨야",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042968.1.14",
          "form": "ㅋㅋ큐ㅠㅠ",
          "original_form": "ㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042968.1.15",
          "form": "ㅇㅇ 그런가봐...",
          "original_form": "ㅇㅇ 그런가봐...",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042968.1.16",
          "form": "여름이 그렇게 빨리 가진 안ㄹ을거 같은데",
          "original_form": "여름이 그렇게 빨리 가진 안ㄹ을거 같은데",
          "speaker_id": "2",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042968.1.17",
          "form": "얇은 옷에 가디건 챙겨서 만나야겠당..",
          "original_form": "얇은 옷에 가디건 챙겨서 만나야겠당..",
          "speaker_id": "2",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042968.1.18",
          "form": "ㅇㅇ 더위 많이 타면",
          "original_form": "ㅇㅇ 더위 많이 타면",
          "speaker_id": "1",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042968.1.19",
          "form": "그래야 할거 같아 라고 심심..아...",
          "original_form": "그래야 할거 같아 라고 심심..아...",
          "speaker_id": "2",
          "time": "20210907 12:22"
        },
        {
          "id": "MDRW2100042968.1.20",
          "form": "이게 어디가 나쁜말이니",
          "original_form": "이게 어디가 나쁜말이니",
          "speaker_id": "2",
          "time": "20210907 12:22"
        },
        {
          "id": "MDRW2100042968.1.21",
          "form": "나오니까 비 다시 오네",
          "original_form": "나오니까 비 다시 오네",
          "speaker_id": "2",
          "time": "20210907 12:22"
        },
        {
          "id": "MDRW2100042968.1.22",
          "form": "우산 챙겼지?",
          "original_form": "우산 챙겼지?",
          "speaker_id": "1",
          "time": "20210907 12:24"
        },
        {
          "id": "MDRW2100042968.1.23",
          "form": "ㅇㅇ 챙겼엉 오락가락하는 날씨라",
          "original_form": "ㅇㅇ 챙겼엉 오락가락하는 날씨라",
          "speaker_id": "2",
          "time": "20210907 12:26"
        },
        {
          "id": "MDRW2100042968.1.24",
          "form": "틔터에서 그랬지",
          "original_form": "틔터에서 그랬지",
          "speaker_id": "2",
          "time": "20210907 12:26"
        },
        {
          "id": "MDRW2100042968.1.25",
          "form": "영국신사라고",
          "original_form": "영국신사라고",
          "speaker_id": "2",
          "time": "20210907 12:27"
        },
        {
          "id": "MDRW2100042968.1.26",
          "form": "좀있으면 중절모도 쓰고 다닐 삘",
          "original_form": "좀있으면 중절모도 쓰고 다닐 삘",
          "speaker_id": "2",
          "time": "20210907 12:27"
        },
        {
          "id": "MDRW2100042968.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:28"
        },
        {
          "id": "MDRW2100042968.1.28",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210907 12:28"
        },
        {
          "id": "MDRW2100042968.1.29",
          "form": "나 김밥이랑 돈가스 먹엇는데",
          "original_form": "나 김밥이랑 돈가스 먹엇는데",
          "speaker_id": "1",
          "time": "20210907 12:28"
        },
        {
          "id": "MDRW2100042968.1.30",
          "form": "식당에서 돈가스만 결제함;;",
          "original_form": "식당에서 돈가스만 결제함;;",
          "speaker_id": "1",
          "time": "20210907 12:28"
        },
