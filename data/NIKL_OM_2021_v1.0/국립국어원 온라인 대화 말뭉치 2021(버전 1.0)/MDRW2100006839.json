      "utterance": [
        {
          "id": "MDRW2100006839.1.1",
          "form": "건강관리는 어떻게 하고 있나요?",
          "original_form": "건강관리는 어떻게 하고 있나요?",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100006839.1.2",
          "form": "걷기만 겨우해요ㅋ",
          "original_form": "걷기만 겨우해요ㅋ",
          "speaker_id": "2",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100006839.1.3",
          "form": "저도 걷기 운동 하고 있습니다",
          "original_form": "저도 걷기 운동 하고 있습니다",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100006839.1.4",
          "form": "날이좋아져서 다시 걸어야하는데 귀찮네요",
          "original_form": "날이좋아져서 다시 걸어야하는데 귀찮네요",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006839.1.5",
          "form": "아침에 산책나갔는데 춥더군요",
          "original_form": "아침에 산책나갔는데 춥더군요",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006839.1.6",
          "form": "오후는 걷기 좋을 듯하지만 아침엔 긴팔 챙겨야 하겠어요",
          "original_form": "오후는 걷기 좋을 듯하지만 아침엔 긴팔 챙겨야 하겠어요",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100006839.1.7",
          "form": "점심에 삼사십분 걷는데 여름엔덥고 요즘은 비온다고 안갓더니 하기싫네요",
          "original_form": "점심에 삼사십분 걷는데 여름엔덥고 요즘은 비온다고 안갓더니 하기싫네요",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100006839.1.8",
          "form": "이제는 점심에 걷기 좋을 거에요",
          "original_form": "이제는 점심에 걷기 좋을 거에요",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006839.1.9",
          "form": "네 날은 좋아졋는데 마음이 떳네요ㅋㅋ",
          "original_form": "네 날은 좋아졋는데 마음이 떳네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100006839.1.10",
          "form": "그래도 걷기라도 해야 건강관리가 돼죠",
          "original_form": "그래도 걷기라도 해야 건강관리가 돼죠",
          "speaker_id": "1",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100006839.1.11",
          "form": "맞아요 암것도 안하니 \n오늘은 나가볼까해요",
          "original_form": "맞아요 암것도 안하니 \n오늘은 나가볼까해요",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100006839.1.12",
          "form": "좋습니다",
          "original_form": "좋습니다",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100006839.1.13",
          "form": "걸으면서 비타민디도 합성하고 해야지요",
          "original_form": "걸으면서 비타민디도 합성하고 해야지요",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100006839.1.14",
          "form": "광합성 중요하네요ㅋㅋ",
          "original_form": "광합성 중요하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100006839.1.15",
          "form": "그렇지요 비타민 디가 면역력도 좋게 해준답니다",
          "original_form": "그렇지요 비타민 디가 면역력도 좋게 해준답니다",
          "speaker_id": "1",
          "time": "20210908 10:57"
        }
