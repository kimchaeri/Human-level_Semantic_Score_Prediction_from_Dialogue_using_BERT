      "utterance": [
        {
          "id": "MDRW2100014172.1.1",
          "form": "어떤 음식좋아하세요?",
          "original_form": "어떤 음식좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014172.1.2",
          "form": "어떤 음식좋아하시나요?",
          "original_form": "어떤 음식좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014172.1.3",
          "form": "요즘에는 쭈꾸미볶음 자주먹어요",
          "original_form": "요즘에는 쭈꾸미볶음 자주먹어요",
          "speaker_id": "2",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014172.1.4",
          "form": "매운거잘드시나요?",
          "original_form": "매운거잘드시나요?",
          "speaker_id": "1",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014172.1.5",
          "form": "아니요 맵찔이인데 숙주나물 많이넣고 마요네즈 찍어먹고 샐러드하고 먹어요",
          "original_form": "아니요 맵찔이인데 숙주나물 많이넣고 마요네즈 찍어먹고 샐러드하고 먹어요",
          "speaker_id": "2",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014172.1.6",
          "form": "어떤음식좋아하세요?",
          "original_form": "어떤음식좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014172.1.7",
          "form": "한식 양식 중식?  면류?  고기?",
          "original_form": "한식 양식 중식?  면류?  고기?",
          "speaker_id": "2",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100014172.1.8",
          "form": "저는 연어회가 지금 먹고싶어요",
          "original_form": "저는 연어회가 지금 먹고싶어요",
          "speaker_id": "1",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100014172.1.9",
          "form": "ㅎㅎㅎ  연어회좋죠",
          "original_form": "ㅎㅎㅎ  연어회좋죠",
          "speaker_id": "2",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100014172.1.10",
          "form": "한식은 다좋아요",
          "original_form": "한식은 다좋아요",
          "speaker_id": "1",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014172.1.11",
          "form": "매운거 잘 드세요?",
          "original_form": "매운거 잘 드세요?",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014172.1.12",
          "form": "저는좋아해요! 떡볶이 카레 이런거 제일 맵게먹는편이에요",
          "original_form": "저는좋아해요! 떡볶이 카레 이런거 제일 맵게먹는편이에요",
          "speaker_id": "1",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014172.1.13",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014172.1.14",
          "form": "원래 매운거 잘드셨어요?",
          "original_form": "원래 매운거 잘드셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014172.1.15",
          "form": "아니면 자주 먹음 더 잘먹을수있나요?",
          "original_form": "아니면 자주 먹음 더 잘먹을수있나요?",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014172.1.16",
          "form": "저는 중간맛으로먹어도 맵거든요ㅋㅋ",
          "original_form": "저는 중간맛으로먹어도 맵거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014172.1.17",
          "form": "자주먹으면 그렇게 되더라구요 ㅎㅎㅎ가족들이 매운걸좋아해서",
          "original_form": "자주먹으면 그렇게 되더라구요 ㅎㅎㅎ가족들이 매운걸좋아해서",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014172.1.18",
          "form": "그럼 더 시도해봐야겠어요!",
          "original_form": "그럼 더 시도해봐야겠어요!",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014172.1.19",
          "form": "오늘저녁은 뭐드세요?",
          "original_form": "오늘저녁은 뭐드세요?",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014172.1.20",
          "form": "오늘저녁엔 집에서 김치찌개를먹으려구요!",
          "original_form": "오늘저녁엔 집에서 김치찌개를먹으려구요!",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014172.1.21",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014172.1.22",
          "form": "저는 알밥 시켜먹을려구요",
          "original_form": "저는 알밥 시켜먹을려구요",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014172.1.23",
          "form": "알밥이 먹고싶어졌어요",
          "original_form": "알밥이 먹고싶어졌어요",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014172.1.24",
          "form": "알밥 맛있죠",
          "original_form": "알밥 맛있죠",
          "speaker_id": "1",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014172.1.25",
          "form": "돌솥알밥!",
          "original_form": "돌솥알밥!",
          "speaker_id": "1",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014172.1.26",
          "form": "김치찌개도 끌리네요",
          "original_form": "김치찌개도 끌리네요",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014172.1.27",
          "form": "김기루넣어서 먹음 맛나는데",
          "original_form": "김기루넣어서 먹음 맛나는데",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014172.1.28",
          "form": "김가루",
          "original_form": "김가루",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014172.1.29",
          "form": "맛잘알!! 맛있는 식사되세요:)",
          "original_form": "맛잘알!! 맛있는 식사되세요:)",
          "speaker_id": "1",
          "time": "20210825 18:46"
        }
      ]
    }
  ]
