      "utterance": [
        {
          "id": "MDRW2100009618.1.1",
          "form": "수영이랑 원예는 좋았어요",
          "original_form": "수영이랑 원예는 좋았어요",
          "speaker_id": "2",
          "time": "20210823 01:48"
        },
        {
          "id": "MDRW2100009618.1.2",
          "form": "아 요즘 코딩 배운다고 하더라구요",
          "original_form": "아 요즘 코딩 배운다고 하더라구요",
          "speaker_id": "1",
          "time": "20210823 01:48"
        },
        {
          "id": "MDRW2100009618.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 01:48"
        },
        {
          "id": "MDRW2100009618.1.4",
          "form": "코딩 어때요 어렵죠",
          "original_form": "코딩 어때요 어렵죠",
          "speaker_id": "1",
          "time": "20210823 01:48"
        },
        {
          "id": "MDRW2100009618.1.5",
          "form": "초등학교 필수 과목이라서",
          "original_form": "초등학교 필수 과목이라서",
          "speaker_id": "2",
          "time": "20210823 01:48"
        },
        {
          "id": "MDRW2100009618.1.6",
          "form": "C플 받고",
          "original_form": "C플 받고",
          "speaker_id": "2",
          "time": "20210823 01:48"
        },
        {
          "id": "MDRW2100009618.1.7",
          "form": "감사한 적은 처음이었어요",
          "original_form": "감사한 적은 처음이었어요",
          "speaker_id": "2",
          "time": "20210823 01:48"
        },
        {
          "id": "MDRW2100009618.1.8",
          "form": "약간 라떼는 이지만 우리때는 안배웠잖아요",
          "original_form": "약간 라떼는 이지만 우리때는 안배웠잖아요",
          "speaker_id": "1",
          "time": "20210823 01:49"
        },
        {
          "id": "MDRW2100009618.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:49"
        },
        {
          "id": "MDRW2100009618.1.10",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210823 01:49"
        },
        {
          "id": "MDRW2100009618.1.11",
          "form": "와 머리 깨지겠다",
          "original_form": "와 머리 깨지겠다",
          "speaker_id": "1",
          "time": "20210823 01:49"
        },
        {
          "id": "MDRW2100009618.1.12",
          "form": "진짜 대학 와서 갑자기 배우는데",
          "original_form": "진짜 대학 와서 갑자기 배우는데",
          "speaker_id": "2",
          "time": "20210823 01:49"
        },
        {
          "id": "MDRW2100009618.1.13",
          "form": "초등학교 4학년이 저보다 잘해요",
          "original_form": "초등학교 4학년이 저보다 잘해요",
          "speaker_id": "2",
          "time": "20210823 01:49"
        },
        {
          "id": "MDRW2100009618.1.14",
          "form": "코딩 이름만 들어도 어려워",
          "original_form": "코딩 이름만 들어도 어려워",
          "speaker_id": "1",
          "time": "20210823 01:49"
        },
        {
          "id": "MDRW2100009618.1.15",
          "form": "제가 너무 못해서",
          "original_form": "제가 너무 못해서",
          "speaker_id": "2",
          "time": "20210823 01:49"
        },
        {
          "id": "MDRW2100009618.1.16",
          "form": "ㅜㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 01:49"
        },
        {
          "id": "MDRW2100009618.1.17",
          "form": "저도 못해요 괜찮아요",
