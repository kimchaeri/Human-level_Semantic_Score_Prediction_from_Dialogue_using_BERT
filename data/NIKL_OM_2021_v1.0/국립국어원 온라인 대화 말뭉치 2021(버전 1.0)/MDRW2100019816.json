      "utterance": [
        {
          "id": "MDRW2100019816.1.1",
          "form": "그니까요 \r\n이런것도 운이 따라줘야 하나봐요",
          "original_form": "그니까요 \r\n이런것도 운이 따라줘야 하나봐요",
          "speaker_id": "2",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019816.1.2",
          "form": "맞아요 운동선수들이 그런부분이 많더라구요",
          "original_form": "맞아요 운동선수들이 그런부분이 많더라구요",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019816.1.3",
          "form": "김제덕 선수에게는  덕일수도 있죠",
          "original_form": "김제덕 선수에게는  덕일수도 있죠",
          "speaker_id": "2",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019816.1.4",
          "form": "그러네요 정말",
          "original_form": "그러네요 정말",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019816.1.5",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019816.1.6",
          "form": "축구는 안 보셨어요??",
          "original_form": "축구는 안 보셨어요??",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019816.1.7",
          "form": "축구는 하나밖에 안본거같은데",
          "original_form": "축구는 하나밖에 안본거같은데",
          "speaker_id": "2",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019816.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019816.1.9",
          "form": "어디랑 한거봤는지 기억이 안나네요",
          "original_form": "어디랑 한거봤는지 기억이 안나네요",
          "speaker_id": "2",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019816.1.10",
          "form": "근데 진거 봐서ㅠ 아쉬웠어요",
          "original_form": "근데 진거 봐서ㅠ 아쉬웠어요",
          "speaker_id": "2",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019816.1.11",
          "form": "흠 여러나라해서저도 어느나라랑 했었는지..",
          "original_form": "흠 여러나라해서저도 어느나라랑 했었는지..",
          "speaker_id": "1",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019816.1.12",
          "form": "아 ㅠㅠㅠ 그러면 첫경기아니면 마지막 경기네요",
          "original_form": "아 ㅠㅠㅠ 그러면 첫경기아니면 마지막 경기네요",
          "speaker_id": "1",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019816.1.13",
          "form": "감독이 전략을 잘못 짰다그러더라구요",
          "original_form": "감독이 전략을 잘못 짰다그러더라구요",
          "speaker_id": "2",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019816.1.14",
          "form": "마지막인가봐요ㅠ",
          "original_form": "마지막인가봐요ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019816.1.15",
          "form": "아그래요? ㅠㅠ 너무 아쉬워요",
          "original_form": "아그래요? ㅠㅠ 너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019816.1.16",
          "form": "상대가 잘하는 나라였는데",
          "original_form": "상대가 잘하는 나라였는데",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019816.1.17",
          "form": "마지막에 전 진거보다 더 볼경기가 없다는게 슬프더라고요",
          "original_form": "마지막에 전 진거보다 더 볼경기가 없다는게 슬프더라고요",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019816.1.18",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019816.1.19",
          "form": "다들 감독 욕하더라구요",
          "original_form": "다들 감독 욕하더라구요",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019816.1.20",
          "form": "마자요 더 보고싶어도 지면 못보니까요ㅠ",
          "original_form": "마자요 더 보고싶어도 지면 못보니까요ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019816.1.21",
          "form": "그래도 내년에",
          "original_form": "그래도 내년에",
          "speaker_id": "1",
          "time": "20210827 22:13"
        }
