      "utterance": [
        {
          "id": "MDRW2100046073.1.1",
          "form": "아 나 밖에 안나가서 멀라",
          "original_form": "아 나 밖에 안나가서 멀라",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046073.1.2",
          "form": "비가 오고 나니 춥네",
          "original_form": "비가 오고 나니 춥네",
          "speaker_id": "1",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046073.1.3",
          "form": "지금도 온다며??",
          "original_form": "지금도 온다며??",
          "speaker_id": "2",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046073.1.4",
          "form": "쌀쌀해 요즘 날씨",
          "original_form": "쌀쌀해 요즘 날씨",
          "speaker_id": "2",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046073.1.5",
          "form": "지금은 안오는것 같아",
          "original_form": "지금은 안오는것 같아",
          "speaker_id": "1",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046073.1.6",
          "form": "??온다곸ㅋㅋㅋ했자나",
          "original_form": "??온다곸ㅋㅋㅋ했자나",
          "speaker_id": "2",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046073.1.7",
          "form": "안온다그??",
          "original_form": "안온다그??",
          "speaker_id": "2",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046073.1.8",
          "form": "안오는게 좋긴하지",
          "original_form": "안오는게 좋긴하지",
          "speaker_id": "2",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046073.1.9",
          "form": "오후에 그친다고 일기예보에서 그랬어",
          "original_form": "오후에 그친다고 일기예보에서 그랬어",
          "speaker_id": "1",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046073.1.10",
          "form": "아아아 그친다구 이제?",
          "original_form": "아아아 그친다구 이제?",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046073.1.11",
          "form": "비 많이왔어",
          "original_form": "비 많이왔어",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046073.1.12",
          "form": "그치면 조케써",
          "original_form": "그치면 조케써",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046073.1.13",
          "form": "오늘 해는 없을것 같네",
          "original_form": "오늘 해는 없을것 같네",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046073.1.14",
          "form": "앗ㅋㅋㅋ그렇다구 해가 반가운건 아니긴햌ㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋ그렇다구 해가 반가운건 아니긴햌ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046073.1.15",
          "form": "왜 해가 있어야 우울증도 없다잖아",
          "original_form": "왜 해가 있어야 우울증도 없다잖아",
          "speaker_id": "1",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046073.1.16",
          "form": "응? 해있으면 더우니까",
          "original_form": "응? 해있으면 더우니까",
          "speaker_id": "2",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046073.1.17",
          "form": "땀나구",
          "original_form": "땀나구",
          "speaker_id": "2",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046073.1.18",
          "form": "요즘 많이 선선해 졌잖아",
          "original_form": "요즘 많이 선선해 졌잖아",
          "speaker_id": "1",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046073.1.19",
          "form": "ㅠㅠ그래두 난 구름이 좋앙",
          "original_form": "ㅠㅠ그래두 난 구름이 좋앙",
          "speaker_id": "2",
          "time": "20210908 10:22"
        }
