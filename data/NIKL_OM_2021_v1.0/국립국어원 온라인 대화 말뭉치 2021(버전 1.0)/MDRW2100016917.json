      "utterance": [
        {
          "id": "MDRW2100016917.1.1",
          "form": "저는 화이트하임 제일 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "저는 화이트하임 제일 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016917.1.2",
          "form": "어떤거 좋아하세요?",
          "original_form": "어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016917.1.3",
          "form": "저는 초코하임 좋아해요 ㅋㅋㅋ 다들 냉장고에 두고먹던데",
          "original_form": "저는 초코하임 좋아해요 ㅋㅋㅋ 다들 냉장고에 두고먹던데",
          "speaker_id": "1",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016917.1.4",
          "form": "저는 살짝 녹은거 좋아해요",
          "original_form": "저는 살짝 녹은거 좋아해요",
          "speaker_id": "1",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016917.1.5",
          "form": "아진짜요? 저는 무조건 얼려먹어욬ㅋㅋㅋㅋ",
          "original_form": "아진짜요? 저는 무조건 얼려먹어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016917.1.6",
          "form": "뭐든 시원하게먹으면 더 맛잇더라구욬ㅋㅋ",
          "original_form": "뭐든 시원하게먹으면 더 맛잇더라구욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016917.1.7",
          "form": "보통 다 차게 먹더라구요 ㅋㅋㅋ",
          "original_form": "보통 다 차게 먹더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016917.1.8",
          "form": "디저트나 빵도 다 얼먹해드시고",
          "original_form": "디저트나 빵도 다 얼먹해드시고",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016917.1.9",
          "form": "전 초코관련은 다 살짝 녹은거 조아해요 생초코처럼 ㅋㅋㅋ",
          "original_form": "전 초코관련은 다 살짝 녹은거 조아해요 생초코처럼 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016917.1.10",
          "form": "오 그렇구나ㅋㅋㅋㅋ",
          "original_form": "오 그렇구나ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016917.1.11",
          "form": "커피나 음료도 단거 좋아하세요?",
          "original_form": "커피나 음료도 단거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016917.1.12",
          "form": "단거 환장해요 없으면 못 살아요 ㅠㅠ",
          "original_form": "단거 환장해요 없으면 못 살아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016917.1.13",
          "form": "ㅋㅋㅋㅋㅋ그렇군요 커피는 그럼 바닐라라떼 좋아하시나요?",
          "original_form": "ㅋㅋㅋㅋㅋ그렇군요 커피는 그럼 바닐라라떼 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016917.1.14",
          "form": "오 맞아요 ㅋㅋㅋㅋ 그거 제일 좋아해요",
          "original_form": "오 맞아요 ㅋㅋㅋㅋ 그거 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016917.1.15",
          "form": "원래 모카좋아했는데 그거도 맛있더라구여",
          "original_form": "원래 모카좋아했는데 그거도 맛있더라구여",
          "speaker_id": "1",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016917.1.16",
          "form": "ㅋㅋㅋㅋㅋ모카가 초코소스 많이들어가서 그런가봐료",
          "original_form": "ㅋㅋㅋㅋㅋ모카가 초코소스 많이들어가서 그런가봐료",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016917.1.17",
          "form": "휘핑크림도 좋아하실거같아요",
          "original_form": "휘핑크림도 좋아하실거같아요",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016917.1.18",
          "form": "맞아요 초코 엄청 좋아해서요 ㅋㅋㅋ",
          "original_form": "맞아요 초코 엄청 좋아해서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016917.1.19",
          "form": "휘핑은 예전엔 좋아했는데 생크림은 또 안 좋아해서 안 먹어요 ㅋㅋㅋ",
          "original_form": "휘핑은 예전엔 좋아했는데 생크림은 또 안 좋아해서 안 먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016917.1.20",
          "form": "아 그렇군요 ㅋㅋㅋㅋ 그럼 아메리카노는 안드세여?",
          "original_form": "아 그렇군요 ㅋㅋㅋㅋ 그럼 아메리카노는 안드세여?",
          "speaker_id": "2",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016917.1.21",
          "form": "그거는 단거랑 먹으면 궁합이 좋더라구요 신세계였어요",
          "original_form": "그거는 단거랑 먹으면 궁합이 좋더라구요 신세계였어요",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016917.1.22",
          "form": "아 맞아요 ㅋㅋㅋㅋㅋ 단거 물릴때쯤 아메가 잡아줘요",
          "original_form": "아 맞아요 ㅋㅋㅋㅋㅋ 단거 물릴때쯤 아메가 잡아줘요",
          "speaker_id": "2",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100016917.1.23",
          "form": "저 지금도 도넛이랑 아메 먹고잇어욬ㅋㅋㅋㅋ",
