      "utterance": [
        {
          "id": "MDRW2100025656.1.1",
          "form": "배달음식은 자주 드세요?",
          "original_form": "배달음식은 자주 드세요?",
          "speaker_id": "1",
          "time": "20210901 15:26"
        },
        {
          "id": "MDRW2100025656.1.2",
          "form": "5대5로 먹는 것 같아요",
          "original_form": "5대5로 먹는 것 같아요",
          "speaker_id": "2",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100025656.1.3",
          "form": "할인 자주 해줘가지구요ㅋㅋㅋ",
          "original_form": "할인 자주 해줘가지구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100025656.1.4",
          "form": "요기요 주로 씁니다",
          "original_form": "요기요 주로 씁니다",
          "speaker_id": "2",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025656.1.5",
          "form": "주로 뭘 자주 시켜드세요?",
          "original_form": "주로 뭘 자주 시켜드세요?",
          "speaker_id": "1",
          "time": "20210901 15:45"
        },
        {
          "id": "MDRW2100025656.1.6",
          "form": "할인하는건 좌다 패스트푸드에요ㅠ",
          "original_form": "할인하는건 좌다 패스트푸드에요ㅠ",
          "speaker_id": "2",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025656.1.7",
          "form": "밥 땡기는 날이 있으면 힘들어도 해먹고요",
          "original_form": "밥 땡기는 날이 있으면 힘들어도 해먹고요",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025656.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ그렇죠 할인하는건 치킨 피자",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그렇죠 할인하는건 치킨 피자",
          "speaker_id": "1",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100025656.1.9",
          "form": "치킨파세요 피자파세요?",
          "original_form": "치킨파세요 피자파세요?",
          "speaker_id": "1",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100025656.1.10",
          "form": "안가립니다",
          "original_form": "안가립니다",
          "speaker_id": "2",
          "time": "20210901 15:59"
        },
        {
          "id": "MDRW2100025656.1.11",
          "form": "그럼 후라이드랑 양념 중에서는요? ㅋㅋ",
          "original_form": "그럼 후라이드랑 양념 중에서는요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025656.1.12",
          "form": "아.. 어릴 땐 양념파였는데",
          "original_form": "아.. 어릴 땐 양념파였는데",
          "speaker_id": "2",
          "time": "20210901 16:06"
        },
        {
          "id": "MDRW2100025656.1.13",
          "form": "크니 맥주엔 후라이드더라구요?",
          "original_form": "크니 맥주엔 후라이드더라구요?",
          "speaker_id": "2",
          "time": "20210901 16:06"
        },
        {
          "id": "MDRW2100025656.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:06"
        },
        {
          "id": "MDRW2100025656.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ저는 어릴때부터 강경한 후라이드파입니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저는 어릴때부터 강경한 후라이드파입니다",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025656.1.16",
          "form": "어느브랜드 후라이드를 제일 좋아하세요?",
          "original_form": "어느브랜드 후라이드를 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025656.1.17",
          "form": "ㅋㅋㅋㅋ강경한ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ강경한ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100025656.1.18",
          "form": "저 bhc랑",
          "original_form": "저 bhc랑",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100025656.1.19",
          "form": "노랑통닭",
          "original_form": "노랑통닭",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100025656.1.20",
          "form": "최근은 후참잘도 괜찮더라구요",
          "original_form": "최근은 후참잘도 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025656.1.21",
          "form": "저는 비비큐랑 후참잘 번갈아 먹어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 비비큐랑 후참잘 번갈아 먹어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100025656.1.22",
          "form": "네 후라이드 외길인생을 걸어왓어요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 후라이드 외길인생을 걸어왓어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100025656.1.23",
          "form": "ㅋㅋㅋㅋ저희동네 비비큐는 맛이 없어요..",
          "original_form": "ㅋㅋㅋㅋ저희동네 비비큐는 맛이 없어요..",
          "speaker_id": "2",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100025656.1.24",
          "form": "기름이 막 절어서와서",
          "original_form": "기름이 막 절어서와서",
          "speaker_id": "2",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100025656.1.25",
          "form": "안시킵니다 지금은 또 바뀌었을순 있지만",
          "original_form": "안시킵니다 지금은 또 바뀌었을순 있지만",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025656.1.26",
          "form": "프렌차이즈가 점바점이 심하죠 ㅠㅠ",
          "original_form": "프렌차이즈가 점바점이 심하죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025656.1.27",
          "form": "저희동네는 굽네가 그렇게 맛이 없어요 ㅠㅠ",
          "original_form": "저희동네는 굽네가 그렇게 맛이 없어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025656.1.28",
          "form": "세상에 굽네를 맛없게 구울수가 있나",
          "original_form": "세상에 굽네를 맛없게 구울수가 있나",
          "speaker_id": "2",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100025656.1.29",
          "form": "그쵸 ㅠㅠ 이사와서 먹어보고 얼마나 슬펏는지 ㅠㅠ",
          "original_form": "그쵸 ㅠㅠ 이사와서 먹어보고 얼마나 슬펏는지 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025656.1.30",
          "form": "그정도면,요식업에서 손 떼야하지 않을까... 생각합니다",
          "original_form": "그정도면,요식업에서 손 떼야하지 않을까... 생각합니다",
          "speaker_id": "2",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100025656.1.31",
