      "utterance": [
        {
          "id": "MDRW2100016197.1.1",
          "form": "최근 쇼핑하거나 사신거 있으신가요?",
          "original_form": "최근 쇼핑하거나 사신거 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100016197.1.2",
          "form": "회사에서 목결이형 냉풍기?",
          "original_form": "회사에서 목결이형 냉풍기?",
          "speaker_id": "1",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100016197.1.3",
          "form": "그거를 나눠졌거든요",
          "original_form": "그거를 나눠졌거든요",
          "speaker_id": "1",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100016197.1.4",
          "form": "어제",
          "original_form": "어제",
          "speaker_id": "1",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100016197.1.5",
          "form": "산건 아니지만 새로 생긴 물건",
          "original_form": "산건 아니지만 새로 생긴 물건",
          "speaker_id": "1",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100016197.1.6",
          "form": "저는 어제 상반기 셀프선물로 가방샀어요",
          "original_form": "저는 어제 상반기 셀프선물로 가방샀어요",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016197.1.7",
          "form": "기분이너무좋아요",
          "original_form": "기분이너무좋아요",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016197.1.8",
          "form": "좋은거 사셨어요?",
          "original_form": "좋은거 사셨어요?",
          "speaker_id": "1",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016197.1.9",
          "form": "ㅎㅎ막비싸진않지만 저한텐 귀하죵",
          "original_form": "ㅎㅎ막비싸진않지만 저한텐 귀하죵",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016197.1.10",
          "form": "일하는데 힘이나네요",
          "original_form": "일하는데 힘이나네요",
          "speaker_id": "2",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016197.1.11",
          "form": "행복감얻고 활력이되면",
          "original_form": "행복감얻고 활력이되면",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016197.1.12",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016197.1.13",
          "form": "위시리스트는 있으신가요?",
          "original_form": "위시리스트는 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016197.1.14",
          "form": "저는 딱히 물건을 소유하는건 좋아하지 않아서",
          "original_form": "저는 딱히 물건을 소유하는건 좋아하지 않아서",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016197.1.15",
          "form": "필요한것 위주로만 구입하거든요",
          "original_form": "필요한것 위주로만 구입하거든요",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016197.1.16",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016197.1.17",
          "form": "얼마전에 와이프가 원해세",
          "original_form": "얼마전에 와이프가 원해세",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016197.1.18",
          "form": "아이패드 사준적 있네요",
          "original_form": "아이패드 사준적 있네요",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016197.1.19",
          "form": "저는 결혼앞두고있어서",
          "original_form": "저는 결혼앞두고있어서",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016197.1.20",
          "form": "여러방면으로",
          "original_form": "여러방면으로",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016197.1.21",
          "form": "많이알아보고있어요",
          "original_form": "많이알아보고있어요",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016197.1.22",
          "form": "갤탭 구매예정 이에요",
          "original_form": "갤탭 구매예정 이에요",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016197.1.23",
          "form": "아 살것도 많고 준비할것도 많겠다",
          "original_form": "아 살것도 많고 준비할것도 많겠다",
          "speaker_id": "1",
          "time": "20210826 11:00"
        }
