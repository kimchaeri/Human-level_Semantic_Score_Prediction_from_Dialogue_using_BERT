      "utterance": [
        {
          "id": "MDRW2100010328.1.1",
          "form": "가고싶은 여행지는?",
          "original_form": "가고싶은 여행지는?",
          "speaker_id": "1",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010328.1.2",
          "form": "해외",
          "original_form": "해외",
          "speaker_id": "2",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010328.1.3",
          "form": "어느나라?",
          "original_form": "어느나라?",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010328.1.4",
          "form": "아시아 쪽에 갈거야",
          "original_form": "아시아 쪽에 갈거야",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010328.1.5",
          "form": "어디?",
          "original_form": "어디?",
          "speaker_id": "1",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010328.1.6",
          "form": "방콕 가고싶은 여행지는?",
          "original_form": "방콕 가고싶은 여행지는?",
          "speaker_id": "2",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010328.1.7",
          "form": "나도 태국가고싶어",
          "original_form": "나도 태국가고싶어",
          "speaker_id": "1",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010328.1.8",
          "form": "태국 어디?",
          "original_form": "태국 어디?",
          "speaker_id": "2",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010328.1.9",
          "form": "아무곳이나",
          "original_form": "아무곳이나",
          "speaker_id": "1",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010328.1.10",
          "form": "태국 음식 먹고싶어서",
          "original_form": "태국 음식 먹고싶어서",
          "speaker_id": "1",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010328.1.11",
          "form": "태국 방콕?",
          "original_form": "태국 방콕?",
          "speaker_id": "2",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010328.1.12",
          "form": "응 팟타이 좋아해",
          "original_form": "응 팟타이 좋아해",
          "speaker_id": "1",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010328.1.13",
          "form": "팟타이 좋아한다고?",
          "original_form": "팟타이 좋아한다고?",
          "speaker_id": "2",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010328.1.14",
          "form": "똠얌꿍도 먹어보고 싶어",
          "original_form": "똠얌꿍도 먹어보고 싶어",
          "speaker_id": "1",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100010328.1.15",
          "form": "팟타이 말고 다른거 좋아하는 음식이 있어?",
          "original_form": "팟타이 말고 다른거 좋아하는 음식이 있어?",
          "speaker_id": "2",
          "time": "20210823 19:50"
        },
        {
          "id": "MDRW2100010328.1.16",
          "form": "너는 왜 태국 가고 싶은데?",
          "original_form": "너는 왜 태국 가고 싶은데?",
          "speaker_id": "1",
          "time": "20210823 19:50"
        },
        {
          "id": "MDRW2100010328.1.17",
          "form": "풋팟뽕커리고 좋아해",
          "original_form": "풋팟뽕커리고 좋아해",
          "speaker_id": "1",
          "time": "20210823 19:50"
        },
        {
          "id": "MDRW2100010328.1.18",
          "form": "나도 태국 음식이 너무 먹고싶어서",
          "original_form": "나도 태국 음식이 너무 먹고싶어서",
          "speaker_id": "2",
          "time": "20210823 19:50"
        }
      ]
