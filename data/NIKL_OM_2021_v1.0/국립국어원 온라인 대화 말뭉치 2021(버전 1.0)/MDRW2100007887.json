      "utterance": [
        {
          "id": "MDRW2100007887.1.1",
          "form": "후훗 이건 어떤가요 (?",
          "original_form": "후훗 이건 어떤가요 (?",
          "speaker_id": "1",
          "time": "20210822 11:36"
        },
        {
          "id": "MDRW2100007887.1.2",
          "form": "점심시간이라.......",
          "original_form": "점심시간이라.......",
          "speaker_id": "1",
          "time": "20210822 11:36"
        },
        {
          "id": "MDRW2100007887.1.3",
          "form": "잠시만요옥",
          "original_form": "잠시만요옥",
          "speaker_id": "1",
          "time": "20210822 11:36"
        },
        {
          "id": "MDRW2100007887.1.4",
          "form": "넵 다녀오십셔",
          "original_form": "넵 다녀오십셔",
          "speaker_id": "2",
          "time": "20210822 11:37"
        },
        {
          "id": "MDRW2100007887.1.5",
          "form": "제가돌아왔네요...(이난리)",
          "original_form": "제가돌아왔네요...(이난리)",
          "speaker_id": "1",
          "time": "20210822 11:50"
        },
        {
          "id": "MDRW2100007887.1.6",
          "form": "삼양라면이 60주년이래요 글쎄~ 대체 몇살인지",
          "original_form": "삼양라면이 60주년이래요 글쎄~ 대체 몇살인지",
          "speaker_id": "1",
          "time": "20210822 11:51"
        },
        {
          "id": "MDRW2100007887.1.7",
          "form": "솔직히 라면이 살은 찌지만 칼로리는 그렇게 안 높은 것 같고 그래요...",
          "original_form": "솔직히 라면이 살은 찌지만 칼로리는 그렇게 안 높은 것 같고 그래요...",
          "speaker_id": "1",
          "time": "20210822 11:54"
        },
        {
          "id": "MDRW2100007887.1.8",
          "form": "라면은 딱 한끼 칼로리죠",
          "original_form": "라면은 딱 한끼 칼로리죠",
          "speaker_id": "2",
          "time": "20210822 11:55"
        },
        {
          "id": "MDRW2100007887.1.9",
          "form": "다만 여러가지를 넣어먹어서",
          "original_form": "다만 여러가지를 넣어먹어서",
          "speaker_id": "2",
          "time": "20210822 11:55"
        },
        {
          "id": "MDRW2100007887.1.10",
          "form": "그 칼로리에서 안끝난다는",
          "original_form": "그 칼로리에서 안끝난다는",
          "speaker_id": "2",
          "time": "20210822 11:55"
        },
        {
          "id": "MDRW2100007887.1.11",
          "form": "맙소사 계란 넣은걸요(?",
          "original_form": "맙소사 계란 넣은걸요(?",
          "speaker_id": "1",
          "time": "20210822 11:58"
        },
        {
          "id": "MDRW2100007887.1.12",
          "form": "계란은 몸에 좋으니까 괜찮을거예요",
          "original_form": "계란은 몸에 좋으니까 괜찮을거예요",
          "speaker_id": "2",
          "time": "20210822 11:58"
        },
        {
          "id": "MDRW2100007887.1.13",
          "form": "|완전식품|",
          "original_form": "|완전식품|",
          "speaker_id": "1",
          "time": "20210822 12:00"
        },
        {
          "id": "MDRW2100007887.1.14",
          "form": "요즘엔 라면 자주 안 먹지만 참 좋아요... 라면... 그보다 이거 식음료인데 예시에 맛집, 유명 카페 있는 거 웃겨요",
          "original_form": "요즘엔 라면 자주 안 먹지만 참 좋아요... 라면... 그보다 이거 식음료인데 예시에 맛집, 유명 카페 있는 거 웃겨요",
          "speaker_id": "1",
          "time": "20210822 12:00"
        },
        {
          "id": "MDRW2100007887.1.15",
          "form": "맛집 유명카페가 있으면 뭐하는지...",
          "original_form": "맛집 유명카페가 있으면 뭐하는지...",
          "speaker_id": "2",
          "time": "20210822 12:01"
        },
        {
          "id": "MDRW2100007887.1.16",
          "form": "코로나 시국에 입맛 돋구는 것도 아니고",
          "original_form": "코로나 시국에 입맛 돋구는 것도 아니고",
          "speaker_id": "2",
          "time": "20210822 12:01"
        },
        {
          "id": "MDRW2100007887.1.17",
          "form": "ㅋㅋㅋ ㅋㅋㅋ 솔직히 그런집들은 바가지도 많고 허위광고도 많구...",
          "original_form": "ㅋㅋㅋ ㅋㅋㅋ 솔직히 그런집들은 바가지도 많고 허위광고도 많구...",
          "speaker_id": "1",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007887.1.18",
          "form": "줄이 긴 게 제일 싫어요~~~ 그냥 체인점을 가겠어",
          "original_form": "줄이 긴 게 제일 싫어요~~~ 그냥 체인점을 가겠어",
          "speaker_id": "1",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007887.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007887.1.20",
          "form": "기다리면서까지 먹을 가치가 있을까요",
          "original_form": "기다리면서까지 먹을 가치가 있을까요",
          "speaker_id": "2",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007887.1.21",
          "form": "시간도 아깝고",
          "original_form": "시간도 아깝고",
          "speaker_id": "2",
          "time": "20210822 12:02"
        }
      ]
