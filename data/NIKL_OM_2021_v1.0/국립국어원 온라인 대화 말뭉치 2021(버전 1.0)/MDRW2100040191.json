      "utterance": [
        {
          "id": "MDRW2100040191.1.1",
          "form": "넹 친구랑 먹었어요",
          "original_form": "넹 친구랑 먹었어요",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100040191.1.2",
          "form": "배부르세요?",
          "original_form": "배부르세요?",
          "speaker_id": "1",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100040191.1.3",
          "form": "네 근데 디저트 먹고싶어요",
          "original_form": "네 근데 디저트 먹고싶어요",
          "speaker_id": "2",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100040191.1.4",
          "form": "전 파운드머거요",
          "original_form": "전 파운드머거요",
          "speaker_id": "1",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100040191.1.5",
          "form": "어머 저도...말차 파운드 먹을거에요",
          "original_form": "어머 저도...말차 파운드 먹을거에요",
          "speaker_id": "2",
          "time": "20210904 13:58"
        },
        {
          "id": "MDRW2100040191.1.6",
          "form": "대박 저두요 말차이요",
          "original_form": "대박 저두요 말차이요",
          "speaker_id": "1",
          "time": "20210904 13:59"
        },
        {
          "id": "MDRW2100040191.1.7",
          "form": "음룐요",
          "original_form": "음룐요",
          "speaker_id": "1",
          "time": "20210904 13:59"
        },
        {
          "id": "MDRW2100040191.1.8",
          "form": "라떼여 라떼좋아하세요?",
          "original_form": "라떼여 라떼좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100040191.1.9",
          "form": "아주조아하죠",
          "original_form": "아주조아하죠",
          "speaker_id": "1",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100040191.1.10",
          "form": "꼰대답게",
          "original_form": "꼰대답게",
          "speaker_id": "1",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100040191.1.11",
          "form": "산미조아해요?",
          "original_form": "산미조아해요?",
          "speaker_id": "1",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100040191.1.12",
          "form": "저는 고소한게 더 좋아요",
          "original_form": "저는 고소한게 더 좋아요",
          "speaker_id": "2",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100040191.1.13",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "2",
          "time": "20210904 14:12"
        },
        {
          "id": "MDRW2100040191.1.14",
          "form": "산미요",
          "original_form": "산미요",
          "speaker_id": "1",
          "time": "20210904 14:12"
        },
        {
          "id": "MDRW2100040191.1.15",
          "form": "고소....좀 촌스러워서",
          "original_form": "고소....좀 촌스러워서",
          "speaker_id": "1",
          "time": "20210904 14:12"
        },
        {
          "id": "MDRW2100040191.1.16",
          "form": "입맛이 아주 고오급이신가봐요?",
          "original_form": "입맛이 아주 고오급이신가봐요?",
          "speaker_id": "2",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100040191.1.17",
          "form": "좀 그렇죠",
          "original_form": "좀 그렇죠",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100040191.1.18",
          "form": "하 참 잘나셨네요!",
          "original_form": "하 참 잘나셨네요!",
          "speaker_id": "2",
          "time": "20210904 14:19"
        }
      ]
