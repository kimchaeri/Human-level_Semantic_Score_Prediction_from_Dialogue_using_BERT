      "utterance": [
        {
          "id": "MDRW2100036476.1.1",
          "form": "챙겨드시는 영양제는 있으신가용!",
          "original_form": "챙겨드시는 영양제는 있으신가용!",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036476.1.2",
          "form": "저 유산균이랑 종합비타민 챙겨 먹고있어요",
          "original_form": "저 유산균이랑 종합비타민 챙겨 먹고있어요",
          "speaker_id": "2",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036476.1.3",
          "form": "챙겨 드시나요?",
          "original_form": "챙겨 드시나요?",
          "speaker_id": "2",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036476.1.4",
          "form": "전 유산균이요!",
          "original_form": "전 유산균이요!",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036476.1.5",
          "form": "다른것도 더 있는데 기억이..잘 안나네요ㅠ",
          "original_form": "다른것도 더 있는데 기억이..잘 안나네요ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036476.1.6",
          "form": "이것저것 맞춰 먹어야한다는데 검사 받을 틈이 안나네요ㅠ",
          "original_form": "이것저것 맞춰 먹어야한다는데 검사 받을 틈이 안나네요ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100036476.1.7",
          "form": "쉽지않죠ㅜㅠ",
          "original_form": "쉽지않죠ㅜㅠ",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036476.1.8",
          "form": "약도 먹는 시간이 있다그러구요ㅠ",
          "original_form": "약도 먹는 시간이 있다그러구요ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036476.1.9",
          "form": "아 그 얘기 들었어요!",
          "original_form": "아 그 얘기 들었어요!",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036476.1.10",
          "form": "근데 너무 귀찮은 관계로 그냥 한번에 먹어요 ㅎ",
          "original_form": "근데 너무 귀찮은 관계로 그냥 한번에 먹어요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036476.1.11",
          "form": "저두요ㅋㅋ",
          "original_form": "저두요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036476.1.12",
          "form": "유산균만 공복에 먹으려하고 다른거는 뭐..ㅋㅋ",
          "original_form": "유산균만 공복에 먹으려하고 다른거는 뭐..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036476.1.13",
          "form": "영양제 찾아 먹는 것도 일인것 같아요",
          "original_form": "영양제 찾아 먹는 것도 일인것 같아요",
          "speaker_id": "2",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100036476.1.14",
          "form": "이것저것 종류별로 효능 찾아 챙겨 먹고 은근히 번거로워서.. ㅎ",
          "original_form": "이것저것 종류별로 효능 찾아 챙겨 먹고 은근히 번거로워서.. ㅎ",
          "speaker_id": "2",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100036476.1.15",
          "form": "진짜요!",
          "original_form": "진짜요!",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100036476.1.16",
          "form": "종합영양제 하나 나와줌 좋겠어요",
          "original_form": "종합영양제 하나 나와줌 좋겠어요",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100036476.1.17",
          "form": "맞춤으로 찾는게 어려운 것 같아요",
          "original_form": "맞춤으로 찾는게 어려운 것 같아요",
          "speaker_id": "2",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100036476.1.18",
          "form": "병원 가서 검사 받으려해도 시간이 너무 걸리고ㅠ",
          "original_form": "병원 가서 검사 받으려해도 시간이 너무 걸리고ㅠ",
          "speaker_id": "2",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100036476.1.19",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036476.1.20",
          "form": "영양제도 은근 비싸잖아요?!",
          "original_form": "영양제도 은근 비싸잖아요?!",
          "speaker_id": "1",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036476.1.21",
          "form": "맞아요 좋은 거 찾다보니 끝도 없어요 ㅎ",
          "original_form": "맞아요 좋은 거 찾다보니 끝도 없어요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 15:34"
        },
        {
          "id": "MDRW2100036476.1.22",
          "form": "그니까요!",
          "original_form": "그니까요!",
          "speaker_id": "1",
          "time": "20210831 16:27"
        }
      ]
