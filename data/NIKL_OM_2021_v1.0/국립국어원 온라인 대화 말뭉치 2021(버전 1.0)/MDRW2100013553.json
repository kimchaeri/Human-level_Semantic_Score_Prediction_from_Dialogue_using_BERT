      "utterance": [
        {
          "id": "MDRW2100013553.1.1",
          "form": "배달음식 자주 시키시나요?",
          "original_form": "배달음식 자주 시키시나요?",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013553.1.2",
          "form": "가끔요 name1님은요?",
          "original_form": "가끔요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013553.1.3",
          "form": "저도 가끔시켜요",
          "original_form": "저도 가끔시켜요",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013553.1.4",
          "form": "치킨이 좋더라고요",
          "original_form": "치킨이 좋더라고요",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013553.1.5",
          "form": "어떤 브랜드 치킨 좋아하세요?",
          "original_form": "어떤 브랜드 치킨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013553.1.6",
          "form": "저는 치킨매니아나 bhc 시켜요",
          "original_form": "저는 치킨매니아나 bhc 시켜요",
          "speaker_id": "1",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013553.1.7",
          "form": "뿌링클 맛있더라고요",
          "original_form": "뿌링클 맛있더라고요",
          "speaker_id": "1",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013553.1.8",
          "form": "오 짭잘한거 좋아하시나봐요",
          "original_form": "오 짭잘한거 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013553.1.9",
          "form": "단걸 좋아해요 ㅋㅋ  name2님은 뭘 좋아하세요?",
          "original_form": "단걸 좋아해요 ㅋㅋ  &name2&님은 뭘 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013553.1.10",
          "form": "저는 간장치킨이요",
          "original_form": "저는 간장치킨이요",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013553.1.11",
          "form": "오 어디브랜드요?",
          "original_form": "오 어디브랜드요?",
          "speaker_id": "1",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013553.1.12",
          "form": "60계나 교촌이요",
          "original_form": "60계나 교촌이요",
          "speaker_id": "2",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013553.1.13",
          "form": "60계 맛있죠 호랑이 치킨 맛있더라고요",
          "original_form": "60계 맛있죠 호랑이 치킨 맛있더라고요",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013553.1.14",
          "form": "호랑이치킨 어떤 맛인가요?",
          "original_form": "호랑이치킨 어떤 맛인가요?",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013553.1.15",
          "form": "되게 짜고 달아서 ㅂㄹ인거같아요",
          "original_form": "되게 짜고 달아서 ㅂㄹ인거같아요",
