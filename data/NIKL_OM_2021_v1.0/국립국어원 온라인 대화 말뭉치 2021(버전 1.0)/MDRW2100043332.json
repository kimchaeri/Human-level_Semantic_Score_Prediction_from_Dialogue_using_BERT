      "utterance": [
        {
          "id": "MDRW2100043332.1.1",
          "form": "대구에서 쓰는 거요 ㅋㅋㅋㅋ",
          "original_form": "대구에서 쓰는 거요 ㅋㅋㅋㅋ",
