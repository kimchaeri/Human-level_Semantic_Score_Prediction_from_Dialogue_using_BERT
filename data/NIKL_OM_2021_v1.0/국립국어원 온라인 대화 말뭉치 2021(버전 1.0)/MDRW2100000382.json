        },
        {
          "id": "MDRW2100000382.1.3",
          "form": "군대는 다녀오셨어요?",
          "original_form": "군대는 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210819 15:47"
        },
        {
          "id": "MDRW2100000382.1.4",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210819 15:47"
        },
        {
          "id": "MDRW2100000382.1.5",
          "form": "군대엔 갈 일이 없어서용",
          "original_form": "군대엔 갈 일이 없어서용",
          "speaker_id": "2",
          "time": "20210819 15:47"
        },
        {
          "id": "MDRW2100000382.1.6",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210819 15:48"
        },
        {
          "id": "MDRW2100000382.1.7",
          "form": "군대갔다오셨어요?",
          "original_form": "군대갔다오셨어요?",
          "speaker_id": "2",
          "time": "20210819 15:48"
        },
        {
          "id": "MDRW2100000382.1.8",
          "form": "전 공군 다녀 왔어요",
          "original_form": "전 공군 다녀 왔어요",
          "speaker_id": "1",
          "time": "20210819 15:48"
        },
        {
          "id": "MDRW2100000382.1.9",
          "form": "제가 안 다녀와서 잘 모르겠는데",
          "original_form": "제가 안 다녀와서 잘 모르겠는데",
          "speaker_id": "2",
          "time": "20210819 15:48"
        },
        {
          "id": "MDRW2100000382.1.10",
          "form": "군생활이 유익했나요?",
          "original_form": "군생활이 유익했나요?",
          "speaker_id": "2",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000382.1.11",
          "form": "규칙적인 생활은 좋지만",
          "original_form": "규칙적인 생활은 좋지만",
          "speaker_id": "1",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000382.1.12",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000382.1.13",
          "form": "아무래도 2번은 가기 싫은 곳이죠",
          "original_form": "아무래도 2번은 가기 싫은 곳이죠",
          "speaker_id": "1",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000382.1.14",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000382.1.15",
          "form": "공군은 자의로 가신거예요?",
          "original_form": "공군은 자의로 가신거예요?",
          "speaker_id": "2",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000382.1.16",
          "form": "네 친구랑 같이 지원 했죠",
          "original_form": "네 친구랑 같이 지원 했죠",
          "speaker_id": "1",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000382.1.17",
          "form": "동반입대셨구나",
          "original_form": "동반입대셨구나",
          "speaker_id": "2",
          "time": "20210819 15:51"
        },
        {
          "id": "MDRW2100000382.1.18",
          "form": "동반입대였지만 자대는 달랐너요",
          "original_form": "동반입대였지만 자대는 달랐너요",
          "speaker_id": "1",
          "time": "20210819 15:52"
        },
        {
          "id": "MDRW2100000382.1.19",
          "form": "아쉬었겠네요",
          "original_form": "아쉬었겠네요",
          "speaker_id": "2",
          "time": "20210819 15:52"
        },
        {
          "id": "MDRW2100000382.1.20",
          "form": "그 전투기는 타보셨어요??",
          "original_form": "그 전투기는 타보셨어요??",
          "speaker_id": "2",
          "time": "20210819 15:52"
        },
        {
          "id": "MDRW2100000382.1.21",
          "form": "전투기는 조종사만 타고 저희는 근처도 못가요",
