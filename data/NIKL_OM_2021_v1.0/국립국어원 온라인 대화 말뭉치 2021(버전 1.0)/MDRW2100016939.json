      "utterance": [
        {
          "id": "MDRW2100016939.1.1",
          "form": "어제 매화주 마셨는데",
          "original_form": "어제 매화주 마셨는데",
          "speaker_id": "2",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100016939.1.2",
          "form": "맛있더라",
          "original_form": "맛있더라",
          "speaker_id": "2",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100016939.1.3",
          "form": "근데 끝맛 씀",
          "original_form": "근데 끝맛 씀",
          "speaker_id": "2",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100016939.1.4",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210826 16:40"
        },
        {
          "id": "MDRW2100016939.1.5",
          "form": "그거 병에 꽃 그려진 술 맞나",
          "original_form": "그거 병에 꽃 그려진 술 맞나",
          "speaker_id": "1",
          "time": "20210826 16:40"
        },
        {
          "id": "MDRW2100016939.1.6",
          "form": "아마도?",
          "original_form": "아마도?",
          "speaker_id": "2",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016939.1.7",
          "form": "앵간 괜찮더아",
          "original_form": "앵간 괜찮더아",
          "speaker_id": "2",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016939.1.8",
          "form": "소주랑 비슷해?",
          "original_form": "소주랑 비슷해?",
          "speaker_id": "1",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100016939.1.9",
          "form": "메실주 같아",
          "original_form": "메실주 같아",
          "speaker_id": "2",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100016939.1.10",
          "form": "근데 좀더 끝맛이 쓴 느낌?",
          "original_form": "근데 좀더 끝맛이 쓴 느낌?",
          "speaker_id": "2",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100016939.1.11",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100016939.1.12",
          "form": "저녁 뭐 먹을 거야?",
          "original_form": "저녁 뭐 먹을 거야?",
          "speaker_id": "1",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100016939.1.13",
          "form": "나 엽떡",
          "original_form": "나 엽떡",
          "speaker_id": "2",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100016939.1.14",
          "form": "점심에 먹고 남은거",
          "original_form": "점심에 먹고 남은거",
          "speaker_id": "2",
          "time": "20210826 16:50"
        },
        {
          "id": "MDRW2100016939.1.15",
          "form": "헐랭 맛있겠다",
          "original_form": "헐랭 맛있겠다",
          "speaker_id": "1",
          "time": "20210826 16:50"
        },
        {
          "id": "MDRW2100016939.1.16",
          "form": "쿨피스랑 먹어야징",
          "original_form": "쿨피스랑 먹어야징",
          "speaker_id": "2",
          "time": "20210826 16:51"
        },
        {
          "id": "MDRW2100016939.1.17",
          "form": "나 떡볶이 못 먹은지 2달은 된 듯...",
          "original_form": "나 떡볶이 못 먹은지 2달은 된 듯...",
          "speaker_id": "1",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100016939.1.18",
          "form": "오유....",
          "original_form": "오유....",
          "speaker_id": "2",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100016939.1.19",
          "form": "나.. 마라탕도 먹고싶어..",
          "original_form": "나.. 마라탕도 먹고싶어..",
          "speaker_id": "2",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100016939.1.20",
          "form": "옥수수면",
          "original_form": "옥수수면",
          "speaker_id": "2",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100016939.1.21",
          "form": "중당...",
          "original_form": "중당...",
          "speaker_id": "2",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100016939.1.22",
          "form": "나 마라탕은 어제 먹음ㅋㅋ",
          "original_form": "나 마라탕은 어제 먹음ㅋㅋ",
