      "utterance": [
        {
          "id": "MDRW2100010627.1.1",
          "form": "기억에 남는 여행지 있나요",
          "original_form": "기억에 남는 여행지 있나요",
          "speaker_id": "1",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100010627.1.2",
          "form": "전 제주도 작년에 좋았어요",
          "original_form": "전 제주도 작년에 좋았어요",
          "speaker_id": "2",
          "time": "20210823 20:43"
        },
        {
          "id": "MDRW2100010627.1.3",
          "form": "애들이 좀 커서 가서 그런가",
          "original_form": "애들이 좀 커서 가서 그런가",
          "speaker_id": "2",
          "time": "20210823 20:43"
        },
        {
          "id": "MDRW2100010627.1.4",
          "form": "잘 다니고 좋았어요",
          "original_form": "잘 다니고 좋았어요",
          "speaker_id": "2",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100010627.1.5",
          "form": "아 그랬군요 어디가 좋으셨나요!!",
          "original_form": "아 그랬군요 어디가 좋으셨나요!!",
          "speaker_id": "1",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100010627.1.6",
          "form": "일부러 유명한데 안가고",
          "original_form": "일부러 유명한데 안가고",
          "speaker_id": "2",
          "time": "20210823 20:46"
        },
        {
          "id": "MDRW2100010627.1.7",
          "form": "코로나라 발닿는데로 갔어요",
          "original_form": "코로나라 발닿는데로 갔어요",
          "speaker_id": "2",
          "time": "20210823 20:46"
        },
        {
          "id": "MDRW2100010627.1.8",
          "form": "애들이 기억나는곳이 ㅋㅋ",
          "original_form": "애들이 기억나는곳이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:46"
        },
        {
          "id": "MDRW2100010627.1.9",
          "form": "애월리 자깐들른 바닷가",
          "original_form": "애월리 자깐들른 바닷가",
          "speaker_id": "2",
          "time": "20210823 20:46"
        },
        {
          "id": "MDRW2100010627.1.10",
          "form": "잠깐들른",
          "original_form": "잠깐들른",
          "speaker_id": "2",
          "time": "20210823 20:46"
        },
        {
          "id": "MDRW2100010627.1.11",
          "form": "소라같은거 주운 바다래요",
          "original_form": "소라같은거 주운 바다래요",
          "speaker_id": "2",
          "time": "20210823 20:46"
        },
        {
          "id": "MDRW2100010627.1.12",
          "form": "그냥 같이 코로나 피해서 조용하게 놀다온게 좋았던거 같아요",
          "original_form": "그냥 같이 코로나 피해서 조용하게 놀다온게 좋았던거 같아요",
          "speaker_id": "2",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100010627.1.13",
          "form": "어디가 좋으셨어요?",
          "original_form": "어디가 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100010627.1.14",
          "form": "최근 여행한곳 있으신가요?",
          "original_form": "최근 여행한곳 있으신가요?",
          "speaker_id": "2",
          "time": "20210823 20:49"
        },
        {
          "id": "MDRW2100010627.1.15",
          "form": "저도 올해 제주도 다녀왔었는데 참 좋았어요 함덕이랑 애월 협재 이렇게 가봤네요^^",
          "original_form": "저도 올해 제주도 다녀왔었는데 참 좋았어요 함덕이랑 애월 협재 이렇게 가봤네요^^",
          "speaker_id": "1",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010627.1.16",
          "form": "오 좋은데만 가셨네요",
          "original_form": "오 좋은데만 가셨네요",
          "speaker_id": "2",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010627.1.17",
          "form": "아 동술도 갔는데 이름이 생각안나네요",
          "original_form": "아 동술도 갔는데 이름이 생각안나네요",
          "speaker_id": "2",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010627.1.18",
          "form": "애가 사격장을 갔는데 실찬이라 신나하더라구요",
          "original_form": "애가 사격장을 갔는데 실찬이라 신나하더라구요",
          "speaker_id": "2",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010627.1.19",
          "form": "실탄",
          "original_form": "실탄",
          "speaker_id": "2",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010627.1.20",
          "form": "참 저희는 이것저것 체험을 했어요",
          "original_form": "참 저희는 이것저것 체험을 했어요",
          "speaker_id": "2",
          "time": "20210823 20:54"
        },
        {
          "id": "MDRW2100010627.1.21",
          "form": "귤도 따보고",
          "original_form": "귤도 따보고",
          "speaker_id": "2",
          "time": "20210823 20:54"
        },
        {
          "id": "MDRW2100010627.1.22",
          "form": "너무 신맛이라 ㅋㅋ못먹고 청했지만요",
          "original_form": "너무 신맛이라 ㅋㅋ못먹고 청했지만요",
          "speaker_id": "2",
          "time": "20210823 20:54"
        },
        {
          "id": "MDRW2100010627.1.23",
          "form": "청을 담갔어요",
          "original_form": "청을 담갔어요",
          "speaker_id": "2",
          "time": "20210823 20:55"
        },
        {
          "id": "MDRW2100010627.1.24",
          "form": "여행에선 기념품이나 먹을거리 참 신경쓰여요",
          "original_form": "여행에선 기념품이나 먹을거리 참 신경쓰여요",
          "speaker_id": "2",
          "time": "20210823 21:06"
        },
        {
          "id": "MDRW2100010627.1.25",
          "form": "아 저는 제주도에서 돌아올때 마음샌드 샀어요! 드셔보셨어요?",
          "original_form": "아 저는 제주도에서 돌아올때 마음샌드 샀어요! 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010627.1.26",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010627.1.27",
          "form": "전 청귤하고 샤브만 실컸먹고 회 먹었어요",
          "original_form": "전 청귤하고 샤브만 실컸먹고 회 먹었어요",
          "speaker_id": "2",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100010627.1.28",
          "form": "모양도 이뿌던데",
          "original_form": "모양도 이뿌던데",
          "speaker_id": "2",
          "time": "20210823 21:16"
        },
        {
          "id": "MDRW2100010627.1.29",
          "form": "크림이랑 맛난가요?",
          "original_form": "크림이랑 맛난가요?",
          "speaker_id": "2",
          "time": "20210823 21:16"
        },
        {
          "id": "MDRW2100010627.1.30",
          "form": "저도 내년엔 사봐야겠네요",
          "original_form": "저도 내년엔 사봐야겠네요",
          "speaker_id": "2",
          "time": "20210823 21:16"
        },
        {
          "id": "MDRW2100010627.1.31",
          "form": "전 갈때 검색안해서 핫한건 항시 못사나봐요",
          "original_form": "전 갈때 검색안해서 핫한건 항시 못사나봐요",
          "speaker_id": "2",
          "time": "20210823 21:17"
        },
        {
          "id": "MDRW2100010627.1.32",
          "form": "그래서 좀 실패인데 먹거리는 현지에 아무데나 가는데도 넘 맛났어요 ㅎㅎ",
          "original_form": "그래서 좀 실패인데 먹거리는 현지에 아무데나 가는데도 넘 맛났어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 21:17"
        },
        {
          "id": "MDRW2100010627.1.33",
          "form": "그냥.비싼 국희샌드 맛이다 그.표현이 딱이더라구요^^ 제주 맛집 많죠~~ 부럽네요",
          "original_form": "그냥.비싼 국희샌드 맛이다 그.표현이 딱이더라구요^^ 제주 맛집 많죠~~ 부럽네요",
          "speaker_id": "1",
          "time": "20210823 21:19"
        },
        {
          "id": "MDRW2100010627.1.34",
          "form": "ㅋㅋ뭔지 알겠네요 ㅋㅋ",
          "original_form": "ㅋㅋ뭔지 알겠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:20"
        },
        {
          "id": "MDRW2100010627.1.35",
          "form": "제주 시내서 그냥 들어간집도 반찬 가득 주셔서 맛났어요",
          "original_form": "제주 시내서 그냥 들어간집도 반찬 가득 주셔서 맛났어요",
          "speaker_id": "2",
          "time": "20210823 21:20"
        },
        {
          "id": "MDRW2100010627.1.36",
          "form": "제주를 대표하는 기념품이다 싶긴한데 비쌌어요 ㅋ",
          "original_form": "제주를 대표하는 기념품이다 싶긴한데 비쌌어요 ㅋ",
          "speaker_id": "1",
          "time": "20210823 21:20"
        },
        {
          "id": "MDRW2100010627.1.37",
          "form": "비싸죠",
          "original_form": "비싸죠",
          "speaker_id": "2",
          "time": "20210823 21:20"
        },
        {
          "id": "MDRW2100010627.1.38",
          "form": "저는 은희네해장국도 맛있게 먹었어요",
          "original_form": "저는 은희네해장국도 맛있게 먹었어요",
          "speaker_id": "1",
          "time": "20210823 21:20"
        },
        {
          "id": "MDRW2100010627.1.39",
          "form": "뭐든기념품점도 초코렛빼곤 다비싸요",
          "original_form": "뭐든기념품점도 초코렛빼곤 다비싸요",
          "speaker_id": "2",
          "time": "20210823 21:20"
        },
        {
          "id": "MDRW2100010627.1.40",
          "form": "요샌 기념품집도 이뿌게 만들어놓고 비싸더라구요",
          "original_form": "요샌 기념품집도 이뿌게 만들어놓고 비싸더라구요",
