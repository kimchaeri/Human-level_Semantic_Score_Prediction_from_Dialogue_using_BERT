      "utterance": [
        {
          "id": "MDRW2100019430.1.1",
          "form": "딸기도 좋아하시죠 ??",
          "original_form": "딸기도 좋아하시죠 ??",
          "speaker_id": "2",
          "time": "20210827 19:14"
        },
        {
          "id": "MDRW2100019430.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210827 19:18"
        },
        {
          "id": "MDRW2100019430.1.3",
          "form": "딸기 좋아하는 품종 있으세요?",
          "original_form": "딸기 좋아하는 품종 있으세요?",
          "speaker_id": "1",
          "time": "20210827 19:19"
        },
        {
          "id": "MDRW2100019430.1.4",
          "form": "저는 딸기라면 다 좋아하는데 ㅎㅎ 품종을 잘 몰라요 ㅠㅠ",
          "original_form": "저는 딸기라면 다 좋아하는데 ㅎㅎ 품종을 잘 몰라요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019430.1.5",
          "form": "좀 맛있는 품종 있나요?",
          "original_form": "좀 맛있는 품종 있나요?",
          "speaker_id": "2",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019430.1.6",
          "form": "저도 잘 몰라요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 잘 몰라요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019430.1.7",
          "form": "듣기로는 설향? 들어봤고",
          "original_form": "듣기로는 설향? 들어봤고",
          "speaker_id": "1",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019430.1.8",
          "form": "빨간 대야에 담겨서 파는 딸기도 맛있었어요!",
          "original_form": "빨간 대야에 담겨서 파는 딸기도 맛있었어요!",
          "speaker_id": "1",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019430.1.9",
          "form": "아 설향은 들어봤네요!",
          "original_form": "아 설향은 들어봤네요!",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019430.1.10",
          "form": "설향우유 ㅋㅋㅋ",
          "original_form": "설향우유 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019430.1.11",
          "form": "맞아요 빨간 대야 ㅋㅋ 저는 막 상한거 아니면 다 좋아해요",
          "original_form": "맞아요 빨간 대야 ㅋㅋ 저는 막 상한거 아니면 다 좋아해요",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019430.1.12",
          "form": "딸기로 뭐 만들어 드시는거 잇나요?",
          "original_form": "딸기로 뭐 만들어 드시는거 잇나요?",
          "speaker_id": "1",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019430.1.13",
          "form": "전 딸기청 만들어먹어요!",
          "original_form": "전 딸기청 만들어먹어요!",
          "speaker_id": "2",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100019430.1.14",
          "form": "담가서 바로 드세요?",
          "original_form": "담가서 바로 드세요?",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019430.1.15",
          "form": "아니요 한 삼일뒤에 먹어요 ㅎㅎ",
          "original_form": "아니요 한 삼일뒤에 먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100019430.1.16",
          "form": "우유에 타서 딸기라떼로 드시나요?",
          "original_form": "우유에 타서 딸기라떼로 드시나요?",
          "speaker_id": "1",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019430.1.17",
          "form": "네 보통 라떼로 먹어요 ㅎㅎ",
          "original_form": "네 보통 라떼로 먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100019430.1.18",
          "form": "에이드는 막 안좋아해서요 !",
          "original_form": "에이드는 막 안좋아해서요 !",
          "speaker_id": "2",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100019430.1.19",
          "form": "저도 라떼로 자주 하는데",
          "original_form": "저도 라떼로 자주 하는데",
          "speaker_id": "1",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100019430.1.20",
          "form": "달달하고 상큼해서 맛있더라구요",
          "original_form": "달달하고 상큼해서 맛있더라구요",
          "speaker_id": "1",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100019430.1.21",
          "form": "한번 해먹고 난 뒤로는 딸기철만 기다려요",
          "original_form": "한번 해먹고 난 뒤로는 딸기철만 기다려요",
          "speaker_id": "1",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100019430.1.22",
          "form": "맞아요 엄청 쉽게 만들 수 있지않나요 ㅎㅎ",
          "original_form": "맞아요 엄청 쉽게 만들 수 있지않나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:30"
        }
      ]
