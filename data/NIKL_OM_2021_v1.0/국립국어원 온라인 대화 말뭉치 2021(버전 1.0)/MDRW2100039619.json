      "utterance": [
        {
          "id": "MDRW2100039619.1.1",
          "form": "영화는 어떤 종류",
          "original_form": "영화는 어떤 종류",
          "speaker_id": "1",
          "time": "20210903 19:47"
        },
        {
          "id": "MDRW2100039619.1.2",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 19:47"
        },
        {
          "id": "MDRW2100039619.1.3",
          "form": "아니ㅜ장르요",
          "original_form": "아니ㅜ장르요",
          "speaker_id": "1",
          "time": "20210903 19:47"
        },
        {
          "id": "MDRW2100039619.1.4",
          "form": "액션 좋아해요",
          "original_form": "액션 좋아해요",
          "speaker_id": "2",
          "time": "20210903 19:48"
        },
        {
          "id": "MDRW2100039619.1.5",
          "form": "범죄 스릴러 같은것도요",
          "original_form": "범죄 스릴러 같은것도요",
          "speaker_id": "2",
          "time": "20210903 19:48"
        },
        {
          "id": "MDRW2100039619.1.6",
          "form": "오 거기서",
          "original_form": "오 거기서",
          "speaker_id": "1",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100039619.1.7",
          "form": "좋아하는 영화 뭔가요?",
          "original_form": "좋아하는 영화 뭔가요?",
          "speaker_id": "1",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100039619.1.8",
          "form": "역시 신세계가 제일 기억남네요ㅋㅋ",
          "original_form": "역시 신세계가 제일 기억남네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039619.1.9",
          "form": "요새도 좋은영화 많겠지만요",
          "original_form": "요새도 좋은영화 많겠지만요",
          "speaker_id": "2",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039619.1.10",
          "form": "신세계 오랜만이네요ㅋㅋㅋㅋㅋ",
          "original_form": "신세계 오랜만이네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100039619.1.11",
          "form": "ㅋㅋㅋ가끔 다시 보면 또 재밌더라구요",
          "original_form": "ㅋㅋㅋ가끔 다시 보면 또 재밌더라구요",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100039619.1.12",
          "form": "맞아요 그런 영화가 꼭 있죠ㅋㅋㅋㅌㅌ",
          "original_form": "맞아요 그런 영화가 꼭 있죠ㅋㅋㅋㅌㅌ",
          "speaker_id": "1",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039619.1.13",
          "form": "좋아하는 영화 있으세요?",
          "original_form": "좋아하는 영화 있으세요?",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039619.1.14",
          "form": "음 저 쥬만지",
          "original_form": "음 저 쥬만지",
          "speaker_id": "1",
          "time": "20210903 21:59"
        },
        {
          "id": "MDRW2100039619.1.15",
          "form": "좋아합니다ㅋㅋ",
          "original_form": "좋아합니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:59"
        },
        {
          "id": "MDRW2100039619.1.16",
          "form": "쥬만지 재밌죠ㅋㅋ 코믹액션 다 있어서 좋아요",
          "original_form": "쥬만지 재밌죠ㅋㅋ 코믹액션 다 있어서 좋아요",
          "speaker_id": "2",
          "time": "20210903 22:01"
        },
        {
          "id": "MDRW2100039619.1.17",
          "form": "ㅋㅋㅋㅋㅋ맞아요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:01"
        },
        {
          "id": "MDRW2100039619.1.18",
          "form": "처음보고 신세계였어요 ..",
          "original_form": "처음보고 신세계였어요 ..",
          "speaker_id": "1",
          "time": "20210903 22:01"
        },
        {
          "id": "MDRW2100039619.1.19",
          "form": "약간 유치한데ㅋㅋ 계속 보게되는 마력이 있어요",
          "original_form": "약간 유치한데ㅋㅋ 계속 보게되는 마력이 있어요",
          "speaker_id": "2",
          "time": "20210903 22:03"
        },
        {
          "id": "MDRW2100039619.1.20",
          "form": "게임도 해보고싶긴 해요. 영화처럼 리얼하게는 말구요ㅋㅋㅋㅋ",
          "original_form": "게임도 해보고싶긴 해요. 영화처럼 리얼하게는 말구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039619.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그렇죠.. 근데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그렇죠.. 근데",
          "speaker_id": "1",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039619.1.22",
          "form": "짱 무서울 고 같은..",
          "original_form": "짱 무서울 고 같은..",
          "speaker_id": "1",
