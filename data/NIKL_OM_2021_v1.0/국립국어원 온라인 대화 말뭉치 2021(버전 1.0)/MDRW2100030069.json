      "utterance": [
        {
          "id": "MDRW2100030069.1.1",
          "form": "저는 여자라서 군대 잘 몰라요",
          "original_form": "저는 여자라서 군대 잘 몰라요",
          "speaker_id": "1",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100030069.1.2",
          "form": "모병제로 우리나라도 가야된다고 생각해요",
          "original_form": "모병제로 우리나라도 가야된다고 생각해요",
          "speaker_id": "1",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100030069.1.3",
          "form": "저는 지금 군인이에요 !",
          "original_form": "저는 지금 군인이에요 !",
          "speaker_id": "2",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100030069.1.4",
          "form": "모병제는 조금 힘들죠..",
          "original_form": "모병제는 조금 힘들죠..",
          "speaker_id": "2",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030069.1.5",
          "form": "양심적 병역거부 합법된거 찬성해요",
          "original_form": "양심적 병역거부 합법된거 찬성해요",
          "speaker_id": "1",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030069.1.6",
          "form": "군인이시면 월급 궁금해요",
          "original_form": "군인이시면 월급 궁금해요",
          "speaker_id": "1",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030069.1.7",
          "form": "악용하는 사람이",
          "original_form": "악용하는 사람이",
          "speaker_id": "2",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030069.1.8",
          "form": "없으면 좋겠네요",
          "original_form": "없으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030069.1.9",
          "form": "악용하는 사람 저도 없으면 좋겠어요",
          "original_form": "악용하는 사람 저도 없으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030069.1.10",
          "form": "오태양 이라고 아세요?",
          "original_form": "오태양 이라고 아세요?",
          "speaker_id": "1",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030069.1.11",
          "form": "월급은 적금빼면 간식 먹을정도..",
          "original_form": "월급은 적금빼면 간식 먹을정도..",
          "speaker_id": "2",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030069.1.12",
          "form": "첫번째 양심적 병역거부 한 사람이예요",
          "original_form": "첫번째 양심적 병역거부 한 사람이예요",
          "speaker_id": "1",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030069.1.13",
          "form": "오태양이 뭔가요?",
          "original_form": "오태양이 뭔가요?",
          "speaker_id": "2",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030069.1.14",
          "form": "역사적 인물이예요",
          "original_form": "역사적 인물이예요",
          "speaker_id": "1",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030069.1.15",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030069.1.16",
          "form": "그사람은 어떻게 됬나요",
          "original_form": "그사람은 어떻게 됬나요",
          "speaker_id": "2",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030069.1.17",
          "form": "나라 지켜주셔서 감사합니다",
          "original_form": "나라 지켜주셔서 감사합니다",
          "speaker_id": "1",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030069.1.18",
          "form": "그 사람은 잘 살고 있죠",
          "original_form": "그 사람은 잘 살고 있죠",
          "speaker_id": "1",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030069.1.19",
          "form": "저도 군인이라 힘든데",
          "original_form": "저도 군인이라 힘든데",
          "speaker_id": "2",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030069.1.20",
          "form": "십몇년전 감옥갔다 왔지만 그분 덕분에 다른 사람들 양심적 병역거부 하잖아요",
          "original_form": "십몇년전 감옥갔다 왔지만 그분 덕분에 다른 사람들 양심적 병역거부 하잖아요",
          "speaker_id": "1",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030069.1.21",
          "form": "그런걸 보면 부럽다는 생각이 먼저 들어요",
          "original_form": "그런걸 보면 부럽다는 생각이 먼저 들어요",
          "speaker_id": "2",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030069.1.22",
          "form": "3년 합숙생활",
          "original_form": "3년 합숙생활",
          "speaker_id": "1",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100030069.1.23",
          "form": "교도소에서 하는건데",
          "original_form": "교도소에서 하는건데",
          "speaker_id": "1",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100030069.1.24",
          "form": "부러우세요?",
          "original_form": "부러우세요?",
          "speaker_id": "1",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100030069.1.25",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100030069.1.26",
          "form": "네 ~",
          "original_form": "네 ~",
          "speaker_id": "1",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100030069.1.27",
          "form": "그럼 왜 병역거부를 한거지",
          "original_form": "그럼 왜 병역거부를 한거지",
          "speaker_id": "2",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100030069.1.28",
          "form": "그러니까요 징벌적이예요",
          "original_form": "그러니까요 징벌적이예요",
          "speaker_id": "1",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100030069.1.29",
          "form": "아직도 제도가 미비하죠",
          "original_form": "아직도 제도가 미비하죠",
          "speaker_id": "1",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100030069.1.30",
          "form": "군 부조리가 없어지면 좀 줄어들텐데",
          "original_form": "군 부조리가 없어지면 좀 줄어들텐데",
          "speaker_id": "2",
          "time": "20210903 20:42"
        }
      ]
