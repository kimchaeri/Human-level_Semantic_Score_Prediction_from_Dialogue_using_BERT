        {
          "id": "MDRW2100022354.1.1",
          "form": "여행조아하시나요",
          "original_form": "여행조아하시나요",
          "speaker_id": "1",
          "time": "20210829 12:59"
        },
        {
          "id": "MDRW2100022354.1.2",
          "form": "힐링여행은 좋아해요ㅎㅎ",
          "original_form": "힐링여행은 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100022354.1.3",
          "form": "어디가보셨나요",
          "original_form": "어디가보셨나요",
          "speaker_id": "1",
          "time": "20210829 13:06"
        },
        {
          "id": "MDRW2100022354.1.4",
          "form": "호캉스나 스파좋죠ㅎㅎ",
          "original_form": "호캉스나 스파좋죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100022354.1.5",
          "form": "해외는 안가보셨나요?",
          "original_form": "해외는 안가보셨나요?",
          "speaker_id": "1",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100022354.1.6",
          "form": "해외는 신혼여행이 ㅠ첨이자 마지막이였네여ㅠㅠ",
          "original_form": "해외는 신혼여행이 ㅠ첨이자 마지막이였네여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100022354.1.7",
          "form": "오 어디다녀오셨나요",
          "original_form": "오 어디다녀오셨나요",
          "speaker_id": "1",
          "time": "20210829 14:05"
        },
        {
          "id": "MDRW2100022354.1.8",
          "form": "푸켓이요^^",
          "original_form": "푸켓이요^^",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022354.1.9",
          "form": "푸켓! 가보고싶네요",
          "original_form": "푸켓! 가보고싶네요",
          "speaker_id": "1",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100022354.1.10",
          "form": "좋았어요~ 근데 그나라바퀴벌레가 엄청 크더라구요 보고 식겁했어여ㅜㅜ",
          "original_form": "좋았어요~ 근데 그나라바퀴벌레가 엄청 크더라구요 보고 식겁했어여ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022354.1.11",
          "form": "아 역시동남아라ㅠ",
          "original_form": "아 역시동남아라ㅠ",
          "speaker_id": "1",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100022354.1.12",
          "form": "그런것. 빼면 좋았어요ㅋㅋ",
          "original_form": "그런것. 빼면 좋았어요ㅋㅋ",
