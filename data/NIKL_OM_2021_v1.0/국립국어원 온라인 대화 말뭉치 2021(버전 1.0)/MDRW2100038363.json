        {
          "id": "MDRW2100038363.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038363.1.2",
          "form": "네 강아지여",
          "original_form": "네 강아지여",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038363.1.3",
          "form": "어떤 친구인가요",
          "original_form": "어떤 친구인가요",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038363.1.4",
          "form": "비숑이에여",
          "original_form": "비숑이에여",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038363.1.5",
          "form": "동물 좋아하세요?",
          "original_form": "동물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100038363.1.6",
          "form": "엄청 좋아해요",
          "original_form": "엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100038363.1.7",
          "form": "아직 키워본적은 없지만",
          "original_form": "아직 키워본적은 없지만",
          "speaker_id": "2",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100038363.1.8",
          "form": "언젠가는 키워보고 싶어요",
          "original_form": "언젠가는 키워보고 싶어요",
          "speaker_id": "2",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100038363.1.9",
          "form": "일은 많은데 귀여워요ㅎ",
          "original_form": "일은 많은데 귀여워요ㅎ",
          "speaker_id": "1",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100038363.1.10",
          "form": "관리하기가 힘든가요?",
          "original_form": "관리하기가 힘든가요?",
          "speaker_id": "2",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100038363.1.11",
          "form": "그냥 아이키우는거랑 같아요ㅋ",
          "original_form": "그냥 아이키우는거랑 같아요ㅋ",
          "speaker_id": "1",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100038363.1.12",
          "form": "오우 갑자기 난이도가 실감났어요",
          "original_form": "오우 갑자기 난이도가 실감났어요",
          "speaker_id": "2",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100038363.1.13",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:12"
        },
        {
          "id": "MDRW2100038363.1.14",
          "form": "그래도 귀여워서 용서되요",
          "original_form": "그래도 귀여워서 용서되요",
          "speaker_id": "1",
          "time": "20210902 12:12"
        },
        {
          "id": "MDRW2100038363.1.15",
          "form": "이쁜 아이인가봐요",
          "original_form": "이쁜 아이인가봐요",
          "speaker_id": "2",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100038363.1.16",
          "form": "그냥 정들어서ㅎ",
          "original_form": "그냥 정들어서ㅎ",
          "speaker_id": "1",
          "time": "20210902 12:15"
