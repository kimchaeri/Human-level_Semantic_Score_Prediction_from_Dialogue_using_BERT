      },
      "utterance": [
        {
          "id": "MDRW2100005125.1.1",
          "form": "난 죠스바 먹었다",
          "original_form": "난 죠스바 먹었다",
          "speaker_id": "2",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005125.1.2",
          "form": "나는 상투과자!!",
          "original_form": "나는 상투과자!!",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005125.1.3",
          "form": "상투과자 뭔지알아?",
          "original_form": "상투과자 뭔지알아?",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005125.1.4",
          "form": "그게 모냐",
          "original_form": "그게 모냐",
          "speaker_id": "2",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005125.1.5",
          "form": "그 옛날과자 생과자 파는곳 보면",
          "original_form": "그 옛날과자 생과자 파는곳 보면",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005125.1.6",
          "form": "생크림틀로 만든것 같은 과자",
          "original_form": "생크림틀로 만든것 같은 과자",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005125.1.7",
          "form": "엄청 얇은거?",
          "original_form": "엄청 얇은거?",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005125.1.8",
          "form": "안에 크림있는거?",
          "original_form": "안에 크림있는거?",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005125.1.9",
          "form": "아니아니",
          "original_form": "아니아니",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005125.1.10",
          "form": "그냥 부드러운과자..??빵같은거",
          "original_form": "그냥 부드러운과자..??빵같은거",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005125.1.11",
          "form": "아 찾아봄",
          "original_form": "아 찾아봄",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005125.1.12",
          "form": "그거 퍽퍽하자나",
          "original_form": "그거 퍽퍽하자나",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005125.1.13",
          "form": "우유랑먹으면 맛있어",
          "original_form": "우유랑먹으면 맛있어",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005125.1.14",
          "form": "너 알몬드우유 아냐",
          "original_form": "너 알몬드우유 아냐",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005125.1.15",
          "form": "언스위튼",
          "original_form": "언스위튼",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005125.1.16",
          "form": "그거 아몬드브리즈??",
          "original_form": "그거 아몬드브리즈??",
          "speaker_id": "1",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005125.1.17",
          "form": "응 그거 안단걸로 먹어봄?",
          "original_form": "응 그거 안단걸로 먹어봄?",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005125.1.18",
          "form": "그럼 맛없을 것 같아",
          "original_form": "그럼 맛없을 것 같아",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005125.1.19",
          "form": "기억이 안나",
          "original_form": "기억이 안나",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005125.1.20",
          "form": "먹어본것 같긴한대",
          "original_form": "먹어본것 같긴한대",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005125.1.21",
          "form": "고소한데",
          "original_form": "고소한데",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005125.1.22",
          "form": "아몬드 안단맛",
          "original_form": "아몬드 안단맛",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005125.1.23",
          "form": "그거랑 상투과자랑 머거봐",
          "original_form": "그거랑 상투과자랑 머거봐",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005125.1.24",
          "form": "난 단백질음료 맛없는거 있지...",
          "original_form": "난 단백질음료 맛없는거 있지...",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005125.1.25",
          "form": "나 한번도 안먹어봄",
          "original_form": "나 한번도 안먹어봄",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005125.1.26",
          "form": "무슨맛이냐",
          "original_form": "무슨맛이냐",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005125.1.27",
          "form": "초코맛은 맛없어서",
          "original_form": "초코맛은 맛없어서",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005125.1.28",
          "form": "미숫가루맛",
          "original_form": "미숫가루맛",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005125.1.29",
          "form": "미숫가루",
          "original_form": "미숫가루",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005125.1.30",
          "form": "미숫가루면 맛있는 맛이자나",
          "original_form": "미숫가루면 맛있는 맛이자나",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005125.1.31",
          "form": "곡물맛은 맛있어",
          "original_form": "곡물맛은 맛있어",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005125.1.32",
          "form": "그거 먹으면 막 근육이 더 생겨?",
          "original_form": "그거 먹으면 막 근육이 더 생겨?",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005125.1.33",
          "form": "아니 최소한양심이야...",
          "original_form": "아니 최소한양심이야...",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005125.1.34",
          "form": "걍 하루치 단백질을 채우는 용?",
          "original_form": "걍 하루치 단백질을 채우는 용?",
          "speaker_id": "2",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005125.1.35",
          "form": "그냥 살좀 덜찌지않을까 생각만..하지",
          "original_form": "그냥 살좀 덜찌지않을까 생각만..하지",
          "speaker_id": "1",
          "time": "20210821 20:55"
        }
      ]
    }
