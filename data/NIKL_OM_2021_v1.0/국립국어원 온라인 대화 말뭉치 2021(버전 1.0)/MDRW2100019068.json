        },
        {
          "id": "MDRW2100019068.1.2",
          "form": "여기도 엄청나요",
          "original_form": "여기도 엄청나요",
          "speaker_id": "2",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100019068.1.3",
          "form": "내일은 안오겠죠?",
          "original_form": "내일은 안오겠죠?",
          "speaker_id": "1",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100019068.1.4",
          "form": "내일어디가세요",
          "original_form": "내일어디가세요",
          "speaker_id": "2",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100019068.1.5",
          "form": "회사요...",
          "original_form": "회사요...",
          "speaker_id": "1",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100019068.1.6",
          "form": "아ㅜㅜ 안타깝네요",
          "original_form": "아ㅜㅜ 안타깝네요",
          "speaker_id": "2",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100019068.1.7",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100019068.1.8",
          "form": "내일은 비안올거에요",
          "original_form": "내일은 비안올거에요",
          "speaker_id": "2",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100019068.1.9",
          "form": "네 그랬으면좋겠네요",
          "original_form": "네 그랬으면좋겠네요",
          "speaker_id": "1",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100019068.1.10",
          "form": "옷 따뜻하게입고가세요",
          "original_form": "옷 따뜻하게입고가세요",
          "speaker_id": "2",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019068.1.11",
          "form": "날이 많이 추워졌어요",
          "original_form": "날이 많이 추워졌어요",
          "speaker_id": "1",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019068.1.12",
          "form": "그니까요 비와서 하루종일 춥네요",
          "original_form": "그니까요 비와서 하루종일 춥네요",
          "speaker_id": "2",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019068.1.13",
          "form": "이제 에어컨 안켜는 게 일상이네요",
          "original_form": "이제 에어컨 안켜는 게 일상이네요",
