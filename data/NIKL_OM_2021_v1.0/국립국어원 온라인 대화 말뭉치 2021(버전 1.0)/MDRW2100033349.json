      "utterance": [
        {
          "id": "MDRW2100033349.1.1",
          "form": "부산에 롯데월드 생긴다던데",
          "original_form": "부산에 롯데월드 생긴다던데",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100033349.1.2",
          "form": "그거 말씀하신거죠?",
          "original_form": "그거 말씀하신거죠?",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100033349.1.3",
          "form": "새로생긴다는거",
          "original_form": "새로생긴다는거",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100033349.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100033349.1.5",
          "form": "롯데월드 생기면",
          "original_form": "롯데월드 생기면",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100033349.1.6",
          "form": "거기 가셔서",
          "original_form": "거기 가셔서",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100033349.1.7",
          "form": "개장을 언제할지 좀 애매하긴하네요 ㅋㅋㅋ",
          "original_form": "개장을 언제할지 좀 애매하긴하네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100033349.1.8",
          "form": "신나게 노세요 ㅋㅋㅋ",
          "original_form": "신나게 노세요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100033349.1.9",
          "form": "남친 놀이기구 잘탄대요?",
          "original_form": "남친 놀이기구 잘탄대요?",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100033349.1.10",
          "form": "ㅋㅋㅋㅋ놀이기구를 제가 잘못타요",
          "original_form": "ㅋㅋㅋㅋ놀이기구를 제가 잘못타요",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100033349.1.11",
          "form": "남친은 잘탄댔어요",
          "original_form": "남친은 잘탄댔어요",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100033349.1.12",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100033349.1.13",
          "form": "과연 남친은 어떻게 할것인가",
          "original_form": "과연 남친은 어떻게 할것인가",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100033349.1.14",
          "form": "저흰 둘다 못타서",
          "original_form": "저흰 둘다 못타서",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033349.1.15",
          "form": "놀이공원데이트 딱 한번했는데 ㅋㅋㅋ",
          "original_form": "놀이공원데이트 딱 한번했는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033349.1.16",
          "form": "ㅋㅋㅋㅋ놀이공원은 근데 하루만 놀아도 너무 지쳐서",
          "original_form": "ㅋㅋㅋㅋ놀이공원은 근데 하루만 놀아도 너무 지쳐서",
          "speaker_id": "2",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100033349.1.17",
          "form": "일년에 한번 갈까말까하게 되더라구요",
          "original_form": "일년에 한번 갈까말까하게 되더라구요",
          "speaker_id": "2",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100033349.1.18",
          "form": "마장ᆢ",
          "original_form": "마장ᆢ",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033349.1.19",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033349.1.20",
          "form": "다리가 후달거리죠 ㅋㅋㅋ",
          "original_form": "다리가 후달거리죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033349.1.21",
          "form": "코로나땜에 데이트할게 마땅찮네요",
          "original_form": "코로나땜에 데이트할게 마땅찮네요",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100033349.1.22",
          "form": "이제 저흰 둘 다 2차 접종을 완료한 사람들!!",
          "original_form": "이제 저흰 둘 다 2차 접종을 완료한 사람들!!",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100033349.1.23",
          "form": "좀 더 상황이 나아지겠죵...ㅋㅋㅋ",
          "original_form": "좀 더 상황이 나아지겠죵...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100033349.1.24",
          "form": "몸은 괜찮으세요?",
          "original_form": "몸은 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100033349.1.25",
          "form": "넹 이제 멀쩡해요",
          "original_form": "넹 이제 멀쩡해요",
          "speaker_id": "2",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100033349.1.26",
          "form": "이렇게 멀쩡하신분도 있군여",
          "original_form": "이렇게 멀쩡하신분도 있군여",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100033349.1.27",
          "form": "저도 멀쩡했으면 좋겠다는...",
          "original_form": "저도 멀쩡했으면 좋겠다는...",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100033349.1.28",
          "form": "근데 저 오늘 출근해보니까",
          "original_form": "근데 저 오늘 출근해보니까",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100033349.1.29",
          "form": "멀쩡했다는 분들이 더 많았어요",
          "original_form": "멀쩡했다는 분들이 더 많았어요",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100033349.1.30",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100033349.1.31",
          "form": "주사맞을날이 바껴가지고",
          "original_form": "주사맞을날이 바껴가지고",
          "speaker_id": "1",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100033349.1.32",
          "form": "그날 연차쓰고 맞고 쉬려고하는데",
          "original_form": "그날 연차쓰고 맞고 쉬려고하는데",
          "speaker_id": "1",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100033349.1.33",
          "form": "안아프면 쉬는게",
          "original_form": "안아프면 쉬는게",
          "speaker_id": "1",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100033349.1.34",
          "form": "더 쉬는거 같으니까 ㅋㅋㅋ",
          "original_form": "더 쉬는거 같으니까 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100033349.1.35",
          "form": "ㅋㅋㅋㅋ하루는 좀 요양해야할거같고",
          "original_form": "ㅋㅋㅋㅋ하루는 좀 요양해야할거같고",
          "speaker_id": "2",
          "time": "20210907 10:58"
        },
        {
          "id": "MDRW2100033349.1.36",
          "form": "담날부턴 무리안하는 산책정도는 할 수 있을듯요",
          "original_form": "담날부턴 무리안하는 산책정도는 할 수 있을듯요",
          "speaker_id": "2",
          "time": "20210907 10:58"
        },
        {
          "id": "MDRW2100033349.1.37",
          "form": "그럼 연차써야겠네요 ㅋㅋ",
          "original_form": "그럼 연차써야겠네요 ㅋㅋ",
