      "utterance": [
        {
          "id": "MDRW2100042390.1.1",
          "form": "음 배고프면 먹어요 ㅋㅋㅋㅋㅋ 배고프면 못자서 ㅠ",
          "original_form": "음 배고프면 먹어요 ㅋㅋㅋㅋㅋ 배고프면 못자서 ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042390.1.2",
          "form": "ㅋㅋㅋㅋ그쵸 미칠노릇이져 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ그쵸 미칠노릇이져 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042390.1.3",
          "form": "다른 분들은 잘 자더라구요",
          "original_form": "다른 분들은 잘 자더라구요",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042390.1.4",
          "form": "전 안되요 ㅠㅠ",
          "original_form": "전 안되요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042390.1.5",
          "form": "뭐라도 드셔야 주무세여?",
          "original_form": "뭐라도 드셔야 주무세여?",
          "speaker_id": "1",
          "time": "20210907 07:57"
        },
        {
          "id": "MDRW2100042390.1.6",
          "form": "넹 ㅠ",
          "original_form": "넹 ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100042390.1.7",
          "form": "저녁 뭐드셨나요",
          "original_form": "저녁 뭐드셨나요",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100042390.1.8",
          "form": "고등어 김치 밥 먹었어요!",
          "original_form": "고등어 김치 밥 먹었어요!",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100042390.1.9",
          "form": "오늘 조선호텔 김치가 도착해서",
          "original_form": "오늘 조선호텔 김치가 도착해서",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100042390.1.10",
          "form": "맛보느라 ㅎㅎㅎ",
          "original_form": "맛보느라 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100042390.1.11",
          "form": "맛나요!",
          "original_form": "맛나요!",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100042390.1.12",
          "form": "오 저 먹어봤어용 !! 맛나요",
          "original_form": "오 저 먹어봤어용 !! 맛나요",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100042390.1.13",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100042390.1.14",
          "form": "어디김치좋아하세요?",
          "original_form": "어디김치좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100042390.1.15",
          "form": "저는 그때그때 그냥끌리는거 사먹어용",
          "original_form": "저는 그때그때 그냥끌리는거 사먹어용",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100042390.1.16",
          "form": "똑같은것만 사지않고요",
          "original_form": "똑같은것만 사지않고요",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100042390.1.17",
          "form": "아하 ㅎㅎ 저는 종갓집이 젤 맛나요",
          "original_form": "아하 ㅎㅎ 저는 종갓집이 젤 맛나요",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100042390.1.18",
          "form": "다른 곳 도전해도 종갓집이 최고더리구요",
          "original_form": "다른 곳 도전해도 종갓집이 최고더리구요",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100042390.1.19",
          "form": "오호 종갓집 김치중에서도 뭐가 젤 맛나요?",
          "original_form": "오호 종갓집 김치중에서도 뭐가 젤 맛나요?",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100042390.1.20",
          "form": "기본!",
          "original_form": "기본!",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100042390.1.21",
          "form": "전라도말고요",
          "original_form": "전라도말고요",
