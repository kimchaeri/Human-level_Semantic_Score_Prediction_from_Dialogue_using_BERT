      "utterance": [
        {
          "id": "MDRW2100028838.1.1",
          "form": "강아지 키우세요?",
          "original_form": "강아지 키우세요?",
          "speaker_id": "1",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100028838.1.2",
          "form": "옛날에.. ㅋㅋ 키웠어요",
          "original_form": "옛날에.. ㅋㅋ 키웠어요",
          "speaker_id": "2",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100028838.1.3",
          "form": "키우세요?",
          "original_form": "키우세요?",
          "speaker_id": "2",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100028838.1.4",
          "form": "전 아무것도 안키워요",
          "original_form": "전 아무것도 안키워요",
          "speaker_id": "1",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100028838.1.5",
          "form": "한번도 안키우신거죠?",
          "original_form": "한번도 안키우신거죠?",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100028838.1.6",
          "form": "네 왜냐면",
          "original_form": "네 왜냐면",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100028838.1.7",
          "form": "아버지가 싫어하세요",
          "original_form": "아버지가 싫어하세요",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100028838.1.8",
          "form": "아... 저희집도...ㅜㅠ 엄마가 별로 안좋아하셨어요ㅜ",
          "original_form": "아... 저희집도...ㅜㅠ 엄마가 별로 안좋아하셨어요ㅜ",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100028838.1.9",
          "form": "좋아하는 동물은 뭐예요?",
          "original_form": "좋아하는 동물은 뭐예요?",
          "speaker_id": "2",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100028838.1.10",
          "form": "동물은 거의 다 좋아하는데",
          "original_form": "동물은 거의 다 좋아하는데",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100028838.1.11",
          "form": "예전에 열대어 키웠었어요 ㅎㅎ",
          "original_form": "예전에 열대어 키웠었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100028838.1.12",
          "form": "열대어도 반려동물인가",
          "original_form": "열대어도 반려동물인가",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100028838.1.13",
          "form": "그죠! ㅎㅎ",
          "original_form": "그죠! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:03"
        },
        {
          "id": "MDRW2100028838.1.14",
          "form": "저도 거북이랑 붕어 키웠었어요",
          "original_form": "저도 거북이랑 붕어 키웠었어요",
          "speaker_id": "2",
          "time": "20210903 21:03"
        },
        {
          "id": "MDRW2100028838.1.15",
          "form": "거북이요?",
          "original_form": "거북이요?",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100028838.1.16",
          "form": "엄청 오래살자나요",
          "original_form": "엄청 오래살자나요",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100028838.1.17",
          "form": "금방... 죽더라고요ㅜ",
          "original_form": "금방... 죽더라고요ㅜ",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100028838.1.18",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100028838.1.19",
          "form": "금방죽었어요?",
          "original_form": "금방죽었어요?",
          "speaker_id": "1",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100028838.1.20",
          "form": "네...ㅜ 자라였나 여튼 손가락 두마디정도 사이즈였는데요ㅜ",
          "original_form": "네...ㅜ 자라였나 여튼 손가락 두마디정도 사이즈였는데요ㅜ",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100028838.1.21",
          "form": "오래산다고 들었는데....",
          "original_form": "오래산다고 들었는데....",
          "speaker_id": "1",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100028838.1.22",
          "form": "붕어는요?",
          "original_form": "붕어는요?",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100028838.1.23",
          "form": "붕어는 사고로...ㅜ",
          "original_form": "붕어는 사고로...ㅜ",
          "speaker_id": "2",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100028838.1.24",
          "form": "이런....",
          "original_form": "이런....",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100028838.1.25",
          "form": "잘크고있었는데..ㅜ",
          "original_form": "잘크고있었는데..ㅜ",
          "speaker_id": "2",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100028838.1.26",
          "form": "그뒤론 안키우신거에요?",
          "original_form": "그뒤론 안키우신거에요?",
          "speaker_id": "1",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100028838.1.27",
          "form": "동물은 함부로 책임지지못할거면 안키워야겠다는 교훈으로ㅜㅠ",
          "original_form": "동물은 함부로 책임지지못할거면 안키워야겠다는 교훈으로ㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100028838.1.28",
          "form": "동물키우시나여?",
          "original_form": "동물키우시나여?",
          "speaker_id": "1",
          "time": "20210905 23:00"
        }
      ]
