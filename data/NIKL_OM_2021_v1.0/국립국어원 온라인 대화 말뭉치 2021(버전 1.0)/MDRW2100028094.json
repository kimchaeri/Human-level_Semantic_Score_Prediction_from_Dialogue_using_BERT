      "utterance": [
        {
          "id": "MDRW2100028094.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028094.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028094.1.3",
          "form": "좋아하는 연예인이나 즐겨보는 방송있으신가요",
          "original_form": "좋아하는 연예인이나 즐겨보는 방송있으신가요",
          "speaker_id": "1",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100028094.1.4",
          "form": "없어요 요즘유투브봐요",
          "original_form": "없어요 요즘유투브봐요",
          "speaker_id": "2",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100028094.1.5",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100028094.1.6",
          "form": "전 넷플릭스랑 유튜브봐여 넷플 재밌더라구요~",
          "original_form": "전 넷플릭스랑 유튜브봐여 넷플 재밌더라구요~",
          "speaker_id": "1",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100028094.1.7",
          "form": "유투브 어떤거보세요",
          "original_form": "유투브 어떤거보세요",
          "speaker_id": "1",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100028094.1.8",
          "form": "아 넷플릭스봐요. 영화요. 최근엔 킹덤",
          "original_form": "아 넷플릭스봐요. 영화요. 최근엔 킹덤",
          "speaker_id": "2",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028094.1.9",
          "form": "유투브는 실시간 인기봐요",
          "original_form": "유투브는 실시간 인기봐요",
          "speaker_id": "2",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028094.1.10",
          "form": "킹덤 시즌2까진 봤는데 아신전 보신건가요?",
          "original_form": "킹덤 시즌2까진 봤는데 아신전 보신건가요?",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028094.1.11",
          "form": "네 아신전봤어요. 보셨어요?",
          "original_form": "네 아신전봤어요. 보셨어요?",
          "speaker_id": "2",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028094.1.12",
          "form": "그냥 볼만해요",
          "original_form": "그냥 볼만해요",
          "speaker_id": "2",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028094.1.13",
          "form": "아신전은 아직 못봤어요. ㅠ 한번 기화되면 봐야겠네요.. 최근에 디피도 잼나더라고요",
          "original_form": "아신전은 아직 못봤어요. ㅠ 한번 기화되면 봐야겠네요.. 최근에 디피도 잼나더라고요",
          "speaker_id": "1",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028094.1.14",
          "form": "정해인나오는거",
          "original_form": "정해인나오는거",
          "speaker_id": "1",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028094.1.15",
          "form": "아 그거 재밌다그러더라고요",
          "original_form": "아 그거 재밌다그러더라고요",
          "speaker_id": "2",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100028094.1.16",
          "form": "시즌제는 못보겠어요  길어서",
          "original_form": "시즌제는 못보겠어요  길어서",
          "speaker_id": "2",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100028094.1.17",
          "form": "아 그렇긴하죠 ㅠ 시즌1에 6화인데 재밌어서 순식간에 보더라구요",
          "original_form": "아 그렇긴하죠 ㅠ 시즌1에 6화인데 재밌어서 순식간에 보더라구요",
          "speaker_id": "1",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100028094.1.18",
          "form": "그렇군요. 저도 시간날때 봐야겠네요.",
          "original_form": "그렇군요. 저도 시간날때 봐야겠네요.",
          "speaker_id": "2",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028094.1.19",
