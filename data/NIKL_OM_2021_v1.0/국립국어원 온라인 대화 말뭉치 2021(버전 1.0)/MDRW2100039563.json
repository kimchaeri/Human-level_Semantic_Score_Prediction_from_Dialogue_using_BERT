      "utterance": [
        {
          "id": "MDRW2100039563.1.1",
          "form": "곧 휴 바사삭",
          "original_form": "곧 휴 바사삭",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100039563.1.2",
          "form": "네..ㅋㅋㅋㅋ",
          "original_form": "네..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100039563.1.3",
          "form": "완전 몇년만에 먹어서 색다르더라구요!!",
          "original_form": "완전 몇년만에 먹어서 색다르더라구요!!",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100039563.1.4",
          "form": "오랜만에 드셨나요?",
          "original_form": "오랜만에 드셨나요?",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039563.1.5",
          "form": "저도 올른 먹고싶어요 곧 휴 바사삭!!",
          "original_form": "저도 올른 먹고싶어요 곧 휴 바사삭!!",
          "speaker_id": "2",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039563.1.6",
          "form": "ㅋㅋㅋㅋ저는 굽네치킨 잘 안먹거든요!",
          "original_form": "ㅋㅋㅋㅋ저는 굽네치킨 잘 안먹거든요!",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039563.1.7",
          "form": "학교에서나 밖에서 단체로 시킬때 말곤 안먹어본것 같아요!",
          "original_form": "학교에서나 밖에서 단체로 시킬때 말곤 안먹어본것 같아요!",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039563.1.8",
          "form": "헐 학교애서 굽네를 드셨다니",
          "original_form": "헐 학교애서 굽네를 드셨다니",
          "speaker_id": "2",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039563.1.9",
          "form": "최고의 학교 아님니가!",
          "original_form": "최고의 학교 아님니가!",
          "speaker_id": "2",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039563.1.10",
          "form": "전 소녀대때문에..먹었던거 같아요..",
          "original_form": "전 소녀대때문에..먹었던거 같아요..",
          "speaker_id": "2",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039563.1.11",
          "form": "어렴풋한 기억에..?",
          "original_form": "어렴풋한 기억에..?",
          "speaker_id": "2",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039563.1.12",
          "form": "그쵸 뭔가 어렴풋한 기억.. 진짜 5년도 더 됐어요 ㅎㅎ",
          "original_form": "그쵸 뭔가 어렴풋한 기억.. 진짜 5년도 더 됐어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:47"
        },
        {
          "id": "MDRW2100039563.1.13",
          "form": "저 먹었을땐 진짜 순수 굽네치킨만 있었던것 같은데",
          "original_form": "저 먹었을땐 진짜 순수 굽네치킨만 있었던것 같은데",
          "speaker_id": "1",
          "time": "20210903 19:48"
        },
        {
          "id": "MDRW2100039563.1.14",
          "form": "물론 정확하지 않음..오 ㅋㅋ",
          "original_form": "물론 정확하지 않음..오 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:48"
        },
        {
          "id": "MDRW2100039563.1.15",
          "form": "앗 그랬음 그럴만두 해요",
          "original_form": "앗 그랬음 그럴만두 해요",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039563.1.16",
          "form": "저도 오리지널은 약간 별로!",
          "original_form": "저도 오리지널은 약간 별로!",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039563.1.17",
          "form": "치킨을 어떻게 구울생각을 했는지 ㅎㅎㅎ",
          "original_form": "치킨을 어떻게 구울생각을 했는지 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039563.1.18",
          "form": "먹었는데도 기름지지않고 부담스럽지 않아서 좋았어요!!",
          "original_form": "먹었는데도 기름지지않고 부담스럽지 않아서 좋았어요!!",
          "speaker_id": "1",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039563.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 20:14"
        },
        {
          "id": "MDRW2100039563.1.20",
          "form": "같이주는 소스가 맛있지않나요?",
          "original_form": "같이주는 소스가 맛있지않나요?",
          "speaker_id": "2",
          "time": "20210903 20:14"
        },
        {
          "id": "MDRW2100039563.1.21",
          "form": "맛은 있는데 무슨맛인지 모르겠어요 ㅋㅋㅋ",
          "original_form": "맛은 있는데 무슨맛인지 모르겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:14"
        },
        {
          "id": "MDRW2100039563.1.22",
          "form": "고블링 마블링..",
          "original_form": "고블링 마블링..",
          "speaker_id": "1",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100039563.1.23",
          "form": "고추장 마요네즈..?",
          "original_form": "고추장 마요네즈..?",
          "speaker_id": "1",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100039563.1.24",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039563.1.25",
          "form": "저는 고브링 좋아해요",
          "original_form": "저는 고브링 좋아해요",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039563.1.26",
          "form": "저도 얼른 먹고파요~!~!",
          "original_form": "저도 얼른 먹고파요~!~!",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039563.1.27",
          "form": "오맞나요? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오맞나요? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039563.1.28",
          "form": "무슨맛인지 추측해서 먹었어요",
          "original_form": "무슨맛인지 추측해서 먹었어요",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039563.1.29",
          "form": "살짝 시큼한게 요거트맛도 나고 뭐지? 했거드뇽ㅋㅋ",
          "original_form": "살짝 시큼한게 요거트맛도 나고 뭐지? 했거드뇽ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039563.1.30",
          "form": "저도 정확히는 모르겠어요! 근데 저도 비슷한 맛을 느꼈어요!!",
          "original_form": "저도 정확히는 모르겠어요! 근데 저도 비슷한 맛을 느꼈어요!!",
          "speaker_id": "2",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100039563.1.31",
          "form": "청양마요 고추마요.. 이런건 알겠는데 고블링 마블링..",
          "original_form": "청양마요 고추마요.. 이런건 알겠는데 고블링 마블링..",
          "speaker_id": "1",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100039563.1.32",
          "form": "그냥 도깨비만 생각나는ㅋㅋㅋ",
          "original_form": "그냥 도깨비만 생각나는ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100039563.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039563.1.34",
          "form": "맞아요 고블링 마블링..ㅎ",
          "original_form": "맞아요 고블링 마블링..ㅎ",
          "speaker_id": "2",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039563.1.35",
          "form": "전 심지어 첨에 이름때문에 약간 꺼려졌어요..",
          "original_form": "전 심지어 첨에 이름때문에 약간 꺼려졌어요..",
          "speaker_id": "2",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039563.1.36",
          "form": "그나저나 저는 다이어트 중이라 굽네치킨도 저 빼고 먹게 되는거 아닌지 모르겧어료",
          "original_form": "그나저나 저는 다이어트 중이라 굽네치킨도 저 빼고 먹게 되는거 아닌지 모르겧어료",
          "speaker_id": "2",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039563.1.37",
          "form": "지금 운동하러 왔어요..",
          "original_form": "지금 운동하러 왔어요..",
          "speaker_id": "2",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039563.1.38",
          "form": "30분했는대 죽을것 같은 이유 뭐죠..",
          "original_form": "30분했는대 죽을것 같은 이유 뭐죠..",
          "speaker_id": "2",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039563.1.39",
          "form": "그래도 굽네 곧 휴 바사삭을 위해",
          "original_form": "그래도 굽네 곧 휴 바사삭을 위해",
