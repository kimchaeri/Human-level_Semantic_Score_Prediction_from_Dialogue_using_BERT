        },
        {
          "id": "MDRW2100046643.1.2",
          "form": "반가워요",
          "original_form": "반가워요",
          "speaker_id": "1",
          "time": "20211103 22:25"
        },
        {
          "id": "MDRW2100046643.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211103 22:25"
        },
        {
          "id": "MDRW2100046643.1.4",
          "form": "어떤 장르의 음악을 좋아하시나요?",
          "original_form": "어떤 장르의 음악을 좋아하시나요?",
          "speaker_id": "2",
          "time": "20211103 22:26"
        },
        {
          "id": "MDRW2100046643.1.5",
          "form": "edm 주로 들어요",
          "original_form": "edm 주로 들어요",
          "speaker_id": "1",
          "time": "20211103 22:26"
        },
        {
          "id": "MDRW2100046643.1.6",
          "form": "신나는 음악으로",
          "original_form": "신나는 음악으로",
          "speaker_id": "1",
          "time": "20211103 22:26"
        },
        {
          "id": "MDRW2100046643.1.7",
          "form": "edm 신나죠 ㅋㅋㅋㅋㅋ",
          "original_form": "edm 신나죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211103 22:27"
        },
        {
          "id": "MDRW2100046643.1.8",
          "form": "듣다보면 저도 모르게 발가락으로 춤추게 돼요",
          "original_form": "듣다보면 저도 모르게 발가락으로 춤추게 돼요",
          "speaker_id": "2",
          "time": "20211103 22:27"
        },
        {
          "id": "MDRW2100046643.1.9",
          "form": "ㅋㅋ 맞아요",
          "original_form": "ㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20211103 22:32"
        },
        {
          "id": "MDRW2100046643.1.10",
          "form": "운전할때도 듣기 좋고",
          "original_form": "운전할때도 듣기 좋고",
          "speaker_id": "1",
          "time": "20211103 22:32"
        },
        {
          "id": "MDRW2100046643.1.11",
          "form": "신나요",
          "original_form": "신나요",
          "speaker_id": "1",
          "time": "20211103 22:32"
        },
        {
          "id": "MDRW2100046643.1.12",
          "form": "맞아요 ㅋㅋㅋ 저는 막 팝송 중간에 edm으로 쿵짝쿵짝 하는 그런 노래들이 좋더라구요",
          "original_form": "맞아요 ㅋㅋㅋ 저는 막 팝송 중간에 edm으로 쿵짝쿵짝 하는 그런 노래들이 좋더라구요",
          "speaker_id": "2",
          "time": "20211103 22:33"
        },
        {
          "id": "MDRW2100046643.1.13",
          "form": "그런 장르를 뭐라고 하는지는 모르겠지만요",
          "original_form": "그런 장르를 뭐라고 하는지는 모르겠지만요",
          "speaker_id": "2",
          "time": "20211103 22:33"
        },
        {
          "id": "MDRW2100046643.1.14",
          "form": "그것도 edm으로 쳐요 ㅋㅋ",
          "original_form": "그것도 edm으로 쳐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211103 22:33"
        },
        {
          "id": "MDRW2100046643.1.15",
          "form": "흥이남ㅋㅋ",
          "original_form": "흥이남ㅋㅋ",
          "speaker_id": "1",
          "time": "20211103 22:34"
        },
        {
          "id": "MDRW2100046643.1.16",
          "form": "아 그것도 edm이군요 ㅋㅋㅋ",
          "original_form": "아 그것도 edm이군요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211103 22:36"
        },
        {
          "id": "MDRW2100046643.1.17",
          "form": "edm도 엄청 다양한가봐요",
          "original_form": "edm도 엄청 다양한가봐요",
          "speaker_id": "2",
          "time": "20211103 22:36"
        },
        {
          "id": "MDRW2100046643.1.18",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211103 22:36"
        },
        {
          "id": "MDRW2100046643.1.19",
          "form": "옛날엔 클럽 음악만 edm이라고 생각했는데",
          "original_form": "옛날엔 클럽 음악만 edm이라고 생각했는데",
          "speaker_id": "2",
          "time": "20211103 22:36"
        },
        {
          "id": "MDRW2100046643.1.20",
          "form": "근데 뭐 어차피 다 같은 음악이니까 ㅋㅋ",
          "original_form": "근데 뭐 어차피 다 같은 음악이니까 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211103 22:36"
        },
        {
          "id": "MDRW2100046643.1.21",
          "form": "장르가 중요한가요 ㅋㅋ",
          "original_form": "장르가 중요한가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211103 22:37"
        },
        {
          "id": "MDRW2100046643.1.22",
          "form": "그렇긴 해요 ㅋㅋㅋ",
          "original_form": "그렇긴 해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211103 22:37"
        },
        {
