        {
          "id": "MDRW2100002331.1.1",
          "form": "너정도면 sk실트론에 입사 가능하지 않을끠·",
          "original_form": "너정도면 sk실트론에 입사 가능하지 않을끠·",
          "speaker_id": "2",
          "time": "20210820 09:38"
        },
        {
          "id": "MDRW2100002331.1.2",
          "form": "와이프는 대충 중소기업 사무직가고",
          "original_form": "와이프는 대충 중소기업 사무직가고",
          "speaker_id": "2",
          "time": "20210820 09:38"
        },
        {
          "id": "MDRW2100002331.1.3",
          "form": "와이프  엔씨에스 강의 끊어주려고",
          "original_form": "와이프  엔씨에스 강의 끊어주려고",
          "speaker_id": "1",
          "time": "20210820 09:40"
        },
        {
          "id": "MDRW2100002331.1.4",
          "form": "울산에 솔직히 직장은 많아",
          "original_form": "울산에 솔직히 직장은 많아",
          "speaker_id": "1",
          "time": "20210820 09:40"
        },
        {
          "id": "MDRW2100002331.1.5",
          "form": "중소기업도 많고",
          "original_form": "중소기업도 많고",
          "speaker_id": "1",
          "time": "20210820 09:40"
        },
        {
          "id": "MDRW2100002331.1.6",
          "form": "울산 경기 다 살아남?",
          "original_form": "울산 경기 다 살아남?",
          "speaker_id": "2",
          "time": "20210820 09:41"
        },
        {
          "id": "MDRW2100002331.1.7",
          "form": "너 연봉 5천은 넘지?",
          "original_form": "너 연봉 5천은 넘지?",
          "speaker_id": "2",
          "time": "20210820 09:41"
        },
        {
          "id": "MDRW2100002331.1.8",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210820 09:43"
        },
        {
          "id": "MDRW2100002331.1.9",
          "form": "괜찮네",
          "original_form": "괜찮네",
          "speaker_id": "2",
          "time": "20210820 09:43"
        },
        {
          "id": "MDRW2100002331.1.10",
          "form": "그래도 맞벌이해야하는디",
          "original_form": "그래도 맞벌이해야하는디",
          "speaker_id": "1",
          "time": "20210820 09:44"
        },
        {
          "id": "MDRW2100002331.1.11",
          "form": "다달이 대출이자 월세만해도 얼마여",
          "original_form": "다달이 대출이자 월세만해도 얼마여",
          "speaker_id": "1",
          "time": "20210820 09:44"
        },
        {
          "id": "MDRW2100002331.1.12",
          "form": "고정지출이 많아",
          "original_form": "고정지출이 많아",
          "speaker_id": "1",
          "time": "20210820 09:44"
        },
        {
          "id": "MDRW2100002331.1.13",
          "form": "통신비 대출이자 월세 아파트관리비 보험료",
          "original_form": "통신비 대출이자 월세 아파트관리비 보험료",
          "speaker_id": "1",
          "time": "20210820 09:44"
        },
        {
          "id": "MDRW2100002331.1.14",
          "form": "난 3.8정도",
          "original_form": "난 3.8정도",
          "speaker_id": "2",
          "time": "20210820 09:45"
        },
        {
          "id": "MDRW2100002331.1.15",
          "form": "근데 소득공제하면 연간 소비가 4-6천이네? 쩝",
          "original_form": "근데 소득공제하면 연간 소비가 4-6천이네? 쩝",
          "speaker_id": "2",
          "time": "20210820 09:45"
        },
        {
          "id": "MDRW2100002331.1.16",
          "form": "그게 되나",
          "original_form": "그게 되나",
          "speaker_id": "1",
          "time": "20210820 09:51"
        },
        {
          "id": "MDRW2100002331.1.17",
          "form": "소득보다 많이 쓰는게",
          "original_form": "소득보다 많이 쓰는게",
          "speaker_id": "1",
          "time": "20210820 09:51"
        },
        {
          "id": "MDRW2100002331.1.18",
          "form": "ㅇㅇ 되더라",
          "original_form": "ㅇㅇ 되더라",
          "speaker_id": "2",
          "time": "20210820 10:03"
        },
        {
          "id": "MDRW2100002331.1.19",
          "form": "퇴직금 다 까먹고",
          "original_form": "퇴직금 다 까먹고",
          "speaker_id": "2",
          "time": "20210820 10:03"
        },
        {
          "id": "MDRW2100002331.1.20",
          "form": "실비보험 대체하고",
          "original_form": "실비보험 대체하고",
          "speaker_id": "2",
          "time": "20210820 10:03"
        },
        {
          "id": "MDRW2100002331.1.21",
          "form": "아 모아둔돈...",
          "original_form": "아 모아둔돈...",
          "speaker_id": "1",
          "time": "20210820 10:03"
        },
        {
          "id": "MDRW2100002331.1.22",
          "form": "애들이제 초등학생이야?",
          "original_form": "애들이제 초등학생이야?",
          "speaker_id": "1",
          "time": "20210820 10:03"
        },
        {
          "id": "MDRW2100002331.1.23",
          "form": "6 9",
          "original_form": "6 9",
          "speaker_id": "2",
          "time": "20210820 10:03"
        },
        {
          "id": "MDRW2100002331.1.24",
          "form": "3천만원 다 사라지고",
          "original_form": "3천만원 다 사라지고",
          "speaker_id": "2",
          "time": "20210820 10:04"
        },
        {
          "id": "MDRW2100002331.1.25",
          "form": "와이프 맞벌이한대?",
          "original_form": "와이프 맞벌이한대?",
          "speaker_id": "1",
          "time": "20210820 10:04"
        },
        {
          "id": "MDRW2100002331.1.26",
          "form": "친구덕에 치과보험 이득본거 2천만원도 사라지고",
          "original_form": "친구덕에 치과보험 이득본거 2천만원도 사라지고",
          "speaker_id": "2",
          "time": "20210820 10:04"
        },
        {
          "id": "MDRW2100002331.1.27",
          "form": "애들 커야 되지",
          "original_form": "애들 커야 되지",
          "speaker_id": "2",
          "time": "20210820 10:04"
        },
        {
          "id": "MDRW2100002331.1.28",
          "form": "첫째만 고학년 되도 괜찮은데",
          "original_form": "첫째만 고학년 되도 괜찮은데",
          "speaker_id": "2",
          "time": "20210820 10:04"
        },
        {
          "id": "MDRW2100002331.1.29",
          "form": "아 친구잘뒀네",
          "original_form": "아 친구잘뒀네",
          "speaker_id": "1",
          "time": "20210820 10:05"
        },
        {
          "id": "MDRW2100002331.1.30",
          "form": "와이프도 몸이 안즣아",
          "original_form": "와이프도 몸이 안즣아",
          "speaker_id": "2",
          "time": "20210820 10:05"
        },
        {
          "id": "MDRW2100002331.1.31",
          "form": "허리랑 무릎 다나감",
          "original_form": "허리랑 무릎 다나감",
          "speaker_id": "2",
          "time": "20210820 10:05"
        },
        {
          "id": "MDRW2100002331.1.32",
          "form": "왜 어쩌다가?",
          "original_form": "왜 어쩌다가?",
          "speaker_id": "1",
          "time": "20210820 10:06"
        },
        {
          "id": "MDRW2100002331.1.33",
          "form": "원래 안좋은데",
          "original_form": "원래 안좋은데",
          "speaker_id": "2",
          "time": "20210820 10:06"
        },
        {
          "id": "MDRW2100002331.1.34",
          "form": "산후조리 잘 못해서",
          "original_form": "산후조리 잘 못해서",
          "speaker_id": "2",
          "time": "20210820 10:06"
        },
        {
          "id": "MDRW2100002331.1.35",
          "form": "아하...",
          "original_form": "아하...",
          "speaker_id": "1",
          "time": "20210820 10:06"
        },
        {
          "id": "MDRW2100002331.1.36",
          "form": "더 안좋아진것도",
          "original_form": "더 안좋아진것도",
          "speaker_id": "2",
          "time": "20210820 10:06"
        },
        {
          "id": "MDRW2100002331.1.37",
          "form": "김상 힘내",
          "original_form": "김상 힘내",
          "speaker_id": "1",
          "time": "20210820 10:06"
        },
        {
          "id": "MDRW2100002331.1.38",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210820 10:07"
        },
        {
          "id": "MDRW2100002331.1.39",
          "form": "허리",
          "original_form": "허리",
          "speaker_id": "2",
          "time": "20210820 10:07"
        },
        {
          "id": "MDRW2100002331.1.40",
          "form": "치료 중이고",
          "original_form": "치료 중이고",
          "speaker_id": "2",
          "time": "20210820 10:07"
        },
        {
          "id": "MDRW2100002331.1.41",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 10:07"
        },
        {
          "id": "MDRW2100002331.1.42",
          "form": "뭐가 나쁜말이지",
          "original_form": "뭐가 나쁜말이지",
          "speaker_id": "2",
          "time": "20210820 10:07"
        },
        {
          "id": "MDRW2100002331.1.43",
          "form": "나도 허리",
          "original_form": "나도 허리",
          "speaker_id": "2",
          "time": "20210820 10:07"
        },
        {
          "id": "MDRW2100002331.1.44",
          "form": "리치",
          "original_form": "리치",
          "speaker_id": "2",
          "time": "20210820 10:07"
        },
        {
          "id": "MDRW2100002331.1.45",
          "form": "리치료",
          "original_form": "리치료",
          "speaker_id": "2",
          "time": "20210820 10:07"
        },
        {
          "id": "MDRW2100002331.1.46",
          "form": "허리치",
          "original_form": "허리치",
          "speaker_id": "2",
          "time": "20210820 10:07"
        },
        {
          "id": "MDRW2100002331.1.47",
          "form": "나도 한가하다",
          "original_form": "나도 한가하다",
          "speaker_id": "1",
          "time": "20210820 10:08"
        },
        {
          "id": "MDRW2100002331.1.48",
          "form": "나는 목이 아파서",
          "original_form": "나는 목이 아파서",
          "speaker_id": "1",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002331.1.49",
          "form": "정형외과가서 엠알아이까지 찍었는데",
          "original_form": "정형외과가서 엠알아이까지 찍었는데",
          "speaker_id": "1",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002331.1.50",
          "form": "목디스크?",
          "original_form": "목디스크?",
          "speaker_id": "2",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002331.1.51",
          "form": "디스크 아니래",
          "original_form": "디스크 아니래",
          "speaker_id": "1",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002331.1.52",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002331.1.53",
          "form": "그런데 목은 계속 아파",
          "original_form": "그런데 목은 계속 아파",
          "speaker_id": "1",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002331.1.54",
          "form": "너 운동 잘못해서 그런거 아냐?",
          "original_form": "너 운동 잘못해서 그런거 아냐?",
          "speaker_id": "2",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002331.1.55",
          "form": "두통 동반 가능",
          "original_form": "두통 동반 가능",
          "speaker_id": "2",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002331.1.56",
          "form": "운동 때문은 아닌것같고",
          "original_form": "운동 때문은 아닌것같고",
          "speaker_id": "1",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002331.1.57",
          "form": "턱걸이 잘못하면",
          "original_form": "턱걸이 잘못하면",
          "speaker_id": "2",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002331.1.58",
          "form": "편두통은 주기적으로 와",
          "original_form": "편두통은 주기적으로 와",
          "speaker_id": "1",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002331.1.59",
          "form": "승모랑 목에 힘들어가서",
          "original_form": "승모랑 목에 힘들어가서",
          "speaker_id": "2",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002331.1.60",
          "form": "주로 왼쪽이 아픈데 가끔 오른쪽도 아파",
          "original_form": "주로 왼쪽이 아픈데 가끔 오른쪽도 아파",
          "speaker_id": "1",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002331.1.61",
          "form": "가능함",
          "original_form": "가능함",
          "speaker_id": "2",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002331.1.62",
          "form": "어떡하지",
          "original_form": "어떡하지",
          "speaker_id": "1",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002331.1.63",
          "form": "목이랑 두통 통증",
          "original_form": "목이랑 두통 통증",
          "speaker_id": "2",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002331.1.64",
          "form": "운동 하지마",
          "original_form": "운동 하지마",
          "speaker_id": "2",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002331.1.65",
          "form": "턱걸이 푸쉬업 하고 있다면",
          "original_form": "턱걸이 푸쉬업 하고 있다면",
          "speaker_id": "2",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002331.1.66",
          "form": "자세 교정 받고 한동안 쉬어",
          "original_form": "자세 교정 받고 한동안 쉬어",
          "speaker_id": "2",
          "time": "20210820 10:11"
        },
        {
          "id": "MDRW2100002331.1.67",
          "form": "스트레칭만하고",
          "original_form": "스트레칭만하고",
          "speaker_id": "2",
          "time": "20210820 10:11"
        },
        {
          "id": "MDRW2100002331.1.68",
          "form": "스쿼트는?",
          "original_form": "스쿼트는?",
          "speaker_id": "1",
          "time": "20210820 10:11"
        },
        {
          "id": "MDRW2100002331.1.69",
          "form": "괜찮을거 같은데",
          "original_form": "괜찮을거 같은데",
          "speaker_id": "2",
          "time": "20210820 10:11"
        },
        {
          "id": "MDRW2100002331.1.70",
          "form": "연관 근육 없어서",
          "original_form": "연관 근육 없어서",
          "speaker_id": "2",
          "time": "20210820 10:11"
        },
        {
          "id": "MDRW2100002331.1.71",
          "form": "힘 줄 때",
          "original_form": "힘 줄 때",
          "speaker_id": "2",
          "time": "20210820 10:11"
        },
        {
