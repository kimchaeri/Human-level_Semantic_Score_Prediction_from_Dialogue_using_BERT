          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006316.1.3",
          "form": "기독교입니다",
          "original_form": "기독교입니다",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006316.1.4",
          "form": "name1은요",
          "original_form": "&name1&은요",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006316.1.5",
          "form": "저는 종교 없어요",
          "original_form": "저는 종교 없어요",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006316.1.6",
          "form": "개인적으로 종교는 사람들이 도덕적으로 살아가는데 도움을 준다고 생각해요",
          "original_form": "개인적으로 종교는 사람들이 도덕적으로 살아가는데 도움을 준다고 생각해요",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006316.1.7",
          "form": "성경이든 불경이든 바르게 살라고 하잖아요",
          "original_form": "성경이든 불경이든 바르게 살라고 하잖아요",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006316.1.8",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006316.1.9",
          "form": "저는 종교가",
          "original_form": "저는 종교가",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006316.1.10",
          "form": "뭔가 자기가 잘못한일 뒤집어씌우려고",
          "original_form": "뭔가 자기가 잘못한일 뒤집어씌우려고",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006316.1.11",
          "form": "만든거같아요!",
          "original_form": "만든거같아요!",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006316.1.12",
          "form": "잘되면 내덕",
          "original_form": "잘되면 내덕",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006316.1.13",
          "form": "잘안되면 신때문!",
          "original_form": "잘안되면 신때문!",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006316.1.14",
          "form": "아닌것 같아요",
          "original_form": "아닌것 같아요",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006316.1.15",
          "form": "그건ㅋㅋㅋㅋㅋ",
          "original_form": "그건ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006316.1.16",
          "form": "name2 신앙심 깊으신가요?",
          "original_form": "&name2& 신앙심 깊으신가요?",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006316.1.17",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006316.1.18",
          "form": "아뇨 ㅋ큐큐ㅠㅠ",
          "original_form": "아뇨 ㅋ큐큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006316.1.19",
          "form": "깊으면",
          "original_form": "깊으면",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006316.1.20",
          "form": "이런말을할리가없겠죵",
          "original_form": "이런말을할리가없겠죵",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006316.1.21",
          "form": "뭔가 종교는 일단 결국 정치 아닐까요",
          "original_form": "뭔가 종교는 일단 결국 정치 아닐까요",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006316.1.22",
          "form": "그쵸ㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006316.1.23",
          "form": "힘있는사람들이",
          "original_form": "힘있는사람들이",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006316.1.24",
          "form": "자신들의 말을 좀더",
          "original_form": "자신들의 말을 좀더",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006316.1.25",
          "form": "힘잇게 전달하려고",
          "original_form": "힘잇게 전달하려고",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006316.1.26",
          "form": "신을 모방한 사람들을 이용해서",
          "original_form": "신을 모방한 사람들을 이용해서",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006316.1.27",
          "form": "자신들의 뜻을따르게하기위해서",
          "original_form": "자신들의 뜻을따르게하기위해서",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006316.1.28",
          "form": "만든 하나의 사회라고 생각하네용",
          "original_form": "만든 하나의 사회라고 생각하네용",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006316.1.29",
          "form": "음 그럼요 종교를 그런식으로 사용하는 사람들도 있으니까",
          "original_form": "음 그럼요 종교를 그런식으로 사용하는 사람들도 있으니까",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006316.1.30",
          "form": "예아...",
          "original_form": "예아...",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006316.1.31",
          "form": "name1은",
          "original_form": "&name1&은",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006316.1.32",
          "form": "어떻게생각하세용",
          "original_form": "어떻게생각하세용",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006316.1.33",
          "form": "종교가 왜만들어졌을까요?!",
          "original_form": "종교가 왜만들어졌을까요?!",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006316.1.34",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006316.1.35",
          "form": "그거 책인가 어디선가 본것 같은데 인간이 자연재해나 고난으로부터",
          "original_form": "그거 책인가 어디선가 본것 같은데 인간이 자연재해나 고난으로부터",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100006316.1.36",
          "form": "벗어나기 위한 주술같은거에서 시작된거 아닌가요",
          "original_form": "벗어나기 위한 주술같은거에서 시작된거 아닌가요",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006316.1.37",
          "form": "옛날에는 곰도 믿고 호랑이도 믿고",
          "original_form": "옛날에는 곰도 믿고 호랑이도 믿고",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006316.1.38",
          "form": "바위도 믿고 나무도 믿고 했다잖아요",
          "original_form": "바위도 믿고 나무도 믿고 했다잖아요",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006316.1.39",
          "form": "뉴스보면 가끔 종교인들 사고친거 나와서 이미지 안좋읏데",
          "original_form": "뉴스보면 가끔 종교인들 사고친거 나와서 이미지 안좋읏데",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100006316.1.40",
          "form": "제 주변 종교인들은 되게 바르고 선한 사람들이던데",
          "original_form": "제 주변 종교인들은 되게 바르고 선한 사람들이던데",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100006316.1.41",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006316.1.42",
          "form": "그춍",
          "original_form": "그춍",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006316.1.43",
          "form": "바르고선한사람들많아용",
          "original_form": "바르고선한사람들많아용",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006316.1.44",
          "form": "근데 그 기원알살펴본다면",
          "original_form": "근데 그 기원알살펴본다면",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006316.1.45",
          "form": "기원을살펴본다면어떨까요",
          "original_form": "기원을살펴본다면어떨까요",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006316.1.46",
          "form": "천주교라던가",
          "original_form": "천주교라던가",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006316.1.47",
          "form": "성경의 기원이라던가",
          "original_form": "성경의 기원이라던가",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006316.1.48",
          "form": "응? 어떤거요?",
          "original_form": "응? 어떤거요?",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006316.1.49",
          "form": "성경이",
          "original_form": "성경이",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006316.1.50",
          "form": "왜써졌을까요",
          "original_form": "왜써졌을까요",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006316.1.51",
          "form": "누가썼을까요?!",
          "original_form": "누가썼을까요?!",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006316.1.52",
          "form": "아 그러게요",
          "original_form": "아 그러게요",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006316.1.53",
          "form": "누굴까요",
          "original_form": "누굴까요",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006316.1.54",
          "form": "흠...",
          "original_form": "흠...",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006316.1.55",
          "form": "넘신기하지않나요",
          "original_form": "넘신기하지않나요",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006316.1.56",
          "form": "왜 썼을까",
          "original_form": "왜 썼을까",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006316.1.57",
          "form": "양도 방대한대",
          "original_form": "양도 방대한대",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006316.1.58",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006316.1.59",
          "form": "뭔가",
          "original_form": "뭔가",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006316.1.60",
          "form": "역사서라고할수도있는데",
          "original_form": "역사서라고할수도있는데",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006316.1.61",
          "form": "역사는 원래 힘있는자들이 쓰는거잖아요",
          "original_form": "역사는 원래 힘있는자들이 쓰는거잖아요",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006316.1.62",
          "form": "아마도 힘있는자들이",
          "original_form": "아마도 힘있는자들이",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006316.1.63",
          "form": "자신들의 위대함을",
          "original_form": "자신들의 위대함을",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006316.1.64",
          "form": "은근 나타낸게아닐까용",
          "original_form": "은근 나타낸게아닐까용",
          "speaker_id": "2",
