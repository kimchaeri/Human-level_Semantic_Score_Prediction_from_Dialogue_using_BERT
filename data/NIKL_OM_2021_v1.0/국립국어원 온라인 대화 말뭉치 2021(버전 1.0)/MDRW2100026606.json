      "utterance": [
        {
          "id": "MDRW2100026606.1.1",
          "form": "싸이월드부활정말무섭네요",
          "original_form": "싸이월드부활정말무섭네요",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026606.1.2",
          "form": "뉴스에서봤어요",
          "original_form": "뉴스에서봤어요",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026606.1.3",
          "form": "그오래된 사이트가부활한다니;;",
          "original_form": "그오래된 사이트가부활한다니;;",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026606.1.4",
          "form": "많은자료가있는곳이죠?",
          "original_form": "많은자료가있는곳이죠?",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026606.1.5",
          "form": "숨기고싶은곳이죠",
          "original_form": "숨기고싶은곳이죠",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026606.1.6",
          "form": "그렇게숨길게있나요",
          "original_form": "그렇게숨길게있나요",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026606.1.7",
          "form": "어릴때사진이라던가글...지금도부끄럽네요",
          "original_form": "어릴때사진이라던가글...지금도부끄럽네요",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026606.1.8",
          "form": "귀여울것같은데 백업하기싫나요",
          "original_form": "귀여울것같은데 백업하기싫나요",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026606.1.9",
          "form": "네 그냥사라지는게 편하죠",
          "original_form": "네 그냥사라지는게 편하죠",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026606.1.10",
          "form": "아이고 인터넷에 자료가있다는건 무서운일이군요",
          "original_form": "아이고 인터넷에 자료가있다는건 무서운일이군요",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026606.1.11",
          "form": "네 누군가는반가워도 반갑지않은사람도있죠",
          "original_form": "네 누군가는반가워도 반갑지않은사람도있죠",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026606.1.12",
          "form": "반갑지않아도 어쩌겠어요",
          "original_form": "반갑지않아도 어쩌겠어요",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026606.1.13",
          "form": "네 빨리자료를없애야죠",
          "original_form": "네 빨리자료를없애야죠",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026606.1.14",
          "form": "응원할게요 다름사람이안보길",
          "original_form": "응원할게요 다름사람이안보길",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026606.1.15",
          "form": "네감사합니다 무슨고생인지싶어요",
          "original_form": "네감사합니다 무슨고생인지싶어요",
          "speaker_id": "1",
          "time": "20210901 23:17"
        }
