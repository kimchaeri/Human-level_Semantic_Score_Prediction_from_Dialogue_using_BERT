      "utterance": [
        {
          "id": "MDRW2100024460.1.1",
          "form": "집안일좀 해요?",
          "original_form": "집안일좀 해요?",
          "speaker_id": "1",
          "time": "20210831 10:36"
        },
        {
          "id": "MDRW2100024460.1.2",
          "form": "아씨",
          "original_form": "아씨",
          "speaker_id": "2",
          "time": "20210831 10:37"
        },
        {
          "id": "MDRW2100024460.1.3",
          "form": "저 발에 뭐 밟히는거",
          "original_form": "저 발에 뭐 밟히는거",
          "speaker_id": "2",
          "time": "20210831 10:37"
        },
        {
          "id": "MDRW2100024460.1.4",
          "form": "극.혐함",
          "original_form": "극.혐함",
          "speaker_id": "2",
          "time": "20210831 10:37"
        },
        {
          "id": "MDRW2100024460.1.5",
          "form": "오 집 되게 깔끔하겠따 그러면",
          "original_form": "오 집 되게 깔끔하겠따 그러면",
          "speaker_id": "1",
          "time": "20210831 10:37"
        },
        {
          "id": "MDRW2100024460.1.6",
          "form": "돌돌이 필수 ㄹㅇ",
          "original_form": "돌돌이 필수 ㄹㅇ",
          "speaker_id": "1",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024460.1.7",
          "form": "방에 있음ㅋㅋ",
          "original_form": "방에 있음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024460.1.8",
          "form": "되게 깔끔...은 아니구요",
          "original_form": "되게 깔끔...은 아니구요",
          "speaker_id": "2",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024460.1.9",
          "form": "저도 발에 뭐 밟히는거 싫어함",
          "original_form": "저도 발에 뭐 밟히는거 싫어함",
          "speaker_id": "1",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100024460.1.10",
          "form": "남친자취할때 맨날청소만함",
          "original_form": "남친자취할때 맨날청소만함",
          "speaker_id": "1",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100024460.1.11",
          "form": "천사네",
          "original_form": "천사네",
          "speaker_id": "2",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100024460.1.12",
          "form": "와서 청소해주고",
          "original_form": "와서 청소해주고",
          "speaker_id": "2",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100024460.1.13",
          "form": "밥만안햇음 ㅋㅋㅋㅋ",
          "original_form": "밥만안햇음 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100024460.1.14",
          "form": "생각해보니까 요리는 다 남친이했네",
          "original_form": "생각해보니까 요리는 다 남친이했네",
          "speaker_id": "1",
          "time": "20210831 10:40"
        },
        {
          "id": "MDRW2100024460.1.15",
          "form": "전 제가 해다먹였는데",
          "original_form": "전 제가 해다먹였는데",
          "speaker_id": "2",
          "time": "20210831 10:40"
        },
        {
          "id": "MDRW2100024460.1.16",
          "form": "오 같네요",
          "original_form": "오 같네요",
          "speaker_id": "2",
          "time": "20210831 10:40"
        },
        {
          "id": "MDRW2100024460.1.17",
          "form": "오 요리좀하나봐요",
          "original_form": "오 요리좀하나봐요",
          "speaker_id": "1",
          "time": "20210831 10:40"
        },
        {
          "id": "MDRW2100024460.1.18",
          "form": "좋아하는데요",
          "original_form": "좋아하는데요",
          "speaker_id": "2",
          "time": "20210831 10:40"
        },
        {
          "id": "MDRW2100024460.1.19",
          "form": "설거지가 싫어요",
          "original_form": "설거지가 싫어요",
          "speaker_id": "2",
          "time": "20210831 10:40"
        },
        {
          "id": "MDRW2100024460.1.20",
          "form": "요리해주면 설거지는 해야죠",
          "original_form": "요리해주면 설거지는 해야죠",
          "speaker_id": "1",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100024460.1.21",
          "form": "그건 국룰아닌가",
          "original_form": "그건 국룰아닌가",
          "speaker_id": "1",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100024460.1.22",
          "form": "근데 또 말이 그렇지",
          "original_form": "근데 또 말이 그렇지",
          "speaker_id": "2",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100024460.1.23",
          "form": "하다보면 글케 안댐",
          "original_form": "하다보면 글케 안댐",
          "speaker_id": "2",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100024460.1.24",
          "form": "저희는 항상 가위바위보함",
          "original_form": "저희는 항상 가위바위보함",
          "speaker_id": "1",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100024460.1.25",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100024460.1.26",
          "form": "묵찌빠를 더많이한듯",
          "original_form": "묵찌빠를 더많이한듯",
          "speaker_id": "1",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100024460.1.27",
          "form": "가위바위보 하다가 지면 제가 묵찌빠로 급바꿈ㅋㅋㅋㅋㅋㅋ",
          "original_form": "가위바위보 하다가 지면 제가 묵찌빠로 급바꿈ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100024460.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100024460.1.29",
          "form": "이바이바..",
          "original_form": "이바이바..",
          "speaker_id": "2",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100024460.1.30",
          "form": "아니근데 남친은 설거지하면",
          "original_form": "아니근데 남친은 설거지하면",
          "speaker_id": "1",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100024460.1.31",
          "form": "이게 일케된다니까요",
          "original_form": "이게 일케된다니까요",
          "speaker_id": "2",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100024460.1.32",
          "form": "설거지만해서 문제",
          "original_form": "설거지만해서 문제",
          "speaker_id": "1",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100024460.1.33",
          "form": "요리하다가 옆에튄거잇으면 같이닦아야지",
          "original_form": "요리하다가 옆에튄거잇으면 같이닦아야지",
          "speaker_id": "1",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100024460.1.34",
          "form": "설거지만함",
          "original_form": "설거지만함",
          "speaker_id": "1",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100024460.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:43"
        },
        {
          "id": "MDRW2100024460.1.36",
          "form": "남의 집에서 가스렌지 뒤집어엎은적 있음ㅋㅋㅋㅋㅋ",
          "original_form": "남의 집에서 가스렌지 뒤집어엎은적 있음ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:43"
        },
        {
          "id": "MDRW2100024460.1.37",
          "form": "설거지하다가ㅋㅋㅋㅋ",
          "original_form": "설거지하다가ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:43"
        },
        {
          "id": "MDRW2100024460.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:43"
        },
        {
          "id": "MDRW2100024460.1.39",
          "form": "성격비슷하니 말이잘통하네요",
          "original_form": "성격비슷하니 말이잘통하네요",
          "speaker_id": "1",
          "time": "20210831 10:43"
        },
        {
          "id": "MDRW2100024460.1.40",
          "form": "화장실청소는 진짜하기싫던데",
          "original_form": "화장실청소는 진짜하기싫던데",
          "speaker_id": "1",
          "time": "20210831 10:44"
        },
        {
          "id": "MDRW2100024460.1.41",
          "form": "저 원래 별생각 안하고 살았는데",
          "original_form": "저 원래 별생각 안하고 살았는데",
          "speaker_id": "2",
          "time": "20210831 10:44"
        },
        {
          "id": "MDRW2100024460.1.42",
          "form": "군대가 의무대였잖아요",
          "original_form": "군대가 의무대였잖아요",
          "speaker_id": "2",
          "time": "20210831 10:44"
        },
        {
          "id": "MDRW2100024460.1.43",
          "form": "갔다와서 결벽증이 째꼼 생긴듯",
          "original_form": "갔다와서 결벽증이 째꼼 생긴듯",
          "speaker_id": "2",
          "time": "20210831 10:44"
        },
        {
          "id": "MDRW2100024460.1.44",
          "form": "아 그런게 또 있구나",
          "original_form": "아 그런게 또 있구나",
          "speaker_id": "1",
          "time": "20210831 10:44"
        },
        {
          "id": "MDRW2100024460.1.45",
          "form": "화장실도 어느 정도는 깨끗해야해서",
          "original_form": "화장실도 어느 정도는 깨끗해야해서",
          "speaker_id": "2",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100024460.1.46",
          "form": "적당히 치우고는 살아요",
          "original_form": "적당히 치우고는 살아요",
          "speaker_id": "2",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100024460.1.47",
          "form": "그래도 화장실청소는 깨끗하게 해놓더라구여 저 갈때는 ㅋㅋㅋㅋ",
          "original_form": "그래도 화장실청소는 깨끗하게 해놓더라구여 저 갈때는 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100024460.1.48",
          "form": "바닥에서 빛남",
          "original_form": "바닥에서 빛남",
          "speaker_id": "1",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100024460.1.49",
          "form": "친구네 놀러가면 ...이러고 어케 살지 하고 닦아서 쓰고",
          "original_form": "친구네 놀러가면 ...이러고 어케 살지 하고 닦아서 쓰고",
          "speaker_id": "2",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100024460.1.50",
          "form": "와 그정도로요?",
          "original_form": "와 그정도로요?",
          "speaker_id": "2",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100024460.1.51",
          "form": "화장실에 창문없어서 곰팡이진짜 개쩔드라구여",
          "original_form": "화장실에 창문없어서 곰팡이진짜 개쩔드라구여",
          "speaker_id": "1",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100024460.1.52",
          "form": "나름 청결하시니",
          "original_form": "나름 청결하시니",
          "speaker_id": "2",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100024460.1.53",
          "form": "그거 관리 잘하면 되는데 문 열어두고",
          "original_form": "그거 관리 잘하면 되는데 문 열어두고",
          "speaker_id": "2",
          "time": "20210831 10:46"
        },
        {
          "id": "MDRW2100024460.1.54",
          "form": "안깔끔했으면",
          "original_form": "안깔끔했으면",
          "speaker_id": "1",
          "time": "20210831 10:46"
        },
        {
          "id": "MDRW2100024460.1.55",
          "form": "자취방갈때마다",
          "original_form": "자취방갈때마다",
          "speaker_id": "1",
          "time": "20210831 10:46"
        },
        {
          "id": "MDRW2100024460.1.56",
          "form": "개,고생했을듯",
          "original_form": "개,고생했을듯",
          "speaker_id": "1",
          "time": "20210831 10:46"
        },
        {
          "id": "MDRW2100024460.1.57",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:46"
        },
        {
          "id": "MDRW2100024460.1.58",
          "form": "청소아주머니",
          "original_form": "청소아주머니",
          "speaker_id": "2",
          "time": "20210831 10:46"
        }
      ]
