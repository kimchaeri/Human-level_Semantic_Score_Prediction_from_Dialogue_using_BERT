        },
        {
          "id": "MDRW2100042430.1.2",
          "form": "하셨나요?",
          "original_form": "하셨나요?",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042430.1.3",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042430.1.4",
          "form": "거기도 비왔나요?!",
          "original_form": "거기도 비왔나요?!",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042430.1.5",
          "form": "네 비가 오네요 ㅎ",
          "original_form": "네 비가 오네요 ㅎ",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042430.1.6",
          "form": "거기도 오나요 ?",
          "original_form": "거기도 오나요 ?",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042430.1.7",
          "form": "네ㅠ 옵니다ㅠ",
          "original_form": "네ㅠ 옵니다ㅠ",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042430.1.8",
          "form": "내일도 온다고하네요",
          "original_form": "내일도 온다고하네요",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042430.1.9",
          "form": "헉 내일도요?!",
          "original_form": "헉 내일도요?!",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042430.1.10",
          "form": "비싫어해요 ?",
          "original_form": "비싫어해요 ?",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
