      "utterance": [
        {
          "id": "MDRW2100021953.1.1",
          "form": "추석에 집에간다고?",
          "original_form": "추석에 집에간다고?",
          "speaker_id": "2",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021953.1.2",
          "form": "집안일은 해도해도 끝이안나는듯..",
          "original_form": "집안일은 해도해도 끝이안나는듯..",
          "speaker_id": "1",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021953.1.3",
          "form": "엉ㅋㅋ",
          "original_form": "엉ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021953.1.4",
          "form": "집안일은 원래ㅋㅋ",
          "original_form": "집안일은 원래ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021953.1.5",
          "form": "머타고 가?",
          "original_form": "머타고 가?",
          "speaker_id": "2",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021953.1.6",
          "form": "가능하면 기차 신청할려고",
          "original_form": "가능하면 기차 신청할려고",
          "speaker_id": "1",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021953.1.7",
          "form": "오 케틱스?",
          "original_form": "오 케틱스?",
          "speaker_id": "2",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021953.1.8",
          "form": "안되면..다음날 도전하거나 버스타겟지..",
          "original_form": "안되면..다음날 도전하거나 버스타겟지..",
          "speaker_id": "1",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021953.1.9",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021953.1.10",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021953.1.11",
          "form": "수요일이네 그럼 9시",
          "original_form": "수요일이네 그럼 9시",
          "speaker_id": "2",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021953.1.12",
          "form": "수요일인가",
          "original_form": "수요일인가",
          "speaker_id": "1",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021953.1.13",
          "form": "조카볼생각에 떨리겟다 너",
          "original_form": "조카볼생각에 떨리겟다 너",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021953.1.14",
          "form": "아마도 가족만나러가는건 진짜 간만",
          "original_form": "아마도 가족만나러가는건 진짜 간만",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021953.1.15",
          "form": "떨리지는...x",
          "original_form": "떨리지는...x",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021953.1.16",
          "form": "니도 프사바뀌는거 아냐??",
          "original_form": "니도 프사바뀌는거 아냐??",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021953.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021953.1.18",
          "form": "아니ㅋㅋㅋㅋㅋㅋ 절대ㅋㅋㅋ",
          "original_form": "아니ㅋㅋㅋㅋㅋㅋ 절대ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021953.1.19",
          "form": "절대란 없는것... 가랏 조카몬",
          "original_form": "절대란 없는것... 가랏 조카몬",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021953.1.20",
          "form": "조카는 그저 오빠 아들일뿐... 내새꾸는 아니란말이야",
          "original_form": "조카는 그저 오빠 아들일뿐... 내새꾸는 아니란말이야",
          "speaker_id": "2",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021953.1.21",
          "form": "그렇긴하지..",
          "original_form": "그렇긴하지..",
          "speaker_id": "1",
