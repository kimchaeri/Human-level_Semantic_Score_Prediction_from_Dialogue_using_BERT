      "utterance": [
        {
          "id": "MDRW2100030690.1.1",
          "form": "다음에 베트남으로 또 갈 건가요",
          "original_form": "다음에 베트남으로 또 갈 건가요",
          "speaker_id": "2",
          "time": "20210904 11:28"
        },
        {
          "id": "MDRW2100030690.1.2",
          "form": "그래서 사람들이 게으른가 봐요",
          "original_form": "그래서 사람들이 게으른가 봐요",
          "speaker_id": "1",
          "time": "20210904 11:28"
        },
        {
          "id": "MDRW2100030690.1.3",
          "form": "베트남은 익숙하니까 언제든지 갈 수 있지요",
          "original_form": "베트남은 익숙하니까 언제든지 갈 수 있지요",
          "speaker_id": "1",
          "time": "20210904 11:29"
        },
        {
          "id": "MDRW2100030690.1.4",
          "form": "더우면 게으르게 되기 마련이죠",
          "original_form": "더우면 게으르게 되기 마련이죠",
          "speaker_id": "2",
          "time": "20210904 11:29"
        },
        {
          "id": "MDRW2100030690.1.5",
          "form": "정말 좋아하는 음식은 한국에는 없어요",
          "original_form": "정말 좋아하는 음식은 한국에는 없어요",
          "speaker_id": "1",
          "time": "20210904 11:29"
        },
        {
          "id": "MDRW2100030690.1.6",
          "form": "코로나가 빨리 끝나야 할 텐데 말이죠",
          "original_form": "코로나가 빨리 끝나야 할 텐데 말이죠",
          "speaker_id": "2",
          "time": "20210904 11:29"
        },
        {
          "id": "MDRW2100030690.1.7",
          "form": "먹고 싶으면 어쩌죠",
          "original_form": "먹고 싶으면 어쩌죠",
          "speaker_id": "1",
          "time": "20210904 11:29"
        },
        {
          "id": "MDRW2100030690.1.8",
          "form": "어떤 음식 좋아하는데요",
          "original_form": "어떤 음식 좋아하는데요",
          "speaker_id": "2",
          "time": "20210904 11:29"
        },
        {
          "id": "MDRW2100030690.1.9",
          "form": "두부랑 새우젓이랑 쌀국수여",
          "original_form": "두부랑 새우젓이랑 쌀국수여",
          "speaker_id": "1",
          "time": "20210904 11:29"
        },
        {
          "id": "MDRW2100030690.1.10",
          "form": "섞여 나오는 음식이 있어요",
          "original_form": "섞여 나오는 음식이 있어요",
          "speaker_id": "1",
          "time": "20210904 11:29"
        },
        {
          "id": "MDRW2100030690.1.11",
          "form": "쌀국수에 진심 이콩교",
          "original_form": "쌀국수에 진심 이콩교",
          "speaker_id": "2",
          "time": "20210904 11:29"
        },
        {
          "id": "MDRW2100030690.1.12",
          "form": "분더우맘똠",
          "original_form": "분더우맘똠",
          "speaker_id": "1",
          "time": "20210904 11:29"
        },
        {
          "id": "MDRW2100030690.1.13",
          "form": "분더우맘똠",
          "original_form": "분더우맘똠",
          "speaker_id": "2",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100030690.1.14",
          "form": "분은 국수 더우는 두부 맘똠은 젓갈",
          "original_form": "분은 국수 더우는 두부 맘똠은 젓갈",
          "speaker_id": "1",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100030690.1.15",
          "form": "발음 신기하네요",
          "original_form": "발음 신기하네요",
          "speaker_id": "2",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100030690.1.16",
          "form": "젓갈에 두부랑 국수를 찍어 먹어요",
          "original_form": "젓갈에 두부랑 국수를 찍어 먹어요",
          "speaker_id": "1",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100030690.1.17",
          "form": "너는 젓갈 들어가는 음식을 잘 못 먹어요",
          "original_form": "너는 젓갈 들어가는 음식을 잘 못 먹어요",
          "speaker_id": "2",
          "time": "20210904 11:30"
