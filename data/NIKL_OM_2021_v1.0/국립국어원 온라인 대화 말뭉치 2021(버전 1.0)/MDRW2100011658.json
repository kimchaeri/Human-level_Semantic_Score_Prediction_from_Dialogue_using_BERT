      "utterance": [
        {
          "id": "MDRW2100011658.1.1",
          "form": "집안일 많나?",
          "original_form": "집안일 많나?",
          "speaker_id": "1",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011658.1.2",
          "form": "집안일 많아?",
          "original_form": "집안일 많아?",
          "speaker_id": "1",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100011658.1.3",
          "form": "많다 3번이나 밥차려야하고",
          "original_form": "많다 3번이나 밥차려야하고",
          "speaker_id": "2",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100011658.1.4",
          "form": "3끼나 먹어?",
          "original_form": "3끼나 먹어?",
          "speaker_id": "1",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100011658.1.5",
          "form": "빨래도 많고..",
          "original_form": "빨래도 많고..",
          "speaker_id": "2",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100011658.1.6",
          "form": "점심엔 간단히..",
          "original_form": "점심엔 간단히..",
          "speaker_id": "2",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100011658.1.7",
          "form": "방학이라 애도 봐야하고",
          "original_form": "방학이라 애도 봐야하고",
          "speaker_id": "2",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100011658.1.8",
          "form": "점심 뭐 먹었어?",
          "original_form": "점심 뭐 먹었어?",
          "speaker_id": "1",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100011658.1.9",
          "form": "떡볶이 해먹었지",
          "original_form": "떡볶이 해먹었지",
          "speaker_id": "2",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100011658.1.10",
          "form": "잘 먹던?",
          "original_form": "잘 먹던?",
          "speaker_id": "1",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100011658.1.11",
          "form": "ㅇㅇ맛있다했어",
          "original_form": "ㅇㅇ맛있다했어",
          "speaker_id": "2",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100011658.1.12",
          "form": "요즘 요리 잘 하네",
          "original_form": "요즘 요리 잘 하네",
          "speaker_id": "1",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100011658.1.13",
          "form": "애들은 집에있나",
          "original_form": "애들은 집에있나",
          "speaker_id": "2",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100011658.1.14",
          "form": "학원갔어",
          "original_form": "학원갔어",
          "speaker_id": "1",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100011658.1.15",
          "form": "우린 학원수업...온라인으로 한다",
          "original_form": "우린 학원수업...온라인으로 한다",
          "speaker_id": "2",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100011658.1.16",
          "form": "맞나?ㅜ온라인 좋아해? 니가 힘들겠다ㅜ",
          "original_form": "맞나?ㅜ온라인 좋아해? 니가 힘들겠다ㅜ",
          "speaker_id": "1",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100011658.1.17",
          "form": "학원가고나면.. 집이조용하겠네",
          "original_form": "학원가고나면.. 집이조용하겠네",
          "speaker_id": "2",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100011658.1.18",
          "form": "그치 육아해방",
          "original_form": "그치 육아해방",
          "speaker_id": "1",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100011658.1.19",
          "form": "온라인하면...애들은 좋아하지만 내가 힘듦",
          "original_form": "온라인하면...애들은 좋아하지만 내가 힘듦",
          "speaker_id": "2",
          "time": "20210824 14:57"
        }
      ]
