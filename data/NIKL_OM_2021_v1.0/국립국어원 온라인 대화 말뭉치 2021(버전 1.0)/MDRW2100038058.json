      "utterance": [
        {
          "id": "MDRW2100038058.1.1",
          "form": "복숭아배터지겟다",
          "original_form": "복숭아배터지겟다",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100038058.1.2",
          "form": "세리머니클럽알아?",
          "original_form": "세리머니클럽알아?",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100038058.1.3",
          "form": "ㅋㅋ몇개먹었슈",
          "original_form": "ㅋㅋ몇개먹었슈",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100038058.1.4",
          "form": "ㅋㅋ골프 치는거지? 재밌어?",
          "original_form": "ㅋㅋ골프 치는거지? 재밌어?",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100038058.1.5",
          "form": "ㅌㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038058.1.6",
          "form": "6개",
          "original_form": "6개",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038058.1.7",
          "form": "나 저번에 골프왕? 그거 잠깐 봤는데 웃기던뎅ㅋㅋㅋ 박상면, 윤다훈 게스트가 웃겨서 그런가",
          "original_form": "나 저번에 골프왕? 그거 잠깐 봤는데 웃기던뎅ㅋㅋㅋ 박상면, 윤다훈 게스트가 웃겨서 그런가",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038058.1.8",
          "form": "깎은거 같음",
          "original_form": "깎은거 같음",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038058.1.9",
          "form": "ㅋㅋㅋㅋ골프프로 많지?",
          "original_form": "ㅋㅋㅋㅋ골프프로 많지?",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038058.1.10",
          "form": "진짜 많아짐ㅋㅋㅋ",
          "original_form": "진짜 많아짐ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038058.1.11",
          "form": "그만큼 대중적인 스포츠가 된건가",
          "original_form": "그만큼 대중적인 스포츠가 된건가",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038058.1.12",
          "form": "그니깐",
          "original_form": "그니깐",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100038058.1.13",
          "form": "개그맨들도 많이혀",
          "original_form": "개그맨들도 많이혀",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038058.1.14",
          "form": "진짜 연예인 벌이는 다르니깐..쳇 ㅋㅋㅋ",
          "original_form": "진짜 연예인 벌이는 다르니깐..쳇 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038058.1.15",
          "form": "ㅠㅠ 그러게",
          "original_form": "ㅠㅠ 그러게",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100038058.1.16",
          "form": "필드나가는데",
          "original_form": "필드나가는데",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038058.1.17",
          "form": "엄청 비싸겠지",
          "original_form": "엄청 비싸겠지",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038058.1.18",
          "form": "난 또 언제 잠들지 모르겠어 미리 알림ㅋㅋㅋㅋ",
          "original_form": "난 또 언제 잠들지 모르겠어 미리 알림ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038058.1.19",
          "form": "ㅋㅋ그럴걸!! 싸게 가도 자주 나가면...",
          "original_form": "ㅋㅋ그럴걸!! 싸게 가도 자주 나가면...",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038058.1.20",
          "form": "ㅋㅋㅋㅋㅋ언2제든",
          "original_form": "ㅋㅋㅋㅋㅋ언2제든",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038058.1.21",
          "form": "편히가 ㅋㅋㅋ",
          "original_form": "편히가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038058.1.22",
          "form": "난쇼파에 찌그러져잇어",
          "original_form": "난쇼파에 찌그러져잇어",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038058.1.23",
          "form": "ㅋㅋ지금은 뭐보ㅏ?",
          "original_form": "ㅋㅋ지금은 뭐보ㅏ?",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038058.1.24",
          "form": "정.착. 못하고",
          "original_form": "정.착. 못하고",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038058.1.25",
          "form": "계속 돌려 ㅋㅋㅋ",
          "original_form": "계속 돌려 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038058.1.26",
          "form": "나는 soro?",
          "original_form": "나는 soro?",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038058.1.27",
          "form": "그러고 지금은 홈쇼핑이요",
          "original_form": "그러고 지금은 홈쇼핑이요",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038058.1.28",
          "form": "표리부동프로그램아십니꽈",
          "original_form": "표리부동프로그램아십니꽈",
          "speaker_id": "2",
          "time": "20210902 09:13"
        },
        {
          "id": "MDRW2100038058.1.29",
          "form": "그게 무엇이죠 ㅋㅋ",
          "original_form": "그게 무엇이죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100038058.1.30",
          "form": "범죄사건다루는거!",
          "original_form": "범죄사건다루는거!",
