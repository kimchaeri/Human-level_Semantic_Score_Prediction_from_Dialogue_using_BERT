        },
        {
          "id": "MDRW2100008361.1.6",
          "form": "키우고싶은ㄴ동물 잇나~",
          "original_form": "키우고싶은ㄴ동물 잇나~",
          "speaker_id": "2",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008361.1.7",
          "form": "고양이!!!!!",
          "original_form": "고양이!!!!!",
          "speaker_id": "1",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008361.1.8",
          "form": "고양이최고",
          "original_form": "고양이최고",
          "speaker_id": "1",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008361.1.9",
          "form": "말랑말랑~",
          "original_form": "말랑말랑~",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008361.1.10",
          "form": "기엽지",
          "original_form": "기엽지",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008361.1.11",
          "form": "키우먄 이름은 모라지을거야",
          "original_form": "키우먄 이름은 모라지을거야",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008361.1.12",
          "form": "name3~",
          "original_form": "&name3&~",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008361.1.13",
          "form": "말랑말랑",
          "original_form": "말랑말랑",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008361.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008361.1.15",
          "form": "어이없어",
          "original_form": "어이없어",
          "speaker_id": "2",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008361.1.16",
          "form": "그럼 햇갛리잖아 내가",
          "original_form": "그럼 햇갛리잖아 내가",
          "speaker_id": "2",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008361.1.17",
          "form": "ㅋㅎㅋㅎㅋㅎㅋ",
          "original_form": "ㅋㅎㅋㅎㅋㅎㅋ",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008361.1.18",
          "form": "별로 샹각해본적이없여",
          "original_form": "별로 샹각해본적이없여",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008361.1.19",
          "form": "동물을 키우는 데는..",
          "original_form": "동물을 키우는 데는..",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008361.1.20",
          "form": "노력이 많이 팔요하니가",
          "original_form": "노력이 많이 팔요하니가",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008361.1.21",
          "form": "구치..",
          "original_form": "구치..",
          "speaker_id": "2",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008361.1.22",
          "form": "그래서 나도 못할거같아",
          "original_form": "그래서 나도 못할거같아",
          "speaker_id": "2",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008361.1.23",
          "form": "나는 뱀이 조은데",
          "original_form": "나는 뱀이 조은데",
          "speaker_id": "2",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008361.1.24",
          "form": "노란뱀 귀엽더라",
          "original_form": "노란뱀 귀엽더라",
          "speaker_id": "1",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008361.1.25",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100008361.1.26",
          "form": "엄청 예뻐",
          "original_form": "엄청 예뻐",
          "speaker_id": "2",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100008361.1.27",
          "form": "하지만 본적도업지..",
          "original_form": "하지만 본적도업지..",
          "speaker_id": "2",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100008361.1.28",
          "form": "뱀도 키우기 힘들겠지?",
          "original_form": "뱀도 키우기 힘들겠지?",
          "speaker_id": "1",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008361.1.29",
          "form": "생고기 먹여야하나",
          "original_form": "생고기 먹여야하나",
          "speaker_id": "1",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008361.1.30",
          "form": "그렇갯지?",
          "original_form": "그렇갯지?",
          "speaker_id": "2",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008361.1.31",
