      "utterance": [
        {
          "id": "MDRW2100004995.1.1",
          "form": "맞아요 ㅠㅠ 저도 식단이 제일 어려웠어요",
          "original_form": "맞아요 ㅠㅠ 저도 식단이 제일 어려웠어요",
          "speaker_id": "1",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004995.1.2",
          "form": "매운걸 너무 좋아하는데 한번에 뚝 끊었거든요",
          "original_form": "매운걸 너무 좋아하는데 한번에 뚝 끊었거든요",
          "speaker_id": "1",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004995.1.3",
          "form": "짠 음식도 좋아하는데",
          "original_form": "짠 음식도 좋아하는데",
          "speaker_id": "2",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100004995.1.4",
          "form": "아무래도 운동하면 끊어야 되는 음식 이겠죠?",
          "original_form": "아무래도 운동하면 끊어야 되는 음식 이겠죠?",
          "speaker_id": "2",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100004995.1.5",
          "form": "운동은 어떤 거 하세요??",
          "original_form": "운동은 어떤 거 하세요??",
          "speaker_id": "2",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100004995.1.6",
          "form": "나트륨은 줄이는게 아무래도 좋으니까요",
          "original_form": "나트륨은 줄이는게 아무래도 좋으니까요",
          "speaker_id": "1",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100004995.1.7",
          "form": "건강 생각하면 소금은 줄이고 설탕은 끊어야죠 ㅠㅠ",
          "original_form": "건강 생각하면 소금은 줄이고 설탕은 끊어야죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 20:06"
        },
        {
          "id": "MDRW2100004995.1.8",
          "form": "운동은 헬스장 다니고 있어요",
          "original_form": "운동은 헬스장 다니고 있어요",
          "speaker_id": "1",
          "time": "20210821 20:06"
        },
        {
          "id": "MDRW2100004995.1.9",
          "form": "가끔 산책로 러닝하기도하고요",
          "original_form": "가끔 산책로 러닝하기도하고요",
          "speaker_id": "1",
          "time": "20210821 20:06"
        },
        {
          "id": "MDRW2100004995.1.10",
          "form": "단거는 안 좋아해서 설탕끊는 건 쉽더라고요!",
          "original_form": "단거는 안 좋아해서 설탕끊는 건 쉽더라고요!",
          "speaker_id": "2",
          "time": "20210821 20:11"
        },
        {
          "id": "MDRW2100004995.1.11",
          "form": "헬스라니 피티도 받는 건가요?",
          "original_form": "헬스라니 피티도 받는 건가요?",
          "speaker_id": "2",
          "time": "20210821 20:11"
        },
        {
          "id": "MDRW2100004995.1.12",
          "form": "우와 부러워요 ㅠㅠ",
          "original_form": "우와 부러워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100004995.1.13",
          "form": "네 피티도 받고있어요",
          "original_form": "네 피티도 받고있어요",
          "speaker_id": "1",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100004995.1.14",
          "form": "운동 처음 시작하는데 혼자하긴 어렵더라고요",
          "original_form": "운동 처음 시작하는데 혼자하긴 어렵더라고요",
          "speaker_id": "1",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100004995.1.15",
          "form": "저는 집에서 유튜브 보면서 하는데",
          "original_form": "저는 집에서 유튜브 보면서 하는데",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100004995.1.16",
          "form": "아무래도 자세잡기가 제일 어렵네요",
          "original_form": "아무래도 자세잡기가 제일 어렵네요",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100004995.1.17",
          "form": "피티받는데 비용 많이 드나요?",
          "original_form": "피티받는데 비용 많이 드나요?",
          "speaker_id": "2",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004995.1.18",
          "form": "비용은 많이 나가긴해요 ㅠㅠ",
          "original_form": "비용은 많이 나가긴해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100004995.1.19",
          "form": "20회에 100만원 정도 지불했어요",
          "original_form": "20회에 100만원 정도 지불했어요",
          "speaker_id": "1",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100004995.1.20",
          "form": "저도 혼자 운동했었는데 꾸준히 하는거랑 자세안되는게 제일 문제더라고요",
          "original_form": "저도 혼자 운동했었는데 꾸준히 하는거랑 자세안되는게 제일 문제더라고요",
          "speaker_id": "1",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100004995.1.21",
          "form": "그래도 확실히 돈값은 해요",
          "original_form": "그래도 확실히 돈값은 해요",
          "speaker_id": "1",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100004995.1.22",
          "form": "100만원 이라니",
          "original_form": "100만원 이라니",
          "speaker_id": "2",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100004995.1.23",
          "form": "건강 챙기기 참 어렵네요",
          "original_form": "건강 챙기기 참 어렵네요",
          "speaker_id": "2",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100004995.1.24",
          "form": "태권도 같은 운동 배워보신 적 있나요??",
          "original_form": "태권도 같은 운동 배워보신 적 있나요??",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100004995.1.25",
          "form": "어렸을때 좀 해봤네요!",
          "original_form": "어렸을때 좀 해봤네요!",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100004995.1.26",
          "form": "태권도 하시나요?",
          "original_form": "태권도 하시나요?",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100004995.1.27",
          "form": "운동은 따로 배워 본 적이 없어서",
          "original_form": "운동은 따로 배워 본 적이 없어서",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100004995.1.28",
          "form": "돈 좀 모이면 꼭 하나 배워보고 싶어요",
          "original_form": "돈 좀 모이면 꼭 하나 배워보고 싶어요",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100004995.1.29",
          "form": "복싱도 좋고 주짓수도 좋고요",
          "original_form": "복싱도 좋고 주짓수도 좋고요",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100004995.1.30",
          "form": "와 저도 주짓수 배우고 싶어요",
          "original_form": "와 저도 주짓수 배우고 싶어요",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100004995.1.31",
          "form": "예전에는 복싱을 해봤는데",
          "original_form": "예전에는 복싱을 해봤는데",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100004995.1.32",
          "form": "타격감이 기분 좋긴하더라고요",
          "original_form": "타격감이 기분 좋긴하더라고요",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100004995.1.33",
          "form": "근데 글러브를 잃어버려서 ㅎㅎ",
          "original_form": "근데 글러브를 잃어버려서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100004995.1.34",
          "form": "은근 비싼거라 다시 할 마음이 ㅋㅋㅋ",
          "original_form": "은근 비싼거라 다시 할 마음이 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100004995.1.35",
          "form": "장비값도 꽤 하나 보네요",
          "original_form": "장비값도 꽤 하나 보네요",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100004995.1.36",
          "form": "그래도 근육 늘릴 생각만 해도 좋은 것 같아요",
          "original_form": "그래도 근육 늘릴 생각만 해도 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100004995.1.37",
          "form": "타고난 근수저들 정말 부럽더라고요",
          "original_form": "타고난 근수저들 정말 부럽더라고요",
          "speaker_id": "2",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100004995.1.38",
          "form": "아 그쵸 ㅋㅋㅋㅋ",
          "original_form": "아 그쵸 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100004995.1.39",
          "form": "타고난 사람들은 진짜 타고난 것 같아요",
          "original_form": "타고난 사람들은 진짜 타고난 것 같아요",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100004995.1.40",
          "form": "원래 근육만드는거에 큰 관심 없었는데",
          "original_form": "원래 근육만드는거에 큰 관심 없었는데",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100004995.1.41",
          "form": "피티하다보니 생기내요",
          "original_form": "피티하다보니 생기내요",
          "speaker_id": "1",
          "time": "20210821 21:02"
        }
      ]
    }
