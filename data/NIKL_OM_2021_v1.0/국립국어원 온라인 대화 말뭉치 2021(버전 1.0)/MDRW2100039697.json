      "utterance": [
        {
          "id": "MDRW2100039697.1.1",
          "form": "크리스피크림 좋아하세요?",
          "original_form": "크리스피크림 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100039697.1.2",
          "form": "넵 좋아해요",
          "original_form": "넵 좋아해요",
          "speaker_id": "2",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100039697.1.3",
          "form": "예전에는 크리스피에 줄서 있으면",
          "original_form": "예전에는 크리스피에 줄서 있으면",
          "speaker_id": "2",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100039697.1.4",
          "form": "막 구운 도넛 하나씩 나눠줬는데 그게 너무 맛있었어요",
          "original_form": "막 구운 도넛 하나씩 나눠줬는데 그게 너무 맛있었어요",
          "speaker_id": "2",
          "time": "20210903 20:43"
        },
        {
          "id": "MDRW2100039697.1.5",
          "form": "앗 마자요 넘 좋죠!!!!!",
          "original_form": "앗 마자요 넘 좋죠!!!!!",
          "speaker_id": "1",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100039697.1.6",
          "form": "저도 엄청 좋아하는데",
          "original_form": "저도 엄청 좋아하는데",
          "speaker_id": "1",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100039697.1.7",
          "form": "그거 들으시구 카톡 친추하면 할인쿠폰 준다구? 알려주셨어용",
          "original_form": "그거 들으시구 카톡 친추하면 할인쿠폰 준다구? 알려주셨어용",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039697.1.8",
          "form": "그리고 요즘엔 그거 구독서비스도 있대요",
          "original_form": "그리고 요즘엔 그거 구독서비스도 있대요",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039697.1.9",
          "form": "한달에 4번!!",
          "original_form": "한달에 4번!!",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039697.1.10",
          "form": "근데 그거 좀 여러명이서 나눠서 먹으면 좋을 거 같지 않나용?",
          "original_form": "근데 그거 좀 여러명이서 나눠서 먹으면 좋을 거 같지 않나용?",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039697.1.11",
          "form": "그래서 꿀팁이라고 생가했어요",
          "original_form": "그래서 꿀팁이라고 생가했어요",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039697.1.12",
          "form": "한 달 네 번이나!!",
          "original_form": "한 달 네 번이나!!",
          "speaker_id": "2",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039697.1.13",
          "form": "사가서 다 같이 나눠먹어도 좋을 것 같아요",
          "original_form": "사가서 다 같이 나눠먹어도 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039697.1.14",
          "form": "좋은 것은 나눠먹어야 제 맛이죠ㅎㅎ",
          "original_form": "좋은 것은 나눠먹어야 제 맛이죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039697.1.15",
          "form": "그쳐그쳐",
          "original_form": "그쳐그쳐",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039697.1.16",
          "form": "그래서...역시 이런 곳에서 꿀팁 얻는 거 좋은 거 같아요",
          "original_form": "그래서...역시 이런 곳에서 꿀팁 얻는 거 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100039697.1.17",
          "form": "맞아요ㅋㅋㅋ저도 크리스피는 아니지만",
          "original_form": "맞아요ㅋㅋㅋ저도 크리스피는 아니지만",
          "speaker_id": "2",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100039697.1.18",
          "form": "유통기한 임박식품같은거 싸게 사고",
          "original_form": "유통기한 임박식품같은거 싸게 사고",
          "speaker_id": "2",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039697.1.19",
          "form": "냉동식품이나 사탕같은거 유통기한 임박제품 정보 얻어서 쟁여놨어요",
          "original_form": "냉동식품이나 사탕같은거 유통기한 임박제품 정보 얻어서 쟁여놨어요",
          "speaker_id": "2",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039697.1.20",
          "form": "헐 그거 좋네용",
          "original_form": "헐 그거 좋네용",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039697.1.21",
          "form": "도착하고 바로 먹으면 되니까",
          "original_form": "도착하고 바로 먹으면 되니까",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039697.1.22",
          "form": "저두 오명가명 임박몰? 본 거 같은데",
          "original_form": "저두 오명가명 임박몰? 본 거 같은데",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039697.1.23",
          "form": "담에 한 번 이용해봐야겠어요",
          "original_form": "담에 한 번 이용해봐야겠어요",
          "speaker_id": "1",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039697.1.24",
          "form": "그...name2님은 저녁 뭐드셨나요",
          "original_form": "그...&name2&님은 저녁 뭐드셨나요",
          "speaker_id": "1",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039697.1.25",
          "form": "저는 대충 밥과 닭을 먹었어요!",
          "original_form": "저는 대충 밥과 닭을 먹었어요!",
          "speaker_id": "2",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039697.1.26",
          "form": "오오 치킨 드셨나봐요~><",
          "original_form": "오오 치킨 드셨나봐요~><",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100039697.1.27",
          "form": "오늘은 치킨은 아니지만ㅋㅋㅋ",
          "original_form": "오늘은 치킨은 아니지만ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100039697.1.28",
          "form": "조만간 뿌링클을 먹고싶어요ㅋㅋㅋㅋ",
          "original_form": "조만간 뿌링클을 먹고싶어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100039697.1.29",
          "form": "앗ㅋㅋㅋㅋ그러셨군요...",
          "original_form": "앗ㅋㅋㅋㅋ그러셨군요...",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100039697.1.30",
          "form": "마자요 저두 뿌링클 젛아해요",
          "original_form": "마자요 저두 뿌링클 젛아해요",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100039697.1.31",
          "form": "사실 ㄱㅊㅂㅅㅅ을 제일 좋아하는데",
          "original_form": "사실 ㄱㅊㅂㅅㅅ을 제일 좋아하는데",
          "speaker_id": "1",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039697.1.32",
          "form": "심심이에서 나쁜말이라고 차단하더라구요...",
          "original_form": "심심이에서 나쁜말이라고 차단하더라구요...",
          "speaker_id": "1",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039697.1.33",
          "form": "그냥 ㄱㅊ가 나쁜말 설정된 건가봐요... 초성으로 하니까 진짜 나쁜말 같네....",
          "original_form": "그냥 ㄱㅊ가 나쁜말 설정된 건가봐요... 초성으로 하니까 진짜 나쁜말 같네....",
          "speaker_id": "1",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100039697.1.34",
          "form": "저도 마침 그 치킨이 궁금했는데",
          "original_form": "저도 마침 그 치킨이 궁금했는데",
          "speaker_id": "2",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100039697.1.35",
          "form": "나쁜말 범위가 은근 넓더라구요",
          "original_form": "나쁜말 범위가 은근 넓더라구요",
          "speaker_id": "2",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039697.1.36",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039697.1.37",
          "form": "그 바사삭 치킨은 구운 치킨이죠?",
          "original_form": "그 바사삭 치킨은 구운 치킨이죠?",
          "speaker_id": "2",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039697.1.38",
          "form": "맞아요ㅠㅜ 가끔 엉뚱한 게 차단되고 그래요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅠㅜ 가끔 엉뚱한 게 차단되고 그래요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039697.1.39",
          "form": "네네!! 구운치킨이라 맛있어요",
          "original_form": "네네!! 구운치킨이라 맛있어요",
          "speaker_id": "1",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039697.1.40",
          "form": "유명한데 아직 못먹어봤어요!",
          "original_form": "유명한데 아직 못먹어봤어요!",
          "speaker_id": "2",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039697.1.41",
          "form": "오 강력추천이에용 소스도 되게 맛있어서",
          "original_form": "오 강력추천이에용 소스도 되게 맛있어서",
          "speaker_id": "1",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039697.1.42",
          "form": "소스 추가하셔도 좋을 거 같아요!!!",
          "original_form": "소스 추가하셔도 좋을 거 같아요!!!",
          "speaker_id": "1",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100039697.1.43",
          "form": "마요네즈? 같은 소스였나요?",
          "original_form": "마요네즈? 같은 소스였나요?",
          "speaker_id": "2",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100039697.1.44",
          "form": "앗 네 마자요 마요네즈같은 마블링소스랑 좀 꾸덕한 고블링소스가 잇는데",
          "original_form": "앗 네 마자요 마요네즈같은 마블링소스랑 좀 꾸덕한 고블링소스가 잇는데",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100039697.1.45",
          "form": "취향에 따라 다르겠지만 전 마블링소스 좋아했어요ㅋㅋㅋㅋ",
          "original_form": "취향에 따라 다르겠지만 전 마블링소스 좋아했어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100039697.1.46",
          "form": "고블링은 매운건가요?",
          "original_form": "고블링은 매운건가요?",
          "speaker_id": "2",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039697.1.47",
          "form": "이름만들어도 맛있을것 같고..",
          "original_form": "이름만들어도 맛있을것 같고..",
          "speaker_id": "2",
          "time": "20210903 21:01"
        }
