      "utterance": [
        {
          "id": "MDRW2100018900.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018900.1.2",
          "form": "요즘 개학하셧나요",
          "original_form": "요즘 개학하셧나요",
          "speaker_id": "1",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018900.1.3",
          "form": "다음 달에 개강해요!",
          "original_form": "다음 달에 개강해요!",
          "speaker_id": "2",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018900.1.4",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018900.1.5",
          "form": "앗 대학생이시구나",
          "original_form": "앗 대학생이시구나",
          "speaker_id": "1",
          "time": "20210827 13:23"
        },
        {
          "id": "MDRW2100018900.1.6",
          "form": "전 졸업하고 일하고 있어요",
          "original_form": "전 졸업하고 일하고 있어요",
          "speaker_id": "1",
          "time": "20210827 13:23"
        },
        {
          "id": "MDRW2100018900.1.7",
          "form": "멋있으세요!",
          "original_form": "멋있으세요!",
          "speaker_id": "2",
          "time": "20210827 13:23"
        },
        {
          "id": "MDRW2100018900.1.8",
          "form": "일하는 개미인걸욜..ㅠㅠ",
          "original_form": "일하는 개미인걸욜..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 13:24"
        },
        {
          "id": "MDRW2100018900.1.9",
          "form": "몇학년이세요??",
          "original_form": "몇학년이세요??",
          "speaker_id": "1",
          "time": "20210827 13:24"
        },
        {
          "id": "MDRW2100018900.1.10",
          "form": "올해 입학한 1학년입니다!",
          "original_form": "올해 입학한 1학년입니다!",
          "speaker_id": "2",
          "time": "20210827 13:25"
        },
        {
          "id": "MDRW2100018900.1.11",
          "form": "앗 파릇파릇하네요 !!",
          "original_form": "앗 파릇파릇하네요 !!",
          "speaker_id": "1",
          "time": "20210827 13:25"
        },
        {
          "id": "MDRW2100018900.1.12",
          "form": "근데 입학후에 학교에 많이 가셧나요?ㅠㅠ",
          "original_form": "근데 입학후에 학교에 많이 가셧나요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 13:25"
        },
        {
          "id": "MDRW2100018900.1.13",
          "form": "저희는 1학기에 대면이랑 비대면이랑 섞여있어서 좀 갔어요!",
          "original_form": "저희는 1학기에 대면이랑 비대면이랑 섞여있어서 좀 갔어요!",
          "speaker_id": "2",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100018900.1.14",
          "form": "ㅎㅎ오 그래도 다행이네요!!",
          "original_form": "ㅎㅎ오 그래도 다행이네요!!",
          "speaker_id": "1",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018900.1.15",
          "form": "제동생은 2학년인데 학교간날이 안간날보다 적어서",
          "original_form": "제동생은 2학년인데 학교간날이 안간날보다 적어서",
          "speaker_id": "1",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018900.1.16",
          "form": "좀 걱정이네요 ㅎ",
          "original_form": "좀 걱정이네요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018900.1.17",
          "form": "저도 아직 학교 구조도 잘 몰라요ㅠ 수업 있는 건물만 외웠어요...ㅎㅎ",
          "original_form": "저도 아직 학교 구조도 잘 몰라요ㅠ 수업 있는 건물만 외웠어요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018900.1.18",
          "form": "ㅎㅎ빨리 코로나가 끝나서",
          "original_form": "ㅎㅎ빨리 코로나가 끝나서",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018900.1.19",
          "form": "학교생활을 즐겨보는게 좋을텐데말이에요..",
          "original_form": "학교생활을 즐겨보는게 좋을텐데말이에요..",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018900.1.20",
          "form": "맞아요ㅠㅠ 아는 동기도 별로 없고... 동아리도 활동을 거의 못 한다고 해서 못 들어 갔어요",
          "original_form": "맞아요ㅠㅠ 아는 동기도 별로 없고... 동아리도 활동을 거의 못 한다고 해서 못 들어 갔어요",
          "speaker_id": "2",
          "time": "20210827 13:30"
        }
      ]
