        {
          "id": "MDRW2100009209.1.25",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100009209.1.26",
          "form": "올리브영 할인 다음달에 크게하려나",
          "original_form": "올리브영 할인 다음달에 크게하려나",
