      "utterance": [
        {
          "id": "MDRW2100009209.1.1",
          "form": "맞다 나 교정칫솔사야되는데 계속 미루는중",
          "original_form": "맞다 나 교정칫솔사야되는데 계속 미루는중",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009209.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009209.1.3",
          "form": "ㅋㅋㅋ나는 치실 쟁여야하는데",
          "original_form": "ㅋㅋㅋ나는 치실 쟁여야하는데",
          "speaker_id": "1",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009209.1.4",
          "form": "치아관리하자...삼천만원",
          "original_form": "치아관리하자...삼천만원",
          "speaker_id": "1",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009209.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이미 교정비용으로......",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이미 교정비용으로......",
          "speaker_id": "2",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009209.1.6",
          "form": "눈물 줄줄",
          "original_form": "눈물 줄줄",
          "speaker_id": "2",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009209.1.7",
          "form": "달팽이크림도 다써가는데 동생남친이 군인이라 피엑스에서 사다줌ㅋㅋㅋ",
          "original_form": "달팽이크림도 다써가는데 동생남친이 군인이라 피엑스에서 사다줌ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009209.1.8",
          "form": "교정비 눈물나지...",
          "original_form": "교정비 눈물나지...",
          "speaker_id": "1",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009209.1.9",
          "form": "동생덕 보는구만ㅋㅋㅋㅋ",
          "original_form": "동생덕 보는구만ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009209.1.10",
          "form": "군대 엄청 싸잖아",
          "original_form": "군대 엄청 싸잖아",
          "speaker_id": "1",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009209.1.11",
          "form": "맞아ㅋㅋㅋㅋㅋ근데 달팽이크림도 닥터지 최고",
          "original_form": "맞아ㅋㅋㅋㅋㅋ근데 달팽이크림도 닥터지 최고",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009209.1.12",
          "form": "닥터지꺼가 더 꾸덕꾸덕한게 좋더라",
          "original_form": "닥터지꺼가 더 꾸덕꾸덕한게 좋더라",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009209.1.13",
          "form": "너가 건성인가?",
          "original_form": "너가 건성인가?",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009209.1.14",
          "form": "나는 요즘 마스크때문인지 트러블장난없어",
          "original_form": "나는 요즘 마스크때문인지 트러블장난없어",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009209.1.15",
          "form": "그런듯......그래서 달팽이크림 넘 좋아",
          "original_form": "그런듯......그래서 달팽이크림 넘 좋아",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009209.1.16",
          "form": "마스크때문에 트러블 오져ㅋㅋㅋㅋ그래도 화장품은 안사게되더라",
          "original_form": "마스크때문에 트러블 오져ㅋㅋㅋㅋ그래도 화장품은 안사게되더라",
          "speaker_id": "2",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009209.1.17",
          "form": "화장안해도되서 그건좋음ㅎ",
          "original_form": "화장안해도되서 그건좋음ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009209.1.18",
          "form": "화장안한지 어언 2년째",
          "original_form": "화장안한지 어언 2년째",
          "speaker_id": "1",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009209.1.19",
          "form": "비비도 없어서 고모가 주셨는데 쓸일없음",
          "original_form": "비비도 없어서 고모가 주셨는데 쓸일없음",
          "speaker_id": "1",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009209.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ나도 요새 선크림까지만 발라ㅋㅌㅌ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ나도 요새 선크림까지만 발라ㅋㅌㅌ",
          "speaker_id": "2",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009209.1.21",
          "form": "선크림도 사긴해야됔ㅋㅋㅌ",
          "original_form": "선크림도 사긴해야됔ㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009209.1.22",
          "form": "선크림 차단지수 논란떠서 딱히 쓸만한게 없어",
          "original_form": "선크림 차단지수 논란떠서 딱히 쓸만한게 없어",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009209.1.23",
          "form": "맞아 선크림 다 못믿겠더라",
          "original_form": "맞아 선크림 다 못믿겠더라",
          "speaker_id": "2",
          "time": "20210823 07:43"
        },
        {
          "id": "MDRW2100009209.1.24",
          "form": "그래도 꾸역꾸역 바르긴해",
          "original_form": "그래도 꾸역꾸역 바르긴해",
          "speaker_id": "1",
          "time": "20210823 10:50"
