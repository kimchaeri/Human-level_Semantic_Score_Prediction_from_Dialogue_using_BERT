      "utterance": [
        {
          "id": "MDRW2100014965.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014965.1.2",
          "form": "게임 콘텐츠면..",
          "original_form": "게임 콘텐츠면..",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014965.1.3",
          "form": "할 얘기 많지..",
          "original_form": "할 얘기 많지..",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014965.1.4",
          "form": "요즘 인기 게임",
          "original_form": "요즘 인기 게임",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014965.1.5",
          "form": "앙상블 스타즈",
          "original_form": "앙상블 스타즈",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014965.1.6",
          "form": "요즘 사람들 앙스타하면",
          "original_form": "요즘 사람들 앙스타하면",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014965.1.7",
          "form": "아 그거..? 하잖아",
          "original_form": "아 그거..? 하잖아",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014965.1.8",
          "form": "..",
          "original_form": "..",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014965.1.9",
          "form": "...너무 갓겜이라서",
          "original_form": "...너무 갓겜이라서",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014965.1.10",
          "form": "....",
          "original_form": "....",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014965.1.11",
          "form": "갓겜 그래서",
          "original_form": "갓겜 그래서",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014965.1.12",
          "form": "유성대 하코는 언제쯤",
          "original_form": "유성대 하코는 언제쯤",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014965.1.13",
          "form": "주는거지",
          "original_form": "주는거지",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014965.1.14",
          "form": "... 1년걸릴듯",
          "original_form": "... 1년걸릴듯",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014965.1.15",
          "form": "올해말?",
          "original_form": "올해말?",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014965.1.16",
          "form": "....",
          "original_form": "....",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014965.1.17",
          "form": "생각해보니 셔플도 좀 와야하네",
          "original_form": "생각해보니 셔플도 좀 와야하네",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014965.1.18",
          "form": "왤케 밀렸냐",
          "original_form": "왤케 밀렸냐",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014965.1.19",
          "form": "ㅠㅠ 빨리 신곡 노래도 듣고싶다",
          "original_form": "ㅠㅠ 빨리 신곡 노래도 듣고싶다",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014965.1.20",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014965.1.21",
          "form": "혜성 하레이션 같은 거 말고",
          "original_form": "혜성 하레이션 같은 거 말고",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014965.1.22",
          "form": "유성대 하코곡 또 트로트면",
          "original_form": "유성대 하코곡 또 트로트면",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014965.1.23",
          "form": "부술거임",
          "original_form": "부술거임",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014965.1.24",
          "form": "좀 있어보이는 걸로...",
          "original_form": "좀 있어보이는 걸로...",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014965.1.25",
          "form": "마제매직 같은 거..",
          "original_form": "마제매직 같은 거..",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014965.1.26",
          "form": "칼군무...",
          "original_form": "칼군무...",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014965.1.27",
          "form": "edm..",
          "original_form": "edm..",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014965.1.28",
          "form": "ㅠㅠ 마제매직 좋지",
          "original_form": "ㅠㅠ 마제매직 좋지",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014965.1.29",
          "form": "ㅠㅜㅜㅜㅜ",
          "original_form": "ㅠㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014965.1.30",
          "form": "테토라 5성이려나",
          "original_form": "테토라 5성이려나",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014965.1.31",
          "form": "닌닌이 아님 테토라인데",
          "original_form": "닌닌이 아님 테토라인데",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014965.1.32",
          "form": "테토라가 생각보다 늦네",
          "original_form": "테토라가 생각보다 늦네",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014965.1.33",
          "form": "아 닌닌이려나ㅜㅜ",
          "original_form": "아 닌닌이려나ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014965.1.34",
          "form": "설마 테토라 셔플이냐",
          "original_form": "설마 테토라 셔플이냐",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014965.1.35",
          "form": "닌닌이면... 그 컨셉일텐데",
          "original_form": "닌닌이면... 그 컨셉일텐데",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014965.1.36",
          "form": ".....",
          "original_form": ".....",
