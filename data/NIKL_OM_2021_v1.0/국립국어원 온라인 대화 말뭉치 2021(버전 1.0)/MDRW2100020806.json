      "utterance": [
        {
          "id": "MDRW2100020806.1.1",
          "form": "결혼하셧어요?",
          "original_form": "결혼하셧어요?",
          "speaker_id": "2",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100020806.1.2",
          "form": "알알랴줌... ㅋㅋㅋ \r\n결혼 안하셧죠?",
          "original_form": "알알랴줌... ㅋㅋㅋ \r\n결혼 안하셧죠?",
          "speaker_id": "1",
          "time": "20210828 13:00"
        },
        {
          "id": "MDRW2100020806.1.3",
          "form": "저도 안알랴줌 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 안알랴줌 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:00"
        },
        {
          "id": "MDRW2100020806.1.4",
          "form": "네.. 미혼씨... ㅋㅋㅋ \r\n비혼주의이신가여?",
          "original_form": "네.. 미혼씨... ㅋㅋㅋ \r\n비혼주의이신가여?",
          "speaker_id": "1",
          "time": "20210828 13:01"
        },
        {
          "id": "MDRW2100020806.1.5",
          "form": "네 결혼생각 없어요",
          "original_form": "네 결혼생각 없어요",
          "speaker_id": "2",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100020806.1.6",
          "form": "그럴 상대는 있어봤구요? ㅋㅋㅋㅋ",
          "original_form": "그럴 상대는 있어봤구요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100020806.1.7",
          "form": "아 결혼을 생각햇던 상대가 잇엇냐구요?",
          "original_form": "아 결혼을 생각햇던 상대가 잇엇냐구요?",
          "speaker_id": "2",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020806.1.8",
          "form": "네... 어릴때부터 비혼주의셧어여?",
          "original_form": "네... 어릴때부터 비혼주의셧어여?",
          "speaker_id": "1",
          "time": "20210828 13:04"
        },
        {
          "id": "MDRW2100020806.1.9",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020806.1.10",
          "form": "어느시점부터는 누굴만날때도 미리 말을 하고 만났어요",
          "original_form": "어느시점부터는 누굴만날때도 미리 말을 하고 만났어요",
          "speaker_id": "2",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020806.1.11",
          "form": "결혼생각은 없다구요",
          "original_form": "결혼생각은 없다구요",
          "speaker_id": "2",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020806.1.12",
          "form": "아.. 요즘 대부분 그런거 같아요..",
          "original_form": "아.. 요즘 대부분 그런거 같아요..",
          "speaker_id": "1",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020806.1.13",
          "form": "혼인률봐도 그렇고..",
          "original_form": "혼인률봐도 그렇고..",
          "speaker_id": "1",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020806.1.14",
          "form": "name1님은 어떠세요?",
          "original_form": "&name1&님은 어떠세요?",
