        },
        {
          "id": "MDRW2100015950.1.2",
          "form": "아 저도 뿌링클 진짜 좋아해요",
          "original_form": "아 저도 뿌링클 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210826 03:36"
        },
        {
          "id": "MDRW2100015950.1.3",
          "form": "순살, 뼈, 콤보 어떤거 좋아하세요?",
          "original_form": "순살, 뼈, 콤보 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 03:37"
        },
        {
          "id": "MDRW2100015950.1.4",
          "form": "뼈 좋아해요",
          "original_form": "뼈 좋아해요",
          "speaker_id": "1",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100015950.1.5",
          "form": "저는 콤보요",
          "original_form": "저는 콤보요",
          "speaker_id": "2",
          "time": "20210826 03:40"
        },
        {
          "id": "MDRW2100015950.1.6",
          "form": "콤보가 다리랑 날개인가요?",
          "original_form": "콤보가 다리랑 날개인가요?",
          "speaker_id": "1",
          "time": "20210826 03:40"
        },
        {
          "id": "MDRW2100015950.1.7",
          "form": "네 맞아요 존맛",
          "original_form": "네 맞아요 존맛",
          "speaker_id": "2",
          "time": "20210826 03:41"
        },
        {
          "id": "MDRW2100015950.1.8",
          "form": "최애 부위가 어디에요?",
          "original_form": "최애 부위가 어디에요?",
          "speaker_id": "1",
          "time": "20210826 03:41"
        },
        {
          "id": "MDRW2100015950.1.9",
          "form": "다리요!",
          "original_form": "다리요!",
          "speaker_id": "2",
          "time": "20210826 03:42"
        },
        {
          "id": "MDRW2100015950.1.10",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210826 03:42"
        },
        {
          "id": "MDRW2100015950.1.11",
          "form": "닭가슴살처럼 생겼는데 다리살맛나는 부위도 좋아해요",
          "original_form": "닭가슴살처럼 생겼는데 다리살맛나는 부위도 좋아해요",
          "speaker_id": "1",
          "time": "20210826 03:42"
        },
        {
          "id": "MDRW2100015950.1.12",
