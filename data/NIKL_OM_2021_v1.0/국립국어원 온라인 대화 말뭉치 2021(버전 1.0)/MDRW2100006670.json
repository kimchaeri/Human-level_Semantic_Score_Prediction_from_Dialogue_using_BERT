      "utterance": [
        {
          "id": "MDRW2100006670.1.1",
          "form": "친구들이랑 가서 즐거웟어요",
          "original_form": "친구들이랑 가서 즐거웟어요",
          "speaker_id": "2",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100006670.1.2",
          "form": "왕복 비행기 탔나요?",
          "original_form": "왕복 비행기 탔나요?",
          "speaker_id": "1",
          "time": "20210908 06:01"
        },
        {
          "id": "MDRW2100006670.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210908 06:02"
        },
        {
          "id": "MDRW2100006670.1.4",
          "form": "저는 제주도가 첫 비행기었는데 ㅎㅎ",
          "original_form": "저는 제주도가 첫 비행기었는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 06:03"
        },
        {
          "id": "MDRW2100006670.1.5",
          "form": "그럼 수학여행이 첫 비행기셧어요?",
          "original_form": "그럼 수학여행이 첫 비행기셧어요?",
          "speaker_id": "2",
          "time": "20210908 06:04"
        },
        {
          "id": "MDRW2100006670.1.6",
          "form": "네 멀미봉투 아세요?",
          "original_form": "네 멀미봉투 아세요?",
          "speaker_id": "1",
          "time": "20210908 06:05"
        },
        {
          "id": "MDRW2100006670.1.7",
          "form": "네 알아요",
          "original_form": "네 알아요",
          "speaker_id": "2",
          "time": "20210908 06:05"
        },
        {
          "id": "MDRW2100006670.1.8",
          "form": "그걸 그시절 처음보고 신기하고 웃기기도해서 친구들이랑 배꼽 잡았던 기억이 나요",
          "original_form": "그걸 그시절 처음보고 신기하고 웃기기도해서 친구들이랑 배꼽 잡았던 기억이 나요",
          "speaker_id": "1",
          "time": "20210908 06:05"
        },
        {
          "id": "MDRW2100006670.1.9",
          "form": "ㅋㅋㅋㅋㅋ친구들과 함께하는 첫비행기라니 수학여행이 더욱 설렛겟어요",
          "original_form": "ㅋㅋㅋㅋㅋ친구들과 함께하는 첫비행기라니 수학여행이 더욱 설렛겟어요",
          "speaker_id": "2",
          "time": "20210908 06:06"
        },
        {
          "id": "MDRW2100006670.1.10",
          "form": "네 그래서 별거 아닌일도 크게 웃고 기억에 남아요",
          "original_form": "네 그래서 별거 아닌일도 크게 웃고 기억에 남아요",
          "speaker_id": "1",
          "time": "20210908 06:07"
        },
        {
          "id": "MDRW2100006670.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그땐 특히나 별거아닌일에도 그렇게 즐거웟던것같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그땐 특히나 별거아닌일에도 그렇게 즐거웟던것같아요",
          "speaker_id": "2",
          "time": "20210908 06:09"
        },
        {
          "id": "MDRW2100006670.1.12",
          "form": "학창시절엔 뭐가 그렇게 재밋엇는지",
          "original_form": "학창시절엔 뭐가 그렇게 재밋엇는지",
          "speaker_id": "2",
          "time": "20210908 06:09"
        },
        {
          "id": "MDRW2100006670.1.13",
          "form": "일본이 첫 해외여행지었나요,",
          "original_form": "일본이 첫 해외여행지었나요,",
          "speaker_id": "1",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100006670.1.14",
          "form": "네 일본이 첫 해외여행지엿어요",
          "original_form": "네 일본이 첫 해외여행지엿어요",
