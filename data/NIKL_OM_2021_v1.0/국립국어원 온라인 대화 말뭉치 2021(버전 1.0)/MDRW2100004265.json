      "utterance": [
        {
          "id": "MDRW2100004265.1.1",
          "form": "혹시 해외여행 가보셨어요?",
          "original_form": "혹시 해외여행 가보셨어요?",
          "speaker_id": "1",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004265.1.2",
          "form": "네 태어나서 딱 한번이요 ㅠ",
          "original_form": "네 태어나서 딱 한번이요 ㅠ",
          "speaker_id": "2",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004265.1.3",
          "form": "어디 가보셨어요?",
          "original_form": "어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004265.1.4",
          "form": "베트남이요 그쪽은요??",
          "original_form": "베트남이요 그쪽은요??",
          "speaker_id": "2",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004265.1.5",
          "form": "저는 아직 안가봤어요",
          "original_form": "저는 아직 안가봤어요",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004265.1.6",
          "form": "베트남 어디 가보셨어요?",
          "original_form": "베트남 어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004265.1.7",
          "form": "다낭이랑 호이안이요!",
          "original_form": "다낭이랑 호이안이요!",
          "speaker_id": "2",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004265.1.8",
          "form": "맛있는거 많아요?",
          "original_form": "맛있는거 많아요?",
          "speaker_id": "1",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004265.1.9",
          "form": "우리가 흔히 먹는 쌀국수랑 짜조 분짜 이런것만 맛있고 다른건 비리고 역하고 그랬어요",
          "original_form": "우리가 흔히 먹는 쌀국수랑 짜조 분짜 이런것만 맛있고 다른건 비리고 역하고 그랬어요",
          "speaker_id": "2",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004265.1.10",
          "form": "치안은 어땠어요?",
          "original_form": "치안은 어땠어요?",
          "speaker_id": "1",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004265.1.11",
          "form": "딱히 치안이 안 좋은 건 못느꼈어요 ㅋㅋ시장가는길에 오토바이가 엄청 많아서 힘든것뿐",
          "original_form": "딱히 치안이 안 좋은 건 못느꼈어요 ㅋㅋ시장가는길에 오토바이가 엄청 많아서 힘든것뿐",
          "speaker_id": "2",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004265.1.12",
          "form": "거리는 깔끔한편이에요?",
          "original_form": "거리는 깔끔한편이에요?",
          "speaker_id": "1",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004265.1.13",
          "form": "아뇨.. 딱히",
          "original_form": "아뇨.. 딱히",
          "speaker_id": "2",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100004265.1.14",
          "form": "아 음식은 되게 맛있었을거같아요",
          "original_form": "아 음식은 되게 맛있었을거같아요",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100004265.1.15",
          "form": "우리나라에 잘 알려진 음식은 맛있는데 로컬음식은 좀 그렇더라구요 ㅠ 해산물도 길거리에 널려있고 ㅠ",
          "original_form": "우리나라에 잘 알려진 음식은 맛있는데 로컬음식은 좀 그렇더라구요 ㅠ 해산물도 길거리에 널려있고 ㅠ",
          "speaker_id": "2",
          "time": "20210821 22:56"
        }
      ]
