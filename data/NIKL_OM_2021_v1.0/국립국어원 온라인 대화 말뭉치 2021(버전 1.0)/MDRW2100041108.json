        },
        {
          "id": "MDRW2100041108.1.2",
          "form": "난 또",
          "original_form": "난 또",
          "speaker_id": "2",
          "time": "20210905 17:05"
        },
        {
          "id": "MDRW2100041108.1.3",
          "form": "뭐가 문제인지 안알려주고",
          "original_form": "뭐가 문제인지 안알려주고",
          "speaker_id": "1",
          "time": "20210905 17:05"
        },
        {
          "id": "MDRW2100041108.1.4",
          "form": "저녁메뉴시간인줄",
          "original_form": "저녁메뉴시간인줄",
          "speaker_id": "2",
          "time": "20210905 17:05"
        },
        {
          "id": "MDRW2100041108.1.5",
          "form": "난 얘네 퍼즐",
          "original_form": "난 얘네 퍼즐",
          "speaker_id": "2",
          "time": "20210905 17:05"
        },
        {
          "id": "MDRW2100041108.1.6",
          "form": "걍 범위에 도착했으니까 알아서 탐색하라는 퀘가 젤 짜증나",
          "original_form": "걍 범위에 도착했으니까 알아서 탐색하라는 퀘가 젤 짜증나",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100041108.1.7",
          "form": "리셋 시키는 기능이라도 좀",
          "original_form": "리셋 시키는 기능이라도 좀",
          "speaker_id": "2",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100041108.1.8",
          "form": "주면 좋겠어",
          "original_form": "주면 좋겠어",
          "speaker_id": "2",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100041108.1.9",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅇㅇ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅇㅇ",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100041108.1.10",
          "form": "내가 한참 해보다가",
          "original_form": "내가 한참 해보다가",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100041108.1.11",
          "form": "안돼서 공략 찾는건데",
          "original_form": "안돼서 공략 찾는건데",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100041108.1.12",
          "form": "정 안깨지면 공략이라도 보고 따라하게",
          "original_form": "정 안깨지면 공략이라도 보고 따라하게",
          "speaker_id": "2",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100041108.1.13",
          "form": "공략은 최초상태 기준이라",
          "original_form": "공략은 최초상태 기준이라",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100041108.1.14",
          "form": "ㅇㅇ......",
          "original_form": "ㅇㅇ......",
          "speaker_id": "2",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100041108.1.15",
          "form": "아니 얘네",
          "original_form": "아니 얘네",
          "speaker_id": "2",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100041108.1.16",
          "form": "길치를 위한",
          "original_form": "길치를 위한",
          "speaker_id": "2",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100041108.1.17",
          "form": "네비모드를",
          "original_form": "네비모드를",
          "speaker_id": "2",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100041108.1.18",
          "form": "내놔라",
          "original_form": "내놔라",
          "speaker_id": "2",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100041108.1.19",
          "form": "그니까 ㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100041108.1.20",
          "form": "난 봐도 모르겠다고...",
          "original_form": "난 봐도 모르겠다고...",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100041108.1.21",
          "form": "ㅇㅇ.......",
          "original_form": "ㅇㅇ.......",
          "speaker_id": "2",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100041108.1.22",
          "form": "계속 화면 돌려가면서 뒤지는거",
          "original_form": "계속 화면 돌려가면서 뒤지는거",
          "speaker_id": "2",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100041108.1.23",
          "form": "너무 어지럽다고....",
          "original_form": "너무 어지럽다고....",
          "speaker_id": "2",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100041108.1.24",
          "form": "길찾기모드를 내놔...",
          "original_form": "길찾기모드를 내놔...",
          "speaker_id": "2",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100041108.1.25",
          "form": "아 근데 이 퀘스트는 대체 뭐가 잘못된거지",
          "original_form": "아 근데 이 퀘스트는 대체 뭐가 잘못된거지",
          "speaker_id": "1",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100041108.1.26",
          "form": "그러게...무슨 서브퀘야?",
          "original_form": "그러게...무슨 서브퀘야?",
          "speaker_id": "2",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100041108.1.27",
          "form": "내가 이따가 해볼게",
          "original_form": "내가 이따가 해볼게",
          "speaker_id": "2",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100041108.1.28",
          "form": "와타츠미의 심장?",
          "original_form": "와타츠미의 심장?",
          "speaker_id": "1",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100041108.1.29",
