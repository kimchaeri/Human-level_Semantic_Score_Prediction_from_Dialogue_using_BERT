        {
          "id": "MDRW2100037281.1.1",
          "form": "비가와유 주룩주룩",
          "original_form": "비가와유 주룩주룩",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037281.1.2",
          "form": "여긴 아침에 왔어요!",
          "original_form": "여긴 아침에 왔어요!",
          "speaker_id": "2",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100037281.1.3",
          "form": "여기는 호우주의보요",
          "original_form": "여기는 호우주의보요",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037281.1.4",
          "form": "배달기사분들 별일없었음좋겠구먼",
          "original_form": "배달기사분들 별일없었음좋겠구먼",
          "speaker_id": "1",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037281.1.5",
          "form": "바람도 많이 부나요?",
          "original_form": "바람도 많이 부나요?",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100037281.1.6",
          "form": "바람은 그닥",
          "original_form": "바람은 그닥",
          "speaker_id": "1",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037281.1.7",
          "form": "호우주의보인데 바람이 안부나요?",
          "original_form": "호우주의보인데 바람이 안부나요?",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037281.1.8",
          "form": "비가콸콸와요",
          "original_form": "비가콸콸와요",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100037281.1.9",
          "form": "가을이 되려나보네요",
          "original_form": "가을이 되려나보네요",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037281.1.10",
          "form": "장마순서가 달라져불었서",
          "original_form": "장마순서가 달라져불었서",
          "speaker_id": "1",
          "time": "20210901 00:05"
        },
        {
          "id": "MDRW2100037281.1.11",
          "form": "가을장마가 더 심하니까ㅎ",
          "original_form": "가을장마가 더 심하니까ㅎ",
          "speaker_id": "2",
          "time": "20210901 00:08"
        },
        {
          "id": "MDRW2100037281.1.12",
