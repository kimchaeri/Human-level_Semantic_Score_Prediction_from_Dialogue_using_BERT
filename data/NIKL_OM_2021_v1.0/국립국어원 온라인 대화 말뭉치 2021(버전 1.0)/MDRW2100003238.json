      "utterance": [
        {
          "id": "MDRW2100003238.1.1",
          "form": "혹시 좋아하는 음식이 뭐에요?",
          "original_form": "혹시 좋아하는 음식이 뭐에요?",
          "speaker_id": "1",
          "time": "20210820 20:52"
        },
        {
          "id": "MDRW2100003238.1.2",
          "form": "저는 라면이요",
          "original_form": "저는 라면이요",
          "speaker_id": "2",
          "time": "20210820 20:54"
        },
        {
          "id": "MDRW2100003238.1.3",
          "form": "어떤라면 좋아하세요?",
          "original_form": "어떤라면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 20:56"
        },
        {
          "id": "MDRW2100003238.1.4",
          "form": "저는 진라면이 좋던데",
          "original_form": "저는 진라면이 좋던데",
          "speaker_id": "1",
          "time": "20210820 20:56"
        },
        {
          "id": "MDRW2100003238.1.5",
          "form": "저는 안성탕면이 좋아요",
          "original_form": "저는 안성탕면이 좋아요",
          "speaker_id": "2",
          "time": "20210820 20:56"
        },
        {
          "id": "MDRW2100003238.1.6",
          "form": "진라면 맛있게 끓이는 레시피 있나요?",
          "original_form": "진라면 맛있게 끓이는 레시피 있나요?",
          "speaker_id": "2",
          "time": "20210820 20:57"
        },
        {
          "id": "MDRW2100003238.1.7",
          "form": "그냥 끓이는게 제일 맛있더라구요",
          "original_form": "그냥 끓이는게 제일 맛있더라구요",
          "speaker_id": "1",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003238.1.8",
          "form": "일본 라멘 드셔보셨어요?",
          "original_form": "일본 라멘 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003238.1.9",
          "form": "네 미소라멘 좋아해요",
          "original_form": "네 미소라멘 좋아해요",
          "speaker_id": "2",
          "time": "20210820 20:59"
        },
        {
          "id": "MDRW2100003238.1.10",
          "form": "맛집 아시는데 있나요?",
          "original_form": "맛집 아시는데 있나요?",
          "speaker_id": "2",
          "time": "20210820 20:59"
        },
        {
          "id": "MDRW2100003238.1.11",
          "form": "아뇨 저희동네는 라멘집이 하나밖에 없어서...",
          "original_form": "아뇨 저희동네는 라멘집이 하나밖에 없어서...",
          "speaker_id": "1",
          "time": "20210820 21:02"
        },
        {
          "id": "MDRW2100003238.1.12",
          "form": "근데 거기보다는 이마트에서 파는 라멘이 더 맛있더라구요",
          "original_form": "근데 거기보다는 이마트에서 파는 라멘이 더 맛있더라구요",
          "speaker_id": "1",
          "time": "20210820 21:02"
        },
        {
          "id": "MDRW2100003238.1.13",
          "form": "노브랜드 라멘 맛있죠",
          "original_form": "노브랜드 라멘 맛있죠",
          "speaker_id": "2",
          "time": "20210820 21:02"
        },
        {
          "id": "MDRW2100003238.1.14",
          "form": "혹시 짜장면이 좋으세요 짬뽕이 좋으세요?",
          "original_form": "혹시 짜장면이 좋으세요 짬뽕이 좋으세요?",
          "speaker_id": "1",
          "time": "20210820 21:05"
        },
        {
          "id": "MDRW2100003238.1.15",
          "form": "짜장면이 좋아요",
          "original_form": "짜장면이 좋아요",
          "speaker_id": "2",
          "time": "20210820 21:05"
        },
        {
          "id": "MDRW2100003238.1.16",
          "form": "짬뽕 좋아하세요?",
          "original_form": "짬뽕 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 21:05"
        },
        {
          "id": "MDRW2100003238.1.17",
          "form": "둘다좋아해서 고르기 어렵네요...",
          "original_form": "둘다좋아해서 고르기 어렵네요...",
          "speaker_id": "1",
          "time": "20210820 21:08"
        },
        {
          "id": "MDRW2100003238.1.18",
          "form": "제가 면요리는 다 좋아해서",
          "original_form": "제가 면요리는 다 좋아해서",
          "speaker_id": "1",
          "time": "20210820 21:08"
        },
        {
          "id": "MDRW2100003238.1.19",
          "form": "다른 면요리 뭐좋아하세요?",
          "original_form": "다른 면요리 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 21:09"
        },
        {
          "id": "MDRW2100003238.1.20",
          "form": "스파게티나 칼국수도 좋더라구요",
          "original_form": "스파게티나 칼국수도 좋더라구요",
          "speaker_id": "1",
          "time": "20210820 21:09"
        },
        {
          "id": "MDRW2100003238.1.21",
          "form": "저도 스파게티 진짜 좋아해요",
          "original_form": "저도 스파게티 진짜 좋아해요",
