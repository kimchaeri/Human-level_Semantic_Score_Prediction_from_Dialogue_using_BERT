      "utterance": [
        {
          "id": "MDRW2100002129.1.1",
          "form": "시험기간 끝났나요?",
          "original_form": "시험기간 끝났나요?",
          "speaker_id": "1",
          "time": "20210819 23:52"
        },
        {
          "id": "MDRW2100002129.1.2",
          "form": "왜 갑자기 존댓말해",
          "original_form": "왜 갑자기 존댓말해",
          "speaker_id": "2",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002129.1.3",
          "form": "난 학교생활 작년에 다 끝났는뎅",
          "original_form": "난 학교생활 작년에 다 끝났는뎅",
          "speaker_id": "2",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002129.1.4",
          "form": "머야 나두 학교생활 다 끝냈어",
          "original_form": "머야 나두 학교생활 다 끝냈어",
          "speaker_id": "1",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002129.1.5",
          "form": "졸업하니까 학생때가 좋았더라..",
          "original_form": "졸업하니까 학생때가 좋았더라..",
          "speaker_id": "1",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002129.1.6",
          "form": "으르신이였구만",
          "original_form": "으르신이였구만",
          "speaker_id": "2",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002129.1.7",
          "form": "나도 학교생활이 그립다",
          "original_form": "나도 학교생활이 그립다",
          "speaker_id": "2",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002129.1.8",
          "form": "ㅋㅋㅋㅋㅋ공강 만들려고 어떻게든 강의 몰아넣고 시험기간에 죽어나고",
          "original_form": "ㅋㅋㅋㅋㅋ공강 만들려고 어떻게든 강의 몰아넣고 시험기간에 죽어나고",
          "speaker_id": "1",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002129.1.9",
          "form": "재밌었어",
          "original_form": "재밌었어",
          "speaker_id": "1",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002129.1.10",
          "form": "난 학교생활 포기해서",
          "original_form": "난 학교생활 포기해서",
          "speaker_id": "2",
          "time": "20210819 23:56"
        },
        {
          "id": "MDRW2100002129.1.11",
          "form": "널널하게 짜고 방탕하게 놀다가",
          "original_form": "널널하게 짜고 방탕하게 놀다가",
          "speaker_id": "2",
          "time": "20210819 23:56"
        },
        {
          "id": "MDRW2100002129.1.12",
          "form": "작년에 자퇴했어",
          "original_form": "작년에 자퇴했어",
          "speaker_id": "2",
          "time": "20210819 23:56"
        },
        {
          "id": "MDRW2100002129.1.13",
          "form": "아휴 아깝지 않았어?",
          "original_form": "아휴 아깝지 않았어?",
          "speaker_id": "1",
          "time": "20210819 23:58"
        },
        {
          "id": "MDRW2100002129.1.14",
          "form": "사실 진로랑도 상관없는 과였고",
          "original_form": "사실 진로랑도 상관없는 과였고",
          "speaker_id": "2",
          "time": "20210819 23:58"
        },
        {
          "id": "MDRW2100002129.1.15",
          "form": "등록금 아까워서 계속 고민하다가 그만뒀어",
          "original_form": "등록금 아까워서 계속 고민하다가 그만뒀어",
          "speaker_id": "2",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100002129.1.16",
          "form": "좋은생각이야",
          "original_form": "좋은생각이야",
          "speaker_id": "1",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100002129.1.17",
          "form": "난 미련을 못버려서 졸업까지 해버렸어",
          "original_form": "난 미련을 못버려서 졸업까지 해버렸어",
          "speaker_id": "1",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100002129.1.18",
          "form": "취직 되면 재입학 하고 다른 학교로 편입하려구",
          "original_form": "취직 되면 재입학 하고 다른 학교로 편입하려구",
          "speaker_id": "2",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100002129.1.19",
          "form": "아하 그래도 되겠구나",
          "original_form": "아하 그래도 되겠구나",
          "speaker_id": "1",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100002129.1.20",
          "form": "학교 참 즐거웠지...",
          "original_form": "학교 참 즐거웠지...",
          "speaker_id": "2",
          "time": "20210820 00:00"
        },
        {
          "id": "MDRW2100002129.1.21",
          "form": "휴게실가서 우주공강 떼우고...",
          "original_form": "휴게실가서 우주공강 떼우고...",
          "speaker_id": "1",
          "time": "20210820 00:00"
        },
        {
          "id": "MDRW2100002129.1.22",
          "form": "밥사달라고 각다귀같이 몰려드는 후배들...",
          "original_form": "밥사달라고 각다귀같이 몰려드는 후배들...",
          "speaker_id": "2",
          "time": "20210820 00:00"
        },
        {
          "id": "MDRW2100002129.1.23",
          "form": "주머니에 파리채 꼭 챙겨다녔는데",
          "original_form": "주머니에 파리채 꼭 챙겨다녔는데",
          "speaker_id": "2",
          "time": "20210820 00:00"
        },
        {
          "id": "MDRW2100002129.1.24",
          "form": "ㅋㅋㅋㅋㅋ짱웃기네 그래도 후배들 성격 좋았나보다",
          "original_form": "ㅋㅋㅋㅋㅋ짱웃기네 그래도 후배들 성격 좋았나보다",
          "speaker_id": "1",
          "time": "20210820 00:01"
        },
        {
          "id": "MDRW2100002129.1.25",
          "form": "그렇지 뭐 나랑 놀아준거 보면",
          "original_form": "그렇지 뭐 나랑 놀아준거 보면",
          "speaker_id": "2",
          "time": "20210820 00:01"
        },
        {
          "id": "MDRW2100002129.1.26",
          "form": "근데 나도 우주공강에 눈치없이 학실에 누워있긴 했어",
          "original_form": "근데 나도 우주공강에 눈치없이 학실에 누워있긴 했어",
          "speaker_id": "2",
          "time": "20210820 00:02"
        },
        {
          "id": "MDRW2100002129.1.27",
          "form": "늙은이가 누워있으니까 괴롭히고싶었나봐",
          "original_form": "늙은이가 누워있으니까 괴롭히고싶었나봐",
          "speaker_id": "2",
          "time": "20210820 00:02"
        }
      ]
    }
