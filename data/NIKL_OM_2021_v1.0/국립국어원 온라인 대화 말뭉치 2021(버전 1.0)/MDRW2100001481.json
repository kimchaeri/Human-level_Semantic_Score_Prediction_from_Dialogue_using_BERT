        },
        {
          "id": "MDRW2100001481.1.3",
          "form": "혹시 메타버스 아시나용",
          "original_form": "혹시 메타버스 아시나용",
          "speaker_id": "2",
          "time": "20211015 14:01"
        },
        {
          "id": "MDRW2100001481.1.4",
          "form": "네 메타버스 들어는 봤는데",
          "original_form": "네 메타버스 들어는 봤는데",
          "speaker_id": "1",
          "time": "20211015 14:02"
        },
        {
          "id": "MDRW2100001481.1.5",
          "form": "신사업 분야로 각광받는다고 하더라구요",
          "original_form": "신사업 분야로 각광받는다고 하더라구요",
          "speaker_id": "1",
          "time": "20211015 14:03"
        },
        {
          "id": "MDRW2100001481.1.6",
          "form": "관련 회사 주가가 많이 오르던데",
          "original_form": "관련 회사 주가가 많이 오르던데",
          "speaker_id": "1",
          "time": "20211015 14:03"
        },
        {
          "id": "MDRW2100001481.1.7",
          "form": "ㅎㅎ 무슨 외국 가수가",
          "original_form": "ㅎㅎ 무슨 외국 가수가",
          "speaker_id": "2",
          "time": "20211015 14:04"
        },
        {
          "id": "MDRW2100001481.1.8",
          "form": "게임 안에서 콘서트를 해서 수백억을 벌었다던데",
          "original_form": "게임 안에서 콘서트를 해서 수백억을 벌었다던데",
          "speaker_id": "2",
          "time": "20211015 14:04"
        },
        {
          "id": "MDRW2100001481.1.9",
          "form": "그것도 메타버스랑 관련이 있는거더라구요..",
          "original_form": "그것도 메타버스랑 관련이 있는거더라구요..",
          "speaker_id": "2",
          "time": "20211015 14:04"
        },
        {
          "id": "MDRW2100001481.1.10",
          "form": "ㅠㅠ 주식을 샀어야 했다...!!",
          "original_form": "ㅠㅠ 주식을 샀어야 했다...!!",
          "speaker_id": "2",
          "time": "20211015 14:05"
        },
        {
          "id": "MDRW2100001481.1.11",
          "form": "네 외국가수뿐만 아니라",
          "original_form": "네 외국가수뿐만 아니라",
          "speaker_id": "1",
          "time": "20211015 14:05"
        },
        {
          "id": "MDRW2100001481.1.12",
          "form": "BTS도 가상현실 콘서트해서",
          "original_form": "BTS도 가상현실 콘서트해서",
          "speaker_id": "1",
          "time": "20211015 14:05"
        },
        {
          "id": "MDRW2100001481.1.13",
          "form": "주목 받았다고 하더라구요",
          "original_form": "주목 받았다고 하더라구요",
          "speaker_id": "1",
          "time": "20211015 14:05"
        },
        {
          "id": "MDRW2100001481.1.14",
          "form": "VR기술이 발전하면서",
          "original_form": "VR기술이 발전하면서",
          "speaker_id": "1",
          "time": "20211015 14:05"
        },
        {
          "id": "MDRW2100001481.1.15",
          "form": "지금이라도 메타버스 관련주에 들어가야하는건가!",
          "original_form": "지금이라도 메타버스 관련주에 들어가야하는건가!",
          "speaker_id": "2",
          "time": "20211015 14:05"
        },
        {
          "id": "MDRW2100001481.1.16",
          "form": "가상현실 구축까지 연결되는것 같아요",
          "original_form": "가상현실 구축까지 연결되는것 같아요",
          "speaker_id": "1",
          "time": "20211015 14:05"
        },
        {
          "id": "MDRW2100001481.1.17",
          "form": "이미 늦었죠 ㅎㅎ",
          "original_form": "이미 늦었죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211015 14:05"
        },
        {
          "id": "MDRW2100001481.1.18",
          "form": "BTS라니 ㅋㅋㅋㅋㅋ 대단하네요",
          "original_form": "BTS라니 ㅋㅋㅋㅋㅋ 대단하네요",
