      "utterance": [
        {
          "id": "MDRW2100007766.1.1",
          "form": "마루밑 아리에티가",
          "original_form": "마루밑 아리에티가",
          "speaker_id": "1",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100007766.1.2",
          "form": "흥행은 못했는데",
          "original_form": "흥행은 못했는데",
          "speaker_id": "1",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100007766.1.3",
          "form": "보셨을려나",
          "original_form": "보셨을려나",
          "speaker_id": "1",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100007766.1.4",
          "form": "아 저 봤어요 ㅋㅋㅋ 기억은 잘 안나는데",
          "original_form": "아 저 봤어요 ㅋㅋㅋ 기억은 잘 안나는데",
          "speaker_id": "2",
          "time": "20210823 11:54"
        },
        {
          "id": "MDRW2100007766.1.5",
          "form": "결말이 뭐였죠?!",
          "original_form": "결말이 뭐였죠?!",
          "speaker_id": "2",
          "time": "20210823 11:55"
        },
        {
          "id": "MDRW2100007766.1.6",
          "form": "그 쪼만한 소인들 나오는거였잖아요",
          "original_form": "그 쪼만한 소인들 나오는거였잖아요",
          "speaker_id": "1",
          "time": "20210823 11:59"
        },
        {
          "id": "MDRW2100007766.1.7",
          "form": "기억나시죠 거기까지는",
          "original_form": "기억나시죠 거기까지는",
          "speaker_id": "1",
          "time": "20210823 11:59"
        },
        {
          "id": "MDRW2100007766.1.8",
          "form": "요정사이즈 날개없는 소인들",
          "original_form": "요정사이즈 날개없는 소인들",
          "speaker_id": "1",
          "time": "20210823 12:00"
        },
        {
          "id": "MDRW2100007766.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞아요 그 남자애가 아리에티랑 친해졋던 기억만 나고 결말이 기억이 안나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞아요 그 남자애가 아리에티랑 친해졋던 기억만 나고 결말이 기억이 안나요",
          "speaker_id": "2",
          "time": "20210823 12:12"
        },
        {
          "id": "MDRW2100007766.1.10",
          "form": "그 남자애집에 다른 사람한테 아리에티 엄마가 발견되서",
          "original_form": "그 남자애집에 다른 사람한테 아리에티 엄마가 발견되서",
          "speaker_id": "1",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100007766.1.11",
          "form": "그 할머니가 아리에티 엄마 유리병에가두거든요",
          "original_form": "그 할머니가 아리에티 엄마 유리병에가두거든요",
          "speaker_id": "1",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100007766.1.12",
          "form": "그리고신기한생물취급해요",
          "original_form": "그리고신기한생물취급해요",
          "speaker_id": "1",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100007766.1.13",
          "form": "되게 착하고 친절한 할머니로나왔는데",
          "original_form": "되게 착하고 친절한 할머니로나왔는데",
          "speaker_id": "1",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100007766.1.14",
          "form": "그모습이 좀무서웠던거같아요",
          "original_form": "그모습이 좀무서웠던거같아요",
          "speaker_id": "1",
          "time": "20210823 12:24"
        },
        {
          "id": "MDRW2100007766.1.15",
          "form": "헐 진짜 무섭네요",
          "original_form": "헐 진짜 무섭네요",
          "speaker_id": "2",
          "time": "20210823 12:24"
        },
        {
          "id": "MDRW2100007766.1.16",
          "form": "악의는 없는데 그렇게 폭력적이라니",
          "original_form": "악의는 없는데 그렇게 폭력적이라니",
          "speaker_id": "2",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100007766.1.17",
          "form": "여하간 소인들은 인간한테들키면 더이상 여기서는 살수업다 결론내려요",
          "original_form": "여하간 소인들은 인간한테들키면 더이상 여기서는 살수업다 결론내려요",
          "speaker_id": "1",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100007766.1.18",
          "form": "되게 포스터만 보면 힐링영화일거같고 말랑해보였는데 내용은 되게 ㅋㅋㅋㅋ",
          "original_form": "되게 포스터만 보면 힐링영화일거같고 말랑해보였는데 내용은 되게 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100007766.1.19",
          "form": "그마루밑의 집도 들켰던거같아요 원래살던집",
          "original_form": "그마루밑의 집도 들켰던거같아요 원래살던집",
          "speaker_id": "1",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100007766.1.20",
          "form": "아 진짜 무섭네요",
          "original_form": "아 진짜 무섭네요",
          "speaker_id": "2",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100007766.1.21",
          "form": "우리가 동물이나 다른생물들 대하는 자세도 돌아보게되여",
          "original_form": "우리가 동물이나 다른생물들 대하는 자세도 돌아보게되여",
          "speaker_id": "1",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100007766.1.22",
          "form": "그래서 그 가족은결국 짐보따리 싸서떠나요ㅜ",
          "original_form": "그래서 그 가족은결국 짐보따리 싸서떠나요ㅜ",
          "speaker_id": "1",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100007766.1.23",
          "form": "집이랑 삶의터전버리고ㅜㅜ",
          "original_form": "집이랑 삶의터전버리고ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100007766.1.24",
          "form": "헐 결국 헤어지는 내용이군요",
          "original_form": "헐 결국 헤어지는 내용이군요",
          "speaker_id": "2",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100007766.1.25",
          "form": "그 남자애랑",
          "original_form": "그 남자애랑",
          "speaker_id": "2",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100007766.1.26",
          "form": "안타깝네요",
          "original_form": "안타깝네요",
          "speaker_id": "2",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100007766.1.27",
          "form": "헤어는지는데",
          "original_form": "헤어는지는데",
          "speaker_id": "1",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100007766.1.28",
          "form": "그남자애집에",
          "original_form": "그남자애집에",
          "speaker_id": "1",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100007766.1.29",
          "form": "진짜 우리가 다른 생물들 대하는 자세를 돌아보게되는데요 ㅋㅋ ㅠ 우리의 호의가 타 생물의 고통이 될지도 모른다는..",
          "original_form": "진짜 우리가 다른 생물들 대하는 자세를 돌아보게되는데요 ㅋㅋ ㅠ 우리의 호의가 타 생물의 고통이 될지도 모른다는..",
          "speaker_id": "2",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100007766.1.30",
          "form": "할머니껀데 되게 예쁜 인형의 집 있었던거 기억나시나요",
          "original_form": "할머니껀데 되게 예쁜 인형의 집 있었던거 기억나시나요",
          "speaker_id": "1",
          "time": "20210823 12:27"
        },
        {
          "id": "MDRW2100007766.1.31",
          "form": "오 네 ㅋㅋㅋㅋ",
          "original_form": "오 네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:27"
        },
        {
          "id": "MDRW2100007766.1.32",
          "form": "아까 할머니 말고 그남자주인공 친할머니",
          "original_form": "아까 할머니 말고 그남자주인공 친할머니",
          "speaker_id": "1",
          "time": "20210823 12:27"
        }
      ]
    }
