          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000625.1.12",
          "form": "5시반이여",
          "original_form": "5시반이여",
          "speaker_id": "2",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000625.1.13",
          "form": "곧퇴",
          "original_form": "곧퇴",
          "speaker_id": "2",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000625.1.14",
          "form": "와 부럽다",
          "original_form": "와 부럽다",
          "speaker_id": "1",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000625.1.15",
          "form": "저는 6시",
