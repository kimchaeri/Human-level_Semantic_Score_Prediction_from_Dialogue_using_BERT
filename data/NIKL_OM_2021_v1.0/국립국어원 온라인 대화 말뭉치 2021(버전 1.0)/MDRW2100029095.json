      "utterance": [
        {
          "id": "MDRW2100029095.1.1",
          "form": "유자향 화장품세트",
          "original_form": "유자향 화장품세트",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029095.1.2",
          "form": "이름은 모르겠음",
          "original_form": "이름은 모르겠음",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029095.1.3",
          "form": "기초 라인?",
          "original_form": "기초 라인?",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029095.1.4",
          "form": "스킨 오일 로션같은거 있멌던듯",
          "original_form": "스킨 오일 로션같은거 있멌던듯",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029095.1.5",
          "form": "기초라인 맞음",
          "original_form": "기초라인 맞음",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029095.1.6",
          "form": "피부에 신경쓸 나이",
          "original_form": "피부에 신경쓸 나이",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029095.1.7",
          "form": "돈들여줘먀한",
          "original_form": "돈들여줘먀한",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029095.1.8",
          "form": "잘 때 배고프겠다",
          "original_form": "잘 때 배고프겠다",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029095.1.9",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029095.1.10",
          "form": "배1도 안참",
          "original_form": "배1도 안참",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029095.1.11",
          "form": "먹을거 사고싶다",
          "original_form": "먹을거 사고싶다",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029095.1.12",
          "form": "전에 친구집에서 먹은",
          "original_form": "전에 친구집에서 먹은",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029095.1.13",
          "form": "ㅋㅋㅋ전어?",
          "original_form": "ㅋㅋㅋ전어?",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029095.1.14",
          "form": "곱창이 생각난",
          "original_form": "곱창이 생각난",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029095.1.15",
          "form": "마켓컬리에서",
          "original_form": "마켓컬리에서",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029095.1.16",
          "form": "첫주문해서",
          "original_form": "첫주문해서",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029095.1.17",
          "form": "곱창전골 밀키트 세트",
          "original_form": "곱창전골 밀키트 세트",
          "speaker_id": "1",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029095.1.18",
          "form": "900원에 샀다던데",
          "original_form": "900원에 샀다던데",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029095.1.19",
          "form": "검색했는데 없음",
          "original_form": "검색했는데 없음",
          "speaker_id": "1",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029095.1.20",
          "form": "대구곱창인가 대전곱창",
          "original_form": "대구곱창인가 대전곱창",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029095.1.21",
          "form": "걍 양파넣고",
          "original_form": "걍 양파넣고",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029095.1.22",
          "form": "신세계몰에 없었음ㅜㅜ",
          "original_form": "신세계몰에 없었음ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029095.1.23",
          "form": "프라이펜에 구웠는데",
          "original_form": "프라이펜에 구웠는데",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029095.1.24",
          "form": "대구곱창!",
          "original_form": "대구곱창!",
          "speaker_id": "1",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029095.1.25",
          "form": "대존맛",
          "original_form": "대존맛",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029095.1.26",
          "form": "찍먹 소스도 들어가있음",
          "original_form": "찍먹 소스도 들어가있음",
