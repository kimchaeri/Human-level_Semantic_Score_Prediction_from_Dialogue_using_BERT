      "utterance": [
        {
          "id": "MDRW2100036461.1.1",
          "form": "대구? 알지!",
          "original_form": "대구? 알지!",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036461.1.2",
          "form": "내 고향이 대구야",
          "original_form": "내 고향이 대구야",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036461.1.3",
          "form": "너도 잖아 ㅋㅋ~~",
          "original_form": "너도 잖아 ㅋㅋ~~",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036461.1.4",
          "form": "아아 ㅋㅋㅋㅋ 오해했구나",
          "original_form": "아아 ㅋㅋㅋㅋ 오해했구나",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036461.1.5",
          "form": "그거말고 이번에 대구에서 새로나온",
          "original_form": "그거말고 이번에 대구에서 새로나온",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036461.1.6",
          "form": "대구가 뭔데?",
          "original_form": "대구가 뭔데?",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036461.1.7",
          "form": "배달어플 이름이 대구로야!!",
          "original_form": "배달어플 이름이 대구로야!!",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036461.1.8",
          "form": "다른 뜻이 있는거야?",
          "original_form": "다른 뜻이 있는거야?",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036461.1.9",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036461.1.10",
          "form": "누가 만든거래?",
          "original_form": "누가 만든거래?",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036461.1.11",
          "form": "그걸로 뭐 사먹을 수 있는거야?",
          "original_form": "그걸로 뭐 사먹을 수 있는거야?",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036461.1.12",
          "form": "아마 권영진과 그",
          "original_form": "아마 권영진과 그",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036461.1.13",
          "form": "부하들이 만든거 아닐까??",
          "original_form": "부하들이 만든거 아닐까??",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036461.1.14",
          "form": "응 배달의민족이랑 똑같아",
          "original_form": "응 배달의민족이랑 똑같아",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036461.1.15",
          "form": "우와 신기하다~~ 잘아는구나",
          "original_form": "우와 신기하다~~ 잘아는구나",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036461.1.16",
          "form": "너 그거 이용해봤오?",
          "original_form": "너 그거 이용해봤오?",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036461.1.17",
          "form": "뭐 먹었어 그걸로?",
          "original_form": "뭐 먹었어 그걸로?",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036461.1.18",
          "form": "응응 나는 찜닭을 시켰어",
          "original_form": "응응 나는 찜닭을 시켰어",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036461.1.19",
          "form": "이게 쿠폰도 줘서 너무 잘썼지뭐니",
          "original_form": "이게 쿠폰도 줘서 너무 잘썼지뭐니",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036461.1.20",
          "form": "너는 찜닭을 좋아했다 맞아",
          "original_form": "너는 찜닭을 좋아했다 맞아",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036461.1.21",
          "form": "어머 쿠폰 얼마주는데?",
          "original_form": "어머 쿠폰 얼마주는데?",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036461.1.22",
          "form": "너두 다음에 시켜먹어봐",
          "original_form": "너두 다음에 시켜먹어봐",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036461.1.23",
          "form": "오천원!!",
          "original_form": "오천원!!",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036461.1.24",
          "form": "찜닭에 치즈 있었어?",
          "original_form": "찜닭에 치즈 있었어?",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036461.1.25",
          "form": "음 아니 나는 치즈추가를 잘 안해",
          "original_form": "음 아니 나는 치즈추가를 잘 안해",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036461.1.26",
          "form": "너도야",
          "original_form": "너도야",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036461.1.27",
          "form": "돈이없어",
          "original_form": "돈이없어",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036461.1.28",
          "form": "대구로 5000원 쿠폰 유용하더라",
          "original_form": "대구로 5000원 쿠폰 유용하더라",
          "speaker_id": "2",
