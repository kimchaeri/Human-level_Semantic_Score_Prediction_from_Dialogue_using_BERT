        {
          "id": "MDRW2100021084.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "2",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021084.1.2",
          "form": "피자요 님은요?",
          "original_form": "피자요 님은요?",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021084.1.3",
          "form": "생각중이요",
          "original_form": "생각중이요",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021084.1.4",
          "form": "전 시리얼이오",
          "original_form": "전 시리얼이오",
          "speaker_id": "2",
          "time": "20210901 07:51"
        },
        {
          "id": "MDRW2100021084.1.5",
          "form": "전 구운달걀에 연두부 먹었어요",
          "original_form": "전 구운달걀에 연두부 먹었어요",
          "speaker_id": "1",
          "time": "20210901 07:52"
        },
        {
          "id": "MDRW2100021084.1.6",
          "form": "시리얼 좋아하시나요?",
          "original_form": "시리얼 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 07:52"
        },
        {
          "id": "MDRW2100021084.1.7",
          "form": "아침은 간단히요",
          "original_form": "아침은 간단히요",
          "speaker_id": "2",
          "time": "20210901 07:53"
        },
        {
          "id": "MDRW2100021084.1.8",
          "form": "그렇죠 점심은 보통 뭐 드세요?",
          "original_form": "그렇죠 점심은 보통 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210901 07:53"
        },
        {
          "id": "MDRW2100021084.1.9",
          "form": "정식이요",
          "original_form": "정식이요",
          "speaker_id": "2",
          "time": "20210901 07:54"
        },
        {
          "id": "MDRW2100021084.1.10",
          "form": "식당에서요?",
          "original_form": "식당에서요?",
          "speaker_id": "1",
          "time": "20210901 07:54"
        },
        {
          "id": "MDRW2100021084.1.11",
          "form": "네 근무지는 식당이 없어서요",
          "original_form": "네 근무지는 식당이 없어서요",
          "speaker_id": "2",
          "time": "20210901 07:55"
        },
        {
          "id": "MDRW2100021084.1.12",
          "form": "그럼 어디서 드시는건가요?",
          "original_form": "그럼 어디서 드시는건가요?",
          "speaker_id": "1",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100021084.1.13",
          "form": "여기저기요",
          "original_form": "여기저기요",
          "speaker_id": "2",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100021084.1.14",
          "form": "아 메뉴 선정하시기 힘드시겠어요~",
          "original_form": "아 메뉴 선정하시기 힘드시겠어요~",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100021084.1.15",
          "form": "네 점심마다 결정이 힘들죠",
          "original_form": "네 점심마다 결정이 힘들죠",
