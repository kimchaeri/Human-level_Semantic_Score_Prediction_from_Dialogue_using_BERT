      "utterance": [
        {
          "id": "MDRW2100004768.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004768.1.2",
          "form": "안녕하세용?",
          "original_form": "안녕하세용?",
          "speaker_id": "2",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004768.1.3",
          "form": "학교 생활 어떠신가용?",
          "original_form": "학교 생활 어떠신가용?",
          "speaker_id": "2",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004768.1.4",
          "form": "전 동아리도 과활동도.. 안 하는 아웃싸이더라",
          "original_form": "전 동아리도 과활동도.. 안 하는 아웃싸이더라",
          "speaker_id": "1",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004768.1.5",
          "form": "굉장히 심심하고 단조로운 생활을 했답니다..",
          "original_form": "굉장히 심심하고 단조로운 생활을 했답니다..",
          "speaker_id": "1",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004768.1.6",
          "form": "저랑 같으시군요.. 솔직히 귀찮아요ㅜㅠ",
          "original_form": "저랑 같으시군요.. 솔직히 귀찮아요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004768.1.7",
          "form": "수강신청은 하셨나요?",
          "original_form": "수강신청은 하셨나요?",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004768.1.8",
          "form": "넵 얼마전에요",
          "original_form": "넵 얼마전에요",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004768.1.9",
          "form": "막학기라",
          "original_form": "막학기라",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004768.1.10",
          "form": "몇학점 들을 게 없어서 폰으로 했는데",
          "original_form": "몇학점 들을 게 없어서 폰으로 했는데",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004768.1.11",
          "form": "올클했어요ㅎㅎ",
          "original_form": "올클했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004768.1.12",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004768.1.13",
          "form": "우와 축하드려요 저는 졸업예정자라 안했답니다",
          "original_form": "우와 축하드려요 저는 졸업예정자라 안했답니다",
          "speaker_id": "2",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004768.1.14",
          "form": "막학기면 곧 졸업이겠네요 힘냅시다!!",
          "original_form": "막학기면 곧 졸업이겠네요 힘냅시다!!",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004768.1.15",
          "form": "오우 축하드립니다",
          "original_form": "오우 축하드립니다",
          "speaker_id": "1",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004768.1.16",
          "form": "감사해용~~~",
          "original_form": "감사해용~~~",
          "speaker_id": "1",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004768.1.17",
          "form": "저도 졸시쳐야해요ㅜㅜ",
          "original_form": "저도 졸시쳐야해요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004768.1.18",
          "form": "졸시가 어려워서 걱정이에요",
          "original_form": "졸시가 어려워서 걱정이에요",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004768.1.19",
          "form": "통과할수있겠죠?",
          "original_form": "통과할수있겠죠?",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004768.1.20",
          "form": "실은 제가 공부를안해서 어렵게 느껴지나봐요",
          "original_form": "실은 제가 공부를안해서 어렵게 느껴지나봐요",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004768.1.21",
          "form": "물론이죠",
          "original_form": "물론이죠",
          "speaker_id": "1",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004768.1.22",
          "form": "오늘부터라도 공부해야겠어요",
          "original_form": "오늘부터라도 공부해야겠어요",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004768.1.23",
          "form": "일주일정도",
          "original_form": "일주일정도",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004768.1.24",
          "form": "족보있으면 바짝 공부하시면 통과 쉽게 하실거예요",
          "original_form": "족보있으면 바짝 공부하시면 통과 쉽게 하실거예요",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004768.1.25",
          "form": "화이팅!!!!!!",
          "original_form": "화이팅!!!!!!",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004768.1.26",
          "form": "다행이도 족보있으니 열심히 해볼게요",
          "original_form": "다행이도 족보있으니 열심히 해볼게요",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004768.1.27",
          "form": "감사해요!!!",
          "original_form": "감사해요!!!",
          "speaker_id": "2",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004768.1.28",
          "form": "^^ㅎㅎ 혹시 학교 생활하면서",
          "original_form": "^^ㅎㅎ 혹시 학교 생활하면서",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004768.1.29",
          "form": "가장 인상깊었던 일 있으신가요",
          "original_form": "가장 인상깊었던 일 있으신가요",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004768.1.30",
          "form": "저희학교에 고양이가 있는데",
          "original_form": "저희학교에 고양이가 있는데",
          "speaker_id": "2",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004768.1.31",
          "form": "친구와 공강시간에 학교 풍경을보며 산책하면서 고양이 찾아다니는게",
          "original_form": "친구와 공강시간에 학교 풍경을보며 산책하면서 고양이 찾아다니는게",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004768.1.32",
          "form": "너무 즐거웠어요",
          "original_form": "너무 즐거웠어요",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004768.1.33",
          "form": "와 학교 고양이 너무 귀엽죠",
          "original_form": "와 학교 고양이 너무 귀엽죠",
          "speaker_id": "1",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004768.1.34",
          "form": "저희 학교에도 고양이가 있는데",
          "original_form": "저희 학교에도 고양이가 있는데",
          "speaker_id": "1",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004768.1.35",
          "form": "학업 스트레스 속 한줄기 빛이랍니다",
          "original_form": "학업 스트레스 속 한줄기 빛이랍니다",
          "speaker_id": "1",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004768.1.36",
          "form": "우와 정말요? 고양이들은 너무 사랑스러워요",
          "original_form": "우와 정말요? 고양이들은 너무 사랑스러워요",
          "speaker_id": "2",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004768.1.37",
          "form": "진정한 캠퍼스의 주인!! 학교 고양이^^!!",
          "original_form": "진정한 캠퍼스의 주인!! 학교 고양이^^!!",
          "speaker_id": "1",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004768.1.38",
          "form": "어느학교를 가던 꼭 있는거같아요",
          "original_form": "어느학교를 가던 꼭 있는거같아요",
          "speaker_id": "2",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004768.1.39",
          "form": "고양이 짱!!!",
          "original_form": "고양이 짱!!!",
          "speaker_id": "2",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004768.1.40",
          "form": "졸업 시험 마무리 잘 하세요!!",
          "original_form": "졸업 시험 마무리 잘 하세요!!",
          "speaker_id": "1",
