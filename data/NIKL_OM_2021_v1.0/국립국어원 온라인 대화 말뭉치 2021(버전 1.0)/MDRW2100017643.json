      "utterance": [
        {
          "id": "MDRW2100017643.1.1",
          "form": "운동 하시나요?",
          "original_form": "운동 하시나요?",
          "speaker_id": "1",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017643.1.2",
          "form": "저는 취미로 배드민턴 해욤",
          "original_form": "저는 취미로 배드민턴 해욤",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017643.1.3",
          "form": "배드민턴!",
          "original_form": "배드민턴!",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017643.1.4",
          "form": "저 배드민턴 동호회 가입했는데 코로나때매 못가고있어요ㅠ",
          "original_form": "저 배드민턴 동호회 가입했는데 코로나때매 못가고있어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017643.1.5",
          "form": "오 동네 동호회같은거에여?",
          "original_form": "오 동네 동호회같은거에여?",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017643.1.6",
          "form": "네 동네 동호횐데 한번도 못갓어요ㅠ",
          "original_form": "네 동네 동호횐데 한번도 못갓어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017643.1.7",
          "form": "정모에 인원제한이있어서",
          "original_form": "정모에 인원제한이있어서",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017643.1.8",
          "form": "아하... 아쉽겠어요 ㅠㅠㅠ 저는 그냥 동생이랑 쳐욥",
          "original_form": "아하... 아쉽겠어요 ㅠㅠㅠ 저는 그냥 동생이랑 쳐욥",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017643.1.9",
          "form": "좋으시겠어요  저는 같이 칠 사람이 없어요ㅠ",
          "original_form": "좋으시겠어요  저는 같이 칠 사람이 없어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017643.1.10",
          "form": "또 다른 운동 하시는 거 있나요?",
          "original_form": "또 다른 운동 하시는 거 있나요?",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017643.1.11",
          "form": "걷기? 만보걷기 해요",
          "original_form": "걷기? 만보걷기 해요",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017643.1.12",
          "form": "매일매일 만보 채우시는건가요?",
          "original_form": "매일매일 만보 채우시는건가요?",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017643.1.13",
          "form": "매일매일은 아니고",
          "original_form": "매일매일은 아니고",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017643.1.14",
          "form": "그래도 일주일에 5번 정도는 하려고 노력해요ㅋㅋㅋ",
          "original_form": "그래도 일주일에 5번 정도는 하려고 노력해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017643.1.15",
          "form": "멋있어요!",
          "original_form": "멋있어요!",
          "speaker_id": "1",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017643.1.16",
          "form": "저는 한달전까지 헬스 다녔어요",
          "original_form": "저는 한달전까지 헬스 다녔어요",
          "speaker_id": "1",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017643.1.17",
          "form": "지금은 운동 아무것도안해요ㅠ",
          "original_form": "지금은 운동 아무것도안해요ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017643.1.18",
          "form": "그래도 헬스.다닌게 대단하신걸여",
          "original_form": "그래도 헬스.다닌게 대단하신걸여",
          "speaker_id": "2",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017643.1.19",
          "form": "피티끝나고 헬스장이용권만 남으니까 확실히 안가게되더라구요",
          "original_form": "피티끝나고 헬스장이용권만 남으니까 확실히 안가게되더라구요",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017643.1.20",
          "form": "전 강제성이 필요해요ㅎㅎ",
          "original_form": "전 강제성이 필요해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017643.1.21",
          "form": "저도요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017643.1.22",
          "form": "운동은 확실히 누가 하라고 해야",
          "original_form": "운동은 확실히 누가 하라고 해야",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017643.1.23",
          "form": "하기가 쉬운거 같아여 ㅋㅋㅋ",
          "original_form": "하기가 쉬운거 같아여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:45"
        }
      ]
    }
  ]
