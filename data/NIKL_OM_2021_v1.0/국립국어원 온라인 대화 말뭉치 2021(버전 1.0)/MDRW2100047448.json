      "utterance": [
        {
          "id": "MDRW2100047448.1.1",
          "form": "올드페리 도넛은 드셔보셨어요 ??",
          "original_form": "올드페리 도넛은 드셔보셨어요 ??",
          "speaker_id": "1",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100047448.1.2",
          "form": "아 들어봤는데 아직...드셔보셨어요?",
          "original_form": "아 들어봤는데 아직...드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100047448.1.3",
          "form": "네네 ! 한번 드셔보세요 ㅎㅎ",
          "original_form": "네네 ! 한번 드셔보세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100047448.1.4",
          "form": "저는 거기 도넛이 제일 맛있었어요",
          "original_form": "저는 거기 도넛이 제일 맛있었어요",
          "speaker_id": "1",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100047448.1.5",
          "form": "오 꼭 먹어봐야겠네요 ㅎㅎ요즘 딱 입에 맞는 도넛 찾기힘들어서ㅠㅠ",
          "original_form": "오 꼭 먹어봐야겠네요 ㅎㅎ요즘 딱 입에 맞는 도넛 찾기힘들어서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100047448.1.6",
          "form": "맞아요 ㅎㅎ 비싸기만 하고 ㅠㅠ",
          "original_form": "맞아요 ㅎㅎ 비싸기만 하고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100047448.1.7",
          "form": "가격대비 괜찮은곳은 랜디스인거같구",
          "original_form": "가격대비 괜찮은곳은 랜디스인거같구",
          "speaker_id": "1",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100047448.1.8",
          "form": "인스타맛집에 몇번데였어요ㅠㅠㅋㅋ",
          "original_form": "인스타맛집에 몇번데였어요ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100047448.1.9",
          "form": "아 인스타맛집은 진짜 믿으면 안되는거같아요 ㅋㅋ",
          "original_form": "아 인스타맛집은 진짜 믿으면 안되는거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100047448.1.10",
          "form": "약간 블로그맛집 .. 느낌이랄까",
          "original_form": "약간 블로그맛집 .. 느낌이랄까",
          "speaker_id": "1",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100047448.1.11",
          "form": "정말...ㅋㅋㅋㅋ그니까요 근데 사진이 너무ㅋㅋㅋ잘나와서",
          "original_form": "정말...ㅋㅋㅋㅋ그니까요 근데 사진이 너무ㅋㅋㅋ잘나와서",
          "speaker_id": "2",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100047448.1.12",
          "form": "맞아요 ㅋㄲ 또 인스타는 광고로 뜨니까",
          "original_form": "맞아요 ㅋㄲ 또 인스타는 광고로 뜨니까",
          "speaker_id": "1",
          "time": "20210826 16:26"
        },
        {
          "id": "MDRW2100047448.1.13",
          "form": "안볼수도 없고 혹하게 되는거같아요",
          "original_form": "안볼수도 없고 혹하게 되는거같아요",
          "speaker_id": "1",
          "time": "20210826 16:27"
        },
        {
          "id": "MDRW2100047448.1.14",
          "form": "보통 빵이 많이 뜨더라구요 ㅋㅋ",
          "original_form": "보통 빵이 많이 뜨더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:27"
        },
        {
          "id": "MDRW2100047448.1.15",
          "form": "네 제가 빵좋아하는거 어떻게 알구ㅋㅋㅋ",
          "original_form": "네 제가 빵좋아하는거 어떻게 알구ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:27"
        },
        {
          "id": "MDRW2100047448.1.16",
          "form": "인스타 무서워뇨 추천하는거보면ㅋㅋ",
          "original_form": "인스타 무서워뇨 추천하는거보면ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:27"
        },
        {
          "id": "MDRW2100047448.1.17",
          "form": "맞아요 ㅋㅋ 진짜 네이버에서 검색 한번 하면",
          "original_form": "맞아요 ㅋㅋ 진짜 네이버에서 검색 한번 하면",
          "speaker_id": "1",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100047448.1.18",
          "form": "인스타 들어가면 검색했던거 뜨지않아요 ㅋㅋ ?",
          "original_form": "인스타 들어가면 검색했던거 뜨지않아요 ㅋㅋ ?",
          "speaker_id": "1",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100047448.1.19",
          "form": "네 귀신같이 추천해요ㅠㅠㅋㅋㅋ",
          "original_form": "네 귀신같이 추천해요ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100047448.1.20",
          "form": "그 찾아본 빵집이 뜬적도 있어요ㅋㅋ",
          "original_form": "그 찾아본 빵집이 뜬적도 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100047448.1.21",
          "form": "와 그건 진짜 ㅋㅋ 무서운데요 ㅋㅋ",
          "original_form": "와 그건 진짜 ㅋㅋ 무서운데요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100047448.1.22",
          "form": "근데 빵집은 ㅎㅎ 사진을 보면 거부할수가 없어요",
          "original_form": "근데 빵집은 ㅎㅎ 사진을 보면 거부할수가 없어요",
          "speaker_id": "1",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100047448.1.23",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
