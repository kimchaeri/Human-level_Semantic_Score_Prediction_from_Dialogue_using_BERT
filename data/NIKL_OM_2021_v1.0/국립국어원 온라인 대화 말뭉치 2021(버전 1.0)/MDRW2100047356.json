        },
        {
          "id": "MDRW2100047356.1.3",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100047356.1.4",
          "form": "네 좋아해요 코로나이후론 못 가지만 ㅠㅠ",
          "original_form": "네 좋아해요 코로나이후론 못 가지만 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100047356.1.5",
          "form": "저도요ㅠ",
          "original_form": "저도요ㅠ",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100047356.1.6",
          "form": "강릉가고싶어요",
          "original_form": "강릉가고싶어요",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100047356.1.7",
          "form": "여수나요",
          "original_form": "여수나요",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100047356.1.8",
          "form": "아 여수 가봤는데 좋았어요!!!",
          "original_form": "아 여수 가봤는데 좋았어요!!!",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100047356.1.9",
          "form": "야경이 진짜 멋져요",
          "original_form": "야경이 진짜 멋져요",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100047356.1.10",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100047356.1.11",
          "form": "여수밤바다~~~!",
          "original_form": "여수밤바다~~~!",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100047356.1.12",
          "form": "네 그 음악이 왜 나왔는지 알거같았어요 ㅋㅋㅋ",
          "original_form": "네 그 음악이 왜 나왔는지 알거같았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100047356.1.13",
          "form": "게장도 맛있고",
          "original_form": "게장도 맛있고",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100047356.1.14",
          "form": "ㅋㅋㅋㅋ 저먹고왔죠 여수갔을때",
          "original_form": "ㅋㅋㅋㅋ 저먹고왔죠 여수갔을때",
          "speaker_id": "2",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100047356.1.15",
          "form": "서대회 아세요?",
          "original_form": "서대회 아세요?",
          "speaker_id": "2",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100047356.1.16",
          "form": "아 얘기만 듣고 못 먹어봤어요 기막힌가요??",
          "original_form": "아 얘기만 듣고 못 먹어봤어요 기막힌가요??",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100047356.1.17",
          "form": "ㅋㅋㅋㅋ 맛있더라고요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 맛있더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100047356.1.18",
          "form": "근데 게장이 더맛있긴했어요",
          "original_form": "근데 게장이 더맛있긴했어요",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100047356.1.19",
          "form": "아 또가고싶네요",
          "original_form": "아 또가고싶네요",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100047356.1.20",
          "form": "강릉도 가보신 건가요 전 커피 마시러 가고싶은데 ㅋㅋ",
          "original_form": "강릉도 가보신 건가요 전 커피 마시러 가고싶은데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100047356.1.21",
          "form": "테레로사요?!!!",
          "original_form": "테레로사요?!!!",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100047356.1.22",
          "form": "아 거기도 가보고싶고 그냥 커피 유명허니까 개인카페 등등이여 ㅎㅎ",
          "original_form": "아 거기도 가보고싶고 그냥 커피 유명허니까 개인카페 등등이여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100047356.1.23",
          "form": "ㅋㅋㅋ 강릉너무좋죠",
