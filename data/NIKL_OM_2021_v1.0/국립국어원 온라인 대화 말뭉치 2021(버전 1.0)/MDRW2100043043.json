      "utterance": [
        {
          "id": "MDRW2100043043.1.1",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100043043.1.2",
          "form": "네 고3..ㅎㅎ",
          "original_form": "네 고3..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100043043.1.3",
          "form": "앗 고3이시구나!!",
          "original_form": "앗 고3이시구나!!",
          "speaker_id": "1",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100043043.1.4",
          "form": "지금 그럼 학교..?",
          "original_form": "지금 그럼 학교..?",
          "speaker_id": "1",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043043.1.5",
          "form": "맞아요  ㅋㅋㅋㅋ 담주에 수시원서낼기간이라 폰안걷어가서 하고있어요",
          "original_form": "맞아요  ㅋㅋㅋㅋ 담주에 수시원서낼기간이라 폰안걷어가서 하고있어요",
          "speaker_id": "2",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043043.1.6",
          "form": "공부하는데 방해하고 있는 것 아닌가요?ㅜ",
          "original_form": "공부하는데 방해하고 있는 것 아닌가요?ㅜ",
          "speaker_id": "1",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043043.1.7",
          "form": "지금 쉬는시간 입니당 ㅋㅋㅋㅋ",
          "original_form": "지금 쉬는시간 입니당 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043043.1.8",
          "form": "아핫...그렇군요!!",
          "original_form": "아핫...그렇군요!!",
          "speaker_id": "1",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043043.1.9",
          "form": "고3 9월이면 한참 힘들때네요ㅜ",
          "original_form": "고3 9월이면 한참 힘들때네요ㅜ",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043043.1.10",
          "form": "수험생활은 할말 한가요?",
          "original_form": "수험생활은 할말 한가요?",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043043.1.11",
          "form": "넹 나름.. 사실 오늘 아침에 지각했어요 ㅋㅋㅋ 기숙사에서 늦잠잔..",
          "original_form": "넹 나름.. 사실 오늘 아침에 지각했어요 ㅋㅋㅋ 기숙사에서 늦잠잔..",
          "speaker_id": "2",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043043.1.12",
          "form": "아 기숙생활 하시는구나",
          "original_form": "아 기숙생활 하시는구나",
          "speaker_id": "1",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100043043.1.13",
          "form": "그럼 정말 학교에서 지내는 시간이 길겠네요",
          "original_form": "그럼 정말 학교에서 지내는 시간이 길겠네요",
          "speaker_id": "1",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100043043.1.14",
          "form": "네 주말에만 집가는데 그나마도 24시간이 안되어서 ㅋㅋㅋㅋㅋ",
          "original_form": "네 주말에만 집가는데 그나마도 24시간이 안되어서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043043.1.15",
          "form": "근데 꽤 재밌어요",
          "original_form": "근데 꽤 재밌어요",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043043.1.16",
          "form": "아아 그쵸",
          "original_form": "아아 그쵸",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043043.1.17",
          "form": "저도 지나고보니 고3때 친구들과 자습하고 놀았던 기억이 제일 많이 남더라고요",
          "original_form": "저도 지나고보니 고3때 친구들과 자습하고 놀았던 기억이 제일 많이 남더라고요",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043043.1.18",
          "form": "맞아요 학교에 오래붙어있더보니까 더 기억에 잘 남는거같아요",
          "original_form": "맞아요 학교에 오래붙어있더보니까 더 기억에 잘 남는거같아요",
          "speaker_id": "2",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100043043.1.19",
          "form": "네 시험이나 학업 스트레스는 커도",
          "original_form": "네 시험이나 학업 스트레스는 커도",
          "speaker_id": "1",
          "time": "20210907 13:40"
        },
        {
          "id": "MDRW2100043043.1.20",
          "form": "또 친구들과 추억도 가장 많이 쌓을 수 있는 시기죠",
          "original_form": "또 친구들과 추억도 가장 많이 쌓을 수 있는 시기죠",
          "speaker_id": "1",
          "time": "20210907 13:40"
        }
      ]
