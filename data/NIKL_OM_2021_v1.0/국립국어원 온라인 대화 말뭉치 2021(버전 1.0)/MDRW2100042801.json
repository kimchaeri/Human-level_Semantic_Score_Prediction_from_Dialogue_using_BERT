      "utterance": [
        {
          "id": "MDRW2100042801.1.1",
          "form": "안녕하세요 비가 많이와요!",
          "original_form": "안녕하세요 비가 많이와요!",
          "speaker_id": "1",
          "time": "20210907 09:21"
        },
        {
          "id": "MDRW2100042801.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210907 09:24"
        },
        {
          "id": "MDRW2100042801.1.3",
          "form": "그러니까요ㅜ 그만왔으면...ㅜ",
          "original_form": "그러니까요ㅜ 그만왔으면...ㅜ",
          "speaker_id": "2",
          "time": "20210907 09:24"
        },
        {
          "id": "MDRW2100042801.1.4",
          "form": "어제는 정말 하루종일 오더라구요",
          "original_form": "어제는 정말 하루종일 오더라구요",
          "speaker_id": "1",
          "time": "20210907 09:24"
        },
        {
          "id": "MDRW2100042801.1.5",
          "form": "오늘도 하루종일 오려나요",
          "original_form": "오늘도 하루종일 오려나요",
          "speaker_id": "1",
          "time": "20210907 09:24"
        },
        {
          "id": "MDRW2100042801.1.6",
          "form": "내일 오후까지 온대요",
          "original_form": "내일 오후까지 온대요",
          "speaker_id": "2",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042801.1.7",
          "form": "지역이 어디세요?",
          "original_form": "지역이 어디세요?",
          "speaker_id": "2",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042801.1.8",
          "form": "내일까지요??",
          "original_form": "내일까지요??",
          "speaker_id": "1",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042801.1.9",
          "form": "저 수도권이요",
          "original_form": "저 수도권이요",
          "speaker_id": "1",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042801.1.10",
          "form": "많이 오네요..",
          "original_form": "많이 오네요..",
          "speaker_id": "1",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042801.1.11",
          "form": "저도 서울인데 아깐 좀 덜하더니...ㅜ",
          "original_form": "저도 서울인데 아깐 좀 덜하더니...ㅜ",
          "speaker_id": "2",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042801.1.12",
          "form": "ㅍㅓ붓네요",
          "original_form": "ㅍㅓ붓네요",
          "speaker_id": "2",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042801.1.13",
          "form": "지금 퍼부어요???",
          "original_form": "지금 퍼부어요???",
          "speaker_id": "1",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042801.1.14",
          "form": "아니 근데 요즘 서울이라도",
          "original_form": "아니 근데 요즘 서울이라도",
          "speaker_id": "1",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042801.1.15",
          "form": "서쪽이랑 동쪽 날씨가 다르더라고요???",
          "original_form": "서쪽이랑 동쪽 날씨가 다르더라고요???",
          "speaker_id": "1",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042801.1.16",
          "form": "네 가산동 앞이 안보이네요",
          "original_form": "네 가산동 앞이 안보이네요",
          "speaker_id": "2",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042801.1.17",
          "form": "맞아요 좁은 땅인데ㅋㅋ",
          "original_form": "맞아요 좁은 땅인데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042801.1.18",
          "form": "헉 그정도라니",
          "original_form": "헉 그정도라니",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042801.1.19",
          "form": "여긴 강남인데 사무실 안에라 잘 안보이네요",
          "original_form": "여긴 강남인데 사무실 안에라 잘 안보이네요",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042801.1.20",
          "form": "근데 소리들어보면 많이오는것 같기도 해요",
          "original_form": "근데 소리들어보면 많이오는것 같기도 해요",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042801.1.21",
          "form": "또 지금은 잠잠",
          "original_form": "또 지금은 잠잠",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042801.1.22",
          "form": "아니 요즘 서쪽이 비가 그렇게 퍼붓더라고요.... 어이없어..",
          "original_form": "아니 요즘 서쪽이 비가 그렇게 퍼붓더라고요.... 어이없어..",
          "speaker_id": "1",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042801.1.23",
          "form": "저 집이 서쪽이라",
          "original_form": "저 집이 서쪽이라",
          "speaker_id": "1",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042801.1.24",
          "form": "항상 퇴근하고 집가면 비가 억수로..",
          "original_form": "항상 퇴근하고 집가면 비가 억수로..",
          "speaker_id": "1",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042801.1.25",
          "form": "억울",
          "original_form": "억울",
          "speaker_id": "1",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042801.1.26",
          "form": "서쪽이면 강서구쪽인가요..?",
          "original_form": "서쪽이면 강서구쪽인가요..?",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042801.1.27",
          "form": "저도 금천구에서 동작구 가는데도 시간차 날씨더라고요",
          "original_form": "저도 금천구에서 동작구 가는데도 시간차 날씨더라고요",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042801.1.28",
          "form": "헉 맞아요 강서구 ㅋㅋㅋ",
          "original_form": "헉 맞아요 강서구 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042801.1.29",
          "form": "그니까요 아니 서울이 얼마나 컷다고..",
          "original_form": "그니까요 아니 서울이 얼마나 컷다고..",
          "speaker_id": "1",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100042801.1.30",
          "form": "ㅋㅋ 진짜 코딱지만한 땅인데요ㅋㅋ",
          "original_form": "ㅋㅋ 진짜 코딱지만한 땅인데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:42"
        }
      ]
    }
