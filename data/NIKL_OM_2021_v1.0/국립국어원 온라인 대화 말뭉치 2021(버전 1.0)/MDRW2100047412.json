      "utterance": [
        {
          "id": "MDRW2100047412.1.1",
          "form": "점심은 드셨죠?",
          "original_form": "점심은 드셨죠?",
          "speaker_id": "2",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100047412.1.2",
          "form": "뭐 드셨어요?ㅎㅎ",
          "original_form": "뭐 드셨어요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100047412.1.3",
          "form": "닭이요ㅎㅎ",
          "original_form": "닭이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100047412.1.4",
          "form": "닭요리 드셨구나~",
          "original_form": "닭요리 드셨구나~",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100047412.1.5",
          "form": "저도 오늘 점심메뉴가 닭갈비였는데ㅎㅎ",
          "original_form": "저도 오늘 점심메뉴가 닭갈비였는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100047412.1.6",
          "form": "닭 좋아하세요?",
          "original_form": "닭 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100047412.1.7",
          "form": "닭요리 좋아해요",
          "original_form": "닭요리 좋아해요",
          "speaker_id": "2",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100047412.1.8",
          "form": "그런데 가족 중에 알러지 있는 사람이 있어서",
          "original_form": "그런데 가족 중에 알러지 있는 사람이 있어서",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100047412.1.9",
          "form": "주로 밖에서 먹을 때 닭요리 먹어요",
          "original_form": "주로 밖에서 먹을 때 닭요리 먹어요",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100047412.1.10",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100047412.1.11",
          "form": "치킨 좋아하세요 ?",
          "original_form": "치킨 좋아하세요 ?",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100047412.1.12",
          "form": "치킨도 좋아하죠!",
          "original_form": "치킨도 좋아하죠!",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100047412.1.13",
          "form": "뿌링클vs고추바사삭",
          "original_form": "뿌링클vs고추바사삭",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100047412.1.14",
          "form": "후자요!!",
          "original_form": "후자요!!",
          "speaker_id": "2",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100047412.1.15",
          "form": "뿌링클은 제 입엔 좀 짜더라고욤",
          "original_form": "뿌링클은 제 입엔 좀 짜더라고욤",
          "speaker_id": "2",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100047412.1.16",
          "form": "치즈가루가",
          "original_form": "치즈가루가",
          "speaker_id": "2",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100047412.1.17",
          "form": "순살vs뼈",
          "original_form": "순살vs뼈",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100047412.1.18",
          "form": "ㅎㅎ저는 순살이요",
          "original_form": "ㅎㅎ저는 순살이요",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100047412.1.19",
          "form": "뼈는 후처리가 힘들어요",
          "original_form": "뼈는 후처리가 힘들어요",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100047412.1.20",
          "form": "저도요!!",
          "original_form": "저도요!!",
