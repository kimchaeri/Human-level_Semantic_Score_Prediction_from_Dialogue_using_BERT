      "utterance": [
        {
          "id": "MDRW2100008810.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210822 18:53"
        },
        {
          "id": "MDRW2100008810.1.2",
          "form": "안녕하세요 ~",
          "original_form": "안녕하세요 ~",
          "speaker_id": "2",
          "time": "20210822 18:53"
        },
        {
          "id": "MDRW2100008810.1.3",
          "form": "저녁은 뭐드시나요",
          "original_form": "저녁은 뭐드시나요",
          "speaker_id": "2",
          "time": "20210822 18:53"
        },
        {
          "id": "MDRW2100008810.1.4",
          "form": "돼지불고기 먹으려고 해요~",
          "original_form": "돼지불고기 먹으려고 해요~",
          "speaker_id": "1",
          "time": "20210822 18:55"
        },
        {
          "id": "MDRW2100008810.1.5",
          "form": "부럽네요 전 빵부스러기먹다가",
          "original_form": "부럽네요 전 빵부스러기먹다가",
          "speaker_id": "2",
          "time": "20210822 18:55"
        },
        {
          "id": "MDRW2100008810.1.6",
          "form": "밥먹으러 나가려구요ㅜㅜㅜㅜㅜ",
          "original_form": "밥먹으러 나가려구요ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 18:56"
        },
        {
          "id": "MDRW2100008810.1.7",
          "form": "나가서 더 맛있는거 드시면 되죠~",
          "original_form": "나가서 더 맛있는거 드시면 되죠~",
          "speaker_id": "1",
          "time": "20210822 18:58"
        },
        {
          "id": "MDRW2100008810.1.8",
          "form": "아 하던일 다하고 가려했는데 늦어지네요",
          "original_form": "아 하던일 다하고 가려했는데 늦어지네요",
          "speaker_id": "2",
          "time": "20210822 18:59"
        },
        {
          "id": "MDRW2100008810.1.9",
          "form": "씨유에 폴바셋아이스크림 사먹으려구",
          "original_form": "씨유에 폴바셋아이스크림 사먹으려구",
          "speaker_id": "2",
          "time": "20210822 18:59"
        },
        {
          "id": "MDRW2100008810.1.10",
          "form": "후식은정했는데ㅋㅋㅋ 저녁메뉴가 아직이네요",
          "original_form": "후식은정했는데ㅋㅋㅋ 저녁메뉴가 아직이네요",
          "speaker_id": "2",
          "time": "20210822 18:59"
        },
        {
          "id": "MDRW2100008810.1.11",
          "form": "꺄~ 폴바셋! 여기는 그게 시내에만 있어서 ㅠ 아직 한번도 못 먹어봤어요ㅠ",
          "original_form": "꺄~ 폴바셋! 여기는 그게 시내에만 있어서 ㅠ 아직 한번도 못 먹어봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008810.1.12",
          "form": "CU에 들어왔다고해서 한번 찾아보려구요",
          "original_form": "CU에 들어왔다고해서 한번 찾아보려구요",
          "speaker_id": "2",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008810.1.13",
          "form": "아 씨유를 앞에 못봤네요ㅋㅋㅋㅋ",
          "original_form": "아 씨유를 앞에 못봤네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100008810.1.14",
          "form": "저도 폴바셋매장은 넘멀어서ㅋㅋ 이벤트때 투플원 하더라구요 쟁여놓으려구요ㅋ",
          "original_form": "저도 폴바셋매장은 넘멀어서ㅋㅋ 이벤트때 투플원 하더라구요 쟁여놓으려구요ㅋ",
          "speaker_id": "2",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008810.1.15",
          "form": "검색좀 해봐야겠어요~ㅎㅎ 맛있겠네용",
          "original_form": "검색좀 해봐야겠어요~ㅎㅎ 맛있겠네용",
          "speaker_id": "1",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008810.1.16",
          "form": "앗 유용했네요ㅎㅎ 빨 저녁먹어야겠어요 떡이라두 사먹어야지ㅎㅎ",
          "original_form": "앗 유용했네요ㅎㅎ 빨 저녁먹어야겠어요 떡이라두 사먹어야지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008810.1.17",
          "form": "어서 드시구 폴바셋 먹고 후기좀 남겨주세요",
          "original_form": "어서 드시구 폴바셋 먹고 후기좀 남겨주세요",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008810.1.18",
          "form": "집에가다봤는데",
          "original_form": "집에가다봤는데",
          "speaker_id": "2",
          "time": "20210823 10:58"
        },
        {
          "id": "MDRW2100008810.1.19",
          "form": "베스킨 다음달 신제품이 맥심모카골드맛이더라구요",
