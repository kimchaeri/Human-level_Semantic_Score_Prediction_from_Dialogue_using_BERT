      "utterance": [
        {
          "id": "MDRW2100010904.1.1",
          "form": "애들도 같이 해서 좋더라 ㅎㅎ",
          "original_form": "애들도 같이 해서 좋더라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100010904.1.2",
          "form": "자세가 많이 좋아졌어",
          "original_form": "자세가 많이 좋아졌어",
          "speaker_id": "1",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100010904.1.3",
          "form": "신랑이 매번 알려주니까",
          "original_form": "신랑이 매번 알려주니까",
          "speaker_id": "2",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100010904.1.4",
          "form": "벌써 큰아들은 허벅지 쪽에 근육 붙었더라",
          "original_form": "벌써 큰아들은 허벅지 쪽에 근육 붙었더라",
          "speaker_id": "1",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100010904.1.5",
          "form": "아주 굿이얌",
          "original_form": "아주 굿이얌",
          "speaker_id": "2",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100010904.1.6",
          "form": "자세 좋아지니 허리도 펴지고 ㅎㅎ",
          "original_form": "자세 좋아지니 허리도 펴지고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100010904.1.7",
          "form": "운동하니 요즘에 피곤하다는 소리 덜하는 것 같아",
          "original_form": "운동하니 요즘에 피곤하다는 소리 덜하는 것 같아",
          "speaker_id": "2",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100010904.1.8",
          "form": "그래서 꾸준히 해야해",
          "original_form": "그래서 꾸준히 해야해",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010904.1.9",
          "form": "저녁마다 좀 힘들겠지만 신랑 운동하면서 애들도 좀 부탁해 ㅎㅎ",
          "original_form": "저녁마다 좀 힘들겠지만 신랑 운동하면서 애들도 좀 부탁해 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010904.1.10",
          "form": "걱정마~ 내 전문이니까",
          "original_form": "걱정마~ 내 전문이니까",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010904.1.11",
          "form": "이번에는 왠지 다이어트 성공할 것 같은 좋은 예감",
          "original_form": "이번에는 왠지 다이어트 성공할 것 같은 좋은 예감",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010904.1.12",
          "form": "ㅎㅎ 시작이 좋아",
          "original_form": "ㅎㅎ 시작이 좋아",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010904.1.13",
          "form": "이대로 쭉 가면 좋겠다 그만두는 일 없이 ㅎㅎ",
          "original_form": "이대로 쭉 가면 좋겠다 그만두는 일 없이 ㅎㅎ",
