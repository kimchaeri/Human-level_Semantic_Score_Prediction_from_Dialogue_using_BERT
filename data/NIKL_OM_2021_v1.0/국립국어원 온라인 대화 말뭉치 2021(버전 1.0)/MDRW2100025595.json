      "utterance": [
        {
          "id": "MDRW2100025595.1.1",
          "form": "정말 폴바셋 맛있는데...",
          "original_form": "정말 폴바셋 맛있는데...",
          "speaker_id": "2",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100025595.1.2",
          "form": "매장이진짜 너무적어요",
          "original_form": "매장이진짜 너무적어요",
          "speaker_id": "2",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100025595.1.3",
          "form": "맞아요.. 거의 큰 쇼핑몰 가야 있어서..",
          "original_form": "맞아요.. 거의 큰 쇼핑몰 가야 있어서..",
          "speaker_id": "1",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100025595.1.4",
          "form": "너무 접근성이 ㅠㅠ",
          "original_form": "너무 접근성이 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100025595.1.5",
          "form": "맞아요....초반엔 좀 느ㄴ했는데",
          "original_form": "맞아요....초반엔 좀 느ㄴ했는데",
          "speaker_id": "2",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100025595.1.6",
          "form": "코로나이후로 많이 줄었더라구요",
          "original_form": "코로나이후로 많이 줄었더라구요",
          "speaker_id": "2",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100025595.1.7",
          "form": "아 그게 영향이 있었군요 저런 ㅠㅠ",
          "original_form": "아 그게 영향이 있었군요 저런 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025595.1.8",
          "form": "서울이시면 성신여대 근처에 카페 다두 라고 있는데 나중에 한번 가보세요 여기 커피 레어한 메뉴가 많아요",
          "original_form": "서울이시면 성신여대 근처에 카페 다두 라고 있는데 나중에 한번 가보세요 여기 커피 레어한 메뉴가 많아요",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025595.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100025595.1.10",
          "form": "성신여대 예전에 자주갔었는데!",
          "original_form": "성신여대 예전에 자주갔었는데!",
          "speaker_id": "2",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100025595.1.11",
          "form": "나중에 한번가봐야겠머요",
          "original_form": "나중에 한번가봐야겠머요",
          "speaker_id": "2",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100025595.1.12",
          "form": "네 제가 그쪽 살다 이사와서 지금은 못가네유.. ㅎㅎ",
          "original_form": "네 제가 그쪽 살다 이사와서 지금은 못가네유.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100025595.1.13",
          "form": "저도 예전에 친구가 여기살아서",
          "original_form": "저도 예전에 친구가 여기살아서",
          "speaker_id": "2",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025595.1.14",
          "form": "자주갔었어ㅇㅋㅋㅋ",
          "original_form": "자주갔었어ㅇㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025595.1.15",
          "form": "오 어쩌면 스쳐지나갔을수도 있겠네요 ㅋㅋㅋ",
          "original_form": "오 어쩌면 스쳐지나갔을수도 있겠네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025595.1.16",
          "form": "갔었어요",
          "original_form": "갔었어요",
          "speaker_id": "2",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025595.1.17",
          "form": "닭갈비집 자주",
          "original_form": "닭갈비집 자주",
          "speaker_id": "2",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025595.1.18",
          "form": "갔었는데",
          "original_form": "갔었는데",
          "speaker_id": "2",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025595.1.19",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100025595.1.20",
          "form": "거기 식당이 진짜 많긴 해요",
          "original_form": "거기 식당이 진짜 많긴 해요",
          "speaker_id": "1",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100025595.1.21",
          "form": "그쵸 ㅋㅋㅋㄱ 근데지금은 가던곳들이 다 사라졌더라구요",
          "original_form": "그쵸 ㅋㅋㅋㄱ 근데지금은 가던곳들이 다 사라졌더라구요",
          "speaker_id": "2",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100025595.1.22",
          "form": "맞아요 거기가 유행에 민감해서인지 진짜 자주 바껴요 ㅋㅋㅋ",
          "original_form": "맞아요 거기가 유행에 민감해서인지 진짜 자주 바껴요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100025595.1.23",
          "form": "마카롱집 이런건 한 2년 주기로 없어지더라고요 ㅋㅋ 유행하는 음식으로 ㅋㅋ",
          "original_form": "마카롱집 이런건 한 2년 주기로 없어지더라고요 ㅋㅋ 유행하는 음식으로 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100025595.1.24",
          "form": "그쵸...혜화도그렇고",
          "original_form": "그쵸...혜화도그렇고",
          "speaker_id": "2",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025595.1.25",
          "form": "오래남아있는집들이 없어서",
          "original_form": "오래남아있는집들이 없어서",
          "speaker_id": "2",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025595.1.26",
          "form": "거기에 코로나여파로 점점 없어지는거 같네요 가게들이 ㅠㅠ",
          "original_form": "거기에 코로나여파로 점점 없어지는거 같네요 가게들이 ㅠㅠ",
