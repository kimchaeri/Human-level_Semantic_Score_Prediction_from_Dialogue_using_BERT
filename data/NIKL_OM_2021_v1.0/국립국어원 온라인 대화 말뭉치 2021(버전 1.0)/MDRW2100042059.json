      "utterance": [
        {
          "id": "MDRW2100042059.1.1",
          "form": "가을비가 오늘도 오네요",
          "original_form": "가을비가 오늘도 오네요",
          "speaker_id": "1",
          "time": "20210906 19:15"
        },
        {
          "id": "MDRW2100042059.1.2",
          "form": "앗 비오나요",
          "original_form": "앗 비오나요",
          "speaker_id": "2",
          "time": "20210906 19:24"
        },
        {
          "id": "MDRW2100042059.1.3",
          "form": "저녁에 비온다던데 잘 맞추네요 기상청",
          "original_form": "저녁에 비온다던데 잘 맞추네요 기상청",
          "speaker_id": "2",
          "time": "20210906 19:24"
        },
        {
          "id": "MDRW2100042059.1.4",
          "form": "지금 여긴 비 엄청 많이 와요",
          "original_form": "지금 여긴 비 엄청 많이 와요",
          "speaker_id": "1",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042059.1.5",
          "form": "가을에비많이오면",
          "original_form": "가을에비많이오면",
          "speaker_id": "2",
          "time": "20210907 12:32"
        },
        {
          "id": "MDRW2100042059.1.6",
          "form": "농사짓는 사람들 걱정이 많겠네요",
          "original_form": "농사짓는 사람들 걱정이 많겠네요",
          "speaker_id": "2",
          "time": "20210907 12:32"
        },
        {
          "id": "MDRW2100042059.1.7",
          "form": "과일 많이 떨어 지겠죠",
          "original_form": "과일 많이 떨어 지겠죠",
          "speaker_id": "1",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100042059.1.8",
          "form": "그쵸 안그래도 과일값 비싼데",
          "original_form": "그쵸 안그래도 과일값 비싼데",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100042059.1.9",
          "form": "이 계절에 장마는 첨봐오ㅡ",
          "original_form": "이 계절에 장마는 첨봐오ㅡ",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100042059.1.10",
          "form": "예전에 10월쯤에 태풍올때도있는데",
          "original_form": "예전에 10월쯤에 태풍올때도있는데",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100042059.1.11",
          "form": "태풍은 안왔음 좋겠네요",
          "original_form": "태풍은 안왔음 좋겠네요",
          "speaker_id": "2",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100042059.1.12",
          "form": "태풍도 2~3개가 해마다 추수전에 왔죠",
          "original_form": "태풍도 2~3개가 해마다 추수전에 왔죠",
          "speaker_id": "1",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100042059.1.13",
          "form": "벼쓰러지고 과일 다 떨어지고 했죠",
          "original_form": "벼쓰러지고 과일 다 떨어지고 했죠",
          "speaker_id": "1",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100042059.1.14",
          "form": "그니까요 너무 아까워요",
          "original_form": "그니까요 너무 아까워요",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100042059.1.15",
          "form": "농부들 속타겠어요",
          "original_form": "농부들 속타겠어요",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100042059.1.16",
          "form": "시원해져서 좋긴한데",
          "original_form": "시원해져서 좋긴한데",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100042059.1.17",
          "form": "좀 지나면 추워 지겠죠",
          "original_form": "좀 지나면 추워 지겠죠",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100042059.1.18",
          "form": "지금이 시원하고 좋은데 .....",
          "original_form": "지금이 시원하고 좋은데 .....",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100042059.1.19",
          "form": "네 요즘 너무 좋네요",
          "original_form": "네 요즘 너무 좋네요",
          "speaker_id": "2",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100042059.1.20",
          "form": "여름에 덥다가 한숨돌린거같아요",
          "original_form": "여름에 덥다가 한숨돌린거같아요",
          "speaker_id": "2",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100042059.1.21",
          "form": "옷도 조금 두꺼워 지죠",
          "original_form": "옷도 조금 두꺼워 지죠",
          "speaker_id": "1",
          "time": "20210907 16:36"
        },
        {
          "id": "MDRW2100042059.1.22",
          "form": "밤에 요즘 보일러트세요?",
          "original_form": "밤에 요즘 보일러트세요?",
          "speaker_id": "2",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100042059.1.23",
          "form": "아니요 그렇진 않지만 이불이 두꺼워 졌어요",
          "original_form": "아니요 그렇진 않지만 이불이 두꺼워 졌어요",
          "speaker_id": "1",
          "time": "20210907 16:49"
        }
      ]
