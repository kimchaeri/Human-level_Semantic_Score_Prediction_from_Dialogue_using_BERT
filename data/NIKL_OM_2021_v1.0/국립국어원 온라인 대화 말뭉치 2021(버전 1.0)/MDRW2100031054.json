      "utterance": [
        {
          "id": "MDRW2100031054.1.1",
          "form": "예 굉장히 짜증나네요",
          "original_form": "예 굉장히 짜증나네요",
          "speaker_id": "2",
          "time": "20210904 16:08"
        },
        {
          "id": "MDRW2100031054.1.2",
          "form": "시간도 안가고 좀비가 있는데 겁나 말안듣고",
          "original_form": "시간도 안가고 좀비가 있는데 겁나 말안듣고",
          "speaker_id": "2",
          "time": "20210904 16:09"
        },
        {
          "id": "MDRW2100031054.1.3",
          "form": "뽝치네요",
          "original_form": "뽝치네요",
          "speaker_id": "2",
          "time": "20210904 16:09"
        },
        {
          "id": "MDRW2100031054.1.4",
          "form": "좀비 오늘 활기찬가요..",
          "original_form": "좀비 오늘 활기찬가요..",
          "speaker_id": "1",
          "time": "20210904 16:09"
        },
        {
          "id": "MDRW2100031054.1.5",
          "form": "그것이가장스트레스겠어염",
          "original_form": "그것이가장스트레스겠어염",
          "speaker_id": "1",
          "time": "20210904 16:09"
        },
        {
          "id": "MDRW2100031054.1.6",
          "form": "후",
          "original_form": "후",
          "speaker_id": "2",
          "time": "20210904 16:09"
        },
        {
          "id": "MDRW2100031054.1.7",
          "form": "내 허벅지를 사정없이 때림",
          "original_form": "내 허벅지를 사정없이 때림",
          "speaker_id": "2",
          "time": "20210904 16:10"
        },
        {
          "id": "MDRW2100031054.1.8",
          "form": "조.온.나 스트레스",
          "original_form": "조.온.나 스트레스",
          "speaker_id": "2",
          "time": "20210904 16:10"
        },
        {
          "id": "MDRW2100031054.1.9",
          "form": "ㅋㅋㅋ때렸어여??",
          "original_form": "ㅋㅋㅋ때렸어여??",
          "speaker_id": "1",
          "time": "20210904 18:33"
        },
        {
          "id": "MDRW2100031054.1.10",
          "form": "ㅋㅋㅋ네ㅋㅋ아오 퇴근하니까 좋네요",
          "original_form": "ㅋㅋㅋ네ㅋㅋ아오 퇴근하니까 좋네요",
          "speaker_id": "2",
          "time": "20210904 19:03"
        },
        {
          "id": "MDRW2100031054.1.11",
          "form": "오는길에 영감.탱이가 시비를 걸었지만",
          "original_form": "오는길에 영감.탱이가 시비를 걸었지만",
          "speaker_id": "2",
          "time": "20210904 19:03"
        },
        {
          "id": "MDRW2100031054.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 왜여 뭔일났어여?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 왜여 뭔일났어여?",
          "speaker_id": "1",
          "time": "20210904 19:20"
        },
        {
          "id": "MDRW2100031054.1.13",
          "form": "네 ㅎ 주말에 출근한것도 화가나지만",
          "original_form": "네 ㅎ 주말에 출근한것도 화가나지만",
          "speaker_id": "2",
          "time": "20210904 20:25"
        },
        {
          "id": "MDRW2100031054.1.14",
          "form": "퇴근때 시비걸면서 집 가는길 막는건 더 화나더군요",
          "original_form": "퇴근때 시비걸면서 집 가는길 막는건 더 화나더군요",
          "speaker_id": "2",
          "time": "20210904 20:25"
        },
        {
          "id": "MDRW2100031054.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 안대지안대 누가퇴근길에 시비터여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 안대지안대 누가퇴근길에 시비터여",
