      "utterance": [
        {
          "id": "MDRW2100043661.1.1",
          "form": "음 다이어트중이긴해요",
          "original_form": "음 다이어트중이긴해요",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043661.1.2",
          "form": "앗 진짜 다이어트 하시는 중이었군요",
          "original_form": "앗 진짜 다이어트 하시는 중이었군요",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043661.1.3",
          "form": "저녁엔 적게 드시는 건가요?",
          "original_form": "저녁엔 적게 드시는 건가요?",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043661.1.4",
          "form": "오늘오레오먹어서ㅜㅜ저녁은가볍게먹었어요",
          "original_form": "오늘오레오먹어서ㅜㅜ저녁은가볍게먹었어요",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043661.1.5",
          "form": "아 맛있는데 칼로리가 어마한 것을 드셨군요",
          "original_form": "아 맛있는데 칼로리가 어마한 것을 드셨군요",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043661.1.6",
          "form": "칼로리는맛의단위죠",
          "original_form": "칼로리는맛의단위죠",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043661.1.7",
          "form": "ㅋㅋㅋㅋ큐ㅠㅠㅠㅜㅜㅜ맞아요 칼로리 신경 안 쓰고 먹고 싶어요",
          "original_form": "ㅋㅋㅋㅋ큐ㅠㅠㅠㅜㅜㅜ맞아요 칼로리 신경 안 쓰고 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043661.1.8",
          "form": "ㅎㅎㅎ그럼행복할텐데요",
          "original_form": "ㅎㅎㅎ그럼행복할텐데요",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043661.1.9",
          "form": "그니까요ㅠㅠ",
          "original_form": "그니까요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043661.1.10",
          "form": "이번엔 마라탕 일인분이 삼겹살에 아이스크림 먹은 것보다 칼로리가 높다",
          "original_form": "이번엔 마라탕 일인분이 삼겹살에 아이스크림 먹은 것보다 칼로리가 높다",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043661.1.11",
          "form": "이런 얘기 봐서 넘 우울해요",
          "original_form": "이런 얘기 봐서 넘 우울해요",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043661.1.12",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043661.1.13",
          "form": "국물친거아닐까요",
          "original_form": "국물친거아닐까요",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043661.1.14",
          "form": "그런 거였으면 좋겠어요ㅠㅠ",
          "original_form": "그런 거였으면 좋겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043661.1.15",
          "form": "그럴거에요",
          "original_form": "그럴거에요",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043661.1.16",
          "form": "마라탕 좋아하세요?",
          "original_form": "마라탕 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043661.1.17",
          "form": "네맛있더라고요",
          "original_form": "네맛있더라고요",
          "speaker_id": "1",
          "time": "20210907 20:09"
