      "utterance": [
        {
          "id": "MDRW2100037161.1.1",
          "form": "우리나라 노래가 외국에 나간 거 알아",
          "original_form": "우리나라 노래가 외국에 나간 거 알아",
          "speaker_id": "2",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100037161.1.2",
          "form": "이십세기 힛트송",
          "original_form": "이십세기 힛트송",
          "speaker_id": "2",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100037161.1.3",
          "form": "몇개 있는기 알아요ㅋ",
          "original_form": "몇개 있는기 알아요ㅋ",
          "speaker_id": "1",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100037161.1.4",
          "form": "주주클럽 안나/",
          "original_form": "주주클럽 안나/",
          "speaker_id": "2",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100037161.1.5",
          "form": "이정현의 와",
          "original_form": "이정현의 와",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100037161.1.6",
          "form": "엉 데뷔하자 마다 밀리언셀러로 등극시킨 노래 나는 나 이노래가 번안해서 대만가수가 부름",
          "original_form": "엉 데뷔하자 마다 밀리언셀러로 등극시킨 노래 나는 나 이노래가 번안해서 대만가수가 부름",
          "speaker_id": "2",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100037161.1.7",
          "form": "때때때때로 시작하는거 맞죠?",
          "original_form": "때때때때로 시작하는거 맞죠?",
          "speaker_id": "1",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100037161.1.8",
          "form": "때때때 하며 중독성 강한 노래",
          "original_form": "때때때 하며 중독성 강한 노래",
          "speaker_id": "2",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100037161.1.9",
          "form": "ㅋㅋ 알죠. 전혀 몰랐네요",
          "original_form": "ㅋㅋ 알죠. 전혀 몰랐네요",
          "speaker_id": "1",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100037161.1.10",
          "form": "역시 아네 ㅋㅋ 이게 대만가수가 불러서 중화권을 넘어 동남아까지 인기가 잇었다네",
          "original_form": "역시 아네 ㅋㅋ 이게 대만가수가 불러서 중화권을 넘어 동남아까지 인기가 잇었다네",
          "speaker_id": "2",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100037161.1.11",
          "form": "오 대단한대요",
          "original_form": "오 대단한대요",
          "speaker_id": "1",
          "time": "20210831 21:09"
        },
        {
          "id": "MDRW2100037161.1.12",
          "form": "양파 노래도 잇음",
          "original_form": "양파 노래도 잇음",
          "speaker_id": "2",
          "time": "20210831 21:09"
        },
        {
          "id": "MDRW2100037161.1.13",
          "form": "어떤거죠?",
          "original_form": "어떤거죠?",
          "speaker_id": "1",
          "time": "20210831 21:09"
        },
        {
          "id": "MDRW2100037161.1.14",
          "form": "2집 타이틀곡 알고 싶어요",
          "original_form": "2집 타이틀곡 알고 싶어요",
          "speaker_id": "2",
          "time": "20210831 21:09"
        }
      ]
