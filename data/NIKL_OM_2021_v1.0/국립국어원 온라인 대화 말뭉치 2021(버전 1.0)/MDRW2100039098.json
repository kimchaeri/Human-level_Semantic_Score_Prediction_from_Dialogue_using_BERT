      "utterance": [
        {
          "id": "MDRW2100039098.1.1",
          "form": "넼ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:58"
        },
        {
          "id": "MDRW2100039098.1.2",
          "form": "바다보고싶엉요",
          "original_form": "바다보고싶엉요",
          "speaker_id": "2",
          "time": "20210903 09:59"
        },
        {
          "id": "MDRW2100039098.1.3",
          "form": "부안은 뭐가 맛있었어요???",
          "original_form": "부안은 뭐가 맛있었어요???",
          "speaker_id": "1",
          "time": "20210903 09:59"
        },
        {
          "id": "MDRW2100039098.1.4",
          "form": "앗 바다 좋죠 저도요 222",
          "original_form": "앗 바다 좋죠 저도요 222",
          "speaker_id": "1",
          "time": "20210903 09:59"
        },
        {
          "id": "MDRW2100039098.1.5",
          "form": "부안 ...진짜마싯는데 잇엇는대",
          "original_form": "부안 ...진짜마싯는데 잇엇는대",
          "speaker_id": "2",
          "time": "20210903 09:59"
        },
        {
          "id": "MDRW2100039098.1.6",
          "form": "찍어놧는데 지도에서 사라졋어요 망햇나바요 터미널 근처엿는데 ㅋㅋㅋㅋ",
          "original_form": "찍어놧는데 지도에서 사라졋어요 망햇나바요 터미널 근처엿는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:59"
        },
        {
          "id": "MDRW2100039098.1.7",
          "form": "아니....",
          "original_form": "아니....",
          "speaker_id": "1",
          "time": "20210903 09:59"
        },
        {
          "id": "MDRW2100039098.1.8",
          "form": "한식집이에요???",
          "original_form": "한식집이에요???",
          "speaker_id": "1",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100039098.1.9",
          "form": "네네 ㅠ 두부가튼거 팔앗는대...",
          "original_form": "네네 ㅠ 두부가튼거 팔앗는대...",
          "speaker_id": "2",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100039098.1.10",
          "form": "헐 두부 맛있겠네요",
          "original_form": "헐 두부 맛있겠네요",
          "speaker_id": "1",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100039098.1.11",
          "form": "두부전골 같은 건가요???",
          "original_form": "두부전골 같은 건가요???",
          "speaker_id": "1",
          "time": "20210903 10:01"
        },
        {
          "id": "MDRW2100039098.1.12",
          "form": "순두부도 잇고...",
          "original_form": "순두부도 잇고...",
          "speaker_id": "2",
          "time": "20210903 10:01"
        },
        {
          "id": "MDRW2100039098.1.13",
          "form": "그냥 백반잇고... 그런데엿는뎅",
          "original_form": "그냥 백반잇고... 그런데엿는뎅",
          "speaker_id": "2",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100039098.1.14",
          "form": "아쉽네용  ㅠ",
          "original_form": "아쉽네용  ㅠ",
          "speaker_id": "2",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100039098.1.15",
          "form": "아 터미널에 있는데 사라지다니 너무 아쉬워요ㅠㅠㅠ",
          "original_form": "아 터미널에 있는데 사라지다니 너무 아쉬워요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100039098.1.16",
          "form": "장사가 안되진 않았을텐데요ㅠㅠㅠ",
          "original_form": "장사가 안되진 않았을텐데요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100039098.1.17",
          "form": "저번주에 여수 잠깐 들었다가 기사식당에서 밥이나 먹고 가려고 했는데",
          "original_form": "저번주에 여수 잠깐 들었다가 기사식당에서 밥이나 먹고 가려고 했는데",
          "speaker_id": "1",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100039098.1.18",
          "form": "죄다 문을 닫았더라구요ㅠㅠㅠㅠ 토요일 오후 5시쯤이었는데요....",
          "original_form": "죄다 문을 닫았더라구요ㅠㅠㅠㅠ 토요일 오후 5시쯤이었는데요....",
          "speaker_id": "1",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100039098.1.19",
          "form": "네 잘됏엇는데 ㅠㅠ",
          "original_form": "네 잘됏엇는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 10:05"
        },
        {
          "id": "MDRW2100039098.1.20",
          "form": "ㅜ 코로나때문인가",
          "original_form": "ㅜ 코로나때문인가",
          "speaker_id": "2",
          "time": "20210903 10:05"
        },
        {
          "id": "MDRW2100039098.1.21",
          "form": "그런거 같죠ㅠㅠㅠㅠ",
          "original_form": "그런거 같죠ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 10:05"
        },
        {
          "id": "MDRW2100039098.1.22",
          "form": "보통 백반집 이런 곳은 웬만해서는 장사 될텐데요....",
          "original_form": "보통 백반집 이런 곳은 웬만해서는 장사 될텐데요....",
          "speaker_id": "1",
          "time": "20210903 10:06"
        },
        {
          "id": "MDRW2100039098.1.23",
          "form": "요새는 어딜 가도 밥집찾는게 힘들더라구요",
          "original_form": "요새는 어딜 가도 밥집찾는게 힘들더라구요",
          "speaker_id": "1",
          "time": "20210903 10:07"
        },
        {
          "id": "MDRW2100039098.1.24",
          "form": "ㅜㅜ 마자요 ... 아쉽",
          "original_form": "ㅜㅜ 마자요 ... 아쉽",
          "speaker_id": "2",
          "time": "20210903 10:13"
        },
        {
          "id": "MDRW2100039098.1.25",
          "form": "보통 여행가실 때 맛집 미리 찾아보고 가시나요? 아니면 가서 찾으시나요?",
          "original_form": "보통 여행가실 때 맛집 미리 찾아보고 가시나요? 아니면 가서 찾으시나요?",
          "speaker_id": "1",
          "time": "20210903 10:14"
        },
        {
          "id": "MDRW2100039098.1.26",
          "form": "저요 미리 좀 찾아는 놔요",
          "original_form": "저요 미리 좀 찾아는 놔요",
          "speaker_id": "2",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100039098.1.27",
          "form": "근데 머 정한대로가진 않아요 ㅋㅋ",
          "original_form": "근데 머 정한대로가진 않아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100039098.1.28",
          "form": "앗 저도요 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 저도요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100039098.1.29",
          "form": "정말 그 집이 목적인 여행이 아닌 이상 결국 다른데 가게 되더라구요",
          "original_form": "정말 그 집이 목적인 여행이 아닌 이상 결국 다른데 가게 되더라구요",
          "speaker_id": "1",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100039098.1.30",
          "form": "ㅋㅋㅋ 걍 별표 찍어놓고...",
          "original_form": "ㅋㅋㅋ 걍 별표 찍어놓고...",
          "speaker_id": "2",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100039098.1.31",
          "form": "가까운데잇음 가고",
          "original_form": "가까운데잇음 가고",
          "speaker_id": "2",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100039098.1.32",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 10:19"
        },
        {
          "id": "MDRW2100039098.1.33",
          "form": "대기줄 길면 돌아나오고 ㅋㅋㅋㅋ",
          "original_form": "대기줄 길면 돌아나오고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 10:19"
        }
      ]
