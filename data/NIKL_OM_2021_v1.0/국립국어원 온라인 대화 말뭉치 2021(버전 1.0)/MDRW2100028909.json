        },
        {
          "id": "MDRW2100028909.1.2",
          "form": "애기라 신경많이 쓰시겟아요ㅠ",
          "original_form": "애기라 신경많이 쓰시겟아요ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100028909.1.3",
          "form": "저도 나봐서 아는데 그거 아프진 않은데..",
          "original_form": "저도 나봐서 아는데 그거 아프진 않은데..",
          "speaker_id": "2",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100028909.1.4",
          "form": "심하지 않다고 약도 약라게 주더라구여 ㅎㄹ",
          "original_form": "심하지 않다고 약도 약라게 주더라구여 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100028909.1.5",
          "form": "뭐랄까 내눈에 뭐가 있는듯한 기분이들어서",
          "original_form": "뭐랄까 내눈에 뭐가 있는듯한 기분이들어서",
          "speaker_id": "2",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100028909.1.6",
          "form": "조금 그랬어요",
          "original_form": "조금 그랬어요",
          "speaker_id": "2",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100028909.1.7",
          "form": "간지러워서 자기도모르게 비비는개",
          "original_form": "간지러워서 자기도모르게 비비는개",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100028909.1.8",
          "form": "그래도 그나마 다행이네요ㅎ",
          "original_form": "그래도 그나마 다행이네요ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100028909.1.9",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100028909.1.10",
          "form": "애기는 더 조심해야죠ㅠㅠ",
          "original_form": "애기는 더 조심해야죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100028909.1.11",
          "form": "에고 걱정많이하셨겟내요ㅠ",
          "original_form": "에고 걱정많이하셨겟내요ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100028909.1.12",
          "form": "빨리 낫을꺼에요ㅎㅎ",
          "original_form": "빨리 낫을꺼에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100028909.1.13",
          "form": "아뇨 ㅎㄹ 괜찮아요",
          "original_form": "아뇨 ㅎㄹ 괜찮아요",
          "speaker_id": "1",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100028909.1.14",
          "form": "네넹 금방나을거 같아요 ㅎㅎ",
          "original_form": "네넹 금방나을거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100028909.1.15",
          "form": "애기가 아플때가 가장 힘들죠ㅠㅠ",
