      "utterance": [
        {
          "id": "MDRW2100035235.1.1",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100035235.1.2",
          "form": "새벽까진 엄청 왔어요",
          "original_form": "새벽까진 엄청 왔어요",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100035235.1.3",
          "form": "저도 잠결에 들은 것 같은데 일어나니 해가 있네요ㅋㅋㅋ",
          "original_form": "저도 잠결에 들은 것 같은데 일어나니 해가 있네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:58"
        },
        {
          "id": "MDRW2100035235.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 진짜 너무 밝아져서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 진짜 너무 밝아져서",
          "speaker_id": "2",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100035235.1.5",
          "form": "놀럈어요..",
          "original_form": "놀럈어요..",
          "speaker_id": "2",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100035235.1.6",
          "form": "쨍쨍하진 않지만 그나마 덜 습해서 살만해요ㅋㅋㅋ",
          "original_form": "쨍쨍하진 않지만 그나마 덜 습해서 살만해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100035235.1.7",
          "form": "너무 덥더라구요...",
          "original_form": "너무 덥더라구요...",
          "speaker_id": "2",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100035235.1.8",
          "form": "에어컨 까진 아닌데 선풍기는 틀 날씨...",
          "original_form": "에어컨 까진 아닌데 선풍기는 틀 날씨...",
          "speaker_id": "1",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100035235.1.9",
          "form": "ㅋㅋㅋㅋㅋ 맞아요 ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100035235.1.10",
          "form": "에어컨은 좀 춥구요",
          "original_form": "에어컨은 좀 춥구요",
          "speaker_id": "2",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100035235.1.11",
          "form": "에어컨 켜면 정말 추워지더라구요ㅠ",
          "original_form": "에어컨 켜면 정말 추워지더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100035235.1.12",
          "form": "지금은 선풍기가 적절해요",
          "original_form": "지금은 선풍기가 적절해요",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100035235.1.13",
          "form": "가끔은 선풍기도 춥고요ㅋㅋ",
          "original_form": "가끔은 선풍기도 춥고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100035235.1.14",
          "form": "선풍기 틀면 차가운 걸 못 먹게 돼요 ㅡㅋㅋㅋ",
          "original_form": "선풍기 틀면 차가운 걸 못 먹게 돼요 ㅡㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100035235.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 공감이요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 공감이요",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100035235.1.16",
          "form": "다 녹고 추워요",
          "original_form": "다 녹고 추워요",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100035235.1.17",
          "form": "먹고 이불 덮고 있어야해요ㅋㅋ",
          "original_form": "먹고 이불 덮고 있어야해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100035235.1.18",
          "form": "ㅋㅋㅋㅋ 딱 완벽한 조합",
