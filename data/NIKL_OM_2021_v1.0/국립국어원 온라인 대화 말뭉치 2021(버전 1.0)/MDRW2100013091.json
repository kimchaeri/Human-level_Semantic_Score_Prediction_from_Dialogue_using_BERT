        {
          "id": "MDRW2100013091.1.1",
          "form": "게임 뭐하시나요 ~~?",
          "original_form": "게임 뭐하시나요 ~~?",
          "speaker_id": "2",
          "time": "20210825 12:24"
        },
        {
          "id": "MDRW2100013091.1.2",
          "form": "피파온라인이요~",
          "original_form": "피파온라인이요~",
          "speaker_id": "1",
          "time": "20210825 12:26"
        },
        {
          "id": "MDRW2100013091.1.3",
          "form": "다른 게임은요??",
          "original_form": "다른 게임은요??",
          "speaker_id": "2",
          "time": "20210825 12:26"
        },
        {
          "id": "MDRW2100013091.1.4",
          "form": "배그 예전에 했었어요",
          "original_form": "배그 예전에 했었어요",
          "speaker_id": "1",
          "time": "20210825 12:27"
        },
        {
          "id": "MDRW2100013091.1.5",
          "form": "오 배그 잘하셨나요",
          "original_form": "오 배그 잘하셨나요",
          "speaker_id": "2",
          "time": "20210825 12:30"
        },
        {
          "id": "MDRW2100013091.1.6",
          "form": "배린이었어요ㅠ",
          "original_form": "배린이었어요ㅠ",
          "speaker_id": "1",
          "time": "20210825 12:32"
        },
        {
          "id": "MDRW2100013091.1.7",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210825 12:32"
        },
        {
          "id": "MDRW2100013091.1.8",
          "form": "저더 예전에 폰으러 함번",
          "original_form": "저더 예전에 폰으러 함번",
          "speaker_id": "2",
          "time": "20210825 12:33"
        },
        {
          "id": "MDRW2100013091.1.9",
          "form": "해보려고 깔았는데 바로 지웠어요ㅋㅋ",
          "original_form": "해보려고 깔았는데 바로 지웠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:33"
        },
        {
          "id": "MDRW2100013091.1.10",
          "form": "폰게임도 재밌죠",
          "original_form": "폰게임도 재밌죠",
          "speaker_id": "1",
          "time": "20210825 12:42"
        },
        {
          "id": "MDRW2100013091.1.11",
          "form": "구냥 그렇더라구요",
          "original_form": "구냥 그렇더라구요",
          "speaker_id": "2",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100013091.1.12",
          "form": "ㅋㅋㅋㅋㅋ 전 어렵고",
          "original_form": "ㅋㅋㅋㅋㅋ 전 어렵고",
          "speaker_id": "2",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100013091.1.13",
          "form": "다른거 뭐하시나요",
          "original_form": "다른거 뭐하시나요",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013091.1.14",
          "form": "폰게임은 이제 안해요!!",
          "original_form": "폰게임은 이제 안해요!!",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013091.1.15",
          "form": "폰게임 하세요?",
          "original_form": "폰게임 하세요?",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013091.1.16",
          "form": "아뇨, 안합니다ㅎㅎ",
          "original_form": "아뇨, 안합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100013091.1.17",
          "form": "ㅋㅋ ㅋㅋㅋ 저랑 같네요",
