      "utterance": [
        {
          "id": "MDRW2100020707.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020707.1.2",
          "form": "안녕하세요! 쇼핑 좋아하세요?",
          "original_form": "안녕하세요! 쇼핑 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020707.1.3",
          "form": "쇼핑 좋아해요~",
          "original_form": "쇼핑 좋아해요~",
          "speaker_id": "1",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020707.1.4",
          "form": "최근에 뭐사셨어요?",
          "original_form": "최근에 뭐사셨어요?",
          "speaker_id": "2",
          "time": "20210828 11:26"
        },
        {
          "id": "MDRW2100020707.1.5",
          "form": "최근에 향수샀어요 ㅋㅋㅋㅋ",
          "original_form": "최근에 향수샀어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:27"
        },
        {
          "id": "MDRW2100020707.1.6",
          "form": "요즘 시향이 안된다고하더라고요 늘 사던 향수예요?",
          "original_form": "요즘 시향이 안된다고하더라고요 늘 사던 향수예요?",
          "speaker_id": "2",
          "time": "20210828 11:28"
        },
        {
          "id": "MDRW2100020707.1.7",
          "form": "마쟈요 요새 백화점에서 시향이안되서 넘 슬퍼요 ㅠㅠ",
          "original_form": "마쟈요 요새 백화점에서 시향이안되서 넘 슬퍼요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020707.1.8",
          "form": "아뇨! 블라인드로 구매했어요 ㅌㅋ",
          "original_form": "아뇨! 블라인드로 구매했어요 ㅌㅋ",
          "speaker_id": "1",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020707.1.9",
          "form": "블라인드로 구매하는건 어떤거예요?",
          "original_form": "블라인드로 구매하는건 어떤거예요?",
          "speaker_id": "2",
          "time": "20210828 11:31"
        },
        {
          "id": "MDRW2100020707.1.10",
          "form": "시향없이 향수 노트만보고 질러버리는거에요 ㅋㅋㅋ",
          "original_form": "시향없이 향수 노트만보고 질러버리는거에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:33"
        },
        {
          "id": "MDRW2100020707.1.11",
          "form": "배송오기전까지 두근두근해요 ㅋㅋㅋ",
          "original_form": "배송오기전까지 두근두근해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:33"
        },
        {
          "id": "MDRW2100020707.1.12",
          "form": "아 진짜 두근댈 수 밖에 없네요! 복불복!!",
          "original_form": "아 진짜 두근댈 수 밖에 없네요! 복불복!!",
          "speaker_id": "2",
          "time": "20210828 11:33"
        },
        {
          "id": "MDRW2100020707.1.13",
          "form": "맞아요 ㅋㅋ 실패하는것들은 당근이나 번개장터에 내놓기도해요 ㅋㅋㅋ ㅜㅜ",
          "original_form": "맞아요 ㅋㅋ 실패하는것들은 당근이나 번개장터에 내놓기도해요 ㅋㅋㅋ ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100020707.1.14",
          "form": "당근 너무 후려치지않아요?",
          "original_form": "당근 너무 후려치지않아요?",
          "speaker_id": "2",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100020707.1.15",
          "form": "맞아요.. 절반은 후려쳐야 팔리더라구요 ㅜ",
          "original_form": "맞아요.. 절반은 후려쳐야 팔리더라구요 ㅜ",
          "speaker_id": "1",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100020707.1.16",
          "form": "기프티콘만 몇번 팔아봤는데 일상용품은 진짜 사고팔기 너무 어려운거같아요",
          "original_form": "기프티콘만 몇번 팔아봤는데 일상용품은 진짜 사고팔기 너무 어려운거같아요",
          "speaker_id": "2",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020707.1.17",
          "form": "저번에 숙박권도 팔아봤는데 어머님이 나오셔서 너무 좋아하시니 팔면서도 기분이 좋았어요",
          "original_form": "저번에 숙박권도 팔아봤는데 어머님이 나오셔서 너무 좋아하시니 팔면서도 기분이 좋았어요",
          "speaker_id": "2",
          "time": "20210828 11:38"
        }
      ]
