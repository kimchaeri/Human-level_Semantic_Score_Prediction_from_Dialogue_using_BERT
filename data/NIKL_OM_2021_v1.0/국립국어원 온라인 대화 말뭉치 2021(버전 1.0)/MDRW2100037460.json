      "utterance": [
        {
          "id": "MDRW2100037460.1.1",
          "form": "학교다니셨어요?ㅋ",
          "original_form": "학교다니셨어요?ㅋ",
          "speaker_id": "1",
          "time": "20210901 00:22"
        },
        {
          "id": "MDRW2100037460.1.2",
          "form": "과거에 다녔시요",
          "original_form": "과거에 다녔시요",
          "speaker_id": "2",
          "time": "20210901 00:22"
        },
        {
          "id": "MDRW2100037460.1.3",
          "form": "기억나는 강의있나요?",
          "original_form": "기억나는 강의있나요?",
          "speaker_id": "1",
          "time": "20210901 00:22"
        },
        {
          "id": "MDRW2100037460.1.4",
          "form": "앞에 의욕있게 앉았는데 졸아서 흰눈떴던 수업?",
          "original_form": "앞에 의욕있게 앉았는데 졸아서 흰눈떴던 수업?",
          "speaker_id": "2",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100037460.1.5",
          "form": "교수님이 헉!함",
          "original_form": "교수님이 헉!함",
          "speaker_id": "2",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100037460.1.6",
          "form": "아..강의명이 뭐였나요",
          "original_form": "아..강의명이 뭐였나요",
          "speaker_id": "1",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100037460.1.7",
          "form": "꿀잠교수의 잠잘자수업이요",
          "original_form": "꿀잠교수의 잠잘자수업이요",
          "speaker_id": "2",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100037460.1.8",
          "form": "혹시 교수가 최준인가요?",
          "original_form": "혹시 교수가 최준인가요?",
          "speaker_id": "1",
          "time": "20210901 00:30"
        },
        {
          "id": "MDRW2100037460.1.9",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 00:30"
        },
        {
          "id": "MDRW2100037460.1.10",
          "form": "최준이면 내 헤드벵잉의 예술을 알아봤을텐데 아쉽네요",
          "original_form": "최준이면 내 헤드벵잉의 예술을 알아봤을텐데 아쉽네요",
          "speaker_id": "2",
          "time": "20210901 00:31"
        },
        {
          "id": "MDRW2100037460.1.11",
          "form": "최준이 아니라 다행이네요...",
          "original_form": "최준이 아니라 다행이네요...",
          "speaker_id": "1",
          "time": "20210901 08:53"
        },
        {
          "id": "MDRW2100037460.1.12",
          "form": "name1님학교는어땠습니까",
          "original_form": "&name1&님학교는어땠습니까",
          "speaker_id": "2",
          "time": "20210901 09:03"
        },
        {
          "id": "MDRW2100037460.1.13",
          "form": "산꼭대기에 있는 학교였는데",
          "original_form": "산꼭대기에 있는 학교였는데",
          "speaker_id": "1",
          "time": "20210901 09:21"
        },
        {
          "id": "MDRW2100037460.1.14",
          "form": "뭔가..수용소같은 느낌이었어요..'",
          "original_form": "뭔가..수용소같은 느낌이었어요..'",
          "speaker_id": "1",
          "time": "20210901 09:22"
        },
        {
          "id": "MDRW2100037460.1.15",
          "form": "산꼭대기? 동산위의 하얀집??",
          "original_form": "산꼭대기? 동산위의 하얀집??",
          "speaker_id": "2",
          "time": "20210901 09:41"
        },
        {
          "id": "MDRW2100037460.1.16",
          "form": "하얗지는 않아요ㅋ",
          "original_form": "하얗지는 않아요ㅋ",
          "speaker_id": "1",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100037460.1.17",
          "form": "글쿤요 동산위의 수용소 무시무시한 출신이시군요",
