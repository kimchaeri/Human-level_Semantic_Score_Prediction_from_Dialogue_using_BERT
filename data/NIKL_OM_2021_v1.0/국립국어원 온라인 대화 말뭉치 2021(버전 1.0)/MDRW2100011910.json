        },
        {
          "id": "MDRW2100011910.1.2",
          "form": "코로나 끝나면 가보려고",
          "original_form": "코로나 끝나면 가보려고",
          "speaker_id": "1",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011910.1.3",
          "form": "한 번 갔다와",
          "original_form": "한 번 갔다와",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011910.1.4",
          "form": "어디로 가면 좋을까",
          "original_form": "어디로 가면 좋을까",
          "speaker_id": "1",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011910.1.5",
          "form": "강릉",
          "original_form": "강릉",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011910.1.6",
          "form": "근데 나 강릉 가봤어",
          "original_form": "근데 나 강릉 가봤어",
          "speaker_id": "1",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011910.1.7",
          "form": "안가본데 있어?",
          "original_form": "안가본데 있어?",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011910.1.8",
          "form": "안 가본데 많지",
          "original_form": "안 가본데 많지",
          "speaker_id": "1",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100011910.1.9",
          "form": "어디?",
          "original_form": "어디?",
          "speaker_id": "2",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100011910.1.10",
          "form": "당연히 가본데 보다 안 가본 데가 더 많지",
          "original_form": "당연히 가본데 보다 안 가본 데가 더 많지",
          "speaker_id": "1",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100011910.1.11",
          "form": "그럼 그중에 한군데 가면 되겠네",
          "original_form": "그럼 그중에 한군데 가면 되겠네",
