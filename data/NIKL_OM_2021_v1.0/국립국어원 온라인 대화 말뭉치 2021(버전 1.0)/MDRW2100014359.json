      "utterance": [
        {
          "id": "MDRW2100014359.1.1",
          "form": "최근에 인테리어 쇼핑 다녀왔어요",
          "original_form": "최근에 인테리어 쇼핑 다녀왔어요",
          "speaker_id": "2",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014359.1.2",
          "form": "어떤 것을 사셨나요?",
          "original_form": "어떤 것을 사셨나요?",
          "speaker_id": "1",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014359.1.3",
          "form": "이케아에서 서랍수납장이요",
          "original_form": "이케아에서 서랍수납장이요",
          "speaker_id": "2",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014359.1.4",
          "form": "이불이랑 조명까지",
          "original_form": "이불이랑 조명까지",
          "speaker_id": "2",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014359.1.5",
          "form": "오 인테리어 변화를 많이 주셨네요",
          "original_form": "오 인테리어 변화를 많이 주셨네요",
          "speaker_id": "1",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014359.1.6",
          "form": "이불은 어떤 디자인이에요?",
          "original_form": "이불은 어떤 디자인이에요?",
          "speaker_id": "1",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014359.1.7",
          "form": "꽃무늬? 깔끔?",
          "original_form": "꽃무늬? 깔끔?",
          "speaker_id": "1",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014359.1.8",
          "form": "이불은 체크무늬요",
          "original_form": "이불은 체크무늬요",
          "speaker_id": "2",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014359.1.9",
          "form": "최근에다녀오신 쇼핑 있으세요?",
          "original_form": "최근에다녀오신 쇼핑 있으세요?",
          "speaker_id": "2",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014359.1.10",
          "form": "체크무늬를 놓쳤네요...!",
          "original_form": "체크무늬를 놓쳤네요...!",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014359.1.11",
          "form": "전 인터넷 쇼핑했는데",
          "original_form": "전 인터넷 쇼핑했는데",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014359.1.12",
          "form": "운동용 바지를 하나 샀어요",
          "original_form": "운동용 바지를 하나 샀어요",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014359.1.13",
          "form": "우와 어떤 운동하세요",
          "original_form": "우와 어떤 운동하세요",
          "speaker_id": "2",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014359.1.14",
          "form": "저도 인터넷으로 가을용 옷 샀어요",
          "original_form": "저도 인터넷으로 가을용 옷 샀어요",
          "speaker_id": "2",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014359.1.15",
          "form": "요새 홈트를 시작했어요",
          "original_form": "요새 홈트를 시작했어요",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014359.1.16",
          "form": "오늘이 3일차네요",
          "original_form": "오늘이 3일차네요",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014359.1.17",
          "form": "가을용옷이면 니트나 코트류 인가요?",
          "original_form": "가을용옷이면 니트나 코트류 인가요?",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014359.1.18",
          "form": "우와 홈트",
          "original_form": "우와 홈트",
          "speaker_id": "2",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014359.1.19",
          "form": "저는 셔츠를 샀습니다 아직 더워서 개시를 못하고있어요",
          "original_form": "저는 셔츠를 샀습니다 아직 더워서 개시를 못하고있어요",
          "speaker_id": "2",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014359.1.20",
          "form": "아직은 좀 덥죠",
          "original_form": "아직은 좀 덥죠",
          "speaker_id": "1",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100014359.1.21",
          "form": "다음주는 입을 수 있을 것 같아요.",
          "original_form": "다음주는 입을 수 있을 것 같아요.",
          "speaker_id": "1",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100014359.1.22",
          "form": "중고거래 해본 적 있으세요?",
          "original_form": "중고거래 해본 적 있으세요?",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014359.1.23",
          "form": "네 당근마켓이요",
          "original_form": "네 당근마켓이요",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014359.1.24",
          "form": "판거는 많이 팔아보고 산 것은 보다나 고데기 새상품 사봤어요 요리보고님은요?",
          "original_form": "판거는 많이 팔아보고 산 것은 보다나 고데기 새상품 사봤어요 요리보고님은요?",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014359.1.25",
          "form": "보다나 고데기 저도 있어요",
          "original_form": "보다나 고데기 저도 있어요",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014359.1.26",
          "form": "전 부모님이 화분 사오시는 거 도우러 가봤어요",
          "original_form": "전 부모님이 화분 사오시는 거 도우러 가봤어요",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014359.1.27",
          "form": "당근마켓에서 화분도 팔더라구요",
          "original_form": "당근마켓에서 화분도 팔더라구요",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014359.1.28",
          "form": "오 화분도 파는군요",
          "original_form": "오 화분도 파는군요",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014359.1.29",
          "form": "저는 가구를 다 내놓고 있어요 어머니가 가구 바꾸고 싶다하셔서",
          "original_form": "저는 가구를 다 내놓고 있어요 어머니가 가구 바꾸고 싶다하셔서",
          "speaker_id": "2",
          "time": "20210825 20:20"
        }
      ]
    }
