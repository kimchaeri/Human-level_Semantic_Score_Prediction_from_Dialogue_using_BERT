      "utterance": [
        {
          "id": "MDRW2100036761.1.1",
          "form": "한국인은 국물아닙니꺼",
          "original_form": "한국인은 국물아닙니꺼",
          "speaker_id": "1",
          "time": "20210831 11:40"
        },
        {
          "id": "MDRW2100036761.1.2",
          "form": "ㅎㅎ 얼큰한 국물은 특히 사랑이죠",
          "original_form": "ㅎㅎ 얼큰한 국물은 특히 사랑이죠",
          "speaker_id": "2",
          "time": "20210831 11:41"
        },
        {
          "id": "MDRW2100036761.1.3",
          "form": "자장밥부터 드시나요?",
          "original_form": "자장밥부터 드시나요?",
          "speaker_id": "2",
          "time": "20210831 11:41"
        },
        {
          "id": "MDRW2100036761.1.4",
          "form": "반전있어요",
          "original_form": "반전있어요",
          "speaker_id": "1",
          "time": "20210831 11:48"
        },
        {
          "id": "MDRW2100036761.1.5",
          "form": "친구가와서닭칼국수로 배달",
          "original_form": "친구가와서닭칼국수로 배달",
          "speaker_id": "1",
          "time": "20210831 11:48"
        },
        {
          "id": "MDRW2100036761.1.6",
          "form": "ㅜㅇㅜ 저 닭칼국수 진짜 좋아하는데",
          "original_form": "ㅜㅇㅜ 저 닭칼국수 진짜 좋아하는데",
          "speaker_id": "2",
          "time": "20210831 11:49"
        },
        {
          "id": "MDRW2100036761.1.7",
          "form": "열입만 주세요",
          "original_form": "열입만 주세요",
          "speaker_id": "2",
          "time": "20210831 11:49"
        },
        {
          "id": "MDRW2100036761.1.8",
          "form": "현풍닭칼국수~~~~",
          "original_form": "현풍닭칼국수~~~~",
          "speaker_id": "1",
          "time": "20210831 11:52"
        },
        {
          "id": "MDRW2100036761.1.9",
          "form": "저도 저녁엔 닭한마리 먹을 거예요!!!ㅜㅇㅜ",
          "original_form": "저도 저녁엔 닭한마리 먹을 거예요!!!ㅜㅇㅜ",
          "speaker_id": "2",
          "time": "20210831 11:55"
        },
        {
          "id": "MDRW2100036761.1.10",
          "form": "오오오 ㅋㅋ 꼭드시길ㄲㄱ맛난그",
          "original_form": "오오오 ㅋㅋ 꼭드시길ㄲㄱ맛난그",
          "speaker_id": "1",
          "time": "20210831 11:59"
        },
        {
          "id": "MDRW2100036761.1.11",
          "form": "왜 비웃는 거 같죵ㅜㅇㅜ 분하다",
          "original_form": "왜 비웃는 거 같죵ㅜㅇㅜ 분하다",
          "speaker_id": "2",
          "time": "20210831 11:59"
        },
        {
          "id": "MDRW2100036761.1.12",
          "form": "점심도 쌀국수드시잖아여ㅠㅠ",
          "original_form": "점심도 쌀국수드시잖아여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 12:00"
        },
        {
          "id": "MDRW2100036761.1.13",
          "form": "쌀국수 고기 토핑이 푸짐하지 않아욧!",
          "original_form": "쌀국수 고기 토핑이 푸짐하지 않아욧!",
          "speaker_id": "2",
          "time": "20210831 12:01"
        },
        {
          "id": "MDRW2100036761.1.14",
          "form": "숙주파티..",
          "original_form": "숙주파티..",
          "speaker_id": "1",
          "time": "20210831 12:01"
        },
        {
          "id": "MDRW2100036761.1.15",
          "form": "님은 닭고기가 수북한 칼국수를 드시겠죠ㅎㅎ",
          "original_form": "님은 닭고기가 수북한 칼국수를 드시겠죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 12:01"
        },
        {
          "id": "MDRW2100036761.1.16",
          "form": "인각적으로 숙주보다 고기를 더 줘야 하는거 아닌가요",
          "original_form": "인각적으로 숙주보다 고기를 더 줘야 하는거 아닌가요",
          "speaker_id": "2",
          "time": "20210831 12:02"
        },
        {
          "id": "MDRW2100036761.1.17",
          "form": "여기도수북하진 않아여ㅠㅜ",
          "original_form": "여기도수북하진 않아여ㅠㅜ",
          "speaker_id": "1",
          "time": "20210831 12:03"
        }
      ]
