      "utterance": [
        {
          "id": "MDRW2100009713.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210823 07:47"
        },
        {
          "id": "MDRW2100009713.1.2",
          "form": "여긴 아직 안와요",
          "original_form": "여긴 아직 안와요",
          "speaker_id": "2",
          "time": "20210823 07:48"
        },
        {
          "id": "MDRW2100009713.1.3",
          "form": "여기도요",
          "original_form": "여기도요",
          "speaker_id": "1",
          "time": "20210823 07:49"
        },
        {
          "id": "MDRW2100009713.1.4",
          "form": "근데 바람은 마니 불어요\r\n태풍이 올라오는 중이라고 하네요",
          "original_form": "근데 바람은 마니 불어요\r\n태풍이 올라오는 중이라고 하네요",
          "speaker_id": "2",
          "time": "20210823 10:34"
        },
        {
          "id": "MDRW2100009713.1.5",
          "form": "걱정이에요",
          "original_form": "걱정이에요",
          "speaker_id": "1",
          "time": "20210823 10:45"
        },
        {
          "id": "MDRW2100009713.1.6",
          "form": "방금뉴스보니까 아랫지방은 벌써 피해가 있더라구요",
          "original_form": "방금뉴스보니까 아랫지방은 벌써 피해가 있더라구요",
          "speaker_id": "2",
          "time": "20210823 12:19"
        },
        {
          "id": "MDRW2100009713.1.7",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210823 14:24"
        },
        {
          "id": "MDRW2100009713.1.8",
          "form": "여기도 비오네요\r\n오늘이 고비라던데",
          "original_form": "여기도 비오네요\r\n오늘이 고비라던데",
          "speaker_id": "2",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100009713.1.9",
          "form": "태풍피해 없으시길",
          "original_form": "태풍피해 없으시길",
          "speaker_id": "1",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100009713.1.10",
          "form": "다행히 별다른 일 없이 잘 지나갔네요",
          "original_form": "다행히 별다른 일 없이 잘 지나갔네요",
          "speaker_id": "2",
          "time": "20210824 06:12"
        },
        {
          "id": "MDRW2100009713.1.11",
          "form": "그쪽도 괜찮나요?",
          "original_form": "그쪽도 괜찮나요?",
          "speaker_id": "2",
          "time": "20210824 06:13"
        },
        {
          "id": "MDRW2100009713.1.12",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100009713.1.13",
          "form": "여기는 비가 그치고 흐리기만 해요",
          "original_form": "여기는 비가 그치고 흐리기만 해요",
