      "utterance": [
        {
          "id": "MDRW2100010858.1.1",
          "form": "강아지좋아하세요?",
          "original_form": "강아지좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010858.1.2",
          "form": "강아지 정말 좋아요",
          "original_form": "강아지 정말 좋아요",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010858.1.3",
          "form": "사모예드 정말 좋아해요",
          "original_form": "사모예드 정말 좋아해요",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010858.1.4",
          "form": "키우신적있으세요?",
          "original_form": "키우신적있으세요?",
          "speaker_id": "1",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010858.1.5",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010858.1.6",
          "form": "키운 적은 없엉ㅅ",
          "original_form": "키운 적은 없엉ㅅ",
          "speaker_id": "2",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010858.1.7",
          "form": "없아요",
          "original_form": "없아요",
          "speaker_id": "2",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010858.1.8",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010858.1.9",
          "form": "병아리 키웠어요",
          "original_form": "병아리 키웠어요",
          "speaker_id": "1",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010858.1.10",
          "form": "저는 햄스터 키워봤어요",
          "original_form": "저는 햄스터 키워봤어요",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010858.1.11",
          "form": "햄스터는 뭐먹어요",
          "original_form": "햄스터는 뭐먹어요",
          "speaker_id": "1",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010858.1.12",
          "form": "해바라기씨랑 사료 먹어요",
          "original_form": "해바라기씨랑 사료 먹어요",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010858.1.13",
          "form": "당근도 잘 먹고 멸치도 잘 먹어요",
          "original_form": "당근도 잘 먹고 멸치도 잘 먹어요",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010858.1.14",
          "form": "멸치요?",
          "original_form": "멸치요?",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010858.1.15",
          "form": "육수우려먹나요?",
          "original_form": "육수우려먹나요?",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010858.1.16",
          "form": "그냥 주면 냠냠 열심히 먹어요",
          "original_form": "그냥 주면 냠냠 열심히 먹어요",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010858.1.17",
          "form": "맛있게 먹는 게 귀여워요",
          "original_form": "맛있게 먹는 게 귀여워요",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010858.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010858.1.19",
          "form": "햄스터는 잡종인가요",
          "original_form": "햄스터는 잡종인가요",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010858.1.20",
          "form": "잡식인가요",
          "original_form": "잡식인가요",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010858.1.21",
          "form": "잡식은 아닐 걸요...?",
          "original_form": "잡식은 아닐 걸요...?",
          "speaker_id": "2",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100010858.1.22",
          "form": "아하 저는 초식인줄알았어요",
          "original_form": "아하 저는 초식인줄알았어요",
          "speaker_id": "1",
          "time": "20210823 22:35"
        }
      ]
