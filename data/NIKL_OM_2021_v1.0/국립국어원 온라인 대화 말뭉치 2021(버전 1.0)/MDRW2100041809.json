      "utterance": [
        {
          "id": "MDRW2100041809.1.1",
          "form": "참 밀가루는 맛나요ㅋㅋ",
          "original_form": "참 밀가루는 맛나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:05"
        },
        {
          "id": "MDRW2100041809.1.2",
          "form": "부침개 맛있어영",
          "original_form": "부침개 맛있어영",
          "speaker_id": "1",
          "time": "20210906 16:05"
        },
        {
          "id": "MDRW2100041809.1.3",
          "form": "네 진짜 맛있어서 끊을수가 없어요",
          "original_form": "네 진짜 맛있어서 끊을수가 없어요",
          "speaker_id": "1",
          "time": "20210906 16:05"
        },
        {
          "id": "MDRW2100041809.1.4",
          "form": "네 김치수제비도 땡기네요",
          "original_form": "네 김치수제비도 땡기네요",
          "speaker_id": "2",
          "time": "20210906 16:06"
        },
        {
          "id": "MDRW2100041809.1.5",
          "form": "캬",
          "original_form": "캬",
          "speaker_id": "1",
          "time": "20210906 16:06"
        },
        {
          "id": "MDRW2100041809.1.6",
          "form": "칼국수에 수제비",
          "original_form": "칼국수에 수제비",
          "speaker_id": "1",
          "time": "20210906 16:06"
        },
        {
          "id": "MDRW2100041809.1.7",
          "form": "수제비 잘 만드세요??",
          "original_form": "수제비 잘 만드세요??",
          "speaker_id": "1",
          "time": "20210906 16:06"
        },
        {
          "id": "MDRW2100041809.1.8",
          "form": "아뇨ㅋㅋ 반죽이 중요한데",
          "original_form": "아뇨ㅋㅋ 반죽이 중요한데",
          "speaker_id": "2",
          "time": "20210906 16:06"
        },
        {
          "id": "MDRW2100041809.1.9",
          "form": "잘될때는 잘되는데 어떨땐 디거 딱딱해요",
          "original_form": "잘될때는 잘되는데 어떨땐 디거 딱딱해요",
          "speaker_id": "2",
          "time": "20210906 16:06"
        },
        {
          "id": "MDRW2100041809.1.10",
          "form": "맞아요 힘들더라고요",
          "original_form": "맞아요 힘들더라고요",
          "speaker_id": "1",
          "time": "20210906 16:07"
        },
        {
          "id": "MDRW2100041809.1.11",
          "form": "얻어걸리는 느낌으로 해요ㅋㅋ",
          "original_form": "얻어걸리는 느낌으로 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:07"
        },
        {
          "id": "MDRW2100041809.1.12",
          "form": "저 수제비는 진짜 못떼욬ㅋㅋㅋ",
          "original_form": "저 수제비는 진짜 못떼욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:07"
        },
        {
          "id": "MDRW2100041809.1.13",
          "form": "그것도 요령인거 같더라고요",
          "original_form": "그것도 요령인거 같더라고요",
          "speaker_id": "1",
          "time": "20210906 16:07"
        },
        {
          "id": "MDRW2100041809.1.14",
          "form": "얻어걸리시는거 보면 그래도 잘하시는거 전 얻어걸리지도 않아욬ㅋㅋㅋ",
          "original_form": "얻어걸리시는거 보면 그래도 잘하시는거 전 얻어걸리지도 않아욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:08"
        },
        {
          "id": "MDRW2100041809.1.15",
          "form": "아 얇게 떼는게 진짜 힘들어요",
          "original_form": "아 얇게 떼는게 진짜 힘들어요",
          "speaker_id": "2",
          "time": "20210906 16:08"
        },
        {
          "id": "MDRW2100041809.1.16",
          "form": "딱딱할땐 턱이 아플정도에요ㅋㅋ",
          "original_form": "딱딱할땐 턱이 아플정도에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:08"
        },
        {
          "id": "MDRW2100041809.1.17",
          "form": "아 진짜 얇게 떼면 맛있는데 ㅋㅋㅋㅋㅋ 반죽이 진짜 힘들어여",
          "original_form": "아 진짜 얇게 떼면 맛있는데 ㅋㅋㅋㅋㅋ 반죽이 진짜 힘들어여",
          "speaker_id": "1",
          "time": "20210906 16:09"
        },
        {
          "id": "MDRW2100041809.1.18",
          "form": "역시 사먹어야 맛나요ㅋㅋㅋ",
          "original_form": "역시 사먹어야 맛나요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:10"
        },
        {
          "id": "MDRW2100041809.1.19",
          "form": "저도 수제비는 사먹는게 좋다고 생각해욬ㅋㅋㅋ",
          "original_form": "저도 수제비는 사먹는게 좋다고 생각해욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:10"
        },
        {
          "id": "MDRW2100041809.1.20",
          "form": "수고가 너무 많이 들어가져",
          "original_form": "수고가 너무 많이 들어가져",
          "speaker_id": "2",
          "time": "20210906 16:10"
        },
        {
          "id": "MDRW2100041809.1.21",
          "form": "그러니까요 ㅎㅎㅎㅎ",
          "original_form": "그러니까요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:11"
        }
      ]
