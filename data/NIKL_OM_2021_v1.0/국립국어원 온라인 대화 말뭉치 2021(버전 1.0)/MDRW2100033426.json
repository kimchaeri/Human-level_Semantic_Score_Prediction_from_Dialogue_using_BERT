      "utterance": [
        {
          "id": "MDRW2100033426.1.1",
          "form": "저는 회요 ㅎㅎ",
          "original_form": "저는 회요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100033426.1.2",
          "form": "name1님은?",
          "original_form": "&name1&님은?",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100033426.1.3",
          "form": "전 삼겹살 엄첨 좋아해요",
          "original_form": "전 삼겹살 엄첨 좋아해요",
          "speaker_id": "1",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100033426.1.4",
          "form": "고기 맛있죠 ㅎㅎ",
          "original_form": "고기 맛있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 10:31"
        },
        {
          "id": "MDRW2100033426.1.5",
          "form": "삼겹살 좋아하시나요",
          "original_form": "삼겹살 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 11:00"
        },
        {
          "id": "MDRW2100033426.1.6",
          "form": "한국인인데요 ㅎㅎ",
          "original_form": "한국인인데요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100033426.1.7",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "2",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100033426.1.8",
          "form": "거기에 소주 한잔 짱 좋죠",
          "original_form": "거기에 소주 한잔 짱 좋죠",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100033426.1.9",
          "form": "크",
          "original_form": "크",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100033426.1.10",
          "form": "쌈장에 찍어서",
          "original_form": "쌈장에 찍어서",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100033426.1.11",
          "form": "명이나물과 구운 미나리 같이 먹어도 맛있죠",
          "original_form": "명이나물과 구운 미나리 같이 먹어도 맛있죠",
          "speaker_id": "1",
          "time": "20210907 14:13"
        },
        {
          "id": "MDRW2100033426.1.12",
          "form": "아 ㅎㅎ 밥도 먹고 싶네요",
          "original_form": "아 ㅎㅎ 밥도 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100033426.1.13",
          "form": "밥은 볶음밥이 맛있죠",
          "original_form": "밥은 볶음밥이 맛있죠",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100033426.1.14",
          "form": "아 그쵸 밥은 볶음밥",
          "original_form": "아 그쵸 밥은 볶음밥",
          "speaker_id": "2",
          "time": "20210907 15:59"
        },
        {
          "id": "MDRW2100033426.1.15",
          "form": "정답입니다",
          "original_form": "정답입니다",
          "speaker_id": "2",
          "time": "20210907 15:59"
        },
        {
          "id": "MDRW2100033426.1.16",
          "form": "회 말고 초밥도 좋아하시나요",
          "original_form": "회 말고 초밥도 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100033426.1.17",
          "form": "초밥 좋아해요 ㅎㅎㅎ",
          "original_form": "초밥 좋아해요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:51"
        },
        {
          "id": "MDRW2100033426.1.18",
          "form": "연어초밥!",
          "original_form": "연어초밥!",
          "speaker_id": "2",
          "time": "20210907 16:51"
        },
        {
          "id": "MDRW2100033426.1.19",
          "form": "전 새우 초밥이 맛나더라고요",
          "original_form": "전 새우 초밥이 맛나더라고요",
          "speaker_id": "1",
          "time": "20210907 16:54"
        },
        {
          "id": "MDRW2100033426.1.20",
          "form": "새우도 맛있죠 그 보리새우 좋아하는데",
          "original_form": "새우도 맛있죠 그 보리새우 좋아하는데",
          "speaker_id": "2",
          "time": "20210907 18:19"
        }
