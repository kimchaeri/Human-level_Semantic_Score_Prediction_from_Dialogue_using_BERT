      "utterance": [
        {
          "id": "MDRW2100039928.1.1",
          "form": "어제 저녁엔 뭐 드셨나요?!",
          "original_form": "어제 저녁엔 뭐 드셨나요?!",
          "speaker_id": "2",
          "time": "20210904 07:58"
        },
        {
          "id": "MDRW2100039928.1.2",
          "form": "저는 어제도 때를 놓치는 바람에",
          "original_form": "저는 어제도 때를 놓치는 바람에",
          "speaker_id": "2",
          "time": "20210904 07:58"
        },
        {
          "id": "MDRW2100039928.1.3",
          "form": "편의점 김밥이랑 라면에 입가심으로",
          "original_form": "편의점 김밥이랑 라면에 입가심으로",
          "speaker_id": "2",
          "time": "20210904 07:58"
        },
        {
          "id": "MDRW2100039928.1.4",
          "form": "햄버거를 먹었습니다ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "햄버거를 먹었습니다ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 07:58"
        },
        {
          "id": "MDRW2100039928.1.5",
          "form": "ㅋㅋㅋㅋ편의점 자주 이용하시네용",
          "original_form": "ㅋㅋㅋㅋ편의점 자주 이용하시네용",
          "speaker_id": "1",
          "time": "20210904 09:38"
        },
        {
          "id": "MDRW2100039928.1.6",
          "form": "저는 어제 로제떡볶이!!!",
          "original_form": "저는 어제 로제떡볶이!!!",
          "speaker_id": "1",
          "time": "20210904 09:38"
        },
        {
          "id": "MDRW2100039928.1.7",
          "form": "이제 백신 맞으러 갈거기 때문에 최후의 만찬 쯤으로",
          "original_form": "이제 백신 맞으러 갈거기 때문에 최후의 만찬 쯤으로",
          "speaker_id": "1",
          "time": "20210904 09:38"
        },
        {
          "id": "MDRW2100039928.1.8",
          "form": "크으... 맛잘알이시네요",
          "original_form": "크으... 맛잘알이시네요",
          "speaker_id": "2",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100039928.1.9",
          "form": "백신 맞고나서 더 잘 먹어야죠!",
          "original_form": "백신 맞고나서 더 잘 먹어야죠!",
          "speaker_id": "2",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100039928.1.10",
          "form": "맞아요 ㅋㅋㅋ 주말동안 아주 잘 먹고 잘 쉬는 것이 저의 계획 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ 주말동안 아주 잘 먹고 잘 쉬는 것이 저의 계획 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100039928.1.11",
          "form": "오늘 백신 맞으시는거예요?!",
          "original_form": "오늘 백신 맞으시는거예요?!",
          "speaker_id": "2",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100039928.1.12",
          "form": "네네 아까 오전에 맞고 왔어요!!",
          "original_form": "네네 아까 오전에 맞고 왔어요!!",
          "speaker_id": "1",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100039928.1.13",
          "form": "주말에도 하는구나~",
          "original_form": "주말에도 하는구나~",
          "speaker_id": "2",
          "time": "20210904 12:47"
        },
        {
          "id": "MDRW2100039928.1.14",
          "form": "영향은 없다고 하지만 그래도 혹시모르니가",
          "original_form": "영향은 없다고 하지만 그래도 혹시모르니가",
          "speaker_id": "2",
          "time": "20210904 12:47"
        },
        {
          "id": "MDRW2100039928.1.15",
          "form": "자극적인건 드시지 마세여!",
          "original_form": "자극적인건 드시지 마세여!",
          "speaker_id": "2",
          "time": "20210904 12:47"
        },
        {
          "id": "MDRW2100039928.1.16",
          "form": "넹ㅎㅎ 감사해요 집밥 계속 먹어야겠어요",
          "original_form": "넹ㅎㅎ 감사해요 집밥 계속 먹어야겠어요",
          "speaker_id": "1",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100039928.1.17",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210904 19:39"
        },
        {
          "id": "MDRW2100039928.1.18",
          "form": "네 아구찜 먹었어요! 자극적이었네요 ㅋㅋ",
          "original_form": "네 아구찜 먹었어요! 자극적이었네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100039928.1.19",
          "form": "어제도 맛있는거 드셨나요?!",
          "original_form": "어제도 맛있는거 드셨나요?!",
          "speaker_id": "2",
          "time": "20210906 09:25"
        },
        {
          "id": "MDRW2100039928.1.20",
          "form": "네네 심심이가 준 치킨 ㅋㅋㅋㅋ",
          "original_form": "네네 심심이가 준 치킨 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100039928.1.21",
          "form": "이것도 자극적이네요 ㅋㅋㅋ",
          "original_form": "이것도 자극적이네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100039928.1.22",
          "form": "즐거운 주말이었네요ㅋㅋㅋㅋㅋ",
          "original_form": "즐거운 주말이었네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100039928.1.23",
          "form": "저도 어제 치킨먹고싶었는데",
          "original_form": "저도 어제 치킨먹고싶었는데",
          "speaker_id": "2",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100039928.1.24",
          "form": "10시에 시켜야지 하다가 잠들었어요ㅜ",
          "original_form": "10시에 시켜야지 하다가 잠들었어요ㅜ",
