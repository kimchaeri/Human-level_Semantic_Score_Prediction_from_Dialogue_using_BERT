        },
        {
          "id": "MDRW2100046532.1.5",
          "form": "검은태양하고갯차요",
          "original_form": "검은태양하고갯차요",
          "speaker_id": "1",
          "time": "20211028 20:13"
        },
        {
          "id": "MDRW2100046532.1.6",
          "form": "넷플릭스 드라마는 안보시나요?",
