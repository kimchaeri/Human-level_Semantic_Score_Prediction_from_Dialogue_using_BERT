      "utterance": [
        {
          "id": "MDRW2100009922.1.1",
          "form": "커뮤니티 즐겨 하시나요?",
          "original_form": "커뮤니티 즐겨 하시나요?",
          "speaker_id": "1",
          "time": "20210823 11:45"
        },
        {
          "id": "MDRW2100009922.1.2",
          "form": "네 자주해요~ 뭐 즐겨하시는거 있으세요?",
          "original_form": "네 자주해요~ 뭐 즐겨하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210823 11:45"
        },
        {
          "id": "MDRW2100009922.1.3",
          "form": "저는 재테크 커뮤니티 위주로 해요",
          "original_form": "저는 재테크 커뮤니티 위주로 해요",
          "speaker_id": "1",
          "time": "20210823 11:46"
        },
        {
          "id": "MDRW2100009922.1.4",
          "form": "매미님은 어떤거 위주로 하세요?",
          "original_form": "매미님은 어떤거 위주로 하세요?",
          "speaker_id": "1",
          "time": "20210823 11:46"
        },
        {
          "id": "MDRW2100009922.1.5",
          "form": "저는 일상공유하는 곳이랑 직업커뮤요ㅋㅋ",
          "original_form": "저는 일상공유하는 곳이랑 직업커뮤요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:47"
        },
        {
          "id": "MDRW2100009922.1.6",
          "form": "요즘은 직업 커뮤에서 사는거 같아요",
          "original_form": "요즘은 직업 커뮤에서 사는거 같아요",
          "speaker_id": "2",
          "time": "20210823 11:47"
        },
        {
          "id": "MDRW2100009922.1.7",
          "form": "직업 커뮤 좋죠",
          "original_form": "직업 커뮤 좋죠",
          "speaker_id": "1",
          "time": "20210823 11:48"
        },
        {
          "id": "MDRW2100009922.1.8",
          "form": "관심사가 묘하게 비슷하달까",
          "original_form": "관심사가 묘하게 비슷하달까",
          "speaker_id": "1",
          "time": "20210823 11:48"
        },
        {
          "id": "MDRW2100009922.1.9",
          "form": "그쳐ㅋㅋ사",
          "original_form": "그쳐ㅋㅋ사",
          "speaker_id": "2",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100009922.1.10",
          "form": "재테크 커뮤는 하시면 도움이되나요",
          "original_form": "재테크 커뮤는 하시면 도움이되나요",
          "speaker_id": "2",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100009922.1.11",
          "form": "사람사는게 비슷한가봐요",
          "original_form": "사람사는게 비슷한가봐요",
          "speaker_id": "1",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100009922.1.12",
          "form": "저도 재테크 관심 많아요!",
          "original_form": "저도 재테크 관심 많아요!",
          "speaker_id": "2",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100009922.1.13",
          "form": "재태크면 주식 코인 부동산 여러가지 있는데 특히 어느쪽 자주 활동하세요?",
          "original_form": "재태크면 주식 코인 부동산 여러가지 있는데 특히 어느쪽 자주 활동하세요?",
          "speaker_id": "2",
          "time": "20210823 11:51"
        },
        {
          "id": "MDRW2100009922.1.14",
          "form": "어떤거에 관심 많나요?",
          "original_form": "어떤거에 관심 많나요?",
          "speaker_id": "1",
          "time": "20210823 11:51"
        },
        {
          "id": "MDRW2100009922.1.15",
          "form": "저는 주식이랑 부동산에 관심 있어요",
          "original_form": "저는 주식이랑 부동산에 관심 있어요",
          "speaker_id": "1",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100009922.1.16",
          "form": "특히 주식",
          "original_form": "특히 주식",
          "speaker_id": "1",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100009922.1.17",
          "form": "와 저도 주식이요ㅋㅋ",
          "original_form": "와 저도 주식이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100009922.1.18",
          "form": "저번주 난리났었죠ㅋㅋ",
          "original_form": "저번주 난리났었죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100009922.1.19",
          "form": "주식이 줄줄 흘렀어요",
          "original_form": "주식이 줄줄 흘렀어요",
          "speaker_id": "1",
          "time": "20210823 12:12"
        },
        {
          "id": "MDRW2100009922.1.20",
          "form": "계좌 수익률도 줄줄",
          "original_form": "계좌 수익률도 줄줄",
          "speaker_id": "1",
          "time": "20210823 12:12"
        },
        {
          "id": "MDRW2100009922.1.21",
          "form": "저도요 저번주엔 계좌 잘 확인도 않하고",
          "original_form": "저도요 저번주엔 계좌 잘 확인도 않하고",
          "speaker_id": "2",
          "time": "20210823 12:12"
        },
        {
          "id": "MDRW2100009922.1.22",
          "form": "흘러내린거 몇개만 주웠어요",
          "original_form": "흘러내린거 몇개만 주웠어요",
          "speaker_id": "2",
          "time": "20210823 12:12"
        },
        {
          "id": "MDRW2100009922.1.23",
          "form": "저도 조금 주웠어요",
          "original_form": "저도 조금 주웠어요",
          "speaker_id": "1",
          "time": "20210823 12:14"
        },
        {
          "id": "MDRW2100009922.1.24",
          "form": "주식 커뮤니티 난리더라고요",
          "original_form": "주식 커뮤니티 난리더라고요",
          "speaker_id": "1",
          "time": "20210823 12:15"
        },
        {
          "id": "MDRW2100009922.1.25",
          "form": "맞아요 우는사람 많던데요",
          "original_form": "맞아요 우는사람 많던데요",
          "speaker_id": "2",
          "time": "20210823 12:15"
        },
        {
          "id": "MDRW2100009922.1.26",
          "form": "그냥 기다리라는 사람들도 있고요",
          "original_form": "그냥 기다리라는 사람들도 있고요",
          "speaker_id": "2",
          "time": "20210823 12:15"
        },
        {
          "id": "MDRW2100009922.1.27",
          "form": "네 저는 기다리는 쪽이었어요",
          "original_form": "네 저는 기다리는 쪽이었어요",
          "speaker_id": "1",
          "time": "20210823 12:16"
        },
        {
          "id": "MDRW2100009922.1.28",
          "form": "저도 손절은 안하고 버티는중이에요",
          "original_form": "저도 손절은 안하고 버티는중이에요",
          "speaker_id": "2",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100009922.1.29",
          "form": "제가 선택한 기업이 이대로 폭삭 내려앉을거라고 생각하지는 않거든요",
          "original_form": "제가 선택한 기업이 이대로 폭삭 내려앉을거라고 생각하지는 않거든요",
          "speaker_id": "2",
          "time": "20210823 12:23"
        }
      ]
    }
