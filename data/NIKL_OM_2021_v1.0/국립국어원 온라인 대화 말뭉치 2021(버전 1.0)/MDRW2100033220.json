      "utterance": [
        {
          "id": "MDRW2100033220.1.1",
          "form": "그럼 혹시 한국 작가들 중에",
          "original_form": "그럼 혹시 한국 작가들 중에",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100033220.1.2",
          "form": "박민규 소설 읽어본 적 있어??",
          "original_form": "박민규 소설 읽어본 적 있어??",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100033220.1.3",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100033220.1.4",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100033220.1.5",
          "form": "나 한국작가는 잘몰라ㅠㅠㅠㅠ흑흗",
          "original_form": "나 한국작가는 잘몰라ㅠㅠㅠㅠ흑흗",
          "speaker_id": "2",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100033220.1.6",
          "form": "박민규 작가님 작품이 좋아?",
          "original_form": "박민규 작가님 작품이 좋아?",
          "speaker_id": "2",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100033220.1.7",
          "form": "문체가 독특하다 해야하나",
          "original_form": "문체가 독특하다 해야하나",
          "speaker_id": "1",
          "time": "20210907 15:00"
        },
        {
          "id": "MDRW2100033220.1.8",
          "form": "사람마다 호불호는 좀 갈릴 수 있는데",
          "original_form": "사람마다 호불호는 좀 갈릴 수 있는데",
          "speaker_id": "1",
          "time": "20210907 15:00"
        },
        {
          "id": "MDRW2100033220.1.9",
          "form": "나는 엄청 좋아하는 작가야!",
          "original_form": "나는 엄청 좋아하는 작가야!",
          "speaker_id": "1",
          "time": "20210907 15:00"
        },
        {
          "id": "MDRW2100033220.1.10",
          "form": "오오오옹",
          "original_form": "오오오옹",
          "speaker_id": "2",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100033220.1.11",
          "form": "어떤 느낌의 문체인뎅",
          "original_form": "어떤 느낌의 문체인뎅",
          "speaker_id": "2",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100033220.1.12",
          "form": "무슨소설이양",
          "original_form": "무슨소설이양",
          "speaker_id": "2",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100033220.1.13",
          "form": "요약해서",
          "original_form": "요약해서",
          "speaker_id": "2",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100033220.1.14",
          "form": "알려주랑",
          "original_form": "알려주랑",
          "speaker_id": "2",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100033220.1.15",
          "form": "뭐라 해야 하지...",
          "original_form": "뭐라 해야 하지...",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100033220.1.16",
          "form": "소설 문체보단 랩 가사 같다 해야 하나",
          "original_form": "소설 문체보단 랩 가사 같다 해야 하나",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100033220.1.17",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100033220.1.18",
          "form": "막",
          "original_form": "막",
          "speaker_id": "2",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100033220.1.19",
          "form": "라임",
          "original_form": "라임",
          "speaker_id": "2",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100033220.1.20",
          "form": "있는겨?",
          "original_form": "있는겨?",
          "speaker_id": "2",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100033220.1.21",
          "form": "어휘 선택이나 줄바꿈 심지어 내용에도 한계가 없는",
          "original_form": "어휘 선택이나 줄바꿈 심지어 내용에도 한계가 없는",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100033220.1.22",
          "form": "라임도 좀 있고",
          "original_form": "라임도 좀 있고",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100033220.1.23",
          "form": "오옹옹ㅇ옹",
          "original_form": "오옹옹ㅇ옹",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100033220.1.24",
          "form": "내용은",
          "original_form": "내용은",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100033220.1.25",
          "form": "어떤장르여",
          "original_form": "어떤장르여",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100033220.1.26",
          "form": "규정할 수가 없어...",
          "original_form": "규정할 수가 없어...",
          "speaker_id": "1",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100033220.1.27",
          "form": "대부분 일상판타지? 그런 느낌인데",
          "original_form": "대부분 일상판타지? 그런 느낌인데",
          "speaker_id": "1",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100033220.1.28",
          "form": "(굳이 분류하자면) 스포츠 소설도 있고",
          "original_form": "(굳이 분류하자면) 스포츠 소설도 있고",
          "speaker_id": "1",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100033220.1.29",
          "form": "오오옹",
          "original_form": "오오옹",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100033220.1.30",
          "form": "일상판타지~",
          "original_form": "일상판타지~",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100033220.1.31",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100033220.1.32",
          "form": "(굳이 분류하자면) 슈퍼히어로물도 있고..",
          "original_form": "(굳이 분류하자면) 슈퍼히어로물도 있고..",
          "speaker_id": "1",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100033220.1.33",
          "form": "아 블랙코미디라고 하는게 젤 정확할 듯",
          "original_form": "아 블랙코미디라고 하는게 젤 정확할 듯",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100033220.1.34",
          "form": "옹오오오오오ㅗ",
          "original_form": "옹오오오오오ㅗ",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100033220.1.35",
          "form": "슈퍼히어로물",
          "original_form": "슈퍼히어로물",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100033220.1.36",
          "form": "흥미로운걸",
          "original_form": "흥미로운걸",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100033220.1.37",
          "form": "재밌겠다...",
          "original_form": "재밌겠다...",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100033220.1.38",
          "form": "슈퍼히어로물은",
          "original_form": "슈퍼히어로물은",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100033220.1.39",
          "form": "제목을",
          "original_form": "제목을",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100033220.1.40",
          "form": "알려줄수있겠닝",
          "original_form": "알려줄수있겠닝",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100033220.1.41",
          "form": "지구영웅전설",
          "original_form": "지구영웅전설",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100033220.1.42",
          "form": "근데 흔히 생각하는 슈퍼히어로물을 생각하면 좀 당황스러울거야 ㅋㅋㅋ",
          "original_form": "근데 흔히 생각하는 슈퍼히어로물을 생각하면 좀 당황스러울거야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100033220.1.43",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100033220.1.44",
          "form": "지구영웅전설",
          "original_form": "지구영웅전설",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100033220.1.45",
          "form": "꼭읽어볼게",
          "original_form": "꼭읽어볼게",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100033220.1.46",
          "form": "우허는 책을 참 다양하게 많이읽는구나",
          "original_form": "우허는 책을 참 다양하게 많이읽는구나",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100033220.1.47",
          "form": "나도 학생때 다독상받았는데",
          "original_form": "나도 학생때 다독상받았는데",
