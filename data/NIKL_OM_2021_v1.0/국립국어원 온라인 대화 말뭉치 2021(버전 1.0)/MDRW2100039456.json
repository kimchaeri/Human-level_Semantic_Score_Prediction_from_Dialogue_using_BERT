        },
        {
          "id": "MDRW2100039456.1.3",
          "form": "저녁메뉴는 정하셧나여?",
          "original_form": "저녁메뉴는 정하셧나여?",
          "speaker_id": "1",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100039456.1.4",
          "form": "네 아까 피자로 조금 이른 저녁 먹었어요ㅎㅎ",
          "original_form": "네 아까 피자로 조금 이른 저녁 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100039456.1.5",
          "form": "name1님은 뭐 드실건가요?",
          "original_form": "&name1&님은 뭐 드실건가요?",
          "speaker_id": "2",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100039456.1.6",
          "form": "우왕! 어떤피자드셧나여?",
          "original_form": "우왕! 어떤피자드셧나여?",
          "speaker_id": "1",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039456.1.7",
          "form": "저는 오리고기먹으려구요~",
          "original_form": "저는 오리고기먹으려구요~",
          "speaker_id": "1",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039456.1.8",
          "form": "저는 하와이안 피자 좋아해서 하와이안피자 먹었어요ㅎㅎ",
          "original_form": "저는 하와이안 피자 좋아해서 하와이안피자 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039456.1.9",
          "form": "와!오리고기~ 맛있겠네요ㅜㅜ",
          "original_form": "와!오리고기~ 맛있겠네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039456.1.10",
          "form": "아정말요?? 하와이안피자좋아하시는군요!",
          "original_form": "아정말요?? 하와이안피자좋아하시는군요!",
          "speaker_id": "1",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039456.1.11",
          "form": "오리고기 고기중에젤좋아한답니덩ㅋㅋㅋ",
          "original_form": "오리고기 고기중에젤좋아한답니덩ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039456.1.12",
          "form": "오리고기 진짜 맛있죠.. 담백한 맛이 참 좋은것같아요ㅎㅎ",
          "original_form": "오리고기 진짜 맛있죠.. 담백한 맛이 참 좋은것같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 18:06"
        },
        {
          "id": "MDRW2100039456.1.13",
          "form": "맞아요! 오리고기최고에용 ㅋㅋㅋㅋ",
          "original_form": "맞아요! 오리고기최고에용 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100039456.1.14",
          "form": "새고기들이..다..맛잇죠..닭고기랑..오리고기..ㅋㅋ",
          "original_form": "새고기들이..다..맛잇죠..닭고기랑..오리고기..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:12"
        },
        {
          "id": "MDRW2100039456.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그쵸 두개가 제 최애에욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그쵸 두개가 제 최애에욬ㅋㅋㅋ",
