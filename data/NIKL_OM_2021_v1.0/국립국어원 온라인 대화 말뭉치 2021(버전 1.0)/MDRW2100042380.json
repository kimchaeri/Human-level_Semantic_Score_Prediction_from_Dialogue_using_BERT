      },
      "utterance": [
        {
          "id": "MDRW2100042380.1.1",
          "form": "뭐써? 냄새 잘 안나는거같아",
          "original_form": "뭐써? 냄새 잘 안나는거같아",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042380.1.2",
          "form": "되게 멋부릴때만 써서 그런가봐요",
          "original_form": "되게 멋부릴때만 써서 그런가봐요",
          "speaker_id": "1",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042380.1.3",
          "form": "무슨 향인데 그렇게 아껴써",
          "original_form": "무슨 향인데 그렇게 아껴써",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042380.1.4",
          "form": "저 예전에 엄마아빠랑 같이 인사동 갔을때",
          "original_form": "저 예전에 엄마아빠랑 같이 인사동 갔을때",
          "speaker_id": "1",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042380.1.5",
          "form": "그때사줬던거?",
          "original_form": "그때사줬던거?",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042380.1.6",
          "form": "네 ㅋㅋㅋ 고체향수",
          "original_form": "네 ㅋㅋㅋ 고체향수",
          "speaker_id": "1",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042380.1.7",
          "form": "엄마 그거 모기향 나던데 그게 좋니",
          "original_form": "엄마 그거 모기향 나던데 그게 좋니",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042380.1.8",
          "form": "저는 오렌지 레몬 이런 향이 좋아서 ㅎㅎ",
          "original_form": "저는 오렌지 레몬 이런 향이 좋아서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042380.1.9",
          "form": "자몽이었던거같긴 하네",
          "original_form": "자몽이었던거같긴 하네",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042380.1.10",
          "form": "맞아요 자몽 고체향수 잘쓰고 있어요",
          "original_form": "맞아요 자몽 고체향수 잘쓰고 있어요",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042380.1.11",
          "form": "오래전인데 향 안날아갔니?",
          "original_form": "오래전인데 향 안날아갔니?",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042380.1.12",
          "form": "고체라 더 좋은거같아요 아직 쓸만해요 ㅋㅋㅋ",
          "original_form": "고체라 더 좋은거같아요 아직 쓸만해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042380.1.13",
          "form": "보습도 된다던데 자주 써 향 날아가기 전에",
          "original_form": "보습도 된다던데 자주 써 향 날아가기 전에",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042380.1.14",
          "form": "저도 아끼다 똥되게 하지 말고 자주 서야져 ㅎ",
          "original_form": "저도 아끼다 똥되게 하지 말고 자주 서야져 ㅎ",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042380.1.15",
          "form": "글고 뭐냐 그런거 좀 있던데",
          "original_form": "글고 뭐냐 그런거 좀 있던데",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042380.1.16",
          "form": "나중에 같이 시향하러 가요",
          "original_form": "나중에 같이 시향하러 가요",
          "speaker_id": "1",
          "time": "20210906 21:57"
        }
      ]
