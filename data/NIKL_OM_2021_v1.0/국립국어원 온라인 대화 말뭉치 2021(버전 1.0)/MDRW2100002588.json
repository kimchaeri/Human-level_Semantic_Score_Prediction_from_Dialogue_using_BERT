      "utterance": [
        {
          "id": "MDRW2100002588.1.1",
          "form": "배달 음식 자주 시켜드세요?",
          "original_form": "배달 음식 자주 시켜드세요?",
          "speaker_id": "1",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002588.1.2",
          "form": "네 한달에10회정도 시켜먹는것같아요",
          "original_form": "네 한달에10회정도 시켜먹는것같아요",
          "speaker_id": "2",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002588.1.3",
          "form": "name1님은여",
          "original_form": "&name1&님은여",
          "speaker_id": "2",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002588.1.4",
          "form": "저도 일주일에 2번은 꼭 시키는거 같아요",
          "original_form": "저도 일주일에 2번은 꼭 시키는거 같아요",
          "speaker_id": "1",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002588.1.5",
          "form": "제일 자주 먹는 메뉴는 뭔가요?",
          "original_form": "제일 자주 먹는 메뉴는 뭔가요?",
          "speaker_id": "1",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002588.1.6",
          "form": "저 그때그때 할인많이하는 음식들 시켜먹는데 보통 치킨 제일 자주시켜먹어요 ㅋㅋㅋ",
          "original_form": "저 그때그때 할인많이하는 음식들 시켜먹는데 보통 치킨 제일 자주시켜먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002588.1.7",
          "form": "name1님은 어떤거자주시켜드세요",
          "original_form": "&name1&님은 어떤거자주시켜드세요",
          "speaker_id": "2",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002588.1.8",
          "form": "엇ㅋㅋㅋㅋ저도요 저도요!",
          "original_form": "엇ㅋㅋㅋㅋ저도요 저도요!",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002588.1.9",
          "form": "치킨 제일 자주 먹어요..",
          "original_form": "치킨 제일 자주 먹어요..",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002588.1.10",
          "form": "자주 먹어도 안질리는 메뉴가 치킨인거 같아요",
          "original_form": "자주 먹어도 안질리는 메뉴가 치킨인거 같아요",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002588.1.11",
          "form": "맞아요 일단 종류가 넘나 많구요",
          "original_form": "맞아요 일단 종류가 넘나 많구요",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002588.1.12",
          "form": "허니콤보제일좋아하긴하는데 질리면 다른데 시켜먹고",
          "original_form": "허니콤보제일좋아하긴하는데 질리면 다른데 시켜먹고",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002588.1.13",
          "form": "name1님 최애치킨뭐에요?",
          "original_form": "&name1&님 최애치킨뭐에요?",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002588.1.14",
          "form": "전 때마다 최애가 바뀌는 편인데",
          "original_form": "전 때마다 최애가 바뀌는 편인데",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002588.1.15",
          "form": "요즘은 알마치 자주 시켜먹습니다!!",
          "original_form": "요즘은 알마치 자주 시켜먹습니다!!",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002588.1.16",
          "form": "허니콤보 맛있죠ㅠㅠ 레드소스 추가 필숨",
          "original_form": "허니콤보 맛있죠ㅠㅠ 레드소스 추가 필숨",
          "speaker_id": "1",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002588.1.17",
          "form": "알마치맛있다고들었어요ㅜㅜ저도담에 꼭시켜먹어볼래요",
          "original_form": "알마치맛있다고들었어요ㅜㅜ저도담에 꼭시켜먹어볼래요",
          "speaker_id": "2",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002588.1.18",
          "form": "후라이드도 가끔먹으면 넘맛있고",
          "original_form": "후라이드도 가끔먹으면 넘맛있고",
          "speaker_id": "2",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002588.1.19",
          "form": "진짜 치킨 만드신분 절받아야합니다",
          "original_form": "진짜 치킨 만드신분 절받아야합니다",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002588.1.20",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002588.1.21",
          "form": "후라이드파세요?! 저도 후라이드를 더 좋아해요",
          "original_form": "후라이드파세요?! 저도 후라이드를 더 좋아해요",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002588.1.22",
          "form": "알마치 꼭 드셔보세요 알싸하게 자꾸 당기는맛이에요",
          "original_form": "알마치 꼭 드셔보세요 알싸하게 자꾸 당기는맛이에요",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002588.1.23",
          "form": "전 다좋아해요 ㅋㅋㅋㅋ네 알싸한마늘치킨",
          "original_form": "전 다좋아해요 ㅋㅋㅋㅋ네 알싸한마늘치킨",
          "speaker_id": "2",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002588.1.24",
          "form": "치킨이랑 음료는뭐드세요?",
          "original_form": "치킨이랑 음료는뭐드세요?",
          "speaker_id": "2",
          "time": "20210820 16:57"
        },
        {
