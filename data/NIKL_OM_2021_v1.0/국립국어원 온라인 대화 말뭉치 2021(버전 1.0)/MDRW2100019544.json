      "utterance": [
        {
          "id": "MDRW2100019544.1.1",
          "form": "연애하고 싶진 않아?",
          "original_form": "연애하고 싶진 않아?",
          "speaker_id": "1",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019544.1.2",
          "form": "난 지금은 별로",
          "original_form": "난 지금은 별로",
          "speaker_id": "2",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019544.1.3",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019544.1.4",
          "form": "외롭지가 않아",
          "original_form": "외롭지가 않아",
          "speaker_id": "2",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019544.1.5",
          "form": "심심하잖아 혼자 있으면",
          "original_form": "심심하잖아 혼자 있으면",
          "speaker_id": "1",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019544.1.6",
          "form": "난 그 시간이 소중해",
          "original_form": "난 그 시간이 소중해",
          "speaker_id": "2",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019544.1.7",
          "form": "같이 맛있는게 먹고 싶을 땐?",
          "original_form": "같이 맛있는게 먹고 싶을 땐?",
          "speaker_id": "1",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019544.1.8",
          "form": "친구를 부르지",
          "original_form": "친구를 부르지",
          "speaker_id": "2",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019544.1.9",
          "form": "하긴 연애를 꼭 안해도 되긴 해",
          "original_form": "하긴 연애를 꼭 안해도 되긴 해",
          "speaker_id": "1",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019544.1.10",
          "form": "그래도 좋아하는 사람이 생기면 할거야",
          "original_form": "그래도 좋아하는 사람이 생기면 할거야",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019544.1.11",
          "form": "지금은 그럼 소개팅 이런것도 안 받아?",
          "original_form": "지금은 그럼 소개팅 이런것도 안 받아?",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019544.1.12",
          "form": "최근에 한번 했는데 안맞더라",
          "original_form": "최근에 한번 했는데 안맞더라",
          "speaker_id": "2",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019544.1.13",
          "form": "어떤 점이.",
          "original_form": "어떤 점이.",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019544.1.14",
          "form": "너무 어색했어",
          "original_form": "너무 어색했어",
          "speaker_id": "2",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019544.1.15",
          "form": "하긴 너 성격이랑 안어울려",
          "original_form": "하긴 너 성격이랑 안어울려",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019544.1.16",
          "form": "안념",
          "original_form": "안념",
          "speaker_id": "2",
