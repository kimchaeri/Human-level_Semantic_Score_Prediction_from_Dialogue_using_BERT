      "utterance": [
        {
          "id": "MDRW2100042419.1.1",
          "form": "과자도 드세요?!",
          "original_form": "과자도 드세요?!",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042419.1.2",
          "form": "네 먹어요",
          "original_form": "네 먹어요",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042419.1.3",
          "form": "어떤과자 좋아하세요??",
          "original_form": "어떤과자 좋아하세요??",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042419.1.4",
          "form": "단백질바? 좋아해요",
          "original_form": "단백질바? 좋아해요",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042419.1.5",
          "form": "아 든든하니 맛있죠",
          "original_form": "아 든든하니 맛있죠",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042419.1.6",
          "form": "님은요 ?",
          "original_form": "님은요 ?",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042419.1.7",
          "form": "저는 가리는건 딱히 없는데,",
          "original_form": "저는 가리는건 딱히 없는데,",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042419.1.8",
          "form": "오트밀바이트 드셔보셨어요?",
          "original_form": "오트밀바이트 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042419.1.9",
          "form": "네그거 먹어봤어요",
          "original_form": "네그거 먹어봤어요",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042419.1.10",
          "form": "맛있더라고요",
          "original_form": "맛있더라고요",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042419.1.11",
          "form": "그쵸!!",
          "original_form": "그쵸!!",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042419.1.12",
          "form": "요즘 유행한대요",
          "original_form": "요즘 유행한대요",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042419.1.13",
          "form": "인간사료라고ㅋㅋ",
          "original_form": "인간사료라고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042419.1.14",
          "form": "인간사료 ㅎㅎㅎ",
          "original_form": "인간사료 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042419.1.15",
          "form": "계속 먹히긴 하더라고요",
          "original_form": "계속 먹히긴 하더라고요",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042419.1.16",
          "form": "그쵸 앉아있으면 계속 먹히는ㅋㅋ",
          "original_form": "그쵸 앉아있으면 계속 먹히는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042419.1.17",
          "form": "가격도 저렴하고 ㅎ",
          "original_form": "가격도 저렴하고 ㅎ",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042419.1.18",
          "form": "아맞아요 대용량인데 저렴하더라고요!",
          "original_form": "아맞아요 대용량인데 저렴하더라고요!",
