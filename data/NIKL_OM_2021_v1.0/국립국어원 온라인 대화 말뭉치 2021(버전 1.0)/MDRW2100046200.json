      "utterance": [
        {
          "id": "MDRW2100046200.1.1",
          "form": "송승헌 연기 잘하더라구요",
          "original_form": "송승헌 연기 잘하더라구요",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046200.1.2",
          "form": "옹 데릭조! 맘에 들었어요 ㅎㅎ",
          "original_form": "옹 데릭조! 맘에 들었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046200.1.3",
          "form": "데릭조~~다음 시즌 기다려져요",
          "original_form": "데릭조~~다음 시즌 기다려져요",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046200.1.4",
          "form": "아 진짜 시즌 5 ㅋㅋㅋ너무 기대되여",
          "original_form": "아 진짜 시즌 5 ㅋㅋㅋ너무 기대되여",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046200.1.5",
          "form": "마지막에 센터장님 ㅠㅠ 끌려가셔가지구",
          "original_form": "마지막에 센터장님 ㅠㅠ 끌려가셔가지구",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046200.1.6",
          "form": "자발적으로 간거라고 생각합니다 ㅋ비밀을 알기위해 ㅋ",
          "original_form": "자발적으로 간거라고 생각합니다 ㅋ비밀을 알기위해 ㅋ",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046200.1.7",
          "form": "오 진짜여?? 끌려간게 아니라구 생각하세염",
          "original_form": "오 진짜여?? 끌려간게 아니라구 생각하세염",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046200.1.8",
          "form": "헉 .. 이런 관점은 생각도 못해봤네요",
          "original_form": "헉 .. 이런 관점은 생각도 못해봤네요",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046200.1.9",
          "form": "네. 궁금증을 해소하기 위해 편지남기고 차에 오른거 같아요",
          "original_form": "네. 궁금증을 해소하기 위해 편지남기고 차에 오른거 같아요",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046200.1.10",
          "form": "정신과 의사누군지 궁금해요",
          "original_form": "정신과 의사누군지 궁금해요",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046200.1.11",
          "form": "호오오홍 되게 새로운 시각으로 보시나봐여",
          "original_form": "호오오홍 되게 새로운 시각으로 보시나봐여",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046200.1.12",
          "form": "ㅋㅋㅋㅋㅋ저는 그냥 보여주는대로 믿는 쪽이에욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ저는 그냥 보여주는대로 믿는 쪽이에욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046200.1.13",
          "form": "그런가요 ㅋ",
          "original_form": "그런가요 ㅋ",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046200.1.14",
          "form": "넼ㅋㅋㅋ 저는 응답같은것도 그 사전에 보여줬다는거",
          "original_form": "넼ㅋㅋㅋ 저는 응답같은것도 그 사전에 보여줬다는거",
          "speaker_id": "1",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046200.1.15",
          "form": "하나도 눈치못챘었어요",
          "original_form": "하나도 눈치못챘었어요",
          "speaker_id": "1",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046200.1.16",
          "form": "드라마도 요즘 너무잘 만드는것 같아요\r\n내일 슬의생 기다려집니다",
          "original_form": "드라마도 요즘 너무잘 만드는것 같아요\r\n내일 슬의생 기다려집니다",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046200.1.17",
          "form": "진짜 기대합니다ㅎㅅ",
          "original_form": "진짜 기대합니다ㅎㅅ",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046200.1.18",
          "form": "지난주 못한것 까지 2회 연속 해줬으면 좋겠어요",
          "original_form": "지난주 못한것 까지 2회 연속 해줬으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046200.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ앗 그럼 참 좋을텐데요ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ앗 그럼 참 좋을텐데요ㅠ",
