      "utterance": [
        {
          "id": "MDRW2100027540.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027540.1.2",
          "form": "좋아하시는 선수분 혹시 있으신가욧...?",
          "original_form": "좋아하시는 선수분 혹시 있으신가욧...?",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027540.1.3",
          "form": "배구에서 박정아선수 좋아해요!",
          "original_form": "배구에서 박정아선수 좋아해요!",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027540.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027540.1.5",
          "form": "전 안산 선수님이요 후후후",
          "original_form": "전 안산 선수님이요 후후후",
          "speaker_id": "1",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100027540.1.6",
          "form": "박정아 선수님도 진짜 멋지죠ㅠㅠㅠ",
          "original_form": "박정아 선수님도 진짜 멋지죠ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100027540.1.7",
          "form": "안산선수님도 너무 멋지죠ㅠㅠ",
          "original_form": "안산선수님도 너무 멋지죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027540.1.8",
          "form": "이런말해도되는지 모르겠지만.. 귀여우시구...",
          "original_form": "이런말해도되는지 모르겠지만.. 귀여우시구...",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027540.1.9",
          "form": "아 맞아요... 너무...깜찍.....",
          "original_form": "아 맞아요... 너무...깜찍.....",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027540.1.10",
          "form": "근데 나이에 안맞게 진짜 어른스럽구....",
          "original_form": "근데 나이에 안맞게 진짜 어른스럽구....",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027540.1.11",
          "form": "쏘실때 보면 집중력이 진짜...",
          "original_form": "쏘실때 보면 집중력이 진짜...",
          "speaker_id": "2",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027540.1.12",
          "form": "멘탈관리도 잘하시구...",
          "original_form": "멘탈관리도 잘하시구...",
          "speaker_id": "2",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027540.1.13",
          "form": "그쵸 진짜 대단하신거같아요",
          "original_form": "그쵸 진짜 대단하신거같아요",
          "speaker_id": "1",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027540.1.14",
          "form": "저도 본받고싶음 선수님",
          "original_form": "저도 본받고싶음 선수님",
          "speaker_id": "1",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027540.1.15",
          "form": "저두요 이번에 3관왕하시는거 보고 소리질렀어요",
          "original_form": "저두요 이번에 3관왕하시는거 보고 소리질렀어요",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027540.1.16",
          "form": "저만 떠는줄 알았네요",
          "original_form": "저만 떠는줄 알았네요",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027540.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저만 와들와들했잠ㅎ아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저만 와들와들했잠ㅎ아요",
          "speaker_id": "1",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100027540.1.18",
          "form": "선수님은 평온 ㅡ",
          "original_form": "선수님은 평온 ㅡ",
          "speaker_id": "1",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100027540.1.19",
          "form": "금메달따셔도 흐뭇해하시기만하구 저는 친구랑 소리지르면서 뛰고난리였어요ㅋㅋㅋㅋ",
          "original_form": "금메달따셔도 흐뭇해하시기만하구 저는 친구랑 소리지르면서 뛰고난리였어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027540.1.20",
          "form": "앜ㅋㅋㅋㅋ 그니까요 ㅋㅋㅋㅋㅋ 흐뭇이라니",
          "original_form": "앜ㅋㅋㅋㅋ 그니까요 ㅋㅋㅋㅋㅋ 흐뭇이라니",
          "speaker_id": "1",
          "time": "20210902 16:15"
        },
        {
          "id": "MDRW2100027540.1.21",
          "form": "저였으면 앞구르기 뒤구르기했어요",
          "original_form": "저였으면 앞구르기 뒤구르기했어요",
          "speaker_id": "1",
          "time": "20210902 16:15"
        },
        {
          "id": "MDRW2100027540.1.22",
          "form": "저였으면 하늘로 주먹질 했어요",
          "original_form": "저였으면 하늘로 주먹질 했어요",
          "speaker_id": "2",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100027540.1.23",
          "form": "양궁은 직관 가능한가요?",
          "original_form": "양궁은 직관 가능한가요?",
          "speaker_id": "2",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100027540.1.24",
          "form": "해보고 싶네요",
          "original_form": "해보고 싶네요",
          "speaker_id": "2",
          "time": "20210902 16:17"
        },
        {
          "id": "MDRW2100027540.1.25",
          "form": "그러게요 양궁직관이 되나...?",
          "original_form": "그러게요 양궁직관이 되나...?",
          "speaker_id": "1",
          "time": "20210902 16:17"
        },
        {
          "id": "MDRW2100027540.1.26",
          "form": "저도 눈앞에서 보고싶어욧",
          "original_form": "저도 눈앞에서 보고싶어욧",
          "speaker_id": "1",
          "time": "20210902 16:17"
        }
      ]
