      "utterance": [
        {
          "id": "MDRW2100013952.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013952.1.2",
          "form": "해야죠 그럼요",
          "original_form": "해야죠 그럼요",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100013952.1.3",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100013952.1.4",
          "form": "다 분담되어있나요?",
          "original_form": "다 분담되어있나요?",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100013952.1.5",
          "form": "네 화장실필수요",
          "original_form": "네 화장실필수요",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100013952.1.6",
          "form": "오 바람직하네요 ㅎㅎ 힘들어요 화장실 청소",
          "original_form": "오 바람직하네요 ㅎㅎ 힘들어요 화장실 청소",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100013952.1.7",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100013952.1.8",
          "form": "락스말고 다른걸로하세요",
          "original_form": "락스말고 다른걸로하세요",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100013952.1.9",
          "form": "대체품있니요?",
          "original_form": "대체품있니요?",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100013952.1.10",
          "form": "글쎄요 저흰무슨 스프레이로",
          "original_form": "글쎄요 저흰무슨 스프레이로",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100013952.1.11",
          "form": "곰팡이 제거제 것도 냄새나는",
          "original_form": "곰팡이 제거제 것도 냄새나는",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100013952.1.12",
          "form": "거품나는 스프레이 있던데요",
          "original_form": "거품나는 스프레이 있던데요",
          "speaker_id": "2",
          "time": "20210825 23:55"
