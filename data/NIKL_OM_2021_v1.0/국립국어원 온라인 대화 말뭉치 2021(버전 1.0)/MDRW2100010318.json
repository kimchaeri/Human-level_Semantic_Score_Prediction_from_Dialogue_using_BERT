      "utterance": [
        {
          "id": "MDRW2100010318.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210823 16:45"
        },
        {
          "id": "MDRW2100010318.1.2",
          "form": "최근 읽으신 책이 있나요?",
          "original_form": "최근 읽으신 책이 있나요?",
          "speaker_id": "1",
          "time": "20210823 16:45"
        },
        {
          "id": "MDRW2100010318.1.3",
          "form": "저는 비엘소설 읽어요",
          "original_form": "저는 비엘소설 읽어요",
          "speaker_id": "2",
          "time": "20210823 17:00"
        },
        {
          "id": "MDRW2100010318.1.4",
          "form": "아하.... 저는 역사로맨스물 좋아해요!",
          "original_form": "아하.... 저는 역사로맨스물 좋아해요!",
          "speaker_id": "1",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010318.1.5",
          "form": "주로 종이책으로 읽으세요?",
          "original_form": "주로 종이책으로 읽으세요?",
          "speaker_id": "1",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010318.1.6",
          "form": "아뇨! 주로 이북으로 읽어요",
          "original_form": "아뇨! 주로 이북으로 읽어요",
          "speaker_id": "2",
          "time": "20210824 16:20"
        },
        {
          "id": "MDRW2100010318.1.7",
          "form": "역사로맨스물은 어떤거 읽으시나요?",
          "original_form": "역사로맨스물은 어떤거 읽으시나요?",
          "speaker_id": "2",
          "time": "20210824 16:20"
        },
        {
          "id": "MDRW2100010318.1.8",
          "form": "저는 정은궐작가 좋아합니다! 최근에는 영원의 사자라는 소설 읽었어요",
          "original_form": "저는 정은궐작가 좋아합니다! 최근에는 영원의 사자라는 소설 읽었어요",
          "speaker_id": "1",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100010318.1.9",
          "form": "앗 저도 정은궐 작가님 책 읽어본적있어요!",
          "original_form": "앗 저도 정은궐 작가님 책 읽어본적있어요!",
          "speaker_id": "2",
          "time": "20210825 01:16"
        },
        {
          "id": "MDRW2100010318.1.10",
          "form": "신비주의 작가님 아니신가요?ㅎㅎㅎ",
          "original_form": "신비주의 작가님 아니신가요?ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100010318.1.11",
          "form": "평범한 회사원인데 매달 수천만원 통장에 들어오신다는ㅎㅎㅎ 부러운분",
          "original_form": "평범한 회사원인데 매달 수천만원 통장에 들어오신다는ㅎㅎㅎ 부러운분",
          "speaker_id": "2",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100010318.1.12",
          "form": "엇맞아요! 저도 그런 기사를 읽은 적 있어요",
          "original_form": "엇맞아요! 저도 그런 기사를 읽은 적 있어요",
          "speaker_id": "1",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100010318.1.13",
          "form": "몰랐는데 그동안 계속 작품을 냈더라구요",
          "original_form": "몰랐는데 그동안 계속 작품을 냈더라구요",
          "speaker_id": "1",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100010318.1.14",
          "form": "ㅋㅋㅋ근데 예전에 규장각 유생들의 나날?",
          "original_form": "ㅋㅋㅋ근데 예전에 규장각 유생들의 나날?",
          "speaker_id": "2",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100010318.1.15",
          "form": "그 뭐지 드라마화 하나 됨거 이후론",
          "original_form": "그 뭐지 드라마화 하나 됨거 이후론",
          "speaker_id": "2",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100010318.1.16",
          "form": "엄청 막 이거 유명하다!!",
          "original_form": "엄청 막 이거 유명하다!!",
          "speaker_id": "2",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100010318.1.17",
          "form": "하는건 없는거같긴해요",
          "original_form": "하는건 없는거같긴해요",
          "speaker_id": "2",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100010318.1.18",
          "form": "맞아요 근데 곧 방영할 홍천기도 정은궐 작가님 작품이에요! 해품달도!",
          "original_form": "맞아요 근데 곧 방영할 홍천기도 정은궐 작가님 작품이에요! 해품달도!",
          "speaker_id": "1",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100010318.1.19",
          "form": "앗 진짜요??? 몰랐어요 ㅋㅋㅋㅋ",
          "original_form": "앗 진짜요??? 몰랐어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100010318.1.20",
          "form": "아 해품달이 있었군요!!!",
          "original_form": "아 해품달이 있었군요!!!",
          "speaker_id": "2",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100010318.1.21",
          "form": "작가님 돈많이 버실만하네요 ㅎ",
          "original_form": "작가님 돈많이 버실만하네요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100010318.1.22",
          "form": "그래서 그 드라마 굉장히 기대하는 중이에요!",
          "original_form": "그래서 그 드라마 굉장히 기대하는 중이에요!",
          "speaker_id": "1",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100010318.1.23",
          "form": "캐스팅도 마음에들어서 ㅎ.ㅎ",
          "original_form": "캐스팅도 마음에들어서 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100010318.1.24",
          "form": "오오ㅎㅎㅎ 누가 캐스팅 됐나요?",
          "original_form": "오오ㅎㅎㅎ 누가 캐스팅 됐나요?",
          "speaker_id": "2",
          "time": "20210827 00:34"
        },
        {
          "id": "MDRW2100010318.1.25",
          "form": "그러고보면 그 전작들이 성스랑 해품달 맞죠?",
          "original_form": "그러고보면 그 전작들이 성스랑 해품달 맞죠?",
          "speaker_id": "2",
          "time": "20210827 00:34"
        },
        {
          "id": "MDRW2100010318.1.26",
          "form": "캐스팅 되게 잘된거같아요",
          "original_form": "캐스팅 되게 잘된거같아요",
          "speaker_id": "2",
          "time": "20210827 00:34"
        },
        {
          "id": "MDRW2100010318.1.27",
          "form": "김유정이랑 안효섭이요!",
          "original_form": "김유정이랑 안효섭이요!",
          "speaker_id": "1",
          "time": "20210827 00:35"
        },
        {
          "id": "MDRW2100010318.1.28",
          "form": "유정이 사극은 믿고보는 편이라서ㅋㅋㅋㅋㅋ",
          "original_form": "유정이 사극은 믿고보는 편이라서ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
