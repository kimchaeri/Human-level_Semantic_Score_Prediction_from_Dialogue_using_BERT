      "utterance": [
        {
          "id": "MDRW2100030403.1.1",
          "form": "영업직은 힘들어요",
          "original_form": "영업직은 힘들어요",
          "speaker_id": "2",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030403.1.2",
          "form": "잘못 눌렀어요",
          "original_form": "잘못 눌렀어요",
          "speaker_id": "1",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030403.1.3",
          "form": "진상이 많죠",
          "original_form": "진상이 많죠",
          "speaker_id": "1",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030403.1.4",
          "form": "그냥 하죠",
          "original_form": "그냥 하죠",
          "speaker_id": "2",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030403.1.5",
          "form": "요즘 예민하지 않아요",
          "original_form": "요즘 예민하지 않아요",
          "speaker_id": "1",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030403.1.6",
          "form": "엄마들과 상담이 스트레스예요",
          "original_form": "엄마들과 상담이 스트레스예요",
          "speaker_id": "2",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030403.1.7",
          "form": "예민하잖아요",
          "original_form": "예민하잖아요",
          "speaker_id": "1",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030403.1.8",
          "form": "갑질 하나요",
          "original_form": "갑질 하나요",
          "speaker_id": "1",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030403.1.9",
          "form": "나이 어린 엄마들은 개념이 별로 없어요",
          "original_form": "나이 어린 엄마들은 개념이 별로 없어요",
          "speaker_id": "2",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030403.1.10",
          "form": "엄청 따지는데 그게 현명하다고 생각하지만",
          "original_form": "엄청 따지는데 그게 현명하다고 생각하지만",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030403.1.11",
          "form": "나이가 어리다면 젊은 사람들 일 텐데",
          "original_form": "나이가 어리다면 젊은 사람들 일 텐데",
          "speaker_id": "1",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030403.1.12",
          "form": "자기아이한테 돌아갈 걸 생각 못 하나 봐요",
          "original_form": "자기아이한테 돌아갈 걸 생각 못 하나 봐요",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030403.1.13",
          "form": "융통성 없군요",
          "original_form": "융통성 없군요",
          "speaker_id": "1",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030403.1.14",
          "form": "굉장히 많아요",
          "original_form": "굉장히 많아요",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030403.1.15",
          "form": "예의를 인터넷으로 배웠나 봐요",
          "original_form": "예의를 인터넷으로 배웠나 봐요",
          "speaker_id": "1",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030403.1.16",
          "form": "그 말이 딱입니다",
          "original_form": "그 말이 딱입니다",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030403.1.17",
          "form": "그래놓고 쿨하다고 하겠죠",
          "original_form": "그래놓고 쿨하다고 하겠죠",
          "speaker_id": "1",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030403.1.18",
          "form": "상담하면서 스트레스 받는 경우도 많구요",
          "original_form": "상담하면서 스트레스 받는 경우도 많구요",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030403.1.19",
          "form": "자랑하겠죠",
          "original_form": "자랑하겠죠",
          "speaker_id": "1",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030403.1.20",
          "form": "힐링 받을 때도 많아요",
          "original_form": "힐링 받을 때도 많아요",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030403.1.21",
          "form": "목소리만 크고 원리는 없죠",
          "original_form": "목소리만 크고 원리는 없죠",
          "speaker_id": "1",
          "time": "20210904 00:37"
        }
