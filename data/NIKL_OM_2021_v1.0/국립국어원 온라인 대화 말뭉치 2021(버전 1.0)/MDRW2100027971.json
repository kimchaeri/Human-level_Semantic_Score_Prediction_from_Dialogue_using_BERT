      "utterance": [
        {
          "id": "MDRW2100027971.1.1",
          "form": "이열",
          "original_form": "이열",
          "speaker_id": "2",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027971.1.2",
          "form": "풀강하는 교수님 최악.",
          "original_form": "풀강하는 교수님 최악.",
          "speaker_id": "2",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027971.1.3",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027971.1.4",
          "form": "교수님도 힘드시잖아요",
          "original_form": "교수님도 힘드시잖아요",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027971.1.5",
          "form": "싸게싸게 끝내주세요",
          "original_form": "싸게싸게 끝내주세요",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027971.1.6",
          "form": "아 아까전에 타과 교수님이",
          "original_form": "아 아까전에 타과 교수님이",
          "speaker_id": "2",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027971.1.7",
          "form": "대면하던 시절에 오티 3시간 풀강을 했는데",
          "original_form": "대면하던 시절에 오티 3시간 풀강을 했는데",
          "speaker_id": "2",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027971.1.8",
          "form": "수업 끝나고 수업 들었던 학생이랑 우연히 엘베를 같이 탔다가",
          "original_form": "수업 끝나고 수업 들었던 학생이랑 우연히 엘베를 같이 탔다가",
          "speaker_id": "2",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027971.1.9",
          "form": "아니 그 교수님 오티 풀강했다고? ㅁ1ㅊ거아니야? 소리 듣고",
          "original_form": "아니 그 교수님 오티 풀강했다고? ㅁ1ㅊ거아니야? 소리 듣고",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027971.1.10",
          "form": "오티 풀강하는 교수는 ㅁ1ㅊ 교수구나 깨달으셨다고",
          "original_form": "오티 풀강하는 교수는 ㅁ1ㅊ 교수구나 깨달으셨다고",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027971.1.11",
          "form": "아무래도 그렇지...",
          "original_form": "아무래도 그렇지...",
          "speaker_id": "1",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027971.1.12",
          "form": "그제라도 아셔서 다행",
          "original_form": "그제라도 아셔서 다행",
          "speaker_id": "1",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027971.1.13",
          "form": "그래서 1교시만에 끝내주심",
          "original_form": "그래서 1교시만에 끝내주심",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027971.1.14",
          "form": "3시간 풀강은 진짜 최악",
          "original_form": "3시간 풀강은 진짜 최악",
          "speaker_id": "1",
          "time": "20210902 16:34"
        },
        {
          "id": "MDRW2100027971.1.15",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210902 16:34"
        },
        {
          "id": "MDRW2100027971.1.16",
          "form": "이건 오티가 아니어도 해서는 안되는 극악무도한 짓이다",
          "original_form": "이건 오티가 아니어도 해서는 안되는 극악무도한 짓이다",
          "speaker_id": "1",
          "time": "20210902 16:34"
        },
        {
          "id": "MDRW2100027971.1.17",
          "form": "근데 맨날 풀강하심",
          "original_form": "근데 맨날 풀강하심",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027971.1.18",
          "form": "이러지마새요",
          "original_form": "이러지마새요",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027971.1.19",
          "form": "이러시는 이유가 있을 거 아니에요",
          "original_form": "이러시는 이유가 있을 거 아니에요",
          "speaker_id": "1",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027971.1.20",
          "form": "쉽게쉽게 가자고요",
          "original_form": "쉽게쉽게 가자고요",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027971.1.21",
          "form": "진짜 체력도 좋으셔",
          "original_form": "진짜 체력도 좋으셔",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027971.1.22",
          "form": "어케 3시간 내내 말을 하는 거지",
          "original_form": "어케 3시간 내내 말을 하는 거지",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027971.1.23",
          "form": "그러나 여기선 풀강 안하면 학생이 민원(?) 넣고 그러더라",
          "original_form": "그러나 여기선 풀강 안하면 학생이 민원(?) 넣고 그러더라",
          "speaker_id": "2",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027971.1.24",
          "form": "웃기다",
          "original_form": "웃기다",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027971.1.25",
          "form": "왜 그런 짓을 하는 거야",
          "original_form": "왜 그런 짓을 하는 거야",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027971.1.26",
          "form": "수업시간 채워서 안한다고(...)",
          "original_form": "수업시간 채워서 안한다고(...)",
          "speaker_id": "2",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027971.1.27",
          "form": "우리가 3시간 수업 받을 권리가 있는데 날먹한다고",
          "original_form": "우리가 3시간 수업 받을 권리가 있는데 날먹한다고",
          "speaker_id": "2",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027971.1.28",
          "form": "케바케지",
          "original_form": "케바케지",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027971.1.29",
          "form": "3시간 내용을 2시간 압축하시면",
          "original_form": "3시간 내용을 2시간 압축하시면",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027971.1.30",
          "form": "이런 민원 안 받잖아요",
          "original_form": "이런 민원 안 받잖아요",
          "speaker_id": "1",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027971.1.31",
          "form": "펀쿨어쩌고",
          "original_form": "펀쿨어쩌고",
          "speaker_id": "2",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027971.1.32",
          "form": "3시간 안 채우는 교양이 진짜 꿀인데",
          "original_form": "3시간 안 채우는 교양이 진짜 꿀인데",
          "speaker_id": "1",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027971.1.33",
          "form": "전공은... 모르지만",
          "original_form": "전공은... 모르지만",
          "speaker_id": "1",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027971.1.34",
          "form": "꿀이지...",
          "original_form": "꿀이지...",
          "speaker_id": "2",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027971.1.35",
          "form": "3시간 수업인데 1시간 반만에 끝내주는 교수님",
          "original_form": "3시간 수업인데 1시간 반만에 끝내주는 교수님",
          "speaker_id": "2",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027971.1.36",
          "form": "진짜감사합니다",
          "original_form": "진짜감사합니다",
          "speaker_id": "2",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027971.1.37",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "1",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027971.1.38",
          "form": "3시간 수업인데 1시간 하고 쉬고 30분 하고 보내주시는 교수님",
          "original_form": "3시간 수업인데 1시간 하고 쉬고 30분 하고 보내주시는 교수님",
          "speaker_id": "1",
