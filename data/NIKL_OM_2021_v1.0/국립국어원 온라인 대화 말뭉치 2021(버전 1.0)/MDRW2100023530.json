      "utterance": [
        {
          "id": "MDRW2100023530.1.1",
          "form": "안녕하세요, 혹시 운동하시는거 있나요?",
          "original_form": "안녕하세요, 혹시 운동하시는거 있나요?",
          "speaker_id": "1",
          "time": "20210901 12:19"
        },
        {
          "id": "MDRW2100023530.1.2",
          "form": "헬스합니다!",
          "original_form": "헬스합니다!",
          "speaker_id": "2",
          "time": "20210901 12:20"
        },
        {
          "id": "MDRW2100023530.1.3",
          "form": "헬스! 좋죠.",
          "original_form": "헬스! 좋죠.",
          "speaker_id": "1",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100023530.1.4",
          "form": "저도 예전엔 했답니다. 다만 어느순간 자꾸 안가게 돼서... ㅎㅎ",
          "original_form": "저도 예전엔 했답니다. 다만 어느순간 자꾸 안가게 돼서... ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100023530.1.5",
          "form": "아아 ㅎㅎ 맞아요",
          "original_form": "아아 ㅎㅎ 맞아요",
          "speaker_id": "2",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100023530.1.6",
          "form": "한번 안가면 아예 안가게 됩니다 ㅋㅋ",
          "original_form": "한번 안가면 아예 안가게 됩니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100023530.1.7",
          "form": "전 그래서 지금 암벽등반을 해보고 있어요",
          "original_form": "전 그래서 지금 암벽등반을 해보고 있어요",
          "speaker_id": "1",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100023530.1.8",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "2",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100023530.1.9",
          "form": "재미가 있으면 계속하지 않을까 해서요",
          "original_form": "재미가 있으면 계속하지 않을까 해서요",
          "speaker_id": "1",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100023530.1.10",
          "form": "그 클라이밍 맞나요?",
          "original_form": "그 클라이밍 맞나요?",
          "speaker_id": "2",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100023530.1.11",
          "form": "네. 그거 맞아요",
          "original_form": "네. 그거 맞아요",
          "speaker_id": "1",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100023530.1.12",
          "form": "실내에서 하는거지만요",
          "original_form": "실내에서 하는거지만요",
          "speaker_id": "1",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100023530.1.13",
          "form": "저도 그거 해보고 싶은데 !",
          "original_form": "저도 그거 해보고 싶은데 !",
          "speaker_id": "2",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100023530.1.14",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "2",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100023530.1.15",
          "form": "일단 저한텐 재밌고",
          "original_form": "일단 저한텐 재밌고",
          "speaker_id": "1",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100023530.1.16",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100023530.1.17",
          "form": "운동 많이 돼요.",
          "original_form": "운동 많이 돼요.",
          "speaker_id": "1",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100023530.1.18",
          "form": "전신근육이 다 쓰인다던데",
          "original_form": "전신근육이 다 쓰인다던데",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100023530.1.19",
          "form": "맞나요?",
          "original_form": "맞나요?",
          "speaker_id": "2",
          "time": "20210901 12:24"
        }
      ]
