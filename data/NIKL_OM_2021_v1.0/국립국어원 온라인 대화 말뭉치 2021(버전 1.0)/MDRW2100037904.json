      "utterance": [
        {
          "id": "MDRW2100037904.1.1",
          "form": "배달음식 중 가장 맛났던건 뭐임?",
          "original_form": "배달음식 중 가장 맛났던건 뭐임?",
          "speaker_id": "2",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100037904.1.2",
          "form": "나 요즘 머릿속에 로제 떡볶이 생각뿐이야",
          "original_form": "나 요즘 머릿속에 로제 떡볶이 생각뿐이야",
          "speaker_id": "1",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100037904.1.3",
          "form": "배고프다",
          "original_form": "배고프다",
          "speaker_id": "1",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100037904.1.4",
          "form": "다음에는 분 모자랑 납작 당면 도 추가하고 싶어",
          "original_form": "다음에는 분 모자랑 납작 당면 도 추가하고 싶어",
          "speaker_id": "1",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100037904.1.5",
          "form": "석관동 떡볶이오면 거다 납당 넣자",
          "original_form": "석관동 떡볶이오면 거다 납당 넣자",
          "speaker_id": "2",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100037904.1.6",
          "form": "아 맞다 우리 석관동 떡볶이 시켰지",
          "original_form": "아 맞다 우리 석관동 떡볶이 시켰지",
          "speaker_id": "1",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100037904.1.7",
          "form": "아 다 로제로 시킬걸 그랬나...",
          "original_form": "아 다 로제로 시킬걸 그랬나...",
          "speaker_id": "1",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100037904.1.8",
          "form": "이것저것 먹어보지 뭐",
          "original_form": "이것저것 먹어보지 뭐",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100037904.1.9",
          "form": "손아프니 배달음식이 좋다",
          "original_form": "손아프니 배달음식이 좋다",
          "speaker_id": "2",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100037904.1.10",
          "form": "배달을 만이 시켜먹자 맛난걸로",
          "original_form": "배달을 만이 시켜먹자 맛난걸로",
          "speaker_id": "1",
          "time": "20210902 14:45"
        },
        {
          "id": "MDRW2100037904.1.11",
          "form": "반조리도 좋아",
          "original_form": "반조리도 좋아",
          "speaker_id": "2",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100037904.1.12",
          "form": "저녁은 샤브샤브먹자",
          "original_form": "저녁은 샤브샤브먹자",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100037904.1.13",
          "form": "고기사갈까?",
          "original_form": "고기사갈까?",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100037904.1.14",
          "form": "또 샤브샤브?",
          "original_form": "또 샤브샤브?",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100037904.1.15",
          "form": "사놓은거 먹어야해 안그럼 상해",
          "original_form": "사놓은거 먹어야해 안그럼 상해",
          "speaker_id": "2",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100037904.1.16",
          "form": "그럼 거기다 만두넣어서먹자",
          "original_form": "그럼 거기다 만두넣어서먹자",
          "speaker_id": "2",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100037904.1.17",
          "form": "아나야 고기를 많이 먹어야된다고",
          "original_form": "아나야 고기를 많이 먹어야된다고",
          "speaker_id": "1",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100037904.1.18",
          "form": "고기도먹고 만두도 머자 ㅎ",
          "original_form": "고기도먹고 만두도 머자 ㅎ",
          "speaker_id": "2",
          "time": "20210902 16:55"
        },
        {
          "id": "MDRW2100037904.1.19",
          "form": "만두는 관심 없어 칼국수가 좋아",
          "original_form": "만두는 관심 없어 칼국수가 좋아",
          "speaker_id": "1",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100037904.1.20",
          "form": "아니되오 골고루 먹어야하오 저녁메뉸 만두샤브샤브 우하하",
          "original_form": "아니되오 골고루 먹어야하오 저녁메뉸 만두샤브샤브 우하하",
          "speaker_id": "2",
          "time": "20210902 16:57"
        }
      ]
