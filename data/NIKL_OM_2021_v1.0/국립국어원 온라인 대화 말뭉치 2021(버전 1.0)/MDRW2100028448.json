      "utterance": [
        {
          "id": "MDRW2100028448.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ케톡만 안봐도 심신안정....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ케톡만 안봐도 심신안정....",
          "speaker_id": "2",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028448.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ거긴보면안돼....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ거긴보면안돼....",
          "speaker_id": "1",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028448.1.3",
          "form": "거긴맨날싸워",
          "original_form": "거긴맨날싸워",
          "speaker_id": "2",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028448.1.4",
          "form": "요새 안갔더니 심신이 편안해ㅔ짐",
          "original_form": "요새 안갔더니 심신이 편안해ㅔ짐",
          "speaker_id": "1",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028448.1.5",
          "form": "맨날 싸우고 헐뜯고 변방 트위터 가져와서 또 싸우고ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맨날 싸우고 헐뜯고 변방 트위터 가져와서 또 싸우고ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028448.1.6",
          "form": "에휴",
          "original_form": "에휴",
          "speaker_id": "2",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028448.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ교무실임 완전",
          "original_form": "ㅋㅋㅋㅋㅋㅋ교무실임 완전",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028448.1.8",
          "form": "가져와서 같이싸워달라고 ㅋㅋㅋㅋㅋ",
          "original_form": "가져와서 같이싸워달라고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028448.1.9",
          "form": "정력도 좋음",
          "original_form": "정력도 좋음",
          "speaker_id": "1",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100028448.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞어 ... \r\n그냥 각자 즐거운 머리 꽃밭 덕질할것이지..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞어 ... \r\n그냥 각자 즐거운 머리 꽃밭 덕질할것이지..",
          "speaker_id": "2",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100028448.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ절대 그거 안되고 꼭 까내리고 헐뜯어야 만족하는 사람들...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ절대 그거 안되고 꼭 까내리고 헐뜯어야 만족하는 사람들...",
          "speaker_id": "1",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100028448.1.12",
          "form": "으으...남 까내린다고 자기 아이돌이 올라가는 것도 아닌데 ...참",
          "original_form": "으으...남 까내린다고 자기 아이돌이 올라가는 것도 아닌데 ...참",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028448.1.13",
          "form": "또 거기에 먹이주는",
          "original_form": "또 거기에 먹이주는",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028448.1.14",
          "form": "그니까 ㅇㅇㅇ",
          "original_form": "그니까 ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028448.1.15",
          "form": "눈새팬도 문제야",
          "original_form": "눈새팬도 문제야",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028448.1.16",
          "form": "왜그러고 사는지몰라",
          "original_form": "왜그러고 사는지몰라",
          "speaker_id": "1",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028448.1.17",
          "form": "각자 인생 신경끄고살면 편한데",
          "original_form": "각자 인생 신경끄고살면 편한데",
          "speaker_id": "1",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028448.1.18",
          "form": "눈새들이 일을 더 망쳐",
          "original_form": "눈새들이 일을 더 망쳐",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028448.1.19",
          "form": "그리고 제발 되도 안되는 쉴드치지마...\r\n잘못은 걍 잘못이야ㅜㅜ",
          "original_form": "그리고 제발 되도 안되는 쉴드치지마...\r\n잘못은 걍 잘못이야ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028448.1.20",
          "form": "오버해서 까는 애들은 걔가 문제인거지",
          "original_form": "오버해서 까는 애들은 걔가 문제인거지",
          "speaker_id": "2",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028448.1.21",
          "form": "괜히 쉴드치지말라고...",
          "original_form": "괜히 쉴드치지말라고...",
          "speaker_id": "2",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028448.1.22",
          "form": "ㅇㅇㅇㅇㄹㅇ",
          "original_form": "ㅇㅇㅇㅇㄹㅇ",
          "speaker_id": "1",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028448.1.23",
          "form": "그냥 아 담에그러지말아라",
          "original_form": "그냥 아 담에그러지말아라",
          "speaker_id": "1",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028448.1.24",
