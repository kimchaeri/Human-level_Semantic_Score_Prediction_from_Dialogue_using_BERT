      "utterance": [
        {
          "id": "MDRW2100011836.1.1",
          "form": "영화관 판타지라니",
          "original_form": "영화관 판타지라니",
          "speaker_id": "1",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100011836.1.2",
          "form": "네 그 그런것",
          "original_form": "네 그 그런것",
          "speaker_id": "2",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100011836.1.3",
          "form": "단체로 공포영화 관람시키고 싶어요",
          "original_form": "단체로 공포영화 관람시키고 싶어요",
          "speaker_id": "1",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100011836.1.4",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011836.1.5",
          "form": "공포영화하니까 생각난건데",
          "original_form": "공포영화하니까 생각난건데",
          "speaker_id": "2",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011836.1.6",
          "form": "무서워하는 척 하면서",
          "original_form": "무서워하는 척 하면서",
          "speaker_id": "2",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011836.1.7",
          "form": "막 그런 것들 생각나욬ㅋㅋㅋㅋ",
          "original_form": "막 그런 것들 생각나욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011836.1.8",
          "form": "연애스킬?",
          "original_form": "연애스킬?",
          "speaker_id": "2",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011836.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 그걸 스킬이라고 하는것도 이상하지않나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 그걸 스킬이라고 하는것도 이상하지않나요",
          "speaker_id": "1",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011836.1.10",
          "form": "이런걸 왜 알고 있는지 참 아이러니하네요",
          "original_form": "이런걸 왜 알고 있는지 참 아이러니하네요",
          "speaker_id": "2",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011836.1.11",
          "form": "맞아욬ㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011836.1.12",
          "form": "하고싶음 하고 말려면 마는거지",
          "original_form": "하고싶음 하고 말려면 마는거지",
          "speaker_id": "1",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011836.1.13",
          "form": "뭔 게임도 아니고",
          "original_form": "뭔 게임도 아니고",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011836.1.14",
          "form": "이런거 무슨 조언서가 잇고 기술이 잇고",
          "original_form": "이런거 무슨 조언서가 잇고 기술이 잇고",
          "speaker_id": "1",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011836.1.15",
          "form": "ㄹㅇ 게임적 세계관",
          "original_form": "ㄹㅇ 게임적 세계관",
          "speaker_id": "1",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011836.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011836.1.17",
          "form": "연애 너무 잘 포장되어있어요",
          "original_form": "연애 너무 잘 포장되어있어요",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011836.1.18",
          "form": "저런 스킬 어휴",
          "original_form": "저런 스킬 어휴",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011836.1.19",
          "form": "맞아요 진짜 포장만 그럴듯하고 속엔 암것도없고",
          "original_form": "맞아요 진짜 포장만 그럴듯하고 속엔 암것도없고",
          "speaker_id": "1",
          "time": "20210824 16:31"
        },
        {
          "id": "MDRW2100011836.1.20",
          "form": "연애를 \"진도나가는 것\" 으로 생각하는거잖아요",
          "original_form": "연애를 \"진도나가는 것\" 으로 생각하는거잖아요",
          "speaker_id": "1",
          "time": "20210824 16:31"
        },
        {
          "id": "MDRW2100011836.1.21",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210824 16:31"
        },
        {
          "id": "MDRW2100011836.1.22",
          "form": "어느정도 되면",
          "original_form": "어느정도 되면",
          "speaker_id": "2",
          "time": "20210824 16:31"
        },
        {
          "id": "MDRW2100011836.1.23",
          "form": "어디까지 허용하고",
          "original_form": "어디까지 허용하고",
          "speaker_id": "2",
          "time": "20210824 16:31"
        },
        {
          "id": "MDRW2100011836.1.24",
          "form": "좀 진도가 빠른건지 고민하고",
          "original_form": "좀 진도가 빠른건지 고민하고",
          "speaker_id": "2",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100011836.1.25",
          "form": "그런건 또 여자들만 고민하는 것 같아요",
          "original_form": "그런건 또 여자들만 고민하는 것 같아요",
          "speaker_id": "2",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100011836.1.26",
          "form": "아 맞아요.. 허용의 감각으로 생각하는거같아요",
          "original_form": "아 맞아요.. 허용의 감각으로 생각하는거같아요",
          "speaker_id": "1",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100011836.1.27",
          "form": "하고 안하고 이느낌보다도..",
          "original_form": "하고 안하고 이느낌보다도..",
          "speaker_id": "1",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011836.1.28",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011836.1.29",
          "form": "안한다는 선택지가 없네요",
          "original_form": "안한다는 선택지가 없네요",
          "speaker_id": "2",
          "time": "20210824 16:33"
        }
      ]
    }
  ]
