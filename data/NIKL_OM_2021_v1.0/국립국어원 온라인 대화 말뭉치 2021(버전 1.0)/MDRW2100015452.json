      "utterance": [
        {
          "id": "MDRW2100015452.1.1",
          "form": "그림 동호회 하구 있엉",
          "original_form": "그림 동호회 하구 있엉",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015452.1.2",
          "form": "거기서 뭐해?",
          "original_form": "거기서 뭐해?",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015452.1.3",
          "form": "동호회 사람들하고 같이 그림 그려~",
          "original_form": "동호회 사람들하고 같이 그림 그려~",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015452.1.4",
          "form": "우와 재밌겠다 한달에 몇번가?",
          "original_form": "우와 재밌겠다 한달에 몇번가?",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015452.1.5",
          "form": "2번 정도 가는 것 같앙",
          "original_form": "2번 정도 가는 것 같앙",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015452.1.6",
          "form": "그거밖에 안가?",
          "original_form": "그거밖에 안가?",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015452.1.7",
          "form": "웅웅 요새 코로나라 모임 금지되잖아ㅠ",
          "original_form": "웅웅 요새 코로나라 모임 금지되잖아ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015452.1.8",
          "form": "회비는 있어?",
          "original_form": "회비는 있어?",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015452.1.9",
          "form": "한달에 5만원 정도?",
          "original_form": "한달에 5만원 정도?",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015452.1.10",
          "form": "뭐가 그리 비싸",
          "original_form": "뭐가 그리 비싸",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015452.1.11",
          "form": "재료비랑 장소 대여비가 포함되서 그런 것 같앙",
          "original_form": "재료비랑 장소 대여비가 포함되서 그런 것 같앙",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015452.1.12",
          "form": "그렇구나. 몇명에서 만나?",
          "original_form": "그렇구나. 몇명에서 만나?",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015452.1.13",
          "form": "글쿤",
          "original_form": "글쿤",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015452.1.14",
          "form": "아 그리고 4명 정도 만나",
          "original_form": "아 그리고 4명 정도 만나",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015452.1.15",
          "form": "아 코로나 때문에 그렇구나",
          "original_form": "아 코로나 때문에 그렇구나",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015452.1.16",
          "form": "웅웅 넌 동호회 하는 거 이써?",
          "original_form": "웅웅 넌 동호회 하는 거 이써?",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015452.1.17",
          "form": "없는데?",
          "original_form": "없는데?",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015452.1.18",
          "form": "그렇구나 하고 싶은 거 이써?",
          "original_form": "그렇구나 하고 싶은 거 이써?",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015452.1.19",
          "form": "없는것같아",
          "original_form": "없는것같아",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015452.1.20",
          "form": "웅ㅠ",
          "original_form": "웅ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015452.1.21",
          "form": "나는 취미가 없어",
          "original_form": "나는 취미가 없어",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015452.1.22",
          "form": "엥 쉬는 날에 모하는데?",
          "original_form": "엥 쉬는 날에 모하는데?",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015452.1.23",
          "form": "흠 그나마 관심있는건 게임?",
          "original_form": "흠 그나마 관심있는건 게임?",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015452.1.24",
          "form": "아 그럼 게임관련 동호회는 어때?",
          "original_form": "아 그럼 게임관련 동호회는 어때?",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015452.1.25",
          "form": "괜찮아",
          "original_form": "괜찮아",
          "speaker_id": "1",
          "time": "20210825 23:11"
        }
      ]
    }
