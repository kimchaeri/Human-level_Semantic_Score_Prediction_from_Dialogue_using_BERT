      "utterance": [
        {
          "id": "MDRW2100024056.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100024056.1.2",
          "form": "경기 직관을 해보신적 있나요?",
          "original_form": "경기 직관을 해보신적 있나요?",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100024056.1.3",
          "form": "무려 중학생때 배구장 갔었어요~",
          "original_form": "무려 중학생때 배구장 갔었어요~",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024056.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024056.1.5",
          "form": "저는 야구랑 평창때 컬링 봤어요",
          "original_form": "저는 야구랑 평창때 컬링 봤어요",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024056.1.6",
          "form": "배구!저 요즘 배구직관하고싶은데 어때요?",
          "original_form": "배구!저 요즘 배구직관하고싶은데 어때요?",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024056.1.7",
          "form": "기억나시나요?",
          "original_form": "기억나시나요?",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024056.1.8",
          "form": "평창가셨어요!? 멋지네요!!!",
          "original_form": "평창가셨어요!? 멋지네요!!!",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024056.1.9",
          "form": "ㅋㅋㅋ오 10년도 더 지낫는데 기억나네요",
          "original_form": "ㅋㅋㅋ오 10년도 더 지낫는데 기억나네요",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024056.1.10",
          "form": "사실 잘모르고 친구따라 간거였는데 보는재미가있었어요",
          "original_form": "사실 잘모르고 친구따라 간거였는데 보는재미가있었어요",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024056.1.11",
          "form": "배구농구 이런거 관람도 재밌는거같아요",
          "original_form": "배구농구 이런거 관람도 재밌는거같아요",
          "speaker_id": "2",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100024056.1.12",
          "form": "아시죠 강당 삑삑 소리? 신발소리요 ㅋㅋ 좋아요 ㅋㅋㅋ",
          "original_form": "아시죠 강당 삑삑 소리? 신발소리요 ㅋㅋ 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100024056.1.13",
          "form": "제가 그 핫한 야구경기를 못봤네요 아쉽게도",
          "original_form": "제가 그 핫한 야구경기를 못봤네요 아쉽게도",
          "speaker_id": "2",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100024056.1.14",
          "form": "평창갔었어요",
          "original_form": "평창갔었어요",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024056.1.15",
          "form": "운좋게 표를 구해서 구경갔었죠 강릉을 그때 처음가봤어요",
          "original_form": "운좋게 표를 구해서 구경갔었죠 강릉을 그때 처음가봤어요",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024056.1.16",
          "form": "아~~ 강릉도 같이 놀러다녀오신거에요~?",
          "original_form": "아~~ 강릉도 같이 놀러다녀오신거에요~?",
          "speaker_id": "2",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100024056.1.17",
          "form": "강릉에서 버스타면 평창이였어요",
          "original_form": "강릉에서 버스타면 평창이였어요",
          "speaker_id": "1",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100024056.1.18",
          "form": "올림픽 평창왕복 버스가 있었어요",
          "original_form": "올림픽 평창왕복 버스가 있었어요",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024056.1.19",
          "form": "아~~~~",
          "original_form": "아~~~~",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024056.1.20",
          "form": "그랫군요 ㅎㅎㅎ",
          "original_form": "그랫군요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024056.1.21",
          "form": "이번에는 도쿄올림픽도.. 첨으로 관중없이 하고..",
          "original_form": "이번에는 도쿄올림픽도.. 첨으로 관중없이 하고..",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024056.1.22",
          "form": "ㅎㅎ 코로나가 많은걸 새롭게 햇네요,,",
          "original_form": "ㅎㅎ 코로나가 많은걸 새롭게 햇네요,,",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024056.1.23",
          "form": "그죠....다음올림픽은 직관하고싶네요",
          "original_form": "그죠....다음올림픽은 직관하고싶네요",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024056.1.24",
          "form": "운동 보시는거말고 하시는거 있나요?",
          "original_form": "운동 보시는거말고 하시는거 있나요?",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024056.1.25",
          "form": "ㅎㅎ 스포츠 좋아하시죠!?",
          "original_form": "ㅎㅎ 스포츠 좋아하시죠!?",
          "speaker_id": "2",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024056.1.26",
          "form": "평창도 가시고! 적극적이신편!",
          "original_form": "평창도 가시고! 적극적이신편!",
          "speaker_id": "2",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024056.1.27",
          "form": "아 저는 스포츠는 아니지만 ㅋㅋ 필라테스 다니고있슴다..",
          "original_form": "아 저는 스포츠는 아니지만 ㅋㅋ 필라테스 다니고있슴다..",
          "speaker_id": "2",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024056.1.28",
          "form": "좋아하긴하는데 요즘 통 못하고있어요",
          "original_form": "좋아하긴하는데 요즘 통 못하고있어요",
          "speaker_id": "1",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100024056.1.29",
          "form": "여자배구 경기 꼭 직관한번 가보고싶어요",
          "original_form": "여자배구 경기 꼭 직관한번 가보고싶어요",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024056.1.30",
          "form": "하 여자배구",
          "original_form": "하 여자배구",
          "speaker_id": "2",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024056.1.31",
          "form": "그렇네요",
          "original_form": "그렇네요",
          "speaker_id": "2",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024056.1.32",
          "form": "저도 여자경기 보고싶네요",
          "original_form": "저도 여자경기 보고싶네요",
          "speaker_id": "2",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024056.1.33",
          "form": "스포츠 어떤거 하셨나요?",
          "original_form": "스포츠 어떤거 하셨나요?",
          "speaker_id": "2",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024056.1.34",
          "form": "학교다닐때 농구나 배드민턴 가볍게했어요",
          "original_form": "학교다닐때 농구나 배드민턴 가볍게했어요",
