        {
          "id": "MDRW2100044967.1.1",
          "form": "크림파스타먹을거에용ㅋㅋ",
          "original_form": "크림파스타먹을거에용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044967.1.2",
          "form": "와 맛있겠어여!",
          "original_form": "와 맛있겠어여!",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044967.1.3",
          "form": "전 해산물 좋아해서 ㅎㅎ",
          "original_form": "전 해산물 좋아해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044967.1.4",
          "form": "ㅋㅋ저는 둘다 좋아서 고민이에요",
          "original_form": "ㅋㅋ저는 둘다 좋아서 고민이에요",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044967.1.5",
          "form": "해산물은 매콤하다더라구요",
          "original_form": "해산물은 매콤하다더라구요",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044967.1.6",
          "form": "만드시는거 아니구요??",
          "original_form": "만드시는거 아니구요??",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044967.1.7",
          "form": "아니요.....못만들어요... 솜씨가 없어서ㅜㅜ",
          "original_form": "아니요.....못만들어요... 솜씨가 없어서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100044967.1.8",
          "form": "아 배달?",
          "original_form": "아 배달?",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100044967.1.9",
          "form": "알럽배민ㅋㅋ",
          "original_form": "알럽배민ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100044967.1.10",
          "form": "좋치용!!!!!배달은 사랑입니다.",
          "original_form": "좋치용!!!!!배달은 사랑입니다.",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100044967.1.11",
          "form": "근데 파스타 배달은 처음이어서 걱정됩니다",
          "original_form": "근데 파스타 배달은 처음이어서 걱정됩니다",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100044967.1.12",
          "form": "맛있어여! ㅋㅋㅋ 배달이즈 뭔들",
          "original_form": "맛있어여! ㅋㅋㅋ 배달이즈 뭔들",
