      "utterance": [
        {
          "id": "MDRW2100047485.1.1",
          "form": "네 먹었어요",
          "original_form": "네 먹었어요",
          "speaker_id": "2",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100047485.1.2",
          "form": "아파트 단지에 장이서서",
          "original_form": "아파트 단지에 장이서서",
          "speaker_id": "2",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100047485.1.3",
          "form": "거기서 사왔어요",
          "original_form": "거기서 사왔어요",
          "speaker_id": "2",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100047485.1.4",
          "form": "장에서 치킨도 팔아요?",
          "original_form": "장에서 치킨도 팔아요?",
          "speaker_id": "1",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100047485.1.5",
          "form": "우리아파트는 치킨은 없던데...",
          "original_form": "우리아파트는 치킨은 없던데...",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100047485.1.6",
          "form": "네 치킨 와플 족발 초밥",
          "original_form": "네 치킨 와플 족발 초밥",
          "speaker_id": "2",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100047485.1.7",
          "form": "핫도그 떡볶이 순대 돈까스 이런거",
          "original_form": "핫도그 떡볶이 순대 돈까스 이런거",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100047485.1.8",
          "form": "별거 다팔아요",
          "original_form": "별거 다팔아요",
          "speaker_id": "2",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100047485.1.9",
          "form": "과일 야채팔아요",
          "original_form": "과일 야채팔아요",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100047485.1.10",
          "form": "초밥두요?",
          "original_form": "초밥두요?",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100047485.1.11",
          "form": "기본으로 그런건 필죠",
          "original_form": "기본으로 그런건 필죠",
          "speaker_id": "2",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100047485.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100047485.1.13",
          "form": "와 와플에",
          "original_form": "와 와플에",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100047485.1.14",
          "form": "조그마한 미니바스에서 초밥",
          "original_form": "조그마한 미니바스에서 초밥",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100047485.1.15",
          "form": "고컬러티네요",
          "original_form": "고컬러티네요",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100047485.1.16",
          "form": "팔아요",
          "original_form": "팔아요",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100047485.1.17",
          "form": "근데 초밥은 안사 먹었어요",
          "original_form": "근데 초밥은 안사 먹었어요",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100047485.1.18",
          "form": "좋은세상 ㅎㅎ\r\n시장안온다고 집앞에 시장열어주고 ㅋ",
          "original_form": "좋은세상 ㅎㅎ\r\n시장안온다고 집앞에 시장열어주고 ㅋ",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100047485.1.19",
          "form": "순대볶음 저녁으로 잘사먹어요",
          "original_form": "순대볶음 저녁으로 잘사먹어요",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100047485.1.20",
          "form": "ㅋㅋ목요일에 맨날해요",
          "original_form": "ㅋㅋ목요일에 맨날해요",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100047485.1.21",
          "form": "이때 저녁 때우기 좋아요 ㅎㅎ",
          "original_form": "이때 저녁 때우기 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100047485.1.22",
          "form": "초밥우 좀 아다리? 잘못될까",
          "original_form": "초밥우 좀 아다리? 잘못될까",
          "speaker_id": "1",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100047485.1.23",
          "form": "걱정스러워 안사먹을듯요",
          "original_form": "걱정스러워 안사먹을듯요",
          "speaker_id": "1",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100047485.1.24",
          "form": "네 맞아요 ㅡㅡ",
          "original_form": "네 맞아요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100047485.1.25",
          "form": "사먹기 좀 그래요",
          "original_form": "사먹기 좀 그래요",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100047485.1.26",
          "form": "특히 여름에는요",
          "original_form": "특히 여름에는요",
          "speaker_id": "1",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100047485.1.27",
          "form": "뭐든 바싹 익히고 저녁 반찬이나 뭐 그런거 사먹어요 ㅋㅋ",
          "original_form": "뭐든 바싹 익히고 저녁 반찬이나 뭐 그런거 사먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100047485.1.28",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100047485.1.29",
          "form": "장은 좀 거기선 그나마 많이 조리한거 먹어요",
          "original_form": "장은 좀 거기선 그나마 많이 조리한거 먹어요",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100047485.1.30",
          "form": "오늘 저녁 잘 떼웠네요",
          "original_form": "오늘 저녁 잘 떼웠네요",
          "speaker_id": "2",
          "time": "20210826 20:15"
        }
      ]
    }
