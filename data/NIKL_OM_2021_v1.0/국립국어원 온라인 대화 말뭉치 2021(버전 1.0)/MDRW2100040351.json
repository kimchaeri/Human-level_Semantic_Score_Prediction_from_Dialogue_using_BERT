      "utterance": [
        {
          "id": "MDRW2100040351.1.1",
          "form": "저번에 언더커버 보셨다고 했나요?",
          "original_form": "저번에 언더커버 보셨다고 했나요?",
          "speaker_id": "2",
          "time": "20210904 16:53"
        },
        {
          "id": "MDRW2100040351.1.2",
          "form": "정채연 왜 연기 못하는 이미지였는지...",
          "original_form": "정채연 왜 연기 못하는 이미지였는지...",
          "speaker_id": "1",
          "time": "20210904 16:53"
        },
        {
          "id": "MDRW2100040351.1.3",
          "form": "자연스럽던데",
          "original_form": "자연스럽던데",
          "speaker_id": "1",
          "time": "20210904 16:53"
        },
        {
          "id": "MDRW2100040351.1.4",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "1",
          "time": "20210904 16:53"
        },
        {
          "id": "MDRW2100040351.1.5",
          "form": "계속 같은 역할만 해서 그런것도있는거같아요 ㅜ",
          "original_form": "계속 같은 역할만 해서 그런것도있는거같아요 ㅜ",
          "speaker_id": "2",
          "time": "20210904 16:53"
        },
        {
          "id": "MDRW2100040351.1.6",
          "form": "ㅋㅋㅋㅋ 아 지정생존자는 보셨다고했죠??",
          "original_form": "ㅋㅋㅋㅋ 아 지정생존자는 보셨다고했죠??",
          "speaker_id": "2",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100040351.1.7",
          "form": "네네 손석구 좋아요ㅋㅋㅋㅋㅋ",
          "original_form": "네네 손석구 좋아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100040351.1.8",
          "form": "종이의집이랑 언더커버",
          "original_form": "종이의집이랑 언더커버",
          "speaker_id": "1",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100040351.1.9",
          "form": "보면서 저 허준호님 계속 믿었는데...",
          "original_form": "보면서 저 허준호님 계속 믿었는데...",
          "speaker_id": "2",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100040351.1.10",
          "form": "번갈아봐야겠다!",
          "original_form": "번갈아봐야겠다!",
          "speaker_id": "1",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100040351.1.11",
          "form": "마지막에 그렇게 배신때리기 있냐구요...",
          "original_form": "마지막에 그렇게 배신때리기 있냐구요...",
          "speaker_id": "2",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100040351.1.12",
          "form": "그니꺼요 그분 다른드라마에서도 그러시던데..!!!",
          "original_form": "그니꺼요 그분 다른드라마에서도 그러시던데..!!!",
          "speaker_id": "1",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100040351.1.13",
          "form": "언더커버에서도 허준호님 나오는데 바로 지정생존자 이어서 봤거든요 ㅋㅋㅋㅋ",
          "original_form": "언더커버에서도 허준호님 나오는데 바로 지정생존자 이어서 봤거든요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100040351.1.14",
          "form": "내일그대와 보셨어요?",
          "original_form": "내일그대와 보셨어요?",
          "speaker_id": "1",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100040351.1.15",
          "form": "악역아니겠지아니겠지 했는데 ㅋㅋ",
          "original_form": "악역아니겠지아니겠지 했는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100040351.1.16",
          "form": "아 그거 뭐더라요",
          "original_form": "아 그거 뭐더라요",
          "speaker_id": "2",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100040351.1.17",
          "form": "저도 언더커버 말한거엮어요ㅋㅋㅋㅋ",
          "original_form": "저도 언더커버 말한거엮어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100040351.1.18",
          "form": "안봤어요! ㅋㅋㅋ 내일그대와!재밌나요?",
          "original_form": "안봤어요! ㅋㅋㅋ 내일그대와!재밌나요?",
          "speaker_id": "2",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100040351.1.19",
          "form": "이제훈 신민아나오는거!",
          "original_form": "이제훈 신민아나오는거!",
          "speaker_id": "1",
          "time": "20210904 16:55"
        }
