      "utterance": [
        {
          "id": "MDRW2100043960.1.1",
          "form": "복근같은거없어도",
          "original_form": "복근같은거없어도",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043960.1.2",
          "form": "땅크3분정도는 따라하겠더라구요",
          "original_form": "땅크3분정도는 따라하겠더라구요",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043960.1.3",
          "form": "오 그래요? 해보셨나요",
          "original_form": "오 그래요? 해보셨나요",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043960.1.4",
          "form": "사실 작년까지는 출퇴근걷기와 땅크한시간 등",
          "original_form": "사실 작년까지는 출퇴근걷기와 땅크한시간 등",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043960.1.5",
          "form": "열심히했답니다",
          "original_form": "열심히했답니다",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043960.1.6",
          "form": "돼지되고나니 아무것도하기싫네요",
          "original_form": "돼지되고나니 아무것도하기싫네요",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043960.1.7",
          "form": "우와 마침 딱 하신 분이 있었네요",
          "original_form": "우와 마침 딱 하신 분이 있었네요",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043960.1.8",
          "form": "그때 체중 많이 빠지던가요",
          "original_form": "그때 체중 많이 빠지던가요",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043960.1.9",
          "form": "저는 사실 올해전까진 계속운동했거든요",
          "original_form": "저는 사실 올해전까진 계속운동했거든요",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043960.1.10",
          "form": "68- 48 됐답니다",
          "original_form": "68- 48 됐답니다",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043960.1.11",
          "form": "땅크때문은아니고",
          "original_form": "땅크때문은아니고",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043960.1.12",
          "form": "골고루했어요",
          "original_form": "골고루했어요",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043960.1.13",
          "form": "헐??????",
          "original_form": "헐??????",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043960.1.14",
          "form": "엄청난 변화인데요",
          "original_form": "엄청난 변화인데요",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043960.1.15",
          "form": "아 긴기간동안 정말 많이 노력했어요",
          "original_form": "아 긴기간동안 정말 많이 노력했어요",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043960.1.16",
          "form": "고통의 순간이죠",
          "original_form": "고통의 순간이죠",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043960.1.17",
          "form": "지금은 맘은 행복하네요ㅋㅋ",
          "original_form": "지금은 맘은 행복하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043960.1.18",
          "form": "아 천천히 빼신건가요 계획적으로",
          "original_form": "아 천천히 빼신건가요 계획적으로",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043960.1.19",
          "form": "네 소아비만이라서 계속 꾸준히했어요",
          "original_form": "네 소아비만이라서 계속 꾸준히했어요",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043960.1.20",
          "form": "근데 평생다이어트라 좀 힘들더라구요",
          "original_form": "근데 평생다이어트라 좀 힘들더라구요",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043960.1.21",
          "form": "아 우와 엄청 고생했겠어요.. 저도 17키로 빼본 적 있는데 지금 요요 상태예요 ㅋㅋㅋ",
          "original_form": "아 우와 엄청 고생했겠어요.. 저도 17키로 빼본 적 있는데 지금 요요 상태예요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043960.1.22",
          "form": "항상 절제하고 노력하는삶",
          "original_form": "항상 절제하고 노력하는삶",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043960.1.23",
          "form": "와 17키로를 얼마만에 빼셨어요?",
          "original_form": "와 17키로를 얼마만에 빼셨어요?",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043960.1.24",
          "form": "저는 식도염 걸려서 빠졌어요 근데 자전거도 타긴 해서 한 1년 걸렸나봐요",
          "original_form": "저는 식도염 걸려서 빠졌어요 근데 자전거도 타긴 해서 한 1년 걸렸나봐요",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043960.1.25",
          "form": "앗 아프면 많이 빠지긴하죠",
          "original_form": "앗 아프면 많이 빠지긴하죠",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043960.1.26",
          "form": "저도 52키로가 마의구간이었는데",
          "original_form": "저도 52키로가 마의구간이었는데",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043960.1.27",
          "form": "아프니까 일주일에 오키로빠져서",
          "original_form": "아프니까 일주일에 오키로빠져서",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043960.1.28",
          "form": "그때부터 40대 됐어요",
          "original_form": "그때부터 40대 됐어요",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043960.1.29",
          "form": "와 근데 대단하세요 나이들어서 그렇게 유지하기가 힘든데",
          "original_form": "와 근데 대단하세요 나이들어서 그렇게 유지하기가 힘든데",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043960.1.30",
          "form": "맞아요 나이들수록 빼기힘들다는걸",
          "original_form": "맞아요 나이들수록 빼기힘들다는걸",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043960.1.31",
          "form": "실감하는중이에요",
          "original_form": "실감하는중이에요",
