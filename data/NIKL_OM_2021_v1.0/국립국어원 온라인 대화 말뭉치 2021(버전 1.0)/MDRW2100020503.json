        {
          "id": "MDRW2100020503.1.1",
          "form": "케이크 설빙 먹어봣어??",
          "original_form": "케이크 설빙 먹어봣어??",
          "speaker_id": "2",
          "time": "20210828 08:32"
        },
        {
          "id": "MDRW2100020503.1.2",
          "form": "노노 아니",
          "original_form": "노노 아니",
          "speaker_id": "1",
          "time": "20210828 08:32"
        },
        {
          "id": "MDRW2100020503.1.3",
          "form": "나도 그건 아직 안묵어봣어",
          "original_form": "나도 그건 아직 안묵어봣어",
          "speaker_id": "2",
          "time": "20210828 08:32"
        },
        {
          "id": "MDRW2100020503.1.4",
          "form": "궁금하다",
          "original_form": "궁금하다",
          "speaker_id": "1",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020503.1.5",
          "form": "빵이 잇고 아래에 또 빙수가잇대",
          "original_form": "빵이 잇고 아래에 또 빙수가잇대",
          "speaker_id": "2",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020503.1.6",
          "form": "그래??",
          "original_form": "그래??",
          "speaker_id": "1",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020503.1.7",
          "form": "근데 케이크가 먼맛일지..",
          "original_form": "근데 케이크가 먼맛일지..",
          "speaker_id": "2",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020503.1.8",
          "form": "그러게말야",
          "original_form": "그러게말야",
          "speaker_id": "1",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020503.1.9",
          "form": "좀 상상이 안간다",
          "original_form": "좀 상상이 안간다",
          "speaker_id": "2",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020503.1.10",
          "form": "나두..",
          "original_form": "나두..",
          "speaker_id": "1",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020503.1.11",
          "form": "무슨맛일까?",
          "original_form": "무슨맛일까?",
          "speaker_id": "2",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020503.1.12",
          "form": "후기찾아보자",
          "original_form": "후기찾아보자",
          "speaker_id": "1",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020503.1.13",
          "form": "좋아 알게써",
          "original_form": "좋아 알게써",
          "speaker_id": "2",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020503.1.14",
          "form": "오즘에도 먹는 사람잇겟지?",
          "original_form": "오즘에도 먹는 사람잇겟지?",
          "speaker_id": "1",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020503.1.15",
          "form": "그게 취향이면 잇겟지??",
          "original_form": "그게 취향이면 잇겟지??",
          "speaker_id": "2",
          "time": "20210828 08:33"
        },
        {
