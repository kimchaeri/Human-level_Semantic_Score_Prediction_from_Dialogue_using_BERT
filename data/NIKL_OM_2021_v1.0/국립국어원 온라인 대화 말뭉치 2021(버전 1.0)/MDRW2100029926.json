      "utterance": [
        {
          "id": "MDRW2100029926.1.1",
          "form": "곰표 마케팅 유명하지",
          "original_form": "곰표 마케팅 유명하지",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100029926.1.2",
          "form": "웅 아는거없어도 곰표밀가루 유명하니까",
          "original_form": "웅 아는거없어도 곰표밀가루 유명하니까",
          "speaker_id": "1",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100029926.1.3",
          "form": "곰표에서 비빔국수랑 잔치국수를 내놨길래 편의점에 사러갔더니",
          "original_form": "곰표에서 비빔국수랑 잔치국수를 내놨길래 편의점에 사러갔더니",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100029926.1.4",
          "form": "키트가 아니라 비빔국수용 소면, 잔치국수용 소면이어서 황당했어요",
          "original_form": "키트가 아니라 비빔국수용 소면, 잔치국수용 소면이어서 황당했어요",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100029926.1.5",
          "form": "근데 노약자 헷갈리게 하는 마케팅은 좀 관련법이 있어야할듯",
          "original_form": "근데 노약자 헷갈리게 하는 마케팅은 좀 관련법이 있어야할듯",
          "speaker_id": "1",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100029926.1.6",
          "form": "소면만 판거야?",
          "original_form": "소면만 판거야?",
          "speaker_id": "1",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100029926.1.7",
          "form": "응! 소면만.",
          "original_form": "응! 소면만.",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100029926.1.8",
          "form": "면에 충실하다고 해야할지; 마케팅 기왕 했으면 키트로도 해주지",
          "original_form": "면에 충실하다고 해야할지; 마케팅 기왕 했으면 키트로도 해주지",
          "speaker_id": "1",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100029926.1.9",
          "form": "관련법안 발의되어서 생긴대요",
          "original_form": "관련법안 발의되어서 생긴대요",
          "speaker_id": "2",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100029926.1.10",
          "form": "얼 다행이다 그거 진짜 까딱 실수하면 사람 죽게 생겼더라고요",
          "original_form": "얼 다행이다 그거 진짜 까딱 실수하면 사람 죽게 생겼더라고요",
          "speaker_id": "1",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100029926.1.11",
          "form": "아니 애초에 면만 파는 거면 그냥 소면이라고만 하면 됐잖아 낚였어.",
          "original_form": "아니 애초에 면만 파는 거면 그냥 소면이라고만 하면 됐잖아 낚였어.",
          "speaker_id": "2",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100029926.1.12",
          "form": "마자 허위마케팅이다.",
          "original_form": "마자 허위마케팅이다.",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029926.1.13",
          "form": "곰표 곰이 귀엽긴한데",
          "original_form": "곰표 곰이 귀엽긴한데",
          "speaker_id": "2",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029926.1.14",
          "form": "곰표패딩 이런 거 사면 못입고 다닐 거 같아",
          "original_form": "곰표패딩 이런 거 사면 못입고 다닐 거 같아",
          "speaker_id": "2",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029926.1.15",
          "form": "근데 왜 곰인지는 알 수 없는...",
          "original_form": "근데 왜 곰인지는 알 수 없는...",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029926.1.16",
          "form": "콜라보 패딩도 있어요?",
          "original_form": "콜라보 패딩도 있어요?",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029926.1.17",
          "form": "네 못봤나요 흰색에 녹색으로 크게\r\n곰\r\n표\r\n적혀있어요",
          "original_form": "네 못봤나요 흰색에 녹색으로 크게\r\n곰\r\n표\r\n적혀있어요",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100029926.1.18",
          "form": "못봤어요 쩐다 마케팅 홍보는 확실히 되겠긴한데",
          "original_form": "못봤어요 쩐다 마케팅 홍보는 확실히 되겠긴한데",
          "speaker_id": "1",
          "time": "20210903 16:42"
        }
      ]
