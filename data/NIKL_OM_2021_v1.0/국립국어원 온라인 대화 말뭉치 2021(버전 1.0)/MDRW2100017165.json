      "utterance": [
        {
          "id": "MDRW2100017165.1.1",
          "form": "저는 외항사라 별로 안심했어요",
          "original_form": "저는 외항사라 별로 안심했어요",
          "speaker_id": "2",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100017165.1.2",
          "form": "가끔 한국 사무장님 만날 때만",
          "original_form": "가끔 한국 사무장님 만날 때만",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017165.1.3",
          "form": "어느항공사에 계셨나요",
          "original_form": "어느항공사에 계셨나요",
          "speaker_id": "1",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017165.1.4",
          "form": "아아 역시 심하다! 이렇게",
          "original_form": "아아 역시 심하다! 이렇게",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017165.1.5",
          "form": "느꼈어요",
          "original_form": "느꼈어요",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017165.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017165.1.7",
          "form": "중동 항공사요",
          "original_form": "중동 항공사요",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017165.1.8",
          "form": "거기도",
          "original_form": "거기도",
          "speaker_id": "1",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017165.1.9",
          "form": "중동 항공사는 다 영어로 이야기해서",
          "original_form": "중동 항공사는 다 영어로 이야기해서",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017165.1.10",
          "form": "수직관계 느낄 일이 잘 없어요",
          "original_form": "수직관계 느낄 일이 잘 없어요",
          "speaker_id": "2",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100017165.1.11",
          "form": "아 근데 제 이야기만 하네요",
          "original_form": "아 근데 제 이야기만 하네요",
          "speaker_id": "2",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100017165.1.12",
          "form": "하긴 한국이 제일 빡세긴하죠",
          "original_form": "하긴 한국이 제일 빡세긴하죠",
          "speaker_id": "1",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100017165.1.13",
          "form": "name1님 직장 이야기 하셔도 괜찮아요!",
          "original_form": "&name1&님 직장 이야기 하셔도 괜찮아요!",
          "speaker_id": "2",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100017165.1.14",
          "form": "괜찮은데 혹시 불편하신가요",
          "original_form": "괜찮은데 혹시 불편하신가요",
          "speaker_id": "1",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100017165.1.15",
          "form": "오 아니요",
          "original_form": "오 아니요",
          "speaker_id": "2",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100017165.1.16",
          "form": "아뇨 저 듣는거 좋아해서",
          "original_form": "아뇨 저 듣는거 좋아해서",
          "speaker_id": "1",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100017165.1.17",
          "form": "그냥 제가 수다떨고",
          "original_form": "그냥 제가 수다떨고",
          "speaker_id": "2",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100017165.1.18",
          "form": "name1님만 듣는 기분이라",
          "original_form": "&name1&님만 듣는 기분이라",
          "speaker_id": "2",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100017165.1.19",
          "form": "재밌게 듣고 있었어요",
          "original_form": "재밌게 듣고 있었어요",
          "speaker_id": "1",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100017165.1.20",
          "form": "아 ㅎㅎ 그럼 됐어요",
          "original_form": "아 ㅎㅎ 그럼 됐어요",
          "speaker_id": "2",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100017165.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100017165.1.22",
          "form": "또 제가 워낙에 그 아부성 멘트를 잘 못해요 ㅎㅎ",
          "original_form": "또 제가 워낙에 그 아부성 멘트를 잘 못해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100017165.1.23",
          "form": "약간 속된말로 꼰대..사회",
          "original_form": "약간 속된말로 꼰대..사회",
          "speaker_id": "1",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100017165.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100017165.1.25",
          "form": "한국은 뭐 든 다 좀 빡센거같아요ㅠ",
          "original_form": "한국은 뭐 든 다 좀 빡센거같아요ㅠ",
          "speaker_id": "1",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100017165.1.26",
          "form": "저 예전에 한국인들이랑 비행했는데",
          "original_form": "저 예전에 한국인들이랑 비행했는데",
