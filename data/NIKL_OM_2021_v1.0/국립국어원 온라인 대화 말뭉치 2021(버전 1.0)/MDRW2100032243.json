      "utterance": [
        {
          "id": "MDRW2100032243.1.1",
          "form": "아이 책 많이 사주셔요??",
          "original_form": "아이 책 많이 사주셔요??",
          "speaker_id": "1",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100032243.1.2",
          "form": "아뇨아뇨ㅠ",
          "original_form": "아뇨아뇨ㅠ",
          "speaker_id": "2",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100032243.1.3",
          "form": "책읽는거 좋아해서 많이사주고싶은데",
          "original_form": "책읽는거 좋아해서 많이사주고싶은데",
          "speaker_id": "2",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100032243.1.4",
          "form": "중고책으로 사줘요 ㅎ",
          "original_form": "중고책으로 사줘요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100032243.1.5",
          "form": "그리고 윙크인가? 그걸로 학습용 테블릿해주고있어요",
          "original_form": "그리고 윙크인가? 그걸로 학습용 테블릿해주고있어요",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032243.1.6",
          "form": "아  동원책꾸러미 신청하세요 ㅎㅎ",
          "original_form": "아  동원책꾸러미 신청하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032243.1.7",
          "form": "그게뭐에요?ㅎ",
          "original_form": "그게뭐에요?ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032243.1.8",
          "form": "한달에 1권 책 리스트 안에서 고르면  보내줘요",
          "original_form": "한달에 1권 책 리스트 안에서 고르면  보내줘요",
          "speaker_id": "1",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032243.1.9",
          "form": "오ㅎ 찾아봐야겟음요ㅎ",
          "original_form": "오ㅎ 찾아봐야겟음요ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032243.1.10",
          "form": "매달 말에 1년동안요",
          "original_form": "매달 말에 1년동안요",
          "speaker_id": "1",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032243.1.11",
          "form": "우와ㅎㅎ",
          "original_form": "우와ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032243.1.12",
          "form": "전 이제  몇번 안남았어요",
          "original_form": "전 이제  몇번 안남았어요",
          "speaker_id": "1",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100032243.1.13",
          "form": "저는 3수만에 됏어요",
          "original_form": "저는 3수만에 됏어요",
          "speaker_id": "1",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100032243.1.14",
          "form": "매월 신청이니 ㅎㅎ",
          "original_form": "매월 신청이니 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100032243.1.15",
          "form": "아아 해봐야겟습니다ㅎ",
          "original_form": "아아 해봐야겟습니다ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100032243.1.16",
          "form": "그게 무료로보내주는거에요?",
          "original_form": "그게 무료로보내주는거에요?",
          "speaker_id": "2",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100032243.1.17",
          "form": "넨 뽑히면",
          "original_form": "넨 뽑히면",
          "speaker_id": "1",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100032243.1.18",
          "form": "월초에 책 리스트 창이 잇어툐",
          "original_form": "월초에 책 리스트 창이 잇어툐",
          "speaker_id": "1",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100032243.1.19",
          "form": "거기서 고르면  보내준답니다 ㅎㅎ",
          "original_form": "거기서 고르면  보내준답니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100032243.1.20",
          "form": "그리고  도서관 가까우면",
          "original_form": "그리고  도서관 가까우면",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100032243.1.21",
          "form": "우와우와",
          "original_form": "우와우와",
          "speaker_id": "2",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100032243.1.22",
          "form": "꼭 도서관 ㄷ가세요",
          "original_form": "꼭 도서관 ㄷ가세요",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100032243.1.23",
          "form": "아내한테도 말해놔야겟다",
          "original_form": "아내한테도 말해놔야겟다",
          "speaker_id": "2",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100032243.1.24",
          "form": "전집 종류도 다양해요",
          "original_form": "전집 종류도 다양해요",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100032243.1.25",
          "form": "저도 전집은 수학공룡이라고 딱 그거만 들였눈데  세이펜 되는거",
          "original_form": "저도 전집은 수학공룡이라고 딱 그거만 들였눈데  세이펜 되는거",
          "speaker_id": "1",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032243.1.26",
          "form": "도서관에서  전집이 잇더라구요 ㅎㅎ 음원만 다운받고\r\n도서관에서 빌려와요 ㅎㅎ",
          "original_form": "도서관에서  전집이 잇더라구요 ㅎㅎ 음원만 다운받고\r\n도서관에서 빌려와요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032243.1.27",
          "form": "단행본도 빌리고   아이가좋아하고 사달라는책은\r\n교보랑  예스로 구매하구여",
          "original_form": "단행본도 빌리고   아이가좋아하고 사달라는책은\r\n교보랑  예스로 구매하구여",
          "speaker_id": "1",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032243.1.28",
          "form": "우와ㅎㅎ",
          "original_form": "우와ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100032243.1.29",
          "form": "정말 대단하시다ㅎㅎ",
          "original_form": "정말 대단하시다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100032243.1.30",
          "form": "name1은 아이큐가 얼마에요?ㅎ",
          "original_form": "&name1&은 아이큐가 얼마에요?ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100032243.1.31",
          "form": "머리가 너무 좋으심ㅎ 완전 짱짱~^^",
          "original_form": "머리가 너무 좋으심ㅎ 완전 짱짱~^^",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100032243.1.32",
          "form": "안똑똑해요 ㅎㅎ",
          "original_form": "안똑똑해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100032243.1.33",
          "form": "아이가 엄마닮앗으면 똑똑하겟네요ㅎ",
          "original_form": "아이가 엄마닮앗으면 똑똑하겟네요ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100032243.1.34",
          "form": "아물런 중고 나오심 구매도 좋구요 ㅎㅎ",
          "original_form": "아물런 중고 나오심 구매도 좋구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100032243.1.35",
          "form": "왜그걸생각못했을까요ㅎ",
          "original_form": "왜그걸생각못했을까요ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032243.1.36",
          "form": "중고로 사면 싸고 좋을줄알았는데ㅎ",
          "original_form": "중고로 사면 싸고 좋을줄알았는데ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032243.1.37",
          "form": "아 그게",
          "original_form": "아 그게",
          "speaker_id": "1",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032243.1.38",
          "form": "중고가 좋긴한데 아이취향마다",
          "original_form": "중고가 좋긴한데 아이취향마다",
          "speaker_id": "1",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032243.1.39",
          "form": "안보는 책은 안보더라구오",
          "original_form": "안보는 책은 안보더라구오",
          "speaker_id": "1",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032243.1.40",
          "form": "그래서 도서관 체험먼저",
          "original_form": "그래서 도서관 체험먼저",
          "speaker_id": "1",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032243.1.41",
