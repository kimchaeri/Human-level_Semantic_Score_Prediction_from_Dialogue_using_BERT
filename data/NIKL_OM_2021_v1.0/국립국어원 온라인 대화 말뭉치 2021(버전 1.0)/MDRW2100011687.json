      "utterance": [
        {
          "id": "MDRW2100011687.1.1",
          "form": "우린 부자가 될꺼야",
          "original_form": "우린 부자가 될꺼야",
          "speaker_id": "1",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011687.1.2",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011687.1.3",
          "form": "넌 어떤직업 하고싶어",
          "original_form": "넌 어떤직업 하고싶어",
          "speaker_id": "2",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011687.1.4",
          "form": "난 공무원 하고 싶은데",
          "original_form": "난 공무원 하고 싶은데",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011687.1.5",
          "form": "할 수 있을지 모르겠다",
          "original_form": "할 수 있을지 모르겠다",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011687.1.6",
          "form": "검찰직 아니면 법원직...",
          "original_form": "검찰직 아니면 법원직...",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011687.1.7",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011687.1.8",
          "form": "난 게임개발 하고싶은데",
          "original_form": "난 게임개발 하고싶은데",
          "speaker_id": "2",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011687.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011687.1.10",
          "form": "개발자 너무 빡세다고해서 걱정돼",
          "original_form": "개발자 너무 빡세다고해서 걱정돼",
          "speaker_id": "2",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011687.1.11",
          "form": "열심히 바짝하고",
          "original_form": "열심히 바짝하고",
          "speaker_id": "1",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011687.1.12",
          "form": "퇴직해서",
          "original_form": "퇴직해서",
          "speaker_id": "1",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011687.1.13",
          "form": "너 회사 차려",
          "original_form": "너 회사 차려",
          "speaker_id": "1",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011687.1.14",
          "form": "요즘 바짝 벌고",
          "original_form": "요즘 바짝 벌고",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011687.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋다",
          "speaker_id": "2",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011687.1.16",
          "form": "40전에ㅜ퇴짓하는",
          "original_form": "40전에ㅜ퇴짓하는",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011687.1.17",
          "form": "퇴직",
          "original_form": "퇴직",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011687.1.18",
          "form": "하는 파이어족이 대세라더라",
          "original_form": "하는 파이어족이 대세라더라",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011687.1.19",
          "form": "우린 파이어까지 갈 필요도 없고",
          "original_form": "우린 파이어까지 갈 필요도 없고",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011687.1.20",
          "form": "파이족으로 하자",
          "original_form": "파이족으로 하자",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011687.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011687.1.22",
          "form": "35에 퇴직하는거지",
          "original_form": "35에 퇴직하는거지",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
