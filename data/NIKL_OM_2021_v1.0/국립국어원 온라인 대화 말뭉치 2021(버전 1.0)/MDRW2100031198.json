      "utterance": [
        {
          "id": "MDRW2100031198.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100031198.1.2",
          "form": "좋아하기는 하는데",
          "original_form": "좋아하기는 하는데",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031198.1.3",
          "form": "아직 차가 없어서...",
          "original_form": "아직 차가 없어서...",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031198.1.4",
          "form": "코로나때문에",
          "original_form": "코로나때문에",
          "speaker_id": "1",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031198.1.5",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031198.1.6",
          "form": "원래 작년에",
          "original_form": "원래 작년에",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031198.1.7",
          "form": "해외여행 갈 계획이었는데",
          "original_form": "해외여행 갈 계획이었는데",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031198.1.8",
          "form": "말씀하신 것처럼",
          "original_form": "말씀하신 것처럼",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031198.1.9",
          "form": "시국상..",
          "original_form": "시국상..",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031198.1.10",
          "form": "저도 가고싶은데",
          "original_form": "저도 가고싶은데",
          "speaker_id": "1",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031198.1.11",
          "form": "여행은",
          "original_form": "여행은",
          "speaker_id": "2",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031198.1.12",
          "form": "어떤 종류 좋아하시나요",
          "original_form": "어떤 종류 좋아하시나요",
          "speaker_id": "2",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031198.1.13",
          "form": "패키지여",
          "original_form": "패키지여",
          "speaker_id": "1",
          "time": "20210904 18:38"
        },
        {
          "id": "MDRW2100031198.1.14",
          "form": "아 패키지 좋죠",
          "original_form": "아 패키지 좋죠",
          "speaker_id": "2",
          "time": "20210904 19:50"
        },
        {
          "id": "MDRW2100031198.1.15",
          "form": "솔직히 놀라가는건데",
          "original_form": "솔직히 놀라가는건데",
          "speaker_id": "2",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031198.1.16",
          "form": "놀러",
          "original_form": "놀러",
          "speaker_id": "2",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031198.1.17",
          "form": "머리 아픈 경우가 왕왕 생기는데",
          "original_form": "머리 아픈 경우가 왕왕 생기는데",
          "speaker_id": "2",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031198.1.18",
          "form": "패키지는 덜하기도 하고요",
          "original_form": "패키지는 덜하기도 하고요",
          "speaker_id": "2",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031198.1.19",
          "form": "편하죠",
          "original_form": "편하죠",
          "speaker_id": "1",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031198.1.20",
          "form": "솔직히 어설프게",
          "original_form": "솔직히 어설프게",
          "speaker_id": "2",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100031198.1.21",
          "form": "직접 이것저것 알아봐서 돈 아끼겠다고 하다가",
          "original_form": "직접 이것저것 알아봐서 돈 아끼겠다고 하다가",
          "speaker_id": "2",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100031198.1.22",
          "form": "더 깨지는 경우도 있고요",
          "original_form": "더 깨지는 경우도 있고요",
          "speaker_id": "2",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100031198.1.23",
          "form": "그렇져",
          "original_form": "그렇져",
          "speaker_id": "1",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031198.1.24",
          "form": "라지만",
          "original_form": "라지만",
          "speaker_id": "2",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031198.1.25",
          "form": "말씀하신 것처럼",
          "original_form": "말씀하신 것처럼",
          "speaker_id": "2",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031198.1.26",
          "form": "여행 자체를 가기 힘든 시기라",
          "original_form": "여행 자체를 가기 힘든 시기라",
          "speaker_id": "2",
          "time": "20210904 21:17"
