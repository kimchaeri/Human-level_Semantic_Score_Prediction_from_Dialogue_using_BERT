      "utterance": [
        {
          "id": "MDRW2100011673.1.1",
          "form": "요즘 유튜브에서는",
          "original_form": "요즘 유튜브에서는",
          "speaker_id": "1",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011673.1.2",
          "form": "어떤거봐??",
          "original_form": "어떤거봐??",
          "speaker_id": "1",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011673.1.3",
          "form": "일단 보더로운 생활이랑",
          "original_form": "일단 보더로운 생활이랑",
          "speaker_id": "2",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011673.1.4",
          "form": "약간 무서운 이야기들 종종 보고",
          "original_form": "약간 무서운 이야기들 종종 보고",
          "speaker_id": "2",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011673.1.5",
          "form": "오 무서운이야기..",
          "original_form": "오 무서운이야기..",
          "speaker_id": "1",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011673.1.6",
          "form": "난 자빱이랑 쓰레기걸 자주봐",
          "original_form": "난 자빱이랑 쓰레기걸 자주봐",
          "speaker_id": "1",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011673.1.7",
          "form": "개재밋음",
          "original_form": "개재밋음",
          "speaker_id": "1",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011673.1.8",
          "form": "쓰레기걸은 무슨 내용이야",
          "original_form": "쓰레기걸은 무슨 내용이야",
          "speaker_id": "2",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011673.1.9",
          "form": "그 이상한카드로 청혼하는영상",
          "original_form": "그 이상한카드로 청혼하는영상",
          "speaker_id": "1",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011673.1.10",
          "form": "그거 만든 채널임",
          "original_form": "그거 만든 채널임",
          "speaker_id": "1",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011673.1.11",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011673.1.12",
          "form": "기억안나?",
          "original_form": "기억안나?",
          "speaker_id": "1",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011673.1.13",
          "form": "그때 봤던거",
          "original_form": "그때 봤던거",
          "speaker_id": "2",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011673.1.14",
          "form": "기억난닼ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "기억난닼ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011673.1.15",
          "form": "키갈",
          "original_form": "키갈",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011673.1.16",
          "form": "절대로 키...스 참을슈없어",
          "original_form": "절대로 키...스 참을슈없어",
          "speaker_id": "1",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011673.1.17",
          "form": "이거한 사람들ㅋㅋㅋㅋ",
          "original_form": "이거한 사람들ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011673.1.18",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011673.1.19",
          "form": "참을 수 없지",
          "original_form": "참을 수 없지",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011673.1.20",
          "form": "하지만 난 참을 수 있어",
          "original_form": "하지만 난 참을 수 있어",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011673.1.21",
          "form": "가끔씩 유트브 타로 보는데",
          "original_form": "가끔씩 유트브 타로 보는데",
          "speaker_id": "2",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011673.1.22",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011673.1.23",
          "form": "재밌음",
          "original_form": "재밌음",
          "speaker_id": "2",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011673.1.24",
          "form": "아직보는구나ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아직보는구나ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011673.1.25",
          "form": "오오오오오오 이러다가",
          "original_form": "오오오오오오 이러다가",
          "speaker_id": "2",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011673.1.26",
          "form": "어때 잘맞는거같아?",
          "original_form": "어때 잘맞는거같아?",
          "speaker_id": "1",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011673.1.27",
          "form": "응 이런거 안믿어~",
          "original_form": "응 이런거 안믿어~",
          "speaker_id": "2",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011673.1.28",
          "form": "하면서 끄는게 국룰이지",
          "original_form": "하면서 끄는게 국룰이지",
          "speaker_id": "2",
          "time": "20210824 15:07"
        }
      ]
    }
