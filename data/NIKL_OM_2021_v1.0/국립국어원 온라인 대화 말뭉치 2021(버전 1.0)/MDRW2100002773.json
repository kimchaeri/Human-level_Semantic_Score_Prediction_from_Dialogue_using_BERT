      "utterance": [
        {
          "id": "MDRW2100002773.1.1",
          "form": "올 여름에 즐겨먹은 음식이 있나요?",
          "original_form": "올 여름에 즐겨먹은 음식이 있나요?",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002773.1.2",
          "form": "아이스크림이요 name1님은요?",
          "original_form": "아이스크림이요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002773.1.3",
          "form": "저는 냉면을 즐겨먹었던 거 같아요",
          "original_form": "저는 냉면을 즐겨먹었던 거 같아요",
          "speaker_id": "1",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002773.1.4",
          "form": "아이스크림 어떤거 즐겨드셨나요?",
          "original_form": "아이스크림 어떤거 즐겨드셨나요?",
          "speaker_id": "1",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002773.1.5",
          "form": "아~냉면 좋아하시나봐요",
          "original_form": "아~냉면 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002773.1.6",
          "form": "네 올여름 유난히 즐겨먹었어요",
          "original_form": "네 올여름 유난히 즐겨먹었어요",
          "speaker_id": "1",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002773.1.7",
          "form": "혹시 냉면 좋아하시나요?",
          "original_form": "혹시 냉면 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002773.1.8",
          "form": "시원하고 새콤하고 맛있죠 ㅎ",
          "original_form": "시원하고 새콤하고 맛있죠 ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002773.1.9",
          "form": "비빔냉면과 물냉면 어느걸 더 선호하시나요?",
          "original_form": "비빔냉면과 물냉면 어느걸 더 선호하시나요?",
          "speaker_id": "1",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002773.1.10",
          "form": "저는 비냉요 ㅎㅎ",
          "original_form": "저는 비냉요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002773.1.11",
          "form": "정말요? 저도 그래요!",
          "original_form": "정말요? 저도 그래요!",
          "speaker_id": "1",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002773.1.12",
          "form": "혹시 냉면과 같이 먹는 음식이 있으신가요?",
          "original_form": "혹시 냉면과 같이 먹는 음식이 있으신가요?",
          "speaker_id": "1",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002773.1.13",
          "form": "만두나 고기같은거요",
          "original_form": "만두나 고기같은거요",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002773.1.14",
          "form": "네~고기랑 같이 먹는걸 젤루 좋아해요",
          "original_form": "네~고기랑 같이 먹는걸 젤루 좋아해요",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002773.1.15",
          "form": "그리고 비냉은 돈까스랑 먹어도 맛있어요 ㅎ",
          "original_form": "그리고 비냉은 돈까스랑 먹어도 맛있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002773.1.16",
          "form": "돈까스 조합도 너무 좋아요",
          "original_form": "돈까스 조합도 너무 좋아요",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002773.1.17",
          "form": "뭐랄까 좀 더 고소하다 해야할까 그 맛이 좋아요",
          "original_form": "뭐랄까 좀 더 고소하다 해야할까 그 맛이 좋아요",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002773.1.18",
          "form": "네~~ㅎㅎ 역시 아시는군요!",
          "original_form": "네~~ㅎㅎ 역시 아시는군요!",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002773.1.19",
          "form": "ㅎㅎ 냉면먹는 사람이라면 놓칠 수 없는 조합이니까요",
          "original_form": "ㅎㅎ 냉면먹는 사람이라면 놓칠 수 없는 조합이니까요",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002773.1.20",
          "form": "맛잘알이시이네요",
          "original_form": "맛잘알이시이네요",
          "speaker_id": "1",
          "time": "20210820 15:37"
        }
      ]
