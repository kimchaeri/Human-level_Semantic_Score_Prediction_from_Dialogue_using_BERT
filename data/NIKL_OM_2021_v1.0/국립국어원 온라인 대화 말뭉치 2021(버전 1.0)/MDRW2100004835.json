        },
        {
          "id": "MDRW2100004835.1.2",
          "form": "군대는 잘 몰라요...",
          "original_form": "군대는 잘 몰라요...",
          "speaker_id": "2",
          "time": "20210821 18:48"
        },
        {
          "id": "MDRW2100004835.1.3",
