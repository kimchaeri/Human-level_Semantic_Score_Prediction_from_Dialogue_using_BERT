        {
          "id": "MDRW2100042115.1.1",
          "form": "양식돈까스는 왕돈까스류 말하시는거죠..?",
          "original_form": "양식돈까스는 왕돈까스류 말하시는거죠..?",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042115.1.2",
          "form": "네 소스 부어서 주는거요!",
          "original_form": "네 소스 부어서 주는거요!",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042115.1.3",
          "form": "저는 철판치즈 돈까스 이런것도 좋아해요..",
          "original_form": "저는 철판치즈 돈까스 이런것도 좋아해요..",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042115.1.4",
          "form": "아 철판치즈돈까스 맛있겠네요!",
          "original_form": "아 철판치즈돈까스 맛있겠네요!",
          "speaker_id": "1",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042115.1.5",
          "form": "돈까스랑 치즈 너무 잘어울려요..",
          "original_form": "돈까스랑 치즈 너무 잘어울려요..",
          "speaker_id": "2",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042115.1.6",
          "form": "저는 고구마돈까스도 좋아해요",
          "original_form": "저는 고구마돈까스도 좋아해요",
          "speaker_id": "1",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042115.1.7",
          "form": "고구마 돈까스 달콤한건가요?",
          "original_form": "고구마 돈까스 달콤한건가요?",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042115.1.8",
          "form": "고구마 무스 올라간거요!!",
          "original_form": "고구마 무스 올라간거요!!",
          "speaker_id": "1",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042115.1.9",
          "form": "음 맛있겠어요 이러니 저러니해도 돈까스는 다 맛있는..",
          "original_form": "음 맛있겠어요 이러니 저러니해도 돈까스는 다 맛있는..",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042115.1.10",
          "form": "네네 밥도둑이에욬ㅋㅋ",
          "original_form": "네네 밥도둑이에욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042115.1.11",
          "form": "돈까스랑 같이먹는 장국도 맛있어요",
          "original_form": "돈까스랑 같이먹는 장국도 맛있어요",
          "speaker_id": "2",
