        {
          "id": "MDRW2100022043.1.1",
          "form": "나 이번에 대학원 진학해",
          "original_form": "나 이번에 대학원 진학해",
          "speaker_id": "2",
          "time": "20210829 01:18"
        },
        {
          "id": "MDRW2100022043.1.2",
          "form": "어느 학과 이니",
          "original_form": "어느 학과 이니",
          "speaker_id": "1",
          "time": "20210829 01:18"
        },
        {
          "id": "MDRW2100022043.1.3",
          "form": "내가 컴퓨터 좋아하는 거 알지",
          "original_form": "내가 컴퓨터 좋아하는 거 알지",
          "speaker_id": "2",
          "time": "20210829 01:18"
        },
        {
          "id": "MDRW2100022043.1.4",
          "form": "당연히 알지",
          "original_form": "당연히 알지",
          "speaker_id": "1",
          "time": "20210829 01:18"
        },
        {
          "id": "MDRW2100022043.1.5",
          "form": "에 이 아이 학과를 가고 싶어",
          "original_form": "에 이 아이 학과를 가고 싶어",
          "speaker_id": "2",
          "time": "20210829 01:18"
        },
        {
          "id": "MDRW2100022043.1.6",
          "form": "인공지능이 인기라고는 들었어",
          "original_form": "인공지능이 인기라고는 들었어",
          "speaker_id": "1",
          "time": "20210829 01:18"
        },
        {
          "id": "MDRW2100022043.1.7",
          "form": "맞아 우리 학교 인공지능 도 굉장히 유명해",
          "original_form": "맞아 우리 학교 인공지능 도 굉장히 유명해",
          "speaker_id": "2",
          "time": "20210829 01:19"
        },
        {
          "id": "MDRW2100022043.1.8",
          "form": "그럼 경쟁률이 높겠는데",
          "original_form": "그럼 경쟁률이 높겠는데",
          "speaker_id": "1",
          "time": "20210829 01:19"
        },
        {
          "id": "MDRW2100022043.1.9",
          "form": "내가 학점이 높지 않아서 고민이야",
          "original_form": "내가 학점이 높지 않아서 고민이야",
          "speaker_id": "2",
          "time": "20210829 01:19"
        },
        {
          "id": "MDRW2100022043.1.10",
          "form": "인공지능 학과가 워낙 인기라서 학점이 높아야 된다던데",
          "original_form": "인공지능 학과가 워낙 인기라서 학점이 높아야 된다던데",
          "speaker_id": "1",
          "time": "20210829 01:19"
        },
        {
          "id": "MDRW2100022043.1.11",
          "form": "그래서 최대한 열심히 준비하고 있어",
          "original_form": "그래서 최대한 열심히 준비하고 있어",
          "speaker_id": "2",
          "time": "20210829 01:19"
        },
        {
          "id": "MDRW2100022043.1.12",
          "form": "인공지능 학과에 가고 싶게 된 이유가 궁금해",
          "original_form": "인공지능 학과에 가고 싶게 된 이유가 궁금해",
          "speaker_id": "1",
          "time": "20210829 01:19"
        },
        {
          "id": "MDRW2100022043.1.13",
          "form": "미래를 예측하는 게 재밌어",
          "original_form": "미래를 예측하는 게 재밌어",
