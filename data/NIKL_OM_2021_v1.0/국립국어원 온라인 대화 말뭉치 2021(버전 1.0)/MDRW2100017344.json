      "utterance": [
        {
          "id": "MDRW2100017344.1.1",
          "form": "운동하다보면 건강해지겠죠",
          "original_form": "운동하다보면 건강해지겠죠",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017344.1.2",
          "form": "홈트 가장 힘든건 뭐에요",
          "original_form": "홈트 가장 힘든건 뭐에요",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017344.1.3",
          "form": "딱히 없는 거 같아요 ㅎㅎ",
          "original_form": "딱히 없는 거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017344.1.4",
          "form": "플랭크 힘들다고하던데",
          "original_form": "플랭크 힘들다고하던데",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017344.1.5",
          "form": "코어좋으신가보다",
          "original_form": "코어좋으신가보다",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017344.1.6",
          "form": "딱히 없는거보면",
          "original_form": "딱히 없는거보면",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017344.1.7",
          "form": "아아 제일 힘든 동작이요?",
          "original_form": "아아 제일 힘든 동작이요?",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017344.1.8",
          "form": "다 잘하시나봐요",
          "original_form": "다 잘하시나봐요",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017344.1.9",
          "form": "아아 자세가 아니라",
          "original_form": "아아 자세가 아니라",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017344.1.10",
          "form": "네 힘든 동작이요",
          "original_form": "네 힘든 동작이요",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017344.1.11",
          "form": "그냥 홈트 자체가 저랑 맞는다는 의미였어요",
          "original_form": "그냥 홈트 자체가 저랑 맞는다는 의미였어요",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017344.1.12",
          "form": "아 헷갈리게 말을했나보다",
          "original_form": "아 헷갈리게 말을했나보다",
          "speaker_id": "1",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017344.1.13",
          "form": "푸쉬업이 제일 어렵죠... ㅎㅎ",
          "original_form": "푸쉬업이 제일 어렵죠... ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017344.1.14",
          "form": "풀업이랑!",
          "original_form": "풀업이랑!",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017344.1.15",
          "form": "아무래도 힘이 많이 들어가서 그런가",
          "original_form": "아무래도 힘이 많이 들어가서 그런가",
          "speaker_id": "1",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017344.1.16",
          "form": "등근육 키우는 게 제일 힘든 것 같아요",
          "original_form": "등근육 키우는 게 제일 힘든 것 같아요",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017344.1.17",
          "form": "원래 푸쉬업이랑 풀업이",
          "original_form": "원래 푸쉬업이랑 풀업이",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017344.1.18",
          "form": "남녀차이가 제일 많이 난다고 하더라구요",
          "original_form": "남녀차이가 제일 많이 난다고 하더라구요",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017344.1.19",
          "form": "그쵸 등쪽은 잘 앋붙으니까",
          "original_form": "그쵸 등쪽은 잘 앋붙으니까",
          "speaker_id": "1",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017344.1.20",
          "form": "그래서 그런가 진짜 안늘어요",
          "original_form": "그래서 그런가 진짜 안늘어요",
          "speaker_id": "2",
          "time": "20210826 18:50"
        }
      ]
