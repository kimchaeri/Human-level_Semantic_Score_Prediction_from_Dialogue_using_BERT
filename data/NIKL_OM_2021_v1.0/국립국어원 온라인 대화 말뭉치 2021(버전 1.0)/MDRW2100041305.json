      "utterance": [
        {
          "id": "MDRW2100041305.1.1",
          "form": "음식만들주는아세용??",
          "original_form": "음식만들주는아세용??",
          "speaker_id": "2",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100041305.1.2",
          "form": "네네! 근데 다양하진 않아요ㅠ",
          "original_form": "네네! 근데 다양하진 않아요ㅠ",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100041305.1.3",
          "form": "name2님은요?!",
          "original_form": "&name2&님은요?!",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100041305.1.4",
          "form": "저도 간단한것들만??ㅋㅋ",
          "original_form": "저도 간단한것들만??ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100041305.1.5",
          "form": "저는 라면을 잘 끓여요! ㅋㅋㅋㅋ",
          "original_form": "저는 라면을 잘 끓여요! ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100041305.1.6",
          "form": "오 저도 라면은 가능!!",
          "original_form": "오 저도 라면은 가능!!",
          "speaker_id": "2",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100041305.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100041305.1.8",
          "form": "그거랑 제첩 넣은 콩나물국을 끓일 줄 알아요!!!",
          "original_form": "그거랑 제첩 넣은 콩나물국을 끓일 줄 알아요!!!",
          "speaker_id": "1",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100041305.1.9",
          "form": "헐랜",
          "original_form": "헐랜",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041305.1.10",
          "form": "저보다 두수위근요",
          "original_form": "저보다 두수위근요",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041305.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ군요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ군요!",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041305.1.12",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041305.1.13",
          "form": "저건 엄마가",
          "original_form": "저건 엄마가",
          "speaker_id": "1",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041305.1.14",
          "form": "투병중이셨을 때 만들어본거예요!!!",
          "original_form": "투병중이셨을 때 만들어본거예요!!!",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100041305.1.15",
          "form": "헐랭 멋져요 ㅠㅠ",
          "original_form": "헐랭 멋져요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100041305.1.16",
          "form": "아 그리고 저",
          "original_form": "아 그리고 저",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100041305.1.17",
          "form": "애호박전 만들 줄 알아요!!!",
          "original_form": "애호박전 만들 줄 알아요!!!",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100041305.1.18",
          "form": "헐랭",
          "original_form": "헐랭",
          "speaker_id": "2",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100041305.1.19",
          "form": "머야 잘하시네요!!",
          "original_form": "머야 잘하시네요!!",
          "speaker_id": "2",
          "time": "20210905 22:16"
        },
        {
