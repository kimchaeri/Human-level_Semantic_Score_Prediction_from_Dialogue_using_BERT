      "utterance": [
        {
          "id": "MDRW2100031082.1.1",
          "form": "점점 혓바닥이 강해지고 있구나",
          "original_form": "점점 혓바닥이 강해지고 있구나",
          "speaker_id": "2",
          "time": "20210904 16:29"
        },
        {
          "id": "MDRW2100031082.1.2",
          "form": "가을되면 장어덮밥도",
          "original_form": "가을되면 장어덮밥도",
          "speaker_id": "1",
          "time": "20210904 16:29"
        },
        {
          "id": "MDRW2100031082.1.3",
          "form": "맛있겠어",
          "original_form": "맛있겠어",
          "speaker_id": "1",
          "time": "20210904 16:29"
        },
        {
          "id": "MDRW2100031082.1.4",
          "form": "헐 장어 너무 맛있지",
          "original_form": "헐 장어 너무 맛있지",
          "speaker_id": "2",
          "time": "20210904 16:29"
        },
        {
          "id": "MDRW2100031082.1.5",
          "form": "양념 바른 장어구이 좋아하니",
          "original_form": "양념 바른 장어구이 좋아하니",
          "speaker_id": "2",
          "time": "20210904 16:29"
        },
        {
          "id": "MDRW2100031082.1.6",
          "form": "응 근데 가시를 용납 못해",
          "original_form": "응 근데 가시를 용납 못해",
          "speaker_id": "1",
          "time": "20210904 16:30"
        },
        {
          "id": "MDRW2100031082.1.7",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:30"
        },
        {
          "id": "MDRW2100031082.1.8",
          "form": "포도다음으로 성가셔",
          "original_form": "포도다음으로 성가셔",
          "speaker_id": "1",
          "time": "20210904 16:30"
        },
        {
          "id": "MDRW2100031082.1.9",
          "form": "맞아 가시 너무 많아",
          "original_form": "맞아 가시 너무 많아",
          "speaker_id": "2",
          "time": "20210904 16:30"
        },
        {
          "id": "MDRW2100031082.1.10",
          "form": "난 귀찮아서 그냥 씹어먹어",
          "original_form": "난 귀찮아서 그냥 씹어먹어",
          "speaker_id": "2",
          "time": "20210904 16:30"
        },
        {
          "id": "MDRW2100031082.1.11",
          "form": "넌 다 없애고 먹니",
          "original_form": "넌 다 없애고 먹니",
          "speaker_id": "2",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100031082.1.12",
          "form": "가능만하다며ㆍ",
          "original_form": "가능만하다며ㆍ",
          "speaker_id": "1",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100031082.1.13",
          "form": "그러고싶어",
          "original_form": "그러고싶어",
          "speaker_id": "1",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100031082.1.14",
          "form": "ㅋㅋㅋㅋ근데 발라내려면 살이 다 으깨져서 마음이 아파",
          "original_form": "ㅋㅋㅋㅋ근데 발라내려면 살이 다 으깨져서 마음이 아파",
          "speaker_id": "2",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100031082.1.15",
          "form": "나도 시도는 해 봤는데 결과가 좋지 못했어",
          "original_form": "나도 시도는 해 봤는데 결과가 좋지 못했어",
          "speaker_id": "2",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100031082.1.16",
          "form": "나 지금 과일 먹는데 뭐 먹나 맞춰볼래?",
          "original_form": "나 지금 과일 먹는데 뭐 먹나 맞춰볼래?",
          "speaker_id": "2",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100031082.1.17",
          "form": "시도를 해봤다는 점에서",
          "original_form": "시도를 해봤다는 점에서",
          "speaker_id": "1",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100031082.1.18",
          "form": "박수가 나온다",
          "original_form": "박수가 나온다",
          "speaker_id": "1",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100031082.1.19",
          "form": "ㅋㅋㅋㅋ고맙다야",
          "original_form": "ㅋㅋㅋㅋ고맙다야",
          "speaker_id": "2",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100031082.1.20",
          "form": "바나나먹냐",
          "original_form": "바나나먹냐",
          "speaker_id": "1",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100031082.1.21",
          "form": "땡 바나나 아님",
          "original_form": "땡 바나나 아님",
          "speaker_id": "2",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100031082.1.22",
          "form": "또 토마토냐",
          "original_form": "또 토마토냐",
          "speaker_id": "1",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100031082.1.23",
          "form": "또마또네",
          "original_form": "또마또네",
          "speaker_id": "1",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100031082.1.24",
          "form": "토마토 아냐ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "토마토 아냐ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:33"
