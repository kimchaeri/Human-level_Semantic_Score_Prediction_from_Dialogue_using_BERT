      "utterance": [
        {
          "id": "MDRW2100043873.1.1",
          "form": "머리깍기도 동의했습니다",
          "original_form": "머리깍기도 동의했습니다",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043873.1.2",
          "form": "몇년됐는데  다행히",
          "original_form": "몇년됐는데  다행히",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043873.1.3",
          "form": "아 박사가면 빡빡이인가요",
          "original_form": "아 박사가면 빡빡이인가요",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043873.1.4",
          "form": "ㅋㅋㅋㅋㅋ머리깎기",
          "original_form": "ㅋㅋㅋㅋㅋ머리깎기",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043873.1.5",
          "form": "그친구도 저도 경제활동에 열심히입니다",
          "original_form": "그친구도 저도 경제활동에 열심히입니다",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043873.1.6",
          "form": "멋진 우정이네요",
          "original_form": "멋진 우정이네요",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043873.1.7",
          "form": "빡빡이가 되면",
          "original_form": "빡빡이가 되면",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043873.1.8",
          "form": "원서를 내러가지 못할테니까요..",
          "original_form": "원서를 내러가지 못할테니까요..",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043873.1.9",
          "form": "저희는 취업이 쉽지 않은 전공이라 그런가 다들 대학 근처에서 부랑부랑 하고있네요",
          "original_form": "저희는 취업이 쉽지 않은 전공이라 그런가 다들 대학 근처에서 부랑부랑 하고있네요",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043873.1.10",
          "form": "아니 빡빡이가 부끄러우신가요(?)",
          "original_form": "아니 빡빡이가 부끄러우신가요(?)",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043873.1.11",
          "form": "그때는 그럴줄알았는데요",
          "original_form": "그때는 그럴줄알았는데요",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043873.1.12",
          "form": "지금 생각해보면 빡빡이는",
          "original_form": "지금 생각해보면 빡빡이는",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043873.1.13",
          "form": "별거아닌거같아요",
          "original_form": "별거아닌거같아요",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043873.1.14",
          "form": "인터넷을 끊어놔야...",
          "original_form": "인터넷을 끊어놔야...",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043873.1.15",
          "form": "그쵸 어차피 군대갔다 휴가나오는 애들도 많고",
          "original_form": "그쵸 어차피 군대갔다 휴가나오는 애들도 많고",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043873.1.16",
          "form": "아 그게 아니군요",
          "original_form": "아 그게 아니군요",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043873.1.17",
          "form": "원격접수였던..",
          "original_form": "원격접수였던..",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043873.1.18",
          "form": "ㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043873.1.19",
          "form": "라떼는 가서 접수도 있었지만 지금은 아니니까요..",
          "original_form": "라떼는 가서 접수도 있었지만 지금은 아니니까요..",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043873.1.20",
          "form": "그럼 취업이 힘드시면 다들 학업을 이어가시나요",
          "original_form": "그럼 취업이 힘드시면 다들 학업을 이어가시나요",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043873.1.21",
          "form": "네 학업을 이어가는 자들도 있고 다른분야 취업하기도 하고",
          "original_form": "네 학업을 이어가는 자들도 있고 다른분야 취업하기도 하고",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043873.1.22",
          "form": "결혼하고 포기하기도 하고 그렇네요",
          "original_form": "결혼하고 포기하기도 하고 그렇네요",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043873.1.23",
          "form": "ㅎㅎ저희두 어느정도는 비슷해요",
          "original_form": "ㅎㅎ저희두 어느정도는 비슷해요",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043873.1.24",
          "form": "전공살리는 일이 많지는 않죠",
          "original_form": "전공살리는 일이 많지는 않죠",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043873.1.25",
          "form": "맞아요 ㅜ 대학교도 티오가 줄고 해서",
          "original_form": "맞아요 ㅜ 대학교도 티오가 줄고 해서",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043873.1.26",
          "form": "뭐 베이비붐세대 교수님들 은퇴하시면 자리난다는데",
          "original_form": "뭐 베이비붐세대 교수님들 은퇴하시면 자리난다는데",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043873.1.27",
          "form": "언제쯤ㅋㅋㅋ",
          "original_form": "언제쯤ㅋㅋㅋ",
