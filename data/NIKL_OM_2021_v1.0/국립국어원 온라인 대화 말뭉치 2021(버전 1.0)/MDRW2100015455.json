      "utterance": [
        {
          "id": "MDRW2100015455.1.1",
          "form": "비혼이신가요?",
          "original_form": "비혼이신가요?",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015455.1.2",
          "form": "네 name1 님은요?",
          "original_form": "네 &name1& 님은요?",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015455.1.3",
          "form": "저도 그래용",
          "original_form": "저도 그래용",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015455.1.4",
          "form": "결혼은 괜찮으신지?",
          "original_form": "결혼은 괜찮으신지?",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015455.1.5",
          "form": "오.. 비혼으로 다짐하시게된 이유가 있으신가요?",
          "original_form": "오.. 비혼으로 다짐하시게된 이유가 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015455.1.6",
          "form": "전 언니가 비혼하라고 얘기해서요",
          "original_form": "전 언니가 비혼하라고 얘기해서요",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015455.1.7",
          "form": "이런 언니가 참.. 그렇군요.",
          "original_form": "이런 언니가 참.. 그렇군요.",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015455.1.8",
          "form": "정말요?",
          "original_form": "정말요?",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015455.1.9",
          "form": "저는 남을 쉽게 못믿겠더라고요.",
          "original_form": "저는 남을 쉽게 못믿겠더라고요.",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015455.1.10",
          "form": "저도요!!!",
          "original_form": "저도요!!!",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015455.1.11",
          "form": "저는 어릴때부터 불신의 사건들을 몇가지 겪고나니\r\n아예 모르는 남을 쉽게 믿지못하게되엇어요",
          "original_form": "저는 어릴때부터 불신의 사건들을 몇가지 겪고나니\r\n아예 모르는 남을 쉽게 믿지못하게되엇어요",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015455.1.12",
          "form": "저도 그랬죠",
          "original_form": "저도 그랬죠",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015455.1.13",
          "form": "그래서 가족.\r\n그중에 동생은 믿자 가 제 의견입니다",
          "original_form": "그래서 가족.\r\n그중에 동생은 믿자 가 제 의견입니다",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015455.1.14",
          "form": "동생 참 대단한 분인가보네요",
          "original_form": "동생 참 대단한 분인가보네요",
          "speaker_id": "1",
