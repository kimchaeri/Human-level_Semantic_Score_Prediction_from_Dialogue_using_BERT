      "utterance": [
        {
          "id": "MDRW2100044094.1.1",
          "form": "저녁드셨나요?:)",
          "original_form": "저녁드셨나요?:)",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044094.1.2",
          "form": "저 오늘 곱창이 너무 땡겼는데\r\n남편이 늦어서 간단히 먹었어요ㅠ",
          "original_form": "저 오늘 곱창이 너무 땡겼는데\r\n남편이 늦어서 간단히 먹었어요ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044094.1.3",
          "form": "name2님은 식사하셨어여?",
          "original_form": "&name2&님은 식사하셨어여?",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044094.1.4",
          "form": "우와 곱창~~~~ 전 닭가슴살 먹었어유 ㅎㅎ",
          "original_form": "우와 곱창~~~~ 전 닭가슴살 먹었어유 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044094.1.5",
          "form": "오늘 백신 2차 맞아서 조금만 먹엇어요 ㅎㅎ",
          "original_form": "오늘 백신 2차 맞아서 조금만 먹엇어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044094.1.6",
          "form": "접종날은 \r\n무조건 고기 아니에염?",
          "original_form": "접종날은 \r\n무조건 고기 아니에염?",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044094.1.7",
          "form": "ㅎㅎ근데 오늘 같이 고기 먹을 사람도 없고 해서 패스했어요",
          "original_form": "ㅎㅎ근데 오늘 같이 고기 먹을 사람도 없고 해서 패스했어요",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044094.1.8",
          "form": "내일 먹어줘야할까봐요!",
          "original_form": "내일 먹어줘야할까봐요!",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044094.1.9",
          "form": "남자친구분 바쁘셨나요?",
          "original_form": "남자친구분 바쁘셨나요?",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044094.1.10",
          "form": "내일이라도 고기 꼭 드세영",
          "original_form": "내일이라도 고기 꼭 드세영{emoji:♡}",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044094.1.11",
          "form": "남자친구는 오늘 야간근무라 같이 저녁못먹엇어요 ㅎㅎ",
          "original_form": "남자친구는 오늘 야간근무라 같이 저녁못먹엇어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044094.1.12",
          "form": "내일 꼭 먹어야겠어요!!!!",
          "original_form": "내일 꼭 먹어야겠어요!!!!",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044094.1.13",
          "form": "남자친구는 밥을 5공기를 먹더라구요",
          "original_form": "남자친구는 밥을 5공기를 먹더라구요",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044094.1.14",
          "form": "와 대식가네요 ",
          "original_form": "와 대식가네요 ",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044094.1.15",
          "form": "5공기라니",
          "original_form": "5공기라니",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044094.1.16",
          "form": "정말 대식가에요~!!",
          "original_form": "정말 대식가에요~!!",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044094.1.17",
          "form": "근데 지금 커플통장으로 내고잇는데 사실 남친이 훨씬 더 많이 먹긴 하는데",
          "original_form": "근데 지금 커플통장으로 내고잇는데 사실 남친이 훨씬 더 많이 먹긴 하는데",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044094.1.18",
          "form": "금액이 아깝지 않아요 ㅎ",
          "original_form": "금액이 아깝지 않아요 ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044094.1.19",
          "form": "ㅋㅋ찐사랑이군용ㅋㅋ",
          "original_form": "ㅋㅋ찐사랑이군용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044094.1.20",
          "form": "데이트할땐\r\n주로 뭐 먹어요?",
          "original_form": "데이트할땐\r\n주로 뭐 먹어요?",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044094.1.21",
          "form": "예전엔 1일 1치킨 먹을 정도로 치킨 많이 먹었구요",
          "original_form": "예전엔 1일 1치킨 먹을 정도로 치킨 많이 먹었구요",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044094.1.22",
          "form": "최근에는 밀면이랑 곱도리탕? 그리고 뼈해장국 먹었어요 ㅎㅎ",
          "original_form": "최근에는 밀면이랑 곱도리탕? 그리고 뼈해장국 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044094.1.23",
          "form": "오? 곱도리탕은 뭔가영?",
          "original_form": "오? 곱도리탕은 뭔가영?",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044094.1.24",
          "form": "닭도리탕이랑 곱창 섞은거 같아요! 맛있었어오 ㅎㅎ",
          "original_form": "닭도리탕이랑 곱창 섞은거 같아요! 맛있었어오 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044094.1.25",
          "form": "오ㅋㅋㅋ꿀조화네요 ㅋㅋㅈ",
          "original_form": "오ㅋㅋㅋ꿀조화네요 ㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044094.1.26",
          "form": "먹어보진 않았지만\r\n맛있을거 같아요ㅋㅋ",
          "original_form": "먹어보진 않았지만\r\n맛있을거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044094.1.27",
          "form": "네 맛있었어요!!",
          "original_form": "네 맛있었어요!!",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044094.1.28",
          "form": "떡볶이떡도 들어가있었는데 엄청 쫄깃했어요",
          "original_form": "떡볶이떡도 들어가있었는데 엄청 쫄깃했어요",
