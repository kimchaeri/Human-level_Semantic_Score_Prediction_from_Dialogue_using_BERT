        {
          "id": "MDRW2100047199.1.1",
          "form": "신제품나왔는데 신제품이 훨!!!!!!!씬",
          "original_form": "신제품나왔는데 신제품이 훨!!!!!!!씬",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100047199.1.2",
          "form": "싸더라고요",
          "original_form": "싸더라고요",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100047199.1.3",
          "form": "그럼좋게 싸게",
          "original_form": "그럼좋게 싸게",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100047199.1.4",
          "form": "오 그럼 신제품사죠",
          "original_form": "오 그럼 신제품사죠",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100047199.1.5",
          "form": "다들있고 저만 없는듯해요",
          "original_form": "다들있고 저만 없는듯해요",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100047199.1.6",
          "form": "큰맘먹고 알바 돈으로 살까봐요 ㅋㅋ",
          "original_form": "큰맘먹고 알바 돈으로 살까봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100047199.1.7",
          "form": "이번에",
          "original_form": "이번에",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100047199.1.8",
          "form": "버즈2가",
          "original_form": "버즈2가",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100047199.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100047199.1.10",
          "form": "신제품이 15만?일케나왔대요ㅋㅋㅋ",
          "original_form": "신제품이 15만?일케나왔대요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100047199.1.11",
          "form": "사전예약하면 13",
          "original_form": "사전예약하면 13",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100047199.1.12",
          "form": "까지도 준대요ㅋㅋㅋ",
          "original_form": "까지도 준대요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
