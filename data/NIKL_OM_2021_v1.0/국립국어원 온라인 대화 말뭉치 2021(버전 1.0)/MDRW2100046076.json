      "utterance": [
        {
          "id": "MDRW2100046076.1.1",
          "form": "저 최근에 1 부터 봣어요 ㅋㅋㅋ",
          "original_form": "저 최근에 1 부터 봣어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046076.1.2",
          "form": "엄청 잔인해요",
          "original_form": "엄청 잔인해요",
          "speaker_id": "1",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046076.1.3",
          "form": "저도 1부터봤었어요",
          "original_form": "저도 1부터봤었어요",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046076.1.4",
          "form": "어릴때는 잘봐가지구 ㅋㅋㅋ",
          "original_form": "어릴때는 잘봐가지구 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046076.1.5",
          "form": "ㅋㅋㅋㅋ 와 저는 어릴때 못보고 다커서",
          "original_form": "ㅋㅋㅋㅋ 와 저는 어릴때 못보고 다커서",
          "speaker_id": "1",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046076.1.6",
          "form": "반전이 있드라구요",
          "original_form": "반전이 있드라구요",
          "speaker_id": "1",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046076.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저랑 체질이 바꼈균여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저랑 체질이 바꼈균여",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046076.1.8",
          "form": "넼ㅋㅋ 근데 컨저링 이런건 못보겟어요 ㅋㅋㅋ",
          "original_form": "넼ㅋㅋ 근데 컨저링 이런건 못보겟어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046076.1.9",
          "form": "저는 그런것도 좋아해요",
          "original_form": "저는 그런것도 좋아해요",
          "speaker_id": "2",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100046076.1.10",
          "form": "ㅣ귀막고보는 거지만ㅋㅋㅋ",
          "original_form": "ㅣ귀막고보는 거지만ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100046076.1.11",
          "form": "ㅋㅋㅋ 눈은 안감으세요?",
          "original_form": "ㅋㅋㅋ 눈은 안감으세요?",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046076.1.12",
          "form": "넬넹!!!",
          "original_form": "넬넹!!!",
          "speaker_id": "2",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100046076.1.13",
          "form": "오 공포영화 어떤게 기억에 남으새요",
          "original_form": "오 공포영화 어떤게 기억에 남으새요",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046076.1.14",
          "form": "근데 공포영화가 다비슷해서",
          "original_form": "근데 공포영화가 다비슷해서",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046076.1.15",
          "form": "에나벨??ㅋㅋㅋㅋ",
          "original_form": "에나벨??ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046076.1.16",
          "form": "그거좀쟘게봤어요!",
          "original_form": "그거좀쟘게봤어요!",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046076.1.17",
          "form": "오저도 그거 봑어요 ㅋㅋㅋㄱ",
          "original_form": "오저도 그거 봑어요 ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046076.1.18",
          "form": "그런건 영화관에서 바야해요",
          "original_form": "그런건 영화관에서 바야해요",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046076.1.19",
          "form": "맞아요 묘미더라구용",
          "original_form": "맞아요 묘미더라구용",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046076.1.20",
          "form": "ㅋㅋㅋ 사람들 소리지르는게 너 무서워요",
          "original_form": "ㅋㅋㅋ 사람들 소리지르는게 너 무서워요",
          "speaker_id": "1",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100046076.1.21",
          "form": "저는 안질러요",
          "original_form": "저는 안질러요",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100046076.1.22",
          "form": "지를만큼무서운건아니던뎅",
