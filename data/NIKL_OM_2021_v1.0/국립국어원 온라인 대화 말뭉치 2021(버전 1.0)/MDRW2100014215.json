      "utterance": [
        {
          "id": "MDRW2100014215.1.1",
          "form": "싸이월드하셨어요?",
          "original_form": "싸이월드하셨어요?",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014215.1.2",
          "form": "네... ㄲㅋㅋㅋㅋ 다시 열린다는데",
          "original_form": "네... ㄲㅋㅋㅋㅋ 다시 열린다는데",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014215.1.3",
          "form": "닫아주고싶어요...",
          "original_form": "닫아주고싶어요...",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014215.1.4",
          "form": "제 미니홈피는 폭파해줬으면 좋겠어요",
          "original_form": "제 미니홈피는 폭파해줬으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014215.1.5",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014215.1.6",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014215.1.7",
          "form": "그래도 추억이잖아요",
          "original_form": "그래도 추억이잖아요",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014215.1.8",
          "form": "ㅋㅋㅋㅋ 예전에 한번더 열렸을때 들어갔는데",
          "original_form": "ㅋㅋㅋㅋ 예전에 한번더 열렸을때 들어갔는데",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014215.1.9",
          "form": "안지운 사진에 색들어간 안경쓰고있는 셀카보고 식겁해서 나왔어요ㅋㅋㅋ",
          "original_form": "안지운 사진에 색들어간 안경쓰고있는 셀카보고 식겁해서 나왔어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014215.1.10",
          "form": "그때 다 지웠는지 모르겠는데 지금은 아이디를 몰라서 못들어가요ㅜㅜㅋㅋㅋ",
          "original_form": "그때 다 지웠는지 모르겠는데 지금은 아이디를 몰라서 못들어가요ㅜㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014215.1.11",
          "form": "ㅋㅋㅋㅋ 저도들어가봐야지하고 생각만하고",
          "original_form": "ㅋㅋㅋㅋ 저도들어가봐야지하고 생각만하고",
          "speaker_id": "1",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014215.1.12",
          "form": "들어가보지는않았어요",
          "original_form": "들어가보지는않았어요",
          "speaker_id": "1",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014215.1.13",
          "form": "아이디 비번이 기억나지않는데",
          "original_form": "아이디 비번이 기억나지않는데",
          "speaker_id": "1",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014215.1.14",
          "form": "핸드폰번호도 바꼈거든요",
          "original_form": "핸드폰번호도 바꼈거든요",
          "speaker_id": "1",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014215.1.15",
          "form": "맞아요 앜ㅋㅋㅋㄱ저도",
          "original_form": "맞아요 앜ㅋㅋㅋㄱ저도",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014215.1.16",
          "form": "색있는안경ㅋㅋ 저도 썼었어요ㅋㅋ",
          "original_form": "색있는안경ㅋㅋ 저도 썼었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014215.1.17",
          "form": "그때랑 번호다를거같은데",
          "original_form": "그때랑 번호다를거같은데",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014215.1.18",
          "form": "아이디찾기도못할거같아요ㅜㅜ",
          "original_form": "아이디찾기도못할거같아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014215.1.19",
          "form": "연한갈색ㅋㅋ",
          "original_form": "연한갈색ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014215.1.20",
          "form": "휴ㅋㅋㅋ저만쓴게아니라 다행이에요",
          "original_form": "휴ㅋㅋㅋ저만쓴게아니라 다행이에요",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014215.1.21",
          "form": "근데 부활한다고해도ㅋㅋㅋ",
          "original_form": "근데 부활한다고해도ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014215.1.22",
          "form": "저도여 하늘색도 썼는데",
          "original_form": "저도여 하늘색도 썼는데",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014215.1.23",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014215.1.24",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014215.1.25",
          "form": "왠지 인스타처럼",
          "original_form": "왠지 인스타처럼",
          "speaker_id": "2",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014215.1.26",
          "form": "다행인가요? ㅋㅋ",
          "original_form": "다행인가요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014215.1.27",
          "form": "커지진못할거같아요",
          "original_form": "커지진못할거같아요",
          "speaker_id": "2",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014215.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ일정기간 로그인안한사람은 삭제해줬으면좋겠어요...ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ일정기간 로그인안한사람은 삭제해줬으면좋겠어요...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014215.1.29",
          "form": "ㅋㅋ 맞아요",
          "original_form": "ㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014215.1.30",
          "form": "옛날 흑역사 사진같은거",
          "original_form": "옛날 흑역사 사진같은거",
          "speaker_id": "1",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014215.1.31",
          "form": "내기억에도 지워졌는데",
          "original_form": "내기억에도 지워졌는데",
          "speaker_id": "1",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014215.1.32",
          "form": "ㅋㅋㅋ 그리고 흑역사 멘트들도요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 그리고 흑역사 멘트들도요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014215.1.33",
          "form": "다시보면 ㅋㅋㅋ",
          "original_form": "다시보면 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014215.1.34",
          "form": "저예전에 네이버 지식인,카페댓글보다",
          "original_form": "저예전에 네이버 지식인,카페댓글보다",
          "speaker_id": "2",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014215.1.35",
          "form": "그때 그 감성ㅋㅋ",
          "original_form": "그때 그 감성ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014215.1.36",
          "form": "깜짝놀랐어요ㅋㅋㅋ",
          "original_form": "깜짝놀랐어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014215.1.37",
          "form": "누가 제 아이디 해킹했나싶은 오글오글 글들ㅋㅋㅋ",
          "original_form": "누가 제 아이디 해킹했나싶은 오글오글 글들ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014215.1.38",
          "form": "ㅋㅋㅋㅋ 싸이월드부활 원하는사람들이 많나. . ??",
          "original_form": "ㅋㅋㅋㅋ 싸이월드부활 원하는사람들이 많나. . ??",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014215.1.39",
          "form": "다들싫어하는것같은데요ㅋㅋ",
          "original_form": "다들싫어하는것같은데요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014215.1.40",
          "form": "음....제생각에두요ㅋㅋ싸이월드 관계자만",
          "original_form": "음....제생각에두요ㅋㅋ싸이월드 관계자만",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014215.1.41",
          "form": "원하는거같아요ㅋㅋ",
          "original_form": "원하는거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014215.1.42",
          "form": "일단 싸이월드가 컴퓨터에서 쓰기편한구조라서",
          "original_form": "일단 싸이월드가 컴퓨터에서 쓰기편한구조라서",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014215.1.43",
          "form": "인스타처럼 크긴 힘들거같아요",
          "original_form": "인스타처럼 크긴 힘들거같아요",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014215.1.44",
