      "utterance": [
        {
          "id": "MDRW2100012740.1.1",
          "form": "응 알지 안가봤는데 티비에 많이 나오니",
          "original_form": "응 알지 안가봤는데 티비에 많이 나오니",
          "speaker_id": "2",
          "time": "20210825 00:32"
        },
        {
          "id": "MDRW2100012740.1.2",
          "form": "응 거기에 인력거가 있다",
          "original_form": "응 거기에 인력거가 있다",
          "speaker_id": "1",
          "time": "20210825 00:32"
        },
        {
          "id": "MDRW2100012740.1.3",
          "form": "정말 신기하다",
          "original_form": "정말 신기하다",
          "speaker_id": "2",
          "time": "20210825 00:32"
        },
        {
          "id": "MDRW2100012740.1.4",
          "form": "근데 인력거가 옛날 그런 인력거는 아니야",
          "original_form": "근데 인력거가 옛날 그런 인력거는 아니야",
          "speaker_id": "1",
          "time": "20210825 00:32"
        },
        {
          "id": "MDRW2100012740.1.5",
          "form": "그럼?  뭔데?",
          "original_form": "그럼?  뭔데?",
          "speaker_id": "2",
          "time": "20210825 00:33"
        },
        {
          "id": "MDRW2100012740.1.6",
          "form": "현대식 자전저로 달리는 인력거",
          "original_form": "현대식 자전저로 달리는 인력거",
          "speaker_id": "1",
          "time": "20210825 00:33"
        },
        {
          "id": "MDRW2100012740.1.7",
          "form": "그럼 운전하시는분들 안힘들어서 눈치 안봐도 되겠다",
          "original_form": "그럼 운전하시는분들 안힘들어서 눈치 안봐도 되겠다",
          "speaker_id": "2",
          "time": "20210825 00:33"
        },
        {
          "id": "MDRW2100012740.1.8",
          "form": "응 그래서 그런지 3인승",
          "original_form": "응 그래서 그런지 3인승",
          "speaker_id": "1",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100012740.1.9",
          "form": "3인승이면 뭐 괜찮네 가격은",
          "original_form": "3인승이면 뭐 괜찮네 가격은",
          "speaker_id": "2",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100012740.1.10",
          "form": "1인당으로 게산을 하는가봐",
          "original_form": "1인당으로 게산을 하는가봐",
          "speaker_id": "1",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100012740.1.11",
          "form": "1인당 비싼거아니야",
          "original_form": "1인당 비싼거아니야",
          "speaker_id": "2",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100012740.1.12",
          "form": "이게 60분투어라 그런가봐",
          "original_form": "이게 60분투어라 그런가봐",
          "speaker_id": "1",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100012740.1.13",
          "form": "60분 1시간이네 그럼 또 괜찮은겆같기도",
          "original_form": "60분 1시간이네 그럼 또 괜찮은겆같기도",
          "speaker_id": "2",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100012740.1.14",
          "form": "ㅎㅎ 전화로 예약하고 갈 수 있어서 헛걸음 안할수도 있데",
          "original_form": "ㅎㅎ 전화로 예약하고 갈 수 있어서 헛걸음 안할수도 있데",
          "speaker_id": "1",
