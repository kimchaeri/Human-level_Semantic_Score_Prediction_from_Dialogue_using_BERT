      "utterance": [
        {
          "id": "MDRW2100040272.1.1",
          "form": "최근에 뭐 사신거있어요?",
          "original_form": "최근에 뭐 사신거있어요?",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040272.1.2",
          "form": "최근엔 가을옷을 좀 샀어요ㅎㅎ",
          "original_form": "최근엔 가을옷을 좀 샀어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100040272.1.3",
          "form": "계절 바뀌면 왕창 사두고 그걸로만 계절 나는 편이라 ㅋㅋ",
          "original_form": "계절 바뀌면 왕창 사두고 그걸로만 계절 나는 편이라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040272.1.4",
          "form": "완전 가을로 넘어가는중이죠 ㅎ",
          "original_form": "완전 가을로 넘어가는중이죠 ㅎ",
          "speaker_id": "1",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040272.1.5",
          "form": "어떤 옷 사셨어요?",
          "original_form": "어떤 옷 사셨어요?",
          "speaker_id": "1",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040272.1.6",
          "form": "원피스랑 블라우스랑 스커트요ㅋㅋㅋ",
          "original_form": "원피스랑 블라우스랑 스커트요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040272.1.7",
          "form": "플랙스하셨네요",
          "original_form": "플랙스하셨네요",
          "speaker_id": "1",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100040272.1.8",
          "form": "옷 좋아하세요?",
          "original_form": "옷 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100040272.1.9",
          "form": "막 좋아하는 건 아니었는데 좀 갖춰입고 살아야겠더라구요ㅋㅋㅋ",
          "original_form": "막 좋아하는 건 아니었는데 좀 갖춰입고 살아야겠더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040272.1.10",
          "form": "그쵸 ᆢ",
          "original_form": "그쵸 ᆢ",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040272.1.11",
          "form": "저도 사야하는데 ㅜ",
          "original_form": "저도 사야하는데 ㅜ",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040272.1.12",
          "form": "주로 어디서 쇼핑하세요?",
          "original_form": "주로 어디서 쇼핑하세요?",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040272.1.13",
          "form": "어디 오프라인으로 돌아다닌 건 별로 안좋아해서",
          "original_form": "어디 오프라인으로 돌아다닌 건 별로 안좋아해서",
          "speaker_id": "2",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100040272.1.14",
          "form": "그냥 네이버쇼핑으로 해요",
          "original_form": "그냥 네이버쇼핑으로 해요",
          "speaker_id": "2",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100040272.1.15",
          "form": "그쵸 요즘 코로나도있고",
          "original_form": "그쵸 요즘 코로나도있고",
          "speaker_id": "1",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100040272.1.16",
          "form": "좋아하는 쇼핑몰 있으세요?",
          "original_form": "좋아하는 쇼핑몰 있으세요?",
          "speaker_id": "1",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100040272.1.17",
          "form": "넹 최근에 뭔가 제 취향 쇼핑몰을 만나서 주문해봤는데",
          "original_form": "넹 최근에 뭔가 제 취향 쇼핑몰을 만나서 주문해봤는데",
          "speaker_id": "2",
          "time": "20210904 15:29"
        },
        {
          "id": "MDRW2100040272.1.18",
          "form": "아직 도착을 안해서 제품 봐야될거같아요 ㅋㅋㅋ",
          "original_form": "아직 도착을 안해서 제품 봐야될거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:29"
        },
        {
          "id": "MDRW2100040272.1.19",
          "form": "오오 어딘가요?",
          "original_form": "오오 어딘가요?",
          "speaker_id": "1",
          "time": "20210904 15:29"
        },
        {
          "id": "MDRW2100040272.1.20",
          "form": "제이플로우라는 곳에서 한번 주문해봤어요",
          "original_form": "제이플로우라는 곳에서 한번 주문해봤어요",
          "speaker_id": "2",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100040272.1.21",
          "form": "들어본것도 같은데",
          "original_form": "들어본것도 같은데",
          "speaker_id": "1",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100040272.1.22",
          "form": "찾아봐야겠네요",
          "original_form": "찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210904 15:30"
        }
      ]
    }
