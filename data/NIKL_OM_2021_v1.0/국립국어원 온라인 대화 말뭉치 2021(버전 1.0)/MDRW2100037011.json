      "utterance": [
        {
          "id": "MDRW2100037011.1.1",
          "form": "소고기 ㅋ",
          "original_form": "소고기 ㅋ",
          "speaker_id": "2",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100037011.1.2",
          "form": "소고기 국거리요ㅋㅋ푸핫",
          "original_form": "소고기 국거리요ㅋㅋ푸핫",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100037011.1.3",
          "form": "국ㅋㅋㄱ",
          "original_form": "국ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100037011.1.4",
          "form": "그래 소고기랑 소고기국은 다르지ㅋㅋ",
          "original_form": "그래 소고기랑 소고기국은 다르지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100037011.1.5",
          "form": "그렇죠ㅋㅋㅋㅋㅋ",
          "original_form": "그렇죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100037011.1.6",
          "form": "푸핫.ㅋ",
          "original_form": "푸핫.ㅋ",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100037011.1.7",
          "form": "난 저녁 다차렸다",
          "original_form": "난 저녁 다차렸다",
          "speaker_id": "2",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100037011.1.8",
          "form": "룰루랄라",
          "original_form": "룰루랄라",
          "speaker_id": "2",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100037011.1.9",
          "form": "ㅋㅋㅋ언니 저는 감자탕먹고왔어요. 외식ㅋ",
          "original_form": "ㅋㅋㅋ언니 저는 감자탕먹고왔어요. 외식ㅋ",
          "speaker_id": "1",
          "time": "20210831 23:50"
        },
        {
          "id": "MDRW2100037011.1.10",
          "form": "이제 name3재움ㅋㅋㅋㅋ",
          "original_form": "이제 &name3&재움ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:50"
        },
        {
          "id": "MDRW2100037011.1.11",
          "form": "언니 일진 상장ㄱㄱㅋ아름다움ㅋ",
          "original_form": "언니 일진 상장ㄱㄱㅋ아름다움ㅋ",
          "speaker_id": "1",
          "time": "20210901 09:01"
        },
        {
          "id": "MDRW2100037011.1.12",
          "form": "아름답다",
          "original_form": "아름답다",
          "speaker_id": "2",
          "time": "20210901 09:40"
        },
        {
          "id": "MDRW2100037011.1.13",
          "form": "하나팔고하나있다ㅋㅋ",
          "original_form": "하나팔고하나있다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:40"
        },
        {
          "id": "MDRW2100037011.1.14",
          "form": "괜히팔았나?",
          "original_form": "괜히팔았나?",
          "speaker_id": "2",
          "time": "20210901 09:41"
        },
        {
          "id": "MDRW2100037011.1.15",
          "form": "저도 하나팔까싶어요ㅋ",
          "original_form": "저도 하나팔까싶어요ㅋ",
          "speaker_id": "1",
          "time": "20210901 09:47"
        },
        {
          "id": "MDRW2100037011.1.16",
          "form": "하나는 들고있고",
          "original_form": "하나는 들고있고",
          "speaker_id": "1",
          "time": "20210901 09:47"
        },
        {
          "id": "MDRW2100037011.1.17",
          "form": "우째될지몰라서",
          "original_form": "우째될지몰라서",
          "speaker_id": "1",
          "time": "20210901 09:47"
        },
        {
          "id": "MDRW2100037011.1.18",
          "form": "일단 따상찍었으니--",
          "original_form": "일단 따상찍었으니--{emoji:♡}{emoji:♡}{emoji:♡}",
          "speaker_id": "1",
          "time": "20210901 09:47"
        },
        {
          "id": "MDRW2100037011.1.19",
          "form": "아름다움",
          "original_form": "아름다움",
          "speaker_id": "1",
          "time": "20210901 09:47"
        },
        {
          "id": "MDRW2100037011.1.20",
          "form": "한주 일단 팔고 한주는 내일 하루 더 볼까싶어요ㅋㅋ여튼 오늘도 수익 아름답네유",
          "original_form": "한주 일단 팔고 한주는 내일 하루 더 볼까싶어요ㅋㅋ여튼 오늘도 수익 아름답네유{emoji:♡}{emoji:♡}",
          "speaker_id": "1",
          "time": "20210901 09:52"
        },
        {
          "id": "MDRW2100037011.1.21",
          "form": "아름답다ㅋ 나도 하나는 내일ㅋㅋㅋ",
          "original_form": "아름답다ㅋ 나도 하나는 내일ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100037011.1.22",
          "form": "룰루랄라네",
          "original_form": "룰루랄라네",
          "speaker_id": "2",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100037011.1.23",
          "form": "오늘저녁 소코키?ㅋ",
          "original_form": "오늘저녁 소코키?ㅋ",
          "speaker_id": "2",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100037011.1.24",
          "form": "ㅋㅋ 소고기좋쥬",
          "original_form": "ㅋㅋ 소고기좋쥬",
          "speaker_id": "1",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100037011.1.25",
          "form": "신랑은2주 다 팔았대서 용돈쓰라고줬슈ㅋ",
          "original_form": "신랑은2주 다 팔았대서 용돈쓰라고줬슈ㅋ",
          "speaker_id": "1",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100037011.1.26",
          "form": "좋다ㅋㅋ",
          "original_form": "좋다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100037011.1.27",
          "form": "용돈 이런거 좋네ㅋㅋ",
          "original_form": "용돈 이런거 좋네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:55"
        },
        {
          "id": "MDRW2100037011.1.28",
          "form": "네ㅋㅋ공모주로 번거는 각자 용돈하기로했어유ㅋ",
          "original_form": "네ㅋㅋ공모주로 번거는 각자 용돈하기로했어유ㅋ",
          "speaker_id": "1",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100037011.1.29",
          "form": "에헤야디야ㅋㅋ",
          "original_form": "에헤야디야ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100037011.1.30",
          "form": "좋은 물건이ㅣㅋㅋ",
          "original_form": "좋은 물건이ㅣㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100037011.1.31",
          "form": "난 이제밥먹는당ㅎ",
          "original_form": "난 이제밥먹는당ㅎ",
          "speaker_id": "2",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100037011.1.32",
          "form": "전 진미김밥먹었어요 히히",
          "original_form": "전 진미김밥먹었어요 히히",
          "speaker_id": "1",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100037011.1.33",
          "form": "언니는뭐먹어요?",
          "original_form": "언니는뭐먹어요?",
          "speaker_id": "1",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100037011.1.34",
          "form": "진미김밥은어디고?",
          "original_form": "진미김밥은어디고?",
          "speaker_id": "2",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100037011.1.35",
          "form": "나 분식이다오늘",
          "original_form": "나 분식이다오늘",
          "speaker_id": "2",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100037011.1.36",
          "form": "떡볶이",
          "original_form": "떡볶이",
          "speaker_id": "2",
