      "utterance": [
        {
          "id": "MDRW2100042959.1.1",
          "form": "아침 뭐드셨어요?",
          "original_form": "아침 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210907 12:02"
        },
        {
          "id": "MDRW2100042959.1.2",
          "form": "샌드위치요! name1님은요?",
          "original_form": "샌드위치요! &name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 12:02"
        },
        {
          "id": "MDRW2100042959.1.3",
          "form": "식빵에 크림치즈요!",
          "original_form": "식빵에 크림치즈요!",
          "speaker_id": "1",
          "time": "20210907 12:02"
        },
        {
          "id": "MDRW2100042959.1.4",
          "form": "오! 점심은 뭐 드실거에요?",
          "original_form": "오! 점심은 뭐 드실거에요?",
          "speaker_id": "2",
          "time": "20210907 12:03"
        },
        {
          "id": "MDRW2100042959.1.5",
          "form": "저 학생이라 급식먹어요! 근데 아직메뉴를 확인안했어요 크크",
          "original_form": "저 학생이라 급식먹어요! 근데 아직메뉴를 확인안했어요 크크",
          "speaker_id": "1",
          "time": "20210907 12:03"
        },
        {
          "id": "MDRW2100042959.1.6",
          "form": "화요일이라 수요일보다는 덜 맛있겠어요 ㅋㅋ",
          "original_form": "화요일이라 수요일보다는 덜 맛있겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:06"
        },
        {
          "id": "MDRW2100042959.1.7",
          "form": "오늘 치킨구이 나온대요!",
          "original_form": "오늘 치킨구이 나온대요!",
          "speaker_id": "1",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100042959.1.8",
          "form": "오~~ 맛난거 드시는데~~~",
          "original_form": "오~~ 맛난거 드시는데~~~",
          "speaker_id": "2",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100042959.1.9",
          "form": "ㅋㅋㅋ맛있게 먹었어요",
          "original_form": "ㅋㅋㅋ맛있게 먹었어요",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100042959.1.10",
          "form": "점심 뭐드셨나요!",
          "original_form": "점심 뭐드셨나요!",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100042959.1.11",
          "form": "저는 우동 먹었어요!",
          "original_form": "저는 우동 먹었어요!",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100042959.1.12",
          "form": "근데 맛은.. 없었어요 ㅋㅋㅋ",
          "original_form": "근데 맛은.. 없었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100042959.1.13",
          "form": "헐....",
          "original_form": "헐....",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100042959.1.14",
          "form": "어떻게 우동이 맛없을수가",
          "original_form": "어떻게 우동이 맛없을수가",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100042959.1.15",
          "form": "식당가서 사드셨어요?",
          "original_form": "식당가서 사드셨어요?",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100042959.1.16",
          "form": "회사 구내 식당가서 먹어서 그런것 같은데 ㅎ...",
          "original_form": "회사 구내 식당가서 먹어서 그런것 같은데 ㅎ...",
          "speaker_id": "2",
          "time": "20210907 18:09"
        },
        {
          "id": "MDRW2100042959.1.17",
          "form": "그냥 나가서 먹을걸 그랬어요ㅠㅠ",
          "original_form": "그냥 나가서 먹을걸 그랬어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 18:09"
        },
        {
          "id": "MDRW2100042959.1.18",
          "form": "ㅋㅋㅜ 약간 급식 같은건가요",
          "original_form": "ㅋㅋㅜ 약간 급식 같은건가요",
          "speaker_id": "1",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100042959.1.19",
          "form": "저희 학교 급식도 맛있어보이는 메뉴를 요상하게 만드는 재주가 있거든요",
          "original_form": "저희 학교 급식도 맛있어보이는 메뉴를 요상하게 만드는 재주가 있거든요",
          "speaker_id": "1",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100042959.1.20",
          "form": "앜ㅋㅋ 학교가 잘못했네요",
          "original_form": "앜ㅋㅋ 학교가 잘못했네요",
          "speaker_id": "2",
          "time": "20210907 18:13"
        }
