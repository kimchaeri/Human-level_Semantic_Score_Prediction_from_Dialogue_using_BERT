      "utterance": [
        {
          "id": "MDRW2100022459.1.1",
          "form": "name2은 콘서트 가시나요?",
          "original_form": "&name2&은 콘서트 가시나요?",
          "speaker_id": "1",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100022459.1.2",
          "form": "아뇨 저는 한번도 안 가봤어요ㅠ",
          "original_form": "아뇨 저는 한번도 안 가봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210829 14:33"
        },
        {
          "id": "MDRW2100022459.1.3",
          "form": "그렇군요ㅎ 전 티켓팅이 빡세서 부담이 크더라고요",
          "original_form": "그렇군요ㅎ 전 티켓팅이 빡세서 부담이 크더라고요",
          "speaker_id": "1",
          "time": "20210829 14:44"
        },
        {
          "id": "MDRW2100022459.1.4",
          "form": "와 그니까요",
          "original_form": "와 그니까요",
          "speaker_id": "2",
          "time": "20210829 15:09"
        },
        {
          "id": "MDRW2100022459.1.5",
          "form": "주변에 아이돌팬인 친구말 들어보면 진짜 빡세더라고요",
          "original_form": "주변에 아이돌팬인 친구말 들어보면 진짜 빡세더라고요",
          "speaker_id": "2",
          "time": "20210829 15:09"
        },
        {
          "id": "MDRW2100022459.1.6",
          "form": "전 친구 도와서 방탄 콘서트 티켓팅해본적 잇어요ㅋㅋ",
          "original_form": "전 친구 도와서 방탄 콘서트 티켓팅해본적 잇어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022459.1.7",
          "form": "친구들 보면 여러명한테 부탁해서 티켓팅하더라고요ㅋㅋㅋ",
          "original_form": "친구들 보면 여러명한테 부탁해서 티켓팅하더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:17"
        },
        {
          "id": "MDRW2100022459.1.8",
          "form": "콘서트 티켓팅 몇초컷인가요?보통",
          "original_form": "콘서트 티켓팅 몇초컷인가요?보통",
          "speaker_id": "2",
          "time": "20210829 15:17"
        },
        {
          "id": "MDRW2100022459.1.9",
          "form": "방탄은 일단 서버가 터지죠ㅋㅋ 우왕좌왕하는 사이에 끝나있더라고요",
          "original_form": "방탄은 일단 서버가 터지죠ㅋㅋ 우왕좌왕하는 사이에 끝나있더라고요",
          "speaker_id": "1",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100022459.1.10",
          "form": "와우 다들 어떻게 티켓팅 성공하는걸까요ㅋㅋㅋ",
          "original_form": "와우 다들 어떻게 티켓팅 성공하는걸까요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100022459.1.11",
          "form": "결국 취켓팅하더라고요 밤도 새던데 ㄷㄷ",
          "original_form": "결국 취켓팅하더라고요 밤도 새던데 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022459.1.12",
          "form": "맞아요 다들 밤을 새우더라고요",
          "original_form": "맞아요 다들 밤을 새우더라고요",
          "speaker_id": "2",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022459.1.13",
          "form": "name2은 방탄 누구 최애인가요?",
          "original_form": "&name2&은 방탄 누구 최애인가요?",
          "speaker_id": "1",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022459.1.14",
          "form": "저 제이홉이요!",
          "original_form": "저 제이홉이요!",
          "speaker_id": "2",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022459.1.15",
          "form": "저도 제이홉 춤 잘추고 유쾌해보여서 호감이에요",
          "original_form": "저도 제이홉 춤 잘추고 유쾌해보여서 호감이에요",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022459.1.16",
          "form": "맞아요 저도 그래서 제이홉 실제로 보러 콘서트 가고 싶어요ㅎㅎ",
          "original_form": "맞아요 저도 그래서 제이홉 실제로 보러 콘서트 가고 싶어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 17:59"
        }
