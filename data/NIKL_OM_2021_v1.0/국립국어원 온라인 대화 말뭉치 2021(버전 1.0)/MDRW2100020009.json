      "utterance": [
        {
          "id": "MDRW2100020009.1.1",
          "form": "설거지 싫어요",
          "original_form": "설거지 싫어요",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020009.1.2",
          "form": "청소도 싫어요",
          "original_form": "청소도 싫어요",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020009.1.3",
          "form": "청소 싫어요 설겆이는 괜춘",
          "original_form": "청소 싫어요 설겆이는 괜춘",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020009.1.4",
          "form": "안하면 먹을수가없다",
          "original_form": "안하면 먹을수가없다",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100020009.1.5",
          "form": "근데 요즘 식기세척기 갖고싶다",
          "original_form": "근데 요즘 식기세척기 갖고싶다",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100020009.1.6",
          "form": "안해도 어떻게든 먹더라ㅋㅋ",
          "original_form": "안해도 어떻게든 먹더라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100020009.1.7",
          "form": "기계가 해준다는데 굳이 내가 해야하나 싶고",
          "original_form": "기계가 해준다는데 굳이 내가 해야하나 싶고",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020009.1.8",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020009.1.9",
          "form": "식기세척기 있어",
          "original_form": "식기세척기 있어",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020009.1.10",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020009.1.11",
          "form": "소독기만 있엉",
          "original_form": "소독기만 있엉",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020009.1.12",
          "form": "그렇구만 ㅋㅋㅋ",
          "original_form": "그렇구만 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020009.1.13",
          "form": "아버님 핸드폰은 최신으로 사셨어?",
          "original_form": "아버님 핸드폰은 최신으로 사셨어?",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020009.1.14",
          "form": "그냥 적당한 자급제폰으로",
          "original_form": "그냥 적당한 자급제폰으로",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020009.1.15",
          "form": "그와중에 가장 최신으로ㅎㅎ",
          "original_form": "그와중에 가장 최신으로ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020009.1.16",
          "form": "a시리즈 중에서는 최신이었던듯?",
          "original_form": "a시리즈 중에서는 최신이었던듯?",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020009.1.17",
          "form": "요즘 좀 귀찮은건 동생한테 넘겨서ㅋㅋㅋ",
          "original_form": "요즘 좀 귀찮은건 동생한테 넘겨서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020009.1.18",
          "form": "좋은 현상이다",
          "original_form": "좋은 현상이다",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020009.1.19",
          "form": "동생도 해봐야 알지 ㅋㅋㅋㅋ",
          "original_form": "동생도 해봐야 알지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020009.1.20",
          "form": "따로살때야 어쩔수 없었다지만 같이 사는 이상 지가 해야디",
          "original_form": "따로살때야 어쩔수 없었다지만 같이 사는 이상 지가 해야디",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020009.1.21",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020009.1.22",
          "form": "우리집은 여동생이 쇼핑담당 내가 세팅당담 ㅋㅋㅋㅋㄱㅋ",
          "original_form": "우리집은 여동생이 쇼핑담당 내가 세팅당담 ㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020009.1.23",
          "form": "그런거지",
          "original_form": "그런거지",
          "speaker_id": "1",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020009.1.24",
          "form": "우리집은 동생이 쇼핑 나는 결제ㅋㅋ",
          "original_form": "우리집은 동생이 쇼핑 나는 결제ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020009.1.25",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020009.1.26",
          "form": "나누어서 해야지",
          "original_form": "나누어서 해야지",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020009.1.27",
          "form": "카드깡?ㅋㅋ",
          "original_form": "카드깡?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020009.1.28",
          "form": "나는 쇼핑 귀찮어섴ㅋㅋㅋㄱ",
          "original_form": "나는 쇼핑 귀찮어섴ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020009.1.29",
          "form": "카드깡 할때도 있지 당연힠ㅋㅋ",
          "original_form": "카드깡 할때도 있지 당연힠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020009.1.30",
          "form": "내년에 나랑 동생 두고 백령도로 간다는데",
          "original_form": "내년에 나랑 동생 두고 백령도로 간다는데",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020009.1.31",
          "form": "반만 믿고 있어",
          "original_form": "반만 믿고 있어",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020009.1.32",
          "form": "그런거는 그렇지",
          "original_form": "그런거는 그렇지",
          "speaker_id": "1",
