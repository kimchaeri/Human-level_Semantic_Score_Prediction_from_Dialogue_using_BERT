      "utterance": [
        {
          "id": "MDRW2100022779.1.1",
          "form": "이상형 있으세요?",
          "original_form": "이상형 있으세요?",
          "speaker_id": "1",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022779.1.2",
          "form": "없습니다",
          "original_form": "없습니다",
          "speaker_id": "2",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022779.1.3",
          "form": "결혼은 무엇인가요?",
          "original_form": "결혼은 무엇인가요?",
          "speaker_id": "2",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022779.1.4",
          "form": "사람과 사람이 만나서 결혼하는거라 생각해요",
          "original_form": "사람과 사람이 만나서 결혼하는거라 생각해요",
          "speaker_id": "1",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100022779.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100022779.1.6",
          "form": "저는 그사람과 함께했을때 소중한가 안한가 그게 결혼이라 생각합니다.",
          "original_form": "저는 그사람과 함께했을때 소중한가 안한가 그게 결혼이라 생각합니다.",
          "speaker_id": "2",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100022779.1.7",
          "form": "연애가치관은 어떻게되시나요?",
          "original_form": "연애가치관은 어떻게되시나요?",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022779.1.8",
          "form": "말 통하는게 제일 중요해요",
          "original_form": "말 통하는게 제일 중요해요",
          "speaker_id": "1",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100022779.1.9",
          "form": "저도요 취미도같으면 좋을거같아요",
          "original_form": "저도요 취미도같으면 좋을거같아요",
          "speaker_id": "2",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100022779.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022779.1.11",
          "form": "그것도 중요하죠",
          "original_form": "그것도 중요하죠",
          "speaker_id": "1",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022779.1.12",
          "form": "결혼 생각 있으세요ㅡ?",
          "original_form": "결혼 생각 있으세요ㅡ?",
          "speaker_id": "1",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022779.1.13",
          "form": "결혼 생각은 있는데 능력이 있어야될거같아서 지금은 없습니다",
          "original_form": "결혼 생각은 있는데 능력이 있어야될거같아서 지금은 없습니다",
          "speaker_id": "2",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022779.1.14",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210829 18:29"
        },
        {
          "id": "MDRW2100022779.1.15",
          "form": "이상형은 어떻게되세요?",
          "original_form": "이상형은 어떻게되세요?",
          "speaker_id": "2",
          "time": "20210829 18:29"
        },
        {
          "id": "MDRW2100022779.1.16",
          "form": "전 저보다 키크고 입매 예쁜 사람이요",
          "original_form": "전 저보다 키크고 입매 예쁜 사람이요",
          "speaker_id": "1",
          "time": "20210829 18:30"
        },
        {
          "id": "MDRW2100022779.1.17",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100022779.1.18",
          "form": "해보싶으신 데이트있으신가요?",
          "original_form": "해보싶으신 데이트있으신가요?",
          "speaker_id": "2",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100022779.1.19",
          "form": "딱히 없어요ㅋㅋ",
          "original_form": "딱히 없어요ㅋㅋ",
