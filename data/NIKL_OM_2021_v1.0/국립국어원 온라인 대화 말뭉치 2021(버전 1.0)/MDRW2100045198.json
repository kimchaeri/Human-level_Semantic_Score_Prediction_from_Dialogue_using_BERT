      "utterance": [
        {
          "id": "MDRW2100045198.1.1",
          "form": "그쳐 ㅠㅠ 저도 해주는거 좋은데 ㅎㅎㅎ",
          "original_form": "그쳐 ㅠㅠ 저도 해주는거 좋은데 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045198.1.2",
          "form": "ㅎㅎㅎ 좀 건강한 맛 식혜 나왔으면 좋겠어요",
          "original_form": "ㅎㅎㅎ 좀 건강한 맛 식혜 나왔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045198.1.3",
          "form": "맞아여 ㅋㅋ 맛도 있고!",
          "original_form": "맞아여 ㅋㅋ 맛도 있고!",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045198.1.4",
          "form": "그러니까여!! 또 먹거리 무슨 얘기를 해야할까옄ㅋㅋ",
          "original_form": "그러니까여!! 또 먹거리 무슨 얘기를 해야할까옄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045198.1.5",
          "form": "햄버거 좋아하세요??",
          "original_form": "햄버거 좋아하세요??",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045198.1.6",
          "form": "전 롯데랴",
          "original_form": "전 롯데랴",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045198.1.7",
          "form": "클래식치즈버거",
          "original_form": "클래식치즈버거",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045198.1.8",
          "form": "맥도날드상하시치킨버거.스낵랩.",
          "original_form": "맥도날드상하시치킨버거.스낵랩.",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045198.1.9",
          "form": "좋아합니닷ㅋ",
          "original_form": "좋아합니닷ㅋ",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045198.1.10",
          "form": "맛있져 클래식 치즈버거",
          "original_form": "맛있져 클래식 치즈버거",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045198.1.11",
          "form": "저는 맥도날드는 베토디 불고기버거 좋아해요",
          "original_form": "저는 맥도날드는 베토디 불고기버거 좋아해요",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045198.1.12",
          "form": "버거킹도 좋아하는거 있으신가요??",
          "original_form": "버거킹도 좋아하는거 있으신가요??",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045198.1.13",
          "form": "버거킹은 잘안가여ㅜㅠ",
          "original_form": "버거킹은 잘안가여ㅜㅠ",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045198.1.14",
          "form": "그러시구나!! 그 맘스터치는 혹시??",
          "original_form": "그러시구나!! 그 맘스터치는 혹시??",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045198.1.15",
          "form": "맘스터치좋아해요 싸이버거",
          "original_form": "맘스터치좋아해요 싸이버거",
          "speaker_id": "2",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100045198.1.16",
          "form": "저는 할라피뇨버거!",
          "original_form": "저는 할라피뇨버거!",
          "speaker_id": "1",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100045198.1.17",
          "form": "아그거는안먹어봤는데 맛있나요?",
          "original_form": "아그거는안먹어봤는데 맛있나요?",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100045198.1.18",
          "form": "매운맛!!",
          "original_form": "매운맛!!",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100045198.1.19",
          "form": "네 살짝 매콤해서 치킨 느끼한 맛이 덜해요",
          "original_form": "네 살짝 매콤해서 치킨 느끼한 맛이 덜해요",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100045198.1.20",
          "form": "오다음에한번먹어봐야겠어요",
          "original_form": "오다음에한번먹어봐야겠어요",
