      "utterance": [
        {
          "id": "MDRW2100012708.1.1",
          "form": "ㅎㅎ 그럼 name1 이야기해주세요ㅎㅎ",
          "original_form": "ㅎㅎ 그럼 &name1& 이야기해주세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100012708.1.2",
          "form": "name1 연애세포 다시살아나게ㅎ",
          "original_form": "&name1& 연애세포 다시살아나게ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100012708.1.3",
          "form": "전 딱히 기억날만한 연애가 없어요ㅎ",
          "original_form": "전 딱히 기억날만한 연애가 없어요ㅎ",
          "speaker_id": "1",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100012708.1.4",
          "form": "연애를 많이안해봐서",
          "original_form": "연애를 많이안해봐서",
          "speaker_id": "1",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100012708.1.5",
          "form": "아그래요?ㅎ",
          "original_form": "아그래요?ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100012708.1.6",
          "form": "그래도 모쏠은 아니실꺼아니애요?",
          "original_form": "그래도 모쏠은 아니실꺼아니애요?",
          "speaker_id": "2",
          "time": "20210825 00:11"
        },
        {
          "id": "MDRW2100012708.1.7",
          "form": "어케보면 그럴수도 있고 아닐수도 있고ㅎ",
          "original_form": "어케보면 그럴수도 있고 아닐수도 있고ㅎ",
          "speaker_id": "1",
          "time": "20210825 00:11"
        },
        {
          "id": "MDRW2100012708.1.8",
          "form": "너무 대학땐 철벽치고 했더니",
          "original_form": "너무 대학땐 철벽치고 했더니",
          "speaker_id": "1",
          "time": "20210825 00:12"
        },
        {
          "id": "MDRW2100012708.1.9",
          "form": "이젠 쉽지않네요",
          "original_form": "이젠 쉽지않네요",
          "speaker_id": "1",
          "time": "20210825 00:12"
        },
        {
          "id": "MDRW2100012708.1.10",
          "form": "눈이높아서 그런건가요?ㅎ",
          "original_form": "눈이높아서 그런건가요?ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:13"
        },
        {
          "id": "MDRW2100012708.1.11",
          "form": "그렇지도 않은데ㅋ",
          "original_form": "그렇지도 않은데ㅋ",
          "speaker_id": "1",
          "time": "20210825 00:14"
        },
        {
          "id": "MDRW2100012708.1.12",
          "form": "인연이 없는건지",
          "original_form": "인연이 없는건지",
          "speaker_id": "1",
          "time": "20210825 00:14"
        },
        {
          "id": "MDRW2100012708.1.13",
          "form": "주변에 남자가 없네요ㅋ",
          "original_form": "주변에 남자가 없네요ㅋ",
          "speaker_id": "1",
          "time": "20210825 00:14"
        },
        {
          "id": "MDRW2100012708.1.14",
          "form": "인연이없는거에요ㅎ",
          "original_form": "인연이없는거에요ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:15"
        },
        {
          "id": "MDRW2100012708.1.15",
          "form": "주변에 남자는 많으실꺼에요ㅎ",
          "original_form": "주변에 남자는 많으실꺼에요ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:15"
        },
        {
          "id": "MDRW2100012708.1.16",
          "form": "자세히 안보셔서 그렇고 집에만 있으시니깐ㅎ",
          "original_form": "자세히 안보셔서 그렇고 집에만 있으시니깐ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:16"
        },
        {
          "id": "MDRW2100012708.1.17",
          "form": "어딜가야 있을까요ㅎㅎ",
          "original_form": "어딜가야 있을까요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 00:16"
        },
        {
          "id": "MDRW2100012708.1.18",
          "form": "세상의 반은 남잔데",
          "original_form": "세상의 반은 남잔데",
          "speaker_id": "1",
          "time": "20210825 00:16"
        },
        {
          "id": "MDRW2100012708.1.19",
          "form": "ㅎㅎ 너무걱정마세요ㅎ",
          "original_form": "ㅎㅎ 너무걱정마세요ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012708.1.20",
          "form": "곧나타날꺼에요ㅎ",
          "original_form": "곧나타날꺼에요ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012708.1.21",
          "form": "주위를 잘둘러보시고",
          "original_form": "주위를 잘둘러보시고",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012708.1.22",
          "form": "혹시라도 내가 놓친게 있나 한번 생각해보세요ㅎ",
          "original_form": "혹시라도 내가 놓친게 있나 한번 생각해보세요ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012708.1.23",
          "form": "과연~~~~",
          "original_form": "과연~~~~",
          "speaker_id": "1",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012708.1.24",
          "form": "남자나 여자나 똑같잖아요ㅎ",
          "original_form": "남자나 여자나 똑같잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012708.1.25",
          "form": "내가 좋아하면 시간을 그쪽으로 쓰고 행동도 다를꺼니깐ㅎ",
          "original_form": "내가 좋아하면 시간을 그쪽으로 쓰고 행동도 다를꺼니깐ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012708.1.26",
          "form": "올해도 이렇게가면 듀오라도 가입을ㅎ",
          "original_form": "올해도 이렇게가면 듀오라도 가입을ㅎ",
          "speaker_id": "1",
          "time": "20210825 00:18"
        },
        {
          "id": "MDRW2100012708.1.27",
          "form": "듀오는 아니되요ㅎㅎ",
          "original_form": "듀오는 아니되요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:19"
        },
        {
          "id": "MDRW2100012708.1.28",
          "form": "꼭 결혼을 하고싶으세요?",
          "original_form": "꼭 결혼을 하고싶으세요?",
          "speaker_id": "2",
          "time": "20210825 00:19"
        },
        {
          "id": "MDRW2100012708.1.29",
          "form": "네 전 하자주의인데 고생할바엔 혼자가 낫다 이생각이요",
          "original_form": "네 전 하자주의인데 고생할바엔 혼자가 낫다 이생각이요",
          "speaker_id": "1",
