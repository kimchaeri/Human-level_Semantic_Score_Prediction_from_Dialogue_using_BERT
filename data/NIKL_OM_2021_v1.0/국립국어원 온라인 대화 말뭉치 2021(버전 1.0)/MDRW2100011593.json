      "utterance": [
        {
          "id": "MDRW2100011593.1.1",
          "form": "며칠 전에 시켰는데 배송이 좀 늦었네",
          "original_form": "며칠 전에 시켰는데 배송이 좀 늦었네",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011593.1.2",
          "form": "주말 껴서 그렇겠지",
          "original_form": "주말 껴서 그렇겠지",
          "speaker_id": "1",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011593.1.3",
          "form": "ㅎㅎ 고추장으로 제육 해먹으면 되겠다",
          "original_form": "ㅎㅎ 고추장으로 제육 해먹으면 되겠다",
          "speaker_id": "2",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011593.1.4",
          "form": "성가정 찹쌀 고추장이 순창이랑은 비교도 안되게 비싸던데",
          "original_form": "성가정 찹쌀 고추장이 순창이랑은 비교도 안되게 비싸던데",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011593.1.5",
          "form": "그만큼 맛있으니까",
          "original_form": "그만큼 맛있으니까",
          "speaker_id": "2",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011593.1.6",
          "form": "순창 안먹어 봤잖아.. 순창도 한 번 먹어볼까?",
          "original_form": "순창 안먹어 봤잖아.. 순창도 한 번 먹어볼까?",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011593.1.7",
          "form": "지금 맛있는데 굳이 뭐하러",
          "original_form": "지금 맛있는데 굳이 뭐하러",
          "speaker_id": "2",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011593.1.8",
          "form": "그래도 가격 차이가 너무 나니까",
          "original_form": "그래도 가격 차이가 너무 나니까",
          "speaker_id": "1",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011593.1.9",
          "form": "고추장은 한 번 사면 몇 달씩 먹으니까 그냥 좀 더 써 ㅎㅎ",
          "original_form": "고추장은 한 번 사면 몇 달씩 먹으니까 그냥 좀 더 써 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011593.1.10",
          "form": "ㅎㅎ 신랑이 맛있다니 그럼 그냥 계속 이걸로...",
          "original_form": "ㅎㅎ 신랑이 맛있다니 그럼 그냥 계속 이걸로...",
          "speaker_id": "1",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011593.1.11",
          "form": "내가 먹어보니 비빔밥이고 제육이고 완전 맛이 달라",
          "original_form": "내가 먹어보니 비빔밥이고 제육이고 완전 맛이 달라",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011593.1.12",
          "form": "먹을 때마다 고추장 맛있다고 하더니 진짜 맛있나보네 ㅎㅎ",
          "original_form": "먹을 때마다 고추장 맛있다고 하더니 진짜 맛있나보네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011593.1.13",
          "form": "ㅇㅇ 진짜 맛있어",
          "original_form": "ㅇㅇ 진짜 맛있어",
          "speaker_id": "2",
