      "utterance": [
        {
          "id": "MDRW2100014307.1.1",
          "form": "저녁 드셨나요 ~",
          "original_form": "저녁 드셨나요 ~",
          "speaker_id": "1",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014307.1.2",
          "form": "아직 안먹었어요! 점심을 늦게 먹어서요",
          "original_form": "아직 안먹었어요! 점심을 늦게 먹어서요",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014307.1.3",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014307.1.4",
          "form": "니 감바스요! 점심 뭐드셨어요",
          "original_form": "니 감바스요! 점심 뭐드셨어요",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014307.1.5",
          "form": "네*",
          "original_form": "네*",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014307.1.6",
          "form": "감바스 맛있죠 ㅠㅠ 저도 좋아해요",
          "original_form": "감바스 맛있죠 ㅠㅠ 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014307.1.7",
          "form": "점심에 피자먹었어요ㅎㅎ",
          "original_form": "점심에 피자먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014307.1.8",
          "form": "오 혹시도미노",
          "original_form": "오 혹시도미노",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014307.1.9",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014307.1.10",
          "form": "옐로우피자 먹었어요!",
          "original_form": "옐로우피자 먹었어요!",
          "speaker_id": "2",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014307.1.11",
          "form": "도미노도 맛있죠ㅎㅎ",
          "original_form": "도미노도 맛있죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014307.1.12",
          "form": "앜ㅋㅋ 오늘 도미노 skt 행사잇어서 그거드셨나했어요",
          "original_form": "앜ㅋㅋ 오늘 도미노 skt 행사잇어서 그거드셨나했어요",
          "speaker_id": "1",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014307.1.13",
          "form": "맛있었겠다ㅠㅠ저녁은 뭐드시게요 ?",
          "original_form": "맛있었겠다ㅠㅠ저녁은 뭐드시게요 ?",
          "speaker_id": "1",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014307.1.14",
          "form": "점심을 너무 고칼로리를 먹어서 간단한거 먹을까 생각중이에요!",
          "original_form": "점심을 너무 고칼로리를 먹어서 간단한거 먹을까 생각중이에요!",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014307.1.15",
          "form": "ㅋㅋㅋ 집에서 드시나요",
          "original_form": "ㅋㅋㅋ 집에서 드시나요",
          "speaker_id": "1",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014307.1.16",
          "form": "네네 집에서 먹으려구요",
          "original_form": "네네 집에서 먹으려구요",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014307.1.17",
          "form": "집에서 요리자주 해드세요?",
          "original_form": "집에서 요리자주 해드세요?",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014307.1.18",
          "form": "넹 주로 해먹고있어요...ㅈ반조리식품의 힘을 빌려서 ㅋㅋ",
          "original_form": "넹 주로 해먹고있어요...ㅈ반조리식품의 힘을 빌려서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014307.1.19",
          "form": "맞아요 요즘 반조리식품이 잘나와서 좋더라구요ㅎㅎ",
          "original_form": "맞아요 요즘 반조리식품이 잘나와서 좋더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014307.1.20",
          "form": "만두 먹고싶네요",
          "original_form": "만두 먹고싶네요",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
