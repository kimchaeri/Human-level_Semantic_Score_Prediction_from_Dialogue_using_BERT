      "utterance": [
        {
          "id": "MDRW2100011053.1.1",
          "form": "음 나는 유기견센터에 가고싶어",
          "original_form": "음 나는 유기견센터에 가고싶어",
          "speaker_id": "2",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011053.1.2",
          "form": "오 급식. 대단하다",
          "original_form": "오 급식. 대단하다",
          "speaker_id": "2",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011053.1.3",
          "form": "나는 학교에서 급식당번일때도 힘들었어",
          "original_form": "나는 학교에서 급식당번일때도 힘들었어",
          "speaker_id": "2",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011053.1.4",
          "form": "손이 너무 아프지않니?",
          "original_form": "손이 너무 아프지않니?",
          "speaker_id": "2",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011053.1.5",
          "form": "ㅠㅠ맞아 국을 나눠주는건 힘들었어",
          "original_form": "ㅠㅠ맞아 국을 나눠주는건 힘들었어",
          "speaker_id": "1",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011053.1.6",
          "form": "나는 귀여운 동물들을 위해 일하는 게 좋아",
          "original_form": "나는 귀여운 동물들을 위해 일하는 게 좋아",
          "speaker_id": "2",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011053.1.7",
          "form": "그래도 하고나면 보람찰 것 같아",
          "original_form": "그래도 하고나면 보람찰 것 같아",
          "speaker_id": "1",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011053.1.8",
          "form": "맞아. 뜨겁고 더워",
          "original_form": "맞아. 뜨겁고 더워",
          "speaker_id": "2",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011053.1.9",
          "form": "손아파.",
          "original_form": "손아파.",
          "speaker_id": "2",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011053.1.10",
          "form": "맞아 동물들도 좋지",
          "original_form": "맞아 동물들도 좋지",
          "speaker_id": "1",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011053.1.11",
          "form": "그건그래.",
          "original_form": "그건그래.",
          "speaker_id": "2",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011053.1.12",
          "form": "근데 조금 냄새가 날 것 같기도해",
          "original_form": "근데 조금 냄새가 날 것 같기도해",
          "speaker_id": "1",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011053.1.13",
          "form": "빠른시일내에 급식봉사를 하는 건 어때?",
          "original_form": "빠른시일내에 급식봉사를 하는 건 어때?",
          "speaker_id": "2",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100011053.1.14",
          "form": "뮬론 그건 동물들 탓이 아니야",
          "original_form": "뮬론 그건 동물들 탓이 아니야",
          "speaker_id": "1",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100011053.1.15",
          "form": "나는 인간보단 동물이 좋아",
          "original_form": "나는 인간보단 동물이 좋아",
          "speaker_id": "2",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100011053.1.16",
          "form": "그래서 괜찮아",
          "original_form": "그래서 괜찮아",
          "speaker_id": "2",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100011053.1.17",
          "form": "나도 그건 그래",
          "original_form": "나도 그건 그래",
          "speaker_id": "1",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100011053.1.18",
          "form": "동물들은 죄가 없어",
          "original_form": "동물들은 죄가 없어",
          "speaker_id": "2",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100011053.1.19",
          "form": "사람에게 상처받은 동물들이 많이 올거야.",
          "original_form": "사람에게 상처받은 동물들이 많이 올거야.",
          "speaker_id": "2",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100011053.1.20",
          "form": "ㅠㅠ좀 더 유기견 관련 봉사가 많아졌으면 좋겠다",
          "original_form": "ㅠㅠ좀 더 유기견 관련 봉사가 많아졌으면 좋겠다",
          "speaker_id": "1",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100011053.1.21",
          "form": "정말 슬프다",
          "original_form": "정말 슬프다",
          "speaker_id": "2",
          "time": "20210824 00:12"
        }
      ]
