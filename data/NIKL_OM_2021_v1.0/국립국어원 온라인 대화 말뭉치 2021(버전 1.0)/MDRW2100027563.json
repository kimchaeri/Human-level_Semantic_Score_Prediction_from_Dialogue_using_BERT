      "utterance": [
        {
          "id": "MDRW2100027563.1.1",
          "form": "유튜브 보시나요?",
          "original_form": "유튜브 보시나요?",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027563.1.2",
          "form": "네네 주로 뭐 보세요?",
          "original_form": "네네 주로 뭐 보세요?",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027563.1.3",
          "form": "게임방송이랑 무한도전 다시보기요",
          "original_form": "게임방송이랑 무한도전 다시보기요",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027563.1.4",
          "form": "오 저도 게임 완전 좋아하는데",
          "original_form": "오 저도 게임 완전 좋아하는데",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027563.1.5",
          "form": "누구꺼 보세요",
          "original_form": "누구꺼 보세요",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027563.1.6",
          "form": "풍월량님꺼요 욕을 안하셔서 좋아요",
          "original_form": "풍월량님꺼요 욕을 안하셔서 좋아요",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027563.1.7",
          "form": "헐 풍뎅이다",
          "original_form": "헐 풍뎅이다",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027563.1.8",
          "form": "풍뎅이 알죠?",
          "original_form": "풍뎅이 알죠?",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027563.1.9",
          "form": "소름돋음 ㅋㅋㅋㅋ",
          "original_form": "소름돋음 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027563.1.10",
          "form": "저도 풍월량님꺼 젤 좋아해여 ㅠㅠ",
          "original_form": "저도 풍월량님꺼 젤 좋아해여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027563.1.11",
          "form": "악 관련없는 대화는 안하는게 좋아요 풍뎅이 맞아요",
          "original_form": "악 관련없는 대화는 안하는게 좋아요 풍뎅이 맞아요",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027563.1.12",
          "form": "유투브 관련이라 괜찮지않을까요 ㅎㅎ",
          "original_form": "유투브 관련이라 괜찮지않을까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027563.1.13",
          "form": "애매해서요 최근 좀보이드 재미나게 하시던데 저도 좀보이드 요즘 2시간정도 하는것 같아요",
          "original_form": "애매해서요 최근 좀보이드 재미나게 하시던데 저도 좀보이드 요즘 2시간정도 하는것 같아요",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027563.1.14",
          "form": "오 직접 하시기도 하시나봐요",
          "original_form": "오 직접 하시기도 하시나봐요",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027563.1.15",
          "form": "네 예전부터 하던 게임이긴했는데 업데이트가 늦어져서...",
          "original_form": "네 예전부터 하던 게임이긴했는데 업데이트가 늦어져서...",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027563.1.16",
          "form": "저 그거 포인트 걸었다가 다잃었어요 ㅠㅠ",
          "original_form": "저 그거 포인트 걸었다가 다잃었어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027563.1.17",
          "form": "저는 이번에 첨 알게됐다는",
          "original_form": "저는 이번에 첨 알게됐다는",
          "speaker_id": "2",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027563.1.18",
          "form": "전 생방을 주로보지않아서 아 그리고 마썸카 재미나게봤어요",
          "original_form": "전 생방을 주로보지않아서 아 그리고 마썸카 재미나게봤어요",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027563.1.19",
          "form": "마썸카 진짜 유투브 돌아가면서 다보는중이에요 ㅠㅠㅠㅠ",
          "original_form": "마썸카 진짜 유투브 돌아가면서 다보는중이에요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027563.1.20",
          "form": "유투브 마썸카 다 찾아 보는중이에요 ㅠㅠ",
          "original_form": "유투브 마썸카 다 찾아 보는중이에요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100027563.1.21",
          "form": "요즘은 쉐리님꺼 보는중",
          "original_form": "요즘은 쉐리님꺼 보는중",
          "speaker_id": "2",
          "time": "20210902 13:42"
        }
      ]
