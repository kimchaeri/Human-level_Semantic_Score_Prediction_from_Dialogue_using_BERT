      "utterance": [
        {
          "id": "MDRW2100021736.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100021736.1.2",
          "form": "안녕하신가",
          "original_form": "안녕하신가",
          "speaker_id": "2",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021736.1.3",
          "form": "쇼핑조아하시나요",
          "original_form": "쇼핑조아하시나요",
          "speaker_id": "1",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021736.1.4",
          "form": "쇼핑 좋아하는데 지갑이 얇네요..",
          "original_form": "쇼핑 좋아하는데 지갑이 얇네요..",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021736.1.5",
          "form": "아아.. 결제는 다음달의 내가ㅎㅎ",
          "original_form": "아아.. 결제는 다음달의 내가ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021736.1.6",
          "form": "그때의 나는 직장 잘 다니고 있을까요..ㅠㅠ",
          "original_form": "그때의 나는 직장 잘 다니고 있을까요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021736.1.7",
          "form": "ㅠㅠㅠ최근에 뭐사셨나여",
          "original_form": "ㅠㅠㅠ최근에 뭐사셨나여",
          "speaker_id": "1",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021736.1.8",
          "form": "저 무라벨 생수 샀어요~",
          "original_form": "저 무라벨 생수 샀어요~",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021736.1.9",
          "form": "요즘 생수 핫한거 아시죠?ㅎㅎ",
          "original_form": "요즘 생수 핫한거 아시죠?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021736.1.10",
          "form": "헉 처음들어봤어요",
          "original_form": "헉 처음들어봤어요",
          "speaker_id": "1",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021736.1.11",
          "form": "어머 진짜요~?",
          "original_form": "어머 진짜요~?",
          "speaker_id": "2",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021736.1.12",
          "form": "요즘 제로웨이스트 실천한다구 비닐이랑 플라스틱도 줄이잖아요",
          "original_form": "요즘 제로웨이스트 실천한다구 비닐이랑 플라스틱도 줄이잖아요",
          "speaker_id": "2",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021736.1.13",
          "form": "아아 라벨이 없는거군요ㅎㅎ",
          "original_form": "아아 라벨이 없는거군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100021736.1.14",
          "form": "네 맞아요 ㅎㅎ 그나저나 생수는 수질논란 있던데요",
          "original_form": "네 맞아요 ㅎㅎ 그나저나 생수는 수질논란 있던데요",
          "speaker_id": "2",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100021736.1.15",
          "form": "헉 사셨는데 어쩌죠ㅠ",
          "original_form": "헉 사셨는데 어쩌죠ㅠ",
          "speaker_id": "1",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021736.1.16",
          "form": "다행히 제가 알고 있어서 수질논란 피해서 샀어요!",
          "original_form": "다행히 제가 알고 있어서 수질논란 피해서 샀어요!",
          "speaker_id": "2",
          "time": "20210828 22:35"
        },
        {
          "id": "MDRW2100021736.1.17",
          "form": "평창수랑 삼다수 등은 괜찮대요!",
          "original_form": "평창수랑 삼다수 등은 괜찮대요!",
          "speaker_id": "2",
          "time": "20210828 22:35"
        }
