      "utterance": [
        {
          "id": "MDRW2100019657.1.1",
          "form": "반려동물이 있나요?",
          "original_form": "반려동물이 있나요?",
          "speaker_id": "1",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100019657.1.2",
          "form": "넷 저 고양이 한마리 키운답니다",
          "original_form": "넷 저 고양이 한마리 키운답니다",
          "speaker_id": "2",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100019657.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100019657.1.4",
          "form": "집사시군요 ㅎ 전 없어요 ㅠㅠ",
          "original_form": "집사시군요 ㅎ 전 없어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100019657.1.5",
          "form": "저런...",
          "original_form": "저런...",
          "speaker_id": "2",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019657.1.6",
          "form": "용돈을 아끼실 수 있겠어요",
          "original_form": "용돈을 아끼실 수 있겠어요",
          "speaker_id": "2",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019657.1.7",
          "form": "저는 돈이 다 고양이에게로 들어간답니다...",
          "original_form": "저는 돈이 다 고양이에게로 들어간답니다...",
          "speaker_id": "2",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019657.1.8",
          "form": "그래도 사랑스럽죠? 반려동물 키우시는 분들 대단해요",
          "original_form": "그래도 사랑스럽죠? 반려동물 키우시는 분들 대단해요",
          "speaker_id": "1",
          "time": "20210827 21:03"
        },
        {
          "id": "MDRW2100019657.1.9",
          "form": "맞아요 이 조그만게 뭐라고 귀엽네요ㅋㅋㅋ",
          "original_form": "맞아요 이 조그만게 뭐라고 귀엽네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100019657.1.10",
          "form": "이제 키운지 11달 됐어요",
          "original_form": "이제 키운지 11달 됐어요",
          "speaker_id": "2",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100019657.1.11",
          "form": "좀 있으면 1년이네요ㅎㅎ 이름 물어봐도 될까요?",
          "original_form": "좀 있으면 1년이네요ㅎㅎ 이름 물어봐도 될까요?",
          "speaker_id": "1",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100019657.1.12",
          "form": "이름 name3이에요ㅋㅋㅋ",
          "original_form": "이름 &name3&이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100019657.1.13",
          "form": "이름 말할 때마다 친구들 반응이 안좋은데",
          "original_form": "이름 말할 때마다 친구들 반응이 안좋은데",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100019657.1.14",
          "form": "name3이 이름이 이상한가요??ㅋㅋㅋㅠ",
          "original_form": "&name3&이 이름이 이상한가요??ㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019657.1.15",
          "form": "찰떡인데",
          "original_form": "찰떡인데",
          "speaker_id": "2",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019657.1.16",
          "form": "집사랑 냥이만 좋으면 됐죠 까만 냥인가봐요?",
          "original_form": "집사랑 냥이만 좋으면 됐죠 까만 냥인가봐요?",
          "speaker_id": "1",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100019657.1.17",
          "form": "맞아요 입에도 착붙는 이름이에요",
          "original_form": "맞아요 입에도 착붙는 이름이에요",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019657.1.18",
          "form": "네 새카만 냥이에요ㅋㅋㅋ",
          "original_form": "네 새카만 냥이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019657.1.19",
          "form": "오 추측이 맞았네요ㅎㅎ",
          "original_form": "오 추측이 맞았네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019657.1.20",
          "form": "ㅎㅎㅎ반려동물 키우실 생각은 없으세요?",
          "original_form": "ㅎㅎㅎ반려동물 키우실 생각은 없으세요?",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019657.1.21",
          "form": "네 제가 신경 잘 못 써 줄 것 같아서요 보는 걸로 만족하고 있어요",
          "original_form": "네 제가 신경 잘 못 써 줄 것 같아서요 보는 걸로 만족하고 있어요",
