      "utterance": [
        {
          "id": "MDRW2100042281.1.1",
          "form": "응 그래야지ㅋㅋ 다음 장날 납작만두 도전할거니?ㅋㅋ",
          "original_form": "응 그래야지ㅋㅋ 다음 장날 납작만두 도전할거니?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042281.1.2",
          "form": "비 안오면 ㅋㅋㅋ",
          "original_form": "비 안오면 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042281.1.3",
          "form": "이번 장날엔 나오셔야 할텐데ㅠ",
          "original_form": "이번 장날엔 나오셔야 할텐데ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042281.1.4",
          "form": "추석 가까워오는 장이라 오시겠지",
          "original_form": "추석 가까워오는 장이라 오시겠지",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042281.1.5",
          "form": "이토록 바래 본 적이 없닽ㅋㅋㅋㅋ",
          "original_form": "이토록 바래 본 적이 없닽ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042281.1.6",
          "form": "9일에 또 가봐야하나ㅋㅋ",
          "original_form": "9일에 또 가봐야하나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042281.1.7",
          "form": "이니스프리 앰플 수령은 10일부터네",
          "original_form": "이니스프리 앰플 수령은 10일부터네",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042281.1.8",
          "form": "한큐에 해결이 안 되네",
          "original_form": "한큐에 해결이 안 되네",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042281.1.9",
          "form": "부지런히 움직이자ㅋㅋㅋ",
          "original_form": "부지런히 움직이자ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042281.1.10",
          "form": "9일 가는길에 지원금 신청해라",
          "original_form": "9일 가는길에 지원금 신청해라",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042281.1.11",
          "form": "귀찮구나",
          "original_form": "귀찮구나",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042281.1.12",
          "form": "정부지원금은 신청했니",
          "original_form": "정부지원금은 신청했니",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042281.1.13",
          "form": "했지했지 ㅋㅋㅋㅋ맛있는거 사무야지",
          "original_form": "했지했지 ㅋㅋㅋㅋ맛있는거 사무야지",
          "speaker_id": "1",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042281.1.14",
          "form": "내일 지원금 개시하러 가야디ㅋㅋㅋ  더벤티에 아인슈페너 사무러 가까ㅋㅋ",
          "original_form": "내일 지원금 개시하러 가야디ㅋㅋㅋ  더벤티에 아인슈페너 사무러 가까ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100042281.1.15",
          "form": "아 거기 아인슈페너도 파나 ㅋㅋㅋ",
          "original_form": "아 거기 아인슈페너도 파나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100042281.1.16",
          "form": "더벤티 나름 아인슈페너 맛집이여ㅋㅋ",
          "original_form": "더벤티 나름 아인슈페너 맛집이여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:42"
        },
        {
          "id": "MDRW2100042281.1.17",
          "form": "의외네 손 많이 가는 메뉴라 그런데서 안 팔줄 ㅋㅋㅋ",
          "original_form": "의외네 손 많이 가는 메뉴라 그런데서 안 팔줄 ㅋㅋㅋ",
