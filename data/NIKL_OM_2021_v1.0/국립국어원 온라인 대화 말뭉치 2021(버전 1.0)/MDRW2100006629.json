      "utterance": [
        {
          "id": "MDRW2100006629.1.1",
          "form": "순한맛 보자마자 말문이 턱 막혔습니다",
          "original_form": "순한맛 보자마자 말문이 턱 막혔습니다",
          "speaker_id": "1",
          "time": "20210908 03:48"
        },
        {
          "id": "MDRW2100006629.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:49"
        },
        {
          "id": "MDRW2100006629.1.3",
          "form": "일단 못먹는 음식 1스택 쌓으셨고...ㅋㅋㅋㅋㅋ",
          "original_form": "일단 못먹는 음식 1스택 쌓으셨고...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:50"
        },
        {
          "id": "MDRW2100006629.1.4",
          "form": "급식에 나오는 코다리 어떠세요",
          "original_form": "급식에 나오는 코다리 어떠세요",
          "speaker_id": "2",
          "time": "20210908 03:51"
        },
        {
          "id": "MDRW2100006629.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 전 그런건 가리지 않고 먹습니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 전 그런건 가리지 않고 먹습니다",
          "speaker_id": "1",
          "time": "20210908 03:51"
        },
        {
          "id": "MDRW2100006629.1.6",
          "form": "순한맛이 정말 강적이었어요 ㅋㅋㅋㅋ",
          "original_form": "순한맛이 정말 강적이었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:51"
        },
        {
          "id": "MDRW2100006629.1.7",
          "form": "아닛 급식 코다리보다 진순이 강적이라구요?",
          "original_form": "아닛 급식 코다리보다 진순이 강적이라구요?",
          "speaker_id": "2",
          "time": "20210908 03:51"
        },
        {
          "id": "MDRW2100006629.1.8",
          "form": "더 센게 뭐가 있으려나..",
          "original_form": "더 센게 뭐가 있으려나..",
          "speaker_id": "2",
          "time": "20210908 03:51"
        },
        {
          "id": "MDRW2100006629.1.9",
          "form": "고수 잘 드시나요?",
          "original_form": "고수 잘 드시나요?",
          "speaker_id": "2",
          "time": "20210908 03:52"
        },
        {
          "id": "MDRW2100006629.1.10",
          "form": "중국 여행도 가서 열심히 먹고왔답니다",
          "original_form": "중국 여행도 가서 열심히 먹고왔답니다",
          "speaker_id": "1",
          "time": "20210908 03:52"
        },
        {
          "id": "MDRW2100006629.1.11",
          "form": "고수도 먹는 사람인데 진라면 순한맛은 정말...",
          "original_form": "고수도 먹는 사람인데 진라면 순한맛은 정말...",
          "speaker_id": "1",
          "time": "20210908 03:52"
        },
        {
          "id": "MDRW2100006629.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:52"
        },
        {
          "id": "MDRW2100006629.1.13",
          "form": "아닠ㅋㅋㅋㅋ 난이도 설정이 이상한 거 같은데요ㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋ 난이도 설정이 이상한 거 같은데요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:54"
        },
        {
          "id": "MDRW2100006629.1.14",
          "form": "중국 여행을 가셨다면, 고단백 곤충은 어떻습니까",
          "original_form": "중국 여행을 가셨다면, 고단백 곤충은 어떻습니까",
          "speaker_id": "2",
          "time": "20210908 03:54"
        },
        {
          "id": "MDRW2100006629.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ name2님 정말 대단하신 분이군요 진라면 순한맛이라니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ &name2&님 정말 대단하신 분이군요 진라면 순한맛이라니",
          "speaker_id": "1",
          "time": "20210908 03:54"
        },
        {
          "id": "MDRW2100006629.1.16",
          "form": "전갈 드셔보셨어요?? 메뚜기는요?",
          "original_form": "전갈 드셔보셨어요?? 메뚜기는요?",
          "speaker_id": "1",
          "time": "20210908 03:54"
        },
        {
          "id": "MDRW2100006629.1.17",
          "form": "전 그럴 가위바위보를 져서 먹었답니다",
          "original_form": "전 그럴 가위바위보를 져서 먹었답니다",
          "speaker_id": "1",
          "time": "20210908 03:55"
        },
        {
          "id": "MDRW2100006629.1.18",
          "form": "메뚜기는 못먹어봤는데 전갈은 먹어봤어요!",
          "original_form": "메뚜기는 못먹어봤는데 전갈은 먹어봤어요!",
          "speaker_id": "2",
          "time": "20210908 03:55"
        },
        {
          "id": "MDRW2100006629.1.19",
          "form": "정말 저희 안 먹는게 없군요",
          "original_form": "정말 저희 안 먹는게 없군요",
          "speaker_id": "1",
          "time": "20210908 03:56"
        },
        {
          "id": "MDRW2100006629.1.20",
          "form": "앗..진순이는..",
          "original_form": "앗..진순이는..",
          "speaker_id": "1",
          "time": "20210908 03:56"
        },
        {
          "id": "MDRW2100006629.1.21",
          "form": "비주얼이 너무나도 극악이라 그렇지 맛은 나쁘지 않았어요 고소하니",
          "original_form": "비주얼이 너무나도 극악이라 그렇지 맛은 나쁘지 않았어요 고소하니",
          "speaker_id": "2",
          "time": "20210908 03:56"
        },
        {
          "id": "MDRW2100006629.1.22",
          "form": "신라면도 못먹는 맵찔이는 진순이 맛있다구욧...",
          "original_form": "신라면도 못먹는 맵찔이는 진순이 맛있다구욧...",
          "speaker_id": "2",
          "time": "20210908 03:56"
        },
        {
          "id": "MDRW2100006629.1.23",
          "form": "ㅋㅋㅋㅋ인정해드릴게요",
          "original_form": "ㅋㅋㅋㅋ인정해드릴게요",
          "speaker_id": "1",
          "time": "20210908 03:57"
        },
        {
          "id": "MDRW2100006629.1.24",
          "form": "아까 민초 안 드신다고 하셨죠?",
          "original_form": "아까 민초 안 드신다고 하셨죠?",
          "speaker_id": "1",
          "time": "20210908 03:57"
        },
        {
          "id": "MDRW2100006629.1.25",
          "form": "아니 어떻게 그 사랑스러운걸 안 사드시죠?",
          "original_form": "아니 어떻게 그 사랑스러운걸 안 사드시죠?",
          "speaker_id": "1",
          "time": "20210908 03:57"
        },
        {
          "id": "MDRW2100006629.1.26",
          "form": "아 안먹는 건 아니지만 구우우우우욷이 제 돈 주곤 안사먹는 겁니다ㅎㅎ",
          "original_form": "아 안먹는 건 아니지만 구우우우우욷이 제 돈 주곤 안사먹는 겁니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 03:58"
        },
        {
          "id": "MDRW2100006629.1.27",
          "form": "혐오는 아니라구요?ㅋㅋㅋㅋ",
          "original_form": "혐오는 아니라구요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:58"
        },
        {
          "id": "MDRW2100006629.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ인정해드리죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ인정해드리죠",
          "speaker_id": "1",
          "time": "20210908 03:58"
        },
        {
          "id": "MDRW2100006629.1.29",
          "form": "전 민초 먹을 돈으로 레샤를 먹습니다ㅎㅎ",
          "original_form": "전 민초 먹을 돈으로 레샤를 먹습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 03:59"
        },
        {
          "id": "MDRW2100006629.1.30",
          "form": "레샤라 이것 참 피자먹고 베라를 가야하나",
          "original_form": "레샤라 이것 참 피자먹고 베라를 가야하나",
          "speaker_id": "1",
          "time": "20210908 03:59"
        },
        {
          "id": "MDRW2100006629.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ새벽이라 배만 고프네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ새벽이라 배만 고프네요",
          "speaker_id": "1",
          "time": "20210908 03:59"
        },
        {
          "id": "MDRW2100006629.1.32",
          "form": "레샤는 역시 호불호가 갈리지 않죠",
          "original_form": "레샤는 역시 호불호가 갈리지 않죠",
