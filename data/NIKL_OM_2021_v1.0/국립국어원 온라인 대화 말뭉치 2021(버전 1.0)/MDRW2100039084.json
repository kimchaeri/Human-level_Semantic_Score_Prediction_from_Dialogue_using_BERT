      "utterance": [
        {
          "id": "MDRW2100039084.1.1",
          "form": "네 카페인때문에 많이는 못먹어요ㅜㅠ",
          "original_form": "네 카페인때문에 많이는 못먹어요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039084.1.2",
          "form": "저도 그래서 아침에 겨우 한잔정도가 답니다",
          "original_form": "저도 그래서 아침에 겨우 한잔정도가 답니다",
          "speaker_id": "1",
          "time": "20210903 09:48"
        },
        {
          "id": "MDRW2100039084.1.3",
          "form": "더 마시거나 늦게 마시면 못자요",
          "original_form": "더 마시거나 늦게 마시면 못자요",
          "speaker_id": "1",
          "time": "20210903 09:48"
        },
        {
          "id": "MDRW2100039084.1.4",
          "form": "커피먹음 벌렁대요ㅜㅠ",
          "original_form": "커피먹음 벌렁대요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100039084.1.5",
          "form": "심장이ㅜ",
          "original_form": "심장이ㅜ",
          "speaker_id": "2",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100039084.1.6",
          "form": "오.. 카페인에 약하군요",
          "original_form": "오.. 카페인에 약하군요",
          "speaker_id": "1",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039084.1.7",
          "form": "전 몸에서 티는 안나는데.. 잠을 못자요",
          "original_form": "전 몸에서 티는 안나는데.. 잠을 못자요",
          "speaker_id": "1",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039084.1.8",
          "form": "먹을 땐 좋은데 너무 아쉬워요",
          "original_form": "먹을 땐 좋은데 너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039084.1.9",
          "form": "네 저번에 커피 늦게 먹고 누웠는데 지진난줄 알았어요ㅋㅋ",
          "original_form": "네 저번에 커피 늦게 먹고 누웠는데 지진난줄 알았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039084.1.10",
          "form": "일할땐 꼭 마셔야해요ㅋㅋ",
          "original_form": "일할땐 꼭 마셔야해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039084.1.11",
          "form": "아이고.. 그런 부작용이 있는데도 일할 때 마셔줘야 하는 군요",
          "original_form": "아이고.. 그런 부작용이 있는데도 일할 때 마셔줘야 하는 군요",
          "speaker_id": "1",
          "time": "20210903 19:42"
        },
        {
          "id": "MDRW2100039084.1.12",
          "form": "ㅋㅋ졸릴따 직방이에요ㅋㅋ",
          "original_form": "ㅋㅋ졸릴따 직방이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:49"
        },
        {
          "id": "MDRW2100039084.1.13",
          "form": "오 그런 점이 있네요..",
          "original_form": "오 그런 점이 있네요..",
          "speaker_id": "1",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100039084.1.14",
          "form": "전 잠을 이겨야해서 마셔야하는 경우가 거의 없어서",
          "original_form": "전 잠을 이겨야해서 마셔야하는 경우가 거의 없어서",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039084.1.15",
          "form": "잠 못자는 건 무조건 부작용이라고 생각했는데",
          "original_form": "잠 못자는 건 무조건 부작용이라고 생각했는데",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039084.1.16",
          "form": "저도 한땐 그렇게 거절하고 다녔는데 이젠 맛있기까지해요ㅋㅋ",
          "original_form": "저도 한땐 그렇게 거절하고 다녔는데 이젠 맛있기까지해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100039084.1.17",
          "form": "커피맛을 알게되면 빠져나오기 힘들죠",
          "original_form": "커피맛을 알게되면 빠져나오기 힘들죠",
          "speaker_id": "1",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039084.1.18",
          "form": "고소한거 좋아하세요? 산미있는거 좋아하세요?",
          "original_form": "고소한거 좋아하세요? 산미있는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039084.1.19",
          "form": "산미는 별로에요.. 근데 산미가 있는게 신선하고 고급진거라면서요?",
          "original_form": "산미는 별로에요.. 근데 산미가 있는게 신선하고 고급진거라면서요?",
          "speaker_id": "1",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039084.1.20",
          "form": "근데 잘하는 곳 가면 아! 이래서 산미있는걸 먹는구나싶어요!",
          "original_form": "근데 잘하는 곳 가면 아! 이래서 산미있는걸 먹는구나싶어요!",
          "speaker_id": "2",
          "time": "20210903 19:59"
        }
      ]
