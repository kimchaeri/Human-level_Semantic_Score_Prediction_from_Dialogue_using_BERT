      "utterance": [
        {
          "id": "MDRW2100021823.1.1",
          "form": "ㅋㅋㅋ다이어트 성공적이었겠네요",
          "original_form": "ㅋㅋㅋ다이어트 성공적이었겠네요",
          "speaker_id": "2",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100021823.1.2",
          "form": "전 맨날 다이소에서 수납바구니같은거만 사와서 방에 수납이 참 복잡해욮ㅋㅋㅋㅋ",
          "original_form": "전 맨날 다이소에서 수납바구니같은거만 사와서 방에 수납이 참 복잡해욮ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:47"
        },
        {
          "id": "MDRW2100021823.1.3",
          "form": "수납정리 하려고 산 바구니 때문에",
          "original_form": "수납정리 하려고 산 바구니 때문에",
          "speaker_id": "1",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021823.1.4",
          "form": "오히려 복잡해졌다구요??",
          "original_form": "오히려 복잡해졌다구요??",
          "speaker_id": "1",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021823.1.5",
          "form": "네 ㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021823.1.6",
          "form": "물건을 수납하기 위해 수납장을 사지말고",
          "original_form": "물건을 수납하기 위해 수납장을 사지말고",
          "speaker_id": "2",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021823.1.7",
          "form": "걍 비워라",
          "original_form": "걍 비워라",
          "speaker_id": "2",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021823.1.8",
          "form": "ㅋㅋㅋㅋ버리는게 최고다",
          "original_form": "ㅋㅋㅋㅋ버리는게 최고다",
          "speaker_id": "1",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021823.1.9",
          "form": "이게 미니멀리스트의 지름길인거에요",
          "original_form": "이게 미니멀리스트의 지름길인거에요",
          "speaker_id": "2",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021823.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021823.1.11",
          "form": "그렇지만 저는 언젠간 쓰겠지 하고 안버립니다",
          "original_form": "그렇지만 저는 언젠간 쓰겠지 하고 안버립니다",
          "speaker_id": "1",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021823.1.12",
          "form": "저도 미니멀리스트의 정신을 배워야할까봐요",
          "original_form": "저도 미니멀리스트의 정신을 배워야할까봐요",
          "speaker_id": "1",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100021823.1.13",
          "form": "맞아요 근데 버리면",
          "original_form": "맞아요 근데 버리면",
          "speaker_id": "2",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100021823.1.14",
          "form": "언젠가 쓸일이 생겨서",
          "original_form": "언젠가 쓸일이 생겨서",
          "speaker_id": "2",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100021823.1.15",
          "form": "저 심지어 배달하면 딸려오는 거 다 보관해요..",
          "original_form": "저 심지어 배달하면 딸려오는 거 다 보관해요..",
          "speaker_id": "1",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100021823.1.16",
          "form": "다시 사야해요",
          "original_form": "다시 사야해요",
          "speaker_id": "2",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100021823.1.17",
          "form": "맛소금, 간장, 소스, 젓가락, 고무줄 등등 ㅋㅋㅋ",
          "original_form": "맛소금, 간장, 소스, 젓가락, 고무줄 등등 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100021823.1.18",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100021823.1.19",
          "form": "그래서 저번에 냉장고 보니 유통기한 지난게 한가득 ㅠㅠ",
          "original_form": "그래서 저번에 냉장고 보니 유통기한 지난게 한가득 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100021823.1.20",
          "form": "핫소스 유통기한 지날 정도로 피자를 안먹었구나.. 하고 반성했어요.",
          "original_form": "핫소스 유통기한 지날 정도로 피자를 안먹었구나.. 하고 반성했어요.",
          "speaker_id": "1",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100021823.1.21",
          "form": "그니까요 저도 많이 쌓다가..",
          "original_form": "그니까요 저도 많이 쌓다가..",
          "speaker_id": "2",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021823.1.22",
          "form": "피자는 더 자주 시켜드시지 그랬오요 ..",
          "original_form": "피자는 더 자주 시켜드시지 그랬오요 ..",
          "speaker_id": "2",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021823.1.23",
          "form": "반성합니다..",
          "original_form": "반성합니다..",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021823.1.24",
          "form": "그렇지만 피자 너무 비싼걸요",
          "original_form": "그렇지만 피자 너무 비싼걸요",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021823.1.25",
          "form": "피자같은 완전식품..",
          "original_form": "피자같은 완전식품..",
          "speaker_id": "2",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021823.1.26",
          "form": "50%할인해도 치킨보다 더 비싸요..",
          "original_form": "50%할인해도 치킨보다 더 비싸요..",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021823.1.27",
          "form": "그 같이 나눠먹을 사람 없나요",
          "original_form": "그 같이 나눠먹을 사람 없나요",
          "speaker_id": "2",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021823.1.28",
          "form": "왜 같이 나눠먹어요 이 비싼거를",
          "original_form": "왜 같이 나눠먹어요 이 비싼거를",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021823.1.29",
          "form": "더 치",
          "original_form": "더 치",
          "speaker_id": "2",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021823.1.30",
          "form": "페 이",
          "original_form": "페 이",
          "speaker_id": "2",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100021823.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100021823.1.32",
          "form": "그것은",
          "original_form": "그것은",
          "speaker_id": "1",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100021823.1.33",
          "form": "각자결제가 아니기 때문입니다",
          "original_form": "각자결제가 아니기 때문입니다",
          "speaker_id": "1",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100021823.1.34",
          "form": "돈 똑같이 냈는데 누군 2조각 먹고 누군 6조각 먹으면..",
          "original_form": "돈 똑같이 냈는데 누군 2조각 먹고 누군 6조각 먹으면..",
          "speaker_id": "1",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100021823.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋ어이없네여ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ어이없네여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100021823.1.36",
          "form": "그것은 공정의 정신에 위배되나봐요 ㅠ",
          "original_form": "그것은 공정의 정신에 위배되나봐요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100021823.1.37",
          "form": "똑같이 반갈라야죠!!",
          "original_form": "똑같이 반갈라야죠!!",
