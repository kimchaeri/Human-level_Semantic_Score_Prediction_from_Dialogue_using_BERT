      "utterance": [
        {
          "id": "MDRW2100046208.1.1",
          "form": "생으로... 안먹어요?",
          "original_form": "생으로... 안먹어요?",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046208.1.2",
          "form": "맛있는데...?",
          "original_form": "맛있는데...?",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046208.1.3",
          "form": "약간 그 끈적한게",
          "original_form": "약간 그 끈적한게",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046208.1.4",
          "form": "아삭아삭하고",
          "original_form": "아삭아삭하고",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046208.1.5",
          "form": "저는 마를 먹어본 기억이",
          "original_form": "저는 마를 먹어본 기억이",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046208.1.6",
          "form": "정말 어릴때",
          "original_form": "정말 어릴때",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046208.1.7",
          "form": "엄마가 바나나랑 갈아준게",
          "original_form": "엄마가 바나나랑 갈아준게",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046208.1.8",
          "form": "전부여서...",
          "original_form": "전부여서...",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046208.1.9",
          "form": "아 그래요 ㅋㅋㅋㅋ",
          "original_form": "아 그래요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046208.1.10",
          "form": "저도 어릴때 먹은거라 ㅎ",
          "original_form": "저도 어릴때 먹은거라 ㅎ",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046208.1.11",
          "form": "점심은",
          "original_form": "점심은",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046208.1.12",
          "form": "정하셨어요?",
          "original_form": "정하셨어요?",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046208.1.13",
          "form": "지금 던킨 가고있어요!",
          "original_form": "지금 던킨 가고있어요!",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046208.1.14",
          "form": "민트초코먹을려고요 ㅎㅎㅎ",
          "original_form": "민트초코먹을려고요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046208.1.15",
          "form": "던킨! 어떤거요?!",
          "original_form": "던킨! 어떤거요?!",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046208.1.16",
          "form": "아 던킨에도 민초가 나왔어요?ㅋㅋㅋㅋ",
          "original_form": "아 던킨에도 민초가 나왔어요?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046208.1.17",
          "form": "네!! 좀 됐어요!!",
          "original_form": "네!! 좀 됐어요!!",
          "speaker_id": "2",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046208.1.18",
          "form": "맛있어요!",
          "original_form": "맛있어요!",
          "speaker_id": "2",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046208.1.19",
          "form": "도넛이죠??",
          "original_form": "도넛이죠??",
          "speaker_id": "1",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046208.1.20",
          "form": "도넛 좋아하세요?ㅋㅋ",
          "original_form": "도넛 좋아하세요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046208.1.21",
          "form": "도넛 먹기는 먹어요!!",
          "original_form": "도넛 먹기는 먹어요!!",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046208.1.22",
          "form": "잘 사먹진 않아요!",
          "original_form": "잘 사먹진 않아요!",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046208.1.23",
          "form": "저는 도넛 그거 좋아요",
          "original_form": "저는 도넛 그거 좋아요",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046208.1.24",
          "form": "하얀가루 묻어있는ㅋㅋㅋㅋ",
          "original_form": "하얀가루 묻어있는ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046208.1.25",
          "form": "안에 딸기 들어있는고요?",
          "original_form": "안에 딸기 들어있는고요?",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046208.1.26",
          "form": "네네ㅋㅋㅋㅋ바나나크림도잇고 뭐",
          "original_form": "네네ㅋㅋㅋㅋ바나나크림도잇고 뭐",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046208.1.27",
          "form": "기본이 젤 맛나요",
          "original_form": "기본이 젤 맛나요",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046208.1.28",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046208.1.29",
          "form": "안먹어봤는데 하나 사야겠어요!",
          "original_form": "안먹어봤는데 하나 사야겠어요!",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046208.1.30",
          "form": "그럼 지금 도넛으로",
          "original_form": "그럼 지금 도넛으로",
          "speaker_id": "1",
          "time": "20210908 11:20"
        }
      ]
    }
  ]
