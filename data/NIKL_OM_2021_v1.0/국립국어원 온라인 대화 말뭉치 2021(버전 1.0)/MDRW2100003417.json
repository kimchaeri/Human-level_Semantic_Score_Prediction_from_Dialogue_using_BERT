      "utterance": [
        {
          "id": "MDRW2100003417.1.1",
          "form": "사야지 뭐",
          "original_form": "사야지 뭐",
          "speaker_id": "2",
          "time": "20210820 22:53"
        },
        {
          "id": "MDRW2100003417.1.2",
          "form": "빨간 색연필만 파나?",
          "original_form": "빨간 색연필만 파나?",
          "speaker_id": "1",
          "time": "20210820 22:53"
        },
        {
          "id": "MDRW2100003417.1.3",
          "form": "ㅇㅇ 본 것 같아. 함 찾아봐",
          "original_form": "ㅇㅇ 본 것 같아. 함 찾아봐",
          "speaker_id": "2",
          "time": "20210820 22:53"
        },
        {
          "id": "MDRW2100003417.1.4",
          "form": "온라인에서 사면 배송료가 비싸서",
          "original_form": "온라인에서 사면 배송료가 비싸서",
          "speaker_id": "1",
          "time": "20210820 22:53"
        },
        {
          "id": "MDRW2100003417.1.5",
          "form": "글치 배보다 배꼽이 더 커지겠다 ㅎㅎ",
          "original_form": "글치 배보다 배꼽이 더 커지겠다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 22:54"
        },
        {
          "id": "MDRW2100003417.1.6",
          "form": "요즘 채점하는 양이 많아서 사야되는데",
          "original_form": "요즘 채점하는 양이 많아서 사야되는데",
          "speaker_id": "1",
          "time": "20210820 22:54"
        },
        {
          "id": "MDRW2100003417.1.7",
          "form": "그럼 그냥 문구점 가서 사",
          "original_form": "그럼 그냥 문구점 가서 사",
          "speaker_id": "2",
          "time": "20210820 22:54"
        },
        {
          "id": "MDRW2100003417.1.8",
          "form": "문구점이 꽤 비싸더라고",
          "original_form": "문구점이 꽤 비싸더라고",
          "speaker_id": "1",
          "time": "20210820 22:54"
        },
        {
          "id": "MDRW2100003417.1.9",
          "form": "가격비교 좀 해봐야겠네",
          "original_form": "가격비교 좀 해봐야겠네",
          "speaker_id": "1",
          "time": "20210820 22:54"
        },
        {
          "id": "MDRW2100003417.1.10",
          "form": "하긴 문구점 가격이 거의 두배더라",
          "original_form": "하긴 문구점 가격이 거의 두배더라",
          "speaker_id": "2",
          "time": "20210820 22:54"
        },
        {
          "id": "MDRW2100003417.1.11",
          "form": "ㅇㅇ 저번에 지우개 살때도",
          "original_form": "ㅇㅇ 저번에 지우개 살때도",
          "speaker_id": "1",
          "time": "20210820 22:55"
        },
        {
          "id": "MDRW2100003417.1.12",
          "form": "두배 받아서 깜짝 놀람 ㅎㅎ",
          "original_form": "두배 받아서 깜짝 놀람 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 22:55"
        },
        {
          "id": "MDRW2100003417.1.13",
          "form": "앞으로도 계속 채점해야 하니 좀 넉넉히 사요",
          "original_form": "앞으로도 계속 채점해야 하니 좀 넉넉히 사요",
          "speaker_id": "2",
          "time": "20210820 22:55"
        },
        {
          "id": "MDRW2100003417.1.14",
          "form": "ㅇㅇ 알았응 ㅎㅎ",
          "original_form": "ㅇㅇ 알았응 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 22:55"
        },
        {
          "id": "MDRW2100003417.1.15",
          "form": "색연필이던 싸인펜이던 꼭 한가지 색깔 먼저 떨어지네",
          "original_form": "색연필이던 싸인펜이던 꼭 한가지 색깔 먼저 떨어지네",
          "speaker_id": "2",
          "time": "20210820 22:56"
        },
        {
          "id": "MDRW2100003417.1.16",
          "form": "그러게 말야 ㅋㅋ",
          "original_form": "그러게 말야 ㅋㅋ",
