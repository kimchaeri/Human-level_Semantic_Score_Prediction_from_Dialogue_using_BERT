          "original_form": "&name2&님",
          "speaker_id": "1",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100040513.1.6",
          "form": "네녜",
          "original_form": "네녜",
          "speaker_id": "2",
          "time": "20210906 19:02"
        },
        {
          "id": "MDRW2100040513.1.7",
          "form": "와안녕하세요!",
          "original_form": "와안녕하세요!",
          "speaker_id": "1",
          "time": "20210906 19:03"
        },
        {
          "id": "MDRW2100040513.1.8",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "2",
          "time": "20210906 19:04"
        },
        {
          "id": "MDRW2100040513.1.9",
          "form": "오늘 사신 물건있나요!",
          "original_form": "오늘 사신 물건있나요!",
          "speaker_id": "1",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100040513.1.10",
          "form": "오늘은 안과를 다녀와서",
          "original_form": "오늘은 안과를 다녀와서",
          "speaker_id": "2",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100040513.1.11",
          "form": "안약을 샀더요.",
          "original_form": "안약을 샀더요.",
          "speaker_id": "2",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100040513.1.12",
          "form": "눈이안좋으셨나용",
          "original_form": "눈이안좋으셨나용",
          "speaker_id": "1",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100040513.1.13",
          "form": "안약은안비쌌나요",
          "original_form": "안약은안비쌌나요",
          "speaker_id": "1",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100040513.1.14",
          "form": "알러지가 잇엇나봐요",
          "original_form": "알러지가 잇엇나봐요",
          "speaker_id": "2",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100040513.1.15",
          "form": "안비쌌어됴",
          "original_form": "안비쌌어됴",
          "speaker_id": "2",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100040513.1.16",
          "form": "알러지먹는약은 안사도되던가용",
          "original_form": "알러지먹는약은 안사도되던가용",
          "speaker_id": "1",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100040513.1.17",
          "form": "알러지 안약만 삿어용",
          "original_form": "알러지 안약만 삿어용",
          "speaker_id": "2",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100040513.1.18",
          "form": "다행이예용",
          "original_form": "다행이예용",
          "speaker_id": "1",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100040513.1.19",
          "form": "저녁장은안보셧나요",
          "original_form": "저녁장은안보셧나요",
          "speaker_id": "1",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100040513.1.20",
          "form": "넹넹 장보시나요",
          "original_form": "넹넹 장보시나요",
