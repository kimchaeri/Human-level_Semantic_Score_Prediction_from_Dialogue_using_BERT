      "utterance": [
        {
          "id": "MDRW2100035460.1.1",
          "form": "오 잘쳤나",
          "original_form": "오 잘쳤나",
          "speaker_id": "2",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100035460.1.2",
          "form": "하ㅜ 난그래도 듣기가 읽기보다 쪼끔 낫다생각했는데",
          "original_form": "하ㅜ 난그래도 듣기가 읽기보다 쪼끔 낫다생각했는데",
          "speaker_id": "1",
          "time": "20210829 18:07"
        },
        {
          "id": "MDRW2100035460.1.3",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210829 18:12"
        },
        {
          "id": "MDRW2100035460.1.4",
          "form": "좀찜찜하게본거여?",
          "original_form": "좀찜찜하게본거여?",
          "speaker_id": "2",
          "time": "20210829 18:12"
        },
        {
          "id": "MDRW2100035460.1.5",
          "form": "아니 나만 이렇게 어려운거야?",
          "original_form": "아니 나만 이렇게 어려운거야?",
          "speaker_id": "1",
          "time": "20210829 18:12"
        },
        {
          "id": "MDRW2100035460.1.6",
          "form": "뜨악 이러면서 봄",
          "original_form": "뜨악 이러면서 봄",
          "speaker_id": "1",
          "time": "20210829 18:12"
        },
        {
          "id": "MDRW2100035460.1.7",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100035460.1.8",
          "form": "다른사람도 그렇겠징 난 그시험한번도본적없음",
          "original_form": "다른사람도 그렇겠징 난 그시험한번도본적없음",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100035460.1.9",
          "form": "와 진짜 딱 한번만 더 들려주세요ㅜㅜ",
          "original_form": "와 진짜 딱 한번만 더 들려주세요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100035460.1.10",
          "form": "싶었다는",
          "original_form": "싶었다는",
          "speaker_id": "1",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100035460.1.11",
          "form": "ㅜㅜㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100035460.1.12",
          "form": "맘 고생했다 신경쓴다고",
          "original_form": "맘 고생했다 신경쓴다고",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100035460.1.13",
          "form": "듣기가 어렵네",
          "original_form": "듣기가 어렵네",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100035460.1.14",
          "form": "인터넷으로 가답안봤는데",
          "original_form": "인터넷으로 가답안봤는데",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100035460.1.15",
          "form": "에효ㅜ 고쳤는데 틀린것도있고ㅜㅜ",
          "original_form": "에효ㅜ 고쳤는데 틀린것도있고ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100035460.1.16",
          "form": "가답 천천히 하면댖ㄱ",
          "original_form": "가답 천천히 하면댖ㄱ",
          "speaker_id": "2",
          "time": "20210829 19:25"
        },
        {
          "id": "MDRW2100035460.1.17",
          "form": "급한것도아니구",
          "original_form": "급한것도아니구",
          "speaker_id": "2",
          "time": "20210829 19:25"
        },
        {
          "id": "MDRW2100035460.1.18",
          "form": "안보는게 내 정신건강에 좋을것같기도하고..",
          "original_form": "안보는게 내 정신건강에 좋을것같기도하고..",
          "speaker_id": "1",
          "time": "20210829 20:05"
        },
        {
          "id": "MDRW2100035460.1.19",
          "form": "ㅋㅋ 음 돌이킬순없어도 과거로부터 배울순있잖아",
          "original_form": "ㅋㅋ 음 돌이킬순없어도 과거로부터 배울순있잖아",
          "speaker_id": "2",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100035460.1.20",
          "form": "음 나는 발전하고 있는가 뒤돌아보게되네요 오늘도 ㅜㅜ 웃프네ㅜㅜㅜ",
          "original_form": "음 나는 발전하고 있는가 뒤돌아보게되네요 오늘도 ㅜㅜ 웃프네ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100035460.1.21",
          "form": "나도 한 두달째 외주  할일안하고 쌓여있어서 할말읍네ㅋㅋㅋ",
          "original_form": "나도 한 두달째 외주  할일안하고 쌓여있어서 할말읍네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100035460.1.22",
          "form": "나 필기시험 날짜 그때 말해줬던가요",
          "original_form": "나 필기시험 날짜 그때 말해줬던가요",
          "speaker_id": "1",
          "time": "20210902 16:31"
        }
      ]
