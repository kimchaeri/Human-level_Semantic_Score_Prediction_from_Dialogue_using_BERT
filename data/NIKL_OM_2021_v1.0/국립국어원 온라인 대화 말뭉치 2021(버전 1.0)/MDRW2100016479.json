      "utterance": [
        {
          "id": "MDRW2100016479.1.1",
          "form": "최근 뭐 사신 거 있나요?",
          "original_form": "최근 뭐 사신 거 있나요?",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100016479.1.2",
          "form": "저는 최근에 스웨터를 샀어요.",
          "original_form": "저는 최근에 스웨터를 샀어요.",
          "speaker_id": "1",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016479.1.3",
          "form": "스웨터 어떤 거요?",
          "original_form": "스웨터 어떤 거요?",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016479.1.4",
          "form": "메이커 브랜드 스웨터요",
          "original_form": "메이커 브랜드 스웨터요",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016479.1.5",
          "form": "예쁜가요?",
          "original_form": "예쁜가요?",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016479.1.6",
          "form": "네. 제가 제일 마음에드는 쨍한 색이었어요",
          "original_form": "네. 제가 제일 마음에드는 쨍한 색이었어요",
          "speaker_id": "1",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016479.1.7",
          "form": "어떤색이요?",
          "original_form": "어떤색이요?",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016479.1.8",
          "form": "빨간색이요 ㅎㅎ 원색계통이라 정말예쁘더라고요",
          "original_form": "빨간색이요 ㅎㅎ 원색계통이라 정말예쁘더라고요",
          "speaker_id": "1",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016479.1.9",
          "form": "원색을 좋아하시는 군요?",
          "original_form": "원색을 좋아하시는 군요?",
          "speaker_id": "2",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016479.1.10",
          "form": "네. 10년전부터 좋아하게되었어요.",
          "original_form": "네. 10년전부터 좋아하게되었어요.",
          "speaker_id": "1",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016479.1.11",
          "form": "저도 빨간옷 좋아합니다",
          "original_form": "저도 빨간옷 좋아합니다",
          "speaker_id": "2",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100016479.1.12",
          "form": "그 컬러의 옷을 시작으로 원색에 과감하게 도전할수잇게되엇어요.",
          "original_form": "그 컬러의 옷을 시작으로 원색에 과감하게 도전할수잇게되엇어요.",
          "speaker_id": "1",
          "time": "20210826 14:26"
        },
        {
          "id": "MDRW2100016479.1.13",
          "form": "연두색 옷은 어떤가요?",
          "original_form": "연두색 옷은 어떤가요?",
          "speaker_id": "2",
          "time": "20210826 14:28"
        },
        {
          "id": "MDRW2100016479.1.14",
          "form": "안좋아해요.",
          "original_form": "안좋아해요.",
          "speaker_id": "1",
