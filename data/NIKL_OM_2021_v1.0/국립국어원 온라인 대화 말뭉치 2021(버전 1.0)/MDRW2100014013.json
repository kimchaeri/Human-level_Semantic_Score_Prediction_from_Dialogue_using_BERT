      "utterance": [
        {
          "id": "MDRW2100014013.1.1",
          "form": "너 그러면",
          "original_form": "너 그러면",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014013.1.2",
          "form": "우유 부어먹는 팥빙수는??",
          "original_form": "우유 부어먹는 팥빙수는??",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014013.1.3",
          "form": "그거 개맛있지",
          "original_form": "그거 개맛있지",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014013.1.4",
          "form": "맞지!",
          "original_form": "맞지!",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014013.1.5",
          "form": "거기에",
          "original_form": "거기에",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014013.1.6",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014013.1.7",
          "form": "떡이 좀 뷰족하기때문에",
          "original_form": "떡이 좀 뷰족하기때문에",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014013.1.8",
          "form": "그 팥빙수떡을",
          "original_form": "그 팥빙수떡을",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014013.1.9",
          "form": "옹",
          "original_form": "옹",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014013.1.10",
          "form": "따로 사서 넣어먹으면",
          "original_form": "따로 사서 넣어먹으면",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014013.1.11",
          "form": "완전 풍족하게 먹을수있음",
          "original_form": "완전 풍족하게 먹을수있음",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014013.1.12",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014013.1.13",
          "form": "그렇게 먹어본겨?",
          "original_form": "그렇게 먹어본겨?",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014013.1.14",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014013.1.15",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014013.1.16",
          "form": "생각만해봄",
          "original_form": "생각만해봄",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014013.1.17",
          "form": "그렇군.ㅡ",
          "original_form": "그렇군.ㅡ",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014013.1.18",
          "form": "근데 해보면 좋!을듯!",
          "original_form": "근데 해보면 좋!을듯!",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014013.1.19",
          "form": "먹을때마다 떡이 부족해",
          "original_form": "먹을때마다 떡이 부족해",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014013.1.20",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210825 16:52"
        }
      ]
