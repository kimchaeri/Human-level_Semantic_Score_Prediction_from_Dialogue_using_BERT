      "utterance": [
        {
          "id": "MDRW2100046086.1.1",
          "form": "아녀ㅠ 아침은 자주 굶게되네여",
          "original_form": "아녀ㅠ 아침은 자주 굶게되네여",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046086.1.2",
          "form": "밥 드셨나여?",
          "original_form": "밥 드셨나여?",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046086.1.3",
          "form": "저도아직요ㅠ",
          "original_form": "저도아직요ㅠ",
          "speaker_id": "1",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046086.1.4",
          "form": "배안고프세요?",
          "original_form": "배안고프세요?",
          "speaker_id": "1",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046086.1.5",
          "form": "조금 고프긴 합니다만ㅠㅋㅋㅋ 익숙해요",
          "original_form": "조금 고프긴 합니다만ㅠㅋㅋㅋ 익숙해요",
          "speaker_id": "2",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046086.1.6",
          "form": "ㅜㅜ식단하시는거에요?",
          "original_form": "ㅜㅜ식단하시는거에요?",
          "speaker_id": "1",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046086.1.7",
          "form": "앗 아니요ㅋㅋㅋ아침만 안먹은건데?요?",
          "original_form": "앗 아니요ㅋㅋㅋ아침만 안먹은건데?요?",
          "speaker_id": "2",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046086.1.8",
          "form": "점심은 먹어야져",
          "original_form": "점심은 먹어야져",
          "speaker_id": "2",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046086.1.9",
          "form": "ㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100046086.1.10",
          "form": "그져",
          "original_form": "그져",
          "speaker_id": "1",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100046086.1.11",
          "form": "뭐 드실지 생각해보셨나여",
          "original_form": "뭐 드실지 생각해보셨나여",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046086.1.12",
          "form": "국물있는거 먹구싶네여",
          "original_form": "국물있는거 먹구싶네여",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046086.1.13",
          "form": "샤브샤브맛있겠어요",
          "original_form": "샤브샤브맛있겠어요",
          "speaker_id": "1",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046086.1.14",
          "form": "오 샤브샤브 저 좋아합니다",
          "original_form": "오 샤브샤브 저 좋아합니다",
          "speaker_id": "2",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046086.1.15",
          "form": "쌀국수어떠세요",
          "original_form": "쌀국수어떠세요",
          "speaker_id": "1",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046086.1.16",
          "form": "메뉴선택을 너무 잘하시는데여!",
          "original_form": "메뉴선택을 너무 잘하시는데여!",
          "speaker_id": "2",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046086.1.17",
          "form": "ㅋㅋㅋ좋아요",
          "original_form": "ㅋㅋㅋ좋아요",
          "speaker_id": "2",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046086.1.18",
          "form": "ㅋㅋㅋㅋ뿌듯하네요",
          "original_form": "ㅋㅋㅋㅋ뿌듯하네요",
          "speaker_id": "1",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046086.1.19",
          "form": "ㅎㅎ감사해여 점심 정했네요",
          "original_form": "ㅎㅎ감사해여 점심 정했네요",
          "speaker_id": "2",
          "time": "20210908 10:22"
        }
      ]
