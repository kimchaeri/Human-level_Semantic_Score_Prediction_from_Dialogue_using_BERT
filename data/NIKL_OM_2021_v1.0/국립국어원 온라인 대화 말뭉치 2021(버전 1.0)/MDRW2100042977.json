        {
          "id": "MDRW2100042977.1.1",
          "form": "ㅇㅇ몸이안좋네",
          "original_form": "ㅇㅇ몸이안좋네",
          "speaker_id": "2",
          "time": "20210907 12:22"
        },
        {
          "id": "MDRW2100042977.1.2",
          "form": "오늘 안서방 2차맞고온다",
          "original_form": "오늘 안서방 2차맞고온다",
          "speaker_id": "2",
          "time": "20210907 12:23"
        },
        {
          "id": "MDRW2100042977.1.3",
          "form": "병자가 둘이네ㅜ",
          "original_form": "병자가 둘이네ㅜ",
          "speaker_id": "1",
          "time": "20210907 12:23"
        },
        {
          "id": "MDRW2100042977.1.4",
          "form": "백신이 사람 골로 보내는듯ㅜ",
          "original_form": "백신이 사람 골로 보내는듯ㅜ",
          "speaker_id": "1",
          "time": "20210907 12:23"
        },
        {
          "id": "MDRW2100042977.1.5",
          "form": "그니깐 걱정",
          "original_form": "그니깐 걱정",
          "speaker_id": "2",
          "time": "20210907 12:28"
        },
        {
          "id": "MDRW2100042977.1.6",
          "form": "저녁시켜먹으까..",
          "original_form": "저녁시켜먹으까..",
          "speaker_id": "2",
          "time": "20210907 12:28"
        },
        {
          "id": "MDRW2100042977.1.7",
          "form": "시켜먹어라",
          "original_form": "시켜먹어라",
          "speaker_id": "1",
          "time": "20210907 12:31"
        },
        {
          "id": "MDRW2100042977.1.8",
          "form": "나 37.3",
          "original_form": "나 37.3",
          "speaker_id": "1",
          "time": "20210907 12:31"
        },
        {
          "id": "MDRW2100042977.1.9",
          "form": "미열나기 시작",
          "original_form": "미열나기 시작",
          "speaker_id": "1",
          "time": "20210907 12:32"
        },
        {
          "id": "MDRW2100042977.1.10",
          "form": "언니야도 열있네",
          "original_form": "언니야도 열있네",
          "speaker_id": "2",
          "time": "20210907 12:32"
        },
        {
