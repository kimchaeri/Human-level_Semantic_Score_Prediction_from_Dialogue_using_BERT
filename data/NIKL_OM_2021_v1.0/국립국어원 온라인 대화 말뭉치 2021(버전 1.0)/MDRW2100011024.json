      "utterance": [
        {
          "id": "MDRW2100011024.1.1",
          "form": "그럼~",
          "original_form": "그럼~",
          "speaker_id": "2",
          "time": "20210823 23:50"
        },
        {
          "id": "MDRW2100011024.1.2",
          "form": "근데 아주 마음에 들지 않은 사람들만 나왔어.",
          "original_form": "근데 아주 마음에 들지 않은 사람들만 나왔어.",
          "speaker_id": "2",
          "time": "20210823 23:50"
        },
        {
          "id": "MDRW2100011024.1.3",
          "form": "어머 언제해봤니??",
          "original_form": "어머 언제해봤니??",
          "speaker_id": "1",
          "time": "20210823 23:50"
        },
        {
          "id": "MDRW2100011024.1.4",
          "form": "나 지지난주에~",
          "original_form": "나 지지난주에~",
          "speaker_id": "2",
          "time": "20210823 23:50"
        },
        {
          "id": "MDRW2100011024.1.5",
          "form": "줌으로 만났어.",
          "original_form": "줌으로 만났어.",
          "speaker_id": "2",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100011024.1.6",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "1",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100011024.1.7",
          "form": "요즘 코시국이라 온라인 소개팅이 유행이더라.",
          "original_form": "요즘 코시국이라 온라인 소개팅이 유행이더라.",
          "speaker_id": "2",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100011024.1.8",
          "form": "요즘시대에 맞는 소개팅이네",
          "original_form": "요즘시대에 맞는 소개팅이네",
          "speaker_id": "1",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100011024.1.9",
          "form": "너는 온라인 소개팅에 관심없니?",
          "original_form": "너는 온라인 소개팅에 관심없니?",
          "speaker_id": "2",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100011024.1.10",
          "form": "근데 화면으로만 보면",
          "original_form": "근데 화면으로만 보면",
          "speaker_id": "1",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100011024.1.11",
          "form": "맞아 ㅎㄹ",
          "original_form": "맞아 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100011024.1.12",
          "form": "실물을 믿을 수 없잖아",
          "original_form": "실물을 믿을 수 없잖아",
          "speaker_id": "1",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100011024.1.13",
          "form": "어색하기는 했어. 더 어색하더라.",
          "original_form": "어색하기는 했어. 더 어색하더라.",
          "speaker_id": "2",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100011024.1.14",
          "form": "그래서 나는 온라인 만남은 별로야",
          "original_form": "그래서 나는 온라인 만남은 별로야",
          "speaker_id": "1",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100011024.1.15",
          "form": "각자 자신의 푸드를 준비하고 만났어야했어.",
          "original_form": "각자 자신의 푸드를 준비하고 만났어야했어.",
          "speaker_id": "2",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100011024.1.16",
          "form": "맞아 어색할때는 술을 마셔야지",
          "original_form": "맞아 어색할때는 술을 마셔야지",
          "speaker_id": "1",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100011024.1.17",
          "form": "히지만 그만큼 끊어내기도 쉽더라.",
          "original_form": "히지만 그만큼 끊어내기도 쉽더라.",
          "speaker_id": "2",
          "time": "20210823 23:51"
        }
      ]
