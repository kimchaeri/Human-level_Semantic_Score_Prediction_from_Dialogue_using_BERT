        {
          "id": "MDRW2100020805.1.1",
          "form": "여행 좋아하세요",
          "original_form": "여행 좋아하세요",
          "speaker_id": "1",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100020805.1.2",
          "form": "여행 안좋아해요",
          "original_form": "여행 안좋아해요",
          "speaker_id": "2",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100020805.1.3",
          "form": "여행 좋아하세요??",
          "original_form": "여행 좋아하세요??",
          "speaker_id": "2",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100020805.1.4",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100020805.1.5",
          "form": "요새 못가서 아쉽네요",
          "original_form": "요새 못가서 아쉽네요",
          "speaker_id": "1",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100020805.1.6",
          "form": "제일기억에 남는곳은 어디있으세요?!",
          "original_form": "제일기억에 남는곳은 어디있으세요?!",
          "speaker_id": "2",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100020805.1.7",
          "form": "강릉이요",
          "original_form": "강릉이요",
          "speaker_id": "1",
          "time": "20210901 10:57"
        },
        {
          "id": "MDRW2100020805.1.8",
          "form": "바다보러 갔었는데 예쁘더라구요",
          "original_form": "바다보러 갔었는데 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210901 10:57"
        },
        {
          "id": "MDRW2100020805.1.9",
          "form": "바다,,,!",
          "original_form": "바다,,,!",
          "speaker_id": "2",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100020805.1.10",
          "form": "너무이쁘죠",
          "original_form": "너무이쁘죠",
          "speaker_id": "2",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100020805.1.11",
          "form": "기념품 뭐 사신거있으실까요?!",
          "original_form": "기념품 뭐 사신거있으실까요?!",
          "speaker_id": "2",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100020805.1.12",
          "form": "기념품은 안샀어요ㅋㅋ",
          "original_form": "기념품은 안샀어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100020805.1.13",
          "form": "사진은 잔뜩 찍었어요",
          "original_form": "사진은 잔뜩 찍었어요",
          "speaker_id": "1",
          "time": "20210901 10:59"
        },
        {
          "id": "MDRW2100020805.1.14",
          "form": "코로나끝나면 또가실계획있으세요?!",
          "original_form": "코로나끝나면 또가실계획있으세요?!",
          "speaker_id": "2",
          "time": "20210901 10:59"
        },
        {
          "id": "MDRW2100020805.1.15",
          "form": "사진이 남는거죠!",
          "original_form": "사진이 남는거죠!",
          "speaker_id": "2",
          "time": "20210901 10:59"
        },
        {
          "id": "MDRW2100020805.1.16",
          "form": "네 꼭이요ㅠㅠ 새벽바다도 진짜 예쁘거든요",
          "original_form": "네 꼭이요ㅠㅠ 새벽바다도 진짜 예쁘거든요",
          "speaker_id": "1",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100020805.1.17",
          "form": "와 새벽바다",
          "original_form": "와 새벽바다",
          "speaker_id": "2",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100020805.1.18",
          "form": "새벽공기 맡으면 어렴풋이 그때 그 느낌이 생각날거같아요",
          "original_form": "새벽공기 맡으면 어렴풋이 그때 그 느낌이 생각날거같아요",
          "speaker_id": "2",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100020805.1.19",
          "form": "맞아요ㅎㅎ 새벽엔 사람도 잘 없고 산책하기도 좋더라구요",
          "original_form": "맞아요ㅎㅎ 새벽엔 사람도 잘 없고 산책하기도 좋더라구요",
          "speaker_id": "1",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100020805.1.20",
          "form": "강릉말고 또 어디여행계획 있으세요?!",
          "original_form": "강릉말고 또 어디여행계획 있으세요?!",
          "speaker_id": "2",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100020805.1.21",
          "form": "요샌 먹부림 여행 가고싶어요",
          "original_form": "요샌 먹부림 여행 가고싶어요",
          "speaker_id": "1",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100020805.1.22",
          "form": "서울 맛집투어도 괜찮을거 같구요ㅋㅋㅋ",
          "original_form": "서울 맛집투어도 괜찮을거 같구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100020805.1.23",
