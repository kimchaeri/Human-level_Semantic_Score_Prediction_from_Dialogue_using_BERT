      "utterance": [
        {
          "id": "MDRW2100021299.1.1",
          "form": "자주보는거있으신가요!",
          "original_form": "자주보는거있으신가요!",
          "speaker_id": "2",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100021299.1.2",
          "form": "유튜브로 예능이나 여러가지 봐요",
          "original_form": "유튜브로 예능이나 여러가지 봐요",
          "speaker_id": "1",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021299.1.3",
          "form": "뭐 보시는거 있나요?",
          "original_form": "뭐 보시는거 있나요?",
          "speaker_id": "1",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021299.1.4",
          "form": "런닝맨이요!",
          "original_form": "런닝맨이요!",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021299.1.5",
          "form": "저두 좋아해요",
          "original_form": "저두 좋아해요",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021299.1.6",
          "form": "요새 재밌더라구요",
          "original_form": "요새 재밌더라구요",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021299.1.7",
          "form": "오오옹 저두요!!",
          "original_form": "오오옹 저두요!!",
          "speaker_id": "2",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021299.1.8",
          "form": "클립 영상도 잘 올려주잖아요",
          "original_form": "클립 영상도 잘 올려주잖아요",
          "speaker_id": "1",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100021299.1.9",
          "form": "맞아여맞아여",
          "original_form": "맞아여맞아여",
          "speaker_id": "2",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100021299.1.10",
          "form": "혼밥할때보면 최고죠",
          "original_form": "혼밥할때보면 최고죠",
          "speaker_id": "2",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100021299.1.11",
          "form": "예전 예능도 유튜브에 보이더라구요",
          "original_form": "예전 예능도 유튜브에 보이더라구요",
          "speaker_id": "1",
          "time": "20210828 17:41"
        },
        {
          "id": "MDRW2100021299.1.12",
          "form": "맞아여맞아여",
          "original_form": "맞아여맞아여",
          "speaker_id": "2",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100021299.1.13",
          "form": "무한도전은 계속 봐도 재밌어요",
          "original_form": "무한도전은 계속 봐도 재밌어요",
          "speaker_id": "1",
          "time": "20210828 17:47"
        },
        {
          "id": "MDRW2100021299.1.14",
          "form": "아 그죠 레전드예증",
          "original_form": "아 그죠 레전드예증",
          "speaker_id": "2",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100021299.1.15",
          "form": "능",
          "original_form": "능",
          "speaker_id": "2",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100021299.1.16",
          "form": "맞아요. 요새 재방송도 해주더라구요",
          "original_form": "맞아요. 요새 재방송도 해주더라구요",
