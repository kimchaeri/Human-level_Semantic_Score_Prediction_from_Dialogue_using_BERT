      "utterance": [
        {
          "id": "MDRW2100023564.1.1",
          "form": "아침은 뭐 드셨나요?",
          "original_form": "아침은 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023564.1.2",
          "form": "전 오늘부터 다이어트를 하려고 합니다",
          "original_form": "전 오늘부터 다이어트를 하려고 합니다",
          "speaker_id": "1",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023564.1.3",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "1",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023564.1.4",
          "form": "거짓말이 느셨네요",
          "original_form": "거짓말이 느셨네요",
          "speaker_id": "2",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023564.1.5",
          "form": "크로플을 드셨나요?",
          "original_form": "크로플을 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023564.1.6",
          "form": "크로플 아침에 먹었습니다 향기가 그까지 갓나요?",
          "original_form": "크로플 아침에 먹었습니다 향기가 그까지 갓나요?",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023564.1.7",
          "form": "무슨 다이어트하시나요?",
          "original_form": "무슨 다이어트하시나요?",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023564.1.8",
          "form": "일평생을 하고 있는 거짓말이지요ㅋㅋㅋ",
          "original_form": "일평생을 하고 있는 거짓말이지요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023564.1.9",
          "form": "조혜련 태보 추천합니다",
          "original_form": "조혜련 태보 추천합니다",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023564.1.10",
          "form": "잘 어울리실것 같아요",
          "original_form": "잘 어울리실것 같아요",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023564.1.11",
          "form": "전 그냥 아가방까지 걸어  다니겠습니다",
          "original_form": "전 그냥 아가방까지 걸어  다니겠습니다",
          "speaker_id": "1",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023564.1.12",
          "form": "집안일 하시면서 할 수있을 것 같아요",
          "original_form": "집안일 하시면서 할 수있을 것 같아요",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023564.1.13",
          "form": "아 아가 보고싶네요",
          "original_form": "아 아가 보고싶네요",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023564.1.14",
          "form": "어제 밤새 울었습니다",
          "original_form": "어제 밤새 울었습니다",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023564.1.15",
          "form": "오늘 재택근무 아니신가요?",
          "original_form": "오늘 재택근무 아니신가요?",
          "speaker_id": "1",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023564.1.16",
          "form": "엉엉 울더라구요 아가가",
          "original_form": "엉엉 울더라구요 아가가",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023564.1.17",
          "form": "네 거실에 있습니다",
          "original_form": "네 거실에 있습니다",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023564.1.18",
          "form": "저는 컴터방에 있구요",
          "original_form": "저는 컴터방에 있구요",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023564.1.19",
          "form": "이앓이 중이라 들었습니다~",
          "original_form": "이앓이 중이라 들었습니다~",
          "speaker_id": "1",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023564.1.20",
          "form": "그런 것 같아요",
          "original_form": "그런 것 같아요",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023564.1.21",
          "form": "혹시 자재분도 이앓이해서",
          "original_form": "혹시 자재분도 이앓이해서",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023564.1.22",
          "form": "엉엉 울던가요?",
          "original_form": "엉엉 울던가요?",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023564.1.23",
          "form": "통 잠을 엄청 늦게 잔 듯합니다",
          "original_form": "통 잠을 엄청 늦게 잔 듯합니다",
          "speaker_id": "1",
          "time": "20210830 11:41"
        },
        {
          "id": "MDRW2100023564.1.24",
          "form": "조금 더 지나면 성장통이 올 겁니다",
          "original_form": "조금 더 지나면 성장통이 올 겁니다",
          "speaker_id": "1",
          "time": "20210830 11:41"
        },
        {
          "id": "MDRW2100023564.1.25",
          "form": "또 우나요?",
          "original_form": "또 우나요?",
          "speaker_id": "2",
          "time": "20210830 11:41"
        },
        {
          "id": "MDRW2100023564.1.26",
          "form": "방심하지 마세요",
          "original_form": "방심하지 마세요",
          "speaker_id": "1",
          "time": "20210830 11:41"
        },
        {
          "id": "MDRW2100023564.1.27",
          "form": "밤새 다리를 주물러주어야 합니다",
          "original_form": "밤새 다리를 주물러주어야 합니다",
          "speaker_id": "1",
          "time": "20210830 11:41"
        },
        {
          "id": "MDRW2100023564.1.28",
          "form": "둘째 테스트했는데",
          "original_form": "둘째 테스트했는데",
          "speaker_id": "2",
          "time": "20210830 11:41"
        },
        {
          "id": "MDRW2100023564.1.29",
          "form": "불발입니다",
          "original_form": "불발입니다",
          "speaker_id": "2",
          "time": "20210830 11:41"
        },
        {
          "id": "MDRW2100023564.1.30",
          "form": "다행입니다",
          "original_form": "다행입니다",
          "speaker_id": "2",
          "time": "20210830 11:41"
        },
        {
          "id": "MDRW2100023564.1.31",
          "form": "밤새 다리를 주물러줘야겠습니다",
          "original_form": "밤새 다리를 주물러줘야겠습니다",
          "speaker_id": "2",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100023564.1.32",
          "form": "식사는 하셨나요?",
          "original_form": "식사는 하셨나요?",
          "speaker_id": "2",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100023564.1.33",
          "form": "아니요 퇴근하고 흡입할 예정입니다",
          "original_form": "아니요 퇴근하고 흡입할 예정입니다",
          "speaker_id": "1",
          "time": "20210830 11:44"
        },
        {
          "id": "MDRW2100023564.1.34",
          "form": "집에 못 다한 집안일을 하고",
          "original_form": "집에 못 다한 집안일을 하고",
          "speaker_id": "1",
          "time": "20210830 11:44"
        },
        {
          "id": "MDRW2100023564.1.35",
          "form": "한 끼는 폭식을 합니다ㅋㅋㅋ",
          "original_form": "한 끼는 폭식을 합니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:45"
        }
      ]
    }
