      "utterance": [
        {
          "id": "MDRW2100036961.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036961.1.2",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "1",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036961.1.3",
          "form": "안녕하세요~ 이제 먹습니다ㅜㅜ",
          "original_form": "안녕하세요~ 이제 먹습니다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100036961.1.4",
          "form": "아이고 어제 늦게 뭐드셨어요",
          "original_form": "아이고 어제 늦게 뭐드셨어요",
          "speaker_id": "1",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100036961.1.5",
          "form": "치맥했어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "치맥했어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100036961.1.6",
          "form": "늦게 먹고자면 안되는데",
          "original_form": "늦게 먹고자면 안되는데",
          "speaker_id": "2",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100036961.1.7",
          "form": "배가 너무고프더라구요 ㅜㅜ",
          "original_form": "배가 너무고프더라구요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100036961.1.8",
          "form": "오오 치맥",
          "original_form": "오오 치맥",
          "speaker_id": "1",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100036961.1.9",
          "form": "어디거드셨어요",
          "original_form": "어디거드셨어요",
          "speaker_id": "1",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100036961.1.10",
          "form": "돈 치킨이요!",
          "original_form": "돈 치킨이요!",
          "speaker_id": "2",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100036961.1.11",
          "form": "전 처음들어봤는데 맛있더라구요",
          "original_form": "전 처음들어봤는데 맛있더라구요",
          "speaker_id": "2",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100036961.1.12",
          "form": "아 거기 구운치킨?",
          "original_form": "아 거기 구운치킨?",
          "speaker_id": "1",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100036961.1.13",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100036961.1.14",
          "form": "알고계시나요?",
          "original_form": "알고계시나요?",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100036961.1.15",
          "form": "네네 근데 시켜먹어보진않았어요",
          "original_form": "네네 근데 시켜먹어보진않았어요",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100036961.1.16",
          "form": "젤 기본시키셨나요?",
          "original_form": "젤 기본시키셨나요?",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100036961.1.17",
          "form": "맛이 이것저것 있던데... 친구가 시켜줘서 정확히 뭔지는 모르겠어요 ㅋㅋㅋㅋ",
          "original_form": "맛이 이것저것 있던데... 친구가 시켜줘서 정확히 뭔지는 모르겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100036961.1.18",
          "form": "근데 그냥 구운거랑 매운맛이 있었으니까 후라이드 반 양념 반 아닐까 싶어요",
          "original_form": "근데 그냥 구운거랑 매운맛이 있었으니까 후라이드 반 양념 반 아닐까 싶어요",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100036961.1.19",
          "form": "아하 둘중에 어떤게 더맛있었어요?",
          "original_form": "아하 둘중에 어떤게 더맛있었어요?",
          "speaker_id": "1",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100036961.1.20",
          "form": "전 매운거요 후후",
          "original_form": "전 매운거요 후후",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100036961.1.21",
          "form": "매운걸 진짜 좋아하거든욤",
          "original_form": "매운걸 진짜 좋아하거든욤",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100036961.1.22",
          "form": "오 전 맵찔이라 매운거 잘못먹는데",
          "original_form": "오 전 맵찔이라 매운거 잘못먹는데",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100036961.1.23",
          "form": "치킨중에 그럼 가장좋아하시는건 뭐에요?",
          "original_form": "치킨중에 그럼 가장좋아하시는건 뭐에요?",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100036961.1.24",
          "form": "오 전...볼케이노요.....",
          "original_form": "오 전...볼케이노요.....",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100036961.1.25",
          "form": "매운거 사란해...",
          "original_form": "매운거 사란해...",
          "speaker_id": "2",
          "time": "20210902 10:46"
        }
      ]
    }
  ]
