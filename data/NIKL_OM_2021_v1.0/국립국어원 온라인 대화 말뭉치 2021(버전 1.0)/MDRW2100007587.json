      "utterance": [
        {
          "id": "MDRW2100007587.1.1",
          "form": "너는 학원 알바 어땠엉",
          "original_form": "너는 학원 알바 어땠엉",
          "speaker_id": "1",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007587.1.2",
          "form": "학원알바 나쁘지않아",
          "original_form": "학원알바 나쁘지않아",
          "speaker_id": "2",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007587.1.3",
          "form": "학생들 가르치는 거얌?",
          "original_form": "학생들 가르치는 거얌?",
          "speaker_id": "1",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007587.1.4",
          "form": "애들만 괜찮으면 괜찮은것같아",
          "original_form": "애들만 괜찮으면 괜찮은것같아",
          "speaker_id": "2",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007587.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007587.1.6",
          "form": "학부모들도 이상한사람 없었어서",
          "original_form": "학부모들도 이상한사람 없었어서",
          "speaker_id": "2",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007587.1.7",
          "form": "할만했아,,",
          "original_form": "할만했아,,",
          "speaker_id": "2",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007587.1.8",
          "form": "아항...",
          "original_form": "아항...",
          "speaker_id": "1",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007587.1.9",
          "form": "업무중에 상담도 있어?",
          "original_form": "업무중에 상담도 있어?",
          "speaker_id": "1",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007587.1.10",
          "form": "엉엉 부모님한테 전화해서 애들 상태같은거 말해주는거?",
          "original_form": "엉엉 부모님한테 전화해서 애들 상태같은거 말해주는거?",
          "speaker_id": "2",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007587.1.11",
          "form": "태도같은거나,,,,",
          "original_form": "태도같은거나,,,,",
          "speaker_id": "2",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007587.1.12",
          "form": "가끔 해줘야 좋아하니가,,",
          "original_form": "가끔 해줘야 좋아하니가,,",
          "speaker_id": "2",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007587.1.13",
          "form": "오홍",
          "original_form": "오홍",
          "speaker_id": "1",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007587.1.14",
          "form": "전혀 알바같지 않은데...?",
          "original_form": "전혀 알바같지 않은데...?",
          "speaker_id": "1",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007587.1.15",
          "form": "구냥 선생님이네",
          "original_form": "구냥 선생님이네",
          "speaker_id": "1",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007587.1.16",
          "form": "마자 반 직원이지ㅜㅜㅜ",
          "original_form": "마자 반 직원이지ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007587.1.17",
          "form": "일케 말하니가 돈은 그에비해 안주능거같기도...",
          "original_form": "일케 말하니가 돈은 그에비해 안주능거같기도...",
          "speaker_id": "2",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007587.1.18",
          "form": "쿠팡은 어때??",
          "original_form": "쿠팡은 어때??",
          "speaker_id": "2",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007587.1.19",
          "form": "궁금해쿠팡",
          "original_form": "궁금해쿠팡",
          "speaker_id": "2",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007587.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007587.1.21",
          "form": "그래두 음식점보단 나을듯 ㅠㅡㅠ",
          "original_form": "그래두 음식점보단 나을듯 ㅠㅡㅠ",
          "speaker_id": "1",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007587.1.22",
          "form": "음 쿠팡은 아직이야",
          "original_form": "음 쿠팡은 아직이야",
          "speaker_id": "1",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007587.1.23",
          "form": "학기중에 뛰려구",
          "original_form": "학기중에 뛰려구",
          "speaker_id": "1",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007587.1.24",
          "form": "마자ㅠㅠ 음식점 넘 힘들것같아",
          "original_form": "마자ㅠㅠ 음식점 넘 힘들것같아",
          "speaker_id": "2",
          "time": "20210822 03:40"
        },
        {
          "id": "MDRW2100007587.1.25",
          "form": "쿠팡 은근 쏠쏠하다믄 얘기도 있더라궁",
          "original_form": "쿠팡 은근 쏠쏠하다믄 얘기도 있더라궁",
          "speaker_id": "2",
          "time": "20210822 03:40"
        },
        {
          "id": "MDRW2100007587.1.26",
          "form": "설거지가 헬이지...",
          "original_form": "설거지가 헬이지...",
          "speaker_id": "1",
          "time": "20210822 03:40"
        },
        {
          "id": "MDRW2100007587.1.27",
          "form": "마쟈 친구들 평이 좋았어",
          "original_form": "마쟈 친구들 평이 좋았어",
          "speaker_id": "1",
          "time": "20210822 03:40"
        },
        {
          "id": "MDRW2100007587.1.28",
          "form": "맞아ㅠㅠ 설거지 너무 힘들것같아",
          "original_form": "맞아ㅠㅠ 설거지 너무 힘들것같아",
          "speaker_id": "2",
          "time": "20210822 03:40"
        },
        {
          "id": "MDRW2100007587.1.29",
          "form": "컴공이라 밤 새는데 익숙해서 그런지 야간 마니 가더라",
          "original_form": "컴공이라 밤 새는데 익숙해서 그런지 야간 마니 가더라",
          "speaker_id": "1",
          "time": "20210822 03:41"
        },
        {
          "id": "MDRW2100007587.1.30",
          "form": "그 많은 그릇들을 닦아야하니까ㅠㅠㅠ",
          "original_form": "그 많은 그릇들을 닦아야하니까ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 03:41"
        },
        {
          "id": "MDRW2100007587.1.31",
          "form": "헐 컴공이구나 신기신기",
          "original_form": "헐 컴공이구나 신기신기",
          "speaker_id": "2",
          "time": "20210822 03:41"
        },
        {
          "id": "MDRW2100007587.1.32",
          "form": "취업도 그쪽으로 하겠네!!",
          "original_form": "취업도 그쪽으로 하겠네!!",
          "speaker_id": "2",
          "time": "20210822 03:41"
        },
        {
          "id": "MDRW2100007587.1.33",
          "form": "컴퓨터 멋져부러,,,",
          "original_form": "컴퓨터 멋져부러,,,",
          "speaker_id": "2",
          "time": "20210822 03:41"
        },
        {
          "id": "MDRW2100007587.1.34",
          "form": "ㅋㅋㅋㅋ웅",
          "original_form": "ㅋㅋㅋㅋ웅",
          "speaker_id": "1",
          "time": "20210822 03:41"
        }
      ]
