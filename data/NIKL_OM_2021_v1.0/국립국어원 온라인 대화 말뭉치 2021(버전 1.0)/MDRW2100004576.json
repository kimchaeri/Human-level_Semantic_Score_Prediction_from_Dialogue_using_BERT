      "utterance": [
        {
          "id": "MDRW2100004576.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004576.1.2",
          "form": "네 당근 마켓 종종해요",
          "original_form": "네 당근 마켓 종종해요",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004576.1.3",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004576.1.4",
          "form": "저도 당근",
          "original_form": "저도 당근",
          "speaker_id": "1",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004576.1.5",
          "form": "저는 주로 책 거래했어요",
          "original_form": "저는 주로 책 거래했어요",
          "speaker_id": "2",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004576.1.6",
          "form": "수요가 있나요?",
          "original_form": "수요가 있나요?",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004576.1.7",
          "form": "안 쓰는 책들 거레한 거여서 큰 이득은 없었는데",
          "original_form": "안 쓰는 책들 거레한 거여서 큰 이득은 없었는데",
          "speaker_id": "2",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004576.1.8",
          "form": "그래도 안 쓰는 거 이것저것 팔면 용돈 정도 벌어요",
          "original_form": "그래도 안 쓰는 거 이것저것 팔면 용돈 정도 벌어요",
          "speaker_id": "2",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004576.1.9",
          "form": "맞아뇨",
          "original_form": "맞아뇨",
          "speaker_id": "1",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004576.1.10",
          "form": "창고에 쳐박아두니 돈이라도",
          "original_form": "창고에 쳐박아두니 돈이라도",
          "speaker_id": "1",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004576.1.11",
          "form": "맞아요 당근 참 잘 만든 거 같아요",
          "original_form": "맞아요 당근 참 잘 만든 거 같아요",
          "speaker_id": "2",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004576.1.12",
          "form": "원래 중고거래 어렵게 생각해서 거부감들었는데",
          "original_form": "원래 중고거래 어렵게 생각해서 거부감들었는데",
          "speaker_id": "2",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004576.1.13",
          "form": "안 버리고 파니까 재밌더라고요",
          "original_form": "안 버리고 파니까 재밌더라고요",
          "speaker_id": "2",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004576.1.14",
          "form": "쏠쏠하죠",
          "original_form": "쏠쏠하죠",
          "speaker_id": "1",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004576.1.15",
          "form": "최근에 기프티콘도 팔았는데",
          "original_form": "최근에 기프티콘도 팔았는데",
          "speaker_id": "2",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004576.1.16",
          "form": "편하고 좋더라고요",
          "original_form": "편하고 좋더라고요",
          "speaker_id": "2",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004576.1.17",
          "form": "정말 다양한 물건을 거래할 수 있어서 좋은 거 같아요",
          "original_form": "정말 다양한 물건을 거래할 수 있어서 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004576.1.18",
          "form": "네 필요한 물건도 많아요",
          "original_form": "네 필요한 물건도 많아요",
          "speaker_id": "1",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004576.1.19",
          "form": "한 번 하니까 재미들려서 계속하게 되더라고요",
          "original_form": "한 번 하니까 재미들려서 계속하게 되더라고요",
          "speaker_id": "2",
          "time": "20210821 17:52"
        },
