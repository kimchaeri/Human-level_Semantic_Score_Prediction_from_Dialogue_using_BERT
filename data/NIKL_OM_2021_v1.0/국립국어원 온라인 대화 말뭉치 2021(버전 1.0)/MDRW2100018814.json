      "utterance": [
        {
          "id": "MDRW2100018814.1.1",
          "form": "식단이랑 운동도 하시는거에요?",
          "original_form": "식단이랑 운동도 하시는거에요?",
          "speaker_id": "1",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018814.1.2",
          "form": "네! 아침운동하고 있어요",
          "original_form": "네! 아침운동하고 있어요",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018814.1.3",
          "form": "운동하시나요?",
          "original_form": "운동하시나요?",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018814.1.4",
          "form": "부지런하시네여? 아침에 인나자 마자 하시는거에요? 전 홈트요",
          "original_form": "부지런하시네여? 아침에 인나자 마자 하시는거에요? 전 홈트요",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018814.1.5",
          "form": "저녁에 했는데 아침이 더 잘맞더라구요",
          "original_form": "저녁에 했는데 아침이 더 잘맞더라구요",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018814.1.6",
          "form": "저는 걷기요!",
          "original_form": "저는 걷기요!",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018814.1.7",
          "form": "아침엔 기운 없지 않아요?",
          "original_form": "아침엔 기운 없지 않아요?",
          "speaker_id": "1",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018814.1.8",
          "form": "나가기까지는 그런데 걷다보면 풀리는 기분이 들더라구요",
          "original_form": "나가기까지는 그런데 걷다보면 풀리는 기분이 들더라구요",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018814.1.9",
          "form": "관절은 괜찮으세요? 전 아침에 하면 뼈마디가.. ㅋㅋㅋ",
          "original_form": "관절은 괜찮으세요? 전 아침에 하면 뼈마디가.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018814.1.10",
          "form": "msm을 챙기면 보존하고 있어요..ㅋㅋㅋ",
          "original_form": "msm을 챙기면 보존하고 있어요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018814.1.11",
          "form": "그게 뭐에요?",
          "original_form": "그게 뭐에요?",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018814.1.12",
          "form": "식이유황이라고 관절에 좋다더라구요",
          "original_form": "식이유황이라고 관절에 좋다더라구요",
          "speaker_id": "2",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018814.1.13",
          "form": "ㅋㅋㅋ 벌써 그런거도 드시는거에요?",
          "original_form": "ㅋㅋㅋ 벌써 그런거도 드시는거에요?",
          "speaker_id": "1",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100018814.1.14",
          "form": "다른 영양제고 드시나요?",
          "original_form": "다른 영양제고 드시나요?",
          "speaker_id": "1",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100018814.1.15",
          "form": "관절은 미리 챙겨야한다해서ㅋㅋㅋ",
          "original_form": "관절은 미리 챙겨야한다해서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018814.1.16",
          "form": "그거랑 유산균이요!",
          "original_form": "그거랑 유산균이요!",
          "speaker_id": "2",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018814.1.17",
          "form": "비타민하고 루테인은 생각날때만 먹어요ㅋㅋ",
          "original_form": "비타민하고 루테인은 생각날때만 먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:46"
        },
        {
          "id": "MDRW2100018814.1.18",
          "form": "드시는거 있으세요?",
          "original_form": "드시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210827 12:46"
        },
