      "utterance": [
        {
          "id": "MDRW2100005435.1.1",
          "form": "안녕하세욤",
          "original_form": "안녕하세욤",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005435.1.2",
          "form": "앗 좋아요.ㅋㅋㅋㅋ",
          "original_form": "앗 좋아요.ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005435.1.3",
          "form": "간짜장 좋아하시나봐요",
          "original_form": "간짜장 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005435.1.4",
          "form": "넹ㅋㅋㅋ 귤 좋아하시나요??",
          "original_form": "넹ㅋㅋㅋ 귤 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005435.1.5",
          "form": "그럼요..",
          "original_form": "그럼요..",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005435.1.6",
          "form": "간짜장도 좋아합니다.ㅋㅋ",
          "original_form": "간짜장도 좋아합니다.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005435.1.7",
          "form": "양파가 아삭아삭 씹히는게 좋아요.ㅋㅋ",
          "original_form": "양파가 아삭아삭 씹히는게 좋아요.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005435.1.8",
          "form": "먹을 줄 아시는군요",
          "original_form": "먹을 줄 아시는군요",
          "speaker_id": "1",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005435.1.9",
          "form": "간짜장의 핵심은 양파라고 생각해요",
          "original_form": "간짜장의 핵심은 양파라고 생각해요",
          "speaker_id": "1",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005435.1.10",
          "form": "그쵸.. 가끔 너무 익힌 집들 있어서...",
          "original_form": "그쵸.. 가끔 너무 익힌 집들 있어서...",
          "speaker_id": "2",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005435.1.11",
          "form": "아삭아삭하고 큰 양파는 먹기 넘 좋아요",
          "original_form": "아삭아삭하고 큰 양파는 먹기 넘 좋아요",
          "speaker_id": "1",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005435.1.12",
          "form": "면 다 먹고 남은 양파까지 싹싹...",
          "original_form": "면 다 먹고 남은 양파까지 싹싹...",
          "speaker_id": "1",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005435.1.13",
          "form": "그쵸그쵸.. 저는 생양파도 추가 요청하는 편이에요.ㅋㅋㅋ",
          "original_form": "그쵸그쵸.. 저는 생양파도 추가 요청하는 편이에요.ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005435.1.14",
          "form": "헉 저두요ㅠ 양파 최고",
          "original_form": "헉 저두요ㅠ 양파 최고",
          "speaker_id": "1",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005435.1.15",
          "form": "중화요리는 양파 없으면 못먹어요... 너무 기름져서..",
          "original_form": "중화요리는 양파 없으면 못먹어요... 너무 기름져서..",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005435.1.16",
          "form": "양파가 진짜 입가심 해주는 역할!!",
          "original_form": "양파가 진짜 입가심 해주는 역할!!",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005435.1.17",
          "form": "중화요리는 간짜장만 좋아하세요?",
          "original_form": "중화요리는 간짜장만 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005435.1.18",
          "form": "음 아뇨 간짜장을 제일 좋아하긴 하는데",
          "original_form": "음 아뇨 간짜장을 제일 좋아하긴 하는데",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005435.1.19",
          "form": "짬뽕이나 깐풍기 등등 다른 요리도 먹어요",
          "original_form": "짬뽕이나 깐풍기 등등 다른 요리도 먹어요",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005435.1.20",
          "form": "ㅋㅋㅋ저랑 비슷하시네요.ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ저랑 비슷하시네요.ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005435.1.21",
          "form": "사실 저는 치킨을 가장 좋아하는데..",
          "original_form": "사실 저는 치킨을 가장 좋아하는데..",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005435.1.22",
          "form": "치킨은 안드시나요??",
          "original_form": "치킨은 안드시나요??",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005435.1.23",
          "form": "치킨 너무 좋아하죠 ㅎㅎ",
          "original_form": "치킨 너무 좋아하죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005435.1.24",
          "form": "별명이 치킨 킬러랍니다...",
          "original_form": "별명이 치킨 킬러랍니다...",
          "speaker_id": "1",
          "time": "20210821 22:39"
        }
      ]
    }
