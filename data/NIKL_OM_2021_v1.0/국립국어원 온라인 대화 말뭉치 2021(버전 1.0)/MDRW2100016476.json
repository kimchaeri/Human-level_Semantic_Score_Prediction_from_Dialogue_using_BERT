          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016476.1.5",
          "form": "종교 있으신가요?",
          "original_form": "종교 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016476.1.6",
          "form": "무교예요",
          "original_form": "무교예요",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016476.1.7",
          "form": "절가보신적 있으신가요",
          "original_form": "절가보신적 있으신가요",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016476.1.8",
          "form": "네 가본적 있어요",
