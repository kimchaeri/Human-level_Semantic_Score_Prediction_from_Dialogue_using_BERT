      "utterance": [
        {
          "id": "MDRW2100034148.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100034148.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100034148.1.3",
          "form": "오늘 날씨 좋았나요?",
          "original_form": "오늘 날씨 좋았나요?",
          "speaker_id": "1",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100034148.1.4",
          "form": "네 여기는 선선하니 좋네요 ㅋㅋ",
          "original_form": "네 여기는 선선하니 좋네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:32"
        },
        {
          "id": "MDRW2100034148.1.5",
          "form": "여기는 오늘 하늘은 맑은데 너무 습해서 좋다고 하기가 애매해요 ㅎㅎ",
          "original_form": "여기는 오늘 하늘은 맑은데 너무 습해서 좋다고 하기가 애매해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100034148.1.6",
          "form": "벌써 가을 날씨 같겠어요!",
          "original_form": "벌써 가을 날씨 같겠어요!",
          "speaker_id": "1",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100034148.1.7",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100034148.1.8",
          "form": "요새 가을장마라더라구요 ㅠ",
          "original_form": "요새 가을장마라더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100034148.1.9",
          "form": "오늘 밤부터 또 주말내내 비가 온다네요ㅠ",
          "original_form": "오늘 밤부터 또 주말내내 비가 온다네요ㅠ",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100034148.1.10",
          "form": "요즘은 기상 이변 때문인지 비가 너무... 너무 많이 와요ㅠ",
          "original_form": "요즘은 기상 이변 때문인지 비가 너무... 너무 많이 와요ㅠ",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100034148.1.11",
          "form": "아그래요? 너무 와도 골치인데 ㅠㅠ",
          "original_form": "아그래요? 너무 와도 골치인데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100034148.1.12",
          "form": "그러게요ㅠ\r\n얼마전에 피해본 지역 좀 있던데 이번엔 그냥 적당히 왔다가 갔으면 좋겠어요ㅠ",
          "original_form": "그러게요ㅠ\r\n얼마전에 피해본 지역 좀 있던데 이번엔 그냥 적당히 왔다가 갔으면 좋겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100034148.1.13",
          "form": "맞아요 너무 와서 빨래도 골치에요..ㅋㅋㅋ",
          "original_form": "맞아요 너무 와서 빨래도 골치에요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100034148.1.14",
          "form": "그쵸ㅠ 건조기에 돌려도 습기 먹은 냄새가 나서 너무 속상해요ㅠ",
          "original_form": "그쵸ㅠ 건조기에 돌려도 습기 먹은 냄새가 나서 너무 속상해요ㅠ",
          "speaker_id": "1",
          "time": "20210827 18:40"
        },
        {
          "id": "MDRW2100034148.1.15",
          "form": "이불은 매일 세탁 돌릴수도 없는데ㅠ",
          "original_form": "이불은 매일 세탁 돌릴수도 없는데ㅠ",
          "speaker_id": "1",
          "time": "20210827 18:40"
        },
        {
          "id": "MDRW2100034148.1.16",
          "form": "맞아요 꿉꿉한 냄새 때문에...ㅠㅠ",
          "original_form": "맞아요 꿉꿉한 냄새 때문에...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 18:41"
        },
        {
          "id": "MDRW2100034148.1.17",
          "form": "얼른 날씨가 개면 좋겠어요",
          "original_form": "얼른 날씨가 개면 좋겠어요",
          "speaker_id": "2",
          "time": "20210827 18:41"
        },
        {
          "id": "MDRW2100034148.1.18",
          "form": "빨래 마르는 것만 보면 여름이 참 좋은데..\r\n그래도 여름은 좀 빨리 갔으면 좋겠어요 ㅎㅎ",
          "original_form": "빨래 마르는 것만 보면 여름이 참 좋은데..\r\n그래도 여름은 좀 빨리 갔으면 좋겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:43"
        }
