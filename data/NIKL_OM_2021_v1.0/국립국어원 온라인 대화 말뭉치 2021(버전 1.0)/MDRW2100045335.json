      "utterance": [
        {
          "id": "MDRW2100045335.1.1",
          "form": "언니 요새 관심있는 게임 있어?",
          "original_form": "언니 요새 관심있는 게임 있어?",
          "speaker_id": "1",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045335.1.2",
          "form": "요새는 다시 옛날로 돌아가서 가벼운스팀겜에 관심이가",
          "original_form": "요새는 다시 옛날로 돌아가서 가벼운스팀겜에 관심이가",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045335.1.3",
          "form": "언패킹이라는 도트게임이있는데",
          "original_form": "언패킹이라는 도트게임이있는데",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045335.1.4",
          "form": "소소하게 힐링될거같아서 기대중이야",
          "original_form": "소소하게 힐링될거같아서 기대중이야",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045335.1.5",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045335.1.6",
          "form": "오!!! 나는 요새 스위치로 스카이워드소드 하는데",
          "original_form": "오!!! 나는 요새 스위치로 스카이워드소드 하는데",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045335.1.7",
          "form": "진짜 재밌어 ㅋㅋ 요며칠 못하긴했는데",
          "original_form": "진짜 재밌어 ㅋㅋ 요며칠 못하긴했는데",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045335.1.8",
          "form": "그거 진짜 재밌어보이더라ㅠㅠ 부러워",
          "original_form": "그거 진짜 재밌어보이더라ㅠㅠ 부러워",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045335.1.9",
          "form": "너 혹시 농장게임같은건 취향이아닌가?",
          "original_form": "너 혹시 농장게임같은건 취향이아닌가?",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045335.1.10",
          "form": "너랑 스타듀밸리 멀티모드하면 재밌을거같아",
          "original_form": "너랑 스타듀밸리 멀티모드하면 재밌을거같아",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045335.1.11",
          "form": "아 그거 스팀게임인가?",
          "original_form": "아 그거 스팀게임인가?",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045335.1.12",
          "form": "사실 뭐하는 게임인지 잘 몰라 알려주면 안돼?!",
          "original_form": "사실 뭐하는 게임인지 잘 몰라 알려주면 안돼?!",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045335.1.13",
          "form": "농장을키우고 사냥도하고 연애도하고 모험도하고",
          "original_form": "농장을키우고 사냥도하고 연애도하고 모험도하고",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045335.1.14",
          "form": "나원래 타이쿤식 게임안좋아하는데",
          "original_form": "나원래 타이쿤식 게임안좋아하는데",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045335.1.15",
          "form": "이건 진짜 재밌어서",
          "original_form": "이건 진짜 재밌어서",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045335.1.16",
          "form": "같이 해보면 재밌을거같아!!",
          "original_form": "같이 해보면 재밌을거같아!!",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045335.1.17",
          "form": "오!! 좋아 그럼 다음에 세일하면 살게 같이하자!",
          "original_form": "오!! 좋아 그럼 다음에 세일하면 살게 같이하자!",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045335.1.18",
          "form": "헐 대박 나야 완전좋아",
          "original_form": "헐 대박 나야 완전좋아",
          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045335.1.19",
          "form": "사실 멀티로 같이하는 게임 무서운데 이건재밌을거같아",
          "original_form": "사실 멀티로 같이하는 게임 무서운데 이건재밌을거같아",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045335.1.20",
          "form": "내가 트롤짓할까봐 무서운데..이건 그런거 없을 것 같아서 ㅋㅋㅋ",
          "original_form": "내가 트롤짓할까봐 무서운데..이건 그런거 없을 것 같아서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045335.1.21",
          "form": "아냐 그런게임 아니야!! 완전힐링게임이야",
          "original_form": "아냐 그런게임 아니야!! 완전힐링게임이야",
          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045335.1.22",
          "form": "그리고 리텍하면",
          "original_form": "그리고 리텍하면",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045335.1.23",
          "form": "npc도 잘생기게 만들수있어",
          "original_form": "npc도 잘생기게 만들수있어",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045335.1.24",
          "form": "오 좋다 그럼 npc 이케맨으로 만들어야지",
          "original_form": "오 좋다 그럼 npc 이케맨으로 만들어야지",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045335.1.25",
          "form": "사실 지금도 메이플켜놨어",
          "original_form": "사실 지금도 메이플켜놨어",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045335.1.26",
          "form": "내가 메이플만 안했어도 돈을 훨씬 아꼈겠지",
          "original_form": "내가 메이플만 안했어도 돈을 훨씬 아꼈겠지",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045335.1.27",
          "form": "아 메이플 사냥 돌리는거야? 자동전투로?",
          "original_form": "아 메이플 사냥 돌리는거야? 자동전투로?",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045335.1.28",
          "form": "아니 ㅋㅋ 모든걸 내가 하는 알피지게임",
          "original_form": "아니 ㅋㅋ 모든걸 내가 하는 알피지게임",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045335.1.29",
          "form": "피씨게임에는 자동전투없어",
          "original_form": "피씨게임에는 자동전투없어",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045335.1.30",
          "form": "아 있는것도있나 암튼 메이플은없어",
          "original_form": "아 있는것도있나 암튼 메이플은없어",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045335.1.31",
          "form": "와 헐 충격이다",
          "original_form": "와 헐 충격이다",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045335.1.32",
          "form": "피씨게임을 안해서 잘 몰랐어",
          "original_form": "피씨게임을 안해서 잘 몰랐어",
          "speaker_id": "1",
          "time": "20210908 02:05"
        }
      ]
