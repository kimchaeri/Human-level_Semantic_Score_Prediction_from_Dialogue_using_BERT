      "utterance": [
        {
          "id": "MDRW2100014410.1.1",
          "form": "불고기는 애들 1순위죠",
          "original_form": "불고기는 애들 1순위죠",
          "speaker_id": "2",
          "time": "20210825 18:52"
        },
        {
          "id": "MDRW2100014410.1.2",
          "form": "피자랑 사이다로 저녁끝내니",
          "original_form": "피자랑 사이다로 저녁끝내니",
          "speaker_id": "1",
          "time": "20210825 18:52"
        },
        {
          "id": "MDRW2100014410.1.3",
          "form": "저녁으로 딱",
          "original_form": "저녁으로 딱",
          "speaker_id": "2",
          "time": "20210825 18:52"
        },
        {
          "id": "MDRW2100014410.1.4",
          "form": "편하네요 ㅎ",
          "original_form": "편하네요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 18:52"
        },
        {
          "id": "MDRW2100014410.1.5",
          "form": "외국인도 1위로 꼽죠 ㅎㅎ",
          "original_form": "외국인도 1위로 꼽죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:52"
        },
        {
          "id": "MDRW2100014410.1.6",
          "form": "치울것도 준비할것도 없이 ㅋ",
          "original_form": "치울것도 준비할것도 없이 ㅋ",
          "speaker_id": "1",
          "time": "20210825 18:52"
        },
        {
          "id": "MDRW2100014410.1.7",
          "form": "그럼요 엄마도 간단히 하고 쉬어야죠 ㅋㅋ",
          "original_form": "그럼요 엄마도 간단히 하고 쉬어야죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:53"
        },
        {
          "id": "MDRW2100014410.1.8",
          "form": "퇴근할때 픽업이 준비끝 ㅎ",
          "original_form": "퇴근할때 픽업이 준비끝 ㅎ",
          "speaker_id": "1",
          "time": "20210825 18:53"
        },
        {
          "id": "MDRW2100014410.1.9",
          "form": "특히 50프로 라니 ㅋㅋ",
          "original_form": "특히 50프로 라니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:53"
        },
        {
          "id": "MDRW2100014410.1.10",
          "form": "오 일석이죠 ㅋㅋ",
          "original_form": "오 일석이죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:53"
        },
        {
          "id": "MDRW2100014410.1.11",
          "form": "퇴근하고 밥가져오고",
          "original_form": "퇴근하고 밥가져오고",
          "speaker_id": "2",
          "time": "20210825 18:53"
        },
        {
          "id": "MDRW2100014410.1.12",
          "form": "진짜 좋은데요",
          "original_form": "진짜 좋은데요",
          "speaker_id": "2",
          "time": "20210825 18:53"
        },
        {
          "id": "MDRW2100014410.1.13",
          "form": "50퍼에 페이코인아세요?",
          "original_form": "50퍼에 페이코인아세요?",
          "speaker_id": "1",
          "time": "20210825 18:53"
        },
        {
          "id": "MDRW2100014410.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 18:53"
        },
        {
          "id": "MDRW2100014410.1.15",
          "form": "저도해서 편의점에서 사먹어요",
          "original_form": "저도해서 편의점에서 사먹어요",
          "speaker_id": "2",
          "time": "20210825 18:53"
        },
        {
          "id": "MDRW2100014410.1.16",
          "form": "작년에 페이코인가입하고 머사고 그랬던거",
          "original_form": "작년에 페이코인가입하고 머사고 그랬던거",
          "speaker_id": "1",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014410.1.17",
          "form": "네 그럼요 ㅋㅋ",
          "original_form": "네 그럼요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014410.1.18",
          "form": "같은데 그거 남아있어서",
          "original_form": "같은데 그거 남아있어서",
          "speaker_id": "1",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014410.1.19",
          "form": "그걸로 결제했어요",
          "original_form": "그걸로 결제했어요",
          "speaker_id": "1",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014410.1.20",
          "form": "그게 좀되시나봐요?",
          "original_form": "그게 좀되시나봐요?",
          "speaker_id": "2",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014410.1.21",
          "form": "어머머",
          "original_form": "어머머",
          "speaker_id": "2",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014410.1.22",
          "form": "공짜로 먹은듯한 느낌 ㅋ",
          "original_form": "공짜로 먹은듯한 느낌 ㅋ",
          "speaker_id": "1",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014410.1.23",
          "form": "대박입니다",
          "original_form": "대박입니다",
          "speaker_id": "2",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014410.1.24",
          "form": "전 ㅡㅡ",
          "original_form": "전 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014410.1.25",
          "form": "아뇨 편의점에서 그때",
          "original_form": "아뇨 편의점에서 그때",
          "speaker_id": "1",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014410.1.26",
          "form": "폰바꾸고 사라져서 ㅜㅜ",
          "original_form": "폰바꾸고 사라져서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014410.1.27",
          "form": "새로 조금받았어요",
          "original_form": "새로 조금받았어요",
          "speaker_id": "2",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014410.1.28",
          "form": "다털고 100원단위로 남았었는데",
          "original_form": "다털고 100원단위로 남았었는데",
          "speaker_id": "1",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014410.1.29",
          "form": "어디로 간건지",
          "original_form": "어디로 간건지",
          "speaker_id": "2",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014410.1.30",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014410.1.31",
          "form": "요새 코인이 급등해서",
          "original_form": "요새 코인이 급등해서",
          "speaker_id": "1",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014410.1.32",
          "form": "저도 다 털었어요 ㅋㅋ",
          "original_form": "저도 다 털었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014410.1.33",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014410.1.34",
          "form": "9만원되어있더라구요 ㅎ",
          "original_form": "9만원되어있더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014410.1.35",
          "form": "아 슬퍼요 저는",
          "original_form": "아 슬퍼요 저는",
          "speaker_id": "2",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014410.1.36",
          "form": "근데 더오를줄알고 ㅜㅠ",
          "original_form": "근데 더오를줄알고 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014410.1.37",
          "form": "저녁을 날렸네요 ㅋㅋ",
          "original_form": "저녁을 날렸네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014410.1.38",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014410.1.39",
          "form": "세상에 9만원이요?",
          "original_form": "세상에 9만원이요?",
          "speaker_id": "2",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014410.1.40",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014410.1.41",
          "form": "그럼전 ㅜㅜ얼미였던건지",
          "original_form": "그럼전 ㅜㅜ얼미였던건지",
          "speaker_id": "2",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014410.1.42",
          "form": "놔뒀더니 지금은 떨어져서 3만원대더라구요",
          "original_form": "놔뒀더니 지금은 떨어져서 3만원대더라구요",
          "speaker_id": "1",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014410.1.43",
          "form": "정말 맛나게 쓰셨네요 ㅎㅎ",
          "original_form": "정말 맛나게 쓰셨네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100014410.1.44",
          "form": "그래도 그게 어디예요 ㅎㅎ",
          "original_form": "그래도 그게 어디예요 ㅎㅎ",
