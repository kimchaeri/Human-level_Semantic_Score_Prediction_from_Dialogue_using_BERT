      "utterance": [
        {
          "id": "MDRW2100024425.1.1",
          "form": "시키더라곸 좀 알려주고",
          "original_form": "시키더라곸 좀 알려주고",
          "speaker_id": "2",
          "time": "20210831 08:46"
        },
        {
          "id": "MDRW2100024425.1.2",
          "form": "어려운건 가져가는데",
          "original_form": "어려운건 가져가는데",
          "speaker_id": "2",
          "time": "20210831 08:46"
        },
        {
          "id": "MDRW2100024425.1.3",
          "form": "일반적인 등기나 우편은 좀 해봄..",
          "original_form": "일반적인 등기나 우편은 좀 해봄..",
          "speaker_id": "2",
          "time": "20210831 08:47"
        },
        {
          "id": "MDRW2100024425.1.4",
          "form": "어려운게 따로 있어?",
          "original_form": "어려운게 따로 있어?",
          "speaker_id": "1",
          "time": "20210831 08:48"
        },
        {
          "id": "MDRW2100024425.1.5",
          "form": "내용증명..?",
          "original_form": "내용증명..?",
          "speaker_id": "2",
          "time": "20210831 08:48"
        },
        {
          "id": "MDRW2100024425.1.6",
          "form": "아하....",
          "original_form": "아하....",
          "speaker_id": "1",
          "time": "20210831 08:48"
        },
        {
          "id": "MDRW2100024425.1.7",
          "form": "그게 있었구나 ㅋㅋㅋㅋ",
          "original_form": "그게 있었구나 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 08:48"
        },
        {
          "id": "MDRW2100024425.1.8",
          "form": "뭐 복잡하더라곸ㅋㅋㅋ",
          "original_form": "뭐 복잡하더라곸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 08:49"
        },
        {
          "id": "MDRW2100024425.1.9",
          "form": "그치 복사하고 도장찍고",
          "original_form": "그치 복사하고 도장찍고",
          "speaker_id": "1",
          "time": "20210831 08:49"
        },
        {
          "id": "MDRW2100024425.1.10",
          "form": "막 나도 가지고 너도 가지고 보내기도 하고",
          "original_form": "막 나도 가지고 너도 가지고 보내기도 하고",
          "speaker_id": "2",
          "time": "20210831 08:49"
        },
        {
          "id": "MDRW2100024425.1.11",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210831 08:49"
        },
        {
          "id": "MDRW2100024425.1.12",
          "form": "ㅇㅇ 3부 만드는거라서 ㅋㅋㅋ",
          "original_form": "ㅇㅇ 3부 만드는거라서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 08:49"
        },
        {
          "id": "MDRW2100024425.1.13",
          "form": "그래서 배워야 하냐고 물었더니",
          "original_form": "그래서 배워야 하냐고 물었더니",
          "speaker_id": "2",
          "time": "20210831 08:49"
        },
        {
          "id": "MDRW2100024425.1.14",
          "form": "이것까지 배울필요는 없대",
          "original_form": "이것까지 배울필요는 없대",
          "speaker_id": "2",
          "time": "20210831 08:49"
        },
        {
          "id": "MDRW2100024425.1.15",
          "form": "ㅇㅇ..",
          "original_form": "ㅇㅇ..",
          "speaker_id": "2",
          "time": "20210831 08:49"
        },
        {
          "id": "MDRW2100024425.1.16",
          "form": "쿨하시네 ㅋㅋㅋㅋ",
          "original_form": "쿨하시네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 08:49"
        },
        {
          "id": "MDRW2100024425.1.17",
          "form": "ㅋㅋㅋㅋㅋ다른것도 많으니까 그러신 모양이야",
          "original_form": "ㅋㅋㅋㅋㅋ다른것도 많으니까 그러신 모양이야",
          "speaker_id": "2",
          "time": "20210831 08:49"
        },
        {
          "id": "MDRW2100024425.1.18",
          "form": "준등기 해봤어?",
          "original_form": "준등기 해봤어?",
          "speaker_id": "1",
          "time": "20210831 08:49"
        },
        {
          "id": "MDRW2100024425.1.19",
          "form": "ㅇㅇ 해봤너",
          "original_form": "ㅇㅇ 해봤너",
          "speaker_id": "2",
          "time": "20210831 08:50"
        },
        {
          "id": "MDRW2100024425.1.20",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210831 08:50"
        },
        {
          "id": "MDRW2100024425.1.21",
          "form": "준등기 다량으로 보낸게 청첩장이었지",
          "original_form": "준등기 다량으로 보낸게 청첩장이었지",
          "speaker_id": "2",
          "time": "20210831 08:50"
        },
        {
          "id": "MDRW2100024425.1.22",
          "form": "사수는 따로 있어??",
          "original_form": "사수는 따로 있어??",
          "speaker_id": "1",
          "time": "20210831 08:50"
        },
        {
          "id": "MDRW2100024425.1.23",
          "form": "아 그게 준등기였군 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 그게 준등기였군 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 08:50"
        },
        {
          "id": "MDRW2100024425.1.24",
          "form": "일단 한분 계시고 모르면 그냥 다 물어봐 찔러보기",
          "original_form": "일단 한분 계시고 모르면 그냥 다 물어봐 찔러보기",
          "speaker_id": "2",
          "time": "20210831 08:50"
        },
        {
          "id": "MDRW2100024425.1.25",
          "form": "ㅋㅋㅋㅋ ㅇㅇ 그게 준등기였엌",
          "original_form": "ㅋㅋㅋㅋ ㅇㅇ 그게 준등기였엌",
          "speaker_id": "2",
          "time": "20210831 08:50"
        },
        {
          "id": "MDRW2100024425.1.26",
          "form": "등기는 비싸잖아..",
          "original_form": "등기는 비싸잖아..",
          "speaker_id": "2",
          "time": "20210831 08:50"
        },
        {
          "id": "MDRW2100024425.1.27",
          "form": "난 청첩장이라 일반우편으로 보냈을줄...",
          "original_form": "난 청첩장이라 일반우편으로 보냈을줄...",
          "speaker_id": "1",
          "time": "20210831 08:51"
        },
        {
          "id": "MDRW2100024425.1.28",
          "form": "ㅇㅎ 그렇게 생각할수도 있겠다",
          "original_form": "ㅇㅎ 그렇게 생각할수도 있겠다",
          "speaker_id": "2",
          "time": "20210831 08:51"
        },
        {
          "id": "MDRW2100024425.1.29",
          "form": "일반우편은 추적이 안되긴 하니까",
          "original_form": "일반우편은 추적이 안되긴 하니까",
          "speaker_id": "2",
          "time": "20210831 08:51"
        },
        {
          "id": "MDRW2100024425.1.30",
          "form": "그냥 준등기로 보낸듯?",
          "original_form": "그냥 준등기로 보낸듯?",
          "speaker_id": "2",
          "time": "20210831 08:51"
        },
        {
          "id": "MDRW2100024425.1.31",
          "form": "업무 스트레스 다 털고 가리라(??)",
          "original_form": "업무 스트레스 다 털고 가리라(??)",
          "speaker_id": "2",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100024425.1.32",
          "form": "헛소리해도 그러려니해...",
          "original_form": "헛소리해도 그러려니해...",
          "speaker_id": "2",
          "time": "20210901 20:36"
        }
      ]
    }
