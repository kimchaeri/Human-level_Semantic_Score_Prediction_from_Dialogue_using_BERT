          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014102.1.37",
          "form": "진짜 매우 이상한 곳에 있는 구하기 힘든 보물도",
          "original_form": "진짜 매우 이상한 곳에 있는 구하기 힘든 보물도",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014102.1.38",
          "form": "오타쿠",
          "original_form": "오타쿠",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014102.1.39",
          "form": "입다물어",
          "original_form": "입다물어",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014102.1.40",
          "form": "다 구해다주고",
          "original_form": "다 구해다주고",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
