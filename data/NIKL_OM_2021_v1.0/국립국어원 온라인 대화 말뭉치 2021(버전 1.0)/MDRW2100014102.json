          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014102.1.70",
          "form": "그렇군요....",
          "original_form": "그렇군요....",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
