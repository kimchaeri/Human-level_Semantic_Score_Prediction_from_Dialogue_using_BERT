          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014102.1.12",
          "form": "서있는게 고작인",
          "original_form": "서있는게 고작인",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014102.1.13",
          "form": "깡마른 바이킹",
          "original_form": "깡마른 바이킹",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014102.1.14",
          "form": "복근은 있음",
          "original_form": "복근은 있음",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014102.1.15",
          "form": "안돼요",
          "original_form": "안돼요",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014102.1.16",
          "form": "오래살것같아",
          "original_form": "오래살것같아",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014102.1.17",
          "form": "에이보르 인생에 자아가 없어서",
          "original_form": "에이보르 인생에 자아가 없어서",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014102.1.18",
          "form": "아내한테 전재산 일임하고",
          "original_form": "아내한테 전재산 일임하고",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014102.1.19",
