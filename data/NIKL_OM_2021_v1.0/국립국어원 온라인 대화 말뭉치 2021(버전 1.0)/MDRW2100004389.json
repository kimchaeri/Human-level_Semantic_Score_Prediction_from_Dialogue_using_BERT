      "utterance": [
        {
          "id": "MDRW2100004389.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004389.1.2",
          "form": "아뇨ㅜㅜ보는것만 좋아해서...키우세요?",
          "original_form": "아뇨ㅜㅜ보는것만 좋아해서...키우세요?",
          "speaker_id": "2",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004389.1.3",
          "form": "아뇽ㅜㅜ 여건이 안되가지고..",
          "original_form": "아뇽ㅜㅜ 여건이 안되가지고..",
          "speaker_id": "1",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004389.1.4",
          "form": "2년전에",
          "original_form": "2년전에",
          "speaker_id": "1",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004389.1.5",
          "form": "햄스터 키웠었어요",
          "original_form": "햄스터 키웠었어요",
          "speaker_id": "1",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004389.1.6",
          "form": "아 햄스터ㅋㅋ귀여운데 키울때 힘들다 하더라구요",
          "original_form": "아 햄스터ㅋㅋ귀여운데 키울때 힘들다 하더라구요",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004389.1.7",
          "form": "맞아요 그래도 귀여워서ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 그래도 귀여워서ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004389.1.8",
          "form": "저도 강아지나 고양이 키우고 싶은데 왠지 엄두가 안나요",
          "original_form": "저도 강아지나 고양이 키우고 싶은데 왠지 엄두가 안나요",
          "speaker_id": "2",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004389.1.9",
          "form": "그쵸 돈도 엄청 들고...",
          "original_form": "그쵸 돈도 엄청 들고...",
          "speaker_id": "1",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004389.1.10",
          "form": "그래서 그냥 보는걸로만 만족하고있어요ㅎㅎ",
          "original_form": "그래서 그냥 보는걸로만 만족하고있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004389.1.11",
          "form": "길거리에서 보면 넘 귀여워요",
          "original_form": "길거리에서 보면 넘 귀여워요",
          "speaker_id": "1",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004389.1.12",
          "form": "맞아요ㅋㅋ언젠가는키워야지 하고ㅎㅎ",
          "original_form": "맞아요ㅋㅋ언젠가는키워야지 하고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004389.1.13",
          "form": "마자요 햄스터 잡식인거 아세요?!",
          "original_form": "마자요 햄스터 잡식인거 아세요?!",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004389.1.14",
          "form": "잡식이면 고기종류도 먹나여??해바라기씨만 먹는줄 알았는데ㅎㅎ",
          "original_form": "잡식이면 고기종류도 먹나여??해바라기씨만 먹는줄 알았는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004389.1.15",
          "form": "네네 먹어요ㅋㅋㅋㅋㅋㅋㅋ양념 안된 돼지고기 줬었는데 잘먹더라구요",
          "original_form": "네네 먹어요ㅋㅋㅋㅋㅋㅋㅋ양념 안된 돼지고기 줬었는데 잘먹더라구요",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004389.1.16",
          "form": "오 왠지 기분 이상해요 ㅋㅋ",
