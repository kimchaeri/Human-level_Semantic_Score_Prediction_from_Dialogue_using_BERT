        {
          "id": "MDRW2100002157.1.1",
          "form": "배달 많아 해용?",
          "original_form": "배달 많아 해용?",
          "speaker_id": "2",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002157.1.2",
          "form": "아뇨 전 배달보단  해먹는걸 더 즐겨해요 ㅎ",
          "original_form": "아뇨 전 배달보단  해먹는걸 더 즐겨해요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002157.1.3",
          "form": "잘하시나보다",
          "original_form": "잘하시나보다",
          "speaker_id": "2",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002157.1.4",
          "form": "재미있어요 만드는과정이 ㅎ 물론 배달도 해요 ㅋ",
          "original_form": "재미있어요 만드는과정이 ㅎ 물론 배달도 해요 ㅋ",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002157.1.5",
          "form": "배달이 편해서",
          "original_form": "배달이 편해서",
          "speaker_id": "2",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002157.1.6",
          "form": "편하고 맛있는데 맛있어요 ㅋ.ㅋ",
          "original_form": "편하고 맛있는데 맛있어요 ㅋ.ㅋ",
          "speaker_id": "1",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002157.1.7",
          "form": "ㅋㅋ 그래요",
          "original_form": "ㅋㅋ 그래요",
          "speaker_id": "2",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002157.1.8",
          "form": "배달 메뉴 주로 머드세여 ㅎㅎ",
          "original_form": "배달 메뉴 주로 머드세여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002157.1.9",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "2",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002157.1.10",
          "form": "저도 치킨  \r\n치킨도 집에서 하면 맛있는데 뒷처리 귀찮어요 ㅎㅎ",
          "original_form": "저도 치킨  \r\n치킨도 집에서 하면 맛있는데 뒷처리 귀찮어요 ㅎㅎ",
          "speaker_id": "1",
