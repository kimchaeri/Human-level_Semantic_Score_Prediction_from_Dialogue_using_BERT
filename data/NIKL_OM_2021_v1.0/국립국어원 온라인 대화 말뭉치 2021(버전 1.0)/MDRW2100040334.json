      "utterance": [
        {
          "id": "MDRW2100040334.1.1",
          "form": "이제다시돌아가려구 긴웨이브르ㅡ",
          "original_form": "이제다시돌아가려구 긴웨이브르ㅡ",
          "speaker_id": "2",
          "time": "20210904 16:29"
        },
        {
          "id": "MDRW2100040334.1.2",
          "form": "자네는",
          "original_form": "자네는",
          "speaker_id": "2",
          "time": "20210904 16:29"
        },
        {
          "id": "MDRW2100040334.1.3",
          "form": "오오 나도요",
          "original_form": "오오 나도요",
          "speaker_id": "1",
          "time": "20210904 16:29"
        },
        {
          "id": "MDRW2100040334.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:30"
        },
        {
          "id": "MDRW2100040334.1.5",
          "form": "그냥 중단발 길이정도로 레이어드 할래요",
          "original_form": "그냥 중단발 길이정도로 레이어드 할래요",
          "speaker_id": "1",
          "time": "20210904 16:30"
        },
        {
          "id": "MDRW2100040334.1.6",
          "form": "슬퍼요 ㅋㅋㅋ",
          "original_form": "슬퍼요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:30"
        },
        {
          "id": "MDRW2100040334.1.7",
          "form": "제 카톡 프사 머리 정도요",
          "original_form": "제 카톡 프사 머리 정도요",
          "speaker_id": "1",
          "time": "20210904 16:30"
        },
        {
          "id": "MDRW2100040334.1.8",
          "form": "저 길이가 그냥 젤 편하고 이쁜것같아요",
          "original_form": "저 길이가 그냥 젤 편하고 이쁜것같아요",
          "speaker_id": "1",
          "time": "20210904 16:30"
        },
        {
          "id": "MDRW2100040334.1.9",
          "form": "중단발이딱좋은거같긴해용 관리도적당히편하구잘마르고스타일도낼수있구",
          "original_form": "중단발이딱좋은거같긴해용 관리도적당히편하구잘마르고스타일도낼수있구",
          "speaker_id": "2",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100040334.1.10",
          "form": "맞아요 넘 긴건 싫고",
          "original_form": "맞아요 넘 긴건 싫고",
          "speaker_id": "1",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100040334.1.11",
          "form": "숱 쳐서 가볍게 중단발 길이가 좋은거같아요",
          "original_form": "숱 쳐서 가볍게 중단발 길이가 좋은거같아요",
          "speaker_id": "1",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100040334.1.12",
          "form": "끄덕끄덕",
          "original_form": "끄덕끄덕",
          "speaker_id": "2",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100040334.1.13",
          "form": "긴거좋은데..머리감고말리고할때정말",
          "original_form": "긴거좋은데..머리감고말리고할때정말",
          "speaker_id": "2",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100040334.1.14",
          "form": "흑흑짜증이",
          "original_form": "흑흑짜증이",
          "speaker_id": "2",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100040334.1.15",
          "form": "그치 여름엔 더더육",
          "original_form": "그치 여름엔 더더육",
          "speaker_id": "1",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100040334.1.16",
          "form": "짧아서 그래도 이번 여름 잘 났어",
          "original_form": "짧아서 그래도 이번 여름 잘 났어",
          "speaker_id": "1",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100040334.1.17",
          "form": "맞아맞아 숩이가 빨리잘잘랏엉",
          "original_form": "맞아맞아 숩이가 빨리잘잘랏엉",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100040334.1.18",
          "form": "맞아 이번거만 욕심내지않았다면 완전 만족인데",
          "original_form": "맞아 이번거만 욕심내지않았다면 완전 만족인데",
          "speaker_id": "1",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100040334.1.19",
          "form": "몰러 허허 길겠지",
          "original_form": "몰러 허허 길겠지",
          "speaker_id": "1",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100040334.1.20",
          "form": "맞어금방길잖어",
          "original_form": "맞어금방길잖어",
          "speaker_id": "2",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100040334.1.21",
