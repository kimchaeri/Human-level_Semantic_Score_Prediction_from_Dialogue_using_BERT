      "utterance": [
        {
          "id": "MDRW2100032120.1.1",
          "form": "언니 여행 좋아해?",
          "original_form": "언니 여행 좋아해?",
          "speaker_id": "2",
          "time": "20210905 13:10"
        },
        {
          "id": "MDRW2100032120.1.2",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "1",
          "time": "20210905 13:10"
        },
        {
          "id": "MDRW2100032120.1.3",
          "form": "코시국만 끝나면 가려구!",
          "original_form": "코시국만 끝나면 가려구!",
          "speaker_id": "1",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100032120.1.4",
          "form": "가본 곳 중에 어디가 제일 좋았어?",
          "original_form": "가본 곳 중에 어디가 제일 좋았어?",
          "speaker_id": "2",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100032120.1.5",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100032120.1.6",
          "form": "국내 국외 다 포함이야?",
          "original_form": "국내 국외 다 포함이야?",
          "speaker_id": "1",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100032120.1.7",
          "form": "일단 국내 중에 젤 좋았던 곳!",
          "original_form": "일단 국내 중에 젤 좋았던 곳!",
          "speaker_id": "2",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100032120.1.8",
          "form": "제주도!",
          "original_form": "제주도!",
          "speaker_id": "1",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100032120.1.9",
          "form": "그중에서도 우도가 젤 재밌었어!",
          "original_form": "그중에서도 우도가 젤 재밌었어!",
          "speaker_id": "1",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100032120.1.10",
          "form": "보트 슝슝",
          "original_form": "보트 슝슝",
          "speaker_id": "1",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100032120.1.11",
          "form": "우와 우도? 재밌었겠다ㅜㅠ",
          "original_form": "우와 우도? 재밌었겠다ㅜㅠ",
          "speaker_id": "2",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100032120.1.12",
          "form": "나도 우도 가봤었는데 엄청 좋았어! 갈대밭이 있었나..",
          "original_form": "나도 우도 가봤었는데 엄청 좋았어! 갈대밭이 있었나..",
          "speaker_id": "2",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100032120.1.13",
          "form": "모래밭이 있던걸?",
          "original_form": "모래밭이 있던걸?",
          "speaker_id": "1",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100032120.1.14",
          "form": "갈대밭은",
          "original_form": "갈대밭은",
          "speaker_id": "1",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100032120.1.15",
          "form": "성산일출봉 쪽에 많더라",
          "original_form": "성산일출봉 쪽에 많더라",
          "speaker_id": "1",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100032120.1.16",
          "form": "제주도 너무 오래전에 가봤어ㅠ 나는 초등학생 때 갔었어. 한..2학년인가 3학년인가",
          "original_form": "제주도 너무 오래전에 가봤어ㅠ 나는 초등학생 때 갔었어. 한..2학년인가 3학년인가",
          "speaker_id": "2",
          "time": "20210905 13:19"
        },
        {
          "id": "MDRW2100032120.1.17",
          "form": "그거알아?",
          "original_form": "그거알아?",
          "speaker_id": "1",
          "time": "20210905 13:20"
        },
        {
          "id": "MDRW2100032120.1.18",
          "form": "라떼는 제주도 기념품",
          "original_form": "라떼는 제주도 기념품",
          "speaker_id": "1",
          "time": "20210905 13:20"
        },
        {
          "id": "MDRW2100032120.1.19",
          "form": "다 감귤초콜릿이었는데",
          "original_form": "다 감귤초콜릿이었는데",
          "speaker_id": "1",
          "time": "20210905 13:20"
        },
        {
          "id": "MDRW2100032120.1.20",
          "form": "요즘은 감귤과즐, 타르트 있데",
          "original_form": "요즘은 감귤과즐, 타르트 있데",
          "speaker_id": "1",
          "time": "20210905 13:20"
        },
        {
          "id": "MDRW2100032120.1.21",
          "form": "헉 라떼는 초콜릿 돌하르방이었는데..",
          "original_form": "헉 라떼는 초콜릿 돌하르방이었는데..",
          "speaker_id": "2",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100032120.1.22",
          "form": "언니 그.. 테디베어 박물관 가봤어?",
          "original_form": "언니 그.. 테디베어 박물관 가봤어?",
          "speaker_id": "2",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100032120.1.23",
          "form": "나는 제주도 동쪽코스로 돌아서",
          "original_form": "나는 제주도 동쪽코스로 돌아서",
          "speaker_id": "1",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100032120.1.24",
          "form": "남쪽 찍구 공항에 갔는데",
          "original_form": "남쪽 찍구 공항에 갔는데",
          "speaker_id": "1",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100032120.1.25",
          "form": "테디베어는 서쪽코스라서 못가봤어!",
          "original_form": "테디베어는 서쪽코스라서 못가봤어!",
          "speaker_id": "1",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100032120.1.26",
          "form": "나 어렸을때 궁이라는 드라마가 있었는데",
          "original_form": "나 어렸을때 궁이라는 드라마가 있었는데",
          "speaker_id": "1",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100032120.1.27",
          "form": "거기 테디베어박물관이 나와서",
          "original_form": "거기 테디베어박물관이 나와서",
          "speaker_id": "1",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100032120.1.28",
          "form": "한번쯤 가보고싶었어",
          "original_form": "한번쯤 가보고싶었어",
          "speaker_id": "1",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100032120.1.29",
          "form": "테디베어 박물관 되게 신기해",
          "original_form": "테디베어 박물관 되게 신기해",
          "speaker_id": "2",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100032120.1.30",
          "form": "곰돌이 천국이고",
          "original_form": "곰돌이 천국이고",
          "speaker_id": "2",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100032120.1.31",
          "form": "기념품점에도 온통 곰돌이야ㅋㅋㅋ",
          "original_form": "기념품점에도 온통 곰돌이야ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100032120.1.32",
          "form": "영화 박물관도 가봤는데.. 도깨비언덕인가? 거기두",
          "original_form": "영화 박물관도 가봤는데.. 도깨비언덕인가? 거기두",
          "speaker_id": "2",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100032120.1.33",
          "form": "추억..",
          "original_form": "추억..",
          "speaker_id": "2",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100032120.1.34",
          "form": "기념품 사왔어?",
          "original_form": "기념품 사왔어?",
          "speaker_id": "1",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100032120.1.35",
          "form": "웅! 구냥..필통..",
          "original_form": "웅! 구냥..필통..",
          "speaker_id": "2",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100032120.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:25"
        },
        {
          "id": "MDRW2100032120.1.37",
          "form": "지금 갔으면 인형 사왔을텐데",
          "original_form": "지금 갔으면 인형 사왔을텐데",
          "speaker_id": "2",
          "time": "20210905 13:25"
        },
        {
          "id": "MDRW2100032120.1.38",
          "form": "오 나는",
          "original_form": "오 나는",
          "speaker_id": "1",
          "time": "20210905 13:26"
        },
        {
          "id": "MDRW2100032120.1.39",
          "form": "마그넷 모아",
          "original_form": "마그넷 모아",
          "speaker_id": "1",
          "time": "20210905 13:26"
        },
        {
          "id": "MDRW2100032120.1.40",
          "form": "아직 해외여행은",
          "original_form": "아직 해외여행은",
          "speaker_id": "1",
          "time": "20210905 13:26"
        },
        {
          "id": "MDRW2100032120.1.41",
          "form": "한번밖에 못가봤는데",
          "original_form": "한번밖에 못가봤는데",
          "speaker_id": "1",
          "time": "20210905 13:26"
        },
        {
          "id": "MDRW2100032120.1.42",
          "form": "거기서는 마그넷 사왔어!",
          "original_form": "거기서는 마그넷 사왔어!",
          "speaker_id": "1",
          "time": "20210905 13:26"
        },
        {
          "id": "MDRW2100032120.1.43",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100032120.1.44",
          "form": "해외는 어디루 갔어?",
          "original_form": "해외는 어디루 갔어?",
          "speaker_id": "2",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100032120.1.45",
          "form": "태국",
          "original_form": "태국",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032120.1.46",
          "form": "너는 해외 어디루 가봤어?",
          "original_form": "너는 해외 어디루 가봤어?",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032120.1.47",
          "form": "나는 초등학생 때 일본 오사카~",
          "original_form": "나는 초등학생 때 일본 오사카~",
          "speaker_id": "2",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100032120.1.48",
          "form": "그때 겨울에 갔지..화이트 크리스마스였오!",
          "original_form": "그때 겨울에 갔지..화이트 크리스마스였오!",
          "speaker_id": "2",
          "time": "20210905 13:38"
        },
        {
          "id": "MDRW2100032120.1.49",
          "form": "성인되서는",
          "original_form": "성인되서는",
          "speaker_id": "1",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032120.1.50",
          "form": "아직이구나",
          "original_form": "아직이구나",
          "speaker_id": "1",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032120.1.51",
          "form": "코로나로 피해를 본 사람들이 많넹",
          "original_form": "코로나로 피해를 본 사람들이 많넹",
          "speaker_id": "1",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032120.1.52",
          "form": "맞아..",
          "original_form": "맞아..",
          "speaker_id": "2",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032120.1.53",
          "form": "중학생 때부터",
          "original_form": "중학생 때부터",
          "speaker_id": "2",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032120.1.54",
          "form": "여행 안갔어",
          "original_form": "여행 안갔어",
          "speaker_id": "2",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032120.1.55",
          "form": "못갔오",
          "original_form": "못갔오",
          "speaker_id": "2",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032120.1.56",
          "form": "스키타러 한 중 1 때까지 가고",
          "original_form": "스키타러 한 중 1 때까지 가고",
          "speaker_id": "2",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032120.1.57",
          "form": "계속 여행은 한번두 못갔징.. 6,7년 됐나",
          "original_form": "계속 여행은 한번두 못갔징.. 6,7년 됐나",
          "speaker_id": "2",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100032120.1.58",
          "form": "속상해",
          "original_form": "속상해",
          "speaker_id": "2",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100032120.1.59",
          "form": "나 운동 잘 못해",
          "original_form": "나 운동 잘 못해",
          "speaker_id": "1",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100032120.1.60",
          "form": "스키 개못타",
          "original_form": "스키 개못타",
          "speaker_id": "1",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100032120.1.61",
          "form": "헉 진짜?",
          "original_form": "헉 진짜?",
          "speaker_id": "2",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032120.1.62",
          "form": "괜찮아 name1언니는 뜨끈한 어묵이랑 맛난거 먹으면서 있오 나는 눈밭에서 구르고올게",
