      "utterance": [
        {
          "id": "MDRW2100045738.1.1",
          "form": "학교 전공 맞추기 해볼까요?",
          "original_form": "학교 전공 맞추기 해볼까요?",
          "speaker_id": "1",
          "time": "20210908 06:30"
        },
        {
          "id": "MDRW2100045738.1.2",
          "form": "엄청 어려울 것 같은데...",
          "original_form": "엄청 어려울 것 같은데...",
          "speaker_id": "1",
          "time": "20210908 06:31"
        },
        {
          "id": "MDRW2100045738.1.3",
          "form": "저부터 맞춰볼게요",
          "original_form": "저부터 맞춰볼게요",
          "speaker_id": "2",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045738.1.4",
          "form": "하드코어버전 가보자고~!~!!",
          "original_form": "하드코어버전 가보자고~!~!!",
          "speaker_id": "2",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045738.1.5",
          "form": "문과셨나요?",
          "original_form": "문과셨나요?",
          "speaker_id": "2",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045738.1.6",
          "form": "ㅋㅋㅋㅋㅋ 가즈아!!!!",
          "original_form": "ㅋㅋㅋㅋㅋ 가즈아!!!!",
          "speaker_id": "1",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045738.1.7",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045738.1.8",
          "form": "문과가 아니에요",
          "original_form": "문과가 아니에요",
          "speaker_id": "1",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100045738.1.9",
          "form": "절 문과로 보내주지 말아주세요",
          "original_form": "절 문과로 보내주지 말아주세요",
          "speaker_id": "1",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100045738.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100045738.1.11",
          "form": "이과셨나요?",
          "original_form": "이과셨나요?",
          "speaker_id": "2",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100045738.1.12",
          "form": "아... 님... 너무하신거 아님꽈?",
          "original_form": "아... 님... 너무하신거 아님꽈?",
          "speaker_id": "1",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045738.1.13",
          "form": "문과 싫다고 이과로 보내시다니....",
          "original_form": "문과 싫다고 이과로 보내시다니....",
          "speaker_id": "1",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045738.1.14",
          "form": "ㅌㅋㅋㅋㅋㅋㅋ예체능이샸나요!",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋ예체능이샸나요!",
          "speaker_id": "2",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045738.1.15",
          "form": "네! 예체능쪽입니다",
          "original_form": "네! 예체능쪽입니다",
          "speaker_id": "1",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045738.1.16",
          "form": "그 중 예술쪽이셨나요!",
          "original_form": "그 중 예술쪽이셨나요!",
          "speaker_id": "2",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045738.1.17",
          "form": "아니요 전 예술과 거리가 먼 사람입니다.",
          "original_form": "아니요 전 예술과 거리가 먼 사람입니다.",
          "speaker_id": "1",
          "time": "20210908 06:39"
        },
        {
          "id": "MDRW2100045738.1.18",
          "form": "헉 그럼 체육대학?",
          "original_form": "헉 그럼 체육대학?",
          "speaker_id": "2",
          "time": "20210908 06:40"
        },
        {
          "id": "MDRW2100045738.1.19",
          "form": "앗 예체능쪽이 예술 체육 아니면 어떤 거지..",
          "original_form": "앗 예체능쪽이 예술 체육 아니면 어떤 거지..",
          "speaker_id": "2",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045738.1.20",
          "form": "음악쪽도 아니신가요?",
          "original_form": "음악쪽도 아니신가요?",
          "speaker_id": "2",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045738.1.21",
          "form": "네 아닙니다.",
          "original_form": "네 아닙니다.",
          "speaker_id": "1",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045738.1.22",
          "form": "참고고 저는 박치입니다.",
          "original_form": "참고고 저는 박치입니다.",
          "speaker_id": "1",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045738.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045738.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ박치 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ박치 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045738.1.25",
          "form": "아 뭘까요 예체능인데 예능체능아니면",
          "original_form": "아 뭘까요 예체능인데 예능체능아니면",
          "speaker_id": "2",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045738.1.26",
          "form": "박치가 어때서요!!",
          "original_form": "박치가 어때서요!!",
          "speaker_id": "1",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045738.1.27",
          "form": "ㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045738.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ아이 울지 마세요 정겨워서 그랬어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아이 울지 마세요 정겨워서 그랬어요",
          "speaker_id": "2",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045738.1.29",
          "form": "세상에는 많은 전공들이 있죠. 제 전공을 맞춰주세요ㅠㅠ",
          "original_form": "세상에는 많은 전공들이 있죠. 제 전공을 맞춰주세요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045738.1.30",
          "form": "놀리는 거 아녜욧",
          "original_form": "놀리는 거 아녜욧",
          "speaker_id": "2",
          "time": "20210908 06:44"
        },
        {
