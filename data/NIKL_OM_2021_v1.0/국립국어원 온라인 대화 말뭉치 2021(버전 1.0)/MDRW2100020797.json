      "utterance": [
        {
          "id": "MDRW2100020797.1.1",
          "form": "점심드셨나요",
          "original_form": "점심드셨나요",
          "speaker_id": "1",
          "time": "20210828 12:45"
        },
        {
          "id": "MDRW2100020797.1.2",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "2",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100020797.1.3",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100020797.1.4",
          "form": "저도 아직이요,,",
          "original_form": "저도 아직이요,,",
          "speaker_id": "1",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100020797.1.5",
          "form": "식사메뉴 뭐드실예정이세요?",
          "original_form": "식사메뉴 뭐드실예정이세요?",
          "speaker_id": "1",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100020797.1.6",
          "form": "전 떡볶이 먹으려구요",
          "original_form": "전 떡볶이 먹으려구요",
          "speaker_id": "2",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100020797.1.7",
          "form": "저는 뭐 별거없으면 오동통면 끓일예정이에요",
          "original_form": "저는 뭐 별거없으면 오동통면 끓일예정이에요",
          "speaker_id": "1",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100020797.1.8",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100020797.1.9",
          "form": "떡볶이진짜좋아해요",
          "original_form": "떡볶이진짜좋아해요",
          "speaker_id": "1",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100020797.1.10",
          "form": "오 맛있겠네요",
          "original_form": "오 맛있겠네요",
          "speaker_id": "2",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100020797.1.11",
          "form": "만들어드세요?시켜드세요?",
          "original_form": "만들어드세요?시켜드세요?",
          "speaker_id": "1",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100020797.1.12",
          "form": "배달이요ㅋㅋ",
          "original_form": "배달이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100020797.1.13",
          "form": "로제 한번 먹어보려구요",
          "original_form": "로제 한번 먹어보려구요",
          "speaker_id": "2",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020797.1.14",
          "form": "저로제좋아해요",
          "original_form": "저로제좋아해요",
          "speaker_id": "1",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020797.1.15",
          "form": "그래요? 어디거 좋아하세요?",
          "original_form": "그래요? 어디거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020797.1.16",
          "form": "떡군이 배떡 태리 엽떡 삼첩 로제 다먹어봤는데",
          "original_form": "떡군이 배떡 태리 엽떡 삼첩 로제 다먹어봤는데",
          "speaker_id": "1",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020797.1.17",
          "form": "투탑이에요\n떡군이네랑 태리로제",
          "original_form": "투탑이에요\n떡군이네랑 태리로제",
          "speaker_id": "1",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020797.1.18",
          "form": "태리로제는 처음 들어봤어요",
          "original_form": "태리로제는 처음 들어봤어요",
          "speaker_id": "2",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100020797.1.19",
          "form": "전 떡군이네 먹어보려구요",
          "original_form": "전 떡군이네 먹어보려구요",
          "speaker_id": "2",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100020797.1.20",
          "form": "헉 최고의선택이에요",
          "original_form": "헉 최고의선택이에요",
          "speaker_id": "1",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100020797.1.21",
          "form": "정말맛있어요",
          "original_form": "정말맛있어요",
          "speaker_id": "1",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100020797.1.22",
          "form": "날치알주먹밥 필수",
          "original_form": "날치알주먹밥 필수",
          "speaker_id": "1",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020797.1.23",
          "form": "꼭 먹어볼게요ㅋㅋㅋㅋ",
          "original_form": "꼭 먹어볼게요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:51"
        }
