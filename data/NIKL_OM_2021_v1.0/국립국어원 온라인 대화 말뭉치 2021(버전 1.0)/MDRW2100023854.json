        },
        {
          "id": "MDRW2100023854.1.2",
          "form": "내가 참아줄수 있는 비린내는",
          "original_form": "내가 참아줄수 있는 비린내는",
          "speaker_id": "2",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100023854.1.3",
          "form": "게 비린내 하나 뿐이야",
          "original_form": "게 비린내 하나 뿐이야",
          "speaker_id": "2",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100023854.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100023854.1.5",
          "form": "새우도 생새우냄새는 좀 올라온다구",
          "original_form": "새우도 생새우냄새는 좀 올라온다구",
          "speaker_id": "2",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100023854.1.6",
          "form": "글킨 해",
          "original_form": "글킨 해",
          "speaker_id": "1",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100023854.1.7",
          "form": "내가 진짜 비위 겁나 약해서",
          "original_form": "내가 진짜 비위 겁나 약해서",
          "speaker_id": "2",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100023854.1.8",
          "form": "컨디션 안좋은 날은",
          "original_form": "컨디션 안좋은 날은",
          "speaker_id": "2",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100023854.1.9",
          "form": "계란씹다가도 비린내나서 못먹는데",
          "original_form": "계란씹다가도 비린내나서 못먹는데",
          "speaker_id": "2",
          "time": "20210830 19:02"
        },
        {
          "id": "MDRW2100023854.1.10",
          "form": "무조건 굴떡국을 드셔야하겠대",
          "original_form": "무조건 굴떡국을 드셔야하겠대",
          "speaker_id": "2",
          "time": "20210830 19:02"
        },
        {
          "id": "MDRW2100023854.1.11",
          "form": "갈라져야지 머",
          "original_form": "갈라져야지 머",
          "speaker_id": "2",
          "time": "20210830 19:02"
        },
        {
          "id": "MDRW2100023854.1.12",
          "form": "여튼 음 딱 맞춰서 밥차려주긴 싫으니까",
          "original_form": "여튼 음 딱 맞춰서 밥차려주긴 싫으니까",
          "speaker_id": "2",
          "time": "20210830 19:04"
        },
        {
          "id": "MDRW2100023854.1.13",
          "form": "씻으러 가야겠어",
          "original_form": "씻으러 가야겠어",
          "speaker_id": "2",
          "time": "20210830 19:04"
        },
        {
          "id": "MDRW2100023854.1.14",
          "form": "그래 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:04"
        },
        {
          "id": "MDRW2100023854.1.15",
          "form": "난 만두 다먹었는데",
          "original_form": "난 만두 다먹었는데",
          "speaker_id": "1",
          "time": "20210830 19:04"
        },
        {
          "id": "MDRW2100023854.1.16",
          "form": "엌",
          "original_form": "엌",
          "speaker_id": "2",
          "time": "20210830 19:04"
        },
        {
          "id": "MDRW2100023854.1.17",
          "form": "양이 너무 적어서 이따 운동하고 나오면",
          "original_form": "양이 너무 적어서 이따 운동하고 나오면",
          "speaker_id": "1",
          "time": "20210830 19:05"
        },
        {
          "id": "MDRW2100023854.1.18",
          "form": "야식먹고싶어질듯...",
          "original_form": "야식먹고싶어질듯...",
          "speaker_id": "1",
          "time": "20210830 19:05"
        },
        {
          "id": "MDRW2100023854.1.19",
          "form": "그럼 음",
          "original_form": "그럼 음",
          "speaker_id": "2",
          "time": "20210830 19:05"
        },
        {
          "id": "MDRW2100023854.1.20",
          "form": "먹어야지...",
          "original_form": "먹어야지...",
          "speaker_id": "2",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100023854.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100023854.1.22",
          "form": "운동 끝나면 9시라",
          "original_form": "운동 끝나면 9시라",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100023854.1.23",
          "form": "문 연 음식점이 없아...",
          "original_form": "문 연 음식점이 없아...",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100023854.1.24",
          "form": "ㅋㅋㅋㅋ큐ㅠㅜㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ큐ㅠㅜㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100023854.1.25",
