      "utterance": [
        {
          "id": "MDRW2100004424.1.1",
          "form": "결혼했어요?",
          "original_form": "결혼했어요?",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004424.1.2",
          "form": "아뇨 님은요?",
          "original_form": "아뇨 님은요?",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004424.1.3",
          "form": "저는 했는데",
          "original_form": "저는 했는데",
          "speaker_id": "1",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004424.1.4",
          "form": "축하드립니다",
          "original_form": "축하드립니다",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004424.1.5",
          "form": "아이도 있어요?",
          "original_form": "아이도 있어요?",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004424.1.6",
          "form": "네 초딩이요",
          "original_form": "네 초딩이요",
          "speaker_id": "1",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004424.1.7",
          "form": "와! 결혼한 이야기 들어볼 수 있을까요",
          "original_form": "와! 결혼한 이야기 들어볼 수 있을까요",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004424.1.8",
          "form": "1년 연애하고 했는데 행복해요",
          "original_form": "1년 연애하고 했는데 행복해요",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004424.1.9",
          "form": "행복하다니 정말 축하할 일이네요!",
          "original_form": "행복하다니 정말 축하할 일이네요!",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004424.1.10",
          "form": "함께라서 좋아요",
          "original_form": "함께라서 좋아요",
          "speaker_id": "1",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004424.1.11",
          "form": "혹시 결혼하실때 결혼관은 있으셨나요?",
          "original_form": "혹시 결혼하실때 결혼관은 있으셨나요?",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004424.1.12",
          "form": "맘만 맞으면 바로",
          "original_form": "맘만 맞으면 바로",
          "speaker_id": "1",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004424.1.13",
          "form": "엄청 적극적이시네요!",
          "original_form": "엄청 적극적이시네요!",
          "speaker_id": "2",
          "time": "20210821 16:47"
        },
