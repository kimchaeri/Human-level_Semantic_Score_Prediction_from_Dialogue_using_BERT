      "utterance": [
        {
          "id": "MDRW2100027242.1.1",
          "form": "접종을 계기로?",
          "original_form": "접종을 계기로?",
          "speaker_id": "1",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027242.1.2",
          "form": "예방접종은 안할거에요",
          "original_form": "예방접종은 안할거에요",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027242.1.3",
          "form": "아니ㅋ기 왤케 결론이 극단적이에욧ㅋ",
          "original_form": "아니ㅋ기 왤케 결론이 극단적이에욧ㅋ",
          "speaker_id": "1",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027242.1.4",
          "form": "아 맺고 끊음이 분명합니다",
          "original_form": "아 맺고 끊음이 분명합니다",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027242.1.5",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027242.1.6",
          "form": "너무 아팟서여",
          "original_form": "너무 아팟서여",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027242.1.7",
          "form": "부스터샷 맞으라 하면 어쩌려구요ㅎ",
          "original_form": "부스터샷 맞으라 하면 어쩌려구요ㅎ",
          "speaker_id": "1",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027242.1.8",
          "form": "화생방 만큼 고통스러웠습니다",
          "original_form": "화생방 만큼 고통스러웠습니다",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027242.1.9",
          "form": "안할라구요 ㅎㅎ",
          "original_form": "안할라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027242.1.10",
          "form": "헉 그거 완전 지옥이라던데ㄷㄷ",
          "original_form": "헉 그거 완전 지옥이라던데ㄷㄷ",
          "speaker_id": "1",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027242.1.11",
          "form": "진심 지옥을 맛보셨군요ㅜ",
          "original_form": "진심 지옥을 맛보셨군요ㅜ",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027242.1.12",
          "form": "네 ㅎㅎㅎ건강 잃을까봐 걱정도 많이했구요",
          "original_form": "네 ㅎㅎㅎ건강 잃을까봐 걱정도 많이했구요",
          "speaker_id": "2",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027242.1.13",
          "form": "건강이 최고입니다",
          "original_form": "건강이 최고입니다",
          "speaker_id": "2",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027242.1.14",
          "form": "구급차 안 부르신게 용하네요ㄷㄷ",
          "original_form": "구급차 안 부르신게 용하네요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027242.1.15",
          "form": "끙끙 앓기만 했습니다",
          "original_form": "끙끙 앓기만 했습니다",
          "speaker_id": "2",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027242.1.16",
          "form": "오늘 두꺼비는 잠시 멀리두셔요",
          "original_form": "오늘 두꺼비는 잠시 멀리두셔요",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027242.1.17",
          "form": "건강해칩니다",
          "original_form": "건강해칩니다",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027242.1.18",
          "form": "넹 두꺼비와 내외할게요ㅜ",
          "original_form": "넹 두꺼비와 내외할게요ㅜ",
          "speaker_id": "1",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027242.1.19",
          "form": "미안 두껍아ㅜ",
          "original_form": "미안 두껍아ㅜ",
          "speaker_id": "1",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027242.1.20",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100027242.1.21",
          "form": "평소 기저 질환있으세요?",
          "original_form": "평소 기저 질환있으세요?",
          "speaker_id": "2",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100027242.1.22",
          "form": "아뇨! 비실해보여도 건강체질입니다ㅎㅎ",
          "original_form": "아뇨! 비실해보여도 건강체질입니다ㅎㅎ",
