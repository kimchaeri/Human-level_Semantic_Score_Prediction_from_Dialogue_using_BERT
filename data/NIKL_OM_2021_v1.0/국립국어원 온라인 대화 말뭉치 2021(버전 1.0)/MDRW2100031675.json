      "utterance": [
        {
          "id": "MDRW2100031675.1.1",
          "form": "광복절연휴면 지나긴했네뇨",
          "original_form": "광복절연휴면 지나긴했네뇨",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100031675.1.2",
          "form": "네 ㅋㅋ 광복절에 뭐했어요",
          "original_form": "네 ㅋㅋ 광복절에 뭐했어요",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100031675.1.3",
          "form": "괜찮은 사람 있으면 결혼해야죠",
          "original_form": "괜찮은 사람 있으면 결혼해야죠",
          "speaker_id": "2",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100031675.1.4",
          "form": "없음 굳이 안하구",
          "original_form": "없음 굳이 안하구",
          "speaker_id": "2",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100031675.1.5",
          "form": "잘생겼나요",
          "original_form": "잘생겼나요",
          "speaker_id": "2",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100031675.1.6",
          "form": "아나요",
          "original_form": "아나요",
          "speaker_id": "1",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100031675.1.7",
          "form": "잘생긴건아닌거갘네뇨",
          "original_form": "잘생긴건아닌거갘네뇨",
          "speaker_id": "1",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100031675.1.8",
          "form": "현실에 연애인은 없습니다",
          "original_form": "현실에 연애인은 없습니다",
          "speaker_id": "1",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100031675.1.9",
          "form": "랩몬은 매력있는 얼굴입니다",
          "original_form": "랩몬은 매력있는 얼굴입니다",
          "speaker_id": "2",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100031675.1.10",
          "form": "연예인이니꺼요",
          "original_form": "연예인이니꺼요",
          "speaker_id": "1",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100031675.1.11",
          "form": "연예인급은 아닌",
          "original_form": "연예인급은 아닌",
          "speaker_id": "2",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100031675.1.12",
          "form": "사실 얼굴을 모르네용ㅋㅋ",
          "original_form": "사실 얼굴을 모르네용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100031675.1.13",
          "form": "이상형이 너뭋높으신겇아닌가요",
          "original_form": "이상형이 너뭋높으신겇아닌가요",
          "speaker_id": "1",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100031675.1.14",
          "form": "아 뭐에요 ㅋㅋ",
          "original_form": "아 뭐에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100031675.1.15",
          "form": "너무 현실적인가요 ㅋㅋ",
          "original_form": "너무 현실적인가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100031675.1.16",
          "form": "네 그냥 저는 이대로 사는것도 좋습니다ㅋㅋ",
          "original_form": "네 그냥 저는 이대로 사는것도 좋습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100031675.1.17",
          "form": "지금 좋으면 이대로도 좋지요",
          "original_form": "지금 좋으면 이대로도 좋지요",
          "speaker_id": "1",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100031675.1.18",
          "form": "여자는 특히",
          "original_form": "여자는 특히",
          "speaker_id": "1",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100031675.1.19",
          "form": "비혼하려면 대구 역시 가야겠군요",
          "original_form": "비혼하려면 대구 역시 가야겠군요",
          "speaker_id": "2",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100031675.1.20",
          "form": "비혼이든 연애든",
          "original_form": "비혼이든 연애든",
          "speaker_id": "2",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100031675.1.21",
          "form": "하긴 그러네요 생활환경이 대구가나으니",
          "original_form": "하긴 그러네요 생활환경이 대구가나으니",
          "speaker_id": "1",
          "time": "20210905 00:03"
        }
