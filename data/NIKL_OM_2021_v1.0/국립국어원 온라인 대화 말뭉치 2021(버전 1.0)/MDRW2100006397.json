      "utterance": [
        {
          "id": "MDRW2100006397.1.1",
          "form": "아 공식굿즈는 아니고",
          "original_form": "아 공식굿즈는 아니고",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006397.1.2",
          "form": "트위터에서 팬이 제작한 것...",
          "original_form": "트위터에서 팬이 제작한 것...",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006397.1.3",
          "form": "우와아아ㅏ 대박 어떤거 제작됐어요?",
          "original_form": "우와아아ㅏ 대박 어떤거 제작됐어요?",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006397.1.4",
          "form": "지나 티셔츠 모양이랑 상패 모양 뱃지요",
          "original_form": "지나 티셔츠 모양이랑 상패 모양 뱃지요",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100006397.1.5",
          "form": "잘 만든거 같더라구요 ㅎㅎ",
          "original_form": "잘 만든거 같더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100006397.1.6",
          "form": "대박ㅋㅋㅋㅋ 와 팬들이 역시 포인트를 잘아는군요",
          "original_form": "대박ㅋㅋㅋㅋ 와 팬들이 역시 포인트를 잘아는군요",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100006397.1.7",
          "form": "혹시 구구님은 굿즈같은건 사신적 없으신가요??",
          "original_form": "혹시 구구님은 굿즈같은건 사신적 없으신가요??",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100006397.1.8",
          "form": "저는 아이돌좋아해서 아이돌 굿즈 샀어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 아이돌좋아해서 아이돌 굿즈 샀어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100006397.1.9",
          "form": "얼굴들어간것도 있고 ㅋㅋ 그냥 얼굴없이 로고만 들어간것도 있어요 ㅋㅋㅋ",
          "original_form": "얼굴들어간것도 있고 ㅋㅋ 그냥 얼굴없이 로고만 들어간것도 있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100006397.1.10",
          "form": "오 혹시 무슨 아이돌이에요??",
          "original_form": "오 혹시 무슨 아이돌이에요??",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100006397.1.11",
          "form": "ㅋㅋ아 그냥 두루두루 좋아해요",
          "original_form": "ㅋㅋ아 그냥 두루두루 좋아해요",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100006397.1.12",
          "form": "아이돌노래 들으면 힘나서 좋아요ㅠㅋㅋ",
          "original_form": "아이돌노래 들으면 힘나서 좋아요ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100006397.1.13",
          "form": "오 혹시 노래 추천해주실수 있으신가요",
          "original_form": "오 혹시 노래 추천해주실수 있으신가요",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100006397.1.14",
          "form": "아이돌은 잘 몰라서...",
          "original_form": "아이돌은 잘 몰라서...",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006397.1.15",
          "form": "네 ㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006397.1.16",
          "form": "음 일단 어떤 노래 좋아하세요? 신나는거? 청량한거?",
          "original_form": "음 일단 어떤 노래 좋아하세요? 신나는거? 청량한거?",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006397.1.17",
          "form": "신나는 노래요!",
          "original_form": "신나는 노래요!",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006397.1.18",
          "form": "음 그러면 여자아이돌중에서는 레드벨벳노래 좋아해요 ㅋㅋㅋ",
          "original_form": "음 그러면 여자아이돌중에서는 레드벨벳노래 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006397.1.19",
          "form": "레드벨벳 앨범도 샀었답니다 ㅋㅋㅋㅋㅋ",
          "original_form": "레드벨벳 앨범도 샀었답니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006397.1.20",
          "form": "레드벨벳 빨간맛 예전에 많이 들었던 거 같아요",
          "original_form": "레드벨벳 빨간맛 예전에 많이 들었던 거 같아요",
          "speaker_id": "2",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100006397.1.21",
          "form": "오랜만에 다시 찾아봐야겠네요",
          "original_form": "오랜만에 다시 찾아봐야겠네요",
