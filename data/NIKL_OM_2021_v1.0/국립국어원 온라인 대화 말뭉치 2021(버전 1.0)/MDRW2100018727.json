      "utterance": [
        {
          "id": "MDRW2100018727.1.1",
          "form": "저도 두근두근 기대됩니다 ㅎㅎ",
          "original_form": "저도 두근두근 기대됩니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018727.1.2",
          "form": "스포츠 말고 다른 취미생활 있으세요?",
          "original_form": "스포츠 말고 다른 취미생활 있으세요?",
          "speaker_id": "1",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018727.1.3",
          "form": "전 공연도 자주 보러가요",
          "original_form": "전 공연도 자주 보러가요",
          "speaker_id": "1",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018727.1.4",
          "form": "아 공연도 좋죠",
          "original_form": "아 공연도 좋죠",
          "speaker_id": "2",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018727.1.5",
          "form": "저는 락을 좋아해서",
          "original_form": "저는 락을 좋아해서",
          "speaker_id": "2",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018727.1.6",
          "form": "락밴드 공연 종종 갑니다",
          "original_form": "락밴드 공연 종종 갑니다",
          "speaker_id": "2",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018727.1.7",
          "form": "우와 락 음악 좋아하시는구나",
          "original_form": "우와 락 음악 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018727.1.8",
          "form": "저도 일본 락 가수 좋아해요",
          "original_form": "저도 일본 락 가수 좋아해요",
          "speaker_id": "1",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018727.1.9",
          "form": "그리고 아이돌도 좋아한답니다 ㅎㅎ",
          "original_form": "그리고 아이돌도 좋아한답니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:32"
        },
        {
          "id": "MDRW2100018727.1.10",
          "form": "아~~ 아이돌 공연도 즐겁죠",
          "original_form": "아~~ 아이돌 공연도 즐겁죠",
          "speaker_id": "2",
          "time": "20210827 11:32"
        },
        {
          "id": "MDRW2100018727.1.11",
          "form": "아이돌 콘서트 자주 가보셨겠어요",
          "original_form": "아이돌 콘서트 자주 가보셨겠어요",
          "speaker_id": "2",
          "time": "20210827 11:32"
        },
        {
          "id": "MDRW2100018727.1.12",
          "form": "네 좋어해서 자주 가요ㅋㅋㅋ",
          "original_form": "네 좋어해서 자주 가요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:32"
        },
        {
          "id": "MDRW2100018727.1.13",
          "form": "무대 보는 재미가 있더라구요",
          "original_form": "무대 보는 재미가 있더라구요",
          "speaker_id": "1",
          "time": "20210827 11:33"
        },
        {
          "id": "MDRW2100018727.1.14",
          "form": "그렇긴 하죠 아이돌 콘서트는 무대 연출도 다양하고 신기하더라구요",
          "original_form": "그렇긴 하죠 아이돌 콘서트는 무대 연출도 다양하고 신기하더라구요",
          "speaker_id": "2",
          "time": "20210827 11:33"
        },
        {
          "id": "MDRW2100018727.1.15",
          "form": "전 뮤지컬도 좋아해요",
          "original_form": "전 뮤지컬도 좋아해요",
          "speaker_id": "2",
          "time": "20210827 11:33"
        },
        {
          "id": "MDRW2100018727.1.16",
          "form": "뮤지컬도 좋죠!",
          "original_form": "뮤지컬도 좋죠!",
          "speaker_id": "1",
          "time": "20210827 11:33"
        },
        {
          "id": "MDRW2100018727.1.17",
          "form": "뮤지컬 뭐뭐 보셨어요?",
          "original_form": "뮤지컬 뭐뭐 보셨어요?",
          "speaker_id": "1",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018727.1.18",
          "form": "안나 카레니나, 캣츠요",
          "original_form": "안나 카레니나, 캣츠요",
          "speaker_id": "2",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018727.1.19",
          "form": "들어보셨어요?",
          "original_form": "들어보셨어요?",
          "speaker_id": "2",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018727.1.20",
          "form": "넵 보지는 않았는데 이름은 많이 들어봤어요ㅎㅎ",
          "original_form": "넵 보지는 않았는데 이름은 많이 들어봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018727.1.21",
          "form": "캣츠는 한번 본적 있구요",
          "original_form": "캣츠는 한번 본적 있구요",
          "speaker_id": "1",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018727.1.22",
          "form": "아~!~! 캣츠 재밌지 않나요?",
          "original_form": "아~!~! 캣츠 재밌지 않나요?",
          "speaker_id": "2",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018727.1.23",
          "form": "처음에는 사람이 고양이 분장을 사실적으로 해서 무서웠긴 하지만 ㅋㅋㅋ",
          "original_form": "처음에는 사람이 고양이 분장을 사실적으로 해서 무서웠긴 하지만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:36"
        },
        {
          "id": "MDRW2100018727.1.24",
          "form": "뭔지 알거 같아요ㅋㅋㅋㅋ",
          "original_form": "뭔지 알거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:36"
        },
        {
          "id": "MDRW2100018727.1.25",
          "form": "그래도 정말 고양이처럼 연기를 잘 하시더라고요",
          "original_form": "그래도 정말 고양이처럼 연기를 잘 하시더라고요",
          "speaker_id": "1",
          "time": "20210827 11:36"
        },
        {
          "id": "MDRW2100018727.1.26",
          "form": "뮤지컬은 실감나는 연기 보러 가는거니까",
          "original_form": "뮤지컬은 실감나는 연기 보러 가는거니까",
          "speaker_id": "1",
          "time": "20210827 11:36"
        },
        {
          "id": "MDRW2100018727.1.27",
          "form": "맞아요 그게 뮤지컬의 장점같아요",
          "original_form": "맞아요 그게 뮤지컬의 장점같아요",
          "speaker_id": "2",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100018727.1.28",
          "form": "티켓 가격은 꽤 비싸도",
          "original_form": "티켓 가격은 꽤 비싸도",
          "speaker_id": "2",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100018727.1.29",
          "form": "값어치는 하는듯요",
          "original_form": "값어치는 하는듯요",
          "speaker_id": "2",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100018727.1.30",
          "form": "맞아요 얼른 뮤지컬 보러가고 싶네요 ㅎㅎ",
          "original_form": "맞아요 얼른 뮤지컬 보러가고 싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:39"
        },
        {
          "id": "MDRW2100018727.1.31",
          "form": "다음에 관람하고 서로 후기 남겨보아요",
          "original_form": "다음에 관람하고 서로 후기 남겨보아요",
          "speaker_id": "2",
          "time": "20210827 11:39"
        }
      ]
    }
