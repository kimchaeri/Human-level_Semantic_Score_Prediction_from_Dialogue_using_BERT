      "utterance": [
        {
          "id": "MDRW2100019280.1.1",
          "form": "에이드 먹어봤어요?",
          "original_form": "에이드 먹어봤어요?",
          "speaker_id": "1",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019280.1.2",
          "form": "친구가 되게 맛있게 먹더라고요!",
          "original_form": "친구가 되게 맛있게 먹더라고요!",
          "speaker_id": "2",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019280.1.3",
          "form": "이름이 뭐더라..잠시만여",
          "original_form": "이름이 뭐더라..잠시만여",
          "speaker_id": "2",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100019280.1.4",
          "form": "오~ 무슨 에이드였어요?",
          "original_form": "오~ 무슨 에이드였어요?",
          "speaker_id": "1",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100019280.1.5",
          "form": "아 아이스티구나",
          "original_form": "아 아이스티구나",
          "speaker_id": "2",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100019280.1.6",
          "form": "크리스탈 라이트래요!",
          "original_form": "크리스탈 라이트래요!",
          "speaker_id": "2",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100019280.1.7",
          "form": "아 아이스티 ㅋㅋㅋ",
          "original_form": "아 아이스티 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:59"
        },
        {
          "id": "MDRW2100019280.1.8",
          "form": "크리스탈 라이트라는 아이스티가 있어요?",
          "original_form": "크리스탈 라이트라는 아이스티가 있어요?",
          "speaker_id": "1",
          "time": "20210827 16:59"
        },
        {
          "id": "MDRW2100019280.1.9",
          "form": "제가 아는 건 립톤 뿐인데..",
          "original_form": "제가 아는 건 립톤 뿐인데..",
          "speaker_id": "1",
          "time": "20210827 16:59"
        },
        {
          "id": "MDRW2100019280.1.10",
          "form": "그 설탕대신 대체감미료 써서 칼로리가 엄청 낮다고 들었어요",
          "original_form": "그 설탕대신 대체감미료 써서 칼로리가 엄청 낮다고 들었어요",
          "speaker_id": "2",
          "time": "20210827 17:08"
        },
        {
          "id": "MDRW2100019280.1.11",
          "form": "그리고 짱맛난다고 ㅎㅎ",
          "original_form": "그리고 짱맛난다고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 17:08"
        },
        {
          "id": "MDRW2100019280.1.12",
          "form": "와 좋다",
          "original_form": "와 좋다",
          "speaker_id": "1",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100019280.1.13",
          "form": "에스테리톨 같은거요?",
          "original_form": "에스테리톨 같은거요?",
          "speaker_id": "1",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100019280.1.14",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019280.1.15",
          "form": "음료 좋아하시면 그런것도 괜찮을거같아서요",
          "original_form": "음료 좋아하시면 그런것도 괜찮을거같아서요",
          "speaker_id": "2",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019280.1.16",
          "form": "ㅎㅎ 그래서 저 제로 음료 먹어요",
          "original_form": "ㅎㅎ 그래서 저 제로 음료 먹어요",
          "speaker_id": "1",
          "time": "20210827 17:49"
        },
        {
          "id": "MDRW2100019280.1.17",
          "form": "근데 콜라는 사이다는 알았는데 아이스티는 처음 알았네요",
          "original_form": "근데 콜라는 사이다는 알았는데 아이스티는 처음 알았네요",
          "speaker_id": "1",
          "time": "20210827 17:50"
        },
        {
          "id": "MDRW2100019280.1.18",
          "form": "근데 속쓰림은 없으세요?",
          "original_form": "근데 속쓰림은 없으세요?",
          "speaker_id": "2",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019280.1.19",
          "form": "배아픈일이 많대서",
          "original_form": "배아픈일이 많대서",
          "speaker_id": "2",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019280.1.20",
          "form": "아 진짜요? 전 괜찮던데요~",
          "original_form": "아 진짜요? 전 괜찮던데요~",
          "speaker_id": "1",
          "time": "20210827 19:14"
        },
        {
          "id": "MDRW2100019280.1.21",
          "form": "다이어트 중이라서 죄책감 덜어내기 딱 좋아요 ㅎㅎ",
          "original_form": "다이어트 중이라서 죄책감 덜어내기 딱 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:14"
        },
        {
          "id": "MDRW2100019280.1.22",
          "form": "아 식단 조절중이시구나",
          "original_form": "아 식단 조절중이시구나",
          "speaker_id": "2",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019280.1.23",
          "form": "네네 요즘 젊은 사람들이 당 많이 먹어서 당뇨 걸린대요",
          "original_form": "네네 요즘 젊은 사람들이 당 많이 먹어서 당뇨 걸린대요",
          "speaker_id": "1",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019280.1.24",
          "form": "그래서 저 당 줄이려고 노력중이에요...",
          "original_form": "그래서 저 당 줄이려고 노력중이에요...",
          "speaker_id": "1",
          "time": "20210827 20:48"
        }
      ]
    }
