      "utterance": [
        {
          "id": "MDRW2100006378.1.1",
          "form": "음 중국에서 매미 꼬치를 팔던게 충격적이어서 기억에 남네요",
          "original_form": "음 중국에서 매미 꼬치를 팔던게 충격적이어서 기억에 남네요",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006378.1.2",
          "form": "...ㄱ렇네요 중국은 특이한 음식이 많았던 ㅠㅠ",
          "original_form": "...ㄱ렇네요 중국은 특이한 음식이 많았던 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006378.1.3",
          "form": "차마 먹어보지 못햇습니닼ㅋㅋㅋㅋㅋㅋ",
          "original_form": "차마 먹어보지 못햇습니닼ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100006378.1.4",
          "form": "name1님은 여행지에서 기억에 남는 음식 잇나요?",
          "original_form": "&name1&님은 여행지에서 기억에 남는 음식 잇나요?",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100006378.1.5",
          "form": "싱가폴의 칠리크랩이 기억에 남아요",
          "original_form": "싱가폴의 칠리크랩이 기억에 남아요",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100006378.1.6",
          "form": "칠리크랩!!!",
          "original_form": "칠리크랩!!!",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006378.1.7",
          "form": "아 군침도네요",
          "original_form": "아 군침도네요",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006378.1.8",
          "form": "태국음식 뿌빳퐁커리아시나요",
          "original_form": "태국음식 뿌빳퐁커리아시나요",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006378.1.9",
          "form": "진짜 좋아합니다",
          "original_form": "진짜 좋아합니다",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100006378.1.10",
          "form": "ㅎㅎ맛있죠 그것도 생각나요 ㅎㅎ",
          "original_form": "ㅎㅎ맛있죠 그것도 생각나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100006378.1.11",
          "form": "팟타이도 맛잇죠",
          "original_form": "팟타이도 맛잇죠",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100006378.1.12",
          "form": "네 팟타이 좋아합니다  ㅎㅎ",
          "original_form": "네 팟타이 좋아합니다  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100006378.1.13",
          "form": "과일도 진짜 맛잇엇어요",
          "original_form": "과일도 진짜 맛잇엇어요",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100006378.1.14",
          "form": "저 바나나 별로 안좋아하는데",
          "original_form": "저 바나나 별로 안좋아하는데",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100006378.1.15",
          "form": "바나나마저 진짜 맛잇더라구요 ㅋㅋㅋ",
          "original_form": "바나나마저 진짜 맛잇더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100006378.1.16",
          "form": "아 태국 바나나요? 현지에서?",
          "original_form": "아 태국 바나나요? 현지에서?",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100006378.1.17",
          "form": "네 ㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100006378.1.18",
          "form": "바나나가 너무 맛잇어서 깜짝놀랏어요 ㅋㅋㅋㅋㅋ",
          "original_form": "바나나가 너무 맛잇어서 깜짝놀랏어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100006378.1.19",
          "form": "얼마나 맛난지 궁금하네요\r\n망고스틴도 유명하죠",
          "original_form": "얼마나 맛난지 궁금하네요\r\n망고스틴도 유명하죠",
