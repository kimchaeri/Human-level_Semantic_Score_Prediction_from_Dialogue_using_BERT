      "utterance": [
        {
          "id": "MDRW2100044631.1.1",
          "form": "ㅇㅇㅇ\n\n전전생",
          "original_form": "ㅇㅇㅇ\n\n전전생",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044631.1.2",
          "form": "콘한지 되게오래됨",
          "original_form": "콘한지 되게오래됨",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044631.1.3",
          "form": "바빠서",
          "original_form": "바빠서",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044631.1.4",
          "form": "민현이 돌아오고 1번밖에 콘안했다는 사실이",
          "original_form": "민현이 돌아오고 1번밖에 콘안했다는 사실이",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044631.1.5",
          "form": "ㅁ매우 놀라울지경",
          "original_form": "ㅁ매우 놀라울지경",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044631.1.6",
          "form": "새앨범 내기 힘들면",
          "original_form": "새앨범 내기 힘들면",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044631.1.7",
          "form": "디싱이라도....",
          "original_form": "디싱이라도....",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044631.1.8",
          "form": "싱글이라도 내줘",
          "original_form": "싱글이라도 내줘",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044631.1.9",
          "form": "동호야 범주 요새 산다니던데",
          "original_form": "동호야 범주 요새 산다니던데",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044631.1.10",
          "form": "산다니지ㅏㄹ고 노래만들라고해",
          "original_form": "산다니지ㅏㄹ고 노래만들라고해",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044631.1.11",
          "form": "완전체 콘이 1번인건",
          "original_form": "완전체 콘이 1번인건",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044631.1.12",
          "form": "선넘었다",
          "original_form": "선넘었다",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044631.1.13",
          "form": "산타지말고 노래나해",
          "original_form": "산타지말고 노래나해",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
