        },
        {
          "id": "MDRW2100000480.1.2",
          "form": "우리 여행가?",
          "original_form": "우리 여행가?",
          "speaker_id": "2",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000480.1.3",
          "form": "내 생일선물?",
          "original_form": "내 생일선물?",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000480.1.4",
          "form": "다음에가자..",
          "original_form": "다음에가자..",
          "speaker_id": "1",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000480.1.5",
          "form": "돈이없다..",
          "original_form": "돈이없다..",
          "speaker_id": "1",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000480.1.6",
          "form": "ㅋㅌㅌㅌ다음에 어디가려구",
          "original_form": "ㅋㅌㅌㅌ다음에 어디가려구",
          "speaker_id": "2",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000480.1.7",
          "form": "가고싶은데 있어?",
          "original_form": "가고싶은데 있어?",
          "speaker_id": "2",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000480.1.8",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210819 20:06"
        },
        {
          "id": "MDRW2100000480.1.9",
          "form": "가까이는 해운대???",
          "original_form": "가까이는 해운대???",
          "speaker_id": "1",
          "time": "20210819 20:06"
        },
        {
          "id": "MDRW2100000480.1.10",
          "form": "해운대?",
          "original_form": "해운대?",
          "speaker_id": "2",
          "time": "20210819 20:07"
        },
        {
          "id": "MDRW2100000480.1.11",
          "form": "바다보고싶어?",
          "original_form": "바다보고싶어?",
          "speaker_id": "2",
          "time": "20210819 20:07"
        },
        {
          "id": "MDRW2100000480.1.12",
          "form": "요즘은 어디 못돌아다니니까",
          "original_form": "요즘은 어디 못돌아다니니까",
          "speaker_id": "1",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000480.1.13",
          "form": "그냥 뷰 좋은곳을 생각하니까",
          "original_form": "그냥 뷰 좋은곳을 생각하니까",
          "speaker_id": "1",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000480.1.14",
          "form": "해운대 광안리밖에 생각안나ㅋㅋㅋ",
          "original_form": "해운대 광안리밖에 생각안나ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000480.1.15",
          "form": "ㅌㅋㅋㅋㅋ그건그려",
          "original_form": "ㅌㅋㅋㅋㅋ그건그려",
          "speaker_id": "2",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000480.1.16",
          "form": "산은 별루",
          "original_form": "산은 별루",
          "speaker_id": "2",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000480.1.17",
          "form": "별로야?",
          "original_form": "별로야?",
          "speaker_id": "2",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000480.1.18",
          "form": "산은 여보가 싫어하자나",
          "original_form": "산은 여보가 싫어하자나",
          "speaker_id": "1",
          "time": "20210819 21:16"
        },
        {
          "id": "MDRW2100000480.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:16"
        },
        {
          "id": "MDRW2100000480.1.20",
          "form": "ㅋㅌㅌㅋㅋㅋㅋㅋㅋ겨울엔 ....괜찮을걸??...",
          "original_form": "ㅋㅌㅌㅋㅋㅋㅋㅋㅋ겨울엔 ....괜찮을걸??...",
          "speaker_id": "2",
          "time": "20210819 21:19"
        },
        {
          "id": "MDRW2100000480.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ더추운데 가능?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ더추운데 가능?",
          "speaker_id": "1",
          "time": "20210819 21:20"
        },
        {
          "id": "MDRW2100000480.1.22",
          "form": "이미 다녀와가지고 ㅋㅌㅋㅋㅌ",
          "original_form": "이미 다녀와가지고 ㅋㅌㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100000480.1.23",
          "form": "산여행은 춥긴하더라.......",
          "original_form": "산여행은 춥긴하더라.......",
          "speaker_id": "2",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100000480.1.24",
          "form": "별만 잘보여",
          "original_form": "별만 잘보여",
          "speaker_id": "2",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100000480.1.25",
          "form": "ㅋㅋㅋㅋㅋ나는 좋은뎁",
          "original_form": "ㅋㅋㅋㅋㅋ나는 좋은뎁",
          "speaker_id": "1",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100000480.1.26",
          "form": "산을 좋아하니까",
          "original_form": "산을 좋아하니까",
          "speaker_id": "1",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100000480.1.27",
          "form": "ㅌㅌㅋㅋㅋㅋㅋㅋㅋ나도 산여행 괜찮아",
          "original_form": "ㅌㅌㅋㅋㅋㅋㅋㅋㅋ나도 산여행 괜찮아",
          "speaker_id": "2",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100000480.1.28",
          "form": "벌레만 없으면...",
