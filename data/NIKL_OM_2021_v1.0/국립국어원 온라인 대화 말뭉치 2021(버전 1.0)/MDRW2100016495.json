      "utterance": [
        {
          "id": "MDRW2100016495.1.1",
          "form": "떡볶이말고 뭐 좋아하세요?",
          "original_form": "떡볶이말고 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 13:14"
        },
        {
          "id": "MDRW2100016495.1.2",
          "form": "몸에 안 좋은 건 다 좋아하는데 ㅋㅋ 빵순이에요",
          "original_form": "몸에 안 좋은 건 다 좋아하는데 ㅋㅋ 빵순이에요",
          "speaker_id": "1",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016495.1.3",
          "form": "저도 빵 엄청좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 빵 엄청좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100016495.1.4",
          "form": "무슨빵 좋아하세요?",
          "original_form": "무슨빵 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100016495.1.5",
          "form": "전 마늘빵 엄청 좋아해요 초코나 치즈 들어간거도 다요 ㅋㅋㅋ",
          "original_form": "전 마늘빵 엄청 좋아해요 초코나 치즈 들어간거도 다요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100016495.1.6",
          "form": "저도 마늘빵 진짜 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "저도 마늘빵 진짜 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100016495.1.7",
          "form": "어디거 좋아하세요??",
          "original_form": "어디거 좋아하세요??",
          "speaker_id": "1",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100016495.1.8",
          "form": "마늘빵 맛있는곳 아세요?",
          "original_form": "마늘빵 맛있는곳 아세요?",
          "speaker_id": "2",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100016495.1.9",
          "form": "뚜레쥬르는 간단하게 먹기좋고",
          "original_form": "뚜레쥬르는 간단하게 먹기좋고",
          "speaker_id": "1",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100016495.1.10",
          "form": "만동제과라고 홍대에 유명한 집 있어요",
          "original_form": "만동제과라고 홍대에 유명한 집 있어요",
          "speaker_id": "1",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100016495.1.11",
          "form": "만동제과!!",
          "original_form": "만동제과!!",
          "speaker_id": "2",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100016495.1.12",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100016495.1.13",
          "form": "가보셨구나! 너무 맛있어요 집 근처에 있으면 좋겠어요",
          "original_form": "가보셨구나! 너무 맛있어요 집 근처에 있으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100016495.1.14",
          "form": "맞아요 ㅠㅠ 맨날 먹고싶음 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ 맨날 먹고싶음 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100016495.1.15",
          "form": "홍대에 아오이토리도 좋아해요",
          "original_form": "홍대에 아오이토리도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100016495.1.16",
          "form": "오거긴 첨 들어봐요",
          "original_form": "오거긴 첨 들어봐요",
          "speaker_id": "1",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016495.1.17",
          "form": "거기도 마늘빵으로 유명한가요??",
          "original_form": "거기도 마늘빵으로 유명한가요??",
          "speaker_id": "1",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016495.1.18",
          "form": "아 아오이토리에서는 야키소바빵 좋아해요",
          "original_form": "아 아오이토리에서는 야키소바빵 좋아해요",
          "speaker_id": "2",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100016495.1.19",
          "form": "빵사이에 야키소바가 들어간 그야말로 밀가루폭탄인데... 제입엔 맛잇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "빵사이에 야키소바가 들어간 그야말로 밀가루폭탄인데... 제입엔 맛잇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100016495.1.20",
          "form": "야키소바빵 안 먹어봤는데 궁금해요",
          "original_form": "야키소바빵 안 먹어봤는데 궁금해요",
          "speaker_id": "1",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100016495.1.21",
          "form": "밀가루폭탄이면 맛없없이죠 ㅠㅠ",
          "original_form": "밀가루폭탄이면 맛없없이죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100016495.1.22",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:28"
        }
      ]
