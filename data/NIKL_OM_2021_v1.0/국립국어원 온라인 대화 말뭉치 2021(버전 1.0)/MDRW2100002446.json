      "utterance": [
        {
          "id": "MDRW2100002446.1.1",
          "form": "식사는?",
          "original_form": "식사는?",
          "speaker_id": "2",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002446.1.2",
          "form": "전 먹었어요! 불닭까르보나라요",
          "original_form": "전 먹었어요! 불닭까르보나라요",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002446.1.3",
          "form": "매운거 잘 드시나요?",
          "original_form": "매운거 잘 드시나요?",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002446.1.4",
          "form": "예전만큼은 못먹겠어요",
          "original_form": "예전만큼은 못먹겠어요",
          "speaker_id": "2",
          "time": "20210820 13:39"
        },
        {
          "id": "MDRW2100002446.1.5",
          "form": "장기가 나이드나봐요 ㅎ",
          "original_form": "장기가 나이드나봐요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 13:39"
        },
        {
          "id": "MDRW2100002446.1.6",
          "form": "맛있게 드셨나요",
          "original_form": "맛있게 드셨나요",
          "speaker_id": "2",
          "time": "20210820 13:39"
        },
        {
          "id": "MDRW2100002446.1.7",
          "form": "넵ㅎ.ㅎ 뭐 드셨나요?",
          "original_form": "넵ㅎ.ㅎ 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002446.1.8",
          "form": "점심시간이 훌쩍 지났네요ㅎ.ㅎ",
          "original_form": "점심시간이 훌쩍 지났네요ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002446.1.9",
          "form": "전 들깨칼국수 먹었어요",
          "original_form": "전 들깨칼국수 먹었어요",
          "speaker_id": "2",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002446.1.10",
          "form": "좀짰나봐요 ㅎ",
          "original_form": "좀짰나봐요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002446.1.11",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002446.1.12",
          "form": "저 불닭까르보나라요 ㅎㅎㅎ",
          "original_form": "저 불닭까르보나라요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002446.1.13",
          "form": "그리고 아메리카노를 후식으로 마셨어요",
          "original_form": "그리고 아메리카노를 후식으로 마셨어요",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002446.1.14",
          "form": "아메리카노 잘 드세요?",
          "original_form": "아메리카노 잘 드세요?",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002446.1.15",
          "form": "네 제일 무난한 음료인듯해요 ㅎ",
          "original_form": "네 제일 무난한 음료인듯해요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002446.1.16",
          "form": "저는 후식으로 귤까먹고 널부러져있어요 ㅎ",
          "original_form": "저는 후식으로 귤까먹고 널부러져있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002446.1.17",
          "form": "저녁메뉴 고민하면서요 ㅋㅋㅋㅋ",
          "original_form": "저녁메뉴 고민하면서요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002446.1.18",
          "form": "저녁엔 뭐드시나요 불금이잖아요",
          "original_form": "저녁엔 뭐드시나요 불금이잖아요",
          "speaker_id": "2",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002446.1.19",
          "form": "ㅎ.ㅎ..아직 저녁메뉴는 생각 못했어요",
          "original_form": "ㅎ.ㅎ..아직 저녁메뉴는 생각 못했어요",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002446.1.20",
          "form": "요즘 그릭요거트랑 복숭아 먹는걸 조아하는데...",
          "original_form": "요즘 그릭요거트랑 복숭아 먹는걸 조아하는데...",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002446.1.21",
          "form": "여름 귤 어때요?",
          "original_form": "여름 귤 어때요?",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002446.1.22",
          "form": "복숭아 맛있죠 ㅠㅠ 물렁이도 좋고 딱딱이도 좋아요 ㅠㅠ",
          "original_form": "복숭아 맛있죠 ㅠㅠ 물렁이도 좋고 딱딱이도 좋아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002446.1.23",
          "form": "생각보다 맛있어요 \r\n전 무조건 소과 시켜요 \r\n귤은 작을수록 맛있더라구요",
          "original_form": "생각보다 맛있어요 \r\n전 무조건 소과 시켜요 \r\n귤은 작을수록 맛있더라구요",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002446.1.24",
          "form": "복숭아 곧 들어가니까 많이많이 먹어요",
          "original_form": "복숭아 곧 들어가니까 많이많이 먹어요",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002446.1.25",
          "form": "귤 소과 한번 사먹어봐야겠어요",
          "original_form": "귤 소과 한번 사먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002446.1.26",
          "form": "네 안그래도 그래서 복숭아한박스 시켰어요..",
          "original_form": "네 안그래도 그래서 복숭아한박스 시켰어요..",
          "speaker_id": "1",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002446.1.27",
          "form": "올해의 마지막 복숭아가 될꺼같아요!ㅎ.ㅎ",
          "original_form": "올해의 마지막 복숭아가 될꺼같아요!ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002446.1.28",
          "form": "인터넷으로 주문해요?",
          "original_form": "인터넷으로 주문해요?",
          "speaker_id": "2",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002446.1.29",
          "form": "저 과일 몇번 실패한적있어서 인터넷은 어려워요 ㅠ",
          "original_form": "저 과일 몇번 실패한적있어서 인터넷은 어려워요 ㅠ",
          "speaker_id": "2",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002446.1.30",
          "form": "저 인터넷으로 한번 주문해봤어요!",
          "original_form": "저 인터넷으로 한번 주문해봤어요!",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002446.1.31",
          "form": "사실 저두 동네에서 사먹는데 복숭아 딜이 떳길래...홀린듯",
          "original_form": "사실 저두 동네에서 사먹는데 복숭아 딜이 떳길래...홀린듯",
          "speaker_id": "1",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002446.1.32",
          "form": "사실 과일은 코스트코에서 많이 사먹어요! 맛이 보장되어있더라구요",
          "original_form": "사실 과일은 코스트코에서 많이 사먹어요! 맛이 보장되어있더라구요",
          "speaker_id": "1",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002446.1.33",
          "form": "코스트코",
          "original_form": "코스트코",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002446.1.34",
          "form": "도시사는구나 부러워요",
          "original_form": "도시사는구나 부러워요",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002446.1.35",
          "form": "저도 부산여행 가서 한번 가봣는데 식자재 20만원어치 쟁였는데",
          "original_form": "저도 부산여행 가서 한번 가봣는데 식자재 20만원어치 쟁였는데",
          "speaker_id": "2",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002446.1.36",
          "form": "바보같이 여름날...집에 와서 보니 상한게 많더라구여 ㅠ",
          "original_form": "바보같이 여름날...집에 와서 보니 상한게 많더라구여 ㅠ",
          "speaker_id": "2",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002446.1.37",
          "form": "헐ㅠㅠㅠ너무슬퍼요...상하다니",
          "original_form": "헐ㅠㅠㅠ너무슬퍼요...상하다니",
          "speaker_id": "1",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002446.1.38",
          "form": "먹지도 못하고 상하면 진짜 맘 아파여..",
          "original_form": "먹지도 못하고 상하면 진짜 맘 아파여..",
          "speaker_id": "1",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002446.1.39",
          "form": "어떤거사셨길래요?",
          "original_form": "어떤거사셨길래요?",
          "speaker_id": "1",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002446.1.40",
          "form": "포도는 알이 다 떨어져있구",
          "original_form": "포도는 알이 다 떨어져있구",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002446.1.41",
          "form": "오렌지는 곰팡이가 피어있구",
          "original_form": "오렌지는 곰팡이가 피어있구",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002446.1.42",
          "form": "배는 두개가 물되있더라구요",
          "original_form": "배는 두개가 물되있더라구요",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002446.1.43",
          "form": "ㅠㅠㅠㅠㅠ아이구...",
          "original_form": "ㅠㅠㅠㅠㅠ아이구...",
          "speaker_id": "1",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002446.1.44",
          "form": "오렌지나 배나 포도나 다 과일값 비싼데..",
          "original_form": "오렌지나 배나 포도나 다 과일값 비싼데..",
          "speaker_id": "1",
          "time": "20210820 15:56"
        }
      ]
    }
