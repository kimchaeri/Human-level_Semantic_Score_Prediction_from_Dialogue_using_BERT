      "utterance": [
        {
          "id": "MDRW2100004191.1.1",
          "form": "오늘 정말 새벽부터 비가 하루종일 주륵주륵",
          "original_form": "오늘 정말 새벽부터 비가 하루종일 주륵주륵",
          "speaker_id": "2",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004191.1.2",
          "form": "끝없이 내리네요",
          "original_form": "끝없이 내리네요",
          "speaker_id": "2",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004191.1.3",
          "form": "여긴 오전까지 오다가 해쨍쨍~",
          "original_form": "여긴 오전까지 오다가 해쨍쨍~",
          "speaker_id": "1",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004191.1.4",
          "form": "아 지금 해가쨍쨍인가요^^ 부럽네요",
          "original_form": "아 지금 해가쨍쨍인가요^^ 부럽네요",
          "speaker_id": "2",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004191.1.5",
          "form": "하루종일 집에갇혀서 에어컨 틀고있네요ㅠ",
          "original_form": "하루종일 집에갇혀서 에어컨 틀고있네요ㅠ",
          "speaker_id": "2",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004191.1.6",
          "form": "시원하다 넘더워요 ㅎㅎㅎ",
          "original_form": "시원하다 넘더워요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004191.1.7",
          "form": "비와도 습도 높아서  에어컨 안틀수가 없네요",
          "original_form": "비와도 습도 높아서  에어컨 안틀수가 없네요",
          "speaker_id": "1",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004191.1.8",
          "form": "비오니까 습해서 에어껀 틀고있어요",
          "original_form": "비오니까 습해서 에어껀 틀고있어요",
          "speaker_id": "2",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004191.1.9",
          "form": "그쵸ㅠ",
          "original_form": "그쵸ㅠ",
          "speaker_id": "2",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004191.1.10",
          "form": "좀따 나가야 하는데 귀차니즘 ㅋ",
          "original_form": "좀따 나가야 하는데 귀차니즘 ㅋ",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004191.1.11",
          "form": "제가 사는 지역은",
          "original_form": "제가 사는 지역은",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004191.1.12",
          "form": "앞으로 한주간 내내 비소식이에요ㅠ",
          "original_form": "앞으로 한주간 내내 비소식이에요ㅠ",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004191.1.13",
          "form": "천둥번개도 치고있네요",
          "original_form": "천둥번개도 치고있네요",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004191.1.14",
          "form": "비 와도 싫고 너무 더워도 싫고 ㅎㅎㅎ",
          "original_form": "비 와도 싫고 너무 더워도 싫고 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100004191.1.15",
          "form": "ㅠㅠ아 날씨가 너머 이상해요",
          "original_form": "ㅠㅠ아 날씨가 너머 이상해요",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100004191.1.16",
          "form": "오늘도 비소식있었는데 결국은",
          "original_form": "오늘도 비소식있었는데 결국은",
          "speaker_id": "2",
          "time": "20210822 18:48"
        },
        {
          "id": "MDRW2100004191.1.17",
          "form": "비는안오고  날씨맑았네요^^;",
          "original_form": "비는안오고  날씨맑았네요^^;",
          "speaker_id": "2",
          "time": "20210822 18:48"
        },
        {
          "id": "MDRW2100004191.1.18",
          "form": "오늘은 비오다 말다 오락가락 하는 날씨네요",
          "original_form": "오늘은 비오다 말다 오락가락 하는 날씨네요",
