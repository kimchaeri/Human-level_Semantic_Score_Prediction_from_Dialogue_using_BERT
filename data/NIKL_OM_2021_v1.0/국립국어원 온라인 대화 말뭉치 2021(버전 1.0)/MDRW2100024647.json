      "utterance": [
        {
          "id": "MDRW2100024647.1.1",
          "form": "평소에 자주 보는 컨텐츠가 뭐에요?",
          "original_form": "평소에 자주 보는 컨텐츠가 뭐에요?",
          "speaker_id": "1",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100024647.1.2",
          "form": "먹방 뷰티 홈트요ㅋ",
          "original_form": "먹방 뷰티 홈트요ㅋ",
          "speaker_id": "2",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100024647.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100024647.1.4",
          "form": "겜방송이나 드라마 영화 좋아해요ㅎ",
          "original_form": "겜방송이나 드라마 영화 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210831 17:47"
        },
        {
          "id": "MDRW2100024647.1.5",
          "form": "홈트 추천해주세요!",
          "original_form": "홈트 추천해주세요!",
          "speaker_id": "1",
          "time": "20210831 17:47"
        },
        {
          "id": "MDRW2100024647.1.6",
          "form": "땅끄부부님이요!",
          "original_form": "땅끄부부님이요!",
          "speaker_id": "2",
          "time": "20210831 18:00"
        },
        {
          "id": "MDRW2100024647.1.7",
          "form": "혹시 아시나요ㅋㅋ",
          "original_form": "혹시 아시나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:00"
        },
        {
          "id": "MDRW2100024647.1.8",
          "form": "겜방송은 랄로님거 어쩌다 잠깐 본 적 있는데 되게 웃기더라구요ㅋㅋ",
          "original_form": "겜방송은 랄로님거 어쩌다 잠깐 본 적 있는데 되게 웃기더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024647.1.9",
          "form": "땅끄부부님! 한 때 열심히 따라했었죠..",
          "original_form": "땅끄부부님! 한 때 열심히 따라했었죠..",
          "speaker_id": "1",
          "time": "20210831 18:05"
        },
        {
          "id": "MDRW2100024647.1.10",
          "form": "칼소폭 시리즈별로 다 해본거 같아욬ㅋㅋㅋ",
          "original_form": "칼소폭 시리즈별로 다 해본거 같아욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:05"
        },
        {
          "id": "MDRW2100024647.1.11",
          "form": "넷플이나 티빙같은거 구독하시나요?",
          "original_form": "넷플이나 티빙같은거 구독하시나요?",
          "speaker_id": "1",
          "time": "20210831 18:05"
        },
        {
          "id": "MDRW2100024647.1.12",
          "form": "둘 다 구독했다가 요즘에는 볼 시간도 없구 해서 구독안하구 있어요",
          "original_form": "둘 다 구독했다가 요즘에는 볼 시간도 없구 해서 구독안하구 있어요",
          "speaker_id": "2",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024647.1.13",
          "form": "오 그쵸 은근 본전뽑을 시간이 안나요..ㅎ",
          "original_form": "오 그쵸 은근 본전뽑을 시간이 안나요..ㅎ",
          "speaker_id": "1",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024647.1.14",
          "form": "디즈니 플러스도 곧 한국 지원해준다고 들은거같은데",
          "original_form": "디즈니 플러스도 곧 한국 지원해준다고 들은거같은데",
          "speaker_id": "1",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024647.1.15",
          "form": "디즈니 조아하세요?",
          "original_form": "디즈니 조아하세요?",
          "speaker_id": "1",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024647.1.16",
          "form": "네ㅋㅋ좋아합니다!!",
          "original_form": "네ㅋㅋ좋아합니다!!",
          "speaker_id": "2",
          "time": "20210831 18:35"
        },
        {
          "id": "MDRW2100024647.1.17",
          "form": "저두요 ㅋㅋㅋ 주토피아 인생영화에요.. 초딩같겠지만",
          "original_form": "저두요 ㅋㅋㅋ 주토피아 인생영화에요.. 초딩같겠지만",
          "speaker_id": "1",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100024647.1.18",
          "form": "삽입곡도 넘 신나구..ㅎ",
          "original_form": "삽입곡도 넘 신나구..ㅎ",
          "speaker_id": "1",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100024647.1.19",
          "form": "아후 저 애니메이션 너무 좋아해요!",
          "original_form": "아후 저 애니메이션 너무 좋아해요!",
          "speaker_id": "2",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100024647.1.20",
          "form": "주토피아는 어른들도 좋아할만한 애니아닌가요ㅋㅋ",
          "original_form": "주토피아는 어른들도 좋아할만한 애니아닌가요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100024647.1.21",
          "form": "ㅋㅋㅋㅋㅋ오오 ㅠㅜ 공감해주셔서 좋네요",
          "original_form": "ㅋㅋㅋㅋㅋ오오 ㅠㅜ 공감해주셔서 좋네요",
          "speaker_id": "1",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100024647.1.22",
          "form": "디즈니는 옛날동심으로 돌아가게해줘서 너무 좋아요",
          "original_form": "디즈니는 옛날동심으로 돌아가게해줘서 너무 좋아요",
          "speaker_id": "2",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100024647.1.23",
          "form": "전 아직도 디즈니 비디오 가지고있어요ㅋㅋ",
          "original_form": "전 아직도 디즈니 비디오 가지고있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100024647.1.24",
          "form": "맞아요ㅜ 저는 막 스릴러 추리 뭐 이런 영화 보면 우울해지다라구요",
          "original_form": "맞아요ㅜ 저는 막 스릴러 추리 뭐 이런 영화 보면 우울해지다라구요",
          "speaker_id": "1",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100024647.1.25",
          "form": "우와 카세트비디오..요?ㅋㅋㅋㅋㅋ",
          "original_form": "우와 카세트비디오..요?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100024647.1.26",
          "form": "네!ㅋㅋㅋㅋ",
          "original_form": "네!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100024647.1.27",
          "form": "전 무서운거 못봐요ㅜ",
          "original_form": "전 무서운거 못봐요ㅜ",
          "speaker_id": "2",
