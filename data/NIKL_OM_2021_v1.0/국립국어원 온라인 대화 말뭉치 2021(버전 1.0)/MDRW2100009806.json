      "utterance": [
        {
          "id": "MDRW2100009806.1.1",
          "form": "쇼핑성공하신거 있으세요?",
          "original_form": "쇼핑성공하신거 있으세요?",
          "speaker_id": "2",
          "time": "20210823 10:10"
        },
        {
          "id": "MDRW2100009806.1.2",
          "form": "잘샀다 이런거",
          "original_form": "잘샀다 이런거",
          "speaker_id": "2",
          "time": "20210823 10:10"
        },
        {
          "id": "MDRW2100009806.1.3",
          "form": "저는 제습기요ㅎㅎ 고민하다 샀는데 정말 잘쓰고 있어요",
          "original_form": "저는 제습기요ㅎㅎ 고민하다 샀는데 정말 잘쓰고 있어요",
          "speaker_id": "1",
          "time": "20210823 10:11"
        },
        {
          "id": "MDRW2100009806.1.4",
          "form": "어떤게 있으세요?",
          "original_form": "어떤게 있으세요?",
          "speaker_id": "1",
          "time": "20210823 10:11"
        },
        {
          "id": "MDRW2100009806.1.5",
          "form": "제습기 태풍오는데",
          "original_form": "제습기 태풍오는데",
          "speaker_id": "2",
          "time": "20210823 10:11"
        },
        {
          "id": "MDRW2100009806.1.6",
          "form": "딱좋네요",
          "original_form": "딱좋네요",
          "speaker_id": "2",
          "time": "20210823 10:11"
        },
        {
          "id": "MDRW2100009806.1.7",
          "form": "저는 당근마켓으로산 모자요",
          "original_form": "저는 당근마켓으로산 모자요",
          "speaker_id": "2",
          "time": "20210823 10:12"
        },
        {
          "id": "MDRW2100009806.1.8",
          "form": "오 저도 당근 애용하는데 사본적은 없어요ㅠ",
          "original_form": "오 저도 당근 애용하는데 사본적은 없어요ㅠ",
          "speaker_id": "1",
          "time": "20210823 10:12"
        },
        {
          "id": "MDRW2100009806.1.9",
          "form": "당근 파시기만하셨나요",
          "original_form": "당근 파시기만하셨나요",
          "speaker_id": "2",
          "time": "20210823 10:19"
        },
        {
          "id": "MDRW2100009806.1.10",
          "form": "네 저는 팔기만 해본거같아요ㅎㅎ",
          "original_form": "네 저는 팔기만 해본거같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 10:19"
        },
        {
          "id": "MDRW2100009806.1.11",
          "form": "저는 주로사기만ㅎㅎㅎ 근데 모자 잃어버려서 슬퍼요",
          "original_form": "저는 주로사기만ㅎㅎㅎ 근데 모자 잃어버려서 슬퍼요",
          "speaker_id": "2",
          "time": "20210823 10:20"
        },
        {
          "id": "MDRW2100009806.1.12",
          "form": "어머ㅜㅜㅜ아쉬워요ㅠㅠ",
          "original_form": "어머ㅜㅜㅜ아쉬워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 10:22"
        },
        {
          "id": "MDRW2100009806.1.13",
          "form": "근데 친구가 하나 줬어요",
          "original_form": "근데 친구가 하나 줬어요",
          "speaker_id": "2",
          "time": "20210823 10:23"
        },
        {
          "id": "MDRW2100009806.1.14",
          "form": "제가 옆에서 딱해보였나봐요ㅋㅋㅋㅋ",
          "original_form": "제가 옆에서 딱해보였나봐요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:23"
        },
        {
          "id": "MDRW2100009806.1.15",
          "form": "저도 그런적있어요ㅎㅎ팔찌 사자마자 끊어져서 친구가 선물로 사준ㅋㅋ",
          "original_form": "저도 그런적있어요ㅎㅎ팔찌 사자마자 끊어져서 친구가 선물로 사준ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 10:24"
        },
        {
          "id": "MDRW2100009806.1.16",
          "form": "아ㅜ 좋은친구를 두셨네요",
          "original_form": "아ㅜ 좋은친구를 두셨네요",
          "speaker_id": "2",
          "time": "20210823 10:26"
        },
        {
          "id": "MDRW2100009806.1.17",
          "form": "집에공구있으면 뚝딱수리하는데",
          "original_form": "집에공구있으면 뚝딱수리하는데",
          "speaker_id": "2",
          "time": "20210823 10:26"
        },
        {
          "id": "MDRW2100009806.1.18",
          "form": "고치셨나요",
          "original_form": "고치셨나요",
          "speaker_id": "2",
          "time": "20210823 10:26"
        },
        {
          "id": "MDRW2100009806.1.19",
          "form": "길거리라 비즈가 다떨어져셔ㅎㅎ그냥 버렸어요ㅠㅠ",
          "original_form": "길거리라 비즈가 다떨어져셔ㅎㅎ그냥 버렸어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 10:27"
        },
        {
          "id": "MDRW2100009806.1.20",
          "form": "아..민망하셨겠네요",
          "original_form": "아..민망하셨겠네요",
          "speaker_id": "2",
          "time": "20210823 10:27"
        },
        {
          "id": "MDRW2100009806.1.21",
          "form": "줍는것도 부끄럽고 잘하셨어요ㅜ",
          "original_form": "줍는것도 부끄럽고 잘하셨어요ㅜ",
          "speaker_id": "2",
          "time": "20210823 10:28"
        },
        {
          "id": "MDRW2100009806.1.22",
          "form": "짜증많이 났는데ㅎㅎ 어쩔수없었죠",
          "original_form": "짜증많이 났는데ㅎㅎ 어쩔수없었죠",
          "speaker_id": "1",
          "time": "20210823 10:28"
        }
      ]
    }
