      "utterance": [
        {
          "id": "MDRW2100012292.1.1",
          "form": "지금 kbs에서 올림픽 개막식하네",
          "original_form": "지금 kbs에서 올림픽 개막식하네",
          "speaker_id": "2",
          "time": "20210824 21:01"
        },
        {
          "id": "MDRW2100012292.1.2",
          "form": "개막식 오늘이구나 ㅎㅎ 몸 불편한데도 운동하는 것보면 정신력이 대단한 것 같아",
          "original_form": "개막식 오늘이구나 ㅎㅎ 몸 불편한데도 운동하는 것보면 정신력이 대단한 것 같아",
          "speaker_id": "1",
          "time": "20210824 21:02"
        },
        {
          "id": "MDRW2100012292.1.3",
          "form": "ㅇㅇ 근육들도 장난 아니더만",
          "original_form": "ㅇㅇ 근육들도 장난 아니더만",
          "speaker_id": "2",
          "time": "20210824 21:02"
        },
        {
          "id": "MDRW2100012292.1.4",
          "form": "힘들텐데 어떻게 그렇게들 운동하는지",
          "original_form": "힘들텐데 어떻게 그렇게들 운동하는지",
          "speaker_id": "1",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100012292.1.5",
          "form": "장애 극복하고 메달 따는 건 진짜 정신 승리",
          "original_form": "장애 극복하고 메달 따는 건 진짜 정신 승리",
          "speaker_id": "2",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100012292.1.6",
          "form": "독일이 강국인가봐",
          "original_form": "독일이 강국인가봐",
          "speaker_id": "1",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100012292.1.7",
          "form": "ㅇㅇ 그런듯~ 그동안 메달 엄청 땄네",
          "original_form": "ㅇㅇ 그런듯~ 그동안 메달 엄청 땄네",
          "speaker_id": "2",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100012292.1.8",
          "form": "개막식 하느라 오늘 뉴스 안해?",
          "original_form": "개막식 하느라 오늘 뉴스 안해?",
          "speaker_id": "1",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100012292.1.9",
          "form": "ㅇㅇ 애들 손톱 깎아주면서 뉴스 보려고 했더니 안하네",
          "original_form": "ㅇㅇ 애들 손톱 깎아주면서 뉴스 보려고 했더니 안하네",
          "speaker_id": "2",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100012292.1.10",
          "form": "이런~나도 뉴스 보려고 했는데",
          "original_form": "이런~나도 뉴스 보려고 했는데",
          "speaker_id": "1",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100012292.1.11",
          "form": "다른 채널 찾아볼께 ㅎㅎ 신랑 컴퓨터 다 하면 나와",
          "original_form": "다른 채널 찾아볼께 ㅎㅎ 신랑 컴퓨터 다 하면 나와",
          "speaker_id": "2",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100012292.1.12",
          "form": "ㅎㅎ 알았응",
          "original_form": "ㅎㅎ 알았응",
          "speaker_id": "1",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100012292.1.13",
          "form": "저녁 뉴스를 봐야 하루가 정리되는데 말야 ㅋㅋ",
