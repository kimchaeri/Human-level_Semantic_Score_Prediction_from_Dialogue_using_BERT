      "utterance": [
        {
          "id": "MDRW2100015822.1.1",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "1",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015822.1.2",
          "form": "네 대학생이요",
          "original_form": "네 대학생이요",
          "speaker_id": "2",
          "time": "20210826 01:32"
        },
        {
          "id": "MDRW2100015822.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 01:32"
        },
        {
          "id": "MDRW2100015822.1.4",
          "form": "전 직장인ㅎ",
          "original_form": "전 직장인ㅎ",
          "speaker_id": "1",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100015822.1.5",
          "form": "방학중이겠네요",
          "original_form": "방학중이겠네요",
          "speaker_id": "1",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100015822.1.6",
          "form": "네 개강이 얼마 남지 않았어요",
          "original_form": "네 개강이 얼마 남지 않았어요",
          "speaker_id": "2",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015822.1.7",
          "form": "그러네요 9월개강이니",
          "original_form": "그러네요 9월개강이니",
          "speaker_id": "1",
          "time": "20210826 01:38"
        },
        {
          "id": "MDRW2100015822.1.8",
          "form": "대면수업인가요?",
          "original_form": "대면수업인가요?",
          "speaker_id": "1",
          "time": "20210826 01:38"
        },
        {
          "id": "MDRW2100015822.1.9",
          "form": "지금은 다 비대면이요 ㅎㅎ",
          "original_form": "지금은 다 비대면이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 01:39"
        },
        {
          "id": "MDRW2100015822.1.10",
          "form": "몇학년이세요? 대학생활 즐기질 못하니",
          "original_form": "몇학년이세요? 대학생활 즐기질 못하니",
          "speaker_id": "1",
          "time": "20210826 01:43"
        },
        {
          "id": "MDRW2100015822.1.11",
          "form": "넘 안타까워요",
          "original_form": "넘 안타까워요",
          "speaker_id": "1",
          "time": "20210826 01:43"
        },
        {
          "id": "MDRW2100015822.1.12",
          "form": "요즘학생들보면",
          "original_form": "요즘학생들보면",
          "speaker_id": "1",
          "time": "20210826 01:43"
        },
        {
          "id": "MDRW2100015822.1.13",
          "form": "아직 1학년이요 ㅎㅎ",
          "original_form": "아직 1학년이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 01:44"
        },
        {
          "id": "MDRW2100015822.1.14",
          "form": "아 엠티도 못가보고 친구들도 많이 못만났겠어요",
          "original_form": "아 엠티도 못가보고 친구들도 많이 못만났겠어요",
          "speaker_id": "1",
          "time": "20210826 01:46"
        },
        {
          "id": "MDRW2100015822.1.15",
          "form": "등록금은 다내죠?",
          "original_form": "등록금은 다내죠?",
          "speaker_id": "1",
          "time": "20210826 01:46"
        },
        {
          "id": "MDRW2100015822.1.16",
          "form": "아 네 다 내요 ㅠ",
          "original_form": "아 네 다 내요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 01:46"
        },
        {
          "id": "MDRW2100015822.1.17",
          "form": "흑흐그흐긓그흑흑",
          "original_form": "흑흐그흐긓그흑흑",
          "speaker_id": "2",
          "time": "20210826 01:46"
        },
        {
          "id": "MDRW2100015822.1.18",
          "form": "코로나가 진짜 문제ㅠㅠ",
          "original_form": "코로나가 진짜 문제ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 01:48"
        },
        {
          "id": "MDRW2100015822.1.19",
          "form": "등록금이 진짜 좀 짜증나요 ㅠㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "등록금이 진짜 좀 짜증나요 ㅠㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:49"
        },
        {
          "id": "MDRW2100015822.1.20",
          "form": "그니까요 뭐학교시설을 사용을하나",
          "original_form": "그니까요 뭐학교시설을 사용을하나",
          "speaker_id": "1",
          "time": "20210826 01:50"
        },
        {
          "id": "MDRW2100015822.1.21",
          "form": "교수들도 옛날꺼 틀어주고 수업하기도한다던데",
          "original_form": "교수들도 옛날꺼 틀어주고 수업하기도한다던데",
          "speaker_id": "1",
          "time": "20210826 01:50"
        },
        {
          "id": "MDRW2100015822.1.22",
          "form": "그렇죠 흑흑",
          "original_form": "그렇죠 흑흑",
          "speaker_id": "2",
          "time": "20210826 01:50"
        },
        {
          "id": "MDRW2100015822.1.23",
          "form": "뭐...어쩌겠어요.....",
          "original_form": "뭐...어쩌겠어요.....",
          "speaker_id": "2",
          "time": "20210826 01:50"
        },
        {
          "id": "MDRW2100015822.1.24",
          "form": "빨리 코로나가 잠잠해져야 할텐데 큰일이네요",
          "original_form": "빨리 코로나가 잠잠해져야 할텐데 큰일이네요",
          "speaker_id": "1",
          "time": "20210826 01:51"
        },
        {
          "id": "MDRW2100015822.1.25",
          "form": "대학때 배낭여행도 가보고 그런게 다추억인데",
          "original_form": "대학때 배낭여행도 가보고 그런게 다추억인데",
          "speaker_id": "1",
          "time": "20210826 01:52"
        },
        {
          "id": "MDRW2100015822.1.26",
          "form": "흑흑흑흑ㅎ그그극",
          "original_form": "흑흑흑흑ㅎ그그극",
          "speaker_id": "2",
          "time": "20210826 01:52"
        },
        {
          "id": "MDRW2100015822.1.27",
          "form": "배낭여행 가보셨어요???",
          "original_form": "배낭여행 가보셨어요???",
          "speaker_id": "2",
          "time": "20210826 01:52"
        },
        {
          "id": "MDRW2100015822.1.28",
          "form": "저도 그걸못해보고 졸업해서 평생 후회되더라구요",
          "original_form": "저도 그걸못해보고 졸업해서 평생 후회되더라구요",
          "speaker_id": "1",
          "time": "20210826 01:53"
        },
        {
          "id": "MDRW2100015822.1.29",
          "form": "한달정도 방학때 가고싶었는데",
          "original_form": "한달정도 방학때 가고싶었는데",
          "speaker_id": "1",
          "time": "20210826 01:53"
        },
        {
          "id": "MDRW2100015822.1.30",
          "form": "교환학생 갈려구요 저 3학년 때면 코로나 끝나지 않늘까요",
          "original_form": "교환학생 갈려구요 저 3학년 때면 코로나 끝나지 않늘까요",
          "speaker_id": "2",
          "time": "20210826 01:57"
        },
        {
          "id": "MDRW2100015822.1.31",
          "form": "끝나진 않을거같고 그냥 백신으로 살아가겠죠^^",
          "original_form": "끝나진 않을거같고 그냥 백신으로 살아가겠죠^^",
          "speaker_id": "1",
