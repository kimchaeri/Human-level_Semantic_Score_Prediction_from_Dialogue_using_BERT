      "utterance": [
        {
          "id": "MDRW2100011895.1.1",
          "form": "언제 오는데?",
          "original_form": "언제 오는데?",
          "speaker_id": "2",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100011895.1.2",
          "form": "오늘 올거 같은데",
          "original_form": "오늘 올거 같은데",
          "speaker_id": "1",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100011895.1.3",
          "form": "근데 그릇은 갑자기 왜?",
          "original_form": "근데 그릇은 갑자기 왜?",
          "speaker_id": "2",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100011895.1.4",
          "form": "요거트 시리얼이랑 말아 먹는데 손잡이가 있으면 좋을 거 같아서",
          "original_form": "요거트 시리얼이랑 말아 먹는데 손잡이가 있으면 좋을 거 같아서",
          "speaker_id": "1",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100011895.1.5",
          "form": "아 그럴 수 있지",
          "original_form": "아 그럴 수 있지",
          "speaker_id": "2",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100011895.1.6",
          "form": "색깔도 되게 예뻐",
          "original_form": "색깔도 되게 예뻐",
          "speaker_id": "1",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100011895.1.7",
          "form": "무슨색인데?",
          "original_form": "무슨색인데?",
          "speaker_id": "2",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100011895.1.8",
          "form": "엄청 진한 초록색",
          "original_form": "엄청 진한 초록색",
          "speaker_id": "1",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100011895.1.9",
          "form": "민트는 아니지?",
          "original_form": "민트는 아니지?",
          "speaker_id": "2",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100011895.1.10",
          "form": "민트색은 아니지",
          "original_form": "민트색은 아니지",
          "speaker_id": "1",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100011895.1.11",
          "form": "다행이네",
          "original_form": "다행이네",
          "speaker_id": "2",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100011895.1.12",
          "form": "왜? 민트색 싫어해?",
          "original_form": "왜? 민트색 싫어해?",
          "speaker_id": "1",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100011895.1.13",
          "form": "응  별로야",
          "original_form": "응  별로야",
          "speaker_id": "2",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100011895.1.14",
          "form": "아 그래 왜?",
          "original_form": "아 그래 왜?",
          "speaker_id": "1",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100011895.1.15",
          "form": "색상이 좀 별로라서",
          "original_form": "색상이 좀 별로라서",
          "speaker_id": "2",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100011895.1.16",
          "form": "그럼 좋아하는 색깔은 뭐야?",
          "original_form": "그럼 좋아하는 색깔은 뭐야?",
          "speaker_id": "1",
          "time": "20210824 17:12"
        },
