      "utterance": [
        {
          "id": "MDRW2100013659.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013659.1.2",
          "form": "안녕하세요 좋아하시는 음식 있나요?",
          "original_form": "안녕하세요 좋아하시는 음식 있나요?",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013659.1.3",
          "form": "전 파스타 좋아해요ㅎㅎ",
          "original_form": "전 파스타 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013659.1.4",
          "form": "파스타 좋죠 전 로제파스타가 최애에요",
          "original_form": "파스타 좋죠 전 로제파스타가 최애에요",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013659.1.5",
          "form": "로제파스타 맛있어요ㅎㅎ",
          "original_form": "로제파스타 맛있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013659.1.6",
          "form": "요새 로제 소스 인기 많은거 같아요!",
          "original_form": "요새 로제 소스 인기 많은거 같아요!",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013659.1.7",
          "form": "맞아요 로세떡볶이도 인기많더라구요",
          "original_form": "맞아요 로세떡볶이도 인기많더라구요",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013659.1.8",
          "form": "네 맞아요 은근 잘 어울려요ㅋㅋ",
          "original_form": "네 맞아요 은근 잘 어울려요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013659.1.9",
          "form": "맛집 아세요?",
          "original_form": "맛집 아세요?",
          "speaker_id": "1",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013659.1.10",
          "form": "신전이요",
          "original_form": "신전이요",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013659.1.11",
          "form": "신전 진짜 맛있어요 로제떡볶이는",
          "original_form": "신전 진짜 맛있어요 로제떡볶이는",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013659.1.12",
          "form": "오 먹어봐야겠어요ㅎㅎ",
          "original_form": "오 먹어봐야겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013659.1.13",
          "form": "배떡도 괜찮더라고요ㅋㅋ",
          "original_form": "배떡도 괜찮더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013659.1.14",
          "form": "배떡도 맛있죠 청년다방도 맛있어요",
          "original_form": "배떡도 맛있죠 청년다방도 맛있어요",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013659.1.15",
          "form": "솔님은 무슨파스타가 최애에요?",
          "original_form": "솔님은 무슨파스타가 최애에요?",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013659.1.16",
          "form": "전 크림파스타를 좋아해요ㅎㅎ",
          "original_form": "전 크림파스타를 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013659.1.17",
          "form": "크림 맛있죠  ㅋㅋㅋ 피클하고 같이 먹으면 궁합좋더라고요",
          "original_form": "크림 맛있죠  ㅋㅋㅋ 피클하고 같이 먹으면 궁합좋더라고요",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013659.1.18",
          "form": "맞아요 궁합좋죠ㅎㅎ",
          "original_form": "맞아요 궁합좋죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013659.1.19",
          "form": "전 만들때 새우도 넣어서 먹어요ㅋㅋ",
          "original_form": "전 만들때 새우도 넣어서 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013659.1.20",
          "form": "오 직접 만들어서 드세요??",
          "original_form": "오 직접 만들어서 드세요??",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013659.1.21",
          "form": "네네ㅎㅎ그냥 다 넣고 볶으면 되니까요ㅋㅋㅋ",
          "original_form": "네네ㅎㅎ그냥 다 넣고 볶으면 되니까요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:45"
