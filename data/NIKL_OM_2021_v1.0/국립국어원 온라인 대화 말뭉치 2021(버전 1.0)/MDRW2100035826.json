      "utterance": [
        {
          "id": "MDRW2100035826.1.1",
          "form": "맞아요 그 마음 너무 이해가 돼요ㅠㅠ",
          "original_form": "맞아요 그 마음 너무 이해가 돼요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 03:51"
        },
        {
          "id": "MDRW2100035826.1.2",
          "form": "name2님은 혹시 장 어디서 보세요?",
          "original_form": "&name2&님은 혹시 장 어디서 보세요?",
          "speaker_id": "1",
          "time": "20210830 03:51"
        },
        {
          "id": "MDRW2100035826.1.3",
          "form": "이마트몰에서 쓱배송 시켜요",
          "original_form": "이마트몰에서 쓱배송 시켜요",
          "speaker_id": "2",
          "time": "20210830 03:52"
        },
        {
          "id": "MDRW2100035826.1.4",
          "form": "편하더라구요",
          "original_form": "편하더라구요",
          "speaker_id": "2",
          "time": "20210830 03:52"
        },
        {
          "id": "MDRW2100035826.1.5",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210830 03:52"
        },
        {
          "id": "MDRW2100035826.1.6",
          "form": "식료품 말고도 생필품같은거",
          "original_form": "식료품 말고도 생필품같은거",
          "speaker_id": "1",
          "time": "20210830 03:52"
        },
        {
          "id": "MDRW2100035826.1.7",
          "form": "이마트가 싸더라구요",
          "original_form": "이마트가 싸더라구요",
          "speaker_id": "1",
          "time": "20210830 03:52"
        },
        {
          "id": "MDRW2100035826.1.8",
          "form": "코스트코는 가세용?",
          "original_form": "코스트코는 가세용?",
          "speaker_id": "1",
          "time": "20210830 03:52"
        },
        {
          "id": "MDRW2100035826.1.9",
          "form": "1인가구라 코스트코 같이 대용량으로 파는 곳은 잘 못 가요ㅠㅠ",
          "original_form": "1인가구라 코스트코 같이 대용량으로 파는 곳은 잘 못 가요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 03:53"
        },
        {
          "id": "MDRW2100035826.1.10",
          "form": "어릴 때는 엄청 자주 갔어요ㅋㅋ",
          "original_form": "어릴 때는 엄청 자주 갔어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 03:53"
        },
        {
          "id": "MDRW2100035826.1.11",
          "form": "아이구ㅠㅠ 저도 엄마랑 둘이서 사는데",
          "original_form": "아이구ㅠㅠ 저도 엄마랑 둘이서 사는데",
          "speaker_id": "1",
          "time": "20210830 03:53"
        },
        {
          "id": "MDRW2100035826.1.12",
          "form": "물건 하나를 사도",
          "original_form": "물건 하나를 사도",
          "speaker_id": "1",
          "time": "20210830 03:53"
        },
        {
          "id": "MDRW2100035826.1.13",
          "form": "좀 대용량으로 파는것들은 안사게 되더라구요",
          "original_form": "좀 대용량으로 파는것들은 안사게 되더라구요",
          "speaker_id": "1",
          "time": "20210830 03:53"
        },
        {
          "id": "MDRW2100035826.1.14",
          "form": "맞아요. 특히 식료품은 냉장고에 넣을 곳도 없고 너무 빨리 상해서..ㅠㅠ",
          "original_form": "맞아요. 특히 식료품은 냉장고에 넣을 곳도 없고 너무 빨리 상해서..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 03:55"
        },
        {
          "id": "MDRW2100035826.1.15",
          "form": "name1님은 코스트코 자주 가세요?",
          "original_form": "&name1&님은 코스트코 자주 가세요?",
          "speaker_id": "2",
          "time": "20210830 03:55"
        },
        {
          "id": "MDRW2100035826.1.16",
          "form": "전 가~끔 가는데",
          "original_form": "전 가~끔 가는데",
          "speaker_id": "1",
          "time": "20210830 03:55"
        },
        {
          "id": "MDRW2100035826.1.17",
          "form": "세제나 영양제같은게 저렴해서",
          "original_form": "세제나 영양제같은게 저렴해서",
          "speaker_id": "1",
          "time": "20210830 03:56"
        },
        {
          "id": "MDRW2100035826.1.18",
          "form": "쇼핑하러 갑니당ㅎㅎ",
          "original_form": "쇼핑하러 갑니당ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 03:56"
        },
        {
          "id": "MDRW2100035826.1.19",
          "form": "얼마전에 휴지를 사려고하는데",
          "original_form": "얼마전에 휴지를 사려고하는데",
          "speaker_id": "1",
          "time": "20210830 03:56"
        },
        {
          "id": "MDRW2100035826.1.20",
          "form": "원플러스원이었는데 고민하다 안샀어요ㅠ",
          "original_form": "원플러스원이었는데 고민하다 안샀어요ㅠ",
          "speaker_id": "1",
          "time": "20210830 03:56"
        },
        {
          "id": "MDRW2100035826.1.21",
          "form": "아 그럴 때 너무 아쉬워요 같이 나눌 사람이 있으면 좋은데~",
          "original_form": "아 그럴 때 너무 아쉬워요 같이 나눌 사람이 있으면 좋은데~",
          "speaker_id": "2",
          "time": "20210830 03:57"
        },
        {
          "id": "MDRW2100035826.1.22",
          "form": "네네 맞아요ㅋㅋㅋ",
          "original_form": "네네 맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 03:58"
        },
        {
          "id": "MDRW2100035826.1.23",
          "form": "저도 맨날 인터넷으로 대량구매하고 놓을 곳 없어서 베란다에 둬요ㅠㅠ",
          "original_form": "저도 맨날 인터넷으로 대량구매하고 놓을 곳 없어서 베란다에 둬요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 03:58"
        },
        {
          "id": "MDRW2100035826.1.24",
          "form": "대량으로 사면 싸게 할인해주니까",
          "original_form": "대량으로 사면 싸게 할인해주니까",
          "speaker_id": "1",
          "time": "20210830 03:58"
        },
        {
          "id": "MDRW2100035826.1.25",
          "form": "고민하다가 넘 저렴한건 어쩔수없이",
          "original_form": "고민하다가 넘 저렴한건 어쩔수없이",
          "speaker_id": "1",
          "time": "20210830 03:58"
        },
        {
          "id": "MDRW2100035826.1.26",
          "form": "사게되더라구요ㅋㅋㅋ",
          "original_form": "사게되더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 03:58"
        },
        {
          "id": "MDRW2100035826.1.27",
          "form": "그래서 요새 그런 거 당근마켓으로 나눠 사는 사람들도 있더라구요! 좋은 방법이라고 생각했어요ㅋㅋㅋ",
          "original_form": "그래서 요새 그런 거 당근마켓으로 나눠 사는 사람들도 있더라구요! 좋은 방법이라고 생각했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 03:59"
        },
        {
          "id": "MDRW2100035826.1.28",
          "form": "아 그런방법도 있겠네요!",
          "original_form": "아 그런방법도 있겠네요!",
          "speaker_id": "1",
          "time": "20210830 03:59"
        },
        {
          "id": "MDRW2100035826.1.29",
          "form": "너무 좋은 방법이에요ㅋㅋㅋ",
          "original_form": "너무 좋은 방법이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 04:00"
        },
        {
          "id": "MDRW2100035826.1.30",
          "form": "나중에 대량으로 살때 참고할게요!",
          "original_form": "나중에 대량으로 살때 참고할게요!",
          "speaker_id": "1",
          "time": "20210830 04:00"
        },
        {
          "id": "MDRW2100035826.1.31",
          "form": "흑 얘기 더 나누고 싶었는데",
          "original_form": "흑 얘기 더 나누고 싶었는데",
          "speaker_id": "1",
          "time": "20210830 04:00"
        },
        {
          "id": "MDRW2100035826.1.32",
          "form": "제가 지금 졸려서ㅠㅠㅠ",
          "original_form": "제가 지금 졸려서ㅠㅠㅠ",
