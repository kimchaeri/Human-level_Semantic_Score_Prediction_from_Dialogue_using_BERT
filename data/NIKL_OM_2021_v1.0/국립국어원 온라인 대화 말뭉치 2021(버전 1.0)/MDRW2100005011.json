      "utterance": [
        {
          "id": "MDRW2100005011.1.1",
          "form": "안냐세요!!",
          "original_form": "안냐세요!!",
          "speaker_id": "1",
          "time": "20210821 19:56"
        },
        {
          "id": "MDRW2100005011.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210821 19:56"
        },
        {
          "id": "MDRW2100005011.1.3",
          "form": "공연보는거 좋아하세요..??",
          "original_form": "공연보는거 좋아하세요..??",
          "speaker_id": "1",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100005011.1.4",
          "form": "공연은 많이 안 봤어요",
          "original_form": "공연은 많이 안 봤어요",
          "speaker_id": "2",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100005011.1.5",
          "form": "name1님은 좋아하는 공연 있으세요?",
          "original_form": "&name1&님은 좋아하는 공연 있으세요?",
          "speaker_id": "2",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100005011.1.6",
          "form": "저는 아이돌 좋아해서!",
          "original_form": "저는 아이돌 좋아해서!",
          "speaker_id": "1",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100005011.1.7",
          "form": "콘서트 가는거 좋아해요 !",
          "original_form": "콘서트 가는거 좋아해요 !",
          "speaker_id": "1",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100005011.1.8",
          "form": "아 전 아이돌 좋아할 때도 콘서트 한 번 못가봤네요",
          "original_form": "아 전 아이돌 좋아할 때도 콘서트 한 번 못가봤네요",
          "speaker_id": "2",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100005011.1.9",
          "form": "요즘엔 코로나 때문에 공연 자체가 많이 없어서 아쉬워요",
          "original_form": "요즘엔 코로나 때문에 공연 자체가 많이 없어서 아쉬워요",
          "speaker_id": "2",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100005011.1.10",
          "form": "맞아요..ㅠㅠ",
          "original_form": "맞아요..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100005011.1.11",
          "form": "뮤지컬은 풀렸는데 콘서트는 아직 안풀려서 더 아쉬운거 같아요ㅠ",
          "original_form": "뮤지컬은 풀렸는데 콘서트는 아직 안풀려서 더 아쉬운거 같아요ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100005011.1.12",
          "form": "그러게요 생계 어려운 뮤지션들 많을텐데 코로나는 언제 끝날런지....",
          "original_form": "그러게요 생계 어려운 뮤지션들 많을텐데 코로나는 언제 끝날런지....",
          "speaker_id": "2",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100005011.1.13",
          "form": "전시회는 좋아하세요?",
          "original_form": "전시회는 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100005011.1.14",
          "form": "저 전시회도 좋아해요!",
          "original_form": "저 전시회도 좋아해요!",
          "speaker_id": "1",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100005011.1.15",
          "form": "근데 많이 가본적은 없어서 코로나 괜찮아지면 전시회도 다녀보고 싶어요",
          "original_form": "근데 많이 가본적은 없어서 코로나 괜찮아지면 전시회도 다녀보고 싶어요",
          "speaker_id": "1",
          "time": "20210821 20:02"
        },
        {
          "id": "MDRW2100005011.1.16",
          "form": "제일 좋았던 전시가 뭐예요?",
          "original_form": "제일 좋았던 전시가 뭐예요?",
          "speaker_id": "2",
          "time": "20210821 20:02"
        },
        {
          "id": "MDRW2100005011.1.17",
          "form": "저도 좋아하는데 많이는 못 가봤어요",
          "original_form": "저도 좋아하는데 많이는 못 가봤어요",
          "speaker_id": "2",
          "time": "20210821 20:02"
        },
        {
          "id": "MDRW2100005011.1.18",
          "form": "음.. 제일 마지막으로 가본게 집 근처에서 반고흐 전시회 였어서",
          "original_form": "음.. 제일 마지막으로 가본게 집 근처에서 반고흐 전시회 였어서",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100005011.1.19",
          "form": "이게 젤 좋았던거 같아요",
          "original_form": "이게 젤 좋았던거 같아요",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100005011.1.20",
          "form": "오 저 반 고흐 좋아하는데!",
          "original_form": "오 저 반 고흐 좋아하는데!",
          "speaker_id": "2",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100005011.1.21",
          "form": "그림도 이쁘구.. 되게 잘돼있더라구요",
          "original_form": "그림도 이쁘구.. 되게 잘돼있더라구요",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100005011.1.22",
          "form": "그 전시 뭔지 알 것 같아요",
          "original_form": "그 전시 뭔지 알 것 같아요",
          "speaker_id": "2",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100005011.1.23",
          "form": "전 못 갔지만ㅋㅋ",
          "original_form": "전 못 갔지만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100005011.1.24",
          "form": "친구한테 끌려간거라 기대 안하고 갔는데",
          "original_form": "친구한테 끌려간거라 기대 안하고 갔는데",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100005011.1.25",
          "form": "의외로 넘 좋아서!!",
          "original_form": "의외로 넘 좋아서!!",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100005011.1.26",
          "form": "저는 몇 년 전에 용산 전쟁기념박물관에서 감정에 관한 전시 봤었어요",
          "original_form": "저는 몇 년 전에 용산 전쟁기념박물관에서 감정에 관한 전시 봤었어요",
          "speaker_id": "2",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100005011.1.27",
          "form": "되게 알록달록하고 예쁘게 꾸며져있어서 재밌었어요",
          "original_form": "되게 알록달록하고 예쁘게 꾸며져있어서 재밌었어요",
          "speaker_id": "2",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100005011.1.28",
          "form": "헉 너무 좋았을거 같아요...",
          "original_form": "헉 너무 좋았을거 같아요...",
          "speaker_id": "1",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100005011.1.29",
          "form": "이동하면서 공간을 즐기는 형식이었는데 중간에 풀장이 엄청 재밌었어요!!",
          "original_form": "이동하면서 공간을 즐기는 형식이었는데 중간에 풀장이 엄청 재밌었어요!!",
          "speaker_id": "2",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100005011.1.30",
          "form": "요즘엔 전시회가 엄청 재밌게 잘돼있는거 같아요",
          "original_form": "요즘엔 전시회가 엄청 재밌게 잘돼있는거 같아요",
          "speaker_id": "1",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100005011.1.31",
          "form": "예전에는 지루하고.. 그랬었는데",
          "original_form": "예전에는 지루하고.. 그랬었는데",
          "speaker_id": "1",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100005011.1.32",
          "form": "저두 가보고싶어요ㅠㅠ",
          "original_form": "저두 가보고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 20:05"
        }
      ]
