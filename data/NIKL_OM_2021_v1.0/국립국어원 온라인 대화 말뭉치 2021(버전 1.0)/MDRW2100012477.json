      "utterance": [
        {
          "id": "MDRW2100012477.1.1",
          "form": "name1은 영양제같은거 드세요?",
          "original_form": "&name1&은 영양제같은거 드세요?",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100012477.1.2",
          "form": "네 저는 비타민이랑 유산균 먹고 있어요",
          "original_form": "네 저는 비타민이랑 유산균 먹고 있어요",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100012477.1.3",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100012477.1.4",
          "form": "오 저는 거기에 루테인 먹고있어요",
          "original_form": "오 저는 거기에 루테인 먹고있어요",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100012477.1.5",
          "form": "눈이 언제까지나 잘보이고싶은 욕심ㅎㅎ",
          "original_form": "눈이 언제까지나 잘보이고싶은 욕심ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100012477.1.6",
          "form": "루테인 어때요? 효과가 있나요?",
          "original_form": "루테인 어때요? 효과가 있나요?",
          "speaker_id": "1",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100012477.1.7",
          "form": "오 먹는거랑 안먹는거랑뭔가 기분탓인지 달라요",
          "original_form": "오 먹는거랑 안먹는거랑뭔가 기분탓인지 달라요",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100012477.1.8",
          "form": "저 안구건조증이라서 ㅎㅎ",
          "original_form": "저 안구건조증이라서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100012477.1.9",
          "form": "인공눈물도 자주 쓰시겠어요",
          "original_form": "인공눈물도 자주 쓰시겠어요",
          "speaker_id": "1",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100012477.1.10",
          "form": "네네 ㅎㅎ 일부러 안과진료받고 싸게 샀어요",
          "original_form": "네네 ㅎㅎ 일부러 안과진료받고 싸게 샀어요",
          "speaker_id": "2",
          "time": "20210825 14:40"
        },
        {
          "id": "MDRW2100012477.1.11",
          "form": "그냥 사는것보다 절반이나 할인되었어요",
          "original_form": "그냥 사는것보다 절반이나 할인되었어요",
          "speaker_id": "2",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100012477.1.12",
          "form": "그렇게 사는 게 확실히 싸더라구요",
          "original_form": "그렇게 사는 게 확실히 싸더라구요",
          "speaker_id": "1",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100012477.1.13",
          "form": "네 진단명 안구건조 ㅎㅎ 아르곤님은 유산균 드시면 효과좋은거같나요",
          "original_form": "네 진단명 안구건조 ㅎㅎ 아르곤님은 유산균 드시면 효과좋은거같나요",
          "speaker_id": "2",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100012477.1.14",
          "form": "먹을 땐 잘 모르겠는데 한동안 안 먹으면 티나는 것 같더라구요 ㅋㅋ",
          "original_form": "먹을 땐 잘 모르겠는데 한동안 안 먹으면 티나는 것 같더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100012477.1.15",
          "form": "아 그게 효과가 그렇게 나나보네요",
          "original_form": "아 그게 효과가 그렇게 나나보네요",
          "speaker_id": "2",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100012477.1.16",
          "form": "저는 비타민사과맛 먹어요ㅋㅋ",
          "original_form": "저는 비타민사과맛 먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:43"
        },
        {
          "id": "MDRW2100012477.1.17",
          "form": "젤리형인가요?",
          "original_form": "젤리형인가요?",
          "speaker_id": "1",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100012477.1.18",
          "form": "아니요 씹어먹는건데요",
          "original_form": "아니요 씹어먹는건데요",
          "speaker_id": "2",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100012477.1.19",
          "form": "가루뭉친거 머라하징",
          "original_form": "가루뭉친거 머라하징",
          "speaker_id": "2",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100012477.1.20",
          "form": "캔디형?",
          "original_form": "캔디형?",
          "speaker_id": "1",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100012477.1.21",
          "form": "전 고려은단 비타민씨. 틴케이스가 예뻐서 샀어요ㅋㅋ 미키마우스 콜라보",
          "original_form": "전 고려은단 비타민씨. 틴케이스가 예뻐서 샀어요ㅋㅋ 미키마우스 콜라보",
          "speaker_id": "1",
