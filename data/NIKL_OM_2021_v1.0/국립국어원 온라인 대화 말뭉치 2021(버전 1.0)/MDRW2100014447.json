      "utterance": [
        {
          "id": "MDRW2100014447.1.1",
          "form": "학교다닐때 공부는 좀 했구?",
          "original_form": "학교다닐때 공부는 좀 했구?",
          "speaker_id": "1",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014447.1.2",
          "form": "어 뭐 못하진않았어",
          "original_form": "어 뭐 못하진않았어",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014447.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014447.1.4",
          "form": "공부 못했던애들의 전형적인 대답인가...",
          "original_form": "공부 못했던애들의 전형적인 대답인가...",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014447.1.5",
          "form": "어떤 과목이 젤 좋았어?",
          "original_form": "어떤 과목이 젤 좋았어?",
          "speaker_id": "1",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014447.1.6",
          "form": "미술? 음악?",
          "original_form": "미술? 음악?",
          "speaker_id": "1",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014447.1.7",
          "form": "아 나는 특별히 좋아했던 과목은 없는데 그나마 생물?",
          "original_form": "아 나는 특별히 좋아했던 과목은 없는데 그나마 생물?",
          "speaker_id": "2",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100014447.1.8",
          "form": "그래서 난 생물쪽으로 가게될줄알았어",
          "original_form": "그래서 난 생물쪽으로 가게될줄알았어",
          "speaker_id": "2",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014447.1.9",
          "form": "완전 상관없더라 ㅋㅋ",
          "original_form": "완전 상관없더라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014447.1.10",
          "form": "넌 무슨과목좋아했어?",
          "original_form": "넌 무슨과목좋아했어?",
          "speaker_id": "2",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014447.1.11",
          "form": "나도 다윈 좋아해.. ㅋㅋㅋ 진화론..",
          "original_form": "나도 다윈 좋아해.. ㅋㅋㅋ 진화론..",
          "speaker_id": "1",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014447.1.12",
          "form": "난.. 다 안좋아햇는데.. 공부는 잘했어.. ㅋㅋㅋ",
          "original_form": "난.. 다 안좋아햇는데.. 공부는 잘했어.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014447.1.13",
          "form": "오... 믿어줄게",
          "original_form": "오... 믿어줄게",
          "speaker_id": "2",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014447.1.14",
          "form": "수학도 잘했어?",
          "original_form": "수학도 잘했어?",
          "speaker_id": "2",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014447.1.15",
          "form": "수학잘해야 진짜 공부잘하는건뎅",
          "original_form": "수학잘해야 진짜 공부잘하는건뎅",
          "speaker_id": "2",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014447.1.16",
          "form": "그런 편견을 버려..",
          "original_form": "그런 편견을 버려..",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014447.1.17",
          "form": "그런 전인교육.. 전근대적이야.. ㅋㅋㅋㅋ",
          "original_form": "그런 전인교육.. 전근대적이야.. ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014447.1.18",
          "form": "너도 수학 싫어했겠네? 근데 생물 이과아님?",
          "original_form": "너도 수학 싫어했겠네? 근데 생물 이과아님?",
          "speaker_id": "1",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014447.1.19",
          "form": "웅 이과였지만 수학싫어했음ㅎㅎ...",
          "original_form": "웅 이과였지만 수학싫어했음ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014447.1.20",
          "form": "근데 이과생도 수학못할수도있지 ㅠㅠㅠ 다 잘해야한다는 편견을 버려!",
          "original_form": "근데 이과생도 수학못할수도있지 ㅠㅠㅠ 다 잘해야한다는 편견을 버려!",
          "speaker_id": "2",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014447.1.21",
          "form": "학교는 졸업했어?",
          "original_form": "학교는 졸업했어?",
          "speaker_id": "1",
          "time": "20210825 19:25"
        },
        {
          "id": "MDRW2100014447.1.22",
          "form": "아니 왜 때린데 또 때리는거얔ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니 왜 때린데 또 때리는거얔ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014447.1.23",
          "form": "겁내웃기네",
          "original_form": "겁내웃기네",
          "speaker_id": "2",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014447.1.24",
          "form": "웅 졸업했고 직장인이양\r\n학교 전공이랑 상관없이 먹고살아 ㅋㅋㅋㅋ",
          "original_form": "웅 졸업했고 직장인이양\r\n학교 전공이랑 상관없이 먹고살아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:34"
        },
        {
          "id": "MDRW2100014447.1.25",
          "form": "후회안돼? ㅋㅋㅋ 학생때 성싱하게 살껄하고..",
          "original_form": "후회안돼? ㅋㅋㅋ 학생때 성싱하게 살껄하고..",
          "speaker_id": "1",
          "time": "20210825 19:34"
        },
        {
          "id": "MDRW2100014447.1.26",
          "form": "후회는안해",
          "original_form": "후회는안해",
          "speaker_id": "2",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014447.1.27",
          "form": "성실하긴 했거든...",
          "original_form": "성실하긴 했거든...",
          "speaker_id": "2",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014447.1.28",
          "form": "이게 더 슬픈가",
          "original_form": "이게 더 슬픈가",
          "speaker_id": "2",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014447.1.29",
          "form": "근데 나 아주 못하진않았다 ㅡㅡ",
          "original_form": "근데 나 아주 못하진않았다 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014447.1.30",
          "form": "ㅋㅋㅋ 네.. 알.겠.습.니.다. \r\n일관된 진술 잘 들었습니다.. ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 네.. 알.겠.습.니.다. \r\n일관된 진술 잘 들었습니다.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:37"
        }
      ]
