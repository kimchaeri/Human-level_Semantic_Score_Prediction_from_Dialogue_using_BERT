      "utterance": [
        {
          "id": "MDRW2100031066.1.1",
          "form": "아무 때나 괜찮습니다",
          "original_form": "아무 때나 괜찮습니다",
          "speaker_id": "2",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100031066.1.2",
          "form": "저도 봤어요",
          "original_form": "저도 봤어요",
          "speaker_id": "2",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100031066.1.3",
          "form": "디엘 건설 완전 괜찮은 거 같아요",
          "original_form": "디엘 건설 완전 괜찮은 거 같아요",
          "speaker_id": "1",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100031066.1.4",
          "form": "괜찮아 보이더라구요",
          "original_form": "괜찮아 보이더라구요",
          "speaker_id": "2",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100031066.1.5",
          "form": "일기 옛 도 좋구요",
          "original_form": "일기 옛 도 좋구요",
          "speaker_id": "1",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100031066.1.6",
          "form": "책으로 공부할게 많네요",
          "original_form": "책으로 공부할게 많네요",
          "speaker_id": "2",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100031066.1.7",
          "form": "일기획",
          "original_form": "일기획",
          "speaker_id": "1",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100031066.1.8",
          "form": "재무제표 보는 거라 하던가 차표 차트 보는 것도 해야 해요",
          "original_form": "재무제표 보는 거라 하던가 차표 차트 보는 것도 해야 해요",
          "speaker_id": "2",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100031066.1.9",
          "form": "제일",
          "original_form": "제일",
          "speaker_id": "2",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100031066.1.10",
          "form": "맞아요 디엘 건설은 장기투자하기 좋겠더라구요",
          "original_form": "맞아요 디엘 건설은 장기투자하기 좋겠더라구요",
          "speaker_id": "1",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100031066.1.11",
          "form": "언제 다 읽을 수 있을까요",
          "original_form": "언제 다 읽을 수 있을까요",
          "speaker_id": "2",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100031066.1.12",
          "form": "식품 주가 단타로는 괜찮은 거 같은데 어떠세요",
          "original_form": "식품 주가 단타로는 괜찮은 거 같은데 어떠세요",
          "speaker_id": "1",
          "time": "20210904 16:08"
        },
        {
          "id": "MDRW2100031066.1.13",
          "form": "책을 언제 읽을지 모르겠어요",
          "original_form": "책을 언제 읽을지 모르겠어요",
          "speaker_id": "2",
          "time": "20210904 16:08"
        },
        {
          "id": "MDRW2100031066.1.14",
          "form": "읽을 책은 쌓여가고 시간이 별로 없네요",
          "original_form": "읽을 책은 쌓여가고 시간이 별로 없네요",
          "speaker_id": "2",
          "time": "20210904 16:08"
        },
        {
          "id": "MDRW2100031066.1.15",
          "form": "조금하다가 저는 책부터 책 볼게요",
          "original_form": "조금하다가 저는 책부터 책 볼게요",
          "speaker_id": "1",
          "time": "20210904 16:08"
        },
        {
          "id": "MDRW2100031066.1.16",
          "form": "너무 무지런하셔서 그래요",
          "original_form": "너무 무지런하셔서 그래요",
          "speaker_id": "1",
          "time": "20210904 16:08"
        },
        {
          "id": "MDRW2100031066.1.17",
          "form": "밀린 담겨 있는 팩 두권 부터 읽으면 되나요",
          "original_form": "밀린 담겨 있는 팩 두권 부터 읽으면 되나요",
          "speaker_id": "1",
          "time": "20210904 16:09"
        }
