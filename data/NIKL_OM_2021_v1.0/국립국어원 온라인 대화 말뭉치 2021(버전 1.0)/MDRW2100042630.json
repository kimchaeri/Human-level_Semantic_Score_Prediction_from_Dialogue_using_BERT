      "utterance": [
        {
          "id": "MDRW2100042630.1.1",
          "form": "호이호이라니 ㅋㅋㅋ",
          "original_form": "호이호이라니 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:56"
        },
        {
          "id": "MDRW2100042630.1.2",
          "form": "ㅋㅋㅋ아니그걸몰랐다니 내가 더 당황이디",
          "original_form": "ㅋㅋㅋ아니그걸몰랐다니 내가 더 당황이디",
          "speaker_id": "2",
          "time": "20210906 23:57"
        },
        {
          "id": "MDRW2100042630.1.3",
          "form": "겉에바삭해지니까 꼭 해먹어봐 ㅋㅋ",
          "original_form": "겉에바삭해지니까 꼭 해먹어봐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:57"
        },
        {
          "id": "MDRW2100042630.1.4",
          "form": "ㅋㅋㅋㅋ 그냥 호떡이라 써있지않아?",
          "original_form": "ㅋㅋㅋㅋ 그냥 호떡이라 써있지않아?",
          "speaker_id": "1",
          "time": "20210906 23:57"
        },
        {
          "id": "MDRW2100042630.1.5",
          "form": "저번에 너가 말한 갈로리바란스같은거 뭐였지",
          "original_form": "저번에 너가 말한 갈로리바란스같은거 뭐였지",
          "speaker_id": "2",
          "time": "20210906 23:57"
        },
        {
          "id": "MDRW2100042630.1.6",
          "form": "추억의 옛날 꿀호떡이라 써있는데? ㅋㅋㅋ",
          "original_form": "추억의 옛날 꿀호떡이라 써있는데? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:58"
        },
        {
          "id": "MDRW2100042630.1.7",
          "form": "뭘까그게",
          "original_form": "뭘까그게",
          "speaker_id": "1",
          "time": "20210906 23:58"
        },
        {
          "id": "MDRW2100042630.1.8",
          "form": "칼로리바란스가 아니라 칼바 같은거라고?",
          "original_form": "칼로리바란스가 아니라 칼바 같은거라고?",
          "speaker_id": "1",
          "time": "20210906 23:59"
        },
        {
          "id": "MDRW2100042630.1.9",
          "form": "저번에 말한거 이름이 뭐지ㅠ",
          "original_form": "저번에 말한거 이름이 뭐지ㅠ",
          "speaker_id": "2",
          "time": "20210906 23:59"
        },
        {
          "id": "MDRW2100042630.1.10",
          "form": "칼로리바란스같은거ㅠ",
          "original_form": "칼로리바란스같은거ㅠ",
          "speaker_id": "2",
          "time": "20210906 23:59"
        },
        {
          "id": "MDRW2100042630.1.11",
          "form": "좀더 상황을 얘기해봐 ㅋㅋㅋㅋㅋ언제얘기한거지",
          "original_form": "좀더 상황을 얘기해봐 ㅋㅋㅋㅋㅋ언제얘기한거지",
          "speaker_id": "1",
          "time": "20210906 23:59"
        },
        {
          "id": "MDRW2100042630.1.12",
          "form": "맛이무슨맛이래 ㅋㅋㅋㅋ",
          "original_form": "맛이무슨맛이래 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:59"
        },
        {
          "id": "MDRW2100042630.1.13",
          "form": "딱딱하다고한거",
          "original_form": "딱딱하다고한거",
          "speaker_id": "2",
          "time": "20210907 00:00"
        },
        {
          "id": "MDRW2100042630.1.14",
          "form": "쿠팡에서 사먹었다고",
          "original_form": "쿠팡에서 사먹었다고",
          "speaker_id": "2",
          "time": "20210907 00:00"
        },
        {
          "id": "MDRW2100042630.1.15",
          "form": "뭐지 ㅋㅋㅋㅋㅋ쿠팡에서?",
          "original_form": "뭐지 ㅋㅋㅋㅋㅋ쿠팡에서?",
          "speaker_id": "1",
          "time": "20210907 00:00"
        },
        {
          "id": "MDRW2100042630.1.16",
          "form": "사실더 단단한걸 찾으면 말해주라고",
          "original_form": "사실더 단단한걸 찾으면 말해주라고",
          "speaker_id": "2",
          "time": "20210907 00:00"
        },
        {
          "id": "MDRW2100042630.1.17",
          "form": "아 쿠키말하는거아니가버터쿠키",
          "original_form": "아 쿠키말하는거아니가버터쿠키",
          "speaker_id": "1",
          "time": "20210907 00:01"
        },
        {
          "id": "MDRW2100042630.1.18",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 00:01"
        },
        {
          "id": "MDRW2100042630.1.19",
          "form": "워커스 쇼트브래드",
          "original_form": "워커스 쇼트브래드",
          "speaker_id": "1",
          "time": "20210907 00:01"
        },
        {
          "id": "MDRW2100042630.1.20",
          "form": "트라이앵글쿠키도같은거야?",
          "original_form": "트라이앵글쿠키도같은거야?",
          "speaker_id": "2",
          "time": "20210907 00:09"
        },
        {
          "id": "MDRW2100042630.1.21",
          "form": "어어 모양만다른러",
          "original_form": "어어 모양만다른러",
          "speaker_id": "1",
          "time": "20210907 00:12"
        },
        {
          "id": "MDRW2100042630.1.22",
          "form": "하이랜더스만 설탕 겉에 더 바른거",
          "original_form": "하이랜더스만 설탕 겉에 더 바른거",
          "speaker_id": "1",
          "time": "20210907 00:12"
        },
        {
          "id": "MDRW2100042630.1.23",
          "form": "오 로켓와우잇어서 삿음",
          "original_form": "오 로켓와우잇어서 삿음",
          "speaker_id": "2",
          "time": "20210907 00:12"
        },
        {
          "id": "MDRW2100042630.1.24",
          "form": "모양이다르니 식감이 미세한 차이가 있긴함ㅋㅋ",
          "original_form": "모양이다르니 식감이 미세한 차이가 있긴함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:13"
        },
        {
          "id": "MDRW2100042630.1.25",
          "form": "ㅜㅜㅜㅜ내일새벽도찬이래ㅜㅜ",
          "original_form": "ㅜㅜㅜㅜ내일새벽도찬이래ㅜㅜ",
