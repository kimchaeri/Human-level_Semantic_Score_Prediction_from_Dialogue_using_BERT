      "utterance": [
        {
          "id": "MDRW2100033513.1.1",
          "form": "이제 춥네요",
          "original_form": "이제 춥네요",
          "speaker_id": "2",
          "time": "20210824 10:02"
        },
        {
          "id": "MDRW2100033513.1.2",
          "form": "네 겉옷 챙겼어요 날이 흐려서..",
          "original_form": "네 겉옷 챙겼어요 날이 흐려서..",
          "speaker_id": "1",
          "time": "20210824 10:02"
        },
        {
          "id": "MDRW2100033513.1.3",
          "form": "옷을 깜박했네요",
          "original_form": "옷을 깜박했네요",
          "speaker_id": "2",
          "time": "20210824 10:07"
        },
        {
          "id": "MDRW2100033513.1.4",
          "form": "추울거같아서ㅎㅎ 이젠 챙겨야할거같아요",
          "original_form": "추울거같아서ㅎㅎ 이젠 챙겨야할거같아요",
          "speaker_id": "1",
          "time": "20210824 10:09"
        },
        {
          "id": "MDRW2100033513.1.5",
          "form": "저는 목이 약한데 목수건을 안챙긴게 치명적이네요 ㅜ",
          "original_form": "저는 목이 약한데 목수건을 안챙긴게 치명적이네요 ㅜ",
          "speaker_id": "2",
          "time": "20210824 10:11"
        },
        {
          "id": "MDRW2100033513.1.6",
          "form": "아 저도 그래요 스카프라도 꼭 해야하는...",
          "original_form": "아 저도 그래요 스카프라도 꼭 해야하는...",
          "speaker_id": "1",
          "time": "20210824 10:12"
        },
        {
          "id": "MDRW2100033513.1.7",
          "form": "사무실에 머좀 있는가 뒤져봐야죠ㅋㅋ",
          "original_form": "사무실에 머좀 있는가 뒤져봐야죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100033513.1.8",
          "form": "감기걸리기 딱 좋은 날씨ㅠㅠㅎㅎ",
          "original_form": "감기걸리기 딱 좋은 날씨ㅠㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 10:14"
        },
        {
          "id": "MDRW2100033513.1.9",
          "form": "이제 비 그쳤으려나요",
          "original_form": "이제 비 그쳤으려나요",
          "speaker_id": "2",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100033513.1.10",
          "form": "태풍지나갔나?",
          "original_form": "태풍지나갔나?",
          "speaker_id": "2",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100033513.1.11",
          "form": "스쳐지나간거같은데 잘은 모르겠네요ㅎㅎ",
          "original_form": "스쳐지나간거같은데 잘은 모르겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 10:16"
        },
        {
          "id": "MDRW2100033513.1.12",
          "form": "이제 추워지려나 더워지려나",
          "original_form": "이제 추워지려나 더워지려나",
          "speaker_id": "2",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100033513.1.13",
          "form": "비오면 푹푹 찌던데  여름 지나간거 같아서 또 다를수도ㅎㅎ",
          "original_form": "비오면 푹푹 찌던데  여름 지나간거 같아서 또 다를수도ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 10:18"
        },
        {
          "id": "MDRW2100033513.1.14",
          "form": "이상기후라서 늘 예상대로 안가더래요 요샌",
          "original_form": "이상기후라서 늘 예상대로 안가더래요 요샌",
          "speaker_id": "2",
          "time": "20210824 10:26"
        },
        {
          "id": "MDRW2100033513.1.15",
          "form": "그게 참 그렇더라구요",
          "original_form": "그게 참 그렇더라구요",
          "speaker_id": "1",
          "time": "20210824 10:28"
        }
      ]
