      "utterance": [
        {
          "id": "MDRW2100045152.1.1",
          "form": "그럼 너 그건기억나냐 ㅋㅋㅋ아이스크림",
          "original_form": "그럼 너 그건기억나냐 ㅋㅋㅋ아이스크림",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045152.1.2",
          "form": "아이스크림?",
          "original_form": "아이스크림?",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045152.1.3",
          "form": "콜드스톤! ㅋㅋ",
          "original_form": "콜드스톤! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045152.1.4",
          "form": "기억남! 철판!",
          "original_form": "기억남! 철판!",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045152.1.5",
          "form": "아임백",
          "original_form": "아임백",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045152.1.6",
          "form": "우리거기 꽤 자주갔었어 ㅋㅋㅋㅋ 막 브라우니 얹어주고",
          "original_form": "우리거기 꽤 자주갔었어 ㅋㅋㅋㅋ 막 브라우니 얹어주고",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045152.1.7",
          "form": "자주갔었어?",
          "original_form": "자주갔었어?",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045152.1.8",
          "form": "꽤나 가지않았어? ㅋㅋㅋㅋ",
          "original_form": "꽤나 가지않았어? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045152.1.9",
          "form": "근데 거기 비쌌잖아 ㅜ",
          "original_form": "근데 거기 비쌌잖아 ㅜ",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045152.1.10",
          "form": "그거 시내에도 있던데!",
          "original_form": "그거 시내에도 있던데!",
          "speaker_id": "2",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045152.1.11",
          "form": "엥 어디에??",
          "original_form": "엥 어디에??",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045152.1.12",
          "form": "지금 있다고??",
          "original_form": "지금 있다고??",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045152.1.13",
          "form": "학교앞에거는 금방없어졌잖아",
          "original_form": "학교앞에거는 금방없어졌잖아",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045152.1.14",
          "form": "있던데 철판아이스크림!",
          "original_form": "있던데 철판아이스크림!",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045152.1.15",
          "form": "콜드스톤아닌거아니가?",
          "original_form": "콜드스톤아닌거아니가?",
          "speaker_id": "1",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045152.1.16",
          "form": "아니야! 맞을껄???",
          "original_form": "아니야! 맞을껄???",
          "speaker_id": "2",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045152.1.17",
          "form": "그거 우리나라에서 철수했었잖아 ㅋㅋ",
          "original_form": "그거 우리나라에서 철수했었잖아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045152.1.18",
          "form": "아닌가보다...",
          "original_form": "아닌가보다...",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045152.1.19",
          "form": "이름이 다르네....",
          "original_form": "이름이 다르네....",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045152.1.20",
          "form": "거짓말쟁이..... ㅋㅋㅋㅋㅋ콜드스톤 서울에있을걸?",
          "original_form": "거짓말쟁이..... ㅋㅋㅋㅋㅋ콜드스톤 서울에있을걸?",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045152.1.21",
          "form": "곤트란쉐리에도 한번가보고싶었는데...",
          "original_form": "곤트란쉐리에도 한번가보고싶었는데...",
          "speaker_id": "1",
          "time": "20210908 01:36"
        }
      ]
