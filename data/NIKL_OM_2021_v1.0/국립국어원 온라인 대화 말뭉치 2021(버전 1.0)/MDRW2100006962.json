      "utterance": [
        {
          "id": "MDRW2100006962.1.1",
          "form": "감사합니다 ㅎㅎ",
          "original_form": "감사합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006962.1.2",
          "form": "해삼 맛있는데,, 꼬독꼬독",
          "original_form": "해삼 맛있는데,, 꼬독꼬독",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006962.1.3",
          "form": "맞아요 꼬독꼬독 씹는 맛이 있죠",
          "original_form": "맞아요 꼬독꼬독 씹는 맛이 있죠",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006962.1.4",
          "form": "근데 요즘 해삼 너무 비싸져서 ㅜㅜ",
          "original_form": "근데 요즘 해삼 너무 비싸져서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006962.1.5",
          "form": "슬퍼요ㅠㅠ",
          "original_form": "슬퍼요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006962.1.6",
          "form": "회 좋아하세요?",
          "original_form": "회 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006962.1.7",
          "form": "넹! 회 짱 좋아요!",
          "original_form": "넹! 회 짱 좋아요!",
          "speaker_id": "2",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006962.1.8",
          "form": "헉 저두용!!",
          "original_form": "헉 저두용!!",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006962.1.9",
          "form": "민어회 드셔보셨나요???",
          "original_form": "민어회 드셔보셨나요???",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006962.1.10",
          "form": "이거 요즘 넘 먹어보고 싶어요",
          "original_form": "이거 요즘 넘 먹어보고 싶어요",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006962.1.11",
          "form": "다들 극찬해서 ㅜ",
          "original_form": "다들 극찬해서 ㅜ",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006962.1.12",
          "form": "오오 엄청 맛있나보네요!",
          "original_form": "오오 엄청 맛있나보네요!",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006962.1.13",
          "form": "네 요즘이 제철인 것 같더라구요",
          "original_form": "네 요즘이 제철인 것 같더라구요",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006962.1.14",
          "form": "근데 광어 이런것보다 비싸고 주변에서 안 팔아서 아직 못먹어봤어요 ㅠㅠ",
          "original_form": "근데 광어 이런것보다 비싸고 주변에서 안 팔아서 아직 못먹어봤어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006962.1.15",
          "form": "맘 먹고 먹어야 하는 음식이네요 ㅠㅠ",
          "original_form": "맘 먹고 먹어야 하는 음식이네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006962.1.16",
          "form": "네ㅜㅜ 혼자 먹기도 어려운 것 같아요..",
          "original_form": "네ㅜㅜ 혼자 먹기도 어려운 것 같아요..",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006962.1.17",
          "form": "같이 갈 사람을 구해봐야겠어요",
          "original_form": "같이 갈 사람을 구해봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006962.1.18",
          "form": "어떤 회 좋아하세요?",
          "original_form": "어떤 회 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006962.1.19",
          "form": "전 흰살생선도 맛있구 연어 같은 것두 좋구여",
          "original_form": "전 흰살생선도 맛있구 연어 같은 것두 좋구여",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006962.1.20",
          "form": "새우장 게장 같은 것도 좋아요",
          "original_form": "새우장 게장 같은 것도 좋아요",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006962.1.21",
          "form": "맞아요 연어도 맛있죠",
          "original_form": "맞아요 연어도 맛있죠",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006962.1.22",
          "form": "무한리필 가서 배터지게 먹고싶으면서도",
          "original_form": "무한리필 가서 배터지게 먹고싶으면서도",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006962.1.23",
          "form": "막상 가면 금방 물려서 항상 후회하는 ㅜㅜ",
          "original_form": "막상 가면 금방 물려서 항상 후회하는 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006962.1.24",
          "form": "연어 좀 그런 게 있죠 ㅠㅠ 느끼해서 많이는 못 먹는 ㅠㅠ",
          "original_form": "연어 좀 그런 게 있죠 ㅠㅠ 느끼해서 많이는 못 먹는 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006962.1.25",
          "form": "네ㅜㅜ 근데 또 두툼한 뱃살 보면 많이 먹고싶어지고..",
          "original_form": "네ㅜㅜ 근데 또 두툼한 뱃살 보면 많이 먹고싶어지고..",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006962.1.26",
          "form": "저도 게장도 완전 좋아해요",
          "original_form": "저도 게장도 완전 좋아해요",
          "speaker_id": "1",
          "time": "20210822 00:19"
        }
      ]
    }
  ]
