      "utterance": [
        {
          "id": "MDRW2100035102.1.1",
          "form": "괴물을 물리쳐요 ㅋㅋㅋㅋ",
          "original_form": "괴물을 물리쳐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100035102.1.2",
          "form": "지나갔던 대사속에",
          "original_form": "지나갔던 대사속에",
          "speaker_id": "1",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100035102.1.3",
          "form": "막 아이들끼리 아빠 찾으러 가자고 했던것같은데",
          "original_form": "막 아이들끼리 아빠 찾으러 가자고 했던것같은데",
          "speaker_id": "1",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100035102.1.4",
          "form": "전 사실 영화를 먆이보진않아서ㅠ",
          "original_form": "전 사실 영화를 먆이보진않아서ㅠ",
          "speaker_id": "2",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100035102.1.5",
          "form": "아빠는 만나요?",
          "original_form": "아빠는 만나요?",
          "speaker_id": "1",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100035102.1.6",
          "form": "네 아빠는 죽었어요ㅠ",
          "original_form": "네 아빠는 죽었어요ㅠ",
          "speaker_id": "2",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100035102.1.7",
          "form": "헐ㅠㅠㅠㅠ",
          "original_form": "헐ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100035102.1.8",
          "form": "아이고...",
          "original_form": "아이고...",
          "speaker_id": "1",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100035102.1.9",
          "form": "그 숨겨줬던 아저씨는",
          "original_form": "그 숨겨줬던 아저씨는",
          "speaker_id": "1",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100035102.1.10",
          "form": "어떻게 되나용?",
          "original_form": "어떻게 되나용?",
          "speaker_id": "1",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100035102.1.11",
          "form": "맞나 ㅋㅋㅋ",
          "original_form": "맞나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100035102.1.12",
          "form": "그아저씨랑",
          "original_form": "그아저씨랑",
          "speaker_id": "2",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100035102.1.13",
          "form": "저 영화 결말보고 보는거 좋아해서ㅋㅋㅋㅋ",
          "original_form": "저 영화 결말보고 보는거 좋아해서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100035102.1.14",
          "form": "아이가 어디로 가자해서 혼자떠나다가",
          "original_form": "아이가 어디로 가자해서 혼자떠나다가",
          "speaker_id": "2",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100035102.1.15",
          "form": "아ㅠㅠ 혼자가면 위험한데",
          "original_form": "아ㅠㅠ 혼자가면 위험한데",
          "speaker_id": "1",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100035102.1.16",
          "form": "그아저씨랑 같이 새로운 섬? 같은곳을 찾은거에요! 아이가말한곳으로 가다보니",
          "original_form": "그아저씨랑 같이 새로운 섬? 같은곳을 찾은거에요! 아이가말한곳으로 가다보니",
          "speaker_id": "2",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100035102.1.17",
          "form": "마을 꾸리고 살고잇는곳을 찾은거죠!",
          "original_form": "마을 꾸리고 살고잇는곳을 찾은거죠!",
          "speaker_id": "2",
          "time": "20210829 02:44"
        },
        {
          "id": "MDRW2100035102.1.18",
          "form": "헐ㅋㅋㅋㅋ 아저씨 꽤 비중이 큰 역할이었나봐요",
          "original_form": "헐ㅋㅋㅋㅋ 아저씨 꽤 비중이 큰 역할이었나봐요",
          "speaker_id": "1",
          "time": "20210829 02:44"
        },
        {
          "id": "MDRW2100035102.1.19",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210829 02:44"
        },
        {
          "id": "MDRW2100035102.1.20",
          "form": "다행이다",
          "original_form": "다행이다",
          "speaker_id": "1",
          "time": "20210829 02:44"
        },
        {
          "id": "MDRW2100035102.1.21",
          "form": "아저씨도 마지막까지 살아남나보네용ㅋㅋㅋ",
          "original_form": "아저씨도 마지막까지 살아남나보네용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:44"
        },
        {
          "id": "MDRW2100035102.1.22",
          "form": "그 괴물을 물리칠 소리가잇는데 그걸 스피커로해서 라디오로 전송하면 어디든 괴물을 물리칠수잇다 생각한거같아요!",
          "original_form": "그 괴물을 물리칠 소리가잇는데 그걸 스피커로해서 라디오로 전송하면 어디든 괴물을 물리칠수잇다 생각한거같아요!",
          "speaker_id": "2",
          "time": "20210829 02:44"
        },
        {
          "id": "MDRW2100035102.1.23",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100035102.1.24",
          "form": "똑똑하다",
          "original_form": "똑똑하다",
          "speaker_id": "1",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100035102.1.25",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100035102.1.26",
          "form": "그아저씨도 크게 다치던지 죽던지 할꺼에요 마지막에",
          "original_form": "그아저씨도 크게 다치던지 죽던지 할꺼에요 마지막에",
          "speaker_id": "2",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100035102.1.27",
          "form": "그소리가 막 괴롭거든요 !",
          "original_form": "그소리가 막 괴롭거든요 !",
          "speaker_id": "2",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100035102.1.28",
          "form": "저 이거 다시 봐야겠어욬ㅋㅋㅋ",
          "original_form": "저 이거 다시 봐야겠어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:45"
