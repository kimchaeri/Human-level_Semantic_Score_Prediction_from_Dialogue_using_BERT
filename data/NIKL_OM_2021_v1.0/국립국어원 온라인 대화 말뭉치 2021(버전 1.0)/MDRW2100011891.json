      "utterance": [
        {
          "id": "MDRW2100011891.1.1",
          "form": "좋아하는 음식 있으신가요?",
          "original_form": "좋아하는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011891.1.2",
          "form": "저는 요즘 곱창 먹고싶어요 님은요?",
          "original_form": "저는 요즘 곱창 먹고싶어요 님은요?",
          "speaker_id": "2",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100011891.1.3",
          "form": "전 완전 단 케익요ㅠ",
          "original_form": "전 완전 단 케익요ㅠ",
          "speaker_id": "1",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100011891.1.4",
          "form": "다이어트중이신가요?",
          "original_form": "다이어트중이신가요?",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011891.1.5",
          "form": "마음만요ㅠ",
          "original_form": "마음만요ㅠ",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011891.1.6",
          "form": "전 케익은 당근케익 좋아합니다~",
          "original_form": "전 케익은 당근케익 좋아합니다~",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011891.1.7",
          "form": "오늘 저녁은 뭐 드세요?",
          "original_form": "오늘 저녁은 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011891.1.8",
          "form": "비빔밥요",
          "original_form": "비빔밥요",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011891.1.9",
          "form": "님은여?",
          "original_form": "님은여?",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011891.1.10",
          "form": "전 뭐 먹을지 고민이요",
          "original_form": "전 뭐 먹을지 고민이요",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011891.1.11",
          "form": "비빔밥 만들어서 드세요?",
          "original_form": "비빔밥 만들어서 드세요?",
          "speaker_id": "2",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011891.1.12",
          "form": "네 냉장고털기",
          "original_form": "네 냉장고털기",
          "speaker_id": "1",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011891.1.13",
          "form": "우와 맛나겠어요~",
          "original_form": "우와 맛나겠어요~",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011891.1.14",
          "form": "네맛나요",
          "original_form": "네맛나요",
          "speaker_id": "1",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100011891.1.15",
          "form": "저도 지금 막 저녁 먹었어요~",
          "original_form": "저도 지금 막 저녁 먹었어요~",
          "speaker_id": "2",
          "time": "20210824 18:59"
        },
        {
          "id": "MDRW2100011891.1.16",
          "form": "아침은 드세요?",
          "original_form": "아침은 드세요?",
          "speaker_id": "2",
          "time": "20210824 18:59"
        },
        {
          "id": "MDRW2100011891.1.17",
          "form": "네 꼭먹어요 ㅎ",
          "original_form": "네 꼭먹어요 ㅎ",
