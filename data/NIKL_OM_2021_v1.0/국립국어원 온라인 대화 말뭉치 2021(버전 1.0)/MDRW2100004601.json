      "utterance": [
        {
          "id": "MDRW2100004601.1.1",
          "form": "혹시 해외여행 좋아하세요?",
          "original_form": "혹시 해외여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004601.1.2",
          "form": "해외여행 보다는 국내여행 좋아해요!",
          "original_form": "해외여행 보다는 국내여행 좋아해요!",
          "speaker_id": "2",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004601.1.3",
          "form": "해외여행 좋아하세요?",
          "original_form": "해외여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004601.1.4",
          "form": "해외여행을 못가봐서 한번 가보고싶어요",
          "original_form": "해외여행을 못가봐서 한번 가보고싶어요",
          "speaker_id": "1",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004601.1.5",
          "form": "국내 어디가는거 좋아하세요?",
          "original_form": "국내 어디가는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004601.1.6",
          "form": "국내는 그때 그때 다른 거 같아요",
          "original_form": "국내는 그때 그때 다른 거 같아요",
          "speaker_id": "2",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004601.1.7",
          "form": "1년에 2번은 꼭 여행가는 편인데",
          "original_form": "1년에 2번은 꼭 여행가는 편인데",
          "speaker_id": "2",
          "time": "20210821 17:31"
        },
        {
          "id": "MDRW2100004601.1.8",
          "form": "이번에는 4단계여서 못 갔어요ㅠㅜ",
          "original_form": "이번에는 4단계여서 못 갔어요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 17:31"
        },
        {
          "id": "MDRW2100004601.1.9",
          "form": "최근에 여행간 곳은 경주였어요",
          "original_form": "최근에 여행간 곳은 경주였어요",
          "speaker_id": "2",
          "time": "20210821 17:31"
        },
        {
          "id": "MDRW2100004601.1.10",
          "form": "첨성대 보셨어요?",
          "original_form": "첨성대 보셨어요?",
          "speaker_id": "1",
          "time": "20210821 17:43"
        },
        {
          "id": "MDRW2100004601.1.11",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004601.1.12",
          "form": "첨성대는 언제 봐도 신기하고 흥미로운거 같아요",
          "original_form": "첨성대는 언제 봐도 신기하고 흥미로운거 같아요",
          "speaker_id": "2",
          "time": "20210821 17:45"
        },
        {
          "id": "MDRW2100004601.1.13",
          "form": "경주 한옥마을도 가보셨어요?",
          "original_form": "경주 한옥마을도 가보셨어요?",
          "speaker_id": "1",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004601.1.14",
          "form": "한옥마을은 안 가봤어요ㅠㅜ",
          "original_form": "한옥마을은 안 가봤어요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004601.1.15",
          "form": "우리나라는 한옥 많은곳이 개인적으로 좋은거같아요",
          "original_form": "우리나라는 한옥 많은곳이 개인적으로 좋은거같아요",
          "speaker_id": "1",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100004601.1.16",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004601.1.17",
          "form": "한옥체험은 해봤는데",
          "original_form": "한옥체험은 해봤는데",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004601.1.18",
          "form": "낮, 밤, 새벽에 따라 분위기가 달라서",
          "original_form": "낮, 밤, 새벽에 따라 분위기가 달라서",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004601.1.19",
          "form": "더 좋은거 같아요",
          "original_form": "더 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004601.1.20",
          "form": "밤에가면 좋은곳은 어디있어요?",
          "original_form": "밤에가면 좋은곳은 어디있어요?",
          "speaker_id": "1",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004601.1.21",
          "form": "저는 월정교가 예뻤던거 같아요",
          "original_form": "저는 월정교가 예뻤던거 같아요",
          "speaker_id": "2",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004601.1.22",
          "form": "동궁과 월지도 있지만",
          "original_form": "동궁과 월지도 있지만",
          "speaker_id": "2",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004601.1.23",
          "form": "월정교가 기억에 많이 남아요",
          "original_form": "월정교가 기억에 많이 남아요",
          "speaker_id": "2",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004601.1.24",
          "form": "월정교도 경주에 있죠?",
          "original_form": "월정교도 경주에 있죠?",
          "speaker_id": "1",
          "time": "20210821 18:09"
        },
        {
          "id": "MDRW2100004601.1.25",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210821 18:09"
        }
      ]
    }
  ]
