      "utterance": [
        {
          "id": "MDRW2100025381.1.1",
          "form": "점심 뭐 드시나용~~",
          "original_form": "점심 뭐 드시나용~~",
          "speaker_id": "2",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100025381.1.2",
          "form": "글쎄요 매일 최대 고민이네여",
          "original_form": "글쎄요 매일 최대 고민이네여",
          "speaker_id": "1",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100025381.1.3",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100025381.1.4",
          "form": "먹는게 제일 고민이에요",
          "original_form": "먹는게 제일 고민이에요",
          "speaker_id": "2",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100025381.1.5",
          "form": "지금 땡기는건 칼국수?",
          "original_form": "지금 땡기는건 칼국수?",
          "speaker_id": "1",
          "time": "20210902 11:24"
        },
        {
          "id": "MDRW2100025381.1.6",
          "form": "오~ 칼국수",
          "original_form": "오~ 칼국수",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100025381.1.7",
          "form": "비오나요?",
          "original_form": "비오나요?",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100025381.1.8",
          "form": "전 점심에 안동찜닭 나와서 먹었어요",
          "original_form": "전 점심에 안동찜닭 나와서 먹었어요",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100025381.1.9",
          "form": "아뇨 꾸물꾸물한 날씨였는데",
          "original_form": "아뇨 꾸물꾸물한 날씨였는데",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100025381.1.10",
          "form": "지금은 맑네요",
          "original_form": "지금은 맑네요",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100025381.1.11",
          "form": "걍 집에서 프렌치토스트해먹었어요",
          "original_form": "걍 집에서 프렌치토스트해먹었어요",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100025381.1.12",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100025381.1.13",
          "form": "설탕 뿌리셨나요",
          "original_form": "설탕 뿌리셨나요",
          "speaker_id": "2",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100025381.1.14",
          "form": "진짜 맛있겠네요",
          "original_form": "진짜 맛있겠네요",
          "speaker_id": "2",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100025381.1.15",
          "form": "여긴 비가 왔다안왔다해요",
          "original_form": "여긴 비가 왔다안왔다해요",
          "speaker_id": "2",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100025381.1.16",
          "form": "아뇨 오늘은 잼을 찍었어오",
          "original_form": "아뇨 오늘은 잼을 찍었어오",
          "speaker_id": "1",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100025381.1.17",
          "form": "여긴 비는 안오네요",
          "original_form": "여긴 비는 안오네요",
          "speaker_id": "1",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100025381.1.18",
          "form": "맛있었겠다ㅠ 그냥 식빵에 쨈 발라도 맛있는데",
          "original_form": "맛있었겠다ㅠ 그냥 식빵에 쨈 발라도 맛있는데",
          "speaker_id": "2",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100025381.1.19",
          "form": "저녁드셨어요~?",
          "original_form": "저녁드셨어요~?",
          "speaker_id": "2",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100025381.1.20",
          "form": "아뇽요거트하나 먹었어요",
          "original_form": "아뇽요거트하나 먹었어요",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100025381.1.21",
          "form": "요거트 하나 ㅠ간단히 드셨네용",
          "original_form": "요거트 하나 ㅠ간단히 드셨네용",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100025381.1.22",
          "form": "요즘 그릭요거트에 빠져서 자주먹게돼요",
          "original_form": "요즘 그릭요거트에 빠져서 자주먹게돼요",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100025381.1.23",
          "form": "그릭 요즘 만들어 먹는 사람도 많은 것 같아요",
          "original_form": "그릭 요즘 만들어 먹는 사람도 많은 것 같아요",
          "speaker_id": "1",
          "time": "20210902 21:39"
        }
