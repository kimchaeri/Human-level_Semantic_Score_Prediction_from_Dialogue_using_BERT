      "utterance": [
        {
          "id": "MDRW2100021398.1.1",
          "form": "전과 준비 열심히 하고",
          "original_form": "전과 준비 열심히 하고",
          "speaker_id": "2",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021398.1.2",
          "form": "ㅇㅇ 나 진짜 4.5 받을거임",
          "original_form": "ㅇㅇ 나 진짜 4.5 받을거임",
          "speaker_id": "1",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021398.1.3",
          "form": "사실 학점만 맞춘다고 끝이 아니니까",
          "original_form": "사실 학점만 맞춘다고 끝이 아니니까",
          "speaker_id": "2",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021398.1.4",
          "form": "?!?",
          "original_form": "?!?",
          "speaker_id": "1",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021398.1.5",
          "form": "컴공 관련 공부도 틈틈히 하면서",
          "original_form": "컴공 관련 공부도 틈틈히 하면서",
          "speaker_id": "2",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021398.1.6",
          "form": "너넨 성적말고 다른거도있어?",
          "original_form": "너넨 성적말고 다른거도있어?",
          "speaker_id": "1",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021398.1.7",
          "form": "열심히 보내보자",
          "original_form": "열심히 보내보자",
          "speaker_id": "2",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021398.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021398.1.9",
          "form": "그려요",
          "original_form": "그려요",
          "speaker_id": "1",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021398.1.10",
          "form": "또 늦게가네",
          "original_form": "또 늦게가네",
          "speaker_id": "2",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021398.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:33"
        },
        {
          "id": "MDRW2100021398.1.12",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "2",
          "time": "20210828 18:33"
        },
        {
          "id": "MDRW2100021398.1.13",
