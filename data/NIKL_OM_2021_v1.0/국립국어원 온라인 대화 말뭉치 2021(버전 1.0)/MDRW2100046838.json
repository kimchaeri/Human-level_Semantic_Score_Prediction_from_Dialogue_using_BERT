      "utterance": [
        {
          "id": "MDRW2100046838.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211105 03:44"
        },
        {
          "id": "MDRW2100046838.1.2",
          "form": "안녕하세용!",
          "original_form": "안녕하세용!",
          "speaker_id": "2",
          "time": "20211105 10:14"
        },
        {
          "id": "MDRW2100046838.1.3",
          "form": "낙엽 구경하러 가실 계획이 있으신가용",
          "original_form": "낙엽 구경하러 가실 계획이 있으신가용",
          "speaker_id": "2",
          "time": "20211105 10:15"
        },
        {
          "id": "MDRW2100046838.1.4",
          "form": "좋죠 요즘 단풍 많이 폈던데 곧 낙엽 이쁘겠네요",
          "original_form": "좋죠 요즘 단풍 많이 폈던데 곧 낙엽 이쁘겠네요",
          "speaker_id": "1",
          "time": "20211106 00:12"
        },
        {
          "id": "MDRW2100046838.1.5",
          "form": "그쵸 ㅠㅠ 지금도 이쁜 곳은 정말 절경이더라구오",
          "original_form": "그쵸 ㅠㅠ 지금도 이쁜 곳은 정말 절경이더라구오",
          "speaker_id": "2",
          "time": "20211105 13:20"
        },
        {
          "id": "MDRW2100046838.1.6",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20211105 13:20"
        },
        {
          "id": "MDRW2100046838.1.7",
          "form": "혹시 서울 쪽 사세요?",
          "original_form": "혹시 서울 쪽 사세요?",
          "speaker_id": "2",
          "time": "20211105 13:20"
        },
        {
          "id": "MDRW2100046838.1.8",
          "form": "아뇨 ㅠㅠ 전 제주살아요",
          "original_form": "아뇨 ㅠㅠ 전 제주살아요",
          "speaker_id": "1",
          "time": "20211105 13:42"
        },
        {
          "id": "MDRW2100046838.1.9",
          "form": "서울 야경 이뻐서 맨날 부럽네요",
          "original_form": "서울 야경 이뻐서 맨날 부럽네요",
          "speaker_id": "1",
          "time": "20211105 13:42"
        },
        {
          "id": "MDRW2100046838.1.10",
          "form": "제주도가 더 이쁘지 않나요.? ㅠㅠ",
          "original_form": "제주도가 더 이쁘지 않나요{emoji:…}.? ㅠㅠ",
          "speaker_id": "2",
          "time": "20211105 13:43"
        },
        {
          "id": "MDRW2100046838.1.11",
          "form": "제주는 낮에 이쁘고 서울은 밤에 이쁜거같아요 ㅎㅎ",
          "original_form": "제주는 낮에 이쁘고 서울은 밤에 이쁜거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211105 13:43"
        },
        {
          "id": "MDRW2100046838.1.12",
          "form": "저 곧 제주도 가려고 하는데",
          "original_form": "저 곧 제주도 가려고 하는데",
          "speaker_id": "2",
          "time": "20211105 13:50"
        },
        {
          "id": "MDRW2100046838.1.13",
          "form": "여기만큼은",
          "original_form": "여기만큼은",
          "speaker_id": "2",
          "time": "20211105 13:50"
        },
        {
          "id": "MDRW2100046838.1.14",
          "form": "꼭 가봐야 하는 곳이 있을까요?",
          "original_form": "꼭 가봐야 하는 곳이 있을까요?",
          "speaker_id": "2",
          "time": "20211105 13:50"
        },
        {
          "id": "MDRW2100046838.1.15",
          "form": "어.. 전 역시 해안도로가 짱인거같아요",
          "original_form": "어.. 전 역시 해안도로가 짱인거같아요",
          "speaker_id": "1",
          "time": "20211105 13:51"
        },
        {
          "id": "MDRW2100046838.1.16",
          "form": "우도도 좋고요!",
          "original_form": "우도도 좋고요!",
          "speaker_id": "1",
          "time": "20211105 13:51"
        },
        {
          "id": "MDRW2100046838.1.17",
          "form": "오오 메모 해두고 꼭 가볼게여",
          "original_form": "오오 메모 해두고 꼭 가볼게여",
          "speaker_id": "2",
          "time": "20211105 13:51"
        },
        {
          "id": "MDRW2100046838.1.18",
          "form": "우도 ㅎㅎㅎ 우도도 무조건 갈거에요",
          "original_form": "우도 ㅎㅎㅎ 우도도 무조건 갈거에요",
          "speaker_id": "2",
          "time": "20211105 13:51"
        }
      ]
