      "utterance": [
        {
          "id": "MDRW2100011540.1.1",
          "form": "나도 또 단발해야지ㅋㅋ",
          "original_form": "나도 또 단발해야지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011540.1.2",
          "form": "근데..울 엄마도 머리 진짜 안김..",
          "original_form": "근데..울 엄마도 머리 진짜 안김..",
          "speaker_id": "2",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011540.1.3",
          "form": "우리 애들이 애낳아도 안길꺼아녀 ㅠ",
          "original_form": "우리 애들이 애낳아도 안길꺼아녀 ㅠ",
          "speaker_id": "2",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011540.1.4",
          "form": "아들을 낳아야 ㅋㅋㅋㅋ",
          "original_form": "아들을 낳아야 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011540.1.5",
          "form": "ㅋㅋㅋㅋㅋ유전이네ㅋㅋㅋㅋㄲ",
          "original_form": "ㅋㅋㅋㅋㅋ유전이네ㅋㅋㅋㅋㄲ",
          "speaker_id": "1",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011540.1.6",
          "form": "단발이 진짜 편하긴하다 ㅋㅋㅋㅋㅋ",
          "original_form": "단발이 진짜 편하긴하다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011540.1.7",
          "form": "맞어 단발이 편하지ㅋㅋ",
          "original_form": "맞어 단발이 편하지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011540.1.8",
          "form": "애들걱정은 말거라ㅋㅋㅇ",
          "original_form": "애들걱정은 말거라ㅋㅋㅇ",
          "speaker_id": "1",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011540.1.9",
          "form": "근데 왜 긴머리 풀고 바람에 날리는거 해보고픔 ㅋㅋㅋ",
          "original_form": "근데 왜 긴머리 풀고 바람에 날리는거 해보고픔 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011540.1.10",
          "form": "알아서 하지싶다ㅋㅋㅋ",
          "original_form": "알아서 하지싶다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011540.1.11",
          "form": "웃겨ㅋㅋㅋ",
          "original_form": "웃겨ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011540.1.12",
          "form": "좀 있음 퇴근이네?",
          "original_form": "좀 있음 퇴근이네?",
          "speaker_id": "1",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011540.1.13",
          "form": "좋다ㅋㅋ",
          "original_form": "좋다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011540.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ오늘은 4시까지ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ오늘은 4시까지ㅠ",
          "speaker_id": "2",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011540.1.15",
          "form": "헉 왜 그렇게 늦게까지?",
          "original_form": "헉 왜 그렇게 늦게까지?",
          "speaker_id": "1",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011540.1.16",
          "form": "늦게갔나?",
          "original_form": "늦게갔나?",
          "speaker_id": "1",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100011540.1.17",
          "form": "화욜은 4시~그카고 목욜 쉬는겨ㅠ",
          "original_form": "화욜은 4시~그카고 목욜 쉬는겨ㅠ",
          "speaker_id": "2",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100011540.1.18",
          "form": "그래서 첫찌 안데려왔지~",
          "original_form": "그래서 첫찌 안데려왔지~",
          "speaker_id": "2",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100011540.1.19",
          "form": "2시간 차인데 윽시 시간 안감 ;;",
          "original_form": "2시간 차인데 윽시 시간 안감 ;;",
          "speaker_id": "2",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100011540.1.20",
          "form": "아아 그렇구나",
          "original_form": "아아 그렇구나",
          "speaker_id": "1",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011540.1.21",
          "form": "2시간 더 하는구나",
          "original_form": "2시간 더 하는구나",
          "speaker_id": "1",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011540.1.22",
          "form": "힘들겠다ㅜ",
          "original_form": "힘들겠다ㅜ",
          "speaker_id": "1",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011540.1.23",
          "form": "웅~집에가면 집안일 또 산더미겠지..",
          "original_form": "웅~집에가면 집안일 또 산더미겠지..",
          "speaker_id": "2",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011540.1.24",
          "form": "점심먹은거 과자먹은거~옷 아무데나 던져놓고 ㅋㅋㅋ",
          "original_form": "점심먹은거 과자먹은거~옷 아무데나 던져놓고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011540.1.25",
          "form": "그러이 나는 지금도 집안일 산더미인데ㅋㅋㅋ",
          "original_form": "그러이 나는 지금도 집안일 산더미인데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011540.1.26",
          "form": "맞다맞다 나두ㅋㅋ",
          "original_form": "맞다맞다 나두ㅋㅋ",
