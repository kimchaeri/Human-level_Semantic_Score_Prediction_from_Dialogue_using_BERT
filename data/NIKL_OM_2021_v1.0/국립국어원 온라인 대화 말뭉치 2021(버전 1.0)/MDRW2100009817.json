      "utterance": [
        {
          "id": "MDRW2100009817.1.1",
          "form": "모바일 게임 하시는거 있나요?",
          "original_form": "모바일 게임 하시는거 있나요?",
          "speaker_id": "1",
          "time": "20210823 10:22"
        },
        {
          "id": "MDRW2100009817.1.2",
          "form": "그냥 간단한 퍼즐게임 같은것만해요 ㅋㅋ",
          "original_form": "그냥 간단한 퍼즐게임 같은것만해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100009817.1.3",
          "form": "퍼즐게임 좋아하는데 한개만 추천해주세요",
          "original_form": "퍼즐게임 좋아하는데 한개만 추천해주세요",
          "speaker_id": "1",
          "time": "20210823 10:53"
        },
        {
          "id": "MDRW2100009817.1.4",
          "form": "워터 소트 퍼즐이라구",
          "original_form": "워터 소트 퍼즐이라구",
          "speaker_id": "2",
          "time": "20210823 12:24"
        },
        {
          "id": "MDRW2100009817.1.5",
          "form": "나름 재밌어요",
          "original_form": "나름 재밌어요",
          "speaker_id": "2",
          "time": "20210823 12:24"
        },
        {
          "id": "MDRW2100009817.1.6",
          "form": "스토쿠도 재밌던데",
          "original_form": "스토쿠도 재밌던데",
          "speaker_id": "2",
          "time": "20210823 12:24"
        },
        {
          "id": "MDRW2100009817.1.7",
          "form": "스도쿠하세요~?",
          "original_form": "스도쿠하세요~?",
          "speaker_id": "2",
          "time": "20210823 12:24"
        },
        {
          "id": "MDRW2100009817.1.8",
          "form": "네 스도쿠 엄청 많이 해요",
          "original_form": "네 스도쿠 엄청 많이 해요",
          "speaker_id": "1",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009817.1.9",
          "form": "스도쿠만 거의 5년 했어요",
          "original_form": "스도쿠만 거의 5년 했어요",
          "speaker_id": "1",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009817.1.10",
          "form": "헉 고수시네여 ㅋㅋㅋㅋ",
          "original_form": "헉 고수시네여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100009817.1.11",
          "form": "고수는 아니에요 그냥 오래 했을뿐",
          "original_form": "고수는 아니에요 그냥 오래 했을뿐",
          "speaker_id": "1",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100009817.1.12",
          "form": "그래도 짬밥 이라는게 ㅎ.ㅎ",
          "original_form": "그래도 짬밥 이라는게 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210823 16:38"
        },
        {
          "id": "MDRW2100009817.1.13",
          "form": "다른게임.하시는거 있어요??",
          "original_form": "다른게임.하시는거 있어요??",
          "speaker_id": "2",
          "time": "20210823 16:38"
        },
        {
          "id": "MDRW2100009817.1.14",
          "form": "퍼즐앤드래곤도 스도쿠만큼 오래했어요",
          "original_form": "퍼즐앤드래곤도 스도쿠만큼 오래했어요",
          "speaker_id": "1",
          "time": "20210823 19:14"
        },
        {
          "id": "MDRW2100009817.1.15",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100009817.1.16",
          "form": "폰게임만하세요 ??ㅎㅎ",
          "original_form": "폰게임만하세요 ??ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 20:33"
        },
        {
          "id": "MDRW2100009817.1.17",
          "form": "아니요ㅋㅋ 컴게임도 해요 대신 롤만",
          "original_form": "아니요ㅋㅋ 컴게임도 해요 대신 롤만",
          "speaker_id": "1",
          "time": "20210823 20:45"
        },
        {
          "id": "MDRW2100009817.1.18",
          "form": "아 저도 롤하다가",
          "original_form": "아 저도 롤하다가",
          "speaker_id": "2",
          "time": "20210823 21:19"
        },
        {
          "id": "MDRW2100009817.1.19",
          "form": "접은지 좀 됐어요",
          "original_form": "접은지 좀 됐어요",
          "speaker_id": "2",
          "time": "20210823 21:19"
        },
        {
          "id": "MDRW2100009817.1.20",
          "form": "친구들하고 가끔가다한번씩..ㅋㅋ",
          "original_form": "친구들하고 가끔가다한번씩..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:19"
        },
        {
          "id": "MDRW2100009817.1.21",
          "form": "친구랑 같이 하기 좋은 겜이긴 하죠",
          "original_form": "친구랑 같이 하기 좋은 겜이긴 하죠",
          "speaker_id": "1",
          "time": "20210823 22:13"
        },
        {
          "id": "MDRW2100009817.1.22",
          "form": "같이하기 좋ㄱ도하고",
          "original_form": "같이하기 좋ㄱ도하고",
          "speaker_id": "2",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100009817.1.23",
          "form": "싸움나기도 쉬울수도 있는 겜이죠 ㅋㅋ",
          "original_form": "싸움나기도 쉬울수도 있는 겜이죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100009817.1.24",
          "form": "맞아요 그래서 진짜 친한 친구들이랑만 해요",
          "original_form": "맞아요 그래서 진짜 친한 친구들이랑만 해요",
          "speaker_id": "1",
          "time": "20210823 23:11"
        },
        {
          "id": "MDRW2100009817.1.25",
          "form": "게임 경기같은것도 보세요?",
          "original_form": "게임 경기같은것도 보세요?",
          "speaker_id": "2",
          "time": "20210824 08:04"
        },
        {
          "id": "MDRW2100009817.1.26",
          "form": "네ㅎㅎ 롤이랑 배그 대회 즐겨봐요",
          "original_form": "네ㅎㅎ 롤이랑 배그 대회 즐겨봐요",
          "speaker_id": "1",
          "time": "20210824 09:14"
        },
        {
          "id": "MDRW2100009817.1.27",
          "form": "한때는 카트라이더도 봤어요",
          "original_form": "한때는 카트라이더도 봤어요",
          "speaker_id": "1",
          "time": "20210824 09:14"
        },
        {
          "id": "MDRW2100009817.1.28",
          "form": "아~ 카트라이더 어렸을때 정말 재미있게했는데 ㅎㅎ",
          "original_form": "아~ 카트라이더 어렸을때 정말 재미있게했는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 09:18"
        },
        {
          "id": "MDRW2100009817.1.29",
          "form": "저도 많이 했었는데 죽어도 실력은 안늘더라구요",
          "original_form": "저도 많이 했었는데 죽어도 실력은 안늘더라구요",
          "speaker_id": "1",
          "time": "20210824 09:19"
        },
        {
          "id": "MDRW2100009817.1.30",
          "form": "저도 게임은 좋아하는데 재능은 없는거같아요 ㅋㅋㅋ",
          "original_form": "저도 게임은 좋아하는데 재능은 없는거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:30"
        },
        {
          "id": "MDRW2100009817.1.31",
          "form": "게임에 재능있는 사람들 보면 신기해요",
          "original_form": "게임에 재능있는 사람들 보면 신기해요",
          "speaker_id": "1",
          "time": "20210824 10:32"
        },
        {
          "id": "MDRW2100009817.1.32",
          "form": "모니터 앞에서 오래 있는것도 힘들던데",
          "original_form": "모니터 앞에서 오래 있는것도 힘들던데",
          "speaker_id": "1",
          "time": "20210824 10:32"
        },
        {
          "id": "MDRW2100009817.1.33",
          "form": "그러게요 정말로 정신력이 대단한거같긴해요",
          "original_form": "그러게요 정말로 정신력이 대단한거같긴해요",
          "speaker_id": "2",
          "time": "20210824 10:58"
        }
      ]
