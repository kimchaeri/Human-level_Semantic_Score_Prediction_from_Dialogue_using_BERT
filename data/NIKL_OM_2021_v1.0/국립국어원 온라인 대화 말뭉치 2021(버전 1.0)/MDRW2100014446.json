      "utterance": [
        {
          "id": "MDRW2100014446.1.1",
          "form": "저도 있음 중고 거래 그렇게 하고 싶네요 ㅎㅎ",
          "original_form": "저도 있음 중고 거래 그렇게 하고 싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014446.1.2",
          "form": "근데 저는 당근하면",
          "original_form": "근데 저는 당근하면",
          "speaker_id": "1",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014446.1.3",
          "form": "연세 지긋하신분들이 많이 오세요 ㅎ",
          "original_form": "연세 지긋하신분들이 많이 오세요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014446.1.4",
          "form": "아 그럼 걱정 없네요",
          "original_form": "아 그럼 걱정 없네요",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014446.1.5",
          "form": "전 먹 젊은 엄마가 깎아서 ㅜㅜ슬펐어요",
          "original_form": "전 먹 젊은 엄마가 깎아서 ㅜㅜ슬펐어요",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014446.1.6",
          "form": "제가 덤을 한트럭 드렸는데",
          "original_form": "제가 덤을 한트럭 드렸는데",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014446.1.7",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014446.1.8",
          "form": "넘하네요",
          "original_form": "넘하네요",
          "speaker_id": "1",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014446.1.9",
          "form": "덤도 있는데",
          "original_form": "덤도 있는데",
          "speaker_id": "1",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014446.1.10",
          "form": "연세 있으신분들도 깍으세요 ㅎ",
          "original_form": "연세 있으신분들도 깍으세요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014446.1.11",
          "form": "네 덤가득",
          "original_form": "네 덤가득",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014446.1.12",
          "form": "그게 당근 특징같아요 ㅋㅋ",
          "original_form": "그게 당근 특징같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014446.1.13",
          "form": "사러오셔서는 깍아주면안되겠냐고 ㅋ",
          "original_form": "사러오셔서는 깍아주면안되겠냐고 ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014446.1.14",
          "form": "지난번에는 태백산맥 미니북 판매한다고 올렸는데",
          "original_form": "지난번에는 태백산맥 미니북 판매한다고 올렸는데",
          "speaker_id": "1",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014446.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100014446.1.16",
          "form": "저도 와서 깍아달래요 ㅋㅋ",
          "original_form": "저도 와서 깍아달래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100014446.1.17",
          "form": "올렸는데 어찌됐어요?",
          "original_form": "올렸는데 어찌됐어요?",
          "speaker_id": "2",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014446.1.18",
          "form": "사러오셔서는 미니북인지 몰랐다며 ㅋ",
          "original_form": "사러오셔서는 미니북인지 몰랐다며 ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014446.1.19",
          "form": "할아버지시던데 ㅎㅎ",
          "original_form": "할아버지시던데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014446.1.20",
          "form": "아 그런적 있어요",
          "original_form": "아 그런적 있어요",
          "speaker_id": "2",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014446.1.21",
          "form": "저도 ㅋㅋ",
          "original_form": "저도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014446.1.22",
          "form": "미안하다고 너무작아서 못볼것같다고",
          "original_form": "미안하다고 너무작아서 못볼것같다고",
          "speaker_id": "1",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014446.1.23",
          "form": "돌아가셨어요 ㅎ",
          "original_form": "돌아가셨어요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014446.1.24",
          "form": "모르실만 하긴하네요 ㅋㅋ",
          "original_form": "모르실만 하긴하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014446.1.25",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014446.1.26",
          "form": "아쉬워라 나가랴하는데 ㅡㅡ고생하셨네요",
          "original_form": "아쉬워라 나가랴하는데 ㅡㅡ고생하셨네요",
          "speaker_id": "2",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014446.1.27",
          "form": "나가야하는데",
          "original_form": "나가야하는데",
          "speaker_id": "2",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014446.1.28",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014446.1.29",
          "form": "돌아가는 발걸음이 무거웠어요",
          "original_form": "돌아가는 발걸음이 무거웠어요",
          "speaker_id": "1",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014446.1.30",
          "form": "그럼요 허탕친건데 ㅡㅡ",
          "original_form": "그럼요 허탕친건데 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100014446.1.31",
          "form": "참 더운데",
          "original_form": "참 더운데",
          "speaker_id": "2",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100014446.1.32",
          "form": "그래도 며칠뒤에 중나에서 팔려서 ㅎㅎ 택배로 보냈어요",
          "original_form": "그래도 며칠뒤에 중나에서 팔려서 ㅎㅎ 택배로 보냈어요",
          "speaker_id": "1",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100014446.1.33",
          "form": "오 다행이네요",
          "original_form": "오 다행이네요",
          "speaker_id": "2",
          "time": "20210825 19:13"
        }
      ]
