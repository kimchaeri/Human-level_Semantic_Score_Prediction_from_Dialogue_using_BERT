      "utterance": [
        {
          "id": "MDRW2100027624.1.1",
          "form": "전 카페인에 예민해서 주스 마셔요",
          "original_form": "전 카페인에 예민해서 주스 마셔요",
          "speaker_id": "2",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100027624.1.2",
          "form": "커피가 얼마나 맛난데...",
          "original_form": "커피가 얼마나 맛난데...",
          "speaker_id": "1",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100027624.1.3",
          "form": "시럽이나 크림 들어가면 맛있어요 달아서",
          "original_form": "시럽이나 크림 들어가면 맛있어요 달아서",
          "speaker_id": "2",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100027624.1.4",
          "form": "전 단 건 많이 못 먹요",
          "original_form": "전 단 건 많이 못 먹요",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027624.1.5",
          "form": "디카페인만 가끔 마셔요",
          "original_form": "디카페인만 가끔 마셔요",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027624.1.6",
          "form": "커피는 씁쓸한 맛이 조아요",
          "original_form": "커피는 씁쓸한 맛이 조아요",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027624.1.7",
          "form": "전 라떼 조아해요",
          "original_form": "전 라떼 조아해요",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027624.1.8",
          "form": "디카페인도 안 가려요 저도 커피는 다좋음",
          "original_form": "디카페인도 안 가려요 저도 커피는 다좋음",
          "speaker_id": "1",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027624.1.9",
          "form": "생과일주스도 맛나요",
          "original_form": "생과일주스도 맛나요",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027624.1.10",
          "form": "음 그건 비싸서. 전 커피!",
          "original_form": "음 그건 비싸서. 전 커피!",
          "speaker_id": "1",
          "time": "20210902 14:01"
        },
        {
          "id": "MDRW2100027624.1.11",
          "form": "키위 딸기 자몽 달고 맛있는 음료 당기네요",
          "original_form": "키위 딸기 자몽 달고 맛있는 음료 당기네요",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027624.1.12",
          "form": "생과일쥬스를 많이 좋아하시나봐요",
          "original_form": "생과일쥬스를 많이 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027624.1.13",
          "form": "넹 아니면 코코아 마셔요ㅋ",
          "original_form": "넹 아니면 코코아 마셔요ㅋ",
          "speaker_id": "2",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100027624.1.14",
          "form": "추억의 이름이네요 코코아",
          "original_form": "추억의 이름이네요 코코아",
          "speaker_id": "1",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027624.1.15",
          "form": "차 좋아하세요? 전 유자차",
          "original_form": "차 좋아하세요? 전 유자차",
          "speaker_id": "2",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027624.1.16",
          "form": "음 저도 유자차 좋아해요ㅋ",
          "original_form": "음 저도 유자차 좋아해요ㅋ",
          "speaker_id": "1",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027624.1.17",
          "form": "오늘 저녁은 뭐드시나요?",
          "original_form": "오늘 저녁은 뭐드시나요?",
