      "utterance": [
        {
          "id": "MDRW2100014659.1.1",
          "form": "요즘 태풍이라고 뉴스에서 떠들썩하던데",
          "original_form": "요즘 태풍이라고 뉴스에서 떠들썩하던데",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014659.1.2",
          "form": "name2님네 동네는 괜찮으신가요??",
          "original_form": "&name2&님네 동네는 괜찮으신가요??",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014659.1.3",
          "form": "저희동네는 비가오고있어요 ㅎㅎ",
          "original_form": "저희동네는 비가오고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014659.1.4",
          "form": "저희동네는 오늘은 비가 안오네요!!",
          "original_form": "저희동네는 오늘은 비가 안오네요!!",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014659.1.5",
          "form": "가을장마가 빨리 끝닜으면 좋겠어요",
          "original_form": "가을장마가 빨리 끝닜으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014659.1.6",
          "form": "저희지역은 비가 내.렸다가 그.쳤다가 그러네요.ㅋㅋ",
          "original_form": "저희지역은 비가 내.렸다가 그.쳤다가 그러네요.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014659.1.7",
          "form": "그래도 태풍 피해는 없어서 다행이에요!!",
          "original_form": "그래도 태풍 피해는 없어서 다행이에요!!",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014659.1.8",
          "form": "맞아여 ㅜㅜ 태풍이 또 오진않겟죠?ㅋㅋ",
          "original_form": "맞아여 ㅜㅜ 태풍이 또 오진않겟죠?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014659.1.9",
          "form": "ㅋㅋㅋ스쳐지나갔을거에요!!아마도?",
          "original_form": "ㅋㅋㅋ스쳐지나갔을거에요!!아마도?",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014659.1.10",
          "form": "날씨가 점점 선선해져가는것 같아서 좋아요!",
          "original_form": "날씨가 점점 선선해져가는것 같아서 좋아요!",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014659.1.11",
          "form": "ㅋㅋㅋ 저도요 ㅋㅋ 근데 겨울은 싫네요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 저도요 ㅋㅋ 근데 겨울은 싫네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014659.1.12",
          "form": "전 가을을 제일 좋아해서",
          "original_form": "전 가을을 제일 좋아해서",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014659.1.13",
          "form": "지금 습한거 좀 빠지면 딱 좋을것 같아요 ㅎㅎ",
          "original_form": "지금 습한거 좀 빠지면 딱 좋을것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014659.1.14",
          "form": "오 그렇군요 ㅎㅎ 벌써 가을이라니 시간이 너무나도 빨리감을 실감하게되네요",
          "original_form": "오 그렇군요 ㅎㅎ 벌써 가을이라니 시간이 너무나도 빨리감을 실감하게되네요",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014659.1.15",
          "form": "그러게요 곧 겨울이 되겠죠...?",
          "original_form": "그러게요 곧 겨울이 되겠죠...?",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014659.1.16",
          "form": "겨울엔 더 집에만 있게되서",
          "original_form": "겨울엔 더 집에만 있게되서",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014659.1.17",
          "form": "저는 겨울이 제일 싫어요 ㅠㅠ",
          "original_form": "저는 겨울이 제일 싫어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014659.1.18",
          "form": "빨리오진 않았으면 좋겠어요..",
          "original_form": "빨리오진 않았으면 좋겠어요..",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014659.1.19",
          "form": "ㅠㅠ겨울 왜 싫어하시나요??",
          "original_form": "ㅠㅠ겨울 왜 싫어하시나요??",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014659.1.20",
          "form": "맞아요.ㅋㅋ 눈 많이와서 빙판길되면 위험해서요 ㅋㅋ",
          "original_form": "맞아요.ㅋㅋ 눈 많이와서 빙판길되면 위험해서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014659.1.21",
          "form": "아 인정합니다 ㅋㅋㅋ",
          "original_form": "아 인정합니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014659.1.22",
          "form": "저도 눈오는건 좋은데",
          "original_form": "저도 눈오는건 좋은데",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014659.1.23",
          "form": "눈오고 나서가 싫어요!!",
          "original_form": "눈오고 나서가 싫어요!!",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014659.1.24",
          "form": "맞아요 ㅠㅠ 그럴땐 따뜻한 나라로 떠나고싶어요 ㅋㅋ",
          "original_form": "맞아요 ㅠㅠ 그럴땐 따뜻한 나라로 떠나고싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:24"
        }
      ]
