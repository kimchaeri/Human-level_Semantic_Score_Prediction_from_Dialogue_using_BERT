      "utterance": [
        {
          "id": "MDRW2100004000.1.1",
          "form": "게임하시는거있나용",
          "original_form": "게임하시는거있나용",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100004000.1.2",
          "form": "아니용ㅜㅜ",
          "original_form": "아니용ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100004000.1.3",
          "form": "저는  휴대폰 게임만 조금 해요ㅜㅜ",
          "original_form": "저는  휴대폰 게임만 조금 해요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100004000.1.4",
          "form": "저도요새없어요ㅠ",
          "original_form": "저도요새없어요ㅠ",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100004000.1.5",
          "form": "하시는 거 있으세요??",
          "original_form": "하시는 거 있으세요??",
          "speaker_id": "2",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100004000.1.6",
          "form": "사실 원래",
          "original_form": "사실 원래",
          "speaker_id": "1",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100004000.1.7",
          "form": "테일즈런너 진짜오래했고",
          "original_form": "테일즈런너 진짜오래했고",
          "speaker_id": "1",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100004000.1.8",
          "form": "오버워치 잠깐했고",
          "original_form": "오버워치 잠깐했고",
          "speaker_id": "1",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100004000.1.9",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100004000.1.10",
          "form": "메이플도했는데.. 요샌정말 하나두안해욕ㄱㅋㄱ",
          "original_form": "메이플도했는데.. 요샌정말 하나두안해욕ㄱㅋㄱ",
          "speaker_id": "1",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100004000.1.11",
          "form": "저는 옛날에 콘솔게임..?",
          "original_form": "저는 옛날에 콘솔게임..?",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100004000.1.12",
          "form": "플레이스테이션 이런 거",
          "original_form": "플레이스테이션 이런 거",
          "speaker_id": "2",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100004000.1.13",
          "form": "요즘은 너무 현질유도하는 게임이 많아져서 안 하게 되는 것 같아요",
          "original_form": "요즘은 너무 현질유도하는 게임이 많아져서 안 하게 되는 것 같아요",
          "speaker_id": "2",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100004000.1.14",
          "form": "양산형도 많고",
          "original_form": "양산형도 많고",
          "speaker_id": "2",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100004000.1.15",
          "form": "아 맞아요..",
          "original_form": "아 맞아요..",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100004000.1.16",
          "form": "너무 다 확률성 뽑기 현질 게임이 많아져서",
          "original_form": "너무 다 확률성 뽑기 현질 게임이 많아져서",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100004000.1.17",
          "form": "별로예요",
          "original_form": "별로예요",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100004000.1.18",
          "form": "게임이아니라 도박같아요ㅠㅋㄲ",
          "original_form": "게임이아니라 도박같아요ㅠㅋㄲ",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100004000.1.19",
          "form": "ㄲㅋㄱㅋㄱㅋ맞아요ㅜㅜ",
          "original_form": "ㄲㅋㄱㅋㄱㅋ맞아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100004000.1.20",
          "form": "또 게임 광고도 너무 별로ㅜㅜ",
          "original_form": "또 게임 광고도 너무 별로ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100004000.1.21",
          "form": "근데 제가 좋아하는 스포츠팀이 게임회사에서 운영하는 거라",
          "original_form": "근데 제가 좋아하는 스포츠팀이 게임회사에서 운영하는 거라",
          "speaker_id": "2",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100004000.1.22",
          "form": "게임 회사가 잘됬으면 하는데 유저들한테 욕을 많이 먹어서 슬프네요ㅎ. .",
          "original_form": "게임 회사가 잘됬으면 하는데 유저들한테 욕을 많이 먹어서 슬프네요ㅎ. .",
          "speaker_id": "2",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100004000.1.23",
          "form": "앗ㅠㅠㅠㅠ 눈물이...",
          "original_form": "앗ㅠㅠㅠㅠ 눈물이...",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100004000.1.24",
          "form": "게임회사는 필연적으로 욕먹게되긴 하는거같아요",
          "original_form": "게임회사는 필연적으로 욕먹게되긴 하는거같아요",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100004000.1.25",
          "form": "무슨 스포츠팀 좋아하세요?",
          "original_form": "무슨 스포츠팀 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100004000.1.26",
          "form": "저는 NC SOFT에서 운영하는 NC DINOS 좋아해요",
          "original_form": "저는 NC SOFT에서 운영하는 NC DINOS 좋아해요",
          "speaker_id": "2",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100004000.1.27",
          "form": "그 게임 리니지 만드는 회사에요!",
          "original_form": "그 게임 리니지 만드는 회사에요!",
          "speaker_id": "2",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100004000.1.28",
          "form": "아하 야구좋아하시는구나",
          "original_form": "아하 야구좋아하시는구나",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004000.1.29",
          "form": "야구좋아하시는분들은 뭔가 화날일이많으시던데(잘은 모르지만ㅋㅋ",
          "original_form": "야구좋아하시는분들은 뭔가 화날일이많으시던데(잘은 모르지만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004000.1.30",
          "form": "화이팅입니다..",
          "original_form": "화이팅입니다..",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004000.1.31",
          "form": "ㅋㅋㄱㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004000.1.32",
          "form": "야구.....(할말하않...",
          "original_form": "야구.....(할말하않...",
          "speaker_id": "2",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004000.1.33",
          "form": "NC SOFT가 진짜 현질 유도를 많이 해서",
          "original_form": "NC SOFT가 진짜 현질 유도를 많이 해서",
          "speaker_id": "2",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004000.1.34",
          "form": "막 과금?도 엄청 한다고 악명이 자자하더라구요",
          "original_form": "막 과금?도 엄청 한다고 악명이 자자하더라구요",
          "speaker_id": "2",
          "time": "20210821 14:46"
        },
        {
          "id": "MDRW2100004000.1.35",
          "form": "저도 게임 잘 모르지만",
          "original_form": "저도 게임 잘 모르지만",
          "speaker_id": "1",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004000.1.36",
          "form": "엔씨 악명에대해선 들어본거같네요ㅋㅋ",
          "original_form": "엔씨 악명에대해선 들어본거같네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:52"
        }
      ]
