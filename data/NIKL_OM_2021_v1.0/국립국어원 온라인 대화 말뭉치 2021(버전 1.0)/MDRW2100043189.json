      "utterance": [
        {
          "id": "MDRW2100043189.1.1",
          "form": "쓱배송 엄청 시켜요ㅋㅋㅋ",
          "original_form": "쓱배송 엄청 시켜요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043189.1.2",
          "form": "쓱은 4만이라 부담시렵던데ㅠ",
          "original_form": "쓱은 4만이라 부담시렵던데ㅠ",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100043189.1.3",
          "form": "그래서 한번에 몰아서 시켜요ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 한번에 몰아서 시켜요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043189.1.4",
          "form": "노브랜드사랑....ㅋㅋㅋ",
          "original_form": "노브랜드사랑....ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043189.1.5",
          "form": "ㅋㅋ 꼬치.. 맛나쥬",
          "original_form": "ㅋㅋ 꼬치.. 맛나쥬",
          "speaker_id": "1",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043189.1.6",
          "form": "혹시 닭꼬치 ...?",
          "original_form": "혹시 닭꼬치 ...?",
          "speaker_id": "2",
          "time": "20210907 15:35"
        },
        {
          "id": "MDRW2100043189.1.7",
          "form": "아 그거 대박이던데요",
          "original_form": "아 그거 대박이던데요",
          "speaker_id": "2",
          "time": "20210907 15:35"
        },
        {
          "id": "MDRW2100043189.1.8",
          "form": "맞아여 ㅎㅎㅎ 숯불 머시기...",
          "original_form": "맞아여 ㅎㅎㅎ 숯불 머시기...",
          "speaker_id": "1",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043189.1.9",
          "form": "20갠가 든거",
          "original_form": "20갠가 든거",
          "speaker_id": "1",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043189.1.10",
          "form": "전 양념으로 ㅎㅎㅎ",
          "original_form": "전 양념으로 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043189.1.11",
          "form": "ㅋㅋㅋㅋ그래서 사만원채우면 노브랜드제품 푸짐하게 담아서 냉장고 터져요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ그래서 사만원채우면 노브랜드제품 푸짐하게 담아서 냉장고 터져요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:43"
        },
        {
          "id": "MDRW2100043189.1.12",
          "form": "노브랜드 어떤거 주로사세여?",
          "original_form": "노브랜드 어떤거 주로사세여?",
          "speaker_id": "1",
          "time": "20210907 15:43"
        },
        {
          "id": "MDRW2100043189.1.13",
          "form": "먹을꺼",
          "original_form": "먹을꺼",
          "speaker_id": "2",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043189.1.14",
          "form": "생필품",
          "original_form": "생필품",
          "speaker_id": "2",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043189.1.15",
          "form": "거의 다 노브랜드에요",
          "original_form": "거의 다 노브랜드에요",
          "speaker_id": "2",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043189.1.16",
          "form": "과자는 추천있나요??",
          "original_form": "과자는 추천있나요??",
          "speaker_id": "1",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043189.1.17",
          "form": "콰삭칩 무뚝뚝 요새",
          "original_form": "콰삭칩 무뚝뚝 요새",
          "speaker_id": "2",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100043189.1.18",
          "form": "홀릭이에요",
          "original_form": "홀릭이에요",
          "speaker_id": "2",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100043189.1.19",
          "form": "콰삭칩 어 먹어봤는데 모양이 희안하더라구요",
          "original_form": "콰삭칩 어 먹어봤는데 모양이 희안하더라구요",
