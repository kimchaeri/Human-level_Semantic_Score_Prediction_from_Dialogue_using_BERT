        },
        {
          "id": "MDRW2100016578.1.2",
          "form": "예전에 어떤거하셧나요",
          "original_form": "예전에 어떤거하셧나요",
          "speaker_id": "1",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016578.1.3",
          "form": "젤 오래한 건 메이플이었는데",
          "original_form": "젤 오래한 건 메이플이었는데",
          "speaker_id": "2",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016578.1.4",
          "form": "최근에는 동물의숲 같은 거 했어요",
          "original_form": "최근에는 동물의숲 같은 거 했어요",
          "speaker_id": "2",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016578.1.5",
          "form": "게임 좋아하시나요",
          "original_form": "게임 좋아하시나요",
          "speaker_id": "2",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016578.1.6",
          "form": "네 좋아해요~",
          "original_form": "네 좋아해요~",
          "speaker_id": "1",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100016578.1.7",
          "form": "어떤 게임 하세요",
          "original_form": "어떤 게임 하세요",
