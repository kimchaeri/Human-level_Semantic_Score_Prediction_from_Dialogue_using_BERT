        },
        {
          "id": "MDRW2100012378.1.2",
          "form": "네 ㅎㅎ쇼핑 좋아하세요?",
          "original_form": "네 ㅎㅎ쇼핑 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100012378.1.3",
          "form": "네 ㅋㅋ쇼핑몰 둘러보기도 좋아해요 ㅋㅋ",
          "original_form": "네 ㅋㅋ쇼핑몰 둘러보기도 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012378.1.4",
          "form": "택배가 ㅋㅋ하루에 한번씩와요 ㅜㅜ",
          "original_form": "택배가 ㅋㅋ하루에 한번씩와요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012378.1.5",
          "form": "뭐 사셨어요 최근 에?",
          "original_form": "뭐 사셨어요 최근 에?",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012378.1.6",
          "form": "저 식료품하고",
          "original_form": "저 식료품하고",
          "speaker_id": "1",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012378.1.7",
          "form": "운동화요",
          "original_form": "운동화요",
          "speaker_id": "1",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012378.1.8",
          "form": "쇼핑 좋아하세요",
          "original_form": "쇼핑 좋아하세요",
          "speaker_id": "1",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012378.1.9",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100012378.1.10",
          "form": "피자랑 냉동식품 왔어요",
          "original_form": "피자랑 냉동식품 왔어요",
          "speaker_id": "1",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100012378.1.11",
          "form": "아이랑 남편 운동화요",
          "original_form": "아이랑 남편 운동화요",
          "speaker_id": "1",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100012378.1.12",
          "form": "최근 뭐 사셨어요?",
          "original_form": "최근 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100012378.1.13",
          "form": "매일 먹거리 만 가득이네요",
          "original_form": "매일 먹거리 만 가득이네요",
          "speaker_id": "2",
          "time": "20210824 22:23"
        },
        {
          "id": "MDRW2100012378.1.14",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210824 22:30"
        },
        {
          "id": "MDRW2100012378.1.15",
          "form": "요새 아무래도 잘안너가고 애들 집에 있어서",
          "original_form": "요새 아무래도 잘안너가고 애들 집에 있어서",
          "speaker_id": "1",
          "time": "20210824 22:30"
        },
        {
          "id": "MDRW2100012378.1.16",
          "form": "먹을거리만 사요 ㅜㅜ",
          "original_form": "먹을거리만 사요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 22:30"
        },
        {
          "id": "MDRW2100012378.1.17",
          "form": "어젠 티몬에서",
          "original_form": "어젠 티몬에서",
          "speaker_id": "1",
          "time": "20210824 22:31"
        },
        {
          "id": "MDRW2100012378.1.18",
          "form": "쿠폰 샀어요",
          "original_form": "쿠폰 샀어요",
          "speaker_id": "1",
          "time": "20210824 22:31"
        },
        {
          "id": "MDRW2100012378.1.19",
          "form": "기프티콘도 세일하면 49 프로 해서 넘싸요",
          "original_form": "기프티콘도 세일하면 49 프로 해서 넘싸요",
          "speaker_id": "1",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100012378.1.20",
          "form": "우와 뭐가그리 싸게 팔아요?",
          "original_form": "우와 뭐가그리 싸게 팔아요?",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012378.1.21",
          "form": "버거킹요 ㅋㅋ",
          "original_form": "버거킹요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012378.1.22",
          "form": "애들이 햄버거 좋아해요",
          "original_form": "애들이 햄버거 좋아해요",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012378.1.23",
