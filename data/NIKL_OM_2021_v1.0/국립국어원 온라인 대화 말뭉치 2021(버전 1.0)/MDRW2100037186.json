      "utterance": [
        {
          "id": "MDRW2100037186.1.1",
          "form": "백신 접종하셨나요?",
          "original_form": "백신 접종하셨나요?",
          "speaker_id": "2",
          "time": "20210904 20:31"
        },
        {
          "id": "MDRW2100037186.1.2",
          "form": "아뇨 안했어요",
          "original_form": "아뇨 안했어요",
          "speaker_id": "1",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100037186.1.3",
          "form": "백신 맞기 싫어요",
          "original_form": "백신 맞기 싫어요",
          "speaker_id": "1",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100037186.1.4",
          "form": "저는 어제 2차접종까지 끝냈어요",
          "original_form": "저는 어제 2차접종까지 끝냈어요",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100037186.1.5",
          "form": "그러셨군요",
          "original_form": "그러셨군요",
          "speaker_id": "1",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100037186.1.6",
          "form": "끝나서 좋으시겠어요",
          "original_form": "끝나서 좋으시겠어요",
          "speaker_id": "1",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100037186.1.7",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100037186.1.8",
          "form": "사실 어제 접종했는데 오늘 컨디션이 별로라ㅠ",
          "original_form": "사실 어제 접종했는데 오늘 컨디션이 별로라ㅠ",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100037186.1.9",
          "form": "끝내서 시원한 맘도 있지만, 접종 후에 앓는 건 걱정이예요ㅜ",
          "original_form": "끝내서 시원한 맘도 있지만, 접종 후에 앓는 건 걱정이예요ㅜ",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100037186.1.10",
          "form": "아 ㅠㅠ",
          "original_form": "아 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100037186.1.11",
          "form": "증상이 약간 있으신가봐요",
          "original_form": "증상이 약간 있으신가봐요",
          "speaker_id": "1",
          "time": "20210904 20:35"
        },
        {
          "id": "MDRW2100037186.1.12",
          "form": "네 2차가 더 힘들다더니..",
          "original_form": "네 2차가 더 힘들다더니..",
          "speaker_id": "2",
          "time": "20210904 20:35"
        },
        {
          "id": "MDRW2100037186.1.13",
          "form": "건강하려고 백신 맞았는데",
          "original_form": "건강하려고 백신 맞았는데",
          "speaker_id": "2",
          "time": "20210904 20:35"
        },
        {
          "id": "MDRW2100037186.1.14",
          "form": "백신 후유증이 있으니 몸도 맘도 편안하지만은 않네요ㅜ",
          "original_form": "백신 후유증이 있으니 몸도 맘도 편안하지만은 않네요ㅜ",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100037186.1.15",
          "form": "신문보면 무섭더라구요",
          "original_form": "신문보면 무섭더라구요",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100037186.1.16",
          "form": "제가 맞는다고 그렇게 될거란 생각까진 안하지만",
          "original_form": "제가 맞는다고 그렇게 될거란 생각까진 안하지만",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100037186.1.17",
          "form": "별로 안맞고싶어서 조용히 신청안하고 있어요",
          "original_form": "별로 안맞고싶어서 조용히 신청안하고 있어요",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100037186.1.18",
          "form": "아아 그러시구나",
          "original_form": "아아 그러시구나",
          "speaker_id": "2",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100037186.1.19",
          "form": "백신 후유증이 워낙 사람마다 달라서",
          "original_form": "백신 후유증이 워낙 사람마다 달라서",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100037186.1.20",
          "form": "제 주변에도 안 맞겠단 분들 있으시더라고요",
          "original_form": "제 주변에도 안 맞겠단 분들 있으시더라고요",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100037186.1.21",
          "form": "그렇군요 그걸 너무 무지한 안티백서로 몰아가니",
          "original_form": "그렇군요 그걸 너무 무지한 안티백서로 몰아가니",
          "speaker_id": "1",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100037186.1.22",
          "form": "좀 불편하긴 해요",
          "original_form": "좀 불편하긴 해요",
          "speaker_id": "1",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100037186.1.23",
          "form": "어디 온라인서 맞지말자 선동만 안하면 그만이라 생각하거든요",
          "original_form": "어디 온라인서 맞지말자 선동만 안하면 그만이라 생각하거든요",
          "speaker_id": "1",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100037186.1.24",
          "form": "아 그렇긴 하죠",
          "original_form": "아 그렇긴 하죠",
          "speaker_id": "2",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100037186.1.25",
          "form": "개인 선택일 수 있는 건데",
          "original_form": "개인 선택일 수 있는 건데",
          "speaker_id": "2",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100037186.1.26",
          "form": "대신 코로나 안 걸리게 조심하면서 지내면 되죠",
          "original_form": "대신 코로나 안 걸리게 조심하면서 지내면 되죠",
          "speaker_id": "2",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100037186.1.27",
          "form": "네 밖에 잘안나가요",
          "original_form": "네 밖에 잘안나가요",
          "speaker_id": "1",
          "time": "20210904 20:43"
        },
        {
          "id": "MDRW2100037186.1.28",
          "form": "그래서 굳이안맞고싶어요",
          "original_form": "그래서 굳이안맞고싶어요",
          "speaker_id": "1",
          "time": "20210904 20:43"
        },
        {
          "id": "MDRW2100037186.1.29",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "2",
          "time": "20210904 20:45"
        }
      ]
    }
  ]
