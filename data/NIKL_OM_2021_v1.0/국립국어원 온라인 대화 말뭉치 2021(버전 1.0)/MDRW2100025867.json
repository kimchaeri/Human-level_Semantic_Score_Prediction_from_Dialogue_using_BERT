      "utterance": [
        {
          "id": "MDRW2100025867.1.1",
          "form": "오늘은 무슨노래 들으셨나요?",
          "original_form": "오늘은 무슨노래 들으셨나요?",
          "speaker_id": "2",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100025867.1.2",
          "form": "방금 들은건",
          "original_form": "방금 들은건",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100025867.1.3",
          "form": "박재범 하루하루요",
          "original_form": "박재범 하루하루요",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100025867.1.4",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100025867.1.5",
          "form": "전 라디오 틀었어요 ㅎㅎ",
          "original_form": "전 라디오 틀었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100025867.1.6",
          "form": "라디오 안들은지 진짜 오래됐어요",
          "original_form": "라디오 안들은지 진짜 오래됐어요",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100025867.1.7",
          "form": "밤에 조용히 라디오 틀고 있으면 좋아요",
          "original_form": "밤에 조용히 라디오 틀고 있으면 좋아요",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100025867.1.8",
          "form": "나름 분위기 있을 듯",
          "original_form": "나름 분위기 있을 듯",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100025867.1.9",
          "form": "네 분위기 좋아요 ㅋㅋ",
          "original_form": "네 분위기 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100025867.1.10",
          "form": "주로 무슨라디오 들으세요?",
          "original_form": "주로 무슨라디오 들으세요?",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100025867.1.11",
          "form": "라디오 어플 들어가서 그시간에 가장 맘에드는 노래 틀어주는거 들어요 ㅎㅎ",
          "original_form": "라디오 어플 들어가서 그시간에 가장 맘에드는 노래 틀어주는거 들어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 07:48"
        },
        {
          "id": "MDRW2100025867.1.12",
          "form": "음~",
          "original_form": "음~",
          "speaker_id": "1",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100025867.1.13",
          "form": "전 예전에 박원꺼 좀 들었는데",
          "original_form": "전 예전에 박원꺼 좀 들었는데",
          "speaker_id": "1",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100025867.1.14",
          "form": "노래 자주들으시는군여",
          "original_form": "노래 자주들으시는군여",
          "speaker_id": "2",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100025867.1.15",
          "form": "걍 심심하거나 걸을때나 이동시 자주들어요",
          "original_form": "걍 심심하거나 걸을때나 이동시 자주들어요",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100025867.1.16",
          "form": "학생이에요?",
          "original_form": "학생이에요?",
          "speaker_id": "2",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100025867.1.17",
          "form": "아니요 직장인이요",
          "original_form": "아니요 직장인이요",
          "speaker_id": "1",
          "time": "20210902 14:01"
        },
        {
          "id": "MDRW2100025867.1.18",
          "form": "아 저도 직장인인데",
          "original_form": "아 저도 직장인인데",
          "speaker_id": "2",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100025867.1.19",
          "form": "왜 노래듣기가 힘든지 ㅋㅋㅋ",
          "original_form": "왜 노래듣기가 힘든지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100025867.1.20",
          "form": "통근할때랑 샤워할때",
          "original_form": "통근할때랑 샤워할때",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100025867.1.21",
          "form": "아침에 알람으로 !",
          "original_form": "아침에 알람으로 !",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100025867.1.22",
          "form": "예전엔 출근준비시 메이크업할때도 자주들었는데",
          "original_form": "예전엔 출근준비시 메이크업할때도 자주들었는데",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100025867.1.23",
          "form": "요즘은 화장을 안해서..",
          "original_form": "요즘은 화장을 안해서..",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100025867.1.24",
          "form": "ㅋㅋㅋㅋㅋ 마스크땜에요?",
          "original_form": "ㅋㅋㅋㅋㅋ 마스크땜에요?",
          "speaker_id": "2",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100025867.1.25",
          "form": "네 네 선블록하나로 ㅇㅋ",
          "original_form": "네 네 선블록하나로 ㅇㅋ",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100025867.1.26",
          "form": "예전엔 톤업크림이라도 발랐는데 귀찮네요",
          "original_form": "예전엔 톤업크림이라도 발랐는데 귀찮네요",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100025867.1.27",
          "form": "그거면 커버가 되나여?ㅋㅋ",
          "original_form": "그거면 커버가 되나여?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100025867.1.28",
          "form": "별로 잘 안보이니까 ㅋㅋ",
          "original_form": "별로 잘 안보이니까 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100025867.1.29",
          "form": "자기만족이죠",
          "original_form": "자기만족이죠",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100025867.1.30",
          "form": "그럼 성시경노래도 좋아하시겠어요?",
          "original_form": "그럼 성시경노래도 좋아하시겠어요?",
          "speaker_id": "1",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100025867.1.31",
          "form": "젤 좋아하는 곡은?",
          "original_form": "젤 좋아하는 곡은?",
          "speaker_id": "1",
          "time": "20210903 20:58"
        }
      ]
