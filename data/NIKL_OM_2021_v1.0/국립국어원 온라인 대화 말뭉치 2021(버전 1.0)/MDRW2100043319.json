      "utterance": [
        {
          "id": "MDRW2100043319.1.1",
          "form": "아직 못 정했어요",
          "original_form": "아직 못 정했어요",
          "speaker_id": "2",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100043319.1.2",
          "form": "메뉴 정하셨나요?",
          "original_form": "메뉴 정하셨나요?",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100043319.1.3",
          "form": "저는 잔치국수 해먹을까 싶어용",
          "original_form": "저는 잔치국수 해먹을까 싶어용",
          "speaker_id": "1",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100043319.1.4",
          "form": "오 맛있겠네요ㅋㅋㅋ",
          "original_form": "오 맛있겠네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:44"
        },
        {
          "id": "MDRW2100043319.1.5",
          "form": "저도 면 먹고싶네요. 해먹진 못하겠구요ㅋㅋ",
          "original_form": "저도 면 먹고싶네요. 해먹진 못하겠구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100043319.1.6",
          "form": "네네 ㅋㅋㅋ라면은 어떠세요",
          "original_form": "네네 ㅋㅋㅋ라면은 어떠세요",
          "speaker_id": "1",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100043319.1.7",
          "form": "좋네요ㅋㅋㅋ 라면 언제 먹어도 맛있어요",
          "original_form": "좋네요ㅋㅋㅋ 라면 언제 먹어도 맛있어요",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100043319.1.8",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043319.1.9",
          "form": "자도 짖 도착하니 너무 힘들어서",
          "original_form": "자도 짖 도착하니 너무 힘들어서",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043319.1.10",
          "form": "잔치국수 못해먹겠어요 ㅋㅋㅋ",
          "original_form": "잔치국수 못해먹겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043319.1.11",
          "form": "ㅋㅋㅋㅋ요리 너무 힘들어요... 퇴근하면 기력도 없어가지구",
          "original_form": "ㅋㅋㅋㅋ요리 너무 힘들어요... 퇴근하면 기력도 없어가지구",
          "speaker_id": "2",
          "time": "20210907 18:09"
        },
        {
          "id": "MDRW2100043319.1.12",
          "form": "구쵸 그래거 쌀굳구 시켰어요",
          "original_form": "구쵸 그래거 쌀굳구 시켰어요",
          "speaker_id": "1",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100043319.1.13",
          "form": "이래서ㅜ돈 언제 모아",
          "original_form": "이래서ㅜ돈 언제 모아",
          "speaker_id": "1",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100043319.1.14",
          "form": "저 진짜로 라면 끓이려구요ㅎ",
          "original_form": "저 진짜로 라면 끓이려구요ㅎ",
          "speaker_id": "2",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100043319.1.15",
          "form": "우우아우아",
          "original_form": "우우아우아",
          "speaker_id": "1",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100043319.1.16",
          "form": "무슨 라면요",
          "original_form": "무슨 라면요",
          "speaker_id": "1",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100043319.1.17",
          "form": "진라면이요 할인해서 저번에 많이 사뒀어요",
          "original_form": "진라면이요 할인해서 저번에 많이 사뒀어요",
          "speaker_id": "2",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100043319.1.18",
          "form": "우와 순한 매운?",
          "original_form": "우와 순한 매운?",
          "speaker_id": "1",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100043319.1.19",
          "form": "매운맛이요ㅋㅋㅋㅋ",
          "original_form": "매운맛이요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100043319.1.20",
          "form": "꺄올 진라면이 ㅌ최고야",
          "original_form": "꺄올 진라면이 ㅌ최고야",
          "speaker_id": "1",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100043319.1.21",
          "form": "진비빔면 맛나자나요",
          "original_form": "진비빔면 맛나자나요",
          "speaker_id": "1",
          "time": "20210907 18:18"
        },
        {
          "id": "MDRW2100043319.1.22",
          "form": "맞아요ㅋㅋ 원래 팔도만 먹었는데 맛있더라구요",
          "original_form": "맞아요ㅋㅋ 원래 팔도만 먹었는데 맛있더라구요",
          "speaker_id": "2",
          "time": "20210907 18:18"
        },
        {
          "id": "MDRW2100043319.1.23",
          "form": "맞아요 진비빔면 최고",
          "original_form": "맞아요 진비빔면 최고",
          "speaker_id": "1",
          "time": "20210907 18:19"
        }
      ]
