      "utterance": [
        {
          "id": "MDRW2100043255.1.1",
          "form": "근데 뭐 먹자니 급 식욕도없어",
          "original_form": "근데 뭐 먹자니 급 식욕도없어",
          "speaker_id": "1",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100043255.1.2",
          "form": "아니프로틴이라도 갈기자",
          "original_form": "아니프로틴이라도 갈기자",
          "speaker_id": "2",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100043255.1.3",
          "form": "헤엑 그거도싫은데",
          "original_form": "헤엑 그거도싫은데",
          "speaker_id": "1",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043255.1.4",
          "form": "그러다 액체동물된다구",
          "original_form": "그러다 액체동물된다구",
          "speaker_id": "2",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043255.1.5",
          "form": "마시멜로우로 돌아갈거야?",
          "original_form": "마시멜로우로 돌아갈거야?",
          "speaker_id": "2",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043255.1.6",
          "form": "무슨소리야 아령은할건데",
          "original_form": "무슨소리야 아령은할건데",
          "speaker_id": "1",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043255.1.7",
          "form": "저녁 샤브샤브?",
          "original_form": "저녁 샤브샤브?",
          "speaker_id": "1",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100043255.1.8",
          "form": "ㅇㅇ샤브샤브먹을거야 확정남",
          "original_form": "ㅇㅇ샤브샤브먹을거야 확정남",
          "speaker_id": "2",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100043255.1.9",
          "form": "뭐넣을거야?",
          "original_form": "뭐넣을거야?",
          "speaker_id": "1",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100043255.1.10",
          "form": "알배추잇는줄알았는데 없네?ㅋㅋㅋㅋ 버섯이랑 같ㅇ이사오래",
          "original_form": "알배추잇는줄알았는데 없네?ㅋㅋㅋㅋ 버섯이랑 같ㅇ이사오래",
          "speaker_id": "2",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100043255.1.11",
          "form": "알배추진짜비싸",
          "original_form": "알배추진짜비싸",
          "speaker_id": "1",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100043255.1.12",
          "form": "배추,고기,버섯,청경채, 또 머넣으면 맛있지",
          "original_form": "배추,고기,버섯,청경채, 또 머넣으면 맛있지",
          "speaker_id": "2",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100043255.1.13",
