      "utterance": [
        {
          "id": "MDRW2100002461.1.1",
          "form": "최근에 물건 뭐 구입하셨어요 ㅎ?",
          "original_form": "최근에 물건 뭐 구입하셨어요 ㅎ?",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002461.1.2",
          "form": "저 최근에 옷을 샀어요ㅎㅎㅎ",
          "original_form": "저 최근에 옷을 샀어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002461.1.3",
          "form": "구매하신거있으신가요?",
          "original_form": "구매하신거있으신가요?",
          "speaker_id": "2",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002461.1.4",
          "form": "아직이구 저도 옷 살까 고민이에요",
          "original_form": "아직이구 저도 옷 살까 고민이에요",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002461.1.5",
          "form": "어떤 옷 구매하셨어요?",
          "original_form": "어떤 옷 구매하셨어요?",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002461.1.6",
          "form": "저 원피스하나샀답니다ㅎㅎㅎ",
          "original_form": "저 원피스하나샀답니다ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002461.1.7",
          "form": "무슨옷고민중이세요?",
          "original_form": "무슨옷고민중이세요?",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002461.1.8",
          "form": "원피스 좋죠",
          "original_form": "원피스 좋죠",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002461.1.9",
          "form": "전 벌써 가을옷 고민입니다 ㅎ",
          "original_form": "전 벌써 가을옷 고민입니다 ㅎ",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002461.1.10",
          "form": "아 저도요ㅋㅋㅋ",
          "original_form": "아 저도요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002461.1.11",
          "form": "자케사고싶어요!!!!",
          "original_form": "자케사고싶어요!!!!",
          "speaker_id": "2",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002461.1.12",
          "form": "자켓",
          "original_form": "자켓",
          "speaker_id": "2",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002461.1.13",
          "form": "저두요 자켓이 옷 입기에 매치도 좋고",
          "original_form": "저두요 자켓이 옷 입기에 매치도 좋고",
          "speaker_id": "1",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002461.1.14",
          "form": "좋은 거 같아요 ㅎㅎ",
          "original_form": "좋은 거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002461.1.15",
          "form": "맞아요ㅋㅋ 옷이 편하고",
          "original_form": "맞아요ㅋㅋ 옷이 편하고",
          "speaker_id": "2",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002461.1.16",
          "form": "진짜 가을이 금방온거같아요",
          "original_form": "진짜 가을이 금방온거같아요",
          "speaker_id": "2",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002461.1.17",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002461.1.18",
          "form": "얼른 마음에 드는 자켓을 알아봐야겠어요",
          "original_form": "얼른 마음에 드는 자켓을 알아봐야겠어요",
          "speaker_id": "1",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002461.1.19",
          "form": "저는 자라 구경가보려구요ㅋㅋㅋ",
          "original_form": "저는 자라 구경가보려구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002461.1.20",
          "form": "연갈색자켓있으면 사고싶네요",
          "original_form": "연갈색자켓있으면 사고싶네요",
          "speaker_id": "2",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002461.1.21",
          "form": "오 브라운느낌 자켓 좋죠",
          "original_form": "오 브라운느낌 자켓 좋죠",
          "speaker_id": "1",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002461.1.22",
          "form": "저도 깔별로 자켓 많이 사고싶엉6",
          "original_form": "저도 깔별로 자켓 많이 사고싶엉6",
          "speaker_id": "1",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002461.1.23",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002461.1.24",
          "form": "저는요즘 스마트워치",
          "original_form": "저는요즘 스마트워치",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002461.1.25",
          "form": "사고싶더라고요",
          "original_form": "사고싶더라고요",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002461.1.26",
          "form": "헉 저두요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "헉 저두요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002461.1.27",
          "form": "너무 갖고싶어요",
          "original_form": "너무 갖고싶어요",
