        {
          "id": "MDRW2100010609.1.1",
          "form": "저는 간단하게 계란밥 먹었어용 ㅎㅎ",
          "original_form": "저는 간단하게 계란밥 먹었어용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:00"
        },
        {
          "id": "MDRW2100010609.1.2",
          "form": "오우 계란밥 맛있겠어요!!@",
          "original_form": "오우 계란밥 맛있겠어요!!@",
          "speaker_id": "2",
          "time": "20210823 20:10"
        },
        {
          "id": "MDRW2100010609.1.3",
          "form": "반숙이랑 완숙 중 뭘 좋아하세여?",
          "original_form": "반숙이랑 완숙 중 뭘 좋아하세여?",
          "speaker_id": "2",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100010609.1.4",
          "form": "저는 둘다좋은데 굳이꼽자면",
          "original_form": "저는 둘다좋은데 굳이꼽자면",
          "speaker_id": "1",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010609.1.5",
          "form": "반숙을 더좋아해용",
          "original_form": "반숙을 더좋아해용",
          "speaker_id": "1",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010609.1.6",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010609.1.7",
          "form": "오 저두요 근데 완숙과 반숙 사이의 정도요",
          "original_form": "오 저두요 근데 완숙과 반숙 사이의 정도요",
          "speaker_id": "2",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010609.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ어렵네용 !!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ어렵네용 !!",
          "speaker_id": "1",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010609.1.9",
          "form": "떡볶이 좋아하시나요",
          "original_form": "떡볶이 좋아하시나요",
          "speaker_id": "1",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010609.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010609.1.11",
          "form": "떡볶이 사랑해여 ㅠㅠㅠㅠ",
          "original_form": "떡볶이 사랑해여 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010609.1.12",
          "form": "좋아하시죠?",
          "original_form": "좋아하시죠?",
          "speaker_id": "2",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010609.1.13",
          "form": "저도 완전 ..",
          "original_form": "저도 완전 ..",
          "speaker_id": "1",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010609.1.14",
          "form": "내일은 안그래도 떡볶이먹으려구욥 ㅋㅋ",
          "original_form": "내일은 안그래도 떡볶이먹으려구욥 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010609.1.15",
          "form": "어디 떡볶이 드시려구요??!",
          "original_form": "어디 떡볶이 드시려구요??!",
          "speaker_id": "2",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010609.1.16",
          "form": "집에서 해먹으려구용 !! 제입맛대루 ㅎㅎ",
          "original_form": "집에서 해먹으려구용 !! 제입맛대루 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010609.1.17",
          "form": "집에서 해먹으면 먹고싶은 거",
          "original_form": "집에서 해먹으면 먹고싶은 거",
          "speaker_id": "2",
          "time": "20210823 21:38"
        },
        {
          "id": "MDRW2100010609.1.18",
          "form": "다 넣을 수 있어서 좋죠ㅋㅋㅋㅋㅋ",
          "original_form": "다 넣을 수 있어서 좋죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:38"
        },
        {
          "id": "MDRW2100010609.1.19",
          "form": "네네 ㅋㅋㅋ 사먹는것도 가끔먹는데",
          "original_form": "네네 ㅋㅋㅋ 사먹는것도 가끔먹는데",
          "speaker_id": "1",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010609.1.20",
          "form": "양도많고 비싸규해서",
          "original_form": "양도많고 비싸규해서",
          "speaker_id": "1",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010609.1.21",
          "form": "딱 먹을만큼만 자주만들어먹어용 ㅎㅎ",
          "original_form": "딱 먹을만큼만 자주만들어먹어용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010609.1.22",
          "form": "그게 좋죠",
          "original_form": "그게 좋죠",
          "speaker_id": "2",
          "time": "20210823 21:40"
        },
        {
          "id": "MDRW2100010609.1.23",
          "form": "떡볶이 혼자 먹기에 배달은 너무 비싸요",
          "original_form": "떡볶이 혼자 먹기에 배달은 너무 비싸요",
          "speaker_id": "2",
          "time": "20210823 21:40"
        },
        {
          "id": "MDRW2100010609.1.24",
          "form": "캡사이신 느낌보다 청양고추좋아해서",
          "original_form": "캡사이신 느낌보다 청양고추좋아해서",
          "speaker_id": "1",
