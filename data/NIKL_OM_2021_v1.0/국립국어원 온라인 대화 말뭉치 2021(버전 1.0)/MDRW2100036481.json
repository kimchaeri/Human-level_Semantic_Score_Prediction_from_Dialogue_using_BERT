      "utterance": [
        {
          "id": "MDRW2100036481.1.1",
          "form": "너 스티커 많아?",
          "original_form": "너 스티커 많아?",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.2",
          "form": "전에 네가 사 준 오렌지 스티커 맘에들었어",
          "original_form": "전에 네가 사 준 오렌지 스티커 맘에들었어",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.3",
          "form": "음 나 어릴때",
          "original_form": "음 나 어릴때",
          "speaker_id": "1",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.4",
          "form": "귤스티커였던가?",
          "original_form": "귤스티커였던가?",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.5",
          "form": "모으는게 취미였었어",
          "original_form": "모으는게 취미였었어",
          "speaker_id": "1",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.6",
          "form": "오 나도 어릴 때 스티커를 모았어",
          "original_form": "오 나도 어릴 때 스티커를 모았어",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.7",
          "form": "귤이었을꺼야!!",
          "original_form": "귤이었을꺼야!!",
          "speaker_id": "1",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.8",
          "form": "근데 다 어디갔을까?",
          "original_form": "근데 다 어디갔을까?",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.9",
          "form": "쓰는건 아까워서 잘 못써",
          "original_form": "쓰는건 아까워서 잘 못써",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.10",
          "form": "그러게 근데 그것들은 촌스러워서",
          "original_form": "그러게 근데 그것들은 촌스러워서",
          "speaker_id": "1",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.11",
          "form": "남들은 팍팍 잘쓰던데",
          "original_form": "남들은 팍팍 잘쓰던데",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.12",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.13",
          "form": "지금은 못쓸꺼야",
          "original_form": "지금은 못쓸꺼야",
          "speaker_id": "1",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.14",
          "form": "맞아 나도ㅠㅜ",
          "original_form": "맞아 나도ㅠㅜ",
          "speaker_id": "1",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.15",
          "form": "나 요즘 스티커를 사고5ㅣㅍ어",
          "original_form": "나 요즘 스티커를 사고5ㅣㅍ어",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.16",
          "form": "뜯는게 너무너무 아까워",
          "original_form": "뜯는게 너무너무 아까워",
          "speaker_id": "1",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.17",
          "form": "근데 돈이 아까워",
          "original_form": "근데 돈이 아까워",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036481.1.18",
          "form": "뜯고나면 구멍나잖아 그부분이 너무 맘아파",
          "original_form": "뜯고나면 구멍나잖아 그부분이 너무 맘아파",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036481.1.19",
          "form": "그럼 못사는거겠지",
          "original_form": "그럼 못사는거겠지",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036481.1.20",
          "form": "맞아 그리고 다시 같은 스티커는 못봐",
          "original_form": "맞아 그리고 다시 같은 스티커는 못봐",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036481.1.21",
          "form": "그게 너무 아쉬워",
          "original_form": "그게 너무 아쉬워",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036481.1.22",
          "form": "그치 나중에 돈많이벌면 그때사자",
          "original_form": "그치 나중에 돈많이벌면 그때사자",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036481.1.23",
          "form": "너는 어디에 붙이고싳어?",
          "original_form": "너는 어디에 붙이고싳어?",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036481.1.24",
          "form": "스티커를 3장씩 넣어주면 좋겟어",
          "original_form": "스티커를 3장씩 넣어주면 좋겟어",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036481.1.25",
          "form": "나 아직도 러그 살지말지 고민이야",
          "original_form": "나 아직도 러그 살지말지 고민이야",
          "speaker_id": "1",
          "time": "20210830 23:45"
        }
      ]
    }
