      "utterance": [
        {
          "id": "MDRW2100034664.1.1",
          "form": "좋아하시는 과목 있으세요?",
          "original_form": "좋아하시는 과목 있으세요?",
          "speaker_id": "1",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100034664.1.2",
          "form": "저는 체육이랑 과학이랑 기술가정 좋아했어요ㅋㅋ",
          "original_form": "저는 체육이랑 과학이랑 기술가정 좋아했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100034664.1.3",
          "form": "와 좋아하는 과목이 많으시네요 ㅋㅋㅋ",
          "original_form": "와 좋아하는 과목이 많으시네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100034664.1.4",
          "form": "기술가정 저도 좋아했어요 국어랑요",
          "original_form": "기술가정 저도 좋아했어요 국어랑요",
          "speaker_id": "1",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100034664.1.5",
          "form": "다 공부랑은 관련없는 아이들이네요ㅋㅋ",
          "original_form": "다 공부랑은 관련없는 아이들이네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034664.1.6",
          "form": "국어 좋아하시는군요",
          "original_form": "국어 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034664.1.7",
          "form": "기술가정은 뭐 만들고 이런거 재밌었어요",
          "original_form": "기술가정은 뭐 만들고 이런거 재밌었어요",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034664.1.8",
          "form": "앜ㅋㅋ네",
          "original_form": "앜ㅋㅋ네",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034664.1.9",
          "form": "맞아요 전 바느질했던게 기억에 남네요",
          "original_form": "맞아요 전 바느질했던게 기억에 남네요",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034664.1.10",
          "form": "오  저두요 \n바느질 종류가 여러개인거 처음알았어요",
          "original_form": "오  저두요 \n바느질 종류가 여러개인거 처음알았어요",
          "speaker_id": "2",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100034664.1.11",
          "form": "은근 재밌더라구요",
          "original_form": "은근 재밌더라구요",
          "speaker_id": "2",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100034664.1.12",
          "form": "저도요 ㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100034664.1.13",
          "form": "공부보다 그게 더 좋았어요",
          "original_form": "공부보다 그게 더 좋았어요",
          "speaker_id": "1",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100034664.1.14",
          "form": "요리하는것도 있지 않앟나요?",
          "original_form": "요리하는것도 있지 않앟나요?",
          "speaker_id": "2",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100034664.1.15",
          "form": "저두요ㅋㅋㅋ중간중간 활력소랄까?ㅋㅋ",
          "original_form": "저두요ㅋㅋㅋ중간중간 활력소랄까?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100034664.1.16",
          "form": "오맞아요",
          "original_form": "오맞아요",
          "speaker_id": "1",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100034664.1.17",
          "form": "막 떡볶이도 해먹었던거같아요",
          "original_form": "막 떡볶이도 해먹었던거같아요",
          "speaker_id": "1",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100034664.1.18",
          "form": "완전 간식시간이었죠ㅋㅋㅋ",
          "original_form": "완전 간식시간이었죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100034664.1.19",
          "form": "국어 좋아하셨으면 문과신가요?",
          "original_form": "국어 좋아하셨으면 문과신가요?",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100034664.1.20",
          "form": "아저는 특성화라서요 ㅋㅋ",
          "original_form": "아저는 특성화라서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100034664.1.21",
          "form": "의료관련이었어요",
          "original_form": "의료관련이었어요",
          "speaker_id": "1",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100034664.1.22",
          "form": "우와! 멋있어요..!",
          "original_form": "우와! 멋있어요..!",
          "speaker_id": "2",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100034664.1.23",
          "form": "특성화고라니..",
          "original_form": "특성화고라니..",
          "speaker_id": "2",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100034664.1.24",
          "form": "아니에요 ㅋㅋㅋ 저는 오히려 야자가 없었어서",
          "original_form": "아니에요 ㅋㅋㅋ 저는 오히려 야자가 없었어서",
          "speaker_id": "1",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100034664.1.25",
          "form": "야자하는 로망이 있었어요",
          "original_form": "야자하는 로망이 있었어요",
          "speaker_id": "1",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100034664.1.26",
          "form": "아 진짜요?ㅋㅋㅋ",
          "original_form": "아 진짜요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034664.1.27",
          "form": "야자....하..ㅋㅋㅋ",
          "original_form": "야자....하..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034664.1.28",
          "form": "안해보셨으면 로망일수있죠",
          "original_form": "안해보셨으면 로망일수있죠",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034664.1.29",
          "form": "넵ㅋㅋㅋ",
          "original_form": "넵ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034664.1.30",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034664.1.31",
          "form": "정말 싫으셨나봐욬ㅋㅋㅋㅋ",
          "original_form": "정말 싫으셨나봐욬ㅋㅋㅋㅋ",
