      "utterance": [
        {
          "id": "MDRW2100041936.1.1",
          "form": "학교다니세요?",
          "original_form": "학교다니세요?",
          "speaker_id": "2",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041936.1.2",
          "form": "아뇨 저는 옛날에 졸업해서요 지금은 직장인입니다",
          "original_form": "아뇨 저는 옛날에 졸업해서요 지금은 직장인입니다",
          "speaker_id": "1",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041936.1.3",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041936.1.4",
          "form": "저도요ㅎㅎㅎ",
          "original_form": "저도요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041936.1.5",
          "form": "같이기억을더듬어볼까요",
          "original_form": "같이기억을더듬어볼까요",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041936.1.6",
          "form": "라떼는..비대면이었는데..",
          "original_form": "라떼는..비대면이었는데..",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041936.1.7",
          "form": "비대면 수업 하셨나요? 제 라떼는...무조건 대면 수업이었죠...ㅎㅎ...",
          "original_form": "비대면 수업 하셨나요? 제 라떼는...무조건 대면 수업이었죠...ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041936.1.8",
          "form": "앗 반대로말했어요",
          "original_form": "앗 반대로말했어요",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041936.1.9",
          "form": "ㅋㅋㅋ무조건대면이죵..",
          "original_form": "ㅋㅋㅋ무조건대면이죵..",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041936.1.10",
          "form": "시험도요새는비대면이래요",
          "original_form": "시험도요새는비대면이래요",
          "speaker_id": "2",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041936.1.11",
          "form": "어머 컨닝 가능인가요? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "어머 컨닝 가능인가요? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041936.1.12",
          "form": "코로나가 모든걸 다 바꿨네요",
          "original_form": "코로나가 모든걸 다 바꿨네요",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041936.1.13",
          "form": "같은 클래스에 누가 있는지도 모른다더라구요",
          "original_form": "같은 클래스에 누가 있는지도 모른다더라구요",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041936.1.14",
          "form": "그러게요ㅜㅜ",
          "original_form": "그러게요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041936.1.15",
          "form": "대학때동기도중요한데ㅠ",
          "original_form": "대학때동기도중요한데ㅠ",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041936.1.16",
          "form": "대학때 동기들이랑은 연락 자주 하시나요?",
          "original_form": "대학때 동기들이랑은 연락 자주 하시나요?",
          "speaker_id": "1",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041936.1.17",
          "form": "음 주기적으론 하는데 코로나때문에 만나긴 어렵네요",
          "original_form": "음 주기적으론 하는데 코로나때문에 만나긴 어렵네요",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041936.1.18",
          "form": "동기나선후배 만나세요?",
          "original_form": "동기나선후배 만나세요?",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041936.1.19",
          "form": "아뇨 안 만나요 코로나도 그렇고 다들 살기 바빠서",
          "original_form": "아뇨 안 만나요 코로나도 그렇고 다들 살기 바빠서",
          "speaker_id": "1",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041936.1.20",
          "form": "각자 인생 사는거죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "각자 인생 사는거죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041936.1.21",
          "form": "ㅎㅎㅎㅎㅎ맞아요",
          "original_form": "ㅎㅎㅎㅎㅎ맞아요",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041936.1.22",
          "form": "과제많은과셨어요?",
          "original_form": "과제많은과셨어요?",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041936.1.23",
          "form": "음... 과제 그렇게 많진 않았던거 같아요",
          "original_form": "음... 과제 그렇게 많진 않았던거 같아요",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041936.1.24",
          "form": "오래되니까 기억도 이젠 가물가물하네요 ㅋㅋㅋㅋ",
          "original_form": "오래되니까 기억도 이젠 가물가물하네요 ㅋㅋㅋㅋ",
