      "utterance": [
        {
          "id": "MDRW2100032726.1.1",
          "form": "후 렌드윅이",
          "original_form": "후 렌드윅이",
          "speaker_id": "1",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100032726.1.2",
          "form": "정말 생각보다",
          "original_form": "정말 생각보다",
          "speaker_id": "1",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100032726.1.3",
          "form": "잘해서만족스럽네여",
          "original_form": "잘해서만족스럽네여",
          "speaker_id": "1",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100032726.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ막공엔 얼마나 많이 늘어있을지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ막공엔 얼마나 많이 늘어있을지",
          "speaker_id": "2",
          "time": "20210906 10:29"
        },
        {
          "id": "MDRW2100032726.1.5",
          "form": "기대되는부분",
          "original_form": "기대되는부분",
          "speaker_id": "2",
          "time": "20210906 10:29"
        },
        {
          "id": "MDRW2100032726.1.6",
          "form": "보니까 규랑 민기람 완전비슷한걸보니",
          "original_form": "보니까 규랑 민기람 완전비슷한걸보니",
          "speaker_id": "1",
          "time": "20210906 11:03"
        },
        {
          "id": "MDRW2100032726.1.7",
          "form": "그게원래정해진 헤드윅같고",
          "original_form": "그게원래정해진 헤드윅같고",
          "speaker_id": "1",
          "time": "20210906 11:04"
        },
        {
          "id": "MDRW2100032726.1.8",
          "form": "조만유난히 튀나보던데 ㅋㄱㅋㅋㅋ",
          "original_form": "조만유난히 튀나보던데 ㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 11:04"
        },
        {
          "id": "MDRW2100032726.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ조는.. 조라서 그런가봐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ조는.. 조라서 그런가봐",
          "speaker_id": "2",
          "time": "20210906 16:32"
        },
        {
          "id": "MDRW2100032726.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ아니근데진짜 다른극같아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아니근데진짜 다른극같아",
          "speaker_id": "1",
          "time": "20210906 16:32"
        },
        {
          "id": "MDRW2100032726.1.11",
          "form": "다른캐보면그러자나",
          "original_form": "다른캐보면그러자나",
          "speaker_id": "1",
          "time": "20210906 16:32"
        },
        {
          "id": "MDRW2100032726.1.12",
          "form": "아 얘는 이대사를 이렇게치네",
          "original_form": "아 얘는 이대사를 이렇게치네",
          "speaker_id": "1",
          "time": "20210906 16:32"
        },
        {
          "id": "MDRW2100032726.1.13",
          "form": "이상황에ㅅ이렇게 몸을쓰네",
          "original_form": "이상황에ㅅ이렇게 몸을쓰네",
          "speaker_id": "1",
          "time": "20210906 16:32"
        },
        {
          "id": "MDRW2100032726.1.14",
          "form": "근데극의순서도 노래도 뭐도 다 다르니까",
          "original_form": "근데극의순서도 노래도 뭐도 다 다르니까",
          "speaker_id": "1",
          "time": "20210906 16:33"
        },
        {
          "id": "MDRW2100032726.1.15",
          "form": "ㅋㅋㅋㅋㅋ오만석까지 봐야",
          "original_form": "ㅋㅋㅋㅋㅋ오만석까지 봐야",
          "speaker_id": "1",
          "time": "20210906 16:33"
        },
        {
          "id": "MDRW2100032726.1.16",
          "form": "어떤지 이해하겠지만...",
          "original_form": "어떤지 이해하겠지만...",
          "speaker_id": "1",
          "time": "20210906 16:33"
        },
        {
          "id": "MDRW2100032726.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞네 오만석도 있었지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞네 오만석도 있었지",
          "speaker_id": "2",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100032726.1.18",
          "form": "ㅇㅇㅇ오만석이",
          "original_form": "ㅇㅇㅇ오만석이",
          "speaker_id": "1",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100032726.1.19",
          "form": "근본의근본이라던데",
          "original_form": "근본의근본이라던데",
          "speaker_id": "1",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100032726.1.20",
          "form": "ㅋㅋㅋㅋㅋㄱ조는 자기해석이뚜렷하고",
          "original_form": "ㅋㅋㅋㅋㅋㄱ조는 자기해석이뚜렷하고",
          "speaker_id": "1",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100032726.1.21",
          "form": "오가 진짜근본의헤드윅이라고",
          "original_form": "오가 진짜근본의헤드윅이라고",
          "speaker_id": "1",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100032726.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ오리지널을 보고 싶으면 오를 보라는거군여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ오리지널을 보고 싶으면 오를 보라는거군여",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100032726.1.23",
          "form": "ㅇㅇㅇㅇ그런거같더라구여",
          "original_form": "ㅇㅇㅇㅇ그런거같더라구여",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100032726.1.24",
          "form": "오드윅보러가야하는데",
          "original_form": "오드윅보러가야하는데",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100032726.1.25",
          "form": "언제가나",
          "original_form": "언제가나",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100032726.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ오드윅도 티켓팅 빡센편이야?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ오드윅도 티켓팅 빡센편이야?",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100032726.1.27",
          "form": "앞자리는 다빡세지만",
          "original_form": "앞자리는 다빡세지만",
          "speaker_id": "1",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100032726.1.28",
          "form": "조말고는",
          "original_form": "조말고는",
          "speaker_id": "1",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100032726.1.29",
          "form": "자리는다있는편이라",
          "original_form": "자리는다있는편이라",
          "speaker_id": "1",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100032726.1.30",
          "form": "욕심없음 암때나볼수있쬬",
          "original_form": "욕심없음 암때나볼수있쬬",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100032726.1.31",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100032726.1.32",
          "form": "티켓팅을 우선순위로 잡을 정도는 아니군여",
          "original_form": "티켓팅을 우선순위로 잡을 정도는 아니군여",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100032726.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅇㅇㅇ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100032726.1.34",
          "form": "자리욕심별로없어",
          "original_form": "자리욕심별로없어",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100032726.1.35",
          "form": "어제는",
          "original_form": "어제는",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100032726.1.36",
          "form": "8열이었으니",
          "original_form": "8열이었으니",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100032726.1.37",
          "form": "8열은",
          "original_form": "8열은",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100032726.1.38",
          "form": "망원경들일이없더라구요",
          "original_form": "망원경들일이없더라구요",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100032726.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그정도면 무대 전체적으로 보기에 좋은 자리겟네여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그정도면 무대 전체적으로 보기에 좋은 자리겟네여",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100032726.1.40",
          "form": "ㅇㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100032726.1.41",
          "form": "눈도딱마주치기좋고",
          "original_form": "눈도딱마주치기좋고",
          "speaker_id": "1",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100032726.1.42",
          "form": "망원경안들어도 표정선명하게보이고",
          "original_form": "망원경안들어도 표정선명하게보이고",
