      "utterance": [
        {
          "id": "MDRW2100041813.1.1",
          "form": "제가있는곳은 비와요",
          "original_form": "제가있는곳은 비와요",
          "speaker_id": "2",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041813.1.2",
          "form": "거긴어떤가요?",
          "original_form": "거긴어떤가요?",
          "speaker_id": "2",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041813.1.3",
          "form": "여기는 아직인데 하늘이 꾸리꾸리하네요..",
          "original_form": "여기는 아직인데 하늘이 꾸리꾸리하네요..",
          "speaker_id": "1",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041813.1.4",
          "form": "오늘 계속 비오더라고요ㅜㅠ",
          "original_form": "오늘 계속 비오더라고요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100041813.1.5",
          "form": "저녁까지....ㅠ",
          "original_form": "저녁까지....ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100041813.1.6",
          "form": "와 비 엄청오네요ㅠㅠ",
          "original_form": "와 비 엄청오네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100041813.1.7",
          "form": "빨래가힘들어요",
          "original_form": "빨래가힘들어요",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100041813.1.8",
          "form": "진짜요ㅋㅋ 안마르고",
          "original_form": "진짜요ㅋㅋ 안마르고",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100041813.1.9",
          "form": "꿉꿉하고",
          "original_form": "꿉꿉하고",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100041813.1.10",
          "form": "덜 마른 냄새나고ㅜㅠ",
          "original_form": "덜 마른 냄새나고ㅜㅠ",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100041813.1.11",
          "form": "언제쯤 비가 그만 올까요ㅠㅠ",
          "original_form": "언제쯤 비가 그만 올까요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100041813.1.12",
          "form": "건조기 있으세요?",
          "original_form": "건조기 있으세요?",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100041813.1.13",
          "form": "전 기숙사살아서",
          "original_form": "전 기숙사살아서",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100041813.1.14",
          "form": "500원 내면 1시간돌릴수있어요",
          "original_form": "500원 내면 1시간돌릴수있어요",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100041813.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100041813.1.16",
          "form": "바지빼고 건조기 꼭 돌린답니다....",
          "original_form": "바지빼고 건조기 꼭 돌린답니다....",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100041813.1.17",
          "form": "헉 대박 너무 좋네요... 500원에 한시간..",
          "original_form": "헉 대박 너무 좋네요... 500원에 한시간..",
          "speaker_id": "1",
          "time": "20210907 06:32"
        },
        {
          "id": "MDRW2100041813.1.18",
          "form": "진짜 매번 사용할것같아요",
          "original_form": "진짜 매번 사용할것같아요",
          "speaker_id": "1",
          "time": "20210907 06:32"
        },
        {
          "id": "MDRW2100041813.1.19",
          "form": "맞아요 건조기는",
          "original_form": "맞아요 건조기는",
          "speaker_id": "2",
          "time": "20210907 11:01"
        },
        {
          "id": "MDRW2100041813.1.20",
          "form": "삶의 질을 이만배 올려줘요",
          "original_form": "삶의 질을 이만배 올려줘요",
          "speaker_id": "2",
          "time": "20210907 11:01"
        },
        {
          "id": "MDRW2100041813.1.21",
          "form": "방에 말릴자리가 없기도하고ㅋ쿠ㅠ",
          "original_form": "방에 말릴자리가 없기도하고ㅋ쿠ㅠ",
          "speaker_id": "2",
          "time": "20210907 11:01"
        },
        {
          "id": "MDRW2100041813.1.22",
          "form": "저도 진짜 집이 좁아서 ㅋㅋㅋ 건조기도 못두고있거든요...",
          "original_form": "저도 진짜 집이 좁아서 ㅋㅋㅋ 건조기도 못두고있거든요...",
          "speaker_id": "1",
          "time": "20210907 11:02"
        },
        {
          "id": "MDRW2100041813.1.23",
          "form": "빨래만 하면 어찌 이리 비가 좔좔 내리는지 억울한 수준이에요",
          "original_form": "빨래만 하면 어찌 이리 비가 좔좔 내리는지 억울한 수준이에요",
          "speaker_id": "1",
          "time": "20210907 11:02"
        },
        {
          "id": "MDRW2100041813.1.24",
          "form": "ㅠㅠ 날씨 눈치챙겨!!!",
          "original_form": "ㅠㅠ 날씨 눈치챙겨!!!",
          "speaker_id": "2",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100041813.1.25",
          "form": "진짜 눈치챙겨...",
          "original_form": "진짜 눈치챙겨...",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100041813.1.26",
          "form": "제 걸음걸이가 이상해서 맨날 종아리에 빗물이 튀는걸까요..ㅠ",
          "original_form": "제 걸음걸이가 이상해서 맨날 종아리에 빗물이 튀는걸까요..ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100041813.1.27",
          "form": "어 그거 저도그래요ㅜㅜ",
          "original_form": "어 그거 저도그래요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100041813.1.28",
          "form": "그 오염돼서 까만 빗물",
          "original_form": "그 오염돼서 까만 빗물",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100041813.1.29",
          "form": "방울방울 튀어잇어요",
          "original_form": "방울방울 튀어잇어요",
          "speaker_id": "2",
          "time": "20210907 19:42"
        }
      ]
    }
