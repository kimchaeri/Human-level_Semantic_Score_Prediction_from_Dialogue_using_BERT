        },
        {
          "id": "MDRW2100009592.1.2",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210823 01:16"
        },
        {
          "id": "MDRW2100009592.1.3",
          "form": "지금 몇신데 아직까지 안자",
          "original_form": "지금 몇신데 아직까지 안자",
          "speaker_id": "1",
          "time": "20210823 01:16"
        },
        {
          "id": "MDRW2100009592.1.4",
          "form": "폰 좀만 더 하다 잘게",
          "original_form": "폰 좀만 더 하다 잘게",
          "speaker_id": "2",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009592.1.5",
          "form": "불끄고 하면 눈나빠지니까",
          "original_form": "불끄고 하면 눈나빠지니까",
          "speaker_id": "1",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009592.1.6",
          "form": "얼른 끄고",
          "original_form": "얼른 끄고",
          "speaker_id": "1",
          "time": "20210823 01:18"
        },
        {
          "id": "MDRW2100009592.1.7",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210823 01:18"
        },
        {
          "id": "MDRW2100009592.1.8",
          "form": "알겠어요",
          "original_form": "알겠어요",
          "speaker_id": "2",
          "time": "20210823 01:18"
        },
        {
          "id": "MDRW2100009592.1.9",
          "form": "엄마 내일 시장갈거야?",
          "original_form": "엄마 내일 시장갈거야?",
          "speaker_id": "2",
          "time": "20210823 01:19"
        },
        {
          "id": "MDRW2100009592.1.10",
          "form": "반찬이 없어서 장을보긴 봐야되는데",
          "original_form": "반찬이 없어서 장을보긴 봐야되는데",
          "speaker_id": "1",
          "time": "20210823 01:21"
        },
        {
          "id": "MDRW2100009592.1.11",
          "form": "태풍온대서 모르겠다",
          "original_form": "태풍온대서 모르겠다",
          "speaker_id": "1",
          "time": "20210823 01:22"
        },
        {
          "id": "MDRW2100009592.1.12",
          "form": "그럼 온라인으로봐요",
          "original_form": "그럼 온라인으로봐요",
          "speaker_id": "2",
          "time": "20210823 01:22"
        },
        {
          "id": "MDRW2100009592.1.13",
          "form": "뭐먹고싶은거 있어?",
          "original_form": "뭐먹고싶은거 있어?",
          "speaker_id": "1",
          "time": "20210823 01:23"
        },
        {
          "id": "MDRW2100009592.1.14",
          "form": "불고기",
          "original_form": "불고기",
          "speaker_id": "2",
          "time": "20210823 01:24"
        },
        {
          "id": "MDRW2100009592.1.15",
          "form": "알겠어 해줄게",
          "original_form": "알겠어 해줄게",
          "speaker_id": "1",
          "time": "20210823 01:25"
        },
        {
          "id": "MDRW2100009592.1.16",
          "form": "넴",
          "original_form": "넴",
          "speaker_id": "2",
          "time": "20210823 01:25"
        },
        {
          "id": "MDRW2100009592.1.17",
          "form": "온라인으로 장 보는거 도와줄래?",
