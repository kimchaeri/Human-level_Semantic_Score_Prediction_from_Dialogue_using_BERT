      "utterance": [
        {
          "id": "MDRW2100033765.1.1",
          "form": "이게 그 괴롭힘과 친절 경계에 있으면 잘몰라요;",
          "original_form": "이게 그 괴롭힘과 친절 경계에 있으면 잘몰라요;",
          "speaker_id": "2",
          "time": "20210825 01:31"
        },
        {
          "id": "MDRW2100033765.1.2",
          "form": "음.",
          "original_form": "음.",
          "speaker_id": "2",
          "time": "20210825 01:31"
        },
        {
          "id": "MDRW2100033765.1.3",
          "form": "저 내일 짜장면먹을거예요",
          "original_form": "저 내일 짜장면먹을거예요",
          "speaker_id": "2",
          "time": "20210825 01:31"
        },
        {
          "id": "MDRW2100033765.1.4",
          "form": "오늘도 중식 드셨잖아요",
          "original_form": "오늘도 중식 드셨잖아요",
          "speaker_id": "1",
          "time": "20210825 01:31"
        },
        {
          "id": "MDRW2100033765.1.5",
          "form": "단골집2로 플랜B #가보자고",
          "original_form": "단골집2로 플랜B #가보자고",
          "speaker_id": "2",
          "time": "20210825 01:31"
        },
        {
          "id": "MDRW2100033765.1.6",
          "form": "아뇨.",
          "original_form": "아뇨.",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033765.1.7",
          "form": "그건 피탕.",
          "original_form": "그건 피탕.",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033765.1.8",
          "form": "아나 단골집 2가 있나요",
          "original_form": "아나 단골집 2가 있나요",
          "speaker_id": "1",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033765.1.9",
          "form": "피자탕수육이 그건가요",
          "original_form": "피자탕수육이 그건가요",
          "speaker_id": "1",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033765.1.10",
          "form": "중식아니라고요~~! 네",
          "original_form": "중식아니라고요~~! 네",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033765.1.11",
          "form": "그 피탕은 중국음식보다는 그냥 음식.",
          "original_form": "그 피탕은 중국음식보다는 그냥 음식.",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033765.1.12",
          "form": "피자맛 소스에ㅜ탕수육 적셔진",
          "original_form": "피자맛 소스에ㅜ탕수육 적셔진",
          "speaker_id": "1",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033765.1.13",
          "form": "ㅋㅋㅋㅋㅋㅌㅋㅋㅋㅌㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅌㅋㅋㅋㅌㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033765.1.14",
          "form": "아니 아ㅏ니요!!!!!!!",
          "original_form": "아니 아ㅏ니요!!!!!!!",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033765.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033765.1.16",
          "form": "ㅇㄴ 확고하시네",
          "original_form": "ㅇㄴ 확고하시네",
          "speaker_id": "1",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033765.1.17",
          "form": "피자치즈탕수육이라고요~~~~!",
          "original_form": "피자치즈탕수육이라고요~~~~!",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033765.1.18",
          "form": "님 윤두준이 먹는거 못보셨어요?",
          "original_form": "님 윤두준이 먹는거 못보셨어요?",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033765.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.20",
          "form": "쓰읍",
          "original_form": "쓰읍",
          "speaker_id": "1",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.21",
          "form": "제가 놓친게 만아요",
          "original_form": "제가 놓친게 만아요",
          "speaker_id": "1",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.22",
          "form": "사느라 바빠서(?)",
          "original_form": "사느라 바빠서(?)",
          "speaker_id": "1",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.23",
          "form": "하 ...쓰읍..",
          "original_form": "하 ...쓰읍..",
          "speaker_id": "2",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.24",
          "form": "볼게요",
          "original_form": "볼게요",
          "speaker_id": "1",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.25",
          "form": "아니 제가",
          "original_form": "아니 제가",
          "speaker_id": "1",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.27",
          "form": "치피탕을",
          "original_form": "치피탕을",
          "speaker_id": "1",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.28",
          "form": "대1때 사실 첨먹어봤거든요",
          "original_form": "대1때 사실 첨먹어봤거든요",
          "speaker_id": "1",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.29",
          "form": "근데 그거 맛있어요 약간 그 비주얼이 음쓰같은게 문제라서",
          "original_form": "근데 그거 맛있어요 약간 그 비주얼이 음쓰같은게 문제라서",
          "speaker_id": "2",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.30",
          "form": "그 치피탕 파는 중국집 가서",
          "original_form": "그 치피탕 파는 중국집 가서",
          "speaker_id": "1",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.31",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "2",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.32",
          "form": "거기 안에서 못먹게해가지고",
          "original_form": "거기 안에서 못먹게해가지고",
          "speaker_id": "1",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.33",
          "form": "아안돼 그거 전문점 가야하는데....",
          "original_form": "아안돼 그거 전문점 가야하는데....",
          "speaker_id": "2",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.34",
          "form": "하...쓰읍...",
          "original_form": "하...쓰읍...",
          "speaker_id": "2",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.35",
          "form": "근처 맥주팔고",
          "original_form": "근처 맥주팔고",
          "speaker_id": "1",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033765.1.36",
          "form": "안주 가져갈수있는 집으로 가서",
          "original_form": "안주 가져갈수있는 집으로 가서",
          "speaker_id": "1",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100033765.1.37",
          "form": "먹었는데",
          "original_form": "먹었는데",
          "speaker_id": "1",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100033765.1.38",
          "form": "넘 별로인거에요",
          "original_form": "넘 별로인거에요",
          "speaker_id": "1",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100033765.1.39",
          "form": "그니까요 전문집이 아니었어요",
          "original_form": "그니까요 전문집이 아니었어요",
          "speaker_id": "1",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100033765.1.40",
          "form": "하 진짜 님...그건 피탕전문집가야해요",
          "original_form": "하 진짜 님...그건 피탕전문집가야해요",
          "speaker_id": "2",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100033765.1.41",
          "form": "이게 좀 못하는 곳 가면 진짜 비주얼도 맛도 음쓰되니까",
          "original_form": "이게 좀 못하는 곳 가면 진짜 비주얼도 맛도 음쓰되니까",
          "speaker_id": "2",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100033765.1.42",
          "form": "음쓰 맙아요",
          "original_form": "음쓰 맙아요",
          "speaker_id": "1",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100033765.1.43",
          "form": "딱 그느낌",
          "original_form": "딱 그느낌",
          "speaker_id": "1",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100033765.1.44",
          "form": "큰일나요 하...진짜...제가 다 아쉽네요",
          "original_form": "큰일나요 하...진짜...제가 다 아쉽네요",
