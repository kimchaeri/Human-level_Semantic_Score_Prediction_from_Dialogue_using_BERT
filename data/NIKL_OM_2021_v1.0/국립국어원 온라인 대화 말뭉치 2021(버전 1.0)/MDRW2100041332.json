      "utterance": [
        {
          "id": "MDRW2100041332.1.1",
          "form": "난 거의 사이버연애야",
          "original_form": "난 거의 사이버연애야",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041332.1.2",
          "form": "롱디 진짜 힘들겠다",
          "original_form": "롱디 진짜 힘들겠다",
          "speaker_id": "1",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100041332.1.3",
          "form": "처음부터 롱디였어?",
          "original_form": "처음부터 롱디였어?",
          "speaker_id": "1",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100041332.1.4",
          "form": "아니ㅜㅜ 한 4년만나고 유학갔어",
          "original_form": "아니ㅜㅜ 한 4년만나고 유학갔어",
          "speaker_id": "2",
          "time": "20210905 22:16"
        },
        {
          "id": "MDRW2100041332.1.5",
          "form": "와 길게 만났구나ㅜㅜ",
          "original_form": "와 길게 만났구나ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100041332.1.6",
          "form": "긴 연애는 뭔가 다르긴해",
          "original_form": "긴 연애는 뭔가 다르긴해",
          "speaker_id": "1",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100041332.1.7",
          "form": "모르겠어 인연이 아닌건지",
          "original_form": "모르겠어 인연이 아닌건지",
          "speaker_id": "2",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100041332.1.8",
          "form": "이제 좀 질려ㅠㅠ",
          "original_form": "이제 좀 질려ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100041332.1.9",
          "form": "사람이 아니라 이 상황이 질려",
          "original_form": "사람이 아니라 이 상황이 질려",
          "speaker_id": "2",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100041332.1.10",
          "form": "ㅠㅜ그런생각이들만해",
          "original_form": "ㅠㅜ그런생각이들만해",
          "speaker_id": "1",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100041332.1.11",
          "form": "지쳤구나",
          "original_form": "지쳤구나",
          "speaker_id": "1",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100041332.1.12",
          "form": "응 맞아ㅠㅠ 그런거같아 ...",
          "original_form": "응 맞아ㅠㅠ 그런거같아 ...",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100041332.1.13",
          "form": "맨날 영상통화하는데 이제 난 화내 맨날 ㅋㅋ",
          "original_form": "맨날 영상통화하는데 이제 난 화내 맨날 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100041332.1.14",
          "form": "귀찮다고 끊으라고ㅠㅠ",
          "original_form": "귀찮다고 끊으라고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100041332.1.15",
          "form": "그러고나몬 또  미안해 이거반복이야...",
          "original_form": "그러고나몬 또  미안해 이거반복이야...",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100041332.1.16",
          "form": "화내는 너도 마음이 안좋지ㅠㅠ",
          "original_form": "화내는 너도 마음이 안좋지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100041332.1.17",
          "form": "아휴ㅠㅜ",
          "original_form": "아휴ㅠㅜ",
          "speaker_id": "1",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100041332.1.18",
          "form": "그래요 ... 끝내는게 맞을지ㅠㅠ 부쩍 고민이.많아요",
          "original_form": "그래요 ... 끝내는게 맞을지ㅠㅠ 부쩍 고민이.많아요",
          "speaker_id": "2",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100041332.1.19",
          "form": "사람은 너무너무 좋은데",
          "original_form": "사람은 너무너무 좋은데",
          "speaker_id": "2",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100041332.1.20",
          "form": "에잇ㅠㅜ음 그건사실음..무슨선택을하든 후회를하지않는게좋지ㅠ",
          "original_form": "에잇ㅠㅜ음 그건사실음..무슨선택을하든 후회를하지않는게좋지ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100041332.1.21",
          "form": "나는지치더라도끝까지가서1도미련이안남는게좋은것같아",
          "original_form": "나는지치더라도끝까지가서1도미련이안남는게좋은것같아",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100041332.1.22",
          "form": "안그러면계속생각나더라고..",
          "original_form": "안그러면계속생각나더라고..",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100041332.1.23",
          "form": "그렇지??? 맞아 내가 먼저 차면 평생 후회할거야 ...",
          "original_form": "그렇지??? 맞아 내가 먼저 차면 평생 후회할거야 ...",
          "speaker_id": "2",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100041332.1.24",
          "form": "차라리 차이는게 미련이 안남을거야 ㅋㅋㅋㅋ",
          "original_form": "차라리 차이는게 미련이 안남을거야 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100041332.1.25",
          "form": "차든차이든 할수있는거다해보면 미련이안남지ㅠㅜ",
          "original_form": "차든차이든 할수있는거다해보면 미련이안남지ㅠㅜ",
          "speaker_id": "1",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100041332.1.26",
          "form": "딴남자만나도안잊히드라구ㅜㅜ",
          "original_form": "딴남자만나도안잊히드라구ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 22:36"
        }
      ]
    }
