      "utterance": [
        {
          "id": "MDRW2100028493.1.1",
          "form": "넷플릭스봐?",
          "original_form": "넷플릭스봐?",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028493.1.2",
          "form": "넷플릭스 봐",
          "original_form": "넷플릭스 봐",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028493.1.3",
          "form": "넌 요즘 안보니?",
          "original_form": "넌 요즘 안보니?",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028493.1.4",
          "form": "넷플릭스 디피 봤니?  정말 재밌엉",
          "original_form": "넷플릭스 디피 봤니?  정말 재밌엉",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028493.1.5",
          "form": "나도 그거 봐야겠다",
          "original_form": "나도 그거 봐야겠다",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028493.1.6",
          "form": "난 지금 스트릿 우먼 파이터 보고 있어",
          "original_form": "난 지금 스트릿 우먼 파이터 보고 있어",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028493.1.7",
          "form": "연기가 다들 미쳤음 그거 댄스대결 하는거?",
          "original_form": "연기가 다들 미쳤음 그거 댄스대결 하는거?",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028493.1.8",
          "form": "맞아ㅋㅋㅋ 어그로 끄는거 재밌어",
          "original_form": "맞아ㅋㅋㅋ 어그로 끄는거 재밌어",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028493.1.9",
          "form": "움짤로만 봤어 나도 봐야지 기빨릴거같은데 재밌을거같라",
          "original_form": "움짤로만 봤어 나도 봐야지 기빨릴거같은데 재밌을거같라",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028493.1.10",
          "form": "재밌어 다 춤 잘춘다",
          "original_form": "재밌어 다 춤 잘춘다",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028493.1.11",
          "form": "나도 춤 잘추고 싶어",
          "original_form": "나도 춤 잘추고 싶어",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028493.1.12",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028493.1.13",
          "form": "이거 보면서 대리만족하자",
          "original_form": "이거 보면서 대리만족하자",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028493.1.14",
          "form": "그래 다시 태어나야할듯",
          "original_form": "그래 다시 태어나야할듯",
          "speaker_id": "1",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028493.1.15",
          "form": "태민이 보고싶다",
          "original_form": "태민이 보고싶다",
          "speaker_id": "1",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028493.1.16",
          "form": "태민이 잘 지내니?",
          "original_form": "태민이 잘 지내니?",
          "speaker_id": "2",
          "time": "20210902 20:33"
        },
        {
          "id": "MDRW2100028493.1.17",
          "form": "태민이 군대감..ㅎ..",
          "original_form": "태민이 군대감..ㅎ..",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028493.1.18",
          "form": "인생노잼이다",
          "original_form": "인생노잼이다",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028493.1.19",
          "form": "군대에서 소식 없니?",
          "original_form": "군대에서 소식 없니?",
          "speaker_id": "2",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028493.1.20",
          "form": "없어 꽁꽁 감춰 국방부가 빡빡이 내놔라",
          "original_form": "없어 꽁꽁 감춰 국방부가 빡빡이 내놔라",
          "speaker_id": "1",
          "time": "20210902 20:58"
        }
      ]
