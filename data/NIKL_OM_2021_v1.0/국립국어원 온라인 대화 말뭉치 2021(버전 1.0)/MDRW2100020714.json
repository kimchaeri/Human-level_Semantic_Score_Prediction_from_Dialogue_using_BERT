      "utterance": [
        {
          "id": "MDRW2100020714.1.1",
          "form": "헐 고수",
          "original_form": "헐 고수",
          "speaker_id": "1",
          "time": "20210828 11:31"
        },
        {
          "id": "MDRW2100020714.1.2",
          "form": "못먹어요ㅋㅋㅋㅋㅋ",
          "original_form": "못먹어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:31"
        },
        {
          "id": "MDRW2100020714.1.3",
          "form": "다행이네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "다행이네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:31"
        },
        {
          "id": "MDRW2100020714.1.4",
          "form": "그거 저 쌀국수 먹자마자 뱉었어요",
          "original_form": "그거 저 쌀국수 먹자마자 뱉었어요",
          "speaker_id": "1",
          "time": "20210828 11:31"
        },
        {
          "id": "MDRW2100020714.1.5",
          "form": "아니 진짜 말만 들어봤지ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니 진짜 말만 들어봤지ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:31"
        },
        {
          "id": "MDRW2100020714.1.6",
          "form": "와 저도 저 진짜 웬만하면 풀 안가리는데",
          "original_form": "와 저도 저 진짜 웬만하면 풀 안가리는데",
          "speaker_id": "2",
          "time": "20210828 11:31"
        },
        {
          "id": "MDRW2100020714.1.7",
          "form": "그런맛이 존재한다는게 충격",
          "original_form": "그런맛이 존재한다는게 충격",
          "speaker_id": "1",
          "time": "20210828 11:31"
        },
        {
          "id": "MDRW2100020714.1.8",
          "form": "살다살다 풀잎을 씹었는데 향 때문에 이가 아리더라구요",
          "original_form": "살다살다 풀잎을 씹었는데 향 때문에 이가 아리더라구요",
          "speaker_id": "2",
          "time": "20210828 11:32"
        },
        {
          "id": "MDRW2100020714.1.9",
          "form": "근데 그거 또 맛있게 먹는 사람은",
          "original_form": "근데 그거 또 맛있게 먹는 사람은",
          "speaker_id": "1",
          "time": "20210828 11:32"
        },
        {
          "id": "MDRW2100020714.1.10",
          "form": "잘먹더라구요",
          "original_form": "잘먹더라구요",
          "speaker_id": "1",
          "time": "20210828 11:32"
        },
        {
          "id": "MDRW2100020714.1.11",
          "form": "name1님은 어떤 향을 느끼셨어요",
          "original_form": "&name1&님은 어떤 향을 느끼셨어요",
          "speaker_id": "2",
          "time": "20210828 11:32"
        },
        {
          "id": "MDRW2100020714.1.12",
          "form": "아무리먹어도 퐁퐁맛",
          "original_form": "아무리먹어도 퐁퐁맛",
          "speaker_id": "1",
          "time": "20210828 11:32"
        },
        {
          "id": "MDRW2100020714.1.13",
          "form": "맞아요 제 친구들도 다 잘먹어요",
          "original_form": "맞아요 제 친구들도 다 잘먹어요",
          "speaker_id": "2",
          "time": "20210828 11:32"
        },
        {
          "id": "MDRW2100020714.1.14",
          "form": "와 퐁퐁한사발 들이킨줄",
          "original_form": "와 퐁퐁한사발 들이킨줄",
          "speaker_id": "1",
          "time": "20210828 11:32"
        },
        {
          "id": "MDRW2100020714.1.15",
          "form": "저는 뭔가.. 석유맛?",
          "original_form": "저는 뭔가.. 석유맛?",
          "speaker_id": "2",
          "time": "20210828 11:32"
        },
        {
          "id": "MDRW2100020714.1.16",
