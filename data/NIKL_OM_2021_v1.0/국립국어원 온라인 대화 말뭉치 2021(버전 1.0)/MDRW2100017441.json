      "utterance": [
        {
          "id": "MDRW2100017441.1.1",
          "form": "간식은 뭐좋아하세요?",
          "original_form": "간식은 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017441.1.2",
          "form": "아이스크림이요!",
          "original_form": "아이스크림이요!",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017441.1.3",
          "form": "베스킨라빈스",
          "original_form": "베스킨라빈스",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017441.1.4",
          "form": "ㅋㅋㅋㅋ 많이시켜먹엇어요",
          "original_form": "ㅋㅋㅋㅋ 많이시켜먹엇어요",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017441.1.5",
          "form": "과자도 .. 아몬드도 군옥수수맛아몬드",
          "original_form": "과자도 .. 아몬드도 군옥수수맛아몬드",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017441.1.6",
          "form": "요즘 파바 매일 할인해서...",
          "original_form": "요즘 파바 매일 할인해서...",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017441.1.7",
          "form": "전 인절미과자 너무 좋아요",
          "original_form": "전 인절미과자 너무 좋아요",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017441.1.8",
          "form": "아 그...속 빈거요?",
          "original_form": "아 그...속 빈거요?",
          "speaker_id": "1",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017441.1.9",
          "form": "꼬북칩이랑",
          "original_form": "꼬북칩이랑",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017441.1.10",
          "form": "저도그거조아해욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도그거조아해욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017441.1.11",
          "form": "깃털같은거 ㅋㅋ",
          "original_form": "깃털같은거 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017441.1.12",
          "form": "그런식감 좋아하시는구나!",
          "original_form": "그런식감 좋아하시는구나!",
          "speaker_id": "1",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017441.1.13",
          "form": "ㅋㅋ사실 못먹는게 없어요 마카롱 버터바 이런것도!",
          "original_form": "ㅋㅋ사실 못먹는게 없어요 마카롱 버터바 이런것도!",
          "speaker_id": "2",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017441.1.14",
          "form": "장인약과도 좋아하는데 인기폭발이라 못사고있어요ㅜ",
          "original_form": "장인약과도 좋아하는데 인기폭발이라 못사고있어요ㅜ",
          "speaker_id": "2",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017441.1.15",
          "form": "그게 그렇게 맛있다나요?  첨들어봐요!",
          "original_form": "그게 그렇게 맛있다나요?  첨들어봐요!",
          "speaker_id": "1",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100017441.1.16",
          "form": "버터바도!",
          "original_form": "버터바도!",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100017441.1.17",
          "form": "아 진짜 찐득찐득하고 이가 녹을듯한 단맛이에요",
          "original_form": "아 진짜 찐득찐득하고 이가 녹을듯한 단맛이에요",
          "speaker_id": "2",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100017441.1.18",
          "form": "거기 약과먹고 다른 시중 약과는 못먹겠어요ㅜ",
          "original_form": "거기 약과먹고 다른 시중 약과는 못먹겠어요ㅜ",
          "speaker_id": "2",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100017441.1.19",
          "form": "헉.... 저도 담에꼭 시도해봐야겠어요",
          "original_form": "헉.... 저도 담에꼭 시도해봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100017441.1.20",
          "form": "제 개인적 취향일수도 있지만 요즘 난리난거 보면 확실히 맛있는거 같아요",
          "original_form": "제 개인적 취향일수도 있지만 요즘 난리난거 보면 확실히 맛있는거 같아요",
          "speaker_id": "2",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100017441.1.21",
          "form": "ㅋㅋㅋㅋ 난리난줄도 몰랏어요 추석 앞두고 더그렇겠다",
          "original_form": "ㅋㅋㅋㅋ 난리난줄도 몰랏어요 추석 앞두고 더그렇겠다",
          "speaker_id": "1",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100017441.1.22",
          "form": "파지라고 모양 망가진 약과를 파는데...ㅜㅜ 머지않아 네이버쇼핑에 입점할거라는 소문은 봤어요 꼭 드셔보세요!",
          "original_form": "파지라고 모양 망가진 약과를 파는데...ㅜㅜ 머지않아 네이버쇼핑에 입점할거라는 소문은 봤어요 꼭 드셔보세요!",
          "speaker_id": "2",
          "time": "20210826 22:22"
        }
      ]
    }
  ]
