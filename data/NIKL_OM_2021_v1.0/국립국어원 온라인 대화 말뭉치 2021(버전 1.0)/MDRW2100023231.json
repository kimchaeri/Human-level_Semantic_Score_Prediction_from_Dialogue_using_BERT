      "utterance": [
        {
          "id": "MDRW2100023231.1.1",
          "form": "임용고시는 언제부터 준비하셨어요?",
          "original_form": "임용고시는 언제부터 준비하셨어요?",
          "speaker_id": "1",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100023231.1.2",
          "form": "저는 4학년때 시작하긴 했는데",
          "original_form": "저는 4학년때 시작하긴 했는데",
          "speaker_id": "2",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100023231.1.3",
          "form": "오 저도 4학년 때부터 했어요 헤헤",
          "original_form": "오 저도 4학년 때부터 했어요 헤헤",
          "speaker_id": "1",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100023231.1.4",
          "form": "뭔가 성격이 교사랑 안맞는 거 같아서",
          "original_form": "뭔가 성격이 교사랑 안맞는 거 같아서",
          "speaker_id": "2",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100023231.1.5",
          "form": "제대로 준비 안했던거 같아요 ㅋㅋㅋㅋ",
          "original_form": "제대로 준비 안했던거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100023231.1.6",
          "form": "헐 정말요? ㅜㅜ",
          "original_form": "헐 정말요? ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100023231.1.7",
          "form": "그럼 그때 방황하셨어요?",
          "original_form": "그럼 그때 방황하셨어요?",
          "speaker_id": "1",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100023231.1.8",
          "form": "넼ㅋㅋㅋㅋ그러다 기간제 해보고 결정하자 해서 학교에서 일했는데",
          "original_form": "넼ㅋㅋㅋㅋ그러다 기간제 해보고 결정하자 해서 학교에서 일했는데",
          "speaker_id": "2",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100023231.1.9",
          "form": "기간제는 졸업하고 나서 하신거죠?",
          "original_form": "기간제는 졸업하고 나서 하신거죠?",
          "speaker_id": "1",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100023231.1.10",
          "form": "너무 좋은 학교에서 일했더니 행복하게 살아지더라구요",
          "original_form": "너무 좋은 학교에서 일했더니 행복하게 살아지더라구요",
          "speaker_id": "2",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100023231.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100023231.1.12",
          "form": "첫 경험이 좋으셔서 다행이네요~",
          "original_form": "첫 경험이 좋으셔서 다행이네요~",
          "speaker_id": "1",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100023231.1.13",
          "form": "그래서 뭔가 각잡고 해야겠다 했던 기억이 나요",
          "original_form": "그래서 뭔가 각잡고 해야겠다 했던 기억이 나요",
          "speaker_id": "2",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100023231.1.14",
          "form": "근데 임용고시 치기 전에 교생실습도 하지 않으셨어요??",
          "original_form": "근데 임용고시 치기 전에 교생실습도 하지 않으셨어요??",
          "speaker_id": "1",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100023231.1.15",
          "form": "했지만은 사실 교생실습으로 뭘 깨우치긴 좀 짧은 시간이기도 하고",
          "original_form": "했지만은 사실 교생실습으로 뭘 깨우치긴 좀 짧은 시간이기도 하고",
          "speaker_id": "2",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100023231.1.16",
          "form": "아~거의 매학년마다 했을텐데 짧으셨군요",
          "original_form": "아~거의 매학년마다 했을텐데 짧으셨군요",
          "speaker_id": "1",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100023231.1.17",
          "form": "아 저흰 4학년때만 하는데",
          "original_form": "아 저흰 4학년때만 하는데",
          "speaker_id": "2",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100023231.1.18",
          "form": "아 사대는 4학년 딱 한번만 해요?",
          "original_form": "아 사대는 4학년 딱 한번만 해요?",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023231.1.19",
          "form": "초등은 엄청 마니하져?",
          "original_form": "초등은 엄청 마니하져?",
          "speaker_id": "2",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023231.1.20",
          "form": "..와 그럼 나 지금까지 가짜 사대생 만났나보다",
          "original_form": "..와 그럼 나 지금까지 가짜 사대생 만났나보다",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023231.1.21",
          "form": "네네 4학년에 딱 한달",
          "original_form": "네네 4학년에 딱 한달",
          "speaker_id": "2",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023231.1.22",
          "form": "교대생은 엄청 자주해요",
          "original_form": "교대생은 엄청 자주해요",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023231.1.23",
          "form": "어떤 학년에는 학기마다 할 때도 있었거든요",
          "original_form": "어떤 학년에는 학기마다 할 때도 있었거든요",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023231.1.24",
          "form": "학기별로 나간다고 들은거같아여 ㅋㅋㅋ",
          "original_form": "학기별로 나간다고 들은거같아여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:07"
        }
      ]
    }
