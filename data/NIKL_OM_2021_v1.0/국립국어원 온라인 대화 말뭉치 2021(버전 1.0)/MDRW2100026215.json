      "utterance": [
        {
          "id": "MDRW2100026215.1.1",
          "form": "오 요즘 날씨 너무 별로야",
          "original_form": "오 요즘 날씨 너무 별로야",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026215.1.2",
          "form": "아니 그니까",
          "original_form": "아니 그니까",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026215.1.3",
          "form": "지금도 비 다시 오는 거 봐",
          "original_form": "지금도 비 다시 오는 거 봐",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026215.1.4",
          "form": "완전 장마야 가을 장마",
          "original_form": "완전 장마야 가을 장마",
          "speaker_id": "2",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026215.1.5",
          "form": "우와...",
          "original_form": "우와...",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026215.1.6",
          "form": "장마 너무 싫다",
          "original_form": "장마 너무 싫다",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026215.1.7",
          "form": "완전 축축해",
          "original_form": "완전 축축해",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026215.1.8",
          "form": "나가기도 귀찮어",
          "original_form": "나가기도 귀찮어",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026215.1.9",
          "form": "그치만... 비 오는 날에 시원한 실내에서 빗소리 듣는 건 좋아",
          "original_form": "그치만... 비 오는 날에 시원한 실내에서 빗소리 듣는 건 좋아",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026215.1.10",
          "form": "카페에서 좀 맛있는 거 먹으면서 비 구경하는 거는 좀 로망 있을지도",
          "original_form": "카페에서 좀 맛있는 거 먹으면서 비 구경하는 거는 좀 로망 있을지도",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100026215.1.11",
          "form": "그치만 그렇다고 굳이 비 오는 날에 카페를 찾아가고 싶지 않아",
          "original_form": "그치만 그렇다고 굳이 비 오는 날에 카페를 찾아가고 싶지 않아",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100026215.1.12",
          "form": "맞아맞아ㅋㅋ",
          "original_form": "맞아맞아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026215.1.13",
          "form": "살짝 비 asmr을 찾아서 듣는... 그 정도의 감성이지",
          "original_form": "살짝 비 asmr을 찾아서 듣는... 그 정도의 감성이지",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026215.1.14",
          "form": "실행으로는 옮기지 않을...",
          "original_form": "실행으로는 옮기지 않을...",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026215.1.15",
          "form": "그니까... 비오는 날의 외출은 파멸이다",
          "original_form": "그니까... 비오는 날의 외출은 파멸이다",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026215.1.16",
          "form": "실상은 웅덩이 잘못 밟아서 축축해진 양말밖에 없어",
          "original_form": "실상은 웅덩이 잘못 밟아서 축축해진 양말밖에 없어",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026215.1.17",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026215.1.18",
          "form": "그렇다고 해가 엄청 쨍한 날씨가 좋냐?",
          "original_form": "그렇다고 해가 엄청 쨍한 날씨가 좋냐?",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026215.1.19",
          "form": "그건 또 아님",
          "original_form": "그건 또 아님",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026215.1.20",
          "form": "그건... 너무 기빨려",
          "original_form": "그건... 너무 기빨려",
          "speaker_id": "1",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100026215.1.21",
          "form": "진짜 무슨 햇빛에 놓인 뱀파이어마냥 기력 쭉 빨려서",
          "original_form": "진짜 무슨 햇빛에 놓인 뱀파이어마냥 기력 쭉 빨려서",
          "speaker_id": "1",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100026215.1.22",
          "form": "걍... 적당히 맑고 적당히 선선하고 적당히 구름낀 날씨가 좋다",
          "original_form": "걍... 적당히 맑고 적당히 선선하고 적당히 구름낀 날씨가 좋다",
          "speaker_id": "1",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100026215.1.23",
          "form": "그치... 딱 가을 날씨가 좋은데",
          "original_form": "그치... 딱 가을 날씨가 좋은데",
          "speaker_id": "2",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100026215.1.24",
          "form": "모기 많은 것만 빼면",
          "original_form": "모기 많은 것만 빼면",
          "speaker_id": "2",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100026215.1.25",
          "form": "그니까...",
          "original_form": "그니까...",
          "speaker_id": "1",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100026215.1.26",
          "form": "근데 원래 모기가 가을에 나왔던가",
          "original_form": "근데 원래 모기가 가을에 나왔던가",
          "speaker_id": "1",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100026215.1.27",
          "form": "그렇지 않나? 가을에 많았던 것 같은데",
          "original_form": "그렇지 않나? 가을에 많았던 것 같은데",
          "speaker_id": "2",
          "time": "20210903 01:06"
        }
      ]
