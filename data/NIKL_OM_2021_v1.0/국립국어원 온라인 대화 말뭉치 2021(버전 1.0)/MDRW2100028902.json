        },
        {
          "id": "MDRW2100028902.1.2",
          "form": "그게뭐죠",
          "original_form": "그게뭐죠",
          "speaker_id": "2",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028902.1.3",
          "form": "일본잡진데",
          "original_form": "일본잡진데",
          "speaker_id": "1",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028902.1.4",
          "form": "재있나요",
          "original_form": "재있나요",
          "speaker_id": "2",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028902.1.5",
          "form": "음 글쎄요",
          "original_form": "음 글쎄요",
          "speaker_id": "1",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028902.1.6",
          "form": "그럼 왜보세요?",
          "original_form": "그럼 왜보세요?",
          "speaker_id": "2",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028902.1.7",
          "form": "아 보진않아요",
          "original_form": "아 보진않아요",
          "speaker_id": "1",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028902.1.8",
          "form": "아 그냥 아냐고 물으신거구나",
          "original_form": "아 그냥 아냐고 물으신거구나",
          "speaker_id": "2",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028902.1.9",
          "form": "넹ㅋㅋ",
          "original_form": "넹ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028902.1.10",
          "form": "요즘 핫한 책인가 햏머요",
          "original_form": "요즘 핫한 책인가 햏머요",
          "speaker_id": "2",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028902.1.11",
          "form": "핫한 잡진가요",
          "original_form": "핫한 잡진가요",
          "speaker_id": "2",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028902.1.12",
          "form": "요즘 핫한건 잘 몰라용ㅜ",
          "original_form": "요즘 핫한건 잘 몰라용ㅜ",
          "speaker_id": "1",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028902.1.13",
          "form": "책 안읽은지 꽤돼서",
          "original_form": "책 안읽은지 꽤돼서",
          "speaker_id": "2",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028902.1.14",
          "form": "전 도서관 가는 거 좋아해요",
          "original_form": "전 도서관 가는 거 좋아해요",
          "speaker_id": "1",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028902.1.15",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "2",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028902.1.16",
          "form": "최근에 여행기읽었는데 좋더라구요",
          "original_form": "최근에 여행기읽었는데 좋더라구요",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028902.1.17",
          "form": "어디여행긴가요",
          "original_form": "어디여행긴가요",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028902.1.18",
          "form": "대만입니당",
          "original_form": "대만입니당",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028902.1.19",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
