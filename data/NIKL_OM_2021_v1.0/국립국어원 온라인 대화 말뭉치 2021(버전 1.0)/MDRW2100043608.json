      "utterance": [
        {
          "id": "MDRW2100043608.1.1",
          "form": "샌드위치  음료는어떤걸루",
          "original_form": "샌드위치  음료는어떤걸루",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043608.1.2",
          "form": "아이스라떼요ㅋㅋㅋㅋ",
          "original_form": "아이스라떼요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043608.1.3",
          "form": "시럽넣어드시나여?",
          "original_form": "시럽넣어드시나여?",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043608.1.4",
          "form": "네 그냥 마셔요 ㅎㅎ 시럽넣어서 드시나요?",
          "original_form": "네 그냥 마셔요 ㅎㅎ 시럽넣어서 드시나요?",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043608.1.5",
          "form": "전 단걸좋아해서요 ㅎㅎ그냥은못먹어요 ㅎㅎㅎ",
          "original_form": "전 단걸좋아해서요 ㅎㅎ그냥은못먹어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043608.1.6",
          "form": "카페는 어디자주가세여?",
          "original_form": "카페는 어디자주가세여?",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043608.1.7",
          "form": "단게 땡길때도 있는데 ㅎㅎ 오늘은 차게 얼른 먹을려구요",
          "original_form": "단게 땡길때도 있는데 ㅎㅎ 오늘은 차게 얼른 먹을려구요",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043608.1.8",
          "form": "카페는 근처에 있는 프렌차이즈 가게 되요..ㅎㅎ",
          "original_form": "카페는 근처에 있는 프렌차이즈 가게 되요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043608.1.9",
          "form": "이디야랑 컴포즈요",
          "original_form": "이디야랑 컴포즈요",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043608.1.10",
          "form": "ㅎㅎ이디야 저 일했었는데 어떤음료젤좋아하시나여",
          "original_form": "ㅎㅎ이디야 저 일했었는데 어떤음료젤좋아하시나여",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043608.1.11",
          "form": "연유콜드브루요ㅋㅋㅋㅋ",
          "original_form": "연유콜드브루요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043608.1.12",
          "form": "이디야 그리고 자몽네이블도 좋아해호",
          "original_form": "이디야 그리고 자몽네이블도 좋아해호",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043608.1.13",
          "form": "겨울에는 거의 그거 마셨어요",
          "original_form": "겨울에는 거의 그거 마셨어요",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043608.1.14",
          "form": "ㅋㅋㅋㅋ맞아요 저도 자몽네이븧 엄청먹었어요 은근하게 맛있어요",
          "original_form": "ㅋㅋㅋㅋ맞아요 저도 자몽네이븧 엄청먹었어요 은근하게 맛있어요",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043608.1.15",
          "form": "네 달달한데 씁쓸한게 취향저격이였어요",
          "original_form": "네 달달한데 씁쓸한게 취향저격이였어요",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043608.1.16",
          "form": "ㅋㅋㅋㅋㅋ맞아여 적당한밸런스의 맛",
          "original_form": "ㅋㅋㅋㅋㅋ맞아여 적당한밸런스의 맛",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043608.1.17",
          "form": "평소 좋아하는 카페메뉴는 무엇인가요?",
          "original_form": "평소 좋아하는 카페메뉴는 무엇인가요?",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043608.1.18",
          "form": "전 민트초코좋아하구여 커피는 바닐라라떼요 ㅎㅎ",
          "original_form": "전 민트초코좋아하구여 커피는 바닐라라떼요 ㅎㅎ",
