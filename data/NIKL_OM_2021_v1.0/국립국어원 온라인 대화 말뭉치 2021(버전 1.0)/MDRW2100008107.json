      "utterance": [
        {
          "id": "MDRW2100008107.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008107.1.2",
          "form": "중고거래는 잘 안하고 인터넷쇼핑 좋아해요",
          "original_form": "중고거래는 잘 안하고 인터넷쇼핑 좋아해요",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008107.1.3",
          "form": "머사요?",
          "original_form": "머사요?",
          "speaker_id": "1",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008107.1.4",
          "form": "주로 먹는거 많이 주문해요.",
          "original_form": "주로 먹는거 많이 주문해요.",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008107.1.5",
          "form": "인터넷쇼핑 많이 하시나요?",
          "original_form": "인터넷쇼핑 많이 하시나요?",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008107.1.6",
          "form": "옥션이뇨",
          "original_form": "옥션이뇨",
          "speaker_id": "1",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008107.1.7",
          "form": "스마일클럽이에요",
          "original_form": "스마일클럽이에요",
          "speaker_id": "1",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008107.1.8",
          "form": "오 스마일클럽 한번도 안써봤는데 뭐하는 쇼핑몰이에요?",
          "original_form": "오 스마일클럽 한번도 안써봤는데 뭐하는 쇼핑몰이에요?",
          "speaker_id": "2",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008107.1.9",
          "form": "옥션 회원제요",
          "original_form": "옥션 회원제요",
          "speaker_id": "1",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008107.1.10",
          "form": "아하 저는 요즘 쿠팡만 주로 쓰고있어요",
          "original_form": "아하 저는 요즘 쿠팡만 주로 쓰고있어요",
          "speaker_id": "2",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008107.1.11",
          "form": "쿠팡싸죠?",
          "original_form": "쿠팡싸죠?",
          "speaker_id": "1",
          "time": "20210822 13:30"
        },
        {
          "id": "MDRW2100008107.1.12",
          "form": "넹 가격도 저렴하고 배송도 빨라서 쓰게 되더라고요",
          "original_form": "넹 가격도 저렴하고 배송도 빨라서 쓰게 되더라고요",
          "speaker_id": "2",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008107.1.13",
          "form": "배송이 좋더라구요",
          "original_form": "배송이 좋더라구요",
          "speaker_id": "1",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008107.1.14",
          "form": "배송비 걱정 안하는게 특히 좋아요",
          "original_form": "배송비 걱정 안하는게 특히 좋아요",
          "speaker_id": "2",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008107.1.15",
          "form": "포장도 안해도 되고",
          "original_form": "포장도 안해도 되고",
