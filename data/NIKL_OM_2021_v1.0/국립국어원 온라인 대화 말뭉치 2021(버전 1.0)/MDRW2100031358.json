        {
          "id": "MDRW2100031358.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100031358.1.2",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210904 22:11"
        },
        {
          "id": "MDRW2100031358.1.3",
          "form": "좋져?",
          "original_form": "좋져?",
          "speaker_id": "1",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100031358.1.4",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "2",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100031358.1.5",
          "form": "싫어하는사람은 없겠죠?",
          "original_form": "싫어하는사람은 없겠죠?",
          "speaker_id": "2",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100031358.1.6",
          "form": "아마도요",
          "original_form": "아마도요",
          "speaker_id": "1",
          "time": "20210905 09:40"
        },
        {
          "id": "MDRW2100031358.1.7",
          "form": "어디가 제일좋았나요?",
          "original_form": "어디가 제일좋았나요?",
          "speaker_id": "2",
          "time": "20210905 09:40"
        },
        {
          "id": "MDRW2100031358.1.8",
          "form": "허주",
          "original_form": "허주",
          "speaker_id": "1",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100031358.1.9",
          "form": "호주요",
          "original_form": "호주요",
          "speaker_id": "1",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100031358.1.10",
          "form": "아~~호주요",
          "original_form": "아~~호주요",
          "speaker_id": "2",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100031358.1.11",
          "form": "어때요? 호주는",
          "original_form": "어때요? 호주는",
          "speaker_id": "2",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100031358.1.12",
          "form": "다들 가면 돌아오고싶지않은 나라라고하던데",
          "original_form": "다들 가면 돌아오고싶지않은 나라라고하던데",
          "speaker_id": "2",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100031358.1.13",
          "form": "청정지역이에요",
          "original_form": "청정지역이에요",
          "speaker_id": "1",
          "time": "20210905 09:43"
        },
        {
          "id": "MDRW2100031358.1.14",
          "form": "어디가 좋았나요? 호주중에서",
          "original_form": "어디가 좋았나요? 호주중에서",
          "speaker_id": "2",
          "time": "20210905 09:44"
        },
        {
          "id": "MDRW2100031358.1.15",
          "form": "가보고싶네요",
          "original_form": "가보고싶네요",
          "speaker_id": "2",
          "time": "20210905 09:47"
        },
        {
          "id": "MDRW2100031358.1.16",
          "form": "오페라하우스봤어요",
          "original_form": "오페라하우스봤어요",
          "speaker_id": "1",
          "time": "20210905 09:49"
        },
        {
          "id": "MDRW2100031358.1.17",
          "form": "전 사진으로만..",
          "original_form": "전 사진으로만..",
          "speaker_id": "2",
          "time": "20210905 09:49"
        },
        {
          "id": "MDRW2100031358.1.18",
          "form": "안에들어가보셨나요?",
          "original_form": "안에들어가보셨나요?",
          "speaker_id": "2",
          "time": "20210905 09:50"
        },
        {
          "id": "MDRW2100031358.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100031358.1.20",
          "form": "코로나끝나면 가봐야겠네요",
          "original_form": "코로나끝나면 가봐야겠네요",
          "speaker_id": "2",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100031358.1.21",
          "form": "나이들면 비행기오래못타서 한살이라도젊었을때 가야겠어요",
          "original_form": "나이들면 비행기오래못타서 한살이라도젊었을때 가야겠어요",
          "speaker_id": "2",
          "time": "20210905 09:53"
        },
        {
          "id": "MDRW2100031358.1.22",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210905 10:18"
        }
      ]
