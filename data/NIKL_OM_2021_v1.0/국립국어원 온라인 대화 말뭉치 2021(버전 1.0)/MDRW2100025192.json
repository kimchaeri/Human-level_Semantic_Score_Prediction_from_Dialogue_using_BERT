      "utterance": [
        {
          "id": "MDRW2100025192.1.1",
          "form": "왜! 인터넷 소설 보잖아",
          "original_form": "왜! 인터넷 소설 보잖아",
          "speaker_id": "2",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100025192.1.2",
          "form": "엉 인터넷으로 소설은 보는데 베스터셀러 같은건 안본지 오래",
          "original_form": "엉 인터넷으로 소설은 보는데 베스터셀러 같은건 안본지 오래",
          "speaker_id": "1",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100025192.1.3",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100025192.1.4",
          "form": "뭐가 베스트셀러에 순위에 잇는지도 모르겠다 작년이후로",
          "original_form": "뭐가 베스트셀러에 순위에 잇는지도 모르겠다 작년이후로",
          "speaker_id": "1",
          "time": "20210901 00:30"
        },
        {
          "id": "MDRW2100025192.1.5",
          "form": "검색해봐 그리고 도서관에 빌려",
          "original_form": "검색해봐 그리고 도서관에 빌려",
          "speaker_id": "2",
          "time": "20210901 00:31"
        },
        {
          "id": "MDRW2100025192.1.6",
          "form": "달러구트 꿈 백화점이 베스트셀러네",
          "original_form": "달러구트 꿈 백화점이 베스트셀러네",
          "speaker_id": "1",
          "time": "20210901 00:31"
        },
        {
          "id": "MDRW2100025192.1.7",
          "form": "무슨 내용인데",
          "original_form": "무슨 내용인데",
          "speaker_id": "2",
          "time": "20210901 00:32"
        },
        {
          "id": "MDRW2100025192.1.8",
          "form": "제목그대로인데 ㅋㅋ꿈에 관련된 책인가봐",
          "original_form": "제목그대로인데 ㅋㅋ꿈에 관련된 책인가봐",
          "speaker_id": "1",
          "time": "20210901 00:33"
        },
        {
          "id": "MDRW2100025192.1.9",
          "form": "그래 애들보라하면 좋겠다",
          "original_form": "그래 애들보라하면 좋겠다",
          "speaker_id": "2",
          "time": "20210901 00:33"
        },
        {
          "id": "MDRW2100025192.1.10",
          "form": "그러게 괜찮을듯 작가가 부산출신이다",
          "original_form": "그러게 괜찮을듯 작가가 부산출신이다",
          "speaker_id": "1",
          "time": "20210901 00:33"
        },
        {
          "id": "MDRW2100025192.1.11",
          "form": "구매하면 너두 보여줄께",
          "original_form": "구매하면 너두 보여줄께",
          "speaker_id": "2",
          "time": "20210901 00:34"
        },
        {
          "id": "MDRW2100025192.1.12",
          "form": "고마워",
