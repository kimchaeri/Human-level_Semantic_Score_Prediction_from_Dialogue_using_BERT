      "utterance": [
        {
          "id": "MDRW2100013949.1.1",
          "form": "가보고싶은 여행지 있으신가요",
          "original_form": "가보고싶은 여행지 있으신가요",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013949.1.2",
          "form": "저는 동남아쪽 휴양지 가고싶네용",
          "original_form": "저는 동남아쪽 휴양지 가고싶네용",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013949.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013949.1.4",
          "form": "저는 이탈리아 가보고싶어요",
          "original_form": "저는 이탈리아 가보고싶어요",
          "speaker_id": "1",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013949.1.5",
          "form": "유적지가 많아서 구경할게 많아보이거든요",
          "original_form": "유적지가 많아서 구경할게 많아보이거든요",
          "speaker_id": "1",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013949.1.6",
          "form": "동남아 어디 가고싶으신가요",
          "original_form": "동남아 어디 가고싶으신가요",
          "speaker_id": "1",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013949.1.7",
          "form": "역사 좋아하시면 이탈리아 진짜 좋을 거 같아요",
          "original_form": "역사 좋아하시면 이탈리아 진짜 좋을 거 같아요",
          "speaker_id": "2",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013949.1.8",
          "form": "딱히 정해둔 곳은 업ㄹ믄데 요즘은 그냥 어디든 가고싶네오ㅠㅠ",
          "original_form": "딱히 정해둔 곳은 업ㄹ믄데 요즘은 그냥 어디든 가고싶네오ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013949.1.9",
          "form": "그렇긴해요",
          "original_form": "그렇긴해요",
          "speaker_id": "1",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013949.1.10",
          "form": "코로나로 거의 2년을 아무것도 못햐그있으니",
          "original_form": "코로나로 거의 2년을 아무것도 못햐그있으니",
          "speaker_id": "1",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013949.1.11",
          "form": "동남아 가면 뭐 제일 하고싶으세요",
          "original_form": "동남아 가면 뭐 제일 하고싶으세요",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013949.1.12",
          "form": "저는 그 맑은 에메랄드빛 바다 보고 싶어요",
          "original_form": "저는 그 맑은 에메랄드빛 바다 보고 싶어요",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013949.1.13",
          "form": "석양 지는 것만 봐도 좋을 거 같긴해요 ㅋㅋ",
          "original_form": "석양 지는 것만 봐도 좋을 거 같긴해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013949.1.14",
          "form": "너무 좋네요",
          "original_form": "너무 좋네요",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013949.1.15",
          "form": "상상만으로도 힐링..",
          "original_form": "상상만으로도 힐링..",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013949.1.16",
          "form": "저는 동남아 하면  과일 너무 먹어보고싶어요",
          "original_form": "저는 동남아 하면  과일 너무 먹어보고싶어요",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013949.1.17",
          "form": "망고가 그렇게 맛있다고 하던데",
          "original_form": "망고가 그렇게 맛있다고 하던데",
          "speaker_id": "2",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013949.1.18",
          "form": "진짜 다를까 궁금해요 ㅋㅋㅋ",
          "original_form": "진짜 다를까 궁금해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013949.1.19",
          "form": "막 다들 찬양하긴 하더라구요",
          "original_form": "막 다들 찬양하긴 하더라구요",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013949.1.20",
          "form": "얼마나 맛있기에",
          "original_form": "얼마나 맛있기에",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013949.1.21",
          "form": "야시장 이런것도 재밌을 거 같아요",
          "original_form": "야시장 이런것도 재밌을 거 같아요",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013949.1.22",
          "form": "개구리 뒷다리 꼬치 이런거 ㅋㅋㄴ",
          "original_form": "개구리 뒷다리 꼬치 이런거 ㅋㅋㄴ",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013949.1.23",
          "form": "와 이색적 음식",
          "original_form": "와 이색적 음식",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013949.1.24",
          "form": "그런거 보면 드실 수 있으세요?",
          "original_form": "그런거 보면 드실 수 있으세요?",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013949.1.25",
          "form": "닭고기 맛이래서 궁금하긴 하더라구요",
          "original_form": "닭고기 맛이래서 궁금하긴 하더라구요",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013949.1.26",
          "form": "근데 곤충이런건 못 먹을 거 같아요 ㅋㅋ",
          "original_form": "근데 곤충이런건 못 먹을 거 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013949.1.27",
          "form": "저두요ㅋㅋ",
          "original_form": "저두요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:45"
        }
      ]
    }
