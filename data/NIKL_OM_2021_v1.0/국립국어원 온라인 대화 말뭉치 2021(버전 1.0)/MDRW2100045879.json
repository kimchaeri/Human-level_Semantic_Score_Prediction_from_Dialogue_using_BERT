      "utterance": [
        {
          "id": "MDRW2100045879.1.1",
          "form": "택배파인더 어플에서 택배 한 건당 30원 적립돼요",
          "original_form": "택배파인더 어플에서 택배 한 건당 30원 적립돼요",
          "speaker_id": "2",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100045879.1.2",
          "form": "그게 은근 쏠쏠하더라고요",
          "original_form": "그게 은근 쏠쏠하더라고요",
          "speaker_id": "2",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100045879.1.3",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "1",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100045879.1.4",
          "form": "저는 스마트택배 어플쓰는데",
          "original_form": "저는 스마트택배 어플쓰는데",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045879.1.5",
          "form": "그걸로 약간 합리화하며 쇼핑하는...",
          "original_form": "그걸로 약간 합리화하며 쇼핑하는...",
          "speaker_id": "2",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045879.1.6",
          "form": "당장바꿔야겠네요",
          "original_form": "당장바꿔야겠네요",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045879.1.7",
          "form": "내돈",
          "original_form": "내돈",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045879.1.8",
          "form": "그걸로 포인트 모아서",
          "original_form": "그걸로 포인트 모아서",
          "speaker_id": "2",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045879.1.9",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045879.1.10",
          "form": "기프티콘 살 수 있어요",
          "original_form": "기프티콘 살 수 있어요",
          "speaker_id": "2",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045879.1.11",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045879.1.12",
          "form": "지금까지날린 기프티콘이 몇개인지",
          "original_form": "지금까지날린 기프티콘이 몇개인지",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045879.1.13",
          "form": "요즘은 배송비딜로 화장품 샘플도 많이 팔아서",
          "original_form": "요즘은 배송비딜로 화장품 샘플도 많이 팔아서",
          "speaker_id": "2",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045879.1.14",
          "form": "충격적이네요",
          "original_form": "충격적이네요",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045879.1.15",
          "form": "그런 거 하면 완전 거저예요",
          "original_form": "그런 거 하면 완전 거저예요",
          "speaker_id": "2",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045879.1.16",
          "form": "그거받으려머ㅏㄴ 또 쇼핑해야겠네요",
          "original_form": "그거받으려머ㅏㄴ 또 쇼핑해야겠네요",
          "speaker_id": "1",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045879.1.17",
          "form": "맞아요ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045879.1.18",
          "form": "제가 그래서...ㅋㅋㅋㅋ",
          "original_form": "제가 그래서...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045879.1.19",
          "form": "이거 개미지옥입니다ㅠㅠ",
          "original_form": "이거 개미지옥입니다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045879.1.20",
          "form": "네이버로물건살때 네이버포인트받는건 마이너스하고 계산...ㅋㅋㅋ",
          "original_form": "네이버로물건살때 네이버포인트받는건 마이너스하고 계산...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045879.1.21",
          "form": "저도요ㅋㅋㄲㅋㅋㅋㅋㅋ",
