      "utterance": [
        {
          "id": "MDRW2100010617.1.1",
          "form": "키우는 동물 있으세여?",
          "original_form": "키우는 동물 있으세여?",
          "speaker_id": "2",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010617.1.2",
          "form": "아뇨없어요ㅠ",
          "original_form": "아뇨없어요ㅠ",
          "speaker_id": "1",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010617.1.3",
          "form": "전 토끼 키워요",
          "original_form": "전 토끼 키워요",
          "speaker_id": "2",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010617.1.4",
          "form": "헐 진짜요????",
          "original_form": "헐 진짜요????",
          "speaker_id": "1",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010617.1.5",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "1",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010617.1.6",
          "form": "네~ 올해로 8년째 ㅋㅋ",
          "original_form": "네~ 올해로 8년째 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010617.1.7",
          "form": "와 오래키우셨네요",
          "original_form": "와 오래키우셨네요",
          "speaker_id": "1",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010617.1.8",
          "form": "토끼는 밥 뭐먹어요??",
          "original_form": "토끼는 밥 뭐먹어요??",
          "speaker_id": "1",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010617.1.9",
          "form": "풀하고 건초가 주식이에요!",
          "original_form": "풀하고 건초가 주식이에요!",
          "speaker_id": "2",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100010617.1.10",
          "form": "생각보다 당근은 그렇게까지 좋아하지 않아요ㅋㅋ",
          "original_form": "생각보다 당근은 그렇게까지 좋아하지 않아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100010617.1.11",
          "form": "아하 글쿤요",
          "original_form": "아하 글쿤요",
          "speaker_id": "1",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010617.1.12",
          "form": "달달한 걸 제일 좋아해서",
          "original_form": "달달한 걸 제일 좋아해서",
          "speaker_id": "2",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010617.1.13",
          "form": "과일을 보면 눈 돌아가요ㅋㅋ",
          "original_form": "과일을 보면 눈 돌아가요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010617.1.14",
          "form": "아 상상만해도 넘귀여워요ㅠㅠ",
          "original_form": "아 상상만해도 넘귀여워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 20:25"
        },
        {
          "id": "MDRW2100010617.1.15",
          "form": "방금 전까지 풀어놓고있었는데",
          "original_form": "방금 전까지 풀어놓고있었는데",
          "speaker_id": "2",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100010617.1.16",
          "form": "슬슬 잘때되서 케이지 안에 넣어놨어요",
          "original_form": "슬슬 잘때되서 케이지 안에 넣어놨어요",
          "speaker_id": "2",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100010617.1.17",
          "form": "되게 보들보들할거같아요",
          "original_form": "되게 보들보들할거같아요",
          "speaker_id": "1",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100010617.1.18",
          "form": "볼따구랑 머리 문질러주는걸 좋아하는데",
          "original_form": "볼따구랑 머리 문질러주는걸 좋아하는데",
          "speaker_id": "2",
          "time": "20210823 20:29"
        },
        {
          "id": "MDRW2100010617.1.19",
          "form": "기분 내키면 제 손아래에 얼굴 턱 대고 쓰다듬어달라고 해요ㅋㅋ",
          "original_form": "기분 내키면 제 손아래에 얼굴 턱 대고 쓰다듬어달라고 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:29"
        },
        {
          "id": "MDRW2100010617.1.20",
          "form": "진짜 토끼 제 옆에 있었으면 뽀뽀했을거예요.. 텍스트만 봐도 귀여운데ㅜㅜ",
          "original_form": "진짜 토끼 제 옆에 있었으면 뽀뽀했을거예요.. 텍스트만 봐도 귀여운데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 20:30"
        },
        {
          "id": "MDRW2100010617.1.21",
          "form": "흑흑 너무 귀여워요...",
          "original_form": "흑흑 너무 귀여워요...",
          "speaker_id": "2",
          "time": "20210823 20:30"
        }
      ]
