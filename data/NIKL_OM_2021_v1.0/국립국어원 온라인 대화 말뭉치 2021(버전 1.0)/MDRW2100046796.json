      "utterance": [
        {
          "id": "MDRW2100046796.1.1",
          "form": "방갑습니다",
          "original_form": "방갑습니다",
          "speaker_id": "1",
          "time": "20211105 10:00"
        },
        {
          "id": "MDRW2100046796.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211105 10:04"
        },
        {
          "id": "MDRW2100046796.1.3",
          "form": "좋은 아침이에요 ^_^",
          "original_form": "좋은 아침이에요 ^_^",
          "speaker_id": "2",
          "time": "20211105 10:04"
        },
        {
          "id": "MDRW2100046796.1.4",
          "form": "방가워요",
          "original_form": "방가워요",
          "speaker_id": "1",
          "time": "20211105 10:07"
        },
        {
          "id": "MDRW2100046796.1.5",
          "form": "오늘은좋은아침입니다",
          "original_form": "오늘은좋은아침입니다",
          "speaker_id": "1",
          "time": "20211105 10:08"
        },
        {
          "id": "MDRW2100046796.1.6",
          "form": "오늘 금요일인데 놀 계획 있으신가요? ㅎㅎ",
          "original_form": "오늘 금요일인데 놀 계획 있으신가요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 10:08"
        },
        {
          "id": "MDRW2100046796.1.7",
          "form": "일하고있어요",
          "original_form": "일하고있어요",
          "speaker_id": "1",
          "time": "20211105 10:09"
        },
        {
          "id": "MDRW2100046796.1.8",
          "form": "오늘은name2님은놀러가시나요?",
          "original_form": "오늘은&name2&님은놀러가시나요?",
          "speaker_id": "1",
          "time": "20211105 10:11"
        },
        {
          "id": "MDRW2100046796.1.9",
          "form": "바쁘신가요",
          "original_form": "바쁘신가요",
          "speaker_id": "1",
          "time": "20211105 10:13"
        },
        {
          "id": "MDRW2100046796.1.10",
          "form": "어녀 저는 내일 아침 일찍 놀러가려구요! ㅎㅎ",
          "original_form": "어녀 저는 내일 아침 일찍 놀러가려구요! ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 10:15"
        },
        {
          "id": "MDRW2100046796.1.11",
          "form": "지금은 회사입니다 ㅠㅠ",
          "original_form": "지금은 회사입니다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211105 10:15"
        },
        {
          "id": "MDRW2100046796.1.12",
          "form": "어디로놀러가시나요",
          "original_form": "어디로놀러가시나요",
          "speaker_id": "1",
          "time": "20211105 10:15"
        },
        {
          "id": "MDRW2100046796.1.13",
          "form": "똑같은직장인이군요",
          "original_form": "똑같은직장인이군요",
          "speaker_id": "1",
          "time": "20211105 10:16"
        },
        {
          "id": "MDRW2100046796.1.14",
          "form": "네 ㅠㅠ 저 주말에 여수 가려구요!",
          "original_form": "네 ㅠㅠ 저 주말에 여수 가려구요!",
          "speaker_id": "2",
          "time": "20211105 10:17"
        },
        {
          "id": "MDRW2100046796.1.15",
          "form": "여수 가보셨어요?",
          "original_form": "여수 가보셨어요?",
          "speaker_id": "2",
          "time": "20211105 10:17"
        },
        {
          "id": "MDRW2100046796.1.16",
          "form": "네제고향이전라도입니다",
          "original_form": "네제고향이전라도입니다",
          "speaker_id": "1",
          "time": "20211105 10:17"
        },
        {
          "id": "MDRW2100046796.1.17",
          "form": "여수는몇번가봣죠",
          "original_form": "여수는몇번가봣죠",
          "speaker_id": "1",
          "time": "20211105 10:17"
        },
        {
          "id": "MDRW2100046796.1.18",
          "form": "오 여수에서 추천해줄 곳 있어요?",
          "original_form": "오 여수에서 추천해줄 곳 있어요?",
          "speaker_id": "2",
          "time": "20211105 10:20"
        },
        {
          "id": "MDRW2100046796.1.19",
          "form": "여수는가는곳마다좋아서 음식점도로컬지역으로가세요",
          "original_form": "여수는가는곳마다좋아서 음식점도로컬지역으로가세요",
          "speaker_id": "1",
          "time": "20211105 10:21"
        },
        {
          "id": "MDRW2100046796.1.20",
          "form": "너무블로는믿지마시구요",
          "original_form": "너무블로는믿지마시구요",
          "speaker_id": "1",
          "time": "20211105 10:21"
        },
        {
          "id": "MDRW2100046796.1.21",
          "form": "여수밤바다너무이쁩니다",
          "original_form": "여수밤바다너무이쁩니다",
          "speaker_id": "1",
          "time": "20211105 10:21"
        },
        {
          "id": "MDRW2100046796.1.22",
          "form": "캔맥주사셔서 밤바다를바라보면서데이트하시는거추천합니다",
          "original_form": "캔맥주사셔서 밤바다를바라보면서데이트하시는거추천합니다",
          "speaker_id": "1",
          "time": "20211105 10:22"
        },
        {
          "id": "MDRW2100046796.1.23",
          "form": "우와 진짜 현지인 추천이네요 ㅎㅎㅎ",
          "original_form": "우와 진짜 현지인 추천이네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 10:28"
        },
        {
          "id": "MDRW2100046796.1.24",
          "form": "꼭 가볼게요 밤바다!",
          "original_form": "꼭 가볼게요 밤바다!",
          "speaker_id": "2",
          "time": "20211105 10:28"
        }
      ]
