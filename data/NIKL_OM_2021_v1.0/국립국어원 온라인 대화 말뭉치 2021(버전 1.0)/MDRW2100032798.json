      "utterance": [
        {
          "id": "MDRW2100032798.1.1",
          "form": "과자이야기할까요",
          "original_form": "과자이야기할까요",
          "speaker_id": "1",
          "time": "20210905 23:49"
        },
        {
          "id": "MDRW2100032798.1.2",
          "form": "과자 좋아하세여?",
          "original_form": "과자 좋아하세여?",
          "speaker_id": "2",
          "time": "20210905 23:49"
        },
        {
          "id": "MDRW2100032798.1.3",
          "form": "그냥 심심해서",
          "original_form": "그냥 심심해서",
          "speaker_id": "1",
          "time": "20210905 23:49"
        },
        {
          "id": "MDRW2100032798.1.4",
          "form": "꼬북칩 입이 허전할때여",
          "original_form": "꼬북칩 입이 허전할때여",
          "speaker_id": "1",
          "time": "20210905 23:49"
        },
        {
          "id": "MDRW2100032798.1.5",
          "form": "아하 꼬북칩 맛있죠",
          "original_form": "아하 꼬북칩 맛있죠",
          "speaker_id": "2",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032798.1.6",
          "form": "초코맛 드시보셨어요?",
          "original_form": "초코맛 드시보셨어요?",
          "speaker_id": "2",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032798.1.7",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032798.1.8",
          "form": "전 아무래도 콘스프맛이",
          "original_form": "전 아무래도 콘스프맛이",
          "speaker_id": "1",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032798.1.9",
          "form": "좋은것같아요",
          "original_form": "좋은것같아요",
          "speaker_id": "1",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100032798.1.10",
          "form": "전 초코맛이 맥주 안주로 좋더라구요 ㅋㅋㅋ",
          "original_form": "전 초코맛이 맥주 안주로 좋더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032798.1.11",
          "form": "그럼 옥수수깡 드셔보셨어요?",
          "original_form": "그럼 옥수수깡 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032798.1.12",
          "form": "오 옛날에요",
          "original_form": "오 옛날에요",
          "speaker_id": "1",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100032798.1.13",
          "form": "맥주안주는",
          "original_form": "맥주안주는",
          "speaker_id": "1",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032798.1.14",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "1",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032798.1.15",
          "form": "무뚝뚝한 감자칩 아시나요",
          "original_form": "무뚝뚝한 감자칩 아시나요",
          "speaker_id": "1",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032798.1.16",
          "form": "추천합니당",
          "original_form": "추천합니당",
          "speaker_id": "1",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032798.1.17",
          "form": "아 처음 들어봐요",
          "original_form": "아 처음 들어봐요",
          "speaker_id": "2",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032798.1.18",
          "form": "두꺼운 감자칩 이런건가요?",
          "original_form": "두꺼운 감자칩 이런건가요?",
          "speaker_id": "2",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032798.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100032798.1.20",
          "form": "두꺼운 편인데",
          "original_form": "두꺼운 편인데",
          "speaker_id": "1",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032798.1.21",
          "form": "후추맛이 좀 나구요",
          "original_form": "후추맛이 좀 나구요",
          "speaker_id": "1",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032798.1.22",
          "form": "아하 저 그런거 좋아햐요 ㅋㅋ",
          "original_form": "아하 저 그런거 좋아햐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032798.1.23",
          "form": "다음에 안주로 찾아볼게여!",
          "original_form": "다음에 안주로 찾아볼게여!",
          "speaker_id": "2",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032798.1.24",
          "form": "요즘 콜라보같은거도 많이 하더라구요",
          "original_form": "요즘 콜라보같은거도 많이 하더라구요",
          "speaker_id": "1",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100032798.1.25",
          "form": "비빔면 소스를 넣는 고로케방이라든지(파리바게트)",
          "original_form": "비빔면 소스를 넣는 고로케방이라든지(파리바게트)",
          "speaker_id": "1",
          "time": "20210905 23:54"
        },
        {
          "id": "MDRW2100032798.1.26",
          "form": "오..비빔면소스라서 맛있을 것 같네요 ㅋㅋ",
          "original_form": "오..비빔면소스라서 맛있을 것 같네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:54"
        },
        {
          "id": "MDRW2100032798.1.27",
          "form": "이상한 콜라보도 많지망",
          "original_form": "이상한 콜라보도 많지망",
          "speaker_id": "1",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032798.1.28",
          "form": "비빔면 소스는 왠지 인정이죠",
          "original_form": "비빔면 소스는 왠지 인정이죠",
          "speaker_id": "1",
          "time": "20210905 23:55"
        }
      ]
    }
