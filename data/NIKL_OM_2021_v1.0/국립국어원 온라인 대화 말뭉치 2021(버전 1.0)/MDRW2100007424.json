      "utterance": [
        {
          "id": "MDRW2100007424.1.1",
          "form": "비오는 날씨 좋아하세요?",
          "original_form": "비오는 날씨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 02:02"
        },
        {
          "id": "MDRW2100007424.1.2",
          "form": "저 아까전에 괜히 나갔다가ㅋㅋㅋ 신발 저ㅈ었어요",
          "original_form": "저 아까전에 괜히 나갔다가ㅋㅋㅋ 신발 저ㅈ었어요",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007424.1.3",
          "form": "밖에만 안 나가면 좋은 거 같아요!!",
          "original_form": "밖에만 안 나가면 좋은 거 같아요!!",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007424.1.4",
          "form": "저도 집안에서 비 구경만 하는거ㅋㅋㅋㅋ좋아해요",
          "original_form": "저도 집안에서 비 구경만 하는거ㅋㅋㅋㅋ좋아해요",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007424.1.5",
          "form": "비 다 그치고 나서 살짝 습한 공기도 좋아요",
          "original_form": "비 다 그치고 나서 살짝 습한 공기도 좋아요",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007424.1.6",
          "form": "오 맞아요!!",
          "original_form": "오 맞아요!!",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007424.1.7",
          "form": "공기도 더 상쾌해지는 것 같아요",
          "original_form": "공기도 더 상쾌해지는 것 같아요",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007424.1.8",
          "form": "맞아요 시원하니 좋아요 ㅋㅋㅋ",
          "original_form": "맞아요 시원하니 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007424.1.9",
          "form": "미세먼지도 씻겨 내려가는 느낌ㅋㅋㅋ",
          "original_form": "미세먼지도 씻겨 내려가는 느낌ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007424.1.10",
          "form": "아 맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007424.1.11",
          "form": "좋아하는 계절",
          "original_form": "좋아하는 계절",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007424.1.12",
          "form": "있으신가요?",
          "original_form": "있으신가요?",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007424.1.13",
          "form": "저는 아무래도 가을이요!",
          "original_form": "저는 아무래도 가을이요!",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007424.1.14",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007424.1.15",
          "form": "아무래도 가을이 제일 선선하고 좋죠 ㅋㅋ",
          "original_form": "아무래도 가을이 제일 선선하고 좋죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007424.1.16",
          "form": "전 여름 좋아해요",
          "original_form": "전 여름 좋아해요",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007424.1.17",
          "form": "많이 덥지만",
          "original_form": "많이 덥지만",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007424.1.18",
          "form": "오 여름",
          "original_form": "오 여름",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007424.1.19",
          "form": "그래도 식물들도 생기있고",
          "original_form": "그래도 식물들도 생기있고",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007424.1.20",
          "form": "미화된 여름이라고 해야할까요",
          "original_form": "미화된 여름이라고 해야할까요",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007424.1.21",
          "form": "여름이었다.",
          "original_form": "여름이었다.",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007424.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007424.1.23",
          "form": "이 분위기 좋아해요 ㅋㅋㅋ",
          "original_form": "이 분위기 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007424.1.24",
          "form": "저도 여름이었다 밖에 안 떠올랐어요ㅋㅋㅋ",
          "original_form": "저도 여름이었다 밖에 안 떠올랐어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007424.1.25",
          "form": "여름 청량 분위기 너무 좋아요",
          "original_form": "여름 청량 분위기 너무 좋아요",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007424.1.26",
          "form": "맞아요ㅋㅋㅋㅋㅋ청량의 계절",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ청량의 계절",
          "speaker_id": "2",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007424.1.27",
          "form": "집 앞에 마침 산책로가 있어서",
          "original_form": "집 앞에 마침 산책로가 있어서",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007424.1.28",
          "form": "여름 되면 진짜 파릇파릇하니 감성 제대로거든요",
          "original_form": "여름 되면 진짜 파릇파릇하니 감성 제대로거든요",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007424.1.29",
          "form": "나갈 일 있으면 청량곡 들으면서 걷고 그래요",
          "original_form": "나갈 일 있으면 청량곡 들으면서 걷고 그래요",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007424.1.30",
          "form": "크...완전 좋을 거 같아요",
          "original_form": "크...완전 좋을 거 같아요",
          "speaker_id": "1",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007424.1.31",
          "form": "여름은 벌레만 좀 없으면 좋을텐데",
          "original_form": "여름은 벌레만 좀 없으면 좋을텐데",
          "speaker_id": "1",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007424.1.32",
          "form": "아...그쵸ㅋㅋㅋㅠㅠㅠ",
          "original_form": "아...그쵸ㅋㅋㅋㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007424.1.33",
          "form": "근데 요즘은",
          "original_form": "근데 요즘은",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007424.1.34",
          "form": "너어어무 덥다보니",
          "original_form": "너어어무 덥다보니",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007424.1.35",
          "form": "모기들도 한여름 말고",
          "original_form": "모기들도 한여름 말고",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007424.1.36",
          "form": "가을 넘어가기 직전부터 나오더라구요..",
          "original_form": "가을 넘어가기 직전부터 나오더라구요..",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007424.1.37",
          "form": "그냥 안나와주면 참 좋겠으나..",
          "original_form": "그냥 안나와주면 참 좋겠으나..",
          "speaker_id": "2",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007424.1.38",
          "form": "아...그럼...요즘이 딱 나올 때겠네요ㅠㅠ",
          "original_form": "아...그럼...요즘이 딱 나올 때겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007424.1.39",
          "form": "맞아요.. ㅠㅠㅠ",
          "original_form": "맞아요.. ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007424.1.40",
          "form": "저는 사실 모기 잘 안물리는 편이고",
          "original_form": "저는 사실 모기 잘 안물리는 편이고",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007424.1.41",
          "form": "제 동생이 엄청 잘 물리는데",
          "original_form": "제 동생이 엄청 잘 물리는데",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007424.1.42",
          "form": "어제 벌써 한방 물렸더라구요",
          "original_form": "어제 벌써 한방 물렸더라구요",
          "speaker_id": "2",
          "time": "20210822 02:17"
        }
      ]
    }
