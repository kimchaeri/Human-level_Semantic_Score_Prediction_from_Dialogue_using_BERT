      "utterance": [
        {
          "id": "MDRW2100016778.1.1",
          "form": "퇴근후 배달시켜드시나여?",
          "original_form": "퇴근후 배달시켜드시나여?",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016778.1.2",
          "form": "주로 시켜먹고 사먹어요",
          "original_form": "주로 시켜먹고 사먹어요",
          "speaker_id": "2",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100016778.1.3",
          "form": "요리하세요?",
          "original_form": "요리하세요?",
          "speaker_id": "2",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100016778.1.4",
          "form": "저는 주로 샐러드나 간편식먹어요..",
          "original_form": "저는 주로 샐러드나 간편식먹어요..",
          "speaker_id": "1",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016778.1.5",
          "form": "오 다이어터시구나",
          "original_form": "오 다이어터시구나",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016778.1.6",
          "form": "아니요... 그냥 거지에요.. ㅋㅋㅋ",
          "original_form": "아니요... 그냥 거지에요.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016778.1.7",
          "form": "ㅋㅋ저도거지에요",
          "original_form": "ㅋㅋ저도거지에요",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016778.1.8",
          "form": "싸고 간편한거로..",
          "original_form": "싸고 간편한거로..",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016778.1.9",
          "form": "요리는 안하세요?",
          "original_form": "요리는 안하세요?",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016778.1.10",
          "form": "똥손이고",
          "original_form": "똥손이고",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016778.1.11",
          "form": "할생각이없어요ㅋㅋ",
          "original_form": "할생각이없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016778.1.12",
          "form": "다행히 남친이 잘해요",
          "original_form": "다행히 남친이 잘해요",
          "speaker_id": "2",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016778.1.13",
          "form": "더 할일이없을듯",
          "original_form": "더 할일이없을듯",
          "speaker_id": "2",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016778.1.14",
          "form": "ㅋㅋㅋ 당당한 모던 우먼우시군요.. ㅋㅋ 생각이 없다니.. ㅋㅋ",
          "original_form": "ㅋㅋㅋ 당당한 모던 우먼우시군요.. ㅋㅋ 생각이 없다니.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100016778.1.15",
          "form": "혼자 있을땐 시켜드시구요?",
          "original_form": "혼자 있을땐 시켜드시구요?",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016778.1.16",
          "form": "ㅋㅋ시켜먹거나 사먹거나",
          "original_form": "ㅋㅋ시켜먹거나 사먹거나",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016778.1.17",
          "form": "해놓고간거 데워먹는 편이에요",
          "original_form": "해놓고간거 데워먹는 편이에요",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016778.1.18",
          "form": "노.예?",
          "original_form": "노.예?",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016778.1.19",
          "form": "ㅋㅋ사랑의노예",
          "original_form": "ㅋㅋ사랑의노예",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016778.1.20",
          "form": "남친이라 쓰고 노.예라 읽는다? ㅋㅋㅋ",
          "original_form": "남친이라 쓰고 노.예라 읽는다? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016778.1.21",
          "form": "취향이 다른거져",
          "original_form": "취향이 다른거져",
          "speaker_id": "2",
          "time": "20210826 15:27"
        }
      ]
