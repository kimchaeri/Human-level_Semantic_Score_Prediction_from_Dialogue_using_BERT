      "utterance": [
        {
          "id": "MDRW2100040894.1.1",
          "form": "가고싶어진 여행지 있나요~?",
          "original_form": "가고싶어진 여행지 있나요~?",
          "speaker_id": "1",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100040894.1.2",
          "form": "저는 이탈리아요",
          "original_form": "저는 이탈리아요",
          "speaker_id": "2",
          "time": "20210905 11:33"
        },
        {
          "id": "MDRW2100040894.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 11:33"
        },
        {
          "id": "MDRW2100040894.1.4",
          "form": "와 이탈리아",
          "original_form": "와 이탈리아",
          "speaker_id": "1",
          "time": "20210905 11:38"
        },
        {
          "id": "MDRW2100040894.1.5",
          "form": "맞아요 거기도 가고싶었는데",
          "original_form": "맞아요 거기도 가고싶었는데",
          "speaker_id": "1",
          "time": "20210905 11:38"
        },
        {
          "id": "MDRW2100040894.1.6",
          "form": "저는 호주나 뉴질랜드?",
          "original_form": "저는 호주나 뉴질랜드?",
          "speaker_id": "1",
          "time": "20210905 11:38"
        },
        {
          "id": "MDRW2100040894.1.7",
          "form": "스위스 꼭 가보고싶어요",
          "original_form": "스위스 꼭 가보고싶어요",
          "speaker_id": "1",
          "time": "20210905 11:38"
        },
        {
          "id": "MDRW2100040894.1.8",
          "form": "경치 구경하는거 좋아하시나봐요",
          "original_form": "경치 구경하는거 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210905 11:41"
        },
        {
          "id": "MDRW2100040894.1.9",
          "form": "네! 저 약간 그런거 좋아해요",
          "original_form": "네! 저 약간 그런거 좋아해요",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100040894.1.10",
          "form": "액티비한거 하는것도 좋아하지만",
          "original_form": "액티비한거 하는것도 좋아하지만",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100040894.1.11",
          "form": "경치즐기는거 좋아해요",
          "original_form": "경치즐기는거 좋아해요",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100040894.1.12",
          "form": "우와 그럼 몰디브도 목표 여행지중 하나가 아닐까 예상해봅니다 맞나요??",
          "original_form": "우와 그럼 몰디브도 목표 여행지중 하나가 아닐까 예상해봅니다 맞나요??",
          "speaker_id": "2",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100040894.1.13",
          "form": "몰디브~는 생각못해봤고 사이판은 생각했었어요!",
          "original_form": "몰디브~는 생각못해봤고 사이판은 생각했었어요!",
          "speaker_id": "1",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100040894.1.14",
          "form": "사이판이 동남아쪽인가요?",
          "original_form": "사이판이 동남아쪽인가요?",
          "speaker_id": "2",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100040894.1.15",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "1",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100040894.1.16",
          "form": "아닌가...",
          "original_form": "아닌가...",
          "speaker_id": "1",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100040894.1.17",
          "form": "아무튼 다녀온 지인이 좋다고하더라고요",
          "original_form": "아무튼 다녀온 지인이 좋다고하더라고요",
          "speaker_id": "1",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100040894.1.18",
          "form": "그렇군요 그럼 name1님은 여행갈 때 기념품 사시는 편이신가요?",
          "original_form": "그렇군요 그럼 &name1&님은 여행갈 때 기념품 사시는 편이신가요?",
          "speaker_id": "2",
          "time": "20210905 12:16"
        },
        {
          "id": "MDRW2100040894.1.19",
          "form": "저~ 예전에는 진짜 먹을거 산더미처럼 사왔는데 이젠...ㅋ 적당히만 사요",
          "original_form": "저~ 예전에는 진짜 먹을거 산더미처럼 사왔는데 이젠...ㅋ 적당히만 사요",
          "speaker_id": "1",
          "time": "20210905 12:46"
        },
        {
          "id": "MDRW2100040894.1.20",
          "form": "그럼 지금까지 사신 기념품중에 가장 마음에 드시는 것은 무엇인가요?",
          "original_form": "그럼 지금까지 사신 기념품중에 가장 마음에 드시는 것은 무엇인가요?",
          "speaker_id": "2",
          "time": "20210905 12:48"
        },
        {
          "id": "MDRW2100040894.1.21",
          "form": "음 기념사진같은거 산게 좋았어요!",
          "original_form": "음 기념사진같은거 산게 좋았어요!",
          "speaker_id": "1",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100040894.1.22",
          "form": "뭐타고 찍히는 사진이요",
          "original_form": "뭐타고 찍히는 사진이요",
          "speaker_id": "1",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100040894.1.23",
          "form": "어디서 찍으신건가요?",
          "original_form": "어디서 찍으신건가요?",
          "speaker_id": "2",
          "time": "20210905 15:31"
        },
        {
          "id": "MDRW2100040894.1.24",
          "form": "음 가는곳마다 찍은거같아요ㅋㅋㅋ",
          "original_form": "음 가는곳마다 찍은거같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100040894.1.25",
          "form": "일본에서도 찍고 싱가폴에서도 찍고",
          "original_form": "일본에서도 찍고 싱가폴에서도 찍고",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100040894.1.26",
          "form": "우와 사진보면 기억에 정말 좋을 것 같아요",
          "original_form": "우와 사진보면 기억에 정말 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20210905 20:57"
        }
      ]
    }
