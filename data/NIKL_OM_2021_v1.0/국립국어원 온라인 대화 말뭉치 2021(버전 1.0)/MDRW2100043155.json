      "utterance": [
        {
          "id": "MDRW2100043155.1.1",
          "form": "여행가서 기념품 주로 뭐 사오세요",
          "original_form": "여행가서 기념품 주로 뭐 사오세요",
          "speaker_id": "1",
          "time": "20210907 15:02"
        },
        {
          "id": "MDRW2100043155.1.2",
          "form": "최근 잘 산템 있어??",
          "original_form": "최근 잘 산템 있어??",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043155.1.3",
          "form": "저 사실 컵 모을까하는데",
          "original_form": "저 사실 컵 모을까하는데",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043155.1.4",
          "form": "너무 둘곳이 없고요",
          "original_form": "너무 둘곳이 없고요",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043155.1.5",
          "form": "우와 컵 좋아하는구나",
          "original_form": "우와 컵 좋아하는구나",
          "speaker_id": "1",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100043155.1.6",
          "form": "그럼 슈납장도 하나 사야겠는걸??",
          "original_form": "그럼 슈납장도 하나 사야겠는걸??",
          "speaker_id": "1",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100043155.1.7",
          "form": "그니까요ㅋㅋ",
          "original_form": "그니까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100043155.1.8",
          "form": "지금 이미 한구석이 가득ㅋㅋ",
          "original_form": "지금 이미 한구석이 가득ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100043155.1.9",
          "form": "같이 반말해요ㅠㅠ 반말해야 빨리끝나요",
          "original_form": "같이 반말해요ㅠㅠ 반말해야 빨리끝나요",
          "speaker_id": "1",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100043155.1.10",
          "form": "컵은 사놓고 실제로 사용해??",
          "original_form": "컵은 사놓고 실제로 사용해??",
          "speaker_id": "1",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100043155.1.11",
          "form": "아까워서ㅋㅈㅋ",
          "original_form": "아까워서ㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100043155.1.12",
          "form": "이젠 쓸라구",
          "original_form": "이젠 쓸라구",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100043155.1.13",
          "form": "막ㅋㅋ",
          "original_form": "막ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100043155.1.14",
          "form": "맞아 아끼면 안돼",
          "original_form": "맞아 아끼면 안돼",
          "speaker_id": "1",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043155.1.15",
          "form": "이쁜거 사서 자주 쓰자",
          "original_form": "이쁜거 사서 자주 쓰자",
          "speaker_id": "1",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043155.1.16",
          "form": "뭐사와?? 보통",
          "original_form": "뭐사와?? 보통",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100043155.1.17",
          "form": "나는 옷 많이 사 ㅋㅋㅋ가방이랑",
          "original_form": "나는 옷 많이 사 ㅋㅋㅋ가방이랑",
          "speaker_id": "1",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100043155.1.18",
          "form": "예전에 스티커 많이 사던거 아까워 죽겠오",
          "original_form": "예전에 스티커 많이 사던거 아까워 죽겠오",
          "speaker_id": "1",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100043155.1.19",
          "form": "ㅋㅋ 뭐하려고 산겨???꾸미기???",
          "original_form": "ㅋㅋ 뭐하려고 산겨???꾸미기???",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043155.1.20",
          "form": "엌ㅋㅋㅋ 다이어리 꾸미거나",
          "original_form": "엌ㅋㅋㅋ 다이어리 꾸미거나",
          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043155.1.21",
          "form": "이전에 칰구들 편지써주는거 조아해서",
          "original_form": "이전에 칰구들 편지써주는거 조아해서",
          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043155.1.22",
          "form": "근데 요즌은 편지 잘 안쓰재너",
          "original_form": "근데 요즌은 편지 잘 안쓰재너",
          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043155.1.23",
          "form": "ㅋㅋ그냥 폰꾸해여ㅋㅋ",
          "original_form": "ㅋㅋ그냥 폰꾸해여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100043155.1.24",
          "form": "우산에도 붙이고 패드에도 붙이고ㅋㅋ",
          "original_form": "우산에도 붙이고 패드에도 붙이고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043155.1.25",
          "form": "아 폰꾸 ㅋㅋㅋ그냥 어디 기부할까봐요",
          "original_form": "아 폰꾸 ㅋㅋㅋ그냥 어디 기부할까봐요",
          "speaker_id": "1",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100043155.1.26",
          "form": "애기들이 진짜 좋아함ㅋㅋ",
          "original_form": "애기들이 진짜 좋아함ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100043155.1.27",
          "form": "인기폭발",
          "original_form": "인기폭발",
          "speaker_id": "2",
