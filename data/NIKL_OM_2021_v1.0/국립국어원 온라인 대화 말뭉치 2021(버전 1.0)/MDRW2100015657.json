      "utterance": [
        {
          "id": "MDRW2100015657.1.1",
          "form": "헐 저 경주 완전 좋아하잖아요!",
          "original_form": "헐 저 경주 완전 좋아하잖아요!",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015657.1.2",
          "form": "혹시 경주 가시게 되면 여기당 이라는 식당",
          "original_form": "혹시 경주 가시게 되면 여기당 이라는 식당",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015657.1.3",
          "form": "가보세요! 시래기 비빔밥이랑 전 파는데",
          "original_form": "가보세요! 시래기 비빔밥이랑 전 파는데",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015657.1.4",
          "form": "제가 먹었던 것 중에",
          "original_form": "제가 먹었던 것 중에",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015657.1.5",
          "form": "최고였어요",
          "original_form": "최고였어요",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015657.1.6",
          "form": "우와 저 적어둘래요",
          "original_form": "우와 저 적어둘래요",
          "speaker_id": "1",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015657.1.7",
          "form": "경주 두번 갔었는데 식당을 잘못골랐었어요ㅠㅠ",
          "original_form": "경주 두번 갔었는데 식당을 잘못골랐었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015657.1.8",
          "form": "헉 ㅠㅠ 너무 아쉬웠겠다",
          "original_form": "헉 ㅠㅠ 너무 아쉬웠겠다",
          "speaker_id": "2",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015657.1.9",
          "form": "여행 갔는데 맛없는 거 먹으면 기분 꽝이잖아요",
          "original_form": "여행 갔는데 맛없는 거 먹으면 기분 꽝이잖아요",
          "speaker_id": "2",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015657.1.10",
          "form": "맞아요ㅠㅠ저 황리단길 갔었는데",
          "original_form": "맞아요ㅠㅠ저 황리단길 갔었는데",
          "speaker_id": "1",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015657.1.11",
          "form": "거기 경주피자..?가 너무 별로였어요",
          "original_form": "거기 경주피자..?가 너무 별로였어요",
          "speaker_id": "1",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015657.1.12",
          "form": "으악 처음 들어봤는데 절대 안 가야겠어요",
          "original_form": "으악 처음 들어봤는데 절대 안 가야겠어요",
          "speaker_id": "2",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015657.1.13",
          "form": "피자가 맛없기 힘들잖아요..",
          "original_form": "피자가 맛없기 힘들잖아요..",
          "speaker_id": "2",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015657.1.14",
          "form": "그때 너무 웃겼던게",
          "original_form": "그때 너무 웃겼던게",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100015657.1.15",
          "form": "다른 테이블도 여행오신 분들 같았는데",
          "original_form": "다른 테이블도 여행오신 분들 같았는데",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100015657.1.16",
          "form": "떡볶이 시킬까..? 했는데 다른 테이블에서 저희 보고 크게 엑스 그려주셨어욬ㅋㅋㅋㅋㅋ",
          "original_form": "떡볶이 시킬까..? 했는데 다른 테이블에서 저희 보고 크게 엑스 그려주셨어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100015657.1.17",
          "form": "헐 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 진짜 웃기다",
          "original_form": "헐 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 진짜 웃기다",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100015657.1.18",
          "form": "그분들도.... 고통받고 계셨나보다.....",
          "original_form": "그분들도.... 고통받고 계셨나보다.....",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100015657.1.19",
          "form": "name1님은 여행 가실 때 숙소가 중요하세요!?!?!",
          "original_form": "&name1&님은 여행 가실 때 숙소가 중요하세요!?!?!",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100015657.1.20",
          "form": "음 전 크게는 신경 안써요!",
          "original_form": "음 전 크게는 신경 안써요!",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100015657.1.21",
          "form": "숙소보단 음식을 더ㅎㅎ근데 뷰 좋으면 기분은 좋더라구여",
          "original_form": "숙소보단 음식을 더ㅎㅎ근데 뷰 좋으면 기분은 좋더라구여",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100015657.1.22",
          "form": "맞아요 특히 바다뷰나 엄청 높은 고층뷰면 너무 좋더라고요 ㅎ.ㅎ",
          "original_form": "맞아요 특히 바다뷰나 엄청 높은 고층뷰면 너무 좋더라고요 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100015657.1.23",
          "form": "헐 저도 음식파!! ㅎㅎㄹ",
          "original_form": "헐 저도 음식파!! ㅎㅎㄹ",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100015657.1.24",
          "form": "ㅋㅋㅋㅋ저는 잠은 그냥...자고 나오면 된다 싶어가지궄ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ저는 잠은 그냥...자고 나오면 된다 싶어가지궄ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100015657.1.25",
          "form": "하지만 호캉스도 좋아합니다",
          "original_form": "하지만 호캉스도 좋아합니다",
          "speaker_id": "1",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100015657.1.26",
          "form": "맞아요 호캉스 가서 배달 음식 알차게 시켜먹으면...",
          "original_form": "맞아요 호캉스 가서 배달 음식 알차게 시켜먹으면...",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100015657.1.27",
          "form": "천상!",
          "original_form": "천상!",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100015657.1.28",
          "form": "그쵸ㅠㅠ",
          "original_form": "그쵸ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100015657.1.29",
          "form": "요새 닭발 먹고싶어요",
          "original_form": "요새 닭발 먹고싶어요",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100015657.1.30",
          "form": "족발도..",
          "original_form": "족발도..",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100015657.1.31",
          "form": "호캉스가면 닭발 꼭 먹는..ㅎ",
          "original_form": "호캉스가면 닭발 꼭 먹는..ㅎ",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100015657.1.32",
          "form": "헉 전 둘 다 못 먹어서...",
          "original_form": "헉 전 둘 다 못 먹어서...",
          "speaker_id": "2",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100015657.1.33",
          "form": "저는 호캉스는 떡볶이 앤 치킨이에요 ㅎㅎ",
          "original_form": "저는 호캉스는 떡볶이 앤 치킨이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100015657.1.34",
          "form": "헛 전 족발 닭발 다 못 먹어서",
          "original_form": "헛 전 족발 닭발 다 못 먹어서",
          "speaker_id": "2",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100015657.1.35",
          "form": "호캉스 때는 무조건 치킨이랑 떡볶이 먹어요 ㅎ.ㅎ",
          "original_form": "호캉스 때는 무조건 치킨이랑 떡볶이 먹어요 ㅎ.ㅎ",
          "speaker_id": "2",
