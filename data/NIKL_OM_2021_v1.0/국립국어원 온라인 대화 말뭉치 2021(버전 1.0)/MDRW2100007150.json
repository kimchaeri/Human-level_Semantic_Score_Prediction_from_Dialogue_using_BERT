      "utterance": [
        {
          "id": "MDRW2100007150.1.1",
          "form": "공연이나 뮤지컬 좋아하세요??",
          "original_form": "공연이나 뮤지컬 좋아하세요??",
          "speaker_id": "1",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007150.1.2",
          "form": "오 뮤지컬 좋아하는데",
          "original_form": "오 뮤지컬 좋아하는데",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007150.1.3",
          "form": "비싸서 거의 못 봐요ㅠ",
          "original_form": "비싸서 거의 못 봐요ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007150.1.4",
          "form": "저두요 ㅠ 한번 보려면 큰 마음 먹어야하죠",
          "original_form": "저두요 ㅠ 한번 보려면 큰 마음 먹어야하죠",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007150.1.5",
          "form": "그럼 뮤지컬 영화는 본 적 있으세요?",
          "original_form": "그럼 뮤지컬 영화는 본 적 있으세요?",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007150.1.6",
          "form": "오 뮤지컬 영화 좋아해요",
          "original_form": "오 뮤지컬 영화 좋아해요",
          "speaker_id": "2",
          "time": "20210822 15:19"
        },
        {
          "id": "MDRW2100007150.1.7",
          "form": "맘마미아 레미제라블 라라랜드 이런 류",
          "original_form": "맘마미아 레미제라블 라라랜드 이런 류",
          "speaker_id": "2",
          "time": "20210822 15:19"
        },
        {
          "id": "MDRW2100007150.1.8",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "1",
          "time": "20210822 17:43"
        },
        {
          "id": "MDRW2100007150.1.9",
          "form": "그런 영화는 꼭 영화관가서 보려고해요",
          "original_form": "그런 영화는 꼭 영화관가서 보려고해요",
          "speaker_id": "1",
          "time": "20210822 17:43"
        },
        {
          "id": "MDRW2100007150.1.10",
          "form": "그럼 좋아하는 뮤지컬 넘버는 있으신가요?",
          "original_form": "그럼 좋아하는 뮤지컬 넘버는 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 17:44"
        },
        {
          "id": "MDRW2100007150.1.11",
          "form": "Cell Block Tango요",
          "original_form": "Cell Block Tango요",
          "speaker_id": "2",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100007150.1.12",
          "form": "sweet transvestie도 좋아해요",
          "original_form": "sweet transvestie도 좋아해요",
          "speaker_id": "2",
          "time": "20210822 17:58"
        },
        {
          "id": "MDRW2100007150.1.13",
          "form": "아 첫번째 노래 시카고 넘버죠??",
          "original_form": "아 첫번째 노래 시카고 넘버죠??",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100007150.1.14",
          "form": "얼마전에 넷플에서 시카고를 영화로 봤어요",
          "original_form": "얼마전에 넷플에서 시카고를 영화로 봤어요",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100007150.1.15",
          "form": "보고나니까 직접 공연도 보고싶더라구요",
          "original_form": "보고나니까 직접 공연도 보고싶더라구요",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100007150.1.16",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210822 18:51"
        },
        {
          "id": "MDRW2100007150.1.17",
          "form": "시카고 넷플에 있군요",
          "original_form": "시카고 넷플에 있군요",
          "speaker_id": "2",
          "time": "20210822 18:51"
        },
        {
          "id": "MDRW2100007150.1.18",
          "form": "ㅠㅠ저도 공연 보고 싶어요",
          "original_form": "ㅠㅠ저도 공연 보고 싶어요",
          "speaker_id": "2",
          "time": "20210822 18:51"
        },
        {
          "id": "MDRW2100007150.1.19",
          "form": "레미제라블은 영화관에서 보셨나요??",
          "original_form": "레미제라블은 영화관에서 보셨나요??",
          "speaker_id": "1",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100007150.1.20",
          "form": "아뇨ㅠ 집에서 봤어요",
          "original_form": "아뇨ㅠ 집에서 봤어요",
          "speaker_id": "2",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100007150.1.21",
          "form": "ㅠㅠ 다음에 재개봉하면 꼭 영화관에서 한번 보세요!",
          "original_form": "ㅠㅠ 다음에 재개봉하면 꼭 영화관에서 한번 보세요!",
          "speaker_id": "1",
          "time": "20210823 00:43"
        },
        {
          "id": "MDRW2100007150.1.22",
          "form": "요즘 코로나때문에 재개봉하는 영화들이 많아서 좋은 것 같아요",
          "original_form": "요즘 코로나때문에 재개봉하는 영화들이 많아서 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210823 00:43"
        },
        {
          "id": "MDRW2100007150.1.23",
          "form": "오 맞아요ㅠ재개봉 많이 하더라고요",
          "original_form": "오 맞아요ㅠ재개봉 많이 하더라고요",
          "speaker_id": "2",
          "time": "20210823 02:02"
        },
        {
          "id": "MDRW2100007150.1.24",
          "form": "다시보고싶은 영화 있으신가요?",
          "original_form": "다시보고싶은 영화 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 02:09"
        },
        {
          "id": "MDRW2100007150.1.25",
          "form": "저는 판의 미로 좋아해서",
          "original_form": "저는 판의 미로 좋아해서",
          "speaker_id": "2",
          "time": "20210823 03:05"
        },
        {
          "id": "MDRW2100007150.1.26",
          "form": "극장에서 보고 싶네요",
          "original_form": "극장에서 보고 싶네요",
          "speaker_id": "2",
          "time": "20210823 03:05"
        }
      ]
