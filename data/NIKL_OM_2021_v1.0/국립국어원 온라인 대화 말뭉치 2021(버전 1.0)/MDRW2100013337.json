      "utterance": [
        {
          "id": "MDRW2100013337.1.1",
          "form": "초코 이름 귀엽네요",
          "original_form": "초코 이름 귀엽네요",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013337.1.2",
          "form": "얼른키우시게되길 바랄게요",
          "original_form": "얼른키우시게되길 바랄게요",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013337.1.3",
          "form": "네 간식도 좋은거만 먹일거예요 ㅋㅋ",
          "original_form": "네 간식도 좋은거만 먹일거예요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013337.1.4",
          "form": "근데 사실 고양이도 키우고싶어요",
          "original_form": "근데 사실 고양이도 키우고싶어요",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013337.1.5",
          "form": "둘다 키우면되죠 ㅎㅎ",
          "original_form": "둘다 키우면되죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013337.1.6",
          "form": "오 그러네요 ㅋㅋ 혹시 털알러지 있나요?",
          "original_form": "오 그러네요 ㅋㅋ 혹시 털알러지 있나요?",
          "speaker_id": "2",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013337.1.7",
          "form": "전 사실 가끔 재채기해서 ㅋㅋㅋ",
          "original_form": "전 사실 가끔 재채기해서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013337.1.8",
          "form": "알러지는 없어요",
          "original_form": "알러지는 없어요",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013337.1.9",
          "form": "예전에 할아버지집에 둘다키웠는데여",
          "original_form": "예전에 할아버지집에 둘다키웠는데여",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013337.1.10",
          "form": "나중에 개가 고기생선만 먹어요 ㅋㅋㅋ",
          "original_form": "나중에 개가 고기생선만 먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013337.1.11",
          "form": "개의 고양이입맛화",
          "original_form": "개의 고양이입맛화",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013337.1.12",
          "form": "아 정말요? ㅋㅋㅋㅋ 너무 귀엽네요",
          "original_form": "아 정말요? ㅋㅋㅋㅋ 너무 귀엽네요",
          "speaker_id": "2",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013337.1.13",
          "form": "둘이 싸우진않나요?",
          "original_form": "둘이 싸우진않나요?",
          "speaker_id": "2",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013337.1.14",
          "form": "싸웠다는거같아요",
          "original_form": "싸웠다는거같아요",
          "speaker_id": "1",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100013337.1.15",
          "form": "어릴때라 기억은잘안나요",
          "original_form": "어릴때라 기억은잘안나요",
          "speaker_id": "1",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100013337.1.16",
          "form": "ㅋㅋㅋㅋㅋ저희 할아버지도 동물 키우면 좋을텐데 안좋아하셔서 슬퍼요",
          "original_form": "ㅋㅋㅋㅋㅋ저희 할아버지도 동물 키우면 좋을텐데 안좋아하셔서 슬퍼요",
          "speaker_id": "2",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100013337.1.17",
          "form": "길냥이들도 안좋게 보더라구요 ㅠㅠ",
          "original_form": "길냥이들도 안좋게 보더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100013337.1.18",
          "form": "할아버지가요?",
          "original_form": "할아버지가요?",
          "speaker_id": "1",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100013337.1.19",
          "form": "길냥이를요?",
          "original_form": "길냥이를요?",
          "speaker_id": "1",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100013337.1.20",
          "form": "시골 사시는데 길고양이들 많은데 털난린다고 안좋아하세요 ㅋㅋ",
          "original_form": "시골 사시는데 길고양이들 많은데 털난린다고 안좋아하세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100013337.1.21",
          "form": "저는 사실 반려동물 입양하고싶은데",
          "original_form": "저는 사실 반려동물 입양하고싶은데",
          "speaker_id": "2",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013337.1.22",
          "form": "가족반대가 심하네요",
          "original_form": "가족반대가 심하네요",
          "speaker_id": "2",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013337.1.23",
          "form": "아.. 가족이 반대하면 키우기힘들죠 ㅠㅠ",
          "original_form": "아.. 가족이 반대하면 키우기힘들죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013337.1.24",
          "form": "속상하시겠어요",
          "original_form": "속상하시겠어요",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013337.1.25",
          "form": "맞아요 ㅜㅜ 나중에 자취하면 그땐 꼭 키울라고요 ㅋㅋ",
          "original_form": "맞아요 ㅜㅜ 나중에 자취하면 그땐 꼭 키울라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013337.1.26",
          "form": "네 그런분들 많이봤어요 독립후에 키우시는분들요",
          "original_form": "네 그런분들 많이봤어요 독립후에 키우시는분들요",
          "speaker_id": "1",
          "time": "20210825 14:21"
        }
      ]
    }
