      "utterance": [
        {
          "id": "MDRW2100019249.1.1",
          "form": "앗 뭐라고 답장주셨나요",
          "original_form": "앗 뭐라고 답장주셨나요",
          "speaker_id": "2",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019249.1.2",
          "form": "밤고구마 군고구마?",
          "original_form": "밤고구마 군고구마?",
          "speaker_id": "2",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019249.1.3",
          "form": "저는 요즘 밤고구마파입니다 ...!",
          "original_form": "저는 요즘 밤고구마파입니다 ...!",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019249.1.4",
          "form": "군고구마때는 호박이지만 .. 밤그 퍽퍽한 맛이 좋더라구요",
          "original_form": "군고구마때는 호박이지만 .. 밤그 퍽퍽한 맛이 좋더라구요",
          "speaker_id": "1",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019249.1.5",
          "form": "앗.. 역시 뭔가 배우신분",
          "original_form": "앗.. 역시 뭔가 배우신분",
          "speaker_id": "2",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100019249.1.6",
          "form": "저도 밤고구마는 그 목막히는 맛을 좋아해요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 밤고구마는 그 목막히는 맛을 좋아해요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100019249.1.7",
          "form": "그때 우유랑 먹으면 ..굿이에요..",
          "original_form": "그때 우유랑 먹으면 ..굿이에요..",
          "speaker_id": "1",
          "time": "20210827 17:50"
        },
        {
          "id": "MDRW2100019249.1.8",
          "form": "사실 저는 퍽퍽한 빵도 참 좋아한답니다 ... 백설기도요..",
          "original_form": "사실 저는 퍽퍽한 빵도 참 좋아한답니다 ... 백설기도요..",
          "speaker_id": "1",
          "time": "20210827 17:50"
        },
        {
          "id": "MDRW2100019249.1.9",
          "form": "오모나 배우신분",
          "original_form": "오모나 배우신분",
          "speaker_id": "2",
          "time": "20210827 17:51"
        },
        {
          "id": "MDRW2100019249.1.10",
          "form": "백설기에 우유 와다다 털어먹기....",
          "original_form": "백설기에 우유 와다다 털어먹기....",
          "speaker_id": "2",
          "time": "20210827 17:51"
        },
        {
          "id": "MDRW2100019249.1.11",
          "form": "역시 잘아시는 군요...!",
          "original_form": "역시 잘아시는 군요...!",
          "speaker_id": "1",
          "time": "20210827 17:54"
        },
        {
          "id": "MDRW2100019249.1.12",
          "form": "백설기 맛있지 않나요 ..",
          "original_form": "백설기 맛있지 않나요 ..",
          "speaker_id": "1",
          "time": "20210827 17:54"
        },
        {
          "id": "MDRW2100019249.1.13",
          "form": "저 백설기 조아해요.... 그 턱 막히는 느낌...",
          "original_form": "저 백설기 조아해요.... 그 턱 막히는 느낌...",
          "speaker_id": "2",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100019249.1.14",
          "form": "하튜...",
          "original_form": "하튜...",
          "speaker_id": "2",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100019249.1.15",
          "form": "그게 맞죠.. ㅠㅠ 백설기 못 먹은지 좀 됐능데ㅜ먹고싶네요 하",
          "original_form": "그게 맞죠.. ㅠㅠ 백설기 못 먹은지 좀 됐능데ㅜ먹고싶네요 하",
          "speaker_id": "1",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019249.1.16",
          "form": "저는 일터 근처에 떡집이 있습죠 후후후후",
          "original_form": "저는 일터 근처에 떡집이 있습죠 후후후후",
          "speaker_id": "2",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100019249.1.17",
          "form": "오 .. 출퇴근푸드로 딱이네요...",
          "original_form": "오 .. 출퇴근푸드로 딱이네요...",
          "speaker_id": "1",
          "time": "20210827 18:48"
        },
        {
          "id": "MDRW2100019249.1.18",
          "form": "다른 떡은 어떤거 좋아하세요?",
          "original_form": "다른 떡은 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 18:48"
        },
        {
          "id": "MDRW2100019249.1.19",
          "form": "저 절편도 좋아해요...",
          "original_form": "저 절편도 좋아해요...",
          "speaker_id": "2",
          "time": "20210827 18:54"
        },
        {
          "id": "MDRW2100019249.1.20",
          "form": "기름 좌라락 발림 그 맛",
          "original_form": "기름 좌라락 발림 그 맛",
          "speaker_id": "2",
          "time": "20210827 18:54"
        },
        {
          "id": "MDRW2100019249.1.21",
          "form": "오 맞아요...ㅠ 떡이랑 고소한 기름맛!!",
          "original_form": "오 맞아요...ㅠ 떡이랑 고소한 기름맛!!",
          "speaker_id": "1",
          "time": "20210827 19:23"
        },
        {
          "id": "MDRW2100019249.1.22",
          "form": "저는 콩떡 완두떡 좋아해요 콩밥은 싫어하지만 콩떡은 좋아합니다 ㅎ-ㅎ",
          "original_form": "저는 콩떡 완두떡 좋아해요 콩밥은 싫어하지만 콩떡은 좋아합니다 ㅎ-ㅎ",
          "speaker_id": "1",
          "time": "20210827 19:23"
        },
        {
          "id": "MDRW2100019249.1.23",
          "form": "오오오 저두요!!!",
          "original_form": "오오오 저두요!!!",
          "speaker_id": "2",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100019249.1.24",
          "form": "떡 좋아하시는군요 후후후",
          "original_form": "떡 좋아하시는군요 후후후",
          "speaker_id": "2",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100019249.1.25",
          "form": "맞아요 떡 왕 맛있죠..!",
          "original_form": "맞아요 떡 왕 맛있죠..!",
