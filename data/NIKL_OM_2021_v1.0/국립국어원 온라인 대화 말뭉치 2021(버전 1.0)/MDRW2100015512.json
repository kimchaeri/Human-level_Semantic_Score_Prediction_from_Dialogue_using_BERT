      "utterance": [
        {
          "id": "MDRW2100015512.1.1",
          "form": "저는 초밥도",
          "original_form": "저는 초밥도",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015512.1.2",
          "form": "그냥 연어초밥보다",
          "original_form": "그냥 연어초밥보다",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015512.1.3",
          "form": "아부리연어가더좋아요ㅋㅋㅋ",
          "original_form": "아부리연어가더좋아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015512.1.4",
          "form": "아부리 연어는 어떤 연어 인가요?",
          "original_form": "아부리 연어는 어떤 연어 인가요?",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015512.1.5",
          "form": "연어를",
          "original_form": "연어를",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015512.1.6",
          "form": "구운거죠!!!",
          "original_form": "구운거죠!!!",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015512.1.7",
          "form": "아 구운연어가 아부리연어군요",
          "original_form": "아 구운연어가 아부리연어군요",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015512.1.8",
          "form": "저도 구운 연어 좋아해요",
          "original_form": "저도 구운 연어 좋아해요",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015512.1.9",
          "form": "특유의 맛이 너무 좋아요",
          "original_form": "특유의 맛이 너무 좋아요",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015512.1.10",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015512.1.11",
          "form": "저는 황새치도",
          "original_form": "저는 황새치도",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015512.1.12",
          "form": "구운게훨배좋더라고요",
          "original_form": "구운게훨배좋더라고요",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015512.1.13",
          "form": "황새치도 구운게 맛있군요",
          "original_form": "황새치도 구운게 맛있군요",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015512.1.14",
          "form": "다음에 한번 구운황새치 먹어봐야겠어요",
          "original_form": "다음에 한번 구운황새치 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015512.1.15",
          "form": "네네 맞아요ㅋㅋㅋ제취향이랍니다",
          "original_form": "네네 맞아요ㅋㅋㅋ제취향이랍니다",
          "speaker_id": "1",
          "time": "20210826 13:32"
        },
        {
          "id": "MDRW2100015512.1.16",
          "form": "점심드셨나요?",
          "original_form": "점심드셨나요?",
          "speaker_id": "1",
          "time": "20210826 13:32"
        },
        {
          "id": "MDRW2100015512.1.17",
          "form": "넵 규동먹었습니다",
          "original_form": "넵 규동먹었습니다",
          "speaker_id": "2",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100015512.1.18",
          "form": "name1님은 뭐드셨나요?",
          "original_form": "&name1&님은 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100015512.1.19",
          "form": "규동좋네요",
          "original_form": "규동좋네요",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100015512.1.20",
          "form": "저 비빔밥이요ㅋㅋㅋㅋ",
          "original_form": "저 비빔밥이요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100015512.1.21",
          "form": "아 비빔밥 좋죠 언제먹어도 맛있어요",
          "original_form": "아 비빔밥 좋죠 언제먹어도 맛있어요",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100015512.1.22",
          "form": "윤식당에서 비빔밥 파는것 봤었는데",
          "original_form": "윤식당에서 비빔밥 파는것 봤었는데",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100015512.1.23",
          "form": "고추장 없이도 맛있어보이더라구요",
          "original_form": "고추장 없이도 맛있어보이더라구요",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100015512.1.24",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100015512.1.25",
          "form": "거긴 뭘넣던가요????",
          "original_form": "거긴 뭘넣던가요????",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100015512.1.26",
          "form": "고추장 대신 간장 넣어먹더라구요",
          "original_form": "고추장 대신 간장 넣어먹더라구요",
          "speaker_id": "2",
          "time": "20210826 18:52"
        },
        {
          "id": "MDRW2100015512.1.27",
          "form": "고추장을 넣어야 맛있을꺼라고 생각했는데 고소하게 잘만들면 맛있을꺼같아요",
          "original_form": "고추장을 넣어야 맛있을꺼라고 생각했는데 고소하게 잘만들면 맛있을꺼같아요",
          "speaker_id": "2",
          "time": "20210826 18:53"
        },
        {
          "id": "MDRW2100015512.1.28",
          "form": "와우 건강비빔밥이네요",
          "original_form": "와우 건강비빔밥이네요",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100015512.1.29",
          "form": "저도 주로고추장파인데",
          "original_form": "저도 주로고추장파인데",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100015512.1.30",
          "form": "간장을 넣는다니",
          "original_form": "간장을 넣는다니",
          "speaker_id": "1",
          "time": "20210826 19:00"
        }
      ]
    }
