      "utterance": [
        {
          "id": "MDRW2100041461.1.1",
          "form": "잠깐? 클립만요 ㅎㅎㅎ",
          "original_form": "잠깐? 클립만요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 00:48"
        },
        {
          "id": "MDRW2100041461.1.2",
          "form": "라이프 온 마스 봤어요?",
          "original_form": "라이프 온 마스 봤어요?",
          "speaker_id": "1",
          "time": "20210906 00:48"
        },
        {
          "id": "MDRW2100041461.1.3",
          "form": "라이프몬마스도 봤어요",
          "original_form": "라이프몬마스도 봤어요",
          "speaker_id": "2",
          "time": "20210906 00:49"
        },
        {
          "id": "MDRW2100041461.1.4",
          "form": "이것도 재미있지 않나요?!",
          "original_form": "이것도 재미있지 않나요?!",
          "speaker_id": "1",
          "time": "20210906 00:49"
        },
        {
          "id": "MDRW2100041461.1.5",
          "form": "정겨호ㅠㅠㅠㅠ한태주ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "정겨호ㅠㅠㅠㅠ한태주ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 00:50"
        },
        {
          "id": "MDRW2100041461.1.6",
          "form": "네네 재밌어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네네 재밌어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 00:50"
        },
        {
          "id": "MDRW2100041461.1.7",
          "form": "잘보셨네요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "잘보셨네요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:50"
        },
        {
          "id": "MDRW2100041461.1.8",
          "form": "아진짜 너무재미있었어요ㅋㅋㅋㅋ",
          "original_form": "아진짜 너무재미있었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:51"
        },
        {
          "id": "MDRW2100041461.1.9",
          "form": "시즌2나왔으면 좋을텐데 안나오나요ㅠㅠ",
          "original_form": "시즌2나왔으면 좋을텐데 안나오나요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 00:51"
        },
        {
          "id": "MDRW2100041461.1.10",
          "form": "안나올거같아요..ㅠㅠ 근데 정말 작가님이 내주시면 배우들도 할거같은데8ㅁ8",
          "original_form": "안나올거같아요..ㅠㅠ 근데 정말 작가님이 내주시면 배우들도 할거같은데8ㅁ8",
          "speaker_id": "2",
          "time": "20210906 00:53"
        },
        {
          "id": "MDRW2100041461.1.11",
          "form": "맞아요ㅠ 이게 외국꺼 리메이크한거라서 안나온다고 하더라고요ㅠㅠ",
          "original_form": "맞아요ㅠ 이게 외국꺼 리메이크한거라서 안나온다고 하더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 00:54"
        },
        {
          "id": "MDRW2100041461.1.12",
          "form": "작가님만 써주시면 좋을텐데 말이에요ㅠ",
          "original_form": "작가님만 써주시면 좋을텐데 말이에요ㅠ",
          "speaker_id": "1",
          "time": "20210906 00:54"
        },
        {
          "id": "MDRW2100041461.1.13",
          "form": "배우들 합도 좋은데 말이죠.. 아쉬워요 ㅋㅋ",
          "original_form": "배우들 합도 좋은데 말이죠.. 아쉬워요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 00:55"
        },
        {
          "id": "MDRW2100041461.1.14",
          "form": "그니까요ㅋㅋㅋ진짜 재미있었는데요",
          "original_form": "그니까요ㅋㅋㅋ진짜 재미있었는데요",
          "speaker_id": "1",
          "time": "20210906 00:56"
        },
        {
          "id": "MDRW2100041461.1.15",
          "form": "이거 보셨으면 시그널이나 지정생존자도 보셨어요?",
          "original_form": "이거 보셨으면 시그널이나 지정생존자도 보셨어요?",
          "speaker_id": "1",
          "time": "20210906 00:56"
        },
        {
          "id": "MDRW2100041461.1.16",
          "form": "네네 시그널도 봤느데 사건이 무서워서 제대로 보진 못했어요..ㅋㅋㅋㅋ",
          "original_form": "네네 시그널도 봤느데 사건이 무서워서 제대로 보진 못했어요..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 00:57"
        },
        {
          "id": "MDRW2100041461.1.17",
          "form": "올해한 카이로스 보셨나요?",
          "original_form": "올해한 카이로스 보셨나요?",
          "speaker_id": "2",
          "time": "20210906 00:57"
        },
        {
          "id": "MDRW2100041461.1.18",
          "form": "카이로스는 아직 못봤어요ㅠ",
          "original_form": "카이로스는 아직 못봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210906 00:58"
        },
        {
          "id": "MDRW2100041461.1.19",
          "form": "이것도 장르물 좋아하는 사람들이 잘 본거 같더라고요",
          "original_form": "이것도 장르물 좋아하는 사람들이 잘 본거 같더라고요",
          "speaker_id": "1",
          "time": "20210906 00:58"
        },
        {
          "id": "MDRW2100041461.1.20",
          "form": "재미있나요?",
          "original_form": "재미있나요?",
          "speaker_id": "1",
          "time": "20210906 00:59"
        },
        {
          "id": "MDRW2100041461.1.21",
          "form": "네네 타임워프물 좋아하신다면 추천드려요",
          "original_form": "네네 타임워프물 좋아하신다면 추천드려요",
          "speaker_id": "2",
          "time": "20210906 01:03"
        },
        {
          "id": "MDRW2100041461.1.22",
          "form": "사건도 많이 엮여있는데 잘 풀기도했고",
          "original_form": "사건도 많이 엮여있는데 잘 풀기도했고",
          "speaker_id": "2",
          "time": "20210906 01:03"
        },
        {
          "id": "MDRW2100041461.1.23",
          "form": "오 타임워프물이군요!",
          "original_form": "오 타임워프물이군요!",
          "speaker_id": "1",
          "time": "20210906 01:04"
        },
        {
          "id": "MDRW2100041461.1.24",
          "form": "배우가 누구누구나와요?",
          "original_form": "배우가 누구누구나와요?",
          "speaker_id": "1",
          "time": "20210906 01:04"
        }
      ]
