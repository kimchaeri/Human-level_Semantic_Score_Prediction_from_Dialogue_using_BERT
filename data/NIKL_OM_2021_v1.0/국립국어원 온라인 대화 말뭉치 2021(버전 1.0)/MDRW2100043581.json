      "utterance": [
        {
          "id": "MDRW2100043581.1.1",
          "form": "나 여기서 기억남",
          "original_form": "나 여기서 기억남",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043581.1.2",
          "form": "여기서 주인공 맞죠?",
          "original_form": "여기서 주인공 맞죠?",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043581.1.3",
          "form": "엉 신애라나오는 드라마인데 상대역",
          "original_form": "엉 신애라나오는 드라마인데 상대역",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043581.1.4",
          "form": "기억이 나는거 같음.  보기는 봤는데",
          "original_form": "기억이 나는거 같음.  보기는 봤는데",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043581.1.5",
          "form": "맛있는 인생에도 나오고",
          "original_form": "맛있는 인생에도 나오고",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043581.1.6",
          "form": "생각보다 많이 나왔네요",
          "original_form": "생각보다 많이 나왔네요",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043581.1.7",
          "form": "심지어 신의 여기도",
          "original_form": "심지어 신의 여기도",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043581.1.8",
          "form": "맛있는 인생 여기는 혜리도 나왔어요ㅋ",
          "original_form": "맛있는 인생 여기는 혜리도 나왔어요ㅋ",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043581.1.9",
          "form": "당신의 여자 여기서 다시 주연",
          "original_form": "당신의 여자 여기서 다시 주연",
          "speaker_id": "2",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043581.1.10",
          "form": "그러네요.여주가 이유리",
          "original_form": "그러네요.여주가 이유리",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043581.1.11",
          "form": "맞네 혜리가 네딸중에 막내역할로",
          "original_form": "맞네 혜리가 네딸중에 막내역할로",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043581.1.12",
          "form": "혜리도 은근 출연 마니 했네요",
          "original_form": "혜리도 은근 출연 마니 했네요",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043581.1.13",
          "form": "그녀의 신화 여기도 주연",
          "original_form": "그녀의 신화 여기도 주연",
          "speaker_id": "2",
