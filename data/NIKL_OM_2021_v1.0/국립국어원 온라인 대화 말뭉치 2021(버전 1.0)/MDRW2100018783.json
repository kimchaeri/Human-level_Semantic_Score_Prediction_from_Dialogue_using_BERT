      "utterance": [
        {
          "id": "MDRW2100018783.1.1",
          "form": "동물 좋아하세요?",
          "original_form": "동물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018783.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018783.1.3",
          "form": "특히 고양이라고",
          "original_form": "특히 고양이라고",
          "speaker_id": "2",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018783.1.4",
          "form": "이름이 name3라고",
          "original_form": "이름이 &name3&라고",
          "speaker_id": "2",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018783.1.5",
          "form": "와 저는 거미 키우는데!!",
          "original_form": "와 저는 거미 키우는데!!",
          "speaker_id": "1",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018783.1.6",
          "form": "이름은 name4이에요",
          "original_form": "이름은 &name4&이에요",
          "speaker_id": "1",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100018783.1.7",
          "form": "예...?",
          "original_form": "예...?",
          "speaker_id": "2",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100018783.1.8",
          "form": "우와...",
          "original_form": "우와...",
          "speaker_id": "2",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100018783.1.9",
          "form": "애완거미도 노래 잘 하나요?",
          "original_form": "애완거미도 노래 잘 하나요?",
          "speaker_id": "2",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100018783.1.10",
          "form": "자기들만의 언어로는 할텐데 제 귀가 너무 커서 잘 안들리더라구요",
          "original_form": "자기들만의 언어로는 할텐데 제 귀가 너무 커서 잘 안들리더라구요",
          "speaker_id": "1",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100018783.1.11",
          "form": "아 그쵸그쵸",
          "original_form": "아 그쵸그쵸",
          "speaker_id": "2",
          "time": "20210827 12:10"
        },
        {
          "id": "MDRW2100018783.1.12",
          "form": "name3도 한 곡조 뽑나요?",
          "original_form": "&name3&도 한 곡조 뽑나요?",
          "speaker_id": "1",
          "time": "20210827 12:10"
        },
        {
          "id": "MDRW2100018783.1.13",
          "form": "거미분들도 아마 유거미의 스케치북 다 할 거에요",
          "original_form": "거미분들도 아마 유거미의 스케치북 다 할 거에요",
          "speaker_id": "2",
          "time": "20210827 12:10"
        },
        {
          "id": "MDRW2100018783.1.14",
          "form": "아 잘 뽑죠",
          "original_form": "아 잘 뽑죠",
          "speaker_id": "2",
          "time": "20210827 12:10"
        },
        {
          "id": "MDRW2100018783.1.15",
          "form": "name3는 성악 쪽이에요",
          "original_form": "&name3&는 성악 쪽이에요",
          "speaker_id": "2",
          "time": "20210827 12:11"
        },
        {
          "id": "MDRW2100018783.1.16",
          "form": "옆에서 화음넣어주고싶으시겠어요",
          "original_form": "옆에서 화음넣어주고싶으시겠어요",
          "speaker_id": "1",
          "time": "20210827 12:11"
        },
        {
          "id": "MDRW2100018783.1.17",
          "form": "주로 악을 질러서",
          "original_form": "주로 악을 질러서",
          "speaker_id": "2",
          "time": "20210827 12:11"
        },
        {
          "id": "MDRW2100018783.1.18",
          "form": "간식은 자주 주시나요?",
          "original_form": "간식은 자주 주시나요?",
          "speaker_id": "1",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100018783.1.19",
          "form": "화음 장조로 넣어요 주로",
          "original_form": "화음 장조로 넣어요 주로",
          "speaker_id": "2",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100018783.1.20",
          "form": "1일 10간식이라",
          "original_form": "1일 10간식이라",
          "speaker_id": "2",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100018783.1.21",
          "form": "제 허리가 고양이보다 잘 꺽여요",
          "original_form": "제 허리가 고양이보다 잘 꺽여요",
          "speaker_id": "2",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100018783.1.22",
          "form": "장난감도 잘 가지고 노시나요?",
          "original_form": "장난감도 잘 가지고 노시나요?",
          "speaker_id": "1",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018783.1.23",
          "form": "네 올해 잃어버린 장난감만 해도 10개네요",
          "original_form": "네 올해 잃어버린 장난감만 해도 10개네요",
          "speaker_id": "2",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018783.1.24",
          "form": "거미들은 주로 뭘 하나요?",
          "original_form": "거미들은 주로 뭘 하나요?",
          "speaker_id": "2",
          "time": "20210827 12:14"
        },
        {
          "id": "MDRW2100018783.1.25",
          "form": "집 빼앗기?",
          "original_form": "집 빼앗기?",
          "speaker_id": "2",
          "time": "20210827 12:14"
        },
        {
          "id": "MDRW2100018783.1.26",
          "form": "서로 깜짝 놀래키기 놀이 좋아하더라고요",
          "original_form": "서로 깜짝 놀래키기 놀이 좋아하더라고요",
          "speaker_id": "1",
          "time": "20210827 12:18"
        }
      ]
    }
