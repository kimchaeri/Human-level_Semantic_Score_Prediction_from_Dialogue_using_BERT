        {
          "id": "MDRW2100045644.1.2",
          "form": "내일도 비온대",
          "original_form": "내일도 비온대",
          "speaker_id": "1",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045644.1.3",
          "form": "우산 챙겨야겠다 땡큐",
          "original_form": "우산 챙겨야겠다 땡큐",
          "speaker_id": "2",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045644.1.4",
          "form": "이번주 내내 비오네",
          "original_form": "이번주 내내 비오네",
          "speaker_id": "1",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045644.1.5",
          "form": "가을장마인가?",
          "original_form": "가을장마인가?",
          "speaker_id": "2",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045644.1.6",
          "form": "진짜 가을인가봐",
          "original_form": "진짜 가을인가봐",
          "speaker_id": "1",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045644.1.7",
          "form": "맞아 요즘 날이 추워",
          "original_form": "맞아 요즘 날이 추워",
          "speaker_id": "2",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045644.1.8",
          "form": "반팔입을 날씨는 아니지",
          "original_form": "반팔입을 날씨는 아니지",
          "speaker_id": "1",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045644.1.9",
          "form": "맞어 그 위에 긴팔하나 더 입어야해",
          "original_form": "맞어 그 위에 긴팔하나 더 입어야해",
          "speaker_id": "2",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045644.1.10",
          "form": "무슨 계절을 가장 좋아해?",
          "original_form": "무슨 계절을 가장 좋아해?",
          "speaker_id": "1",
          "time": "20210908 04:41"
        },
        {
          "id": "MDRW2100045644.1.11",
          "form": "나는 봄이 좋아",
          "original_form": "나는 봄이 좋아",
          "speaker_id": "2",
          "time": "20210908 04:41"
        },
        {
          "id": "MDRW2100045644.1.12",
          "form": "나는 가을이 좋아",
          "original_form": "나는 가을이 좋아",
          "speaker_id": "1",
          "time": "20210908 04:41"
        },
        {
          "id": "MDRW2100045644.1.13",
          "form": "옛날은 방학있는 여름이 좋았는데",
          "original_form": "옛날은 방학있는 여름이 좋았는데",
          "speaker_id": "2",
          "time": "20210908 04:41"
        },
        {
          "id": "MDRW2100045644.1.14",
          "form": "ㅋㅋㅋㅋ 학생때는 나도 그랬어",
          "original_form": "ㅋㅋㅋㅋ 학생때는 나도 그랬어",
