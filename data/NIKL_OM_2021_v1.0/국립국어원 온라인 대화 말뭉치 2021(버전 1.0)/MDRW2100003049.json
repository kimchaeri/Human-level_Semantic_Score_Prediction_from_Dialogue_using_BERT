      "utterance": [
        {
          "id": "MDRW2100003049.1.1",
          "form": "코르나때문에 못갔지",
          "original_form": "코르나때문에 못갔지",
          "speaker_id": "2",
          "time": "20210820 17:58"
        },
        {
          "id": "MDRW2100003049.1.2",
          "form": "영양에 12.31~1.1 해맞이 행사한데",
          "original_form": "영양에 12.31~1.1 해맞이 행사한데",
          "speaker_id": "1",
          "time": "20210820 17:59"
        },
        {
          "id": "MDRW2100003049.1.3",
          "form": "강원도 영양?",
          "original_form": "강원도 영양?",
          "speaker_id": "2",
          "time": "20210820 17:59"
        },
        {
          "id": "MDRW2100003049.1.4",
          "form": "엉 맞아",
          "original_form": "엉 맞아",
          "speaker_id": "1",
          "time": "20210820 17:59"
        },
        {
          "id": "MDRW2100003049.1.5",
          "form": "너무 멀다",
          "original_form": "너무 멀다",
          "speaker_id": "2",
          "time": "20210820 17:59"
        },
        {
          "id": "MDRW2100003049.1.6",
          "form": "낙산사에서! 연등달기도 하고 타종식도 있어",
          "original_form": "낙산사에서! 연등달기도 하고 타종식도 있어",
          "speaker_id": "1",
          "time": "20210820 18:00"
        },
        {
          "id": "MDRW2100003049.1.7",
          "form": "사람이 많겠다 그렁",
          "original_form": "사람이 많겠다 그렁",
          "speaker_id": "2",
          "time": "20210820 18:00"
        },
        {
          "id": "MDRW2100003049.1.8",
          "form": "해넘이 축하공연도 있네",
          "original_form": "해넘이 축하공연도 있네",
          "speaker_id": "1",
          "time": "20210820 18:00"
        },
        {
          "id": "MDRW2100003049.1.9",
          "form": "그것도 낙산사야?",
          "original_form": "그것도 낙산사야?",
          "speaker_id": "2",
          "time": "20210820 18:01"
        },
        {
          "id": "MDRW2100003049.1.10",
          "form": "아니 이건 낙산해변에서",
          "original_form": "아니 이건 낙산해변에서",
          "speaker_id": "1",
          "time": "20210820 18:01"
        },
        {
          "id": "MDRW2100003049.1.11",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210820 18:01"
        },
        {
          "id": "MDRW2100003049.1.12",
          "form": "심지어 밤에 불꽃놀이도 한다",
          "original_form": "심지어 밤에 불꽃놀이도 한다",
          "speaker_id": "1",
          "time": "20210820 18:01"
        },
        {
          "id": "MDRW2100003049.1.13",
          "form": "프로그램이 많네",
          "original_form": "프로그램이 많네",
          "speaker_id": "2",
          "time": "20210820 18:01"
        },
        {
          "id": "MDRW2100003049.1.14",
          "form": "그렇지 불꽃축제보고 다음날 해돋이 보고",
          "original_form": "그렇지 불꽃축제보고 다음날 해돋이 보고",
          "speaker_id": "1",
          "time": "20210820 18:02"
        },
        {
          "id": "MDRW2100003049.1.15",
          "form": "좋긴한데 사람 많을 거같고 힘들지 않을까",
          "original_form": "좋긴한데 사람 많을 거같고 힘들지 않을까",
          "speaker_id": "2",
          "time": "20210820 18:02"
        },
        {
          "id": "MDRW2100003049.1.16",
          "form": "감수해야지 그정도는",
          "original_form": "감수해야지 그정도는",
          "speaker_id": "1",
          "time": "20210820 18:02"
        }
      ]
    }
