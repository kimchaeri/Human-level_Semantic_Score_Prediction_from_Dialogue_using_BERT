      "utterance": [
        {
          "id": "MDRW2100039392.1.1",
          "form": "저녁에 뭐 드실 예정이신가요?",
          "original_form": "저녁에 뭐 드실 예정이신가요?",
          "speaker_id": "1",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039392.1.2",
          "form": "저는요",
          "original_form": "저는요",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039392.1.3",
          "form": "복숭아요",
          "original_form": "복숭아요",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039392.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039392.1.5",
          "form": "저는 생선구이 먹을거같아요!!",
          "original_form": "저는 생선구이 먹을거같아요!!",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039392.1.6",
          "form": "복숭아가 저녁이라니!!!",
          "original_form": "복숭아가 저녁이라니!!!",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039392.1.7",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039392.1.8",
          "form": "배고프시겠어요!!",
          "original_form": "배고프시겠어요!!",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039392.1.9",
          "form": "고등어요?",
          "original_form": "고등어요?",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039392.1.10",
          "form": "넹 마자요",
          "original_form": "넹 마자요",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039392.1.11",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039392.1.12",
          "form": "박대라는 생선구이에요!",
          "original_form": "박대라는 생선구이에요!",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039392.1.13",
          "form": "들어보셨나요?",
          "original_form": "들어보셨나요?",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039392.1.14",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039392.1.15",
          "form": "박대기는 알아요",
          "original_form": "박대기는 알아요",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039392.1.16",
          "form": "박대 들어는 봤죠",
          "original_form": "박대 들어는 봤죠",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039392.1.17",
          "form": "박대를요??",
          "original_form": "박대를요??",
          "speaker_id": "1",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100039392.1.18",
          "form": "네 사실 먹어도 봤어요",
          "original_form": "네 사실 먹어도 봤어요",
          "speaker_id": "2",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100039392.1.19",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039392.1.20",
          "form": "저는 이번에 첨 들어보고 첨 먹어봤는데",
          "original_form": "저는 이번에 첨 들어보고 첨 먹어봤는데",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039392.1.21",
          "form": "맛이 밍숭맹숭",
          "original_form": "맛이 밍숭맹숭",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039392.1.22",
          "form": "어떤셨나요",
          "original_form": "어떤셨나요",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
