        },
        {
          "id": "MDRW2100003278.1.2",
          "form": "안녕하세요 ㅎ 커뮤니티 하시나요?",
          "original_form": "안녕하세요 ㅎ 커뮤니티 하시나요?",
          "speaker_id": "2",
          "time": "20210820 21:21"
        },
        {
          "id": "MDRW2100003278.1.3",
          "form": "저는 가끔 디씨 구경만해요",
          "original_form": "저는 가끔 디씨 구경만해요",
          "speaker_id": "1",
          "time": "20210820 21:23"
        },
        {
          "id": "MDRW2100003278.1.4",
          "form": "하시는 커뮤니티 있어요?",
          "original_form": "하시는 커뮤니티 있어요?",
          "speaker_id": "1",
          "time": "20210820 21:24"
        },
        {
          "id": "MDRW2100003278.1.5",
          "form": "저도 그냥 다양하게 하네요",
          "original_form": "저도 그냥 다양하게 하네요",
          "speaker_id": "2",
          "time": "20210820 21:34"
        },
        {
          "id": "MDRW2100003278.1.6",
          "form": "커뮤는 정보가 빨라서 좋은 거 같아요",
          "original_form": "커뮤는 정보가 빨라서 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210820 21:34"
        },
        {
          "id": "MDRW2100003278.1.7",
          "form": "네 취미생활 공유도 하고",
          "original_form": "네 취미생활 공유도 하고",
          "speaker_id": "1",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100003278.1.8",
          "form": "어느 주제로 커뮤니티하세요?",
          "original_form": "어느 주제로 커뮤니티하세요?",
          "speaker_id": "1",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100003278.1.9",
          "form": "저도 일상 부분이랑 취미 그런거요!@",
          "original_form": "저도 일상 부분이랑 취미 그런거요!@",
          "speaker_id": "2",
          "time": "20210820 21:41"
        },
        {
          "id": "MDRW2100003278.1.10",
          "form": "어느 취미 있으세요?",
          "original_form": "어느 취미 있으세요?",
          "speaker_id": "1",
          "time": "20210820 21:45"
        },
        {
          "id": "MDRW2100003278.1.11",
          "form": "저 그냥 소소하게 옷 스타일 이런 거 보는 거 좋아해요",
          "original_form": "저 그냥 소소하게 옷 스타일 이런 거 보는 거 좋아해요",
          "speaker_id": "2",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003278.1.12",
          "form": "취미라고 하기도 그렇죠",
          "original_form": "취미라고 하기도 그렇죠",
          "speaker_id": "2",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003278.1.13",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210820 21:49"
        },
        {
          "id": "MDRW2100003278.1.14",
          "form": "저는 게임이나 그냥 소소한것들?",
          "original_form": "저는 게임이나 그냥 소소한것들?",
          "speaker_id": "1",
          "time": "20210820 21:51"
        },
        {
          "id": "MDRW2100003278.1.15",
          "form": "웃긴거위주로 보는거같아요",
          "original_form": "웃긴거위주로 보는거같아요",
          "speaker_id": "1",
          "time": "20210820 21:51"
        },
        {
          "id": "MDRW2100003278.1.16",
          "form": "오 웃긴 거 좋죠",
          "original_form": "오 웃긴 거 좋죠",
          "speaker_id": "2",
          "time": "20210820 21:53"
        },
        {
          "id": "MDRW2100003278.1.17",
          "form": "웃긴더 보면 기분이 풀리더라구요",
          "original_form": "웃긴더 보면 기분이 풀리더라구요",
          "speaker_id": "2",
          "time": "20210820 21:53"
        },
        {
          "id": "MDRW2100003278.1.18",
          "form": "혹시 정모나 그런거 나가보셨어요?",
          "original_form": "혹시 정모나 그런거 나가보셨어요?",
          "speaker_id": "1",
          "time": "20210820 21:54"
        },
        {
          "id": "MDRW2100003278.1.19",
          "form": "아니요 근데 한두명은 만나봤어요!@",
          "original_form": "아니요 근데 한두명은 만나봤어요!@",
          "speaker_id": "2",
