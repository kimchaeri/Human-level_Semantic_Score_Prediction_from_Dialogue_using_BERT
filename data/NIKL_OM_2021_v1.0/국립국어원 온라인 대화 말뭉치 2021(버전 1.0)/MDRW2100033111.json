      "utterance": [
        {
          "id": "MDRW2100033111.1.1",
          "form": "하이요!",
          "original_form": "하이요!",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100033111.1.2",
          "form": "루둥이~~~~~~~ 반가버요",
          "original_form": "루둥이~~~~~~~ 반가버요",
          "speaker_id": "2",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100033111.1.3",
          "form": "저는 요즘에 제일 인상깊게 읽은 책우",
          "original_form": "저는 요즘에 제일 인상깊게 읽은 책우",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100033111.1.4",
          "form": "은",
          "original_form": "은",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100033111.1.5",
          "form": "이별과 이별할때 인데",
          "original_form": "이별과 이별할때 인데",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100033111.1.6",
          "form": "너무 슬퍼서",
          "original_form": "너무 슬퍼서",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100033111.1.7",
          "form": "눈물질질흘렸어요",
          "original_form": "눈물질질흘렸어요",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100033111.1.8",
          "form": "오!! 전에 추천해 줬던 그 책..",
          "original_form": "오!! 전에 추천해 줬던 그 책..",
          "speaker_id": "2",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100033111.1.9",
          "form": "무슨 내용인데 눈물이 그렇게 흘렀는지..",
          "original_form": "무슨 내용인데 눈물이 그렇게 흘렀는지..",
          "speaker_id": "2",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100033111.1.10",
          "form": "약간만 스포좀 해 주세용",
          "original_form": "약간만 스포좀 해 주세용",
          "speaker_id": "2",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100033111.1.11",
          "form": "작가님이 간호조무사로 일한 이야기를 담은 이야기걸랑요",
          "original_form": "작가님이 간호조무사로 일한 이야기를 담은 이야기걸랑요",
          "speaker_id": "1",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100033111.1.12",
          "form": "이게 가족이 죽는 그 순간이랑.....",
          "original_form": "이게 가족이 죽는 그 순간이랑.....",
          "speaker_id": "1",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100033111.1.13",
          "form": "요양병원에 무슨 일이 있었는지 이야기인데!",
          "original_form": "요양병원에 무슨 일이 있었는지 이야기인데!",
          "speaker_id": "1",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100033111.1.14",
          "form": "진짜 너무 슬퍼요",
          "original_form": "진짜 너무 슬퍼요",
          "speaker_id": "1",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100033111.1.15",
          "form": "진차ㅜㅜㅡㅜㅜㅜㅜㅡㅜㅜㅜ",
          "original_form": "진차ㅜㅜㅡㅜㅜㅜㅜㅡㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100033111.1.16",
          "form": "진짜 인상깊게 읽었어요...",
          "original_form": "진짜 인상깊게 읽었어요...",
          "speaker_id": "1",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100033111.1.17",
          "form": "헉... 그냥 표지만 봤어서 연애 이별 이야기인 줄 알았는데",
          "original_form": "헉... 그냥 표지만 봤어서 연애 이별 이야기인 줄 알았는데",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100033111.1.18",
          "form": "진짜 환자분들 이야기 하나하나 다 인상깊어서..",
          "original_form": "진짜 환자분들 이야기 하나하나 다 인상깊어서..",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100033111.1.19",
          "form": "추천해요",
          "original_form": "추천해요",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100033111.1.20",
          "form": "오케이.. 나중에 종강하고 시간 많을 때 읽어봐야겠으요",
          "original_form": "오케이.. 나중에 종강하고 시간 많을 때 읽어봐야겠으요",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100033111.1.21",
          "form": "나는 사실 요즘 강의 듣고 과제하느라 바빠서 책을 잘 안 보는데.",
          "original_form": "나는 사실 요즘 강의 듣고 과제하느라 바빠서 책을 잘 안 보는데.",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033111.1.22",
          "form": "흠...",
          "original_form": "흠...",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033111.1.23",
          "form": "내가 제일 인상깊게 읽은 책이.. 뭐였지",
          "original_form": "내가 제일 인상깊게 읽은 책이.. 뭐였지",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033111.1.24",
          "form": "나도 요즘에 책은 잘 안읽어",
          "original_form": "나도 요즘에 책은 잘 안읽어",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100033111.1.25",
          "form": "너무....바쁩니다.",
          "original_form": "너무....바쁩니다.",
          "speaker_id": "1",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100033111.1.26",
          "form": "맞아요~ 학교생활만 해도 시간이 부족해~ ㅠ.ㅠ",
          "original_form": "맞아요~ 학교생활만 해도 시간이 부족해~ ㅠ.ㅠ",
          "speaker_id": "2",
          "time": "20210906 17:01"
        },
        {
          "id": "MDRW2100033111.1.27",
          "form": "그래도 책은 읽으면 좋으니까!!",
          "original_form": "그래도 책은 읽으면 좋으니까!!",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100033111.1.28",
          "form": "좀 여유로워지면",
          "original_form": "좀 여유로워지면",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100033111.1.29",
          "form": "읽을려구요",
          "original_form": "읽을려구요",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100033111.1.30",
          "form": "맞아요! 확실히 사람은 책을 많이 읽어야 하는 듯..",
          "original_form": "맞아요! 확실히 사람은 책을 많이 읽어야 하는 듯..",
          "speaker_id": "2",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100033111.1.31",
          "form": "책 너무 안 읽오서 요즘 약간 바보가 된 느낌이잖아.. ㅋㅋㅋ",
          "original_form": "책 너무 안 읽오서 요즘 약간 바보가 된 느낌이잖아.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100033111.1.32",
          "form": "맞앜ㅎㅋㅎㅋㅎㅋㅎㄲ 너무 안 읽어서 어휘력이 떨어졌어",
          "original_form": "맞앜ㅎㅋㅎㅋㅎㅋㅎㄲ 너무 안 읽어서 어휘력이 떨어졌어",
          "speaker_id": "1",
          "time": "20210906 17:07"
        }
      ]
    }
