      "utterance": [
        {
          "id": "MDRW2100004578.1.1",
          "form": "날씨가 완전 가을이네요~",
          "original_form": "날씨가 완전 가을이네요~",
          "speaker_id": "1",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004578.1.2",
          "form": "이제 선선해지고 있네요",
          "original_form": "이제 선선해지고 있네요",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004578.1.3",
          "form": "이렇게 여름이 지나가나봐요~",
          "original_form": "이렇게 여름이 지나가나봐요~",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004578.1.4",
          "form": "어느계절 좋아하세요?",
          "original_form": "어느계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004578.1.5",
          "form": "저는 요즘같은 가을 날씨를 좋아해요",
          "original_form": "저는 요즘같은 가을 날씨를 좋아해요",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004578.1.6",
          "form": "선선하니 좋으네요",
          "original_form": "선선하니 좋으네요",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004578.1.7",
          "form": "저도요 근데 요샌 가을이 훅 지나가는 느낌이라 아쉬워요ㅠㅠ",
          "original_form": "저도요 근데 요샌 가을이 훅 지나가는 느낌이라 아쉬워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004578.1.8",
          "form": "그니까요 ㅜㅜ 곧 겨울이 오겠죠 ?",
          "original_form": "그니까요 ㅜㅜ 곧 겨울이 오겠죠 ?",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004578.1.9",
          "form": "이번 겨울 날씨는 엄청 추울거같아요",
          "original_form": "이번 겨울 날씨는 엄청 추울거같아요",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004578.1.10",
          "form": "이제 여름,겨울만 있는 느낌ㅠㅠ",
          "original_form": "이제 여름,겨울만 있는 느낌ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004578.1.11",
          "form": "맞아요 추울 것 같죠? 이번에도 롱패딩 둘둘..",
          "original_form": "맞아요 추울 것 같죠? 이번에도 롱패딩 둘둘..",
          "speaker_id": "1",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004578.1.12",
          "form": "그니까요 ㅠ 계절이 다 짧아지네요",
          "original_form": "그니까요 ㅠ 계절이 다 짧아지네요",
          "speaker_id": "2",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004578.1.13",
          "form": "요샌 롱패딩 없으면 겨울을 못 버틸거같아요",
          "original_form": "요샌 롱패딩 없으면 겨울을 못 버틸거같아요",
          "speaker_id": "2",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004578.1.14",
          "form": "맞아요 코트 한벌 샀는데 몇번 못 입고 롱패딩으로 갈아탈 느낌ㅎㅎ",
          "original_form": "맞아요 코트 한벌 샀는데 몇번 못 입고 롱패딩으로 갈아탈 느낌ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004578.1.15",
          "form": "다들 길거리에 롱패딩이기도 하고요",
          "original_form": "다들 길거리에 롱패딩이기도 하고요",
          "speaker_id": "1",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004578.1.16",
          "form": "그니까요 전에 코트 입었다가 너무 추워서",
          "original_form": "그니까요 전에 코트 입었다가 너무 추워서",
          "speaker_id": "2",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004578.1.17",
          "form": "겨울동안 롱패딩만 엄청 입었네요",
          "original_form": "겨울동안 롱패딩만 엄청 입었네요",
          "speaker_id": "2",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004578.1.18",
          "form": "맞아요 아니면 코트 속에 패딩조끼 필수!",
          "original_form": "맞아요 아니면 코트 속에 패딩조끼 필수!",
          "speaker_id": "1",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004578.1.19",
          "form": "작년에도 추워서 히트텍같은 내복종류들 많이 껴입었던 기억이 있네요",
          "original_form": "작년에도 추워서 히트텍같은 내복종류들 많이 껴입었던 기억이 있네요",
          "speaker_id": "1",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004578.1.20",
          "form": "사람들이 히트텍 많이 입더라구요",
          "original_form": "사람들이 히트텍 많이 입더라구요",
          "speaker_id": "2",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004578.1.21",
          "form": "저도 올 겨울을 대비해서 하나 사둬야겠어요 ㅎㅎ",
          "original_form": "저도 올 겨울을 대비해서 하나 사둬야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004578.1.22",
          "form": "네 은근 따뜻하고 입고 안입고 크키가 크더라고요!",
          "original_form": "네 은근 따뜻하고 입고 안입고 크키가 크더라고요!",
          "speaker_id": "1",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004578.1.23",
          "form": "지금 아마 찾아보시면 할인하는 가격 많을지도!!",
          "original_form": "지금 아마 찾아보시면 할인하는 가격 많을지도!!",
          "speaker_id": "1",
          "time": "20210821 17:28"
        }
      ]
    }
  ]
