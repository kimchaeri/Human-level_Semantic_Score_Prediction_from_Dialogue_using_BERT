      "utterance": [
        {
          "id": "MDRW2100022271.1.1",
          "form": "다담소스가 또 뭐가 있죠?",
          "original_form": "다담소스가 또 뭐가 있죠?",
          "speaker_id": "2",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022271.1.2",
          "form": "부대찌개도 본 것 같은데..",
          "original_form": "부대찌개도 본 것 같은데..",
          "speaker_id": "2",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022271.1.3",
          "form": "부대찌개도 맛있어요",
          "original_form": "부대찌개도 맛있어요",
          "speaker_id": "1",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022271.1.4",
          "form": "부대찌개는 근데 재료는 다 따로 사야되죠?",
          "original_form": "부대찌개는 근데 재료는 다 따로 사야되죠?",
          "speaker_id": "2",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022271.1.5",
          "form": "탕수육소스도 있고  떡볶이 오징어볶음..",
          "original_form": "탕수육소스도 있고  떡볶이 오징어볶음..",
          "speaker_id": "1",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022271.1.6",
          "form": "네 소스만 파는거라",
          "original_form": "네 소스만 파는거라",
          "speaker_id": "1",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022271.1.7",
          "form": "재료는 사야해서..",
          "original_form": "재료는 사야해서..",
          "speaker_id": "1",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022271.1.8",
          "form": "그건 좀 안끌린에ㅛ",
          "original_form": "그건 좀 안끌린에ㅛ",
          "speaker_id": "2",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022271.1.9",
          "form": "순두부찌개처럼 간편하게 딱 만드는 게 좋아요 저는",
          "original_form": "순두부찌개처럼 간편하게 딱 만드는 게 좋아요 저는",
          "speaker_id": "2",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022271.1.10",
          "form": "전 부대찌개는 다담 안쓰고 밀키트사먹어요",
          "original_form": "전 부대찌개는 다담 안쓰고 밀키트사먹어요",
          "speaker_id": "1",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022271.1.11",
          "form": "강된장? 그것도 좋았어요 ㅎㅎ",
          "original_form": "강된장? 그것도 좋았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022271.1.12",
          "form": "아 밀키트도 있었네요",
          "original_form": "아 밀키트도 있었네요",
          "speaker_id": "2",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022271.1.13",
          "form": "네 밀키트 이용하시나요",
          "original_form": "네 밀키트 이용하시나요",
          "speaker_id": "1",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022271.1.14",
          "form": "자주는 아닌데 종종 이용해요",
          "original_form": "자주는 아닌데 종종 이용해요",
          "speaker_id": "2",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100022271.1.15",
          "form": "밀푀유나베 밀키트 써봤는데 진짜 간편해서 ㅋㅋ",
          "original_form": "밀푀유나베 밀키트 써봤는데 진짜 간편해서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100022271.1.16",
          "form": "밀푀유좋아하세요",
          "original_form": "밀푀유좋아하세요",
          "speaker_id": "1",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100022271.1.17",
          "form": "넣고 끓이면 끝이더라구요?",
          "original_form": "넣고 끓이면 끝이더라구요?",
          "speaker_id": "2",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100022271.1.18",
          "form": "좋아하죠 배추깻잎고기",
          "original_form": "좋아하죠 배추깻잎고기",
          "speaker_id": "2",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100022271.1.19",
          "form": "그게 밀키트의 장점이죠",
          "original_form": "그게 밀키트의 장점이죠",
          "speaker_id": "1",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100022271.1.20",
          "form": "완전식품 아닌가요 이정도면 ㅎㅎ",
          "original_form": "완전식품 아닌가요 이정도면 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:55"
        }
      ]
