      "utterance": [
        {
          "id": "MDRW2100034253.1.1",
          "form": "저 펜트 볼께요 ㅋㅋ",
          "original_form": "저 펜트 볼께요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034253.1.2",
          "form": "곧 펜트하우스 시작이네요",
          "original_form": "곧 펜트하우스 시작이네요",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034253.1.3",
          "form": "네 ㅋㅋ보고 올께요",
          "original_form": "네 ㅋㅋ보고 올께요",
          "speaker_id": "2",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034253.1.4",
          "form": "넵 다녀오시고 알려주세용",
          "original_form": "넵 다녀오시고 알려주세용",
          "speaker_id": "1",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100034253.1.5",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100034253.1.6",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034253.1.7",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100034253.1.8",
          "form": "이건뭐 허무하네요",
          "original_form": "이건뭐 허무하네요",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100034253.1.9",
          "form": "펜트하우스 ㅜㅜ작가님 어찌",
          "original_form": "펜트하우스 ㅜㅜ작가님 어찌",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100034253.1.10",
          "form": "넘 심하니 참나 뭔가 허무해요 ㅋㅋ",
          "original_form": "넘 심하니 참나 뭔가 허무해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100034253.1.11",
          "form": "헤라 팰리스를 폭탄으로 ㅋㅋ날려 버렸어요 ㅋㅋ",
          "original_form": "헤라 팰리스를 폭탄으로 ㅋㅋ날려 버렸어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100034253.1.12",
          "form": "어이가 없네요",
          "original_form": "어이가 없네요",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100034253.1.13",
          "form": "우리나라에 그런 폭탄이 ㅋㅋ있다니",
          "original_form": "우리나라에 그런 폭탄이 ㅋㅋ있다니",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100034253.1.14",
          "form": "세상에ㅋㅋ 그렇게 대박이였나요?",
          "original_form": "세상에ㅋㅋ 그렇게 대박이였나요?",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100034253.1.15",
          "form": "폭탄이라니ㅋㅋ",
          "original_form": "폭탄이라니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100034253.1.16",
          "form": "네 넘 웃겨요",
          "original_form": "네 넘 웃겨요",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100034253.1.17",
          "form": "다 폭파시켰어요",
          "original_form": "다 폭파시켰어요",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034253.1.18",
          "form": "오늘 뭔가 마무리가 그렇지만",
          "original_form": "오늘 뭔가 마무리가 그렇지만",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100034253.1.19",
          "form": "그래도 다봤네요 ㅎㅎ",
          "original_form": "그래도 다봤네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100034253.1.20",
          "form": "ㅋㅋ그게 펜트 매력인가봐요",
          "original_form": "ㅋㅋ그게 펜트 매력인가봐요",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100034253.1.21",
          "form": "그렇죠 ㅎㅎ",
          "original_form": "그렇죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100034253.1.22",
          "form": "담주 마지막 어찌되나 몰라요 ㅎㅎ",
          "original_form": "담주 마지막 어찌되나 몰라요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100034253.1.23",
          "form": "벌써 다음주가 마지막인가요?",
          "original_form": "벌써 다음주가 마지막인가요?",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100034253.1.24",
          "form": "네 드디어 끝이네요",
          "original_form": "네 드디어 끝이네요",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100034253.1.25",
          "form": "마지막은 봐야죠 ㅎㅎ",
          "original_form": "마지막은 봐야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100034253.1.26",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100034253.1.27",
          "form": "유종의 미를 거두면 좋겠어요",
          "original_form": "유종의 미를 거두면 좋겠어요",
          "speaker_id": "1",
          "time": "20210827 23:50"
        }
      ]
    }
  ]
