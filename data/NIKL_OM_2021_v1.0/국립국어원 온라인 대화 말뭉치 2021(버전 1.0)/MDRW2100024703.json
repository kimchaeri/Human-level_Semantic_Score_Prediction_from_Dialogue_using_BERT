      "utterance": [
        {
          "id": "MDRW2100024703.1.1",
          "form": "스타벅스 커피 맛있네요~",
          "original_form": "스타벅스 커피 맛있네요~",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024703.1.2",
          "form": "어떤거드셨어요?",
          "original_form": "어떤거드셨어요?",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024703.1.3",
          "form": "카라멜 마키야또요 ㅎㅎ",
          "original_form": "카라멜 마키야또요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024703.1.4",
          "form": "아..,마키아또는 언제먹어도 맛나요 ㅎㅎ",
          "original_form": "아..,마키아또는 언제먹어도 맛나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024703.1.5",
          "form": "저는 스벅가면 주로 돌체라떼나",
          "original_form": "저는 스벅가면 주로 돌체라떼나",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024703.1.6",
          "form": "에스프레소 프라푸치노 먹어요",
          "original_form": "에스프레소 프라푸치노 먹어요",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024703.1.7",
          "form": "또 급 땡기네요 ㅎ",
          "original_form": "또 급 땡기네요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024703.1.8",
          "form": "에스프레소 프라푸치노는 한번도 안먹어봤는데",
          "original_form": "에스프레소 프라푸치노는 한번도 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024703.1.9",
          "form": "무슨맛일까요? 에스프레소맛? ㅎㅎ",
          "original_form": "무슨맛일까요? 에스프레소맛? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024703.1.10",
          "form": "여름에 시원하게 먹기좋아요",
          "original_form": "여름에 시원하게 먹기좋아요",
          "speaker_id": "2",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024703.1.11",
          "form": "커스텀은요?",
          "original_form": "커스텀은요?",
          "speaker_id": "1",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100024703.1.12",
          "form": "커스텀이 머예요?ㅎ",
          "original_form": "커스텀이 머예요?ㅎ",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100024703.1.13",
          "form": "아 스벅가면 뭐 초코드리즐 추가 우유 두유로 바꾸고 그런거요 ㅎㅎ 에소 프랍 본인만의 커스텀 따로있나해서요 ㅎ",
          "original_form": "아 스벅가면 뭐 초코드리즐 추가 우유 두유로 바꾸고 그런거요 ㅎㅎ 에소 프랍 본인만의 커스텀 따로있나해서요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100024703.1.14",
          "form": "아뇨 저는 그런건안해요",
          "original_form": "아뇨 저는 그런건안해요",
          "speaker_id": "2",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100024703.1.15",
          "form": "있는그대로 먹어요 ㅎ",
          "original_form": "있는그대로 먹어요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100024703.1.16",
          "form": "휘핑 머 그런거 안들어가서 깔끔한거 좋아하심",
          "original_form": "휘핑 머 그런거 안들어가서 깔끔한거 좋아하심",
          "speaker_id": "2",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100024703.1.17",
          "form": "에소프라푸치노 입맛에 맞으실듯요",
          "original_form": "에소프라푸치노 입맛에 맞으실듯요",
          "speaker_id": "2",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100024703.1.18",
          "form": "ㅎㅎ 먹어볼게요 ~ 요즘은 커피없이 살기가 힘드네요 집에서도 커피 내려드시나요?",
          "original_form": "ㅎㅎ 먹어볼게요 ~ 요즘은 커피없이 살기가 힘드네요 집에서도 커피 내려드시나요?",
          "speaker_id": "1",
          "time": "20210831 17:42"
        },
        {
          "id": "MDRW2100024703.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100024703.1.20",
          "form": "커피한잔 안먹는 날이 없는거같아요",
          "original_form": "커피한잔 안먹는 날이 없는거같아요",
          "speaker_id": "2",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100024703.1.21",
          "form": "저는 집에서는 맥심 카누요 ㅋㅋㅋ",
          "original_form": "저는 집에서는 맥심 카누요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100024703.1.22",
          "form": "내려드세요?",
          "original_form": "내려드세요?",
          "speaker_id": "2",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100024703.1.23",
          "form": "저도 카누 먹다가 요즘 더치먹는데 자꾸 머신을 사고 싶어져요 ㅋㅋ훨씬 맛있대요",
          "original_form": "저도 카누 먹다가 요즘 더치먹는데 자꾸 머신을 사고 싶어져요 ㅋㅋ훨씬 맛있대요",
          "speaker_id": "1",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100024703.1.24",
          "form": "저는 돌체있었어요",
          "original_form": "저는 돌체있었어요",
          "speaker_id": "2",
          "time": "20210831 18:31"
        },
        {
          "id": "MDRW2100024703.1.25",
          "form": "첨에는 잘 먹다가 점점 안해먹어서",
          "original_form": "첨에는 잘 먹다가 점점 안해먹어서",
          "speaker_id": "2",
          "time": "20210831 18:31"
        },
        {
          "id": "MDRW2100024703.1.26",
          "form": "당근에 팔았어요 ㅎ",
          "original_form": "당근에 팔았어요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 18:31"
        }
      ]
    }
