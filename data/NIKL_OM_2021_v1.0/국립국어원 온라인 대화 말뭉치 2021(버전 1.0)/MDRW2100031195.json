        {
          "id": "MDRW2100031195.1.1",
          "form": "하지만 흥미로운 이야기가 많아요",
          "original_form": "하지만 흥미로운 이야기가 많아요",
          "speaker_id": "1",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100031195.1.2",
          "form": "군대 다녀왔나요",
          "original_form": "군대 다녀왔나요",
          "speaker_id": "2",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100031195.1.3",
          "form": "다녀왔습니다",
          "original_form": "다녀왔습니다",
          "speaker_id": "1",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100031195.1.4",
          "form": "언제 다녀왔나요",
          "original_form": "언제 다녀왔나요",
          "speaker_id": "2",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100031195.1.5",
          "form": "21살에 다녀왔어요",
          "original_form": "21살에 다녀왔어요",
          "speaker_id": "1",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100031195.1.6",
          "form": "육군인가요 백운인가요",
          "original_form": "육군인가요 백운인가요",
          "speaker_id": "2",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100031195.1.7",
          "form": "육군입니다",
          "original_form": "육군입니다",
          "speaker_id": "1",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100031195.1.8",
          "form": "많이 힘들었겠어요",
          "original_form": "많이 힘들었겠어요",
          "speaker_id": "2",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100031195.1.9",
          "form": "아침에 일찍 일어나는 것이 힘들었음",
          "original_form": "아침에 일찍 일어나는 것이 힘들었음",
          "speaker_id": "1",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031195.1.10",
          "form": "군대에서 친구는 사귀었나요",
          "original_form": "군대에서 친구는 사귀었나요",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031195.1.11",
          "form": "작지만 지금은 더 이상 친구가 아니에요",
          "original_form": "작지만 지금은 더 이상 친구가 아니에요",
