      "utterance": [
        {
          "id": "MDRW2100014257.1.1",
          "form": "요즘 건강챙기기 더 어려운 시국같아요",
          "original_form": "요즘 건강챙기기 더 어려운 시국같아요",
          "speaker_id": "1",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014257.1.2",
          "form": "맞아요 열이나도 병원을 못가고 코로나검사먼저 받아야하니",
          "original_form": "맞아요 열이나도 병원을 못가고 코로나검사먼저 받아야하니",
          "speaker_id": "2",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014257.1.3",
          "form": "코로나 조심해서 다녀야해요 정말...",
          "original_form": "코로나 조심해서 다녀야해요 정말...",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014257.1.4",
          "form": "맞아요 요즘은 변이바이러스가 많아서 또 확진자가 급증하고 있으니까요",
          "original_form": "맞아요 요즘은 변이바이러스가 많아서 또 확진자가 급증하고 있으니까요",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014257.1.5",
          "form": "산책 조금이라도 해서 건강 챙겨야겠어요",
          "original_form": "산책 조금이라도 해서 건강 챙겨야겠어요",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014257.1.6",
          "form": "맞아요 근데 혹시 모르니 사람 많이 안다닐 때 나가세요",
          "original_form": "맞아요 근데 혹시 모르니 사람 많이 안다닐 때 나가세요",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014257.1.7",
          "form": "당연하죠! 늦은 밤이나 이른 아침에 한답니다",
          "original_form": "당연하죠! 늦은 밤이나 이른 아침에 한답니다",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014257.1.8",
          "form": "좋아하는 옷 있으신가요?",
          "original_form": "좋아하는 옷 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014257.1.9",
          "form": "저는 셔츠류를 좋아해요",
          "original_form": "저는 셔츠류를 좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100014257.1.10",
          "form": "좋아하는 브랜드 있으신가요?",
          "original_form": "좋아하는 브랜드 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100014257.1.11",
          "form": "브랜드보다느 뭔가 꽂히면 사는거 같아요",
          "original_form": "브랜드보다느 뭔가 꽂히면 사는거 같아요",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100014257.1.12",
          "form": "그럼 오프라인 쇼핑을 좋아하시나요?",
          "original_form": "그럼 오프라인 쇼핑을 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100014257.1.13",
          "form": "둘다 좋아해요",
          "original_form": "둘다 좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100014257.1.14",
          "form": "스파스타일 옷 좋아하시나요 아니면 인쇼스타일 좋아하시나요??",
          "original_form": "스파스타일 옷 좋아하시나요 아니면 인쇼스타일 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100014257.1.15",
          "form": "어.... 스파스타일에 더 가까운거 같아요",
          "original_form": "어.... 스파스타일에 더 가까운거 같아요",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100014257.1.16",
          "form": "오 그러시군요",
          "original_form": "오 그러시군요",
          "speaker_id": "2",
          "time": "20210825 22:54"
        }
