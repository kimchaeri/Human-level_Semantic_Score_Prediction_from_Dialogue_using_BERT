      "utterance": [
        {
          "id": "MDRW2100032990.1.1",
          "form": "오늘하고 내일 비온다는 것 같아",
          "original_form": "오늘하고 내일 비온다는 것 같아",
          "speaker_id": "2",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100032990.1.2",
          "form": "애들 우산은 챙겨갔어?",
          "original_form": "애들 우산은 챙겨갔어?",
          "speaker_id": "1",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100032990.1.3",
          "form": "아니~오후 늦게부터 비온다는 것 같아서",
          "original_form": "아니~오후 늦게부터 비온다는 것 같아서",
          "speaker_id": "2",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100032990.1.4",
          "form": "금방이라도 쏟아질 것 같은데..",
          "original_form": "금방이라도 쏟아질 것 같은데..",
          "speaker_id": "1",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100032990.1.5",
          "form": "여긴 아니야~ 애들 올 때까지는 괜찮을듯",
          "original_form": "여긴 아니야~ 애들 올 때까지는 괜찮을듯",
          "speaker_id": "2",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100032990.1.6",
          "form": "요즘 비가 자주 내리는 것 같아",
          "original_form": "요즘 비가 자주 내리는 것 같아",
          "speaker_id": "1",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100032990.1.7",
          "form": "그러게 말이야.. 일주일에 한두번은 꼭 내리는듯",
          "original_form": "그러게 말이야.. 일주일에 한두번은 꼭 내리는듯",
          "speaker_id": "2",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100032990.1.8",
          "form": "내일까지 비오고 나면 단풍들겠네",
          "original_form": "내일까지 비오고 나면 단풍들겠네",
          "speaker_id": "1",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100032990.1.9",
          "form": "벌써 나무들 보니 단풍 많이 들었더라",
          "original_form": "벌써 나무들 보니 단풍 많이 들었더라",
          "speaker_id": "2",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100032990.1.10",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "1",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100032990.1.11",
          "form": "ㅇㅇ 쓰레기 버리러 나가다보니 벌써 나무가 울긋불긋해 ㅎㅎ",
          "original_form": "ㅇㅇ 쓰레기 버리러 나가다보니 벌써 나무가 울긋불긋해 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 12:16"
        },
        {
          "id": "MDRW2100032990.1.12",
          "form": "시간이 참 빠르다",
          "original_form": "시간이 참 빠르다",
          "speaker_id": "1",
          "time": "20210906 12:16"
        },
        {
          "id": "MDRW2100032990.1.13",
          "form": "비오고 나면 확 추워지는 것 아닌가 몰라",
          "original_form": "비오고 나면 확 추워지는 것 아닌가 몰라",
          "speaker_id": "2",
          "time": "20210906 12:16"
        },
        {
          "id": "MDRW2100032990.1.14",
          "form": "그러게.. 가을이 없어질까봐 걱정이야",
          "original_form": "그러게.. 가을이 없어질까봐 걱정이야",
          "speaker_id": "1",
          "time": "20210906 12:16"
        },
        {
          "id": "MDRW2100032990.1.15",
          "form": "비오고 나서 바로 겨울오는 건 아니겠지 ㅠ",
          "original_form": "비오고 나서 바로 겨울오는 건 아니겠지 ㅠ",
          "speaker_id": "2",
          "time": "20210906 12:17"
        }
