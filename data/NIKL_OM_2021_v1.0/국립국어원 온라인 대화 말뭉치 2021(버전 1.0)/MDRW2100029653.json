      "utterance": [
        {
          "id": "MDRW2100029653.1.1",
          "form": "음료는 주로 어떤 거 드세요?",
          "original_form": "음료는 주로 어떤 거 드세요?",
          "speaker_id": "1",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029653.1.2",
          "form": "콜라요",
          "original_form": "콜라요",
          "speaker_id": "2",
          "time": "20210903 11:59"
        },
        {
          "id": "MDRW2100029653.1.3",
          "form": "거부할 수 없는 그런 음료죠",
          "original_form": "거부할 수 없는 그런 음료죠",
          "speaker_id": "2",
          "time": "20210903 11:59"
        },
        {
          "id": "MDRW2100029653.1.4",
          "form": "탄산과 카페인의 조합",
          "original_form": "탄산과 카페인의 조합",
          "speaker_id": "1",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100029653.1.5",
          "form": "중독성이 장난 아니죠",
          "original_form": "중독성이 장난 아니죠",
          "speaker_id": "1",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100029653.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 14:02"
        },
        {
          "id": "MDRW2100029653.1.7",
          "form": "name1은 뭐 좋아하세요?",
          "original_form": "&name1&은 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 14:02"
        },
        {
          "id": "MDRW2100029653.1.8",
          "form": "저는 타로버블티 좋아해요",
          "original_form": "저는 타로버블티 좋아해요",
          "speaker_id": "1",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100029653.1.9",
          "form": "보통은 카페에서 그때 그때 땡기는 거 먹네요",
          "original_form": "보통은 카페에서 그때 그때 땡기는 거 먹네요",
          "speaker_id": "1",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100029653.1.10",
          "form": "타로버블티는 아직 못먹어봤어요",
          "original_form": "타로버블티는 아직 못먹어봤어요",
          "speaker_id": "2",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100029653.1.11",
          "form": "무슨 맛인가요",
          "original_form": "무슨 맛인가요",
          "speaker_id": "2",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100029653.1.12",
          "form": "고소하고 달달한 맛이에요",
          "original_form": "고소하고 달달한 맛이에요",
          "speaker_id": "1",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100029653.1.13",
          "form": "서양토란이라는데 고구마랑 비슷하다는 사람도 있더라고요",
          "original_form": "서양토란이라는데 고구마랑 비슷하다는 사람도 있더라고요",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100029653.1.14",
          "form": "아하 그러면 이해가 쉽네요",
          "original_form": "아하 그러면 이해가 쉽네요",
          "speaker_id": "2",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100029653.1.15",
          "form": "전 너무 단 건 싫어서 당도 조절해서 먹는데 공차 말고는 당도 조절이 되는데가 별로 없더라고요",
          "original_form": "전 너무 단 건 싫어서 당도 조절해서 먹는데 공차 말고는 당도 조절이 되는데가 별로 없더라고요",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100029653.1.16",
          "form": "대부분 정해진 채로 팔죠",
          "original_form": "대부분 정해진 채로 팔죠",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100029653.1.17",
          "form": "액상과당 너무 많이 먹으면 비만과 당뇨의 위험이 있다고 해서 줄이려곤 하는데",
          "original_form": "액상과당 너무 많이 먹으면 비만과 당뇨의 위험이 있다고 해서 줄이려곤 하는데",
          "speaker_id": "1",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100029653.1.18",
          "form": "쉽지 않아요 ㅎㅎ",
          "original_form": "쉽지 않아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100029653.1.19",
          "form": "세상이 그렇게 냅두지 않죠",
          "original_form": "세상이 그렇게 냅두지 않죠",
          "speaker_id": "2",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100029653.1.20",
          "form": "맛있는 음료는 쏟아져나오니까요",
          "original_form": "맛있는 음료는 쏟아져나오니까요",
          "speaker_id": "2",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100029653.1.21",
          "form": "왜 맛있는 건 몸에 나쁜 건지",
          "original_form": "왜 맛있는 건 몸에 나쁜 건지",
          "speaker_id": "1",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100029653.1.22",
          "form": "편한 자세는 몸에 나쁜 건지",
          "original_form": "편한 자세는 몸에 나쁜 건지",
          "speaker_id": "1",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100029653.1.23",
          "form": "ㅋㅋㅋㅋㅋ 진짜 왜 그런지 모르겠어요",
          "original_form": "ㅋㅋㅋㅋㅋ 진짜 왜 그런지 모르겠어요",
          "speaker_id": "2",
          "time": "20210903 15:46"
        },
        {
          "id": "MDRW2100029653.1.24",
          "form": "역설적인 세상",
          "original_form": "역설적인 세상",
          "speaker_id": "2",
          "time": "20210903 15:46"
        },
        {
          "id": "MDRW2100029653.1.25",
          "form": "새우 이야기를 하니",
          "original_form": "새우 이야기를 하니",
