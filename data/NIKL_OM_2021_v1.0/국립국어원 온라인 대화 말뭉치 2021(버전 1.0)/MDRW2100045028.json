      "utterance": [
        {
          "id": "MDRW2100045028.1.1",
          "form": "뚱카롱이었요?",
          "original_form": "뚱카롱이었요?",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045028.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045028.1.3",
          "form": "그냥 마카롱",
          "original_form": "그냥 마카롱",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045028.1.4",
          "form": "빈약한 마카롱인데",
          "original_form": "빈약한 마카롱인데",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045028.1.5",
          "form": "2천원 이라구요??",
          "original_form": "2천원 이라구요??",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045028.1.6",
          "form": "비싸죠",
          "original_form": "비싸죠",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045028.1.7",
          "form": "거기 어디죠",
          "original_form": "거기 어디죠",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045028.1.8",
          "form": "불매해야겠네",
          "original_form": "불매해야겠네",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045028.1.9",
          "form": "마카롱은 너무 비싸",
          "original_form": "마카롱은 너무 비싸",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045028.1.10",
          "form": "집앞 정거장근처 카페요",
          "original_form": "집앞 정거장근처 카페요",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045028.1.11",
          "form": "그러면 제가 어케 알아요",
          "original_form": "그러면 제가 어케 알아요",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045028.1.12",
          "form": "앞으론 거기서 마카롱 안살거예요",
          "original_form": "앞으론 거기서 마카롱 안살거예요",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045028.1.13",
          "form": "카페명이 뭔데요",
          "original_form": "카페명이 뭔데요",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045028.1.14",
          "form": "불매할꺼면 몰라야죠",
          "original_form": "불매할꺼면 몰라야죠",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045028.1.15",
          "form": "왜 알려그래",
          "original_form": "왜 알려그래",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045028.1.16",
          "form": "아니 혹시 모르게",
          "original_form": "아니 혹시 모르게",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045028.1.17",
          "form": "살수도 있잖아요",
          "original_form": "살수도 있잖아요",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045028.1.18",
          "form": "다드림?",
          "original_form": "다드림?",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045028.1.19",
          "form": "그런이름",
          "original_form": "그런이름",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045028.1.20",
          "form": "닉값을 못하네요",
          "original_form": "닉값을 못하네요",
          "speaker_id": "2",
          "time": "20210908 00:38"
        }
      ]
