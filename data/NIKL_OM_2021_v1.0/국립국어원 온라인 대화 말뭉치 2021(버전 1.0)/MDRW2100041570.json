      "utterance": [
        {
          "id": "MDRW2100041570.1.1",
          "form": "ㅋㅋㅋㅋㅋ 안그래도 궁금하긴 한데 묻긴 그래서 그냥 있는중",
          "original_form": "ㅋㅋㅋㅋㅋ 안그래도 궁금하긴 한데 묻긴 그래서 그냥 있는중",
          "speaker_id": "2",
          "time": "20210906 11:17"
        },
        {
          "id": "MDRW2100041570.1.2",
          "form": "우체국 물류센터에선 하나 챙겨줬는ㄷ",
          "original_form": "우체국 물류센터에선 하나 챙겨줬는ㄷ",
          "speaker_id": "2",
          "time": "20210906 11:17"
        },
        {
          "id": "MDRW2100041570.1.3",
          "form": "(아마 사무실에 있어서 그런듯",
          "original_form": "(아마 사무실에 있어서 그런듯",
          "speaker_id": "2",
          "time": "20210906 11:17"
        },
        {
          "id": "MDRW2100041570.1.4",
          "form": "ㅋㅋㅋㅋㅋ 여기도 하나 줄 거 같은ㄷ0",
          "original_form": "ㅋㅋㅋㅋㅋ 여기도 하나 줄 거 같은ㄷ0",
          "speaker_id": "1",
          "time": "20210906 11:17"
        },
        {
          "id": "MDRW2100041570.1.5",
          "form": "은데",
          "original_form": "은데",
          "speaker_id": "1",
          "time": "20210906 11:17"
        },
        {
          "id": "MDRW2100041570.1.6",
          "form": "ㅋㅋㅋㅋㅋ 주면.. 뭐 하나 들고 가서 생색을 내는 거죠",
          "original_form": "ㅋㅋㅋㅋㅋ 주면.. 뭐 하나 들고 가서 생색을 내는 거죠",
          "speaker_id": "2",
          "time": "20210906 11:18"
        },
        {
          "id": "MDRW2100041570.1.7",
          "form": "맞아 원래 회사에서 주는건 그렇지 ㅋㅋ",
          "original_form": "맞아 원래 회사에서 주는건 그렇지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:57"
        },
        {
          "id": "MDRW2100041570.1.8",
          "form": "칼퇴 예에에 이제.. 이제... 1시간 반 퇴근길",
          "original_form": "칼퇴 예에에 이제.. 이제... 1시간 반 퇴근길",
          "speaker_id": "2",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100041570.1.9",
          "form": "비와서 더 길어질듯",
          "original_form": "비와서 더 길어질듯",
          "speaker_id": "2",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100041570.1.10",
          "form": "버스 막혀??",
          "original_form": "버스 막혀??",
          "speaker_id": "1",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041570.1.11",
          "form": "ㅇㅇ 길이 원래 막히는데 더 막히는거 같아",
          "original_form": "ㅇㅇ 길이 원래 막히는데 더 막히는거 같아",
          "speaker_id": "2",
          "time": "20210906 18:36"
        },
        {
          "id": "MDRW2100041570.1.12",
          "form": "집가면 8시 되는 건 아니겠지..",
          "original_form": "집가면 8시 되는 건 아니겠지..",
          "speaker_id": "2",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100041570.1.13",
          "form": "ㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 18:52"
        },
        {
          "id": "MDRW2100041570.1.14",
          "form": "비오는날이란...",
          "original_form": "비오는날이란...",
          "speaker_id": "1",
          "time": "20210906 18:52"
        },
        {
          "id": "MDRW2100041570.1.15",
          "form": "원래 출퇴근에 시간 얼마나 걸리는데?",
          "original_form": "원래 출퇴근에 시간 얼마나 걸리는데?",
          "speaker_id": "1",
          "time": "20210906 18:52"
        },
        {
          "id": "MDRW2100041570.1.16",
          "form": "ㅋ큐ㅠㅠ",
          "original_form": "ㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:52"
        },
        {
          "id": "MDRW2100041570.1.17",
          "form": "원래 안막히고 빠르게 가면 40분 안짝인데",
          "original_form": "원래 안막히고 빠르게 가면 40분 안짝인데",
          "speaker_id": "2",
          "time": "20210906 18:52"
        },
        {
          "id": "MDRW2100041570.1.18",
          "form": "퇴근은 총 한시간 반 걸리고",
          "original_form": "퇴근은 총 한시간 반 걸리고",
          "speaker_id": "2",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041570.1.19",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041570.1.20",
          "form": "와 퇴근 장난아니네",
          "original_form": "와 퇴근 장난아니네",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041570.1.21",
          "form": "출근은 다행히 40분쯤 걸려",
          "original_form": "출근은 다행히 40분쯤 걸려",
          "speaker_id": "2",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041570.1.22",
          "form": "ㅇㅇ...",
          "original_form": "ㅇㅇ...",
          "speaker_id": "2",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041570.1.23",
          "form": "그나마 다행...",
          "original_form": "그나마 다행...",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041570.1.24",
          "form": "아침잠은 소중해....",
          "original_form": "아침잠은 소중해....",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041570.1.25",
          "form": "6시 칼퇴! 하고 집 들어가면 7시 반이여..근데..오늘은.. 아직도.. 거리에서..",
          "original_form": "6시 칼퇴! 하고 집 들어가면 7시 반이여..근데..오늘은.. 아직도.. 거리에서..",
          "speaker_id": "2",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041570.1.26",
          "form": "ㅇㅎ.....",
          "original_form": "ㅇㅎ.....",
          "speaker_id": "1",
          "time": "20210906 19:03"
        },
        {
          "id": "MDRW2100041570.1.27",
          "form": "빨리 퇴근해야하는데",
          "original_form": "빨리 퇴근해야하는데",
