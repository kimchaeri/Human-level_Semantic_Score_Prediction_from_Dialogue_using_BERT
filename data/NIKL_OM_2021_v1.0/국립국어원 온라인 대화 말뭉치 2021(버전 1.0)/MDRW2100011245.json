      "utterance": [
        {
          "id": "MDRW2100011245.1.1",
          "form": "아침 드셨어요",
          "original_form": "아침 드셨어요",
          "speaker_id": "2",
          "time": "20210824 09:55"
        },
        {
          "id": "MDRW2100011245.1.2",
          "form": "아니요 원래 아침은 잘 안먹어서요",
          "original_form": "아니요 원래 아침은 잘 안먹어서요",
          "speaker_id": "1",
          "time": "20210824 09:55"
        },
        {
          "id": "MDRW2100011245.1.3",
          "form": "식사 하셨나요?",
          "original_form": "식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210824 09:55"
        },
        {
          "id": "MDRW2100011245.1.4",
          "form": "저는 최근 맛있는 과자?를 알았어요",
          "original_form": "저는 최근 맛있는 과자?를 알았어요",
          "speaker_id": "2",
          "time": "20210824 09:57"
        },
        {
          "id": "MDRW2100011245.1.5",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210825 08:31"
        },
        {
          "id": "MDRW2100011245.1.6",
          "form": "현미랑 물만넣은 현미 누룽지인데",
          "original_form": "현미랑 물만넣은 현미 누룽지인데",
          "speaker_id": "2",
          "time": "20210825 08:33"
        },
        {
          "id": "MDRW2100011245.1.7",
          "form": "특수 기계로 진짜 얇게 눌러서",
          "original_form": "특수 기계로 진짜 얇게 눌러서",
          "speaker_id": "2",
          "time": "20210825 08:34"
        },
        {
          "id": "MDRW2100011245.1.8",
          "form": "손바닥만하게 만든거에요 ㅎ",
          "original_form": "손바닥만하게 만든거에요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 08:34"
        },
        {
          "id": "MDRW2100011245.1.9",
          "form": "마트에서 파나요?",
          "original_form": "마트에서 파나요?",
          "speaker_id": "1",
          "time": "20210825 08:34"
        },
        {
          "id": "MDRW2100011245.1.10",
          "form": "네이버에서 늘조은 현미 누룽지 치시면 나와요",
          "original_form": "네이버에서 늘조은 현미 누룽지 치시면 나와요",
          "speaker_id": "2",
          "time": "20210825 08:36"
        },
        {
          "id": "MDRW2100011245.1.11",
          "form": "오 한번 찾아볼게요 ㅎㅎ",
          "original_form": "오 한번 찾아볼게요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 09:07"
        },
        {
          "id": "MDRW2100011245.1.12",
          "form": "저는 한 봉지만 선물 받은건데 이때까지 먹어본 거중 젤 맛있어서 제돈제산 추가구매 하려고요 ㅋㅋ",
          "original_form": "저는 한 봉지만 선물 받은건데 이때까지 먹어본 거중 젤 맛있어서 제돈제산 추가구매 하려고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:08"
        },
        {
          "id": "MDRW2100011245.1.13",
          "form": "저도 요새 자극적인거만 먹어서",
          "original_form": "저도 요새 자극적인거만 먹어서",
          "speaker_id": "1",
          "time": "20210825 09:23"
        },
        {
          "id": "MDRW2100011245.1.14",
          "form": "뭔가 속이 편할거 같아요 ㅎㅎ",
          "original_form": "뭔가 속이 편할거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 09:23"
        },
        {
          "id": "MDRW2100011245.1.15",
          "form": "사실 다이어트 한다고하니 한봉지 주더라고요 ㅋㅋ",
          "original_form": "사실 다이어트 한다고하니 한봉지 주더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:27"
        },
        {
          "id": "MDRW2100011245.1.16",
          "form": "일단 다른 누룽지에 비해 이빨이 안 아파서 좋았어요 ㅋㅋ",
          "original_form": "일단 다른 누룽지에 비해 이빨이 안 아파서 좋았어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:27"
        },
        {
          "id": "MDRW2100011245.1.17",
          "form": "오 좋네요 ㅎㅎㅎ",
          "original_form": "오 좋네요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 09:28"
        }
      ]
