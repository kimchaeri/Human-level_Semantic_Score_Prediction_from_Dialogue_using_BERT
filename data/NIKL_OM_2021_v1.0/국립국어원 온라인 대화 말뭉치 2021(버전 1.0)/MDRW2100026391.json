      "utterance": [
        {
          "id": "MDRW2100026391.1.1",
          "form": "안녕하세요ㅎㅎ",
          "original_form": "안녕하세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026391.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026391.1.3",
          "form": "저녁 맛있는거 드셨나여",
          "original_form": "저녁 맛있는거 드셨나여",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026391.1.4",
          "form": "저는 가자미조림 먹었어요!!",
          "original_form": "저는 가자미조림 먹었어요!!",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026391.1.5",
          "form": "name2은 뭐 드셨어요?",
          "original_form": "&name2&은 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026391.1.6",
          "form": "오~~~~~~~ 가자미조림 맛나겠네요",
          "original_form": "오~~~~~~~ 가자미조림 맛나겠네요",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026391.1.7",
          "form": "저는 비엔나소시지에 깻잎김치요",
          "original_form": "저는 비엔나소시지에 깻잎김치요",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026391.1.8",
          "form": "밥 두그릇은 뚝딱이네요",
          "original_form": "밥 두그릇은 뚝딱이네요",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026391.1.9",
          "form": "깻잎김치 맛있을거 같아요!!",
          "original_form": "깻잎김치 맛있을거 같아요!!",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026391.1.10",
          "form": "직접 해드셨어요??",
          "original_form": "직접 해드셨어요??",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026391.1.11",
          "form": "아뇨! ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아뇨! ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026391.1.12",
          "form": "제가 하면 그맛이안나서",
          "original_form": "제가 하면 그맛이안나서",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026391.1.13",
          "form": "사먹었어요ㅋㅋ",
          "original_form": "사먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026391.1.14",
          "form": "역시 사먹는게 제일이긴 하죠ㅋㅋㅋ",
          "original_form": "역시 사먹는게 제일이긴 하죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026391.1.15",
          "form": "요즘 자주 시켜먹는 메뉴 있으신가요??",
          "original_form": "요즘 자주 시켜먹는 메뉴 있으신가요??",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026391.1.16",
          "form": "저~ 요즘 배달 시켜먹는거 줄이려고 해서요",
          "original_form": "저~ 요즘 배달 시켜먹는거 줄이려고 해서요",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026391.1.17",
          "form": "보통 그때그때 먹고싶은거 시키는데",
          "original_form": "보통 그때그때 먹고싶은거 시키는데",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026391.1.18",
          "form": "돈까스나 뭐 그런거 먹어요",
          "original_form": "돈까스나 뭐 그런거 먹어요",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026391.1.19",
          "form": "저는 떡볶이 좋아해서 자주 시켜먹어요 ㅎㄹ",
          "original_form": "저는 떡볶이 좋아해서 자주 시켜먹어요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026391.1.20",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026391.1.21",
          "form": "떡볶이 좋아하시구나",
          "original_form": "떡볶이 좋아하시구나",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026391.1.22",
          "form": "저 오뎅을 좋아해요 떡보다는",
          "original_form": "저 오뎅을 좋아해요 떡보다는",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026391.1.23",
          "form": "그래서 엽떡도 오뎅많이만먹는",
          "original_form": "그래서 엽떡도 오뎅많이만먹는",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026391.1.24",
          "form": "저도 떡보다는 부수적인걸 좋아하는데 로제 맛있더라고요",
          "original_form": "저도 떡보다는 부수적인걸 좋아하는데 로제 맛있더라고요",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026391.1.25",
          "form": "중독돼서 자주 먹고 있어욬ㅋㅋ",
          "original_form": "중독돼서 자주 먹고 있어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026391.1.26",
          "form": "로제 안먹어봤어요! 맛있나여!?",
          "original_form": "로제 안먹어봤어요! 맛있나여!?",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026391.1.27",
          "form": "네 배떡에서 시켜드세요 맛있어요!!",
          "original_form": "네 배떡에서 시켜드세요 맛있어요!!",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026391.1.28",
          "form": "신전 좋아하시면 신전 로제도 맛있어요!!",
          "original_form": "신전 좋아하시면 신전 로제도 맛있어요!!",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026391.1.29",
          "form": "배떡은 분모자라고 바꿔드시면 약간 떡맛인데 떡보다 쫄깃하고 맛있어요",
          "original_form": "배떡은 분모자라고 바꿔드시면 약간 떡맛인데 떡보다 쫄깃하고 맛있어요",
          "speaker_id": "1",
          "time": "20210901 23:48"
        }
      ]
