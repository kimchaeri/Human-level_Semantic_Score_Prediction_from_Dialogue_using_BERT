      "utterance": [
        {
          "id": "MDRW2100002408.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002408.1.2",
          "form": "아니요 키우지않아요~ 키우시나요?",
          "original_form": "아니요 키우지않아요~ 키우시나요?",
          "speaker_id": "2",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100002408.1.3",
          "form": "아 네 저는 강아지 한마리요",
          "original_form": "아 네 저는 강아지 한마리요",
          "speaker_id": "1",
          "time": "20210820 11:28"
        },
        {
          "id": "MDRW2100002408.1.4",
          "form": "우와 너무 구엽겠어요",
          "original_form": "우와 너무 구엽겠어요",
          "speaker_id": "2",
          "time": "20210820 11:28"
        },
        {
          "id": "MDRW2100002408.1.5",
          "form": "이제 4년차예요!ㅎ.ㅎ",
          "original_form": "이제 4년차예요!ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210820 11:28"
        },
        {
          "id": "MDRW2100002408.1.6",
          "form": "전 어릴때 강아지 햄스터 병아리 토끼를 키워보았어요",
          "original_form": "전 어릴때 강아지 햄스터 병아리 토끼를 키워보았어요",
          "speaker_id": "2",
          "time": "20210820 11:28"
        },
        {
          "id": "MDRW2100002408.1.7",
          "form": "같이 지내다보니까 시간이 훌쩍 지났네요!",
          "original_form": "같이 지내다보니까 시간이 훌쩍 지났네요!",
          "speaker_id": "1",
          "time": "20210820 11:28"
        },
        {
          "id": "MDRW2100002408.1.8",
          "form": "아 엄청 많이 키워보셨네요?!ㅎ.ㅎ",
          "original_form": "아 엄청 많이 키워보셨네요?!ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210820 11:28"
        },
        {
          "id": "MDRW2100002408.1.9",
          "form": "너무 웃기죠^^;  그중 강아지가 단연 최고에요",
          "original_form": "너무 웃기죠^^;  그중 강아지가 단연 최고에요{emoji:♥}",
          "speaker_id": "2",
          "time": "20210820 11:29"
        },
        {
          "id": "MDRW2100002408.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ강아지는  어떤종이였어요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ강아지는  어떤종이였어요?",
          "speaker_id": "1",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100002408.1.11",
          "form": "저흰 믹스견이었어요 ㅎㅎ;;",
          "original_form": "저흰 믹스견이었어요 ㅎㅎ;;",
          "speaker_id": "2",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100002408.1.12",
          "form": "너무너무귀엽고이쁜",
          "original_form": "너무너무귀엽고이쁜",
          "speaker_id": "2",
          "time": "20210820 11:33"
        },
        {
          "id": "MDRW2100002408.1.13",
          "form": "넘 귀엽겠어요 ㅎ.ㅎ 원래 애들이 그래야 튼튼하다자나요!",
          "original_form": "넘 귀엽겠어요 ㅎ.ㅎ 원래 애들이 그래야 튼튼하다자나요!",
          "speaker_id": "1",
          "time": "20210820 11:34"
        },
        {
          "id": "MDRW2100002408.1.14",
          "form": "아프지않고 건강만하는게 최고예요",
          "original_form": "아프지않고 건강만하는게 최고예요",
          "speaker_id": "1",
          "time": "20210820 11:35"
        },
        {
          "id": "MDRW2100002408.1.15",
          "form": "지금 한..12? 13년쯤 됐는데",
          "original_form": "지금 한..12? 13년쯤 됐는데",
          "speaker_id": "2",
          "time": "20210820 11:37"
        },
        {
          "id": "MDRW2100002408.1.16",
          "form": "가끔가서 보면 이제 여기저기 안좋아진게 보여서 맘이아파요~",
          "original_form": "가끔가서 보면 이제 여기저기 안좋아진게 보여서 맘이아파요~",
          "speaker_id": "2",
          "time": "20210820 11:37"
        },
        {
          "id": "MDRW2100002408.1.17",
          "form": "너무 웃긴게 갑자기 데리고 와서",
          "original_form": "너무 웃긴게 갑자기 데리고 와서",
          "speaker_id": "2",
          "time": "20210820 11:38"
        },
        {
          "id": "MDRW2100002408.1.18",
          "form": "데리고 오자마자 병원으로 갔는데  강아지이름이 뭐냐고 해서",
          "original_form": "데리고 오자마자 병원으로 갔는데  강아지이름이 뭐냐고 해서",
          "speaker_id": "2",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100002408.1.19",
          "form": "엄마가 이름....??? 강아지라고 일단써주세요",
          "original_form": "엄마가 이름....??? 강아지라고 일단써주세요",
          "speaker_id": "2",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100002408.1.20",
          "form": "그뒤로 아지가 되었어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그뒤로 아지가 되었어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100002408.1.21",
          "form": "저도 어렸을때 강아지 이름을 아지로 짓고 싶었어요",
          "original_form": "저도 어렸을때 강아지 이름을 아지로 짓고 싶었어요",
          "speaker_id": "1",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100002408.1.22",
          "form": "제가 성이 강씨였다면",
          "original_form": "제가 성이 강씨였다면",
          "speaker_id": "1",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100002408.1.23",
          "form": "지엇을지도 몰라요!",
          "original_form": "지엇을지도 몰라요!",
          "speaker_id": "1",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100002408.1.24",
          "form": "ㅋㅋ친구가 고양이를 키우는데",
          "original_form": "ㅋㅋ친구가 고양이를 키우는데",
          "speaker_id": "2",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100002408.1.25",
          "form": "고양이 이름이 name3래요",
          "original_form": "고양이 이름이 &name3&래요",
          "speaker_id": "2",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100002408.1.26",
          "form": "고양이니까 성은 고씨라며",
          "original_form": "고양이니까 성은 고씨라며",
          "speaker_id": "2",
          "time": "20210820 11:44"
        },
        {
          "id": "MDRW2100002408.1.27",
          "form": "고율무라고 부르던데요 ㅍㅎㅎㅎ",
          "original_form": "고율무라고 부르던데요 ㅍㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 11:44"
        },
        {
          "id": "MDRW2100002408.1.28",
          "form": "고율무 넘귀욥네요",
          "original_form": "고율무 넘귀욥네요",
          "speaker_id": "1",
          "time": "20210820 11:51"
        },
        {
          "id": "MDRW2100002408.1.29",
          "form": "저희집두 성불러서 불러요ㅋㅋㅋ",
          "original_form": "저희집두 성불러서 불러요ㅋㅋㅋ",
