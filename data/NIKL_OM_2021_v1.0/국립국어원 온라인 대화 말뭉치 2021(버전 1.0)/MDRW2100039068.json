        {
          "id": "MDRW2100039068.1.1",
          "form": "우유에는 역시 화이트죠...",
          "original_form": "우유에는 역시 화이트죠...",
          "speaker_id": "1",
          "time": "20210903 08:53"
        },
        {
          "id": "MDRW2100039068.1.2",
          "form": "그 이번에 포스틱 어쩌고 신메뉴 보셨나여",
          "original_form": "그 이번에 포스틱 어쩌고 신메뉴 보셨나여",
          "speaker_id": "2",
          "time": "20210903 08:53"
        },
        {
          "id": "MDRW2100039068.1.3",
          "form": "포스틱이요? 감자과자?",
          "original_form": "포스틱이요? 감자과자?",
          "speaker_id": "1",
          "time": "20210903 08:53"
        },
        {
          "id": "MDRW2100039068.1.4",
          "form": "네....",
          "original_form": "네....",
          "speaker_id": "2",
          "time": "20210903 08:54"
        },
        {
          "id": "MDRW2100039068.1.5",
          "form": "감자과자가 들어잇는 음료가 나왓대요",
          "original_form": "감자과자가 들어잇는 음료가 나왓대요",
          "speaker_id": "2",
          "time": "20210903 08:55"
        },
        {
          "id": "MDRW2100039068.1.6",
          "form": "감자음료라... 저는 감자 좋아해서 100% 잘먹을거 같아요",
          "original_form": "감자음료라... 저는 감자 좋아해서 100% 잘먹을거 같아요",
          "speaker_id": "1",
          "time": "20210903 08:56"
        },
        {
          "id": "MDRW2100039068.1.7",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210903 08:56"
        },
        {
          "id": "MDRW2100039068.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ 아 저는 아직 잘 모르겠어요 감자과자랑 바닐라음료가 같이나오는건데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 아 저는 아직 잘 모르겠어요 감자과자랑 바닐라음료가 같이나오는건데",
          "speaker_id": "2",
          "time": "20210903 08:56"
        },
        {
          "id": "MDRW2100039068.1.9",
          "form": "감자과자를 찍어먹는 그런느낌같아요",
          "original_form": "감자과자를 찍어먹는 그런느낌같아요",
          "speaker_id": "2",
          "time": "20210903 08:57"
        },
        {
          "id": "MDRW2100039068.1.10",
          "form": "감자를 바닐라에...?",
          "original_form": "감자를 바닐라에...?",
          "speaker_id": "1",
          "time": "20210903 08:57"
        },
        {
          "id": "MDRW2100039068.1.11",
          "form": "무슨 조합이죠 이거",
          "original_form": "무슨 조합이죠 이거",
          "speaker_id": "1",
          "time": "20210903 08:57"
        },
        {
          "id": "MDRW2100039068.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 약간 놀랍죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 약간 놀랍죠",
