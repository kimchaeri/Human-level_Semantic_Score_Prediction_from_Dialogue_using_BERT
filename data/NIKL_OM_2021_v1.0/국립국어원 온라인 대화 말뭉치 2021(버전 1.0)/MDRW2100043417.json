      "utterance": [
        {
          "id": "MDRW2100043417.1.1",
          "form": "쇼핑 주로 어디서하세요",
          "original_form": "쇼핑 주로 어디서하세요",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043417.1.2",
          "form": "저 인터넷으로 주로사요",
          "original_form": "저 인터넷으로 주로사요",
          "speaker_id": "1",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043417.1.3",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043417.1.4",
          "form": "저도 인터넷이요",
          "original_form": "저도 인터넷이요",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043417.1.5",
          "form": "최근엔 네이버쇼핑 자주써요",
          "original_form": "최근엔 네이버쇼핑 자주써요",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043417.1.6",
          "form": "적립금이 쏠쏠하더라구요",
          "original_form": "적립금이 쏠쏠하더라구요",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043417.1.7",
          "form": "저두요 ㅋㅋㅋㅋ페이포인트 쏠쏠하게 모여서",
          "original_form": "저두요 ㅋㅋㅋㅋ페이포인트 쏠쏠하게 모여서",
          "speaker_id": "1",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043417.1.8",
          "form": "쇼핑라이브도 보시나요",
          "original_form": "쇼핑라이브도 보시나요",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043417.1.9",
          "form": "네 근데 요즘도포인트잘주나요? 어느순간 포인트 별로안챙겨줘서 안보고있어요",
          "original_form": "네 근데 요즘도포인트잘주나요? 어느순간 포인트 별로안챙겨줘서 안보고있어요",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043417.1.10",
          "form": "그쵸 옛날에 비하면 너무짜요",
          "original_form": "그쵸 옛날에 비하면 너무짜요",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043417.1.11",
          "form": "그래도 안하는거보단 나아서 그냥 하고있어요",
          "original_form": "그래도 안하는거보단 나아서 그냥 하고있어요",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043417.1.12",
          "form": "ㅜㅜ플러스멤버십도 요번에 해지할지 말지고민이에요 포인트잘쌓여서 좋긴한데",
          "original_form": "ㅜㅜ플러스멤버십도 요번에 해지할지 말지고민이에요 포인트잘쌓여서 좋긴한데",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043417.1.13",
          "form": "그러게요 자주 사는 사람들은 혜택이많다던데",
          "original_form": "그러게요 자주 사는 사람들은 혜택이많다던데",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043417.1.14",
          "form": "저는 쌤쌤정도인거같아요",
          "original_form": "저는 쌤쌤정도인거같아요",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043417.1.15",
          "form": "티빙이용권도 크게볼거없고",
          "original_form": "티빙이용권도 크게볼거없고",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043417.1.16",
          "form": "맞아요 전 웹툰봐서 쿠키로다굽긴하는데",
          "original_form": "맞아요 전 웹툰봐서 쿠키로다굽긴하는데",
          "speaker_id": "1",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043417.1.17",
          "form": "이번에 네이버 현대카드인가 나와서",
          "original_form": "이번에 네이버 현대카드인가 나와서",
          "speaker_id": "2",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043417.1.18",
          "form": "십프로적립해준다던데",
          "original_form": "십프로적립해준다던데",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043417.1.19",
          "form": "오 십프로",
          "original_form": "오 십프로",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043417.1.20",
          "form": "뭐 실적도채워야하고해서",
          "original_form": "뭐 실적도채워야하고해서",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043417.1.21",
          "form": "발급받을지고민중이에요",
          "original_form": "발급받을지고민중이에요",
