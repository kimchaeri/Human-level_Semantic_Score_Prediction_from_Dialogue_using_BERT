      "utterance": [
        {
          "id": "MDRW2100006249.1.1",
          "form": "메이저리그도 잘아시고",
          "original_form": "메이저리그도 잘아시고",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100006249.1.2",
          "form": "완전 야구 좋아하시네요",
          "original_form": "완전 야구 좋아하시네요",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100006249.1.3",
          "form": "메이저리그는 챙겨보진 않아서 그냥 뉴스 보고 아는 정도에요ㅋㅋ",
          "original_form": "메이저리그는 챙겨보진 않아서 그냥 뉴스 보고 아는 정도에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100006249.1.4",
          "form": "그래도 그정도면",
          "original_form": "그래도 그정도면",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100006249.1.5",
          "form": "엄청 아시는데요?ㅋ",
          "original_form": "엄청 아시는데요?ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100006249.1.6",
          "form": "스포츠 뉴스 자주 봐서 그런가봐요ㅋㅋ",
          "original_form": "스포츠 뉴스 자주 봐서 그런가봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100006249.1.7",
          "form": "아주 많이아셔서 놀랐어요 ㅎㅎ",
          "original_form": "아주 많이아셔서 놀랐어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100006249.1.8",
          "form": "전 메이저리그 10년 넘게봐서",
          "original_form": "전 메이저리그 10년 넘게봐서",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006249.1.9",
          "form": "보면서 알게된게 많은데",
          "original_form": "보면서 알게된게 많은데",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006249.1.10",
          "form": "올해 스포티비가 유료중계 하면서 접근성이 더 떨어진 것 같아요. 한동안은 스포티비가 하겠죠?",
          "original_form": "올해 스포티비가 유료중계 하면서 접근성이 더 떨어진 것 같아요. 한동안은 스포티비가 하겠죠?",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006249.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006249.1.12",
          "form": "스포티비 암적인존재",
          "original_form": "스포티비 암적인존재",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006249.1.13",
          "form": "엠스플이 좋았는데ㅜㅜ",
          "original_form": "엠스플이 좋았는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006249.1.14",
          "form": "해설 질도 떨어져요",
          "original_form": "해설 질도 떨어져요",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006249.1.15",
          "form": "김선우가 kbo 중계 많이 하더라구요ㅋㅋ",
          "original_form": "김선우가 kbo 중계 많이 하더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006249.1.16",
          "form": "엠스플에서 안뺐기려고",
          "original_form": "엠스플에서 안뺐기려고",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006249.1.17",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006249.1.18",
          "form": "김형준이야 원래 메이저만 했으니",
          "original_form": "김형준이야 원래 메이저만 했으니",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006249.1.19",
          "form": "스포티비 돈주고 보느니",
          "original_form": "스포티비 돈주고 보느니",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006249.1.20",
          "form": "엠엘비닷컴 결제해서 보겠다고 했었는데 ㅋㅋ",
          "original_form": "엠엘비닷컴 결제해서 보겠다고 했었는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006249.1.21",
          "form": "거기가 더 싼가요?",
          "original_form": "거기가 더 싼가요?",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006249.1.22",
          "form": "그럴껄요?",
          "original_form": "그럴껄요?",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006249.1.23",
          "form": "시즌권처럼 사는거라",
          "original_form": "시즌권처럼 사는거라",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006249.1.24",
          "form": "사려면 시즌 초에 사야겠네요",
          "original_form": "사려면 시즌 초에 사야겠네요",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006249.1.25",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006249.1.26",
          "form": "아니면 여러명 아이디 같이쓰기",
          "original_form": "아니면 여러명 아이디 같이쓰기",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006249.1.27",
          "form": "근데 그건 사기 위험성이 있으니",
          "original_form": "근데 그건 사기 위험성이 있으니",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006249.1.28",
          "form": "그건 좀 위험하긴 하죠ㅎㅎ",
          "original_form": "그건 좀 위험하긴 하죠ㅎㅎ",
