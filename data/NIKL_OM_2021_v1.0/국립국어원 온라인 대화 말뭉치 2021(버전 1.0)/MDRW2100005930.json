        },
        {
          "id": "MDRW2100005930.1.3",
          "form": "너무 신기해",
          "original_form": "너무 신기해",
          "speaker_id": "2",
          "time": "20210907 16:39"
        },
        {
          "id": "MDRW2100005930.1.4",
          "form": "카르마라던가",
          "original_form": "카르마라던가",
          "speaker_id": "2",
          "time": "20210907 16:39"
        },
        {
          "id": "MDRW2100005930.1.5",
          "form": "후생이라던가",
          "original_form": "후생이라던가",
          "speaker_id": "2",
          "time": "20210907 16:39"
        },
        {
          "id": "MDRW2100005930.1.6",
          "form": "reincarnation이 뭐였지",
          "original_form": "reincarnation이 뭐였지",
          "speaker_id": "2",
          "time": "20210907 16:39"
        },
        {
          "id": "MDRW2100005930.1.7",
          "form": "암튼그런거",
          "original_form": "암튼그런거",
          "speaker_id": "2",
          "time": "20210907 16:39"
        },
        {
          "id": "MDRW2100005930.1.8",
          "form": "뭔가 현생이 잦같은걸아는지",
          "original_form": "뭔가 현생이 잦같은걸아는지",
          "speaker_id": "2",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100005930.1.9",
          "form": "윤회?",
          "original_form": "윤회?",
          "speaker_id": "1",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100005930.1.10",
          "form": "업을 잘 쌓으면",
          "original_form": "업을 잘 쌓으면",
          "speaker_id": "2",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100005930.1.11",
          "form": "오오오 맞아",
          "original_form": "오오오 맞아",
          "speaker_id": "2",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100005930.1.12",
          "form": "(검색해봄)",
          "original_form": "(검색해봄)",
          "speaker_id": "1",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100005930.1.13",
          "form": "그런거",
          "original_form": "그런거",
          "speaker_id": "2",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100005930.1.14",
          "form": "멋지다",
          "original_form": "멋지다",
          "speaker_id": "1",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100005930.1.15",
          "form": "후생없이",
          "original_form": "후생없이",
          "speaker_id": "2",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100005930.1.16",
          "form": "냅다 죽는거",
          "original_form": "냅다 죽는거",
          "speaker_id": "2",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100005930.1.17",
          "form": "영어가 먼저 나오는 거...",
          "original_form": "영어가 먼저 나오는 거...",
          "speaker_id": "1",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100005930.1.18",
          "form": "그게가장좋은거같아",
          "original_form": "그게가장좋은거같아",
          "speaker_id": "2",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100005930.1.19",
          "form": "아니야...",
          "original_form": "아니야...",
          "speaker_id": "2",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100005930.1.20",
          "form": "그냥어휘력이부족한거야...",
          "original_form": "그냥어휘력이부족한거야...",
          "speaker_id": "2",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100005930.1.21",
          "form": "불교에 지옥은 어떻게 해야 가는거야?",
          "original_form": "불교에 지옥은 어떻게 해야 가는거야?",
          "speaker_id": "1",
          "time": "20210907 16:42"
        },
        {
          "id": "MDRW2100005930.1.22",
          "form": "악행을 많이 해도 지옥을 가는거 아니라",
          "original_form": "악행을 많이 해도 지옥을 가는거 아니라",
          "speaker_id": "1",
          "time": "20210907 16:42"
        },
        {
          "id": "MDRW2100005930.1.23",
          "form": "미물로 환생하는 거 아닌가...?",
          "original_form": "미물로 환생하는 거 아닌가...?",
          "speaker_id": "1",
          "time": "20210907 16:42"
        },
        {
          "id": "MDRW2100005930.1.24",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100005930.1.25",
          "form": "냅다 나쁘면",
          "original_form": "냅다 나쁘면",
          "speaker_id": "2",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100005930.1.26",
          "form": "그러지않을까",
          "original_form": "그러지않을까",
          "speaker_id": "2",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100005930.1.27",
          "form": "막",
          "original_form": "막",
          "speaker_id": "2",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100005930.1.28",
          "form": "애매한 나쁜짓하면",
          "original_form": "애매한 나쁜짓하면",
          "speaker_id": "2",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100005930.1.29",
          "form": "어디지옥가거나",
          "original_form": "어디지옥가거나",
          "speaker_id": "2",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100005930.1.30",
          "form": "가축으로환생하거나",
          "original_form": "가축으로환생하거나",
          "speaker_id": "2",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100005930.1.31",
          "form": "그렇다던데",
          "original_form": "그렇다던데",
          "speaker_id": "2",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100005930.1.32",
          "form": "신과함께를보니깐...(머쓱",
          "original_form": "신과함께를보니깐...(머쓱",
          "speaker_id": "2",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100005930.1.33",
          "form": "신과함께는",
          "original_form": "신과함께는",
          "speaker_id": "1",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100005930.1.34",
          "form": "불교보다 도교에 좀 더 가까운거 아냐...??",
          "original_form": "불교보다 도교에 좀 더 가까운거 아냐...??",
          "speaker_id": "1",
          "time": "20210907 16:47"
        },
