      "utterance": [
        {
          "id": "MDRW2100008424.1.1",
          "form": "임신했을때 워낙 살이 많이 쪘었어서..",
          "original_form": "임신했을때 워낙 살이 많이 쪘었어서..",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008424.1.2",
          "form": "그거때문인거 같기도 하네요",
          "original_form": "그거때문인거 같기도 하네요",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008424.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100008424.1.4",
          "form": "둘째이후 많이 쪄서 그런가봐요",
          "original_form": "둘째이후 많이 쪄서 그런가봐요",
          "speaker_id": "2",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100008424.1.5",
          "form": "엄마들은 다 똑같군요..ㅠㅠ",
          "original_form": "엄마들은 다 똑같군요..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100008424.1.6",
          "form": "네 돌아오지않고",
          "original_form": "네 돌아오지않고",
          "speaker_id": "2",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008424.1.7",
          "form": "계속빼고 찌고 그래요",
          "original_form": "계속빼고 찌고 그래요",
          "speaker_id": "2",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008424.1.8",
          "form": "코로나라 더 확찐자 됐어요 ㅋㅋ",
          "original_form": "코로나라 더 확찐자 됐어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008424.1.9",
          "form": "그쵸 ㅋㅋㅋ 확찐자 ㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋ 확찐자 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008424.1.10",
          "form": "ㅋㅋ 그게 저지요",
          "original_form": "ㅋㅋ 그게 저지요",
          "speaker_id": "2",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008424.1.11",
          "form": "주말에 집에 있으니 애들 챙겨주면서 자꾸 주섬주섬.. 자꾸 먹어서ㅠㅠ",
          "original_form": "주말에 집에 있으니 애들 챙겨주면서 자꾸 주섬주섬.. 자꾸 먹어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008424.1.12",
          "form": "운동하고 식이를 심하게 했더니 탈모가",
          "original_form": "운동하고 식이를 심하게 했더니 탈모가",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008424.1.13",
          "form": "저 진짜 심해요",
          "original_form": "저 진짜 심해요",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008424.1.14",
          "form": "저도요 아까워먹고 맛있나 먹고",
          "original_form": "저도요 아까워먹고 맛있나 먹고",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008424.1.15",
          "form": "그래도 운동을 좋아하시나봐요",
          "original_form": "그래도 운동을 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008424.1.16",
          "form": "아뇨 싫어요",
          "original_form": "아뇨 싫어요",
          "speaker_id": "2",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008424.1.17",
          "form": "그때 한전 달렸어요 ㅡㅡ",
          "original_form": "그때 한전 달렸어요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008424.1.18",
          "form": "지금 아예 안하는게 다시 하려고요 ㅠㅠ",
          "original_form": "지금 아예 안하는게 다시 하려고요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008424.1.19",
          "form": "아 저는 운동을 1도 안좋아해서 .. 못해요 ㅠㅠ",
          "original_form": "아 저는 운동을 1도 안좋아해서 .. 못해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008424.1.20",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008424.1.21",
          "form": "약이나 먹을까 싶네요",
          "original_form": "약이나 먹을까 싶네요",
          "speaker_id": "2",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008424.1.22",
          "form": "넹... 효과는 참 좋아요 ㅎㅎ",
          "original_form": "넹... 효과는 참 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
