      "utterance": [
        {
          "id": "MDRW2100006619.1.1",
          "form": "학교관련 아르바이트는",
          "original_form": "학교관련 아르바이트는",
          "speaker_id": "1",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100006619.1.2",
          "form": "최저시급 받으시면서 하셨나요?",
          "original_form": "최저시급 받으시면서 하셨나요?",
          "speaker_id": "1",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100006619.1.3",
          "form": "네 최저시급 주더라고요",
          "original_form": "네 최저시급 주더라고요",
          "speaker_id": "2",
          "time": "20210908 03:23"
        },
        {
          "id": "MDRW2100006619.1.4",
          "form": "밖에서 설거지를 해도 최저보단 더 주던데...어쩔 수 없었죠 뭐 ㅋㅋㅋㅋ",
          "original_form": "밖에서 설거지를 해도 최저보단 더 주던데...어쩔 수 없었죠 뭐 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:23"
        },
        {
          "id": "MDRW2100006619.1.5",
          "form": "아하 그러면 지금도 하고 계시는 알바인건가요?",
          "original_form": "아하 그러면 지금도 하고 계시는 알바인건가요?",
          "speaker_id": "1",
          "time": "20210908 03:23"
        },
        {
          "id": "MDRW2100006619.1.6",
          "form": "아니요 예전에 하고 힘들어서 지원도 안했어요!",
          "original_form": "아니요 예전에 하고 힘들어서 지원도 안했어요!",
          "speaker_id": "2",
          "time": "20210908 03:25"
        },
        {
          "id": "MDRW2100006619.1.7",
          "form": "지금은 코로나때문에 알바자리도 없다던데",
          "original_form": "지금은 코로나때문에 알바자리도 없다던데",
          "speaker_id": "1",
          "time": "20210908 03:25"
        },
        {
          "id": "MDRW2100006619.1.8",
          "form": "학교다니시면 지금 취업준비중이신가요",
          "original_form": "학교다니시면 지금 취업준비중이신가요",
          "speaker_id": "1",
          "time": "20210908 03:26"
        },
        {
          "id": "MDRW2100006619.1.9",
          "form": "아직 취업 준비 중은 아니고 복학해서 열심히 공부 하고 있어요",
          "original_form": "아직 취업 준비 중은 아니고 복학해서 열심히 공부 하고 있어요",
          "speaker_id": "2",
          "time": "20210908 03:26"
        },
        {
          "id": "MDRW2100006619.1.10",
          "form": "취업 준비 중이신가요??",
          "original_form": "취업 준비 중이신가요??",
          "speaker_id": "2",
          "time": "20210908 03:26"
        },
        {
          "id": "MDRW2100006619.1.11",
          "form": "이제 곧 4학년이라 취업준비가 다가와서",
          "original_form": "이제 곧 4학년이라 취업준비가 다가와서",
          "speaker_id": "1",
          "time": "20210908 03:27"
        },
        {
          "id": "MDRW2100006619.1.12",
          "form": "고민하는 시기인거같아요",
          "original_form": "고민하는 시기인거같아요",
          "speaker_id": "1",
          "time": "20210908 03:27"
        },
        {
          "id": "MDRW2100006619.1.13",
          "form": "아...그렇죠 요즘 또 힘들잖아요 채용도 적고",
          "original_form": "아...그렇죠 요즘 또 힘들잖아요 채용도 적고",
          "speaker_id": "2",
          "time": "20210908 03:28"
        },
        {
          "id": "MDRW2100006619.1.14",
          "form": "학교만 10년넘게 다니다가",
          "original_form": "학교만 10년넘게 다니다가",
          "speaker_id": "1",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100006619.1.15",
          "form": "갑자기 사회에 던져지는 기분이라 막막하네요..ㅋㅋ",
          "original_form": "갑자기 사회에 던져지는 기분이라 막막하네요..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100006619.1.16",
          "form": "그쵸 뭔가 안정적인 보호막을 잃어버리는 기분이에요 ㅋㅋㅋㅋ",
          "original_form": "그쵸 뭔가 안정적인 보호막을 잃어버리는 기분이에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:30"
        },
        {
          "id": "MDRW2100006619.1.17",
          "form": "혹시 진로쪽은 이미 정하셨나요",
          "original_form": "혹시 진로쪽은 이미 정하셨나요",
          "speaker_id": "1",
          "time": "20210908 03:31"
        },
        {
          "id": "MDRW2100006619.1.18",
          "form": "전 방산업체 쪽으로 나갈 것 같아요",
          "original_form": "전 방산업체 쪽으로 나갈 것 같아요",
          "speaker_id": "2",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100006619.1.19",
          "form": "미사일 개발하고 그런 곳이요",
          "original_form": "미사일 개발하고 그런 곳이요",
          "speaker_id": "2",
          "time": "20210908 03:33"
        },
        {
          "id": "MDRW2100006619.1.20",
          "form": "와 신기하네요",
          "original_form": "와 신기하네요",
          "speaker_id": "1",
          "time": "20210908 03:34"
        },
        {
          "id": "MDRW2100006619.1.21",
          "form": "그런쪽에 가려면 군관련으로 가야하는건가요",
          "original_form": "그런쪽에 가려면 군관련으로 가야하는건가요",
          "speaker_id": "1",
          "time": "20210908 03:34"
        }
      ]
    }
