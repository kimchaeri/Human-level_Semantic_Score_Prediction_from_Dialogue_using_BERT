      "utterance": [
        {
          "id": "MDRW2100043175.1.1",
          "form": "좀있음 퇴근~.~",
          "original_form": "좀있음 퇴근~.~",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043175.1.2",
          "form": "이제 퇴근 아니가요?",
          "original_form": "이제 퇴근 아니가요?",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043175.1.3",
          "form": "3시반인뎅ㅋㅋ",
          "original_form": "3시반인뎅ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100043175.1.4",
          "form": "늦으신데....^..^",
          "original_form": "늦으신데....^..^",
          "speaker_id": "2",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100043175.1.5",
          "form": "아니.... 사장님 왜 맨날 그러노",
          "original_form": "아니.... 사장님 왜 맨날 그러노",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100043175.1.6",
          "form": "늦음 돈 더주나?",
          "original_form": "늦음 돈 더주나?",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100043175.1.7",
          "form": "조금...쳐주겠지....? ㅠ",
          "original_form": "조금...쳐주겠지....? ㅠ",
          "speaker_id": "2",
          "time": "20210907 15:33"
        },
        {
          "id": "MDRW2100043175.1.8",
          "form": "아가들 기다리는데 ㅜㅜ",
          "original_form": "아가들 기다리는데 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 15:33"
        },
        {
          "id": "MDRW2100043175.1.9",
          "form": "쳐주겠지는 뭐양ㅋㅋ",
          "original_form": "쳐주겠지는 뭐양ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:36"
        },
        {
          "id": "MDRW2100043175.1.10",
          "form": "월급 확인 안하세여?",
          "original_form": "월급 확인 안하세여?",
          "speaker_id": "1",
          "time": "20210907 15:36"
        },
        {
          "id": "MDRW2100043175.1.11",
          "form": "하는데..",
          "original_form": "하는데..",
          "speaker_id": "2",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100043175.1.12",
          "form": "막 몇분 늦은건 안해주드라공..ㅋ",
          "original_form": "막 몇분 늦은건 안해주드라공..ㅋ",
          "speaker_id": "2",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100043175.1.13",
          "form": "30븐 늦으시넹...",
          "original_form": "30븐 늦으시넹...",
          "speaker_id": "2",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100043175.1.14",
          "form": "30분은해주셤..ㅎ.ㅎ",
          "original_form": "30분은해주셤..ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100043175.1.15",
          "form": "너무 별로다 사장...",
          "original_form": "너무 별로다 사장...",
          "speaker_id": "1",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100043175.1.16",
          "form": "님도 그렇게 늦게 오면\r\n담날 그만큼 늦게 가세여 ㅋㅋ",
          "original_form": "님도 그렇게 늦게 오면\r\n담날 그만큼 늦게 가세여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100043175.1.17",
          "form": "그르까...ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그르까...ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043175.1.18",
          "form": "그래도 예잔보단",
          "original_form": "그래도 예잔보단",
          "speaker_id": "2",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043175.1.19",
          "form": "늦는게 좀 적어짐 ㅋㅋ",
          "original_form": "늦는게 좀 적어짐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043175.1.20",
          "form": "예전에는\r\n얼마나 더 잦았던거야 ㅋㅋ",
          "original_form": "예전에는\r\n얼마나 더 잦았던거야 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043175.1.21",
          "form": "예전엔 거의 일상이었쥬ㅣ ㅋㅋㅋ",
          "original_form": "예전엔 거의 일상이었쥬ㅣ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100043175.1.22",
          "form": "진짜 너무하네ㅋㅋ",
          "original_form": "진짜 너무하네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100043175.1.23",
          "form": "사장이고 뭐고",
          "original_form": "사장이고 뭐고",
          "speaker_id": "1",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100043175.1.24",
