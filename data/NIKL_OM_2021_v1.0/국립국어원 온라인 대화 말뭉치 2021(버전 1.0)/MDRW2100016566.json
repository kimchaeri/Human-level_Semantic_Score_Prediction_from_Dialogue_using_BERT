      "utterance": [
        {
          "id": "MDRW2100016566.1.1",
          "form": "직장인이신가요?",
          "original_form": "직장인이신가요?",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016566.1.2",
          "form": "전업이에요ㅠ",
          "original_form": "전업이에요ㅠ",
          "speaker_id": "2",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016566.1.3",
          "form": "전업이 뭐에요? 전업 주부여?",
          "original_form": "전업이 뭐에요? 전업 주부여?",
          "speaker_id": "1",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100016566.1.4",
          "form": "네 주부ㅋㅋ",
          "original_form": "네 주부ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016566.1.5",
          "form": "예전엔 어떤일 하셧엇나요?",
          "original_form": "예전엔 어떤일 하셧엇나요?",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016566.1.6",
          "form": "사회복지사요",
          "original_form": "사회복지사요",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016566.1.7",
          "form": "스트레스 엄청 많은 직종이죠?",
          "original_form": "스트레스 엄청 많은 직종이죠?",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016566.1.8",
          "form": "모든일이 그런것같아요^^  그땐 그랬는데 쉬다보니",
          "original_form": "모든일이 그런것같아요^^  그땐 그랬는데 쉬다보니",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016566.1.9",
          "form": "재밌었던 기억도나고",
          "original_form": "재밌었던 기억도나고",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016566.1.10",
          "form": "추억보정? ㅋㅋㅋㅋㅋ",
          "original_form": "추억보정? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016566.1.11",
          "form": "원래 약자? 대하는 직업이 현타가 더 심하다고 하더라구요..",
          "original_form": "원래 약자? 대하는 직업이 현타가 더 심하다고 하더라구요..",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016566.1.12",
          "form": "공무원도 복지파트가 ㅈ젤 힘들고",
          "original_form": "공무원도 복지파트가 ㅈ젤 힘들고",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016566.1.13",
          "form": "아 사람대하는 직업은 다그런것같아요ㅠㅠ",
          "original_form": "아 사람대하는 직업은 다그런것같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016566.1.14",
          "form": "성격이랑 잘 맞으셨나보네요..",
          "original_form": "성격이랑 잘 맞으셨나보네요..",
          "speaker_id": "1",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100016566.1.15",
          "form": "그런것같아요~",
          "original_form": "그런것같아요~",
          "speaker_id": "2",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016566.1.16",
          "form": "공공기관 같은데 근무하셨던 건가요? \n아님 회사 같은거도 많나요? 잘 몰라서..",
          "original_form": "공공기관 같은데 근무하셨던 건가요? \n아님 회사 같은거도 많나요? 잘 몰라서..",
          "speaker_id": "1",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100016566.1.17",
          "form": "단체에요~  공공기관은 아니고 장애인단체",
          "original_form": "단체에요~  공공기관은 아니고 장애인단체",
          "speaker_id": "2",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100016566.1.18",
          "form": "아.. 비영리법인 그런거여?",
          "original_form": "아.. 비영리법인 그런거여?",
          "speaker_id": "1",
          "time": "20210826 14:46"
        },
        {
          "id": "MDRW2100016566.1.19",
