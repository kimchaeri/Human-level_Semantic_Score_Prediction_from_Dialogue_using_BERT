      "utterance": [
        {
          "id": "MDRW2100046188.1.1",
          "form": "떡볶이 좋아하시나요?",
          "original_form": "떡볶이 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046188.1.2",
          "form": "네네 그럼요!",
          "original_form": "네네 그럼요!",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046188.1.3",
          "form": "엽떡좋아해요",
          "original_form": "엽떡좋아해요",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046188.1.4",
          "form": "엽떡 너무 맛있죠!ㅠㅠㅠ 치즈 추가하면 극락",
          "original_form": "엽떡 너무 맛있죠!ㅠㅠㅠ 치즈 추가하면 극락",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046188.1.5",
          "form": "으아 뭔가 아시는분이군요!!ㅠㅠ",
          "original_form": "으아 뭔가 아시는분이군요!!ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046188.1.6",
          "form": "중국당면 추가도 맛있더라구요 ㅎㅎ",
          "original_form": "중국당면 추가도 맛있더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046188.1.7",
          "form": "메추리알이랑",
          "original_form": "메추리알이랑",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046188.1.8",
          "form": "맛없을 수가 없네요ㅠㅠㅠㅠ",
          "original_form": "맛없을 수가 없네요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046188.1.9",
          "form": "중국당면 ㄹㅇ 사기캐",
          "original_form": "중국당면 ㄹㅇ 사기캐",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046188.1.10",
          "form": "ㅠㅠㅠㅠ그니까요",
          "original_form": "ㅠㅠㅠㅠ그니까요",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046188.1.11",
          "form": "완전 공감 ㅠㅠ",
          "original_form": "완전 공감 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046188.1.12",
          "form": "벌써 먹고싶네요 ㅠㅠ",
          "original_form": "벌써 먹고싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046188.1.13",
          "form": "저도요ㅠㅠㅠㅠㅠ 튀김도 찍어먹고요ㅠㅠ",
          "original_form": "저도요ㅠㅠㅠㅠㅠ 튀김도 찍어먹고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046188.1.14",
          "form": "으아 너무 좋죠",
          "original_form": "으아 너무 좋죠",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046188.1.15",
          "form": "신전 어묵튀김아세요?",
          "original_form": "신전 어묵튀김아세요?",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046188.1.16",
          "form": "아 알죠알죠!",
          "original_form": "아 알죠알죠!",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046188.1.17",
          "form": "완전 존맛ㅜㅜ",
          "original_form": "완전 존맛ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046188.1.18",
          "form": "ㅠㅠㅠ그쵸 ㅠㅠ 진짜 최고다라구요 ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ그쵸 ㅠㅠ 진짜 최고다라구요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046188.1.19",
          "form": "무슨 튀김 가장 좋아하세요?",
          "original_form": "무슨 튀김 가장 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046188.1.20",
          "form": "저 김말이요!",
          "original_form": "저 김말이요!",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046188.1.21",
          "form": "우앗 맛있죠 ㅠㅠㅠㅠ",
          "original_form": "우앗 맛있죠 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046188.1.22",
          "form": "가장 좋아하는 떡볶이 브랜드는요??",
          "original_form": "가장 좋아하는 떡볶이 브랜드는요??",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046188.1.23",
          "form": "전 걸작 좋아해요! 치킨도 같이 먹을 수 있어서요ㅋㅋ",
          "original_form": "전 걸작 좋아해요! 치킨도 같이 먹을 수 있어서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046188.1.24",
          "form": "와 걸작 가성비 최고 아닌가요?",
          "original_form": "와 걸작 가성비 최고 아닌가요?",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046188.1.25",
          "form": "맞아요 맛도 있고 가성비도 좋아요!",
          "original_form": "맞아요 맛도 있고 가성비도 좋아요!",
          "speaker_id": "1",
          "time": "20210908 11:12"
        }
      ]
    }
  ]
