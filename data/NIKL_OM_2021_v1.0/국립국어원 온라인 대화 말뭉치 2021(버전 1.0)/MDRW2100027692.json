      "utterance": [
        {
          "id": "MDRW2100027692.1.1",
          "form": "최근에 어디 여행갔다왓니",
          "original_form": "최근에 어디 여행갔다왓니",
          "speaker_id": "1",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027692.1.2",
          "form": "대구여행...? ㅠㅠ",
          "original_form": "대구여행...? ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027692.1.3",
          "form": "ㅏ....",
          "original_form": "ㅏ....",
          "speaker_id": "1",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027692.1.4",
          "form": "어디 다녀왓는지 생각이 안난다...",
          "original_form": "어디 다녀왓는지 생각이 안난다...",
          "speaker_id": "2",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027692.1.5",
          "form": "그정도먄 귀향아니니",
          "original_form": "그정도먄 귀향아니니",
          "speaker_id": "1",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027692.1.6",
          "form": "최근이ㅜ없나봐",
          "original_form": "최근이ㅜ없나봐",
          "speaker_id": "2",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027692.1.7",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027692.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027692.1.9",
          "form": "나도최근은없짘ㅋㅋㅋㅋ",
          "original_form": "나도최근은없짘ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027692.1.10",
          "form": "넌 어디 다녀왓엌ㅋㅋ",
          "original_form": "넌 어디 다녀왓엌ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027692.1.11",
          "form": "나도최근은없고 그나마 가까운시일에는 문경 ㅋㅋㅋㅋ",
          "original_form": "나도최근은없고 그나마 가까운시일에는 문경 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027692.1.12",
          "form": "갑자기 문경??",
          "original_form": "갑자기 문경??",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027692.1.13",
          "form": "ㅋㅋㅋㅋㅋ가까운대 가볍게 가려면 경북근처뿐..",
          "original_form": "ㅋㅋㅋㅋㅋ가까운대 가볍게 가려면 경북근처뿐..",
          "speaker_id": "1",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027692.1.14",
          "form": "그렇긴 하지..근데 문경 좀 멀지않니",
          "original_form": "그렇긴 하지..근데 문경 좀 멀지않니",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027692.1.15",
          "form": "생각보다 가까웠어 한시간반??",
          "original_form": "생각보다 가까웠어 한시간반??",
          "speaker_id": "1",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027692.1.16",
          "form": "경주가는 느끼이랑 비슷햇서",
          "original_form": "경주가는 느끼이랑 비슷햇서",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027692.1.17",
          "form": "오 생각보다 가깝네",
          "original_form": "오 생각보다 가깝네",
          "speaker_id": "2",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027692.1.18",
          "form": "마자 그 유명한 문경세재는 삼십분 더걸리드라",
          "original_form": "마자 그 유명한 문경세재는 삼십분 더걸리드라",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027692.1.19",
          "form": "아하 문경에서 뭐 햇어",
          "original_form": "아하 문경에서 뭐 햇어",
          "speaker_id": "2",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027692.1.20",
          "form": "근데 거기말고도 좀 큰 카페들 많아서 갠차낫어",
          "original_form": "근데 거기말고도 좀 큰 카페들 많아서 갠차낫어",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027692.1.21",
          "form": "요즘 그런데 예쁜 카페 많더라",
          "original_form": "요즘 그런데 예쁜 카페 많더라",
          "speaker_id": "2",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027692.1.22",
          "form": "마자 여행지중에 어디가 젤 조앗어",
          "original_form": "마자 여행지중에 어디가 젤 조앗어",
          "speaker_id": "1",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027692.1.23",
          "form": "국내에서는 제주도지...",
          "original_form": "국내에서는 제주도지...",
          "speaker_id": "2",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027692.1.24",
          "form": "아 6월에 제주도 갔었네",
          "original_form": "아 6월에 제주도 갔었네",
          "speaker_id": "2",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027692.1.25",
          "form": "ㅋㅋㅋㅋㅋ맞아 제주도지",
          "original_form": "ㅋㅋㅋㅋㅋ맞아 제주도지",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027692.1.26",
          "form": "제주도에셔 어디가 조앗어",
          "original_form": "제주도에셔 어디가 조앗어",
          "speaker_id": "1",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100027692.1.27",
          "form": "저번에 금능해수욕장쪽 갔는데",
          "original_form": "저번에 금능해수욕장쪽 갔는데",
          "speaker_id": "2",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100027692.1.28",
          "form": "한적한데 바다 너무 예쁘고 좋도라",
          "original_form": "한적한데 바다 너무 예쁘고 좋도라",
          "speaker_id": "2",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100027692.1.29",
          "form": "넌 어디가 좋앗어",
          "original_form": "넌 어디가 좋앗어",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027692.1.30",
          "form": "오 금능은 첨들너봐",
          "original_form": "오 금능은 첨들너봐",
          "speaker_id": "1",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027692.1.31",
          "form": "나는 함덕 좋앗어",
          "original_form": "나는 함덕 좋앗어",
          "speaker_id": "1",
          "time": "20210902 14:25"
        }
      ]
