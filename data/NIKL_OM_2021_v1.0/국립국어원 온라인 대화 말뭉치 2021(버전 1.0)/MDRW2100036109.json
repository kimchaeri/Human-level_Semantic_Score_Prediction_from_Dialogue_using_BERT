      "utterance": [
        {
          "id": "MDRW2100036109.1.1",
          "form": "저녁은 샌드위치요!",
          "original_form": "저녁은 샌드위치요!",
          "speaker_id": "2",
          "time": "20210830 16:42"
        },
        {
          "id": "MDRW2100036109.1.2",
          "form": "샌드위치 하나여?",
          "original_form": "샌드위치 하나여?",
          "speaker_id": "1",
          "time": "20210830 16:42"
        },
        {
          "id": "MDRW2100036109.1.3",
          "form": "어디꺼 드세오?",
          "original_form": "어디꺼 드세오?",
          "speaker_id": "1",
          "time": "20210830 16:42"
        },
        {
          "id": "MDRW2100036109.1.4",
          "form": "카페마마스 거요ㅋㅋ",
          "original_form": "카페마마스 거요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100036109.1.5",
          "form": "맛있나여!!",
          "original_form": "맛있나여!!",
          "speaker_id": "1",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100036109.1.6",
          "form": "네ㅋㅋ여기거 맛있어요. 파니니도 맛있구 샌드위치도 맛있구요!",
          "original_form": "네ㅋㅋ여기거 맛있어요. 파니니도 맛있구 샌드위치도 맛있구요!",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100036109.1.7",
          "form": "오.. 맛있겠어요 ㅠㅠㅠㅠ",
          "original_form": "오.. 맛있겠어요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 08:34"
        },
        {
          "id": "MDRW2100036109.1.8",
          "form": "처음 듣는 카페인데",
          "original_form": "처음 듣는 카페인데",
          "speaker_id": "1",
          "time": "20210831 08:34"
        },
        {
          "id": "MDRW2100036109.1.9",
          "form": "먹어보고싳어요",
          "original_form": "먹어보고싳어요",
          "speaker_id": "1",
          "time": "20210831 08:34"
        },
        {
          "id": "MDRW2100036109.1.10",
          "form": "프랜차이즈라서 꽤 많은 거로 알고 있어요ㅎ",
          "original_form": "프랜차이즈라서 꽤 많은 거로 알고 있어요ㅎ",
          "speaker_id": "2",
          "time": "20210831 13:16"
        },
        {
          "id": "MDRW2100036109.1.11",
          "form": "나중에 한 번 먹어보세요ㅎㅎ",
          "original_form": "나중에 한 번 먹어보세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:16"
        },
        {
          "id": "MDRW2100036109.1.12",
          "form": "ㅎ 찾아봐야겠네요",
          "original_form": "ㅎ 찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210831 13:53"
        },
        {
          "id": "MDRW2100036109.1.13",
          "form": "오늘 점심은 뭐드셨나요?",
          "original_form": "오늘 점심은 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210831 13:54"
        },
        {
          "id": "MDRW2100036109.1.14",
          "form": "저 어묵탕이요 ㅎ",
          "original_form": "저 어묵탕이요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 14:00"
        },
        {
          "id": "MDRW2100036109.1.15",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210831 14:00"
        },
        {
          "id": "MDRW2100036109.1.16",
          "form": "저는 떡볶이요!",
          "original_form": "저는 떡볶이요!",
          "speaker_id": "2",
          "time": "20210831 14:01"
        },
        {
          "id": "MDRW2100036109.1.17",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100036109.1.18",
          "form": "떡볶이",
          "original_form": "떡볶이",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100036109.1.19",
          "form": "완전 맛있겠네요",
          "original_form": "완전 맛있겠네요",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100036109.1.20",
          "form": "청년다방 차돌떡볶이 세트로 먹었어요! 엄청 맛있었네요ㅋㅋ",
          "original_form": "청년다방 차돌떡볶이 세트로 먹었어요! 엄청 맛있었네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:12"
        },
        {
          "id": "MDRW2100036109.1.21",
          "form": "아아아아아아악",
          "original_form": "아아아아아아악",
          "speaker_id": "1",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100036109.1.22",
          "form": "저 거기꺼 완전 좋아해요",
          "original_form": "저 거기꺼 완전 좋아해요",
          "speaker_id": "1",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100036109.1.23",
          "form": "거기 감튀도 진심 넘맛 ㅠㅠ",
          "original_form": "거기 감튀도 진심 넘맛 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100036109.1.24",
          "form": "맞아요 진짜맛있!!!",
          "original_form": "맞아요 진짜맛있!!!",
          "speaker_id": "2",
          "time": "20210831 17:25"
        }
      ]
