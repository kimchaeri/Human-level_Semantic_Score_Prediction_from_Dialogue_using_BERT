      "utterance": [
        {
          "id": "MDRW2100037481.1.1",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 01:08"
        },
        {
          "id": "MDRW2100037481.1.2",
          "form": "좋아하진 않아요",
          "original_form": "좋아하진 않아요",
          "speaker_id": "1",
          "time": "20210901 01:09"
        },
        {
          "id": "MDRW2100037481.1.3",
          "form": "운동 자주 하세요?",
          "original_form": "운동 자주 하세요?",
          "speaker_id": "1",
          "time": "20210901 01:09"
        },
        {
          "id": "MDRW2100037481.1.4",
          "form": "전 숨쉬기운동ㅠ",
          "original_form": "전 숨쉬기운동ㅠ",
          "speaker_id": "2",
          "time": "20210901 01:09"
        },
        {
          "id": "MDRW2100037481.1.5",
          "form": "이번 올림픽 하는 거 재밌게 보셨어요?",
          "original_form": "이번 올림픽 하는 거 재밌게 보셨어요?",
          "speaker_id": "1",
          "time": "20210901 01:09"
        },
        {
          "id": "MDRW2100037481.1.6",
          "form": "배구 재밌었어요",
          "original_form": "배구 재밌었어요",
          "speaker_id": "2",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100037481.1.7",
          "form": "멋지더라구요",
          "original_form": "멋지더라구요",
          "speaker_id": "2",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100037481.1.8",
          "form": "여자배구 말씀이시죠?",
          "original_form": "여자배구 말씀이시죠?",
          "speaker_id": "1",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100037481.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100037481.1.10",
          "form": "메달은 못땄지만",
          "original_form": "메달은 못땄지만",
          "speaker_id": "2",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100037481.1.11",
          "form": "운동 경기 보면 생명력이 느껴져요",
          "original_form": "운동 경기 보면 생명력이 느껴져요",
          "speaker_id": "1",
          "time": "20210901 01:11"
        },
        {
          "id": "MDRW2100037481.1.12",
          "form": "박진감 느껴지고요",
          "original_form": "박진감 느껴지고요",
          "speaker_id": "2",
          "time": "20210901 01:11"
        },
        {
          "id": "MDRW2100037481.1.13",
          "form": "그리고 스포츠는 반칙 없이 하면 그게 참 정정당당 해 보여서요",
          "original_form": "그리고 스포츠는 반칙 없이 하면 그게 참 정정당당 해 보여서요",
          "speaker_id": "1",
          "time": "20210901 01:12"
        },
        {
          "id": "MDRW2100037481.1.14",
          "form": "맞아요 져도 멋져요",
          "original_form": "맞아요 져도 멋져요",
          "speaker_id": "2",
          "time": "20210901 01:41"
        },
        {
          "id": "MDRW2100037481.1.15",
          "form": "머니머니해도 건강이 최고같아요",
          "original_form": "머니머니해도 건강이 최고같아요",
          "speaker_id": "1",
          "time": "20210901 01:43"
        }
