      "utterance": [
        {
          "id": "MDRW2100021154.1.1",
          "form": "네 코난도 좋아해요 ㅋㅋㅋ",
          "original_form": "네 코난도 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100021154.1.2",
          "form": "코난 보시나요?",
          "original_form": "코난 보시나요?",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100021154.1.3",
          "form": "애니요? ㅋㅋㅋㅋ",
          "original_form": "애니요? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100021154.1.4",
          "form": "초창기엔.봣는데.. 너무 질질 끌어서..",
          "original_form": "초창기엔.봣는데.. 너무 질질 끌어서..",
          "speaker_id": "2",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100021154.1.5",
          "form": "아 하긴 질질 끌죠 ㅋㅋㅋ",
          "original_form": "아 하긴 질질 끌죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100021154.1.6",
          "form": "전 짱구 더 좋아해요 ㅋㅋ",
          "original_form": "전 짱구 더 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100021154.1.7",
          "form": "저.. 짱구도 엄청 많이 봣는데.. ㅋㅋㅋ",
          "original_form": "저.. 짱구도 엄청 많이 봣는데.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100021154.1.8",
          "form": "노하라 신노스케다죠~~~~",
          "original_form": "노하라 신노스케다죠~~~~",
          "speaker_id": "2",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100021154.1.9",
          "form": "아 공감가네욬ㅋㄱㅋ",
          "original_form": "아 공감가네욬ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021154.1.10",
          "form": "전 요즘도 봐요 ㅋㅋ 예전만큼 재미는 없지만",
          "original_form": "전 요즘도 봐요 ㅋㅋ 예전만큼 재미는 없지만",
          "speaker_id": "1",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021154.1.11",
          "form": "요즘도 실시간 어서 볼 수 있나요?",
          "original_form": "요즘도 실시간 어서 볼 수 있나요?",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021154.1.12",
          "form": "예전엔 누가 녹화떠서 올리는 사이트 있어서 바로바로 봤었는데..",
          "original_form": "예전엔 누가 녹화떠서 올리는 사이트 있어서 바로바로 봤었는데..",
          "speaker_id": "2",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021154.1.13",
          "form": "전 유튜브나 가끔 티비로도 봐요 ㅋㅋ",
          "original_form": "전 유튜브나 가끔 티비로도 봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021154.1.14",
          "form": "투니버스에서 하더라구요",
          "original_form": "투니버스에서 하더라구요",
          "speaker_id": "1",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021154.1.15",
          "form": "투니버스는 더빙인가여?",
          "original_form": "투니버스는 더빙인가여?",
          "speaker_id": "2",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100021154.1.16",
          "form": "네 더빙이요 ㅎㅎ",
          "original_form": "네 더빙이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100021154.1.17",
          "form": "원작으로 보세요?",
          "original_form": "원작으로 보세요?",
          "speaker_id": "1",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100021154.1.18",
          "form": "아.. 일본어판 성우가 더 좋더라구요..",
          "original_form": "아.. 일본어판 성우가 더 좋더라구요..",
          "speaker_id": "2",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100021154.1.19",
          "form": "번역하면 의미 안통하는거도 있고..",
          "original_form": "번역하면 의미 안통하는거도 있고..",
          "speaker_id": "2",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100021154.1.20",
          "form": "저는 극장판은 항상 원작으로 봐요",
          "original_form": "저는 극장판은 항상 원작으로 봐요",
          "speaker_id": "1",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100021154.1.21",
          "form": "성우는 확실히 차이가 나긴하더라구요 ㅋㅋ",
          "original_form": "성우는 확실히 차이가 나긴하더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100021154.1.22",
          "form": "극장판은 5편정도까진 본거 같은데..\r\n요즘도 계속 나오죠?",
          "original_form": "극장판은 5편정도까진 본거 같은데..\r\n요즘도 계속 나오죠?",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021154.1.23",
          "form": "네 계속 나와요 !",
          "original_form": "네 계속 나와요 !",
