      "utterance": [
        {
          "id": "MDRW2100020987.1.1",
          "form": "정보에 밝으신것 같아요",
          "original_form": "정보에 밝으신것 같아요",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100020987.1.2",
          "form": "요즘 컴터하는 시간이 많아서요..",
          "original_form": "요즘 컴터하는 시간이 많아서요..",
          "speaker_id": "2",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100020987.1.3",
          "form": "자연스럽게 알게되요..",
          "original_form": "자연스럽게 알게되요..",
          "speaker_id": "2",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100020987.1.4",
          "form": "추석오면 또 여기도기 세일시작하겟내요..",
          "original_form": "추석오면 또 여기도기 세일시작하겟내요..",
          "speaker_id": "2",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100020987.1.5",
          "form": "아 그렇겠네요",
          "original_form": "아 그렇겠네요",
          "speaker_id": "1",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100020987.1.6",
          "form": "세일할 때 좋은거 사야하는데 말이죠",
          "original_form": "세일할 때 좋은거 사야하는데 말이죠",
          "speaker_id": "1",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100020987.1.7",
          "form": "뭐 사려는거 있나요?",
          "original_form": "뭐 사려는거 있나요?",
          "speaker_id": "2",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100020987.1.8",
          "form": "냉동새우요!",
          "original_form": "냉동새우요!",
          "speaker_id": "1",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100020987.1.9",
          "form": "온라인으로요?",
          "original_form": "온라인으로요?",
          "speaker_id": "2",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100020987.1.10",
          "form": "넹 칵테일 새우 사려구요~",
          "original_form": "넹 칵테일 새우 사려구요~",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100020987.1.11",
          "form": "사시려는거 있어요?",
          "original_form": "사시려는거 있어요?",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100020987.1.12",
          "form": "전 지금은 딱히 없어요.. \r\n온라인몰 주로 어디쓰시나여?",
          "original_form": "전 지금은 딱히 없어요.. \r\n온라인몰 주로 어디쓰시나여?",
          "speaker_id": "2",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100020987.1.13",
          "form": "저는 그냥 네이버스토어에서 구매해요",
          "original_form": "저는 그냥 네이버스토어에서 구매해요",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100020987.1.14",
          "form": "가끔 티몬이나 위메프 이용하구요",
          "original_form": "가끔 티몬이나 위메프 이용하구요",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100020987.1.15",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100020987.1.16",
          "form": "전 이베이 계열이요..",
          "original_form": "전 이베이 계열이요..",
          "speaker_id": "2",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100020987.1.17",
          "form": "옥션 지마켓..",
          "original_form": "옥션 지마켓..",
          "speaker_id": "2",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100020987.1.18",
          "form": "가격이 젤 저렴해서",
          "original_form": "가격이 젤 저렴해서",
          "speaker_id": "2",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100020987.1.19",
          "form": "옥션! 옥션은 잘몰랐네요",
          "original_form": "옥션! 옥션은 잘몰랐네요",
          "speaker_id": "1",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100020987.1.20",
          "form": "부모님은 쿠팡 많이 쓰시더라구요",
          "original_form": "부모님은 쿠팡 많이 쓰시더라구요",
          "speaker_id": "1",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100020987.1.21",
          "form": "이상하게 중장년층이 쿠팡 많이 쓰시데요..",
          "original_form": "이상하게 중장년층이 쿠팡 많이 쓰시데요..",
          "speaker_id": "2",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100020987.1.22",
          "form": "광고때문인지...",
          "original_form": "광고때문인지...",
          "speaker_id": "2",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100020987.1.23",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100020987.1.24",
          "form": "배송이 빨라서라셨는데",
          "original_form": "배송이 빨라서라셨는데",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100020987.1.25",
          "form": "가격이 싼건 아닌거같던데말이죠",
          "original_form": "가격이 싼건 아닌거같던데말이죠",
          "speaker_id": "1",
          "time": "20210828 16:49"
        }
      ]
    }
