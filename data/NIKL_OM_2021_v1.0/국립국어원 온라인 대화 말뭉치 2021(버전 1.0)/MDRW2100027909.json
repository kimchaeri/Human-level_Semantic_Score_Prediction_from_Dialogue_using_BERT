      "utterance": [
        {
          "id": "MDRW2100027909.1.1",
          "form": "오... 대구 갑자기 엄청 선선해졋어요",
          "original_form": "오... 대구 갑자기 엄청 선선해졋어요",
          "speaker_id": "2",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100027909.1.2",
          "form": "그저께까지 여름이엇는데 오늘 갑자기 가을인 느낌",
          "original_form": "그저께까지 여름이엇는데 오늘 갑자기 가을인 느낌",
          "speaker_id": "2",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100027909.1.3",
          "form": "긴팔옷 살지말지 고민중이에요...",
          "original_form": "긴팔옷 살지말지 고민중이에요...",
          "speaker_id": "2",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100027909.1.4",
          "form": "9월이라고 갑자기 선선해진 느낌이에요",
          "original_form": "9월이라고 갑자기 선선해진 느낌이에요",
          "speaker_id": "1",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100027909.1.5",
          "form": "그러게용",
          "original_form": "그러게용",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027909.1.6",
          "form": "여름옷 정리하고 긴팔 꺼내야하는데",
          "original_form": "여름옷 정리하고 긴팔 꺼내야하는데",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027909.1.7",
          "form": "귀찮아요...",
          "original_form": "귀찮아요...",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027909.1.8",
          "form": "ㅋㅋㅋ아 옷정리...",
          "original_form": "ㅋㅋㅋ아 옷정리...",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027909.1.9",
          "form": "아.......(생각만해도귀찮음",
          "original_form": "아.......(생각만해도귀찮음",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027909.1.10",
          "form": "안입는 옷들 상자에 넣어서 치워뒀는데",
          "original_form": "안입는 옷들 상자에 넣어서 치워뒀는데",
          "speaker_id": "1",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027909.1.11",
          "form": "그거 꺼낼 생각을 하니 눈앞이 캄캄해요",
          "original_form": "그거 꺼낼 생각을 하니 눈앞이 캄캄해요",
          "speaker_id": "1",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027909.1.12",
          "form": "저 사실 그거 하기싫어서...",
          "original_form": "저 사실 그거 하기싫어서...",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027909.1.13",
          "form": "옷을 그냥 계절별로 구분해서 서랍에 넣어놧어요",
          "original_form": "옷을 그냥 계절별로 구분해서 서랍에 넣어놧어요",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027909.1.14",
          "form": "name2님",
          "original_form": "&name2&님",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027909.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ다 안들어가서 리빙박스에 넣어놓은 니트 몇개만 꺼내면 되는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ다 안들어가서 리빙박스에 넣어놓은 니트 몇개만 꺼내면 되는데",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027909.1.16",
          "form": "그게이럿개하기싫구...",
          "original_form": "그게이럿개하기싫구...",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027909.1.17",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027909.1.18",
          "form": "저는 철지난 옷이 보기가 싫어서",
          "original_form": "저는 철지난 옷이 보기가 싫어서",
          "speaker_id": "1",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027909.1.19",
          "form": "싹 리빙박스에 넣어서 창고방으로 치워뒀거든요",
          "original_form": "싹 리빙박스에 넣어서 창고방으로 치워뒀거든요",
          "speaker_id": "1",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027909.1.20",
          "form": "속은 시원한데 꺼내는게 일이에요",
          "original_form": "속은 시원한데 꺼내는게 일이에요",
          "speaker_id": "1",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027909.1.21",
          "form": "오 깔끔쓰",
