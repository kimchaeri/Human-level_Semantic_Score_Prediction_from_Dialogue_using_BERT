      "utterance": [
        {
          "id": "MDRW2100036093.1.1",
          "form": "비빔면은 어떤거 드세요?",
          "original_form": "비빔면은 어떤거 드세요?",
          "speaker_id": "1",
          "time": "20210830 16:06"
        },
        {
          "id": "MDRW2100036093.1.2",
          "form": "팔도...밖에 먹은게 없는거같네요 ㅎㅎ",
          "original_form": "팔도...밖에 먹은게 없는거같네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 16:07"
        },
        {
          "id": "MDRW2100036093.1.3",
          "form": "요즘 맛있는게 있나여??",
          "original_form": "요즘 맛있는게 있나여??",
          "speaker_id": "2",
          "time": "20210830 16:07"
        },
        {
          "id": "MDRW2100036093.1.4",
          "form": "삼양에서 나온 열무비빔면 이거 맛있어용",
          "original_form": "삼양에서 나온 열무비빔면 이거 맛있어용",
          "speaker_id": "1",
          "time": "20210830 16:07"
        },
        {
          "id": "MDRW2100036093.1.5",
          "form": "파는곳이 적긴하지만...",
          "original_form": "파는곳이 적긴하지만...",
          "speaker_id": "1",
          "time": "20210830 16:07"
        },
        {
          "id": "MDRW2100036093.1.6",
          "form": "오오 한번 시도해볼게요 면 조아해서 ㅋㅋㅋ",
          "original_form": "오오 한번 시도해볼게요 면 조아해서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:08"
        },
        {
          "id": "MDRW2100036093.1.7",
          "form": "면 색이 다른 제품들보다",
          "original_form": "면 색이 다른 제품들보다",
          "speaker_id": "1",
          "time": "20210830 16:08"
        },
        {
          "id": "MDRW2100036093.1.8",
          "form": "주황색에 가까워요ㅋㅋㅋㅋ",
          "original_form": "주황색에 가까워요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:08"
        },
        {
          "id": "MDRW2100036093.1.9",
          "form": "안짜고 덜자극적이에요",
          "original_form": "안짜고 덜자극적이에요",
          "speaker_id": "1",
          "time": "20210830 16:08"
        },
        {
          "id": "MDRW2100036093.1.10",
          "form": "ㅋㅋㅋ오 조아여 요즘 맵찔이라 딱이네여",
          "original_form": "ㅋㅋㅋ오 조아여 요즘 맵찔이라 딱이네여",
          "speaker_id": "2",
          "time": "20210830 16:09"
        },
        {
          "id": "MDRW2100036093.1.11",
          "form": "네네 저도 매운거 못먹는데 딱 맛있게 매운정도?",
          "original_form": "네네 저도 매운거 못먹는데 딱 맛있게 매운정도?",
          "speaker_id": "1",
          "time": "20210830 16:09"
        },
        {
          "id": "MDRW2100036093.1.12",
          "form": "오오 그렇구나 배홍동 이것도 유명하던게 드셔보셨나여?",
          "original_form": "오오 그렇구나 배홍동 이것도 유명하던게 드셔보셨나여?",
          "speaker_id": "2",
          "time": "20210830 16:09"
        },
        {
          "id": "MDRW2100036093.1.13",
          "form": "넹넹 이것도",
          "original_form": "넹넹 이것도",
          "speaker_id": "1",
          "time": "20210830 16:10"
        },
        {
          "id": "MDRW2100036093.1.14",
          "form": "좀 시원한 맛이 나더라구요",
          "original_form": "좀 시원한 맛이 나더라구요",
          "speaker_id": "1",
          "time": "20210830 16:10"
        },
        {
          "id": "MDRW2100036093.1.15",
          "form": "오그렇구나 둘중 머가 맛있어요?",
          "original_form": "오그렇구나 둘중 머가 맛있어요?",
          "speaker_id": "2",
          "time": "20210830 16:10"
        },
        {
          "id": "MDRW2100036093.1.16",
          "form": "전 열무가 더 맛있었어용!ㅎㅎ",
          "original_form": "전 열무가 더 맛있었어용!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 16:10"
        },
        {
          "id": "MDRW2100036093.1.17",
          "form": "아하 감사합니다 담에 마트가서 찾으면 담을게욬ㅋㅋ",
          "original_form": "아하 감사합니다 담에 마트가서 찾으면 담을게욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100036093.1.18",
          "form": "ㅎㅎ 입에 맞으셨으면 좋겠네용!",
          "original_form": "ㅎㅎ 입에 맞으셨으면 좋겠네용!",
          "speaker_id": "1",
          "time": "20210830 16:11"
        }
      ]
