      "utterance": [
        {
          "id": "MDRW2100020051.1.1",
          "form": "제 썰을 풀자면ㅋㅋㅋㅋㅋ",
          "original_form": "제 썰을 풀자면ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
