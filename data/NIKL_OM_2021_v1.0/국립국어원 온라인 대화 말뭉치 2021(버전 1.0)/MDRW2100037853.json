      "utterance": [
        {
          "id": "MDRW2100037853.1.1",
          "form": "저녁은 드셨나요?.",
          "original_form": "저녁은 드셨나요?.",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037853.1.2",
          "form": "어제 피곤해서 저녁 패쓰하고 잣어요",
          "original_form": "어제 피곤해서 저녁 패쓰하고 잣어요",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100037853.1.3",
          "form": "배고픕니다 흑",
          "original_form": "배고픕니다 흑",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100037853.1.4",
          "form": "아??? 그리고 아침도요??? 견딜 수 없어요ㅜ",
          "original_form": "아??? 그리고 아침도요??? 견딜 수 없어요ㅜ",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100037853.1.5",
          "form": "아침에 미숫가루먹엇어요ㅜㅋㅋ",
          "original_form": "아침에 미숫가루먹엇어요ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100037853.1.6",
          "form": "아침엔 주로 머드시나용",
          "original_form": "아침엔 주로 머드시나용",
          "speaker_id": "2",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100037853.1.7",
          "form": "전 밥ㅋㅋ 가족들과 같이 살아서ㅋㅋ 한식 챙깁니다ㅋㅋ",
          "original_form": "전 밥ㅋㅋ 가족들과 같이 살아서ㅋㅋ 한식 챙깁니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100037853.1.8",
          "form": "점심은 든든하게 드셨나요?",
          "original_form": "점심은 든든하게 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100037853.1.9",
          "form": "아 저는 직장에서 다같이 도시락 배달시켜먹어요",
          "original_form": "아 저는 직장에서 다같이 도시락 배달시켜먹어요",
          "speaker_id": "2",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100037853.1.10",
          "form": "맛은그닥이지만 배고파서잘먹어요",
          "original_form": "맛은그닥이지만 배고파서잘먹어요",
          "speaker_id": "2",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100037853.1.11",
          "form": "ㅋㅋ저는 집에서 싸온거 먹었는데 과식했네요",
          "original_form": "ㅋㅋ저는 집에서 싸온거 먹었는데 과식했네요",
          "speaker_id": "1",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100037853.1.12",
          "form": "지금도 배불러요",
          "original_form": "지금도 배불러요",
          "speaker_id": "1",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100037853.1.13",
          "form": "저는 배고파요ㅜㅜ 다섯시되자마자",
          "original_form": "저는 배고파요ㅜㅜ 다섯시되자마자",
          "speaker_id": "2",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100037853.1.14",
          "form": "집에가서 저녁먹을꺼에요",
          "original_form": "집에가서 저녁먹을꺼에요",
          "speaker_id": "2",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100037853.1.15",
          "form": "퇴근얼마안남앗어요!!",
          "original_form": "퇴근얼마안남앗어요!!",
          "speaker_id": "2",
          "time": "20210902 16:17"
        },
        {
          "id": "MDRW2100037853.1.16",
          "form": "5시퇴근이신거예요!!! 엄청 부럽습니다!!!!",
          "original_form": "5시퇴근이신거예요!!! 엄청 부럽습니다!!!!",
          "speaker_id": "1",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100037853.1.17",
          "form": "저녁은 뭐드실거예요??",
          "original_form": "저녁은 뭐드실거예요??",
