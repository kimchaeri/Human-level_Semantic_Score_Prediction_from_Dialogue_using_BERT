        {
          "id": "MDRW2100023758.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 16:45"
        },
        {
          "id": "MDRW2100023758.1.2",
          "form": "좋아하죠!",
          "original_form": "좋아하죠!",
          "speaker_id": "2",
          "time": "20210830 16:56"
        },
        {
          "id": "MDRW2100023758.1.3",
          "form": "어떤게임 하세요?",
          "original_form": "어떤게임 하세요?",
          "speaker_id": "1",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100023758.1.4",
          "form": "요즘은 로스트아크하고있어요",
          "original_form": "요즘은 로스트아크하고있어요",
          "speaker_id": "2",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100023758.1.5",
          "form": "로아 하시는구나 직업이 뭐예요?",
          "original_form": "로아 하시는구나 직업이 뭐예요?",
          "speaker_id": "1",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100023758.1.6",
          "form": "다있어요!",
          "original_form": "다있어요!",
          "speaker_id": "2",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100023758.1.7",
          "form": "전직업다키웁니다",
          "original_form": "전직업다키웁니다",
          "speaker_id": "2",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100023758.1.8",
          "form": "매주 돌리시겠네요 쏠쏠 하게 벌리나요?",
          "original_form": "매주 돌리시겠네요 쏠쏠 하게 벌리나요?",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100023758.1.9",
          "form": "순수골드로 15만?",
          "original_form": "순수골드로 15만?",
          "speaker_id": "2",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100023758.1.10",
          "form": "정도 되네요",
          "original_form": "정도 되네요",
          "speaker_id": "2",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100023758.1.11",
          "form": "우와 템렙 높으실거 같아요",
          "original_form": "우와 템렙 높으실거 같아요",
          "speaker_id": "1",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100023758.1.12",
          "form": "아뇨 높지는않아요 ㅋㅋㅋ",
          "original_form": "아뇨 높지는않아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100023758.1.13",
          "form": "얼마예요?",
          "original_form": "얼마예요?",
          "speaker_id": "1",
          "time": "20210830 18:00"
        },
        {
          "id": "MDRW2100023758.1.14",
          "form": "1510이 본캐구요",
          "original_form": "1510이 본캐구요",
          "speaker_id": "2",
          "time": "20210830 18:04"
        },
        {
          "id": "MDRW2100023758.1.15",
          "form": "나머진 배럭들이라서 높진않습니다.",
          "original_form": "나머진 배럭들이라서 높진않습니다.",
          "speaker_id": "2",
          "time": "20210830 18:04"
        },
        {
          "id": "MDRW2100023758.1.16",
          "form": "그러시구나 언제부터 하셨나요?",
          "original_form": "그러시구나 언제부터 하셨나요?",
