      "utterance": [
        {
          "id": "MDRW2100008443.1.1",
          "form": "무슨 일 하시나요",
          "original_form": "무슨 일 하시나요",
          "speaker_id": "1",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100008443.1.2",
          "form": "현재 일은 하고있지않아요",
          "original_form": "현재 일은 하고있지않아요",
          "speaker_id": "2",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008443.1.3",
          "form": "일을 하고계신가요",
          "original_form": "일을 하고계신가요",
          "speaker_id": "2",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008443.1.4",
          "form": "저는 인턴하다가 얼마전에 그만 뒀어요",
          "original_form": "저는 인턴하다가 얼마전에 그만 뒀어요",
          "speaker_id": "1",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008443.1.5",
          "form": "아르바이트는 해보셨나요?",
          "original_form": "아르바이트는 해보셨나요?",
          "speaker_id": "1",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008443.1.6",
          "form": "피시방알바 해봤어요",
          "original_form": "피시방알바 해봤어요",
          "speaker_id": "2",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008443.1.7",
          "form": "인턴생활은 어땠나요",
          "original_form": "인턴생활은 어땠나요",
          "speaker_id": "2",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008443.1.8",
          "form": "어우 너무 힘들더라고요",
          "original_form": "어우 너무 힘들더라고요",
          "speaker_id": "1",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008443.1.9",
          "form": "사람들 눈치보는게 제일 힘들었어요",
          "original_form": "사람들 눈치보는게 제일 힘들었어요",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008443.1.10",
          "form": "어떤점이 힘들었나요",
          "original_form": "어떤점이 힘들었나요",
          "speaker_id": "2",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008443.1.11",
          "form": "아 ㅠ대인관계",
          "original_form": "아 ㅠ대인관계",
          "speaker_id": "2",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008443.1.12",
          "form": "네ㅠㅠ",
          "original_form": "네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008443.1.13",
          "form": "맞아요 대인관계힘든건 답도없어요",
          "original_form": "맞아요 대인관계힘든건 답도없어요",
          "speaker_id": "2",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008443.1.14",
          "form": "사실 제가 일할건 없어서",
          "original_form": "사실 제가 일할건 없어서",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008443.1.15",
          "form": "상관없었는데",
          "original_form": "상관없었는데",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008443.1.16",
          "form": "그건 제가 노력한다고 되는게 아니더라구요..",
          "original_form": "그건 제가 노력한다고 되는게 아니더라구요..",
          "speaker_id": "2",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008443.1.17",
          "form": "회사 분위기가 좀 안좋을데 들어가서",
          "original_form": "회사 분위기가 좀 안좋을데 들어가서",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008443.1.18",
          "form": "힘들더라고요 ㅠㅠ",
          "original_form": "힘들더라고요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008443.1.19",
          "form": "힘드셨겠어요ㅠㅠ",
          "original_form": "힘드셨겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008443.1.20",
          "form": "피방 알바는 어땟어요?",
          "original_form": "피방 알바는 어땟어요?",
          "speaker_id": "1",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008443.1.21",
          "form": "헬이었어요",
          "original_form": "헬이었어요",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008443.1.22",
          "form": "다시는 하고싶지않아요ㅋㅋㅋ",
          "original_form": "다시는 하고싶지않아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008443.1.23",
          "form": "라면셔틀인줄..",
          "original_form": "라면셔틀인줄..",
          "speaker_id": "2",
          "time": "20210822 15:56"
        }
      ]
