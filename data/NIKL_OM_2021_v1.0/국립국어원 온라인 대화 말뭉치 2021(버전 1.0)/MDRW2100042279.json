      "utterance": [
        {
          "id": "MDRW2100042279.1.1",
          "form": "어우 정말요?",
          "original_form": "어우 정말요?",
          "speaker_id": "2",
          "time": "20210907 08:12"
        },
        {
          "id": "MDRW2100042279.1.2",
          "form": "제 취향은 아니겠어요ㅜㅜ",
          "original_form": "제 취향은 아니겠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 08:13"
        },
        {
          "id": "MDRW2100042279.1.3",
          "form": "드라마 어떤거 보세요?",
          "original_form": "드라마 어떤거 보세요?",
          "speaker_id": "1",
          "time": "20210907 08:58"
        },
        {
          "id": "MDRW2100042279.1.4",
          "form": "저는 드라마 몰아보는게 더 좋아요 히히",
          "original_form": "저는 드라마 몰아보는게 더 좋아요 히히",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100042279.1.5",
          "form": "요새 로스쿨 정주행 거의 끝나갑니다 히히",
          "original_form": "요새 로스쿨 정주행 거의 끝나갑니다 히히",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100042279.1.6",
          "form": "오오 로맨스물인가요??",
          "original_form": "오오 로맨스물인가요??",
          "speaker_id": "1",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100042279.1.7",
          "form": "아뇨 법률 드라마에요 ㅋㅋㅋㅋㅋ 친구가 법조인이라 같이 보고있어요",
          "original_form": "아뇨 법률 드라마에요 ㅋㅋㅋㅋㅋ 친구가 법조인이라 같이 보고있어요",
          "speaker_id": "2",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100042279.1.8",
          "form": "제가 로맨스물을 별로안좋아해가지구ㅠㅠ",
          "original_form": "제가 로맨스물을 별로안좋아해가지구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100042279.1.9",
          "form": "아아 우아 법률 드라마 재밋죠!!",
          "original_form": "아아 우아 법률 드라마 재밋죠!!",
          "speaker_id": "1",
          "time": "20210907 17:06"
        },
        {
          "id": "MDRW2100042279.1.10",
          "form": "그러면 수사물 이런거 좋아하세요?",
          "original_form": "그러면 수사물 이런거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 17:06"
        },
        {
          "id": "MDRW2100042279.1.11",
          "form": "오 네!! 피튀기는거맘 아니면 잘봅니다 후후후후",
          "original_form": "오 네!! 피튀기는거맘 아니면 잘봅니다 후후후후",
          "speaker_id": "2",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100042279.1.12",
          "form": "추리물도 대박 봏아해요",
          "original_form": "추리물도 대박 봏아해요",
          "speaker_id": "2",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100042279.1.13",
          "form": "오오 추리물 너무 좋죠!!",
          "original_form": "오오 추리물 너무 좋죠!!",
          "speaker_id": "1",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100042279.1.14",
          "form": "시그널 보셧나요?",
          "original_form": "시그널 보셧나요?",
          "speaker_id": "1",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100042279.1.15",
          "form": "아악 듣기만 했어요",
          "original_form": "아악 듣기만 했어요",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100042279.1.16",
          "form": "그렇게 재밌다면서요",
          "original_form": "그렇게 재밌다면서요",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100042279.1.17",
          "form": "네네 아직도 시즌2 기다리고 있다하네요 ㅋㅋㅋㅋㅋ",
          "original_form": "네네 아직도 시즌2 기다리고 있다하네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100042279.1.18",
          "form": "앜ㅋㅋㅋㅋㅋ 아직도",
          "original_form": "앜ㅋㅋㅋㅋㅋ 아직도",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100042279.1.19",
          "form": "이미 나온줄 알았어요 저는 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이미 나온줄 알았어요 저는 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100042279.1.20",
          "form": "ㅋㅋㅋㅋ 시리즈물은 몰아서 봐야 좋드라구요",
          "original_form": "ㅋㅋㅋㅋ 시리즈물은 몰아서 봐야 좋드라구요",
