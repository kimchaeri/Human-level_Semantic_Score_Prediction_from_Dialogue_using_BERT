      "utterance": [
        {
          "id": "MDRW2100041589.1.1",
          "form": "요거트요? 플레인으로 드세요?",
          "original_form": "요거트요? 플레인으로 드세요?",
          "speaker_id": "2",
          "time": "20210906 11:54"
        },
        {
          "id": "MDRW2100041589.1.2",
          "form": "네 플레인요!",
          "original_form": "네 플레인요!",
          "speaker_id": "1",
          "time": "20210906 11:54"
        },
        {
          "id": "MDRW2100041589.1.3",
          "form": "요거트만 드시는건가요",
          "original_form": "요거트만 드시는건가요",
          "speaker_id": "2",
          "time": "20210906 11:54"
        },
        {
          "id": "MDRW2100041589.1.4",
          "form": "견과류랑 먹기도 하고",
          "original_form": "견과류랑 먹기도 하고",
          "speaker_id": "1",
          "time": "20210906 11:54"
        },
        {
          "id": "MDRW2100041589.1.5",
          "form": "저는 건과일이랑 자주 먹어요",
          "original_form": "저는 건과일이랑 자주 먹어요",
          "speaker_id": "2",
          "time": "20210906 11:54"
        },
        {
          "id": "MDRW2100041589.1.6",
          "form": "건과일이면",
          "original_form": "건과일이면",
          "speaker_id": "1",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100041589.1.7",
          "form": "건 크랜베리나 건블루베리 같은거요!",
          "original_form": "건 크랜베리나 건블루베리 같은거요!",
          "speaker_id": "2",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100041589.1.8",
          "form": "그것들도 달달하지 않나요!",
          "original_form": "그것들도 달달하지 않나요!",
          "speaker_id": "1",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100041589.1.9",
          "form": "건자두를 제일 많이 먹어요 요즘은",
          "original_form": "건자두를 제일 많이 먹어요 요즘은",
          "speaker_id": "2",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100041589.1.10",
          "form": "건자두도 맛있어요?",
          "original_form": "건자두도 맛있어요?",
          "speaker_id": "1",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100041589.1.11",
          "form": "푸룬이라고 배변활동에 좋아요 ㅎㅎ",
          "original_form": "푸룬이라고 배변활동에 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100041589.1.12",
          "form": "오 저도 먹어봐야겠어요 저는 오트밀이나 견과류랑만 먹었거든요",
          "original_form": "오 저도 먹어봐야겠어요 저는 오트밀이나 견과류랑만 먹었거든요",
          "speaker_id": "1",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100041589.1.13",
          "form": "오트밀이나 견과류랑 섞어서 먹어도 좋아요",
          "original_form": "오트밀이나 견과류랑 섞어서 먹어도 좋아요",
