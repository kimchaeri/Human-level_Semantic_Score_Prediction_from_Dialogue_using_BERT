      "utterance": [
        {
          "id": "MDRW2100037601.1.1",
          "form": "콩국수 좋아하세요?",
          "original_form": "콩국수 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100037601.1.2",
          "form": "일년에 한 두번 정도? 먹을 정도로 좋아해요",
          "original_form": "일년에 한 두번 정도? 먹을 정도로 좋아해요",
          "speaker_id": "1",
          "time": "20210901 16:38"
        },
        {
          "id": "MDRW2100037601.1.3",
          "form": "아 많이는 아니군요",
          "original_form": "아 많이는 아니군요",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100037601.1.4",
          "form": "저도 이번에 처음 먹어봤는데",
          "original_form": "저도 이번에 처음 먹어봤는데",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100037601.1.5",
          "form": "음.. 그렇더라구요",
          "original_form": "음.. 그렇더라구요",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100037601.1.6",
          "form": "ㅋㅋ좀 삼삼하고 시원한 국물 느낌 시원하 면 촉감",
          "original_form": "ㅋㅋ좀 삼삼하고 시원한 국물 느낌 시원하 면 촉감",
          "speaker_id": "1",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037601.1.7",
          "form": "이걸 즐기는 음식이라고 생각해요",
          "original_form": "이걸 즐기는 음식이라고 생각해요",
          "speaker_id": "1",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037601.1.8",
          "form": "약간 꾸덕하고",
          "original_form": "약간 꾸덕하고",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037601.1.9",
          "form": "뭔가 느끼하더라구요..",
          "original_form": "뭔가 느끼하더라구요..",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037601.1.10",
          "form": "안그러세요?",
          "original_form": "안그러세요?",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037601.1.11",
          "form": "꾸덕,,? 어디서 드셨나요? 땅콩버터를 넣은 레시피일지두",
          "original_form": "꾸덕,,? 어디서 드셨나요? 땅콩버터를 넣은 레시피일지두",
          "speaker_id": "1",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100037601.1.12",
          "form": "저 시켜서 먹었어요",
          "original_form": "저 시켜서 먹었어요",
          "speaker_id": "2",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100037601.1.13",
          "form": "국물 보다는 약간.. 장같은 느낌?",
          "original_form": "국물 보다는 약간.. 장같은 느낌?",
          "speaker_id": "2",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100037601.1.14",
          "form": "콩국물만 내려서 넣음 밍밍하고 중국 또우장같아요",
          "original_form": "콩국물만 내려서 넣음 밍밍하고 중국 또우장같아요",
          "speaker_id": "1",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100037601.1.15",
          "form": "근데 좀 끈적하게 만들려고 곡물가루나 땅콩버터넣기두 해요",
          "original_form": "근데 좀 끈적하게 만들려고 곡물가루나 땅콩버터넣기두 해요",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100037601.1.16",
          "form": "아하.. 설탕하고 소금도 왕창 넣었는데",
          "original_form": "아하.. 설탕하고 소금도 왕창 넣었는데",
          "speaker_id": "2",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100037601.1.17",
          "form": "아쉬웠어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아쉬웠어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100037601.1.18",
          "form": "아 그럼 좀 밍밍한 콩국수를 함 드셔보세요",
          "original_form": "아 그럼 좀 밍밍한 콩국수를 함 드셔보세요",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100037601.1.19",
          "form": "어디서 파는진 모르지만 ㅠ0ㅠㅋㅋ",
          "original_form": "어디서 파는진 모르지만 ㅠ0ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100037601.1.20",
          "form": "ㅋㅋㅋㅋ약간 제 스타일이 아닌 느낌!!",
          "original_form": "ㅋㅋㅋㅋ약간 제 스타일이 아닌 느낌!!",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100037601.1.21",
          "form": "그래도 담에",
          "original_form": "그래도 담에",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100037601.1.22",
          "form": "전 그냥 콩국 좋아해요 묽은 콩국",
          "original_form": "전 그냥 콩국 좋아해요 묽은 콩국",
