      "utterance": [
        {
          "id": "MDRW2100018759.1.1",
          "form": "네 좋아해요 name1은요",
          "original_form": "네 좋아해요 &name1&은요",
          "speaker_id": "2",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018759.1.2",
          "form": "메세지가 잘안가네요ㅋㅋ오류가좀 있나봐요",
          "original_form": "메세지가 잘안가네요ㅋㅋ오류가좀 있나봐요",
          "speaker_id": "2",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100018759.1.3",
          "form": "전 주변에 볼 수 있는 공연장이 없어서.....",
          "original_form": "전 주변에 볼 수 있는 공연장이 없어서.....",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100018759.1.4",
          "form": "보고싶어도 많이 못봐요ㅜ",
          "original_form": "보고싶어도 많이 못봐요ㅜ",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100018759.1.5",
          "form": "서울 놀러가면 타이밍 맞을때?",
          "original_form": "서울 놀러가면 타이밍 맞을때?",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100018759.1.6",
          "form": "저는 요샌 거의 못갔어요ㅜㅜ",
          "original_form": "저는 요샌 거의 못갔어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100018759.1.7",
          "form": "전에는 종종갔는데ㅜㅜ",
          "original_form": "전에는 종종갔는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 15:22"
        },
        {
          "id": "MDRW2100018759.1.8",
          "form": "요즘 헤드윅 하더라구요ㅜㅜ",
          "original_form": "요즘 헤드윅 하더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100018759.1.9",
          "form": "진짜 보고싶어도",
          "original_form": "진짜 보고싶어도",
          "speaker_id": "1",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100018759.1.10",
          "form": "일단 표구하기도 힘드네요ㅎ",
          "original_form": "일단 표구하기도 힘드네요ㅎ",
          "speaker_id": "1",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100018759.1.11",
          "form": "조승우",
          "original_form": "조승우",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100018759.1.12",
          "form": "주연나오든뎅",
          "original_form": "주연나오든뎅",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100018759.1.13",
          "form": "조승우 오만석 이규형 다ㅠㅜ",
          "original_form": "조승우 오만석 이규형 다ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100018759.1.14",
          "form": "ㅜㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100018759.1.15",
          "form": "보고싶어요",
          "original_form": "보고싶어요",
          "speaker_id": "2",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100018759.1.16",
          "form": "조승우님은 티켓팅 하는것 부터........",
          "original_form": "조승우님은 티켓팅 하는것 부터........",
          "speaker_id": "1",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100018759.1.17",
          "form": "제친구 조승우팬이라 라만차의기사1열보고왔대요",
          "original_form": "제친구 조승우팬이라 라만차의기사1열보고왔대요",
          "speaker_id": "2",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100018759.1.18",
          "form": "신이라고 불러줬습죠 ㅋㅋㅋ",
          "original_form": "신이라고 불러줬습죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100018759.1.19",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "1",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100018759.1.20",
          "form": "1열이요...??",
          "original_form": "1열이요...??",
          "speaker_id": "1",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100018759.1.21",
          "form": "그게 가능한가요ㅜ",
          "original_form": "그게 가능한가요ㅜ",
          "speaker_id": "1",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100018759.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100018759.1.23",
          "form": "좋겠다",
          "original_form": "좋겠다",
          "speaker_id": "2",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100018759.1.24",
          "form": "그건 정말 어떻게 해야 가능한건가요ㅜㅜ",
          "original_form": "그건 정말 어떻게 해야 가능한건가요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100018759.1.25",
          "form": "정말 부럽네요ㅠ",
          "original_form": "정말 부럽네요ㅠ",
          "speaker_id": "1",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100018759.1.26",
          "form": "ㅋㅋ혼자 티켓팅 연습하더라구요 다른공연",
          "original_form": "ㅋㅋ혼자 티켓팅 연습하더라구요 다른공연",
          "speaker_id": "2",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100018759.1.27",
          "form": "연습한다고 되는건가요ㅜ",
          "original_form": "연습한다고 되는건가요ㅜ",
          "speaker_id": "1",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100018759.1.28",
          "form": "ㅋㅋㅋㅜㅜ",
          "original_form": "ㅋㅋㅋㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100018759.1.29",
          "form": "전..안되던데..",
          "original_form": "전..안되던데..",
          "speaker_id": "2",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100018759.1.30",
          "form": "다른사람 티켓팅 방해나하지ㅜㅜㅋㅋㅋㅋ",
          "original_form": "다른사람 티켓팅 방해나하지ㅜㅜㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100018759.1.31",
          "form": "와 비법이라도 전수받고 싶네요ㅜ",
          "original_form": "와 비법이라도 전수받고 싶네요ㅜ",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100018759.1.32",
          "form": "저도요 ㅋㄱㄱ근데 코로나라 공연댕기기 눈치보여요",
          "original_form": "저도요 ㅋㄱㄱ근데 코로나라 공연댕기기 눈치보여요",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100018759.1.33",
          "form": "근다 가는사람들은 다 가더라구요ㅜ",
