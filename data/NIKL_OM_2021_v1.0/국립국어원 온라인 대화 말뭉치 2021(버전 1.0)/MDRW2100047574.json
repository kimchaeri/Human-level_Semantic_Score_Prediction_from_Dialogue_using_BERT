      "utterance": [
        {
          "id": "MDRW2100047574.1.1",
          "form": "따로 가신 콘서트는 없으세요?",
          "original_form": "따로 가신 콘서트는 없으세요?",
          "speaker_id": "1",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100047574.1.2",
          "form": "저는 박정현씨 콘서트 갔었어요",
          "original_form": "저는 박정현씨 콘서트 갔었어요",
          "speaker_id": "2",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100047574.1.3",
          "form": "친구분 티켓팅만 도와줬던 건가요ㅋㅋㅋㄱ",
          "original_form": "친구분 티켓팅만 도와줬던 건가요ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100047574.1.4",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "1",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100047574.1.5",
          "form": "어땠어요?",
          "original_form": "어땠어요?",
          "speaker_id": "1",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100047574.1.6",
          "form": "저는 사실 노래를 다알지는 않아서",
          "original_form": "저는 사실 노래를 다알지는 않아서",
          "speaker_id": "2",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100047574.1.7",
          "form": "아는 노래반 아닌노래반이였는데",
          "original_form": "아는 노래반 아닌노래반이였는데",
          "speaker_id": "2",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100047574.1.8",
          "form": "그럼에도 저 눈물 줄줄 하고 나왔어요",
          "original_form": "그럼에도 저 눈물 줄줄 하고 나왔어요",
          "speaker_id": "2",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100047574.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ감수성 넘치네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ감수성 넘치네요",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100047574.1.10",
          "form": "저는 콘서트는 아니었는데",
          "original_form": "저는 콘서트는 아니었는데",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100047574.1.11",
          "form": "유리향기 본 적 있어요",
          "original_form": "유리향기 본 적 있어요",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100047574.1.12",
          "form": "아니뭐래",
          "original_form": "아니뭐래",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100047574.1.13",
          "form": "유리상자?)",
          "original_form": "유리상자?)",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100047574.1.14",
          "form": "옛날 가수요",
          "original_form": "옛날 가수요",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100047574.1.15",
          "form": "앜ㄲㅋㅋㅋ넘하시네 옛날가수",
          "original_form": "앜ㄲㅋㅋㅋ넘하시네 옛날가수",
          "speaker_id": "2",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100047574.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100047574.1.17",
          "form": "어디서 보셨나요?",
          "original_form": "어디서 보셨나요?",
          "speaker_id": "2",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100047574.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100047574.1.19",
          "form": "유희열의 스케치북 같은 느낌의 극장에서",
          "original_form": "유희열의 스케치북 같은 느낌의 극장에서",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100047574.1.20",
          "form": "쭉 노래를 들었어요",
          "original_form": "쭉 노래를 들었어요",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100047574.1.21",
          "form": "사실 아는 노래는 없었는데",
          "original_form": "사실 아는 노래는 없었는데",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100047574.1.22",
          "form": "음색은 너무 좋았어요",
          "original_form": "음색은 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100047574.1.23",
          "form": "우와 신기해요",
          "original_form": "우와 신기해요",
          "speaker_id": "2",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100047574.1.24",
          "form": "그럴수도 있군요",
          "original_form": "그럴수도 있군요",
          "speaker_id": "2",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100047574.1.25",
          "form": "유리상자 좋아하시나요",
          "original_form": "유리상자 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100047574.1.26",
          "form": "아..아니요",
          "original_form": "아..아니요",
          "speaker_id": "2",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100047574.1.27",
          "form": "그렇딤 발언을 살짝쿵 취소를..",
          "original_form": "그렇딤 발언을 살짝쿵 취소를..",
          "speaker_id": "1",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100047574.1.28",
          "form": "ㅋㄱㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㄱㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100047574.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100047574.1.30",
          "form": "엑소콘서트가셨을때",
          "original_form": "엑소콘서트가셨을때",
          "speaker_id": "2",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100047574.1.31",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100047574.1.32",
          "form": "좌석은 어디셨나요",
          "original_form": "좌석은 어디셨나요",
          "speaker_id": "2",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100047574.1.33",
          "form": "가까우셨나요?",
          "original_form": "가까우셨나요?",
          "speaker_id": "2",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100047574.1.34",
          "form": "저 스탠딩이였어요",
          "original_form": "저 스탠딩이였어요",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100047574.1.35",
          "form": "대박이죠",
          "original_form": "대박이죠",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100047574.1.36",
          "form": "헐대박 앵콜까지 버티셨나요!",
          "original_form": "헐대박 앵콜까지 버티셨나요!",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100047574.1.37",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210826 21:06"
        }
      ]
