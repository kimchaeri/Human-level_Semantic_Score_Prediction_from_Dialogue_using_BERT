      "utterance": [
        {
          "id": "MDRW2100037010.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100037010.1.2",
          "form": "네 고양이1마리키우고있어요",
          "original_form": "네 고양이1마리키우고있어요",
          "speaker_id": "1",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100037010.1.3",
          "form": "name2님은여",
          "original_form": "&name2&님은여",
          "speaker_id": "1",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100037010.1.4",
          "form": "저는 지금은 없어요. 어릴 땐 새 키웠었어요",
          "original_form": "저는 지금은 없어요. 어릴 땐 새 키웠었어요",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100037010.1.5",
          "form": "어떤 새 키우셨었나요",
          "original_form": "어떤 새 키우셨었나요",
          "speaker_id": "1",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100037010.1.6",
          "form": "백문조 키웠었어요",
          "original_form": "백문조 키웠었어요",
          "speaker_id": "2",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100037010.1.7",
          "form": "하얀새인가요?",
          "original_form": "하얀새인가요?",
          "speaker_id": "1",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100037010.1.8",
          "form": "네ㅋㅋ 참새 사이즈에 흰 깃털, 다홍색 부리에요",
          "original_form": "네ㅋㅋ 참새 사이즈에 흰 깃털, 다홍색 부리에요",
          "speaker_id": "2",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100037010.1.9",
          "form": "오오 넘귀여울거같아요",
          "original_form": "오오 넘귀여울거같아요",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100037010.1.10",
          "form": "고양이는 몇 살인가요?",
          "original_form": "고양이는 몇 살인가요?",
          "speaker_id": "2",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100037010.1.11",
          "form": "1마리였나여",
          "original_form": "1마리였나여",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100037010.1.12",
          "form": "저희애5살이여",
          "original_form": "저희애5살이여",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100037010.1.13",
          "form": "쌍으로 키웠었는데 얘들이 알 낳고 좀 이따 보면 부리에 노란 게 발라져있던 기억이..",
          "original_form": "쌍으로 키웠었는데 얘들이 알 낳고 좀 이따 보면 부리에 노란 게 발라져있던 기억이..",
          "speaker_id": "2",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100037010.1.14",
          "form": "알도낳았군여 신기해요 ㅎㄹ",
          "original_form": "알도낳았군여 신기해요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100037010.1.15",
          "form": "고양이 용품도 종류가 되게 다양하더라구요",
          "original_form": "고양이 용품도 종류가 되게 다양하더라구요",
          "speaker_id": "2",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100037010.1.16",
          "form": "네 넘많고 근데 사줘도 안쓰는게 더많아요ㅠ",
          "original_form": "네 넘많고 근데 사줘도 안쓰는게 더많아요ㅠ",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100037010.1.17",
          "form": "ㅋㅋ 마음 같지가 않군요. 선물보다 박스를 선호하나요?",
          "original_form": "ㅋㅋ 마음 같지가 않군요. 선물보다 박스를 선호하나요?",
          "speaker_id": "2",
          "time": "20210831 17:12"
        },
        {
          "id": "MDRW2100037010.1.18",
          "form": "걍 쳐다도안보더라구여 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "걍 쳐다도안보더라구여 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:12"
        },
        {
          "id": "MDRW2100037010.1.19",
          "form": "박스는 한번 들어갔다나오고하네요",
          "original_form": "박스는 한번 들어갔다나오고하네요",
          "speaker_id": "1",
          "time": "20210831 17:12"
        },
        {
          "id": "MDRW2100037010.1.20",
          "form": "흑흑",
          "original_form": "흑흑",
