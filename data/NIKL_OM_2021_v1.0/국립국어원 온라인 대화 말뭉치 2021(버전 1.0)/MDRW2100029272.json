      "utterance": [
        {
          "id": "MDRW2100029272.1.1",
          "form": "그나마 출근이 좀 늦는 직장이에요ㅎ 10시",
          "original_form": "그나마 출근이 좀 늦는 직장이에요ㅎ 10시",
          "speaker_id": "1",
          "time": "20210903 00:55"
        },
        {
          "id": "MDRW2100029272.1.2",
          "form": "물론 퇴근도 늦어집니다ㅜ",
          "original_form": "물론 퇴근도 늦어집니다ㅜ",
          "speaker_id": "1",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029272.1.3",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029272.1.4",
          "form": "회장님?",
          "original_form": "회장님?",
          "speaker_id": "2",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029272.1.5",
          "form": "10시출근 좋네요",
          "original_form": "10시출근 좋네요",
          "speaker_id": "2",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029272.1.6",
          "form": "사실 저도 유연근무제라",
          "original_form": "사실 저도 유연근무제라",
          "speaker_id": "2",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029272.1.7",
          "form": "ㅎㅎ 러시아워 피해서 좋긴해요ㅎ",
          "original_form": "ㅎㅎ 러시아워 피해서 좋긴해요ㅎ",
          "speaker_id": "1",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029272.1.8",
          "form": "10시 4시 코어타임만 지키면됩니다",
          "original_form": "10시 4시 코어타임만 지키면됩니다",
          "speaker_id": "2",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029272.1.9",
          "form": "삼성이랑 빗ㅇㅅ",
          "original_form": "삼성이랑 빗ㅇㅅ",
          "speaker_id": "2",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029272.1.10",
          "form": "비슷",
          "original_form": "비슷",
          "speaker_id": "2",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029272.1.11",
          "form": "아 근데 점심시간을 한시간반 잡아놔서ㅜ",
          "original_form": "아 근데 점심시간을 한시간반 잡아놔서ㅜ",
          "speaker_id": "1",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029272.1.12",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029272.1.13",
          "form": "그건 부럽네요",
          "original_form": "그건 부럽네요",
          "speaker_id": "2",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029272.1.14",
          "form": "7시반퇴근;;",
          "original_form": "7시반퇴근;;",
          "speaker_id": "1",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029272.1.15",
          "form": "심지어 잘 못지킴",
          "original_form": "심지어 잘 못지킴",
          "speaker_id": "1",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029272.1.16",
          "form": "아아뇨 이게 다 퇴근시간을 늦게 잡기 위한 사주의 수작임!",
          "original_form": "아아뇨 이게 다 퇴근시간을 늦게 잡기 위한 사주의 수작임!",
          "speaker_id": "1",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029272.1.17",
          "form": "대한민국 직장인들 다 그렇죠 ㅠ",
          "original_form": "대한민국 직장인들 다 그렇죠 ㅠ",
          "speaker_id": "2",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029272.1.18",
          "form": "아 맞아요?",
          "original_form": "아 맞아요?",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100029272.1.19",
          "form": "전 6시되면 튑니다",
          "original_form": "전 6시되면 튑니다",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100029272.1.20",
          "form": "텨텨",
          "original_form": "텨텨",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100029272.1.21",
          "form": "ㅎ 눈치보느라 못가는게 아니라",
          "original_form": "ㅎ 눈치보느라 못가는게 아니라",
          "speaker_id": "1",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100029272.1.22",
          "form": "일이 안 끝나서ㅜ",
          "original_form": "일이 안 끝나서ㅜ",
          "speaker_id": "1",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100029272.1.23",
          "form": "근무시간때문이네요 ㅎㅎ",
          "original_form": "근무시간때문이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100029272.1.24",
          "form": "태그하셔야되니까",
          "original_form": "태그하셔야되니까",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100029272.1.25",
          "form": "회사는 가깝습니까",
          "original_form": "회사는 가깝습니까",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100029272.1.26",
          "form": "그리고 업무특성상 반드시 그날 처리해야 해요ㅜ",
          "original_form": "그리고 업무특성상 반드시 그날 처리해야 해요ㅜ",
          "speaker_id": "1",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100029272.1.27",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100029272.1.28",
          "form": "아뇨 그닥ㅜ",
          "original_form": "아뇨 그닥ㅜ",
          "speaker_id": "1",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100029272.1.29",
          "form": "당일처리?",
          "original_form": "당일처리?",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100029272.1.30",
          "form": "와 빡센데여",
          "original_form": "와 빡센데여",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100029272.1.31",
          "form": "내일로 미룰수도 없고 매일매일 마감이라ㅜ",
          "original_form": "내일로 미룰수도 없고 매일매일 마감이라ㅜ",
          "speaker_id": "1",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100029272.1.32",
          "form": "쉽지 않네요",
          "original_form": "쉽지 않네요",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100029272.1.33",
          "form": "그래도 깔끔하네요 하루에 처리가 다되는 일이라",
          "original_form": "그래도 깔끔하네요 하루에 처리가 다되는 일이라",
          "speaker_id": "2",
          "time": "20210903 01:00"
        }
      ]
