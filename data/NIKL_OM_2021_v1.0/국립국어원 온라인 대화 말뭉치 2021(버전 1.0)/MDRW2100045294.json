      "utterance": [
        {
          "id": "MDRW2100045294.1.1",
          "form": "지금 드시고 싶으신거 잇으세용",
          "original_form": "지금 드시고 싶으신거 잇으세용",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045294.1.2",
          "form": "전 디저트 먹고싶어요",
          "original_form": "전 디저트 먹고싶어요",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045294.1.3",
          "form": "우아 어쩐 디저트용?",
          "original_form": "우아 어쩐 디저트용?",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045294.1.4",
          "form": "스콘이요!",
          "original_form": "스콘이요!",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045294.1.5",
          "form": "지금 생각나는 음식 있으신가요?",
          "original_form": "지금 생각나는 음식 있으신가요?",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045294.1.6",
          "form": "우아 스콘 ㅜㅜ",
          "original_form": "우아 스콘 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045294.1.7",
          "form": "잼이랑 먹고 싶네요 ㅋㅋ저는",
          "original_form": "잼이랑 먹고 싶네요 ㅋㅋ저는",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045294.1.8",
          "form": "라면이용",
          "original_form": "라면이용",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045294.1.9",
          "form": "아 ㅋㅋㅋㅋ 밤에 어울리는 음식이네요",
          "original_form": "아 ㅋㅋㅋㅋ 밤에 어울리는 음식이네요",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045294.1.10",
          "form": "라면 자주 드세요?",
          "original_form": "라면 자주 드세요?",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045294.1.11",
          "form": "네 ㅋㅋㅋㅋ 그냥 무난하게 한끼 때우기 좋은거같아요",
          "original_form": "네 ㅋㅋㅋㅋ 그냥 무난하게 한끼 때우기 좋은거같아요",
          "speaker_id": "2",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045294.1.12",
          "form": "저는 물을 못맞춰서 항상 실패해요 ㅋㅋㅋ",
          "original_form": "저는 물을 못맞춰서 항상 실패해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045294.1.13",
          "form": "아 ㅋㅋㅋㅋ 그쵸 라면물 은근 어려워요",
          "original_form": "아 ㅋㅋㅋㅋ 그쵸 라면물 은근 어려워요",
          "speaker_id": "2",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045294.1.14",
          "form": "전 아예 전용냄비를 샀어요 ㅋㅋㅋ",
          "original_form": "전 아예 전용냄비를 샀어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045294.1.15",
          "form": "와 대박 ㅋㅋㅋㅋㅋ 물 맞춰주는 냄비인가요?",
          "original_form": "와 대박 ㅋㅋㅋㅋㅋ 물 맞춰주는 냄비인가요?",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045294.1.16",
          "form": "안에 눈금선이 잇어서",
          "original_form": "안에 눈금선이 잇어서",
          "speaker_id": "2",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045294.1.17",
          "form": "그만큼만 물 넣으면 돼요 ㅋㅋㅋㅋ",
          "original_form": "그만큼만 물 넣으면 돼요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045294.1.18",
          "form": "우아 완전 꿀팁이다 ㅠㅜ",
          "original_form": "우아 완전 꿀팁이다 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210908 01:50"
        }
      ]
