      "utterance": [
        {
          "id": "MDRW2100010054.1.1",
          "form": "저는 채식을 좋아합니다.",
          "original_form": "저는 채식을 좋아합니다.",
          "speaker_id": "1",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100010054.1.2",
          "form": "저는 고기에 파김치",
          "original_form": "저는 고기에 파김치",
          "speaker_id": "2",
          "time": "20210823 13:45"
        },
        {
          "id": "MDRW2100010054.1.3",
          "form": "근데 교촌치킨두 먹고싶기도해요",
          "original_form": "근데 교촌치킨두 먹고싶기도해요",
          "speaker_id": "2",
          "time": "20210823 13:45"
        },
        {
          "id": "MDRW2100010054.1.4",
          "form": "교촌치킨이 맛있긴 한데",
          "original_form": "교촌치킨이 맛있긴 한데",
          "speaker_id": "1",
          "time": "20210823 13:51"
        },
        {
          "id": "MDRW2100010054.1.5",
          "form": "비싸지 않나요",
          "original_form": "비싸지 않나요",
          "speaker_id": "1",
          "time": "20210823 13:51"
        },
        {
          "id": "MDRW2100010054.1.6",
          "form": "그렇겠죠",
          "original_form": "그렇겠죠",
          "speaker_id": "2",
          "time": "20210823 13:53"
        },
        {
          "id": "MDRW2100010054.1.7",
          "form": "근데 오늘은 치즈빵과 옥수수먹었어요",
          "original_form": "근데 오늘은 치즈빵과 옥수수먹었어요",
          "speaker_id": "2",
          "time": "20210823 13:53"
        },
        {
          "id": "MDRW2100010054.1.8",
          "form": "치즈가 속에 들어 있는 빵인가요?",
          "original_form": "치즈가 속에 들어 있는 빵인가요?",
          "speaker_id": "1",
          "time": "20210823 13:54"
        },
        {
          "id": "MDRW2100010054.1.9",
          "form": "그렇다는데",
          "original_form": "그렇다는데",
          "speaker_id": "2",
          "time": "20210823 13:54"
        },
        {
          "id": "MDRW2100010054.1.10",
          "form": "딱히",
          "original_form": "딱히",
          "speaker_id": "2",
          "time": "20210823 13:54"
        },
        {
          "id": "MDRW2100010054.1.11",
          "form": "요즘엔 베이커리 카페가 많더라고요",
          "original_form": "요즘엔 베이커리 카페가 많더라고요",
          "speaker_id": "1",
          "time": "20210823 13:57"
        },
        {
          "id": "MDRW2100010054.1.12",
          "form": "식물원카페도가보거싶어",
          "original_form": "식물원카페도가보거싶어",
          "speaker_id": "2",
          "time": "20210823 14:01"
        },
        {
          "id": "MDRW2100010054.1.13",
          "form": "사람이 많아서 위험하지 않을까요",
          "original_form": "사람이 많아서 위험하지 않을까요",
          "speaker_id": "1",
          "time": "20210823 14:03"
        },
        {
          "id": "MDRW2100010054.1.14",
          "form": "그렇죠 언젠가 가봐야죠",
          "original_form": "그렇죠 언젠가 가봐야죠",
          "speaker_id": "2",
          "time": "20210823 14:15"
        },
        {
          "id": "MDRW2100010054.1.15",
          "form": "어제 도미구이는 맛이 있었어요",
          "original_form": "어제 도미구이는 맛이 있었어요",
          "speaker_id": "2",
          "time": "20210824 09:17"
        },
        {
          "id": "MDRW2100010054.1.16",
          "form": "버터를 넣어볼 걸 그랬어요",
          "original_form": "버터를 넣어볼 걸 그랬어요",
          "speaker_id": "1",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100010054.1.17",
          "form": "그럼 느끼했을겨",
          "original_form": "그럼 느끼했을겨",
          "speaker_id": "2",
          "time": "20210824 12:18"
        },
        {
          "id": "MDRW2100010054.1.18",
          "form": "오늘 저녁은 기본찬을 먹을지 배달할지 고민이넹",
          "original_form": "오늘 저녁은 기본찬을 먹을지 배달할지 고민이넹",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100010054.1.19",
          "form": "오늘은",
          "original_form": "오늘은",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100010054.1.20",
          "form": "고구마줄기..?",
          "original_form": "고구마줄기..?",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100010054.1.21",
          "form": "하긴 나 점심 라면먹었어",
          "original_form": "하긴 나 점심 라면먹었어",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100010054.1.22",
          "form": "아 컵라면이요?",
          "original_form": "아 컵라면이요?",
          "speaker_id": "1",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100010054.1.23",
          "form": "만두 어떻게 해 ㅠㅠ",
          "original_form": "만두 어떻게 해 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 16:26"
        }
      ]
