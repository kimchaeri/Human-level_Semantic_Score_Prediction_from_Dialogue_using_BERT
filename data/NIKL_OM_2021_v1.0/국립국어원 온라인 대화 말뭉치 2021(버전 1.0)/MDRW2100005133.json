      "utterance": [
        {
          "id": "MDRW2100005133.1.1",
          "form": "간식 많이 주세요?",
          "original_form": "간식 많이 주세요?",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005133.1.2",
          "form": "앗 무슨말하셨죠 못봤어요 ㅠ 우셨다고 한것만 봤는데",
          "original_form": "앗 무슨말하셨죠 못봤어요 ㅠ 우셨다고 한것만 봤는데",
          "speaker_id": "2",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005133.1.3",
          "form": "애가 아파서요 ㅜㅜ",
          "original_form": "애가 아파서요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005133.1.4",
          "form": "간식 하루에 한번씩 주다가",
          "original_form": "간식 하루에 한번씩 주다가",
          "speaker_id": "2",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005133.1.5",
          "form": "도마뱀이 아파서 울었어요",
          "original_form": "도마뱀이 아파서 울었어요",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005133.1.6",
          "form": "간수치가 많이 올라가서",
          "original_form": "간수치가 많이 올라가서",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005133.1.7",
          "form": "저희애들 식성이 틀려요 ㅡㅡ",
          "original_form": "저희애들 식성이 틀려요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005133.1.8",
          "form": "자제해서 주고있어요ㅠㅠ",
          "original_form": "자제해서 주고있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005133.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005133.1.10",
          "form": "아무래도 동물이 아프면 말이안통해서 ㅠ",
          "original_form": "아무래도 동물이 아프면 말이안통해서 ㅠ",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005133.1.11",
          "form": "눈물나는거같아요 걱정되고",
          "original_form": "눈물나는거같아요 걱정되고",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005133.1.12",
          "form": "저희애도  단백질가루만 먹는애가 있어서 ㅜㅜ걱정이예요",
          "original_form": "저희애도  단백질가루만 먹는애가 있어서 ㅜㅜ걱정이예요",
          "speaker_id": "1",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005133.1.13",
          "form": "맞아요 뭔지 모르고 어찌할지도 모르고요",
          "original_form": "맞아요 뭔지 모르고 어찌할지도 모르고요",
          "speaker_id": "1",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005133.1.14",
          "form": "name3는 단백질가루",
          "original_form": "&name3&는 단백질가루",
          "speaker_id": "1",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005133.1.15",
          "form": "name4는 밀웜 먹어요",
          "original_form": "&name4&는 밀웜 먹어요",
          "speaker_id": "1",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005133.1.16",
          "form": "저밀웜도 키우는데 벌레가 아기밀웜 낳고 그래요",
          "original_form": "저밀웜도 키우는데 벌레가 아기밀웜 낳고 그래요",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005133.1.17",
          "form": "어쩌다 곤충농장도 됐네요",
          "original_form": "어쩌다 곤충농장도 됐네요",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005133.1.18",
          "form": "아 ㅋㅋㅋㅋㅋ 아기밀웜이라니",
          "original_form": "아 ㅋㅋㅋㅋㅋ 아기밀웜이라니",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005133.1.19",
          "form": "완전 작아요 ㅋㅋ",
          "original_form": "완전 작아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005133.1.20",
          "form": "곤충 별로 무섭진않으세요??",
          "original_form": "곤충 별로 무섭진않으세요??",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005133.1.21",
          "form": "귀여워요",
          "original_form": "귀여워요",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005133.1.22",
          "form": "무서웠죠 어쩌다 이리 됐어요",
          "original_form": "무서웠죠 어쩌다 이리 됐어요",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005133.1.23",
          "form": "아 사람은 적응의 동물이네요 정말 ㅋㅋㅋㅋ",
          "original_form": "아 사람은 적응의 동물이네요 정말 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005133.1.24",
          "form": "네 ㅡㅡ만지지도 못했어요",
          "original_form": "네 ㅡㅡ만지지도 못했어요",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005133.1.25",
          "form": "저는 그거땜에 파충류 못 키울거같아요 ㅜㅜ",
          "original_form": "저는 그거땜에 파충류 못 키울거같아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005133.1.26",
          "form": "좋아는하는데ㅜㅜ",
          "original_form": "좋아는하는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005133.1.27",
          "form": "네 그럼 못키워요",
          "original_form": "네 그럼 못키워요",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005133.1.28",
          "form": "사료로 길들여도 돼요",
          "original_form": "사료로 길들여도 돼요",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005133.1.29",
          "form": "저희 name3는 안멋어요",
          "original_form": "저희 &name3&는 안멋어요",
          "speaker_id": "1",
          "time": "20210821 20:51"
        }
      ]
