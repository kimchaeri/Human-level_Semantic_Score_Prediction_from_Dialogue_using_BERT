      "utterance": [
        {
          "id": "MDRW2100004990.1.1",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "1",
          "time": "20210821 19:47"
        },
        {
          "id": "MDRW2100004990.1.2",
          "form": "지금 먹고 있어요",
          "original_form": "지금 먹고 있어요",
          "speaker_id": "2",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100004990.1.3",
          "form": "애들먹이는게 재일 힘들어요ㅜ",
          "original_form": "애들먹이는게 재일 힘들어요ㅜ",
          "speaker_id": "2",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100004990.1.4",
          "form": "몇살부터 혼자 안흘리고 잘 먹을까요??",
          "original_form": "몇살부터 혼자 안흘리고 잘 먹을까요??",
          "speaker_id": "2",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100004990.1.5",
          "form": "ㅠㅠ 맞죠.\r\n안흘리고 혼자 잘먹는건.. 한 6살인가.\r\n 오래되서 기억도 없네요",
          "original_form": "ㅠㅠ 맞죠.\r\n안흘리고 혼자 잘먹는건.. 한 6살인가.\r\n 오래되서 기억도 없네요",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100004990.1.6",
          "form": "아직도 3.4년이나 남았네요 ㅜ",
          "original_form": "아직도 3.4년이나 남았네요 ㅜ",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100004990.1.7",
          "form": "집안일이고 육아고 끝이 없네요 ㅜ",
          "original_form": "집안일이고 육아고 끝이 없네요 ㅜ",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100004990.1.8",
          "form": "너무 힘들어요",
          "original_form": "너무 힘들어요",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100004990.1.9",
          "form": "해도 티도 안나고 안하면 더 티나고",
          "original_form": "해도 티도 안나고 안하면 더 티나고",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100004990.1.10",
          "form": "맞아여ㅋㅋ헬입니다ㅜ",
          "original_form": "맞아여ㅋㅋ헬입니다ㅜ",
          "speaker_id": "1",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100004990.1.11",
          "form": "이래서 육아키우는게 어려운가봐요",
          "original_form": "이래서 육아키우는게 어려운가봐요",
          "speaker_id": "2",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100004990.1.12",
          "form": "차라리 돈벌어 오는게 난거같아요",
          "original_form": "차라리 돈벌어 오는게 난거같아요",
          "speaker_id": "2",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100004990.1.13",
          "form": "네ㅡ 쉽지않죠ㅋㅋ일할래 애볼래하면 일한다고 한다 잖아요ㅋㅋ저도요",
          "original_form": "네ㅡ 쉽지않죠ㅋㅋ일할래 애볼래하면 일한다고 한다 잖아요ㅋㅋ저도요",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100004990.1.14",
          "form": "그러고보면  일과 육아 집안살림 다 하는 친구들 보면 진짜 대단하더라고요 ㅎ",
          "original_form": "그러고보면  일과 육아 집안살림 다 하는 친구들 보면 진짜 대단하더라고요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100004990.1.15",
          "form": "전지금 육아 살림도 죽을거 같은데 돈까지 버는 친구보면 진짜 놀라워요",
          "original_form": "전지금 육아 살림도 죽을거 같은데 돈까지 버는 친구보면 진짜 놀라워요",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100004990.1.16",
          "form": "진짜 맞죠 내주변에도 있거든요?",
          "original_form": "진짜 맞죠 내주변에도 있거든요?",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100004990.1.17",
          "form": "그친구보면 뭔가 대단하면서",
          "original_form": "그친구보면 뭔가 대단하면서",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100004990.1.18",
          "form": "안쓰러우면서",
          "original_form": "안쓰러우면서",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100004990.1.19",
          "form": "세가지 병행해도 쉴때 다 쉬고 일자리만 잘 찾으면 할만 한가바요 ㅎ",
          "original_form": "세가지 병행해도 쉴때 다 쉬고 일자리만 잘 찾으면 할만 한가바요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100004990.1.20",
          "form": "아 하는 일이 어떠냐에 따라 조금 다르긴하지먀",
          "original_form": "아 하는 일이 어떠냐에 따라 조금 다르긴하지먀",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100004990.1.21",
          "form": "만",
          "original_form": "만",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100004990.1.22",
          "form": "그래도 대단한것같아요",
          "original_form": "그래도 대단한것같아요",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100004990.1.23",
          "form": "맞아요. 저도 볼때마다  대단하다고 생각해요",
          "original_form": "맞아요. 저도 볼때마다  대단하다고 생각해요",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100004990.1.24",
          "form": "생활력이 참 대단한거 같아요",
          "original_form": "생활력이 참 대단한거 같아요",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100004990.1.25",
          "form": "전 집안일도 지쳐서리",
          "original_form": "전 집안일도 지쳐서리",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100004990.1.26",
          "form": "저도요.\r\n유지하늣것만도 용타 생각해요",
          "original_form": "저도요.\r\n유지하늣것만도 용타 생각해요",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100004990.1.27",
          "form": "암요암요.",
          "original_form": "암요암요.",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100004990.1.28",
          "form": "애보는게 재일 돈버는거래요",
          "original_form": "애보는게 재일 돈버는거래요",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100004990.1.29",
          "form": "육아가 그만큼 힘들단거겟죠",
          "original_form": "육아가 그만큼 힘들단거겟죠",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100004990.1.30",
          "form": "마자여 ㅋ 정답도 없고ㅠ",
          "original_form": "마자여 ㅋ 정답도 없고ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:57"
        }
      ]
