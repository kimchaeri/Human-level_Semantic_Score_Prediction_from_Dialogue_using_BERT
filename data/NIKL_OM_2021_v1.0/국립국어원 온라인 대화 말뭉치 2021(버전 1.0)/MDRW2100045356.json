      "utterance": [
        {
          "id": "MDRW2100045356.1.1",
          "form": "응응?",
          "original_form": "응응?",
          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045356.1.2",
          "form": "그앞에 지나가기만해도",
          "original_form": "그앞에 지나가기만해도",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045356.1.3",
          "form": "냄새대박이었는데",
          "original_form": "냄새대박이었는데",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045356.1.4",
          "form": "거기 안지나다님?",
          "original_form": "거기 안지나다님?",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045356.1.5",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045356.1.6",
          "form": "생각해보니까ㅋㅋ 나거의 봉고타고 다니고",
          "original_form": "생각해보니까ㅋㅋ 나거의 봉고타고 다니고",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045356.1.7",
          "form": "교회집사님들이 얼마나 자주 다녔다고 거기ㅋㅋㅋㅋ",
          "original_form": "교회집사님들이 얼마나 자주 다녔다고 거기ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045356.1.8",
          "form": "고등학교 초반까지도 있던거같은데",
          "original_form": "고등학교 초반까지도 있던거같은데",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045356.1.9",
          "form": "전혀몰라ㅋㅋㅋ기억안나ㅜㅜ",
          "original_form": "전혀몰라ㅋㅋㅋ기억안나ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045356.1.10",
          "form": "너 고딩때도 봉고탐? ㅋㅋㅋ",
          "original_form": "너 고딩때도 봉고탐? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045356.1.11",
          "form": "그래서 로티보이를 모르는거야?",
          "original_form": "그래서 로티보이를 모르는거야?",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045356.1.12",
          "form": "그 맛있는 번을 모르다니 진짜 통탄할일이다",
          "original_form": "그 맛있는 번을 모르다니 진짜 통탄할일이다",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045356.1.13",
          "form": "ㅇㅇ고딩때도 봉고탐ㅋㅋㅋ",
          "original_form": "ㅇㅇ고딩때도 봉고탐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045356.1.14",
          "form": "진짜슬프다 니가 그걸 안먹었다니",
          "original_form": "진짜슬프다 니가 그걸 안먹었다니",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045356.1.15",
          "form": "너가 아는 커피번은 무슨맛이니",
          "original_form": "너가 아는 커피번은 무슨맛이니",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045356.1.16",
          "form": "ㅜㅜ나도덩달아슬프다ㅜㅜ",
          "original_form": "ㅜㅜ나도덩달아슬프다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045356.1.17",
          "form": "나는 파바나 뚜레쥬르지ㅜㅜㅜ",
          "original_form": "나는 파바나 뚜레쥬르지ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045356.1.18",
          "form": "진짜 거기꺼의 열배는맛있어",
          "original_form": "진짜 거기꺼의 열배는맛있어",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045356.1.19",
          "form": "진심이야",
          "original_form": "진심이야",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045356.1.20",
          "form": "안에 생크림따위없다구",
          "original_form": "안에 생크림따위없다구",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045356.1.21",
          "form": "헝 슬프다ㅜㅜㅜㅜㅜ",
          "original_form": "헝 슬프다ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045356.1.22",
          "form": "생크림???",
          "original_form": "생크림???",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045356.1.23",
          "form": "요즘은 번에 생크림넣는곳들이있던데",
          "original_form": "요즘은 번에 생크림넣는곳들이있던데",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045356.1.24",
          "form": "헝 난싫어ㅜㅜ생크림",
          "original_form": "헝 난싫어ㅜㅜ생크림",
          "speaker_id": "2",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100045356.1.25",
          "form": "근데 베이비슈는 좋이",
          "original_form": "근데 베이비슈는 좋이",
          "speaker_id": "2",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045356.1.26",
          "form": "나도싫어 ㅋㅋㅋㅋㅋ",
          "original_form": "나도싫어 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045356.1.27",
          "form": "로티보이는 안에 버터야",
          "original_form": "로티보이는 안에 버터야",
          "speaker_id": "1",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045356.1.28",
          "form": "내일 너 만나고 빵집도 갈거야",
          "original_form": "내일 너 만나고 빵집도 갈거야",
          "speaker_id": "2",
          "time": "20210908 02:09"
        }
      ]
    }
