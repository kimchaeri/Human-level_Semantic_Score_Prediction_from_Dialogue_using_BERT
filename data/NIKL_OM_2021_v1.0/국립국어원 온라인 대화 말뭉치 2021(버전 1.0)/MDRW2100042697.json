        },
        {
          "id": "MDRW2100042697.1.2",
          "form": "요즘 무슨 드라마 봐해",
          "original_form": "요즘 무슨 드라마 봐해",
          "speaker_id": "1",
          "time": "20210907 01:42"
        },
        {
          "id": "MDRW2100042697.1.3",
          "form": "요즘 갯마을 차차차 보고 있어",
          "original_form": "요즘 갯마을 차차차 보고 있어",
          "speaker_id": "2",
          "time": "20210907 01:42"
        },
        {
          "id": "MDRW2100042697.1.4",
          "form": "누구가 나오니",
          "original_form": "누구가 나오니",
          "speaker_id": "1",
          "time": "20210907 01:43"
        },
        {
          "id": "MDRW2100042697.1.5",
          "form": "신민아 김선호",
          "original_form": "신민아 김선호",
          "speaker_id": "2",
          "time": "20210907 01:43"
        },
        {
          "id": "MDRW2100042697.1.6",
          "form": "이쁘고 잘생겼겠다",
          "original_form": "이쁘고 잘생겼겠다",
          "speaker_id": "1",
          "time": "20210907 01:43"
        },
        {
          "id": "MDRW2100042697.1.7",
          "form": "아자 비주얼 좋아",
          "original_form": "아자 비주얼 좋아",
          "speaker_id": "2",
          "time": "20210907 01:43"
        },
        {
          "id": "MDRW2100042697.1.8",
          "form": "둘 다 보조개 있지",
          "original_form": "둘 다 보조개 있지",
          "speaker_id": "1",
          "time": "20210907 01:44"
        },
        {
          "id": "MDRW2100042697.1.9",
          "form": "아자 비주얼 너무 좋아",
          "original_form": "아자 비주얼 너무 좋아",
          "speaker_id": "2",
          "time": "20210907 01:44"
        },
        {
          "id": "MDRW2100042697.1.10",
          "form": "여주인공이 치과의사 이던데",
          "original_form": "여주인공이 치과의사 이던데",
          "speaker_id": "1",
          "time": "20210907 01:44"
        },
        {
          "id": "MDRW2100042697.1.11",
          "form": "김선호는 별명이 홍반장이야",
          "original_form": "김선호는 별명이 홍반장이야",
          "speaker_id": "2",
          "time": "20210907 01:45"
        },
        {
          "id": "MDRW2100042697.1.12",
          "form": "직업이 뭐야",
          "original_form": "직업이 뭐야",
          "speaker_id": "1",
          "time": "20210907 01:45"
        },
        {
          "id": "MDRW2100042697.1.13",
          "form": "나도 궁금해 직업이 엄청 많아",
          "original_form": "나도 궁금해 직업이 엄청 많아",
          "speaker_id": "2",
          "time": "20210907 01:45"
        },
        {
          "id": "MDRW2100042697.1.14",
          "form": "능력자 네",
          "original_form": "능력자 네",
          "speaker_id": "1",
          "time": "20210907 01:46"
        },
        {
          "id": "MDRW2100042697.1.15",
          "form": "삼화 부터 엄청 재밌어",
          "original_form": "삼화 부터 엄청 재밌어",
