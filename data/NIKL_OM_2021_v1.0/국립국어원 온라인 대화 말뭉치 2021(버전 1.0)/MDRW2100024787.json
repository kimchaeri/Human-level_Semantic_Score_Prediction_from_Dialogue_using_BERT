      "utterance": [
        {
          "id": "MDRW2100024787.1.1",
          "form": "애기가 몇살이에요?",
          "original_form": "애기가 몇살이에요?",
          "speaker_id": "1",
          "time": "20210831 20:17"
        },
        {
          "id": "MDRW2100024787.1.2",
          "form": "이제 세돌 지났어요",
          "original_form": "이제 세돌 지났어요",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100024787.1.3",
          "form": "그러면 말도 잘하고 잘 뛰어다니겠네요",
          "original_form": "그러면 말도 잘하고 잘 뛰어다니겠네요",
          "speaker_id": "1",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100024787.1.4",
          "form": "귀엽겠어요",
          "original_form": "귀엽겠어요",
          "speaker_id": "1",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100024787.1.5",
          "form": "한창 고집부릴때죠 ㅎㅎ",
          "original_form": "한창 고집부릴때죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100024787.1.6",
          "form": "유치원 다니나요?",
          "original_form": "유치원 다니나요?",
          "speaker_id": "1",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100024787.1.7",
          "form": "코로나로 안보낸지 두달이 넘어가요ㅜ",
          "original_form": "코로나로 안보낸지 두달이 넘어가요ㅜ",
          "speaker_id": "2",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100024787.1.8",
          "form": "아 유치원을 아예 못보내시는군요... 아이구 힘드시겠어요",
          "original_form": "아 유치원을 아예 못보내시는군요... 아이구 힘드시겠어요",
          "speaker_id": "1",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100024787.1.9",
          "form": "네 ㅎㅎ 긴급보육도 있는데 일단은 조심하고있어요",
          "original_form": "네 ㅎㅎ 긴급보육도 있는데 일단은 조심하고있어요",
          "speaker_id": "2",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100024787.1.10",
          "form": "육아는 name2님이 주로 하세요?",
          "original_form": "육아는 &name2&님이 주로 하세요?",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100024787.1.11",
          "form": "아니요 친정어머니가 고생하시죠",
          "original_form": "아니요 친정어머니가 고생하시죠",
          "speaker_id": "2",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100024787.1.12",
          "form": "그렇군요 친정어머니께서 젊으신가봐요?",
          "original_form": "그렇군요 친정어머니께서 젊으신가봐요?",
          "speaker_id": "1",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100024787.1.13",
          "form": "이제 환갑 지나셨어요 항상 죄송하죠",
          "original_form": "이제 환갑 지나셨어요 항상 죄송하죠",
          "speaker_id": "2",
          "time": "20210905 18:27"
        },
        {
          "id": "MDRW2100024787.1.14",
          "form": "아직도 육아는 여자들의 몫인가봐요",
          "original_form": "아직도 육아는 여자들의 몫인가봐요",
          "speaker_id": "1",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100024787.1.15",
          "form": "그쵸 아직은 그래요  초등학교 들어갈때쯤엔 저도 육아에 올인하겠죠",
