      "utterance": [
        {
          "id": "MDRW2100033252.1.1",
          "form": "나 그거 사고 싶어 후드집업",
          "original_form": "나 그거 사고 싶어 후드집업",
          "speaker_id": "2",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100033252.1.2",
          "form": "후드집업이 왜 없지",
          "original_form": "후드집업이 왜 없지",
          "speaker_id": "2",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100033252.1.3",
          "form": "너 이번에 지원금 받아?",
          "original_form": "너 이번에 지원금 받아?",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100033252.1.4",
          "form": "그걸로 옷 사자",
          "original_form": "그걸로 옷 사자",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100033252.1.5",
          "form": "두근두근",
          "original_form": "두근두근",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100033252.1.6",
          "form": "난 가을에 입을 옷이 없어서 나도 옷이나 살까 생각 중이야",
          "original_form": "난 가을에 입을 옷이 없어서 나도 옷이나 살까 생각 중이야",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100033252.1.7",
          "form": "헐 맞다 지원금이 있구나",
          "original_form": "헐 맞다 지원금이 있구나",
          "speaker_id": "2",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100033252.1.8",
          "form": "나 내일 신청이야",
          "original_form": "나 내일 신청이야",
          "speaker_id": "2",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100033252.1.9",
          "form": "나는 최근에... 전공책을 샀어^^",
          "original_form": "나는 최근에... 전공책을 샀어^^",
          "speaker_id": "1",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100033252.1.10",
          "form": "나는 오늘 교제를 사지 않아도 된다고 교수님이 말씀하셨어",
          "original_form": "나는 오늘 교제를 사지 않아도 된다고 교수님이 말씀하셨어",
          "speaker_id": "2",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100033252.1.11",
          "form": "우히힉",
          "original_form": "우히힉",
          "speaker_id": "2",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100033252.1.12",
          "form": "교제 아니고 교재 아님?",
          "original_form": "교제 아니고 교재 아님?",
          "speaker_id": "1",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100033252.1.13",
          "form": "오타임",
          "original_form": "오타임",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100033252.1.14",
          "form": "열밧네",
          "original_form": "열밧네",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100033252.1.15",
          "form": "아랏어.",
          "original_form": "아랏어.",
          "speaker_id": "1",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100033252.1.16",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100033252.1.17",
          "form": "난 세 권이나 샀어...",
          "original_form": "난 세 권이나 샀어...",
          "speaker_id": "1",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100033252.1.18",
          "form": "마음이 아파 그래서",
          "original_form": "마음이 아파 그래서",
          "speaker_id": "1",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100033252.1.19",
          "form": "저런...",
          "original_form": "저런...",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100033252.1.20",
          "form": "나는 문구류 사고 싶어",
          "original_form": "나는 문구류 사고 싶어",
          "speaker_id": "1",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100033252.1.21",
          "form": "근데 나도 살듯",
          "original_form": "근데 나도 살듯",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100033252.1.22",
          "form": "오늘부터 시작이라",
          "original_form": "오늘부터 시작이라",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100033252.1.23",
          "form": "싫다 진짜",
          "original_form": "싫다 진짜",
          "speaker_id": "1",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100033252.1.24",
          "form": "문구류 뭐 사게",
          "original_form": "문구류 뭐 사게",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100033252.1.25",
          "form": "다이어리는 내년에 사고 싶어",
          "original_form": "다이어리는 내년에 사고 싶어",
          "speaker_id": "1",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100033252.1.26",
          "form": "지금 사면... 그럴 수도 있지",
          "original_form": "지금 사면... 그럴 수도 있지",
          "speaker_id": "2",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100033252.1.27",
          "form": "플래너 다른 디자인으로 된 거 갖고싶어",
          "original_form": "플래너 다른 디자인으로 된 거 갖고싶어",
          "speaker_id": "1",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100033252.1.28",
          "form": "오올",
          "original_form": "오올",
          "speaker_id": "2",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100033252.1.29",
          "form": "그리고... 그리고 독서대...",
          "original_form": "그리고... 그리고 독서대...",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100033252.1.30",
          "form": "왜냐하면 공부할 때 쓰면 뭔가",
          "original_form": "왜냐하면 공부할 때 쓰면 뭔가",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100033252.1.31",
          "form": "멋져",
          "original_form": "멋져",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100033252.1.32",
          "form": "아니 나 독서대 어릴 때 썼었거든",
          "original_form": "아니 나 독서대 어릴 때 썼었거든",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100033252.1.33",
          "form": "편하긴 하더라",
          "original_form": "편하긴 하더라",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100033252.1.34",
          "form": "지금은 왜 안 써요?",
          "original_form": "지금은 왜 안 써요?",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100033252.1.35",
          "form": "지금은",
          "original_form": "지금은",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100033252.1.36",
          "form": "내가 너무 컸어",
          "original_form": "내가 너무 컸어",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100033252.1.37",
          "form": "어어... 그래",
          "original_form": "어어... 그래",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100033252.1.38",
          "form": "음하하하하하",
          "original_form": "음하하하하하",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100033252.1.39",
          "form": "나 요즘 중고거래로 산 닌텐도 하잖아",
          "original_form": "나 요즘 중고거래로 산 닌텐도 하잖아",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100033252.1.40",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100033252.1.41",
          "form": "그거 하느라 공부를 안 해",
          "original_form": "그거 하느라 공부를 안 해",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100033252.1.42",
          "form": "아무튼 재밌다는 소리야.",
          "original_form": "아무튼 재밌다는 소리야.",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100033252.1.43",
          "form": "재밌지",
          "original_form": "재밌지",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100033252.1.44",
          "form": "포켓몬은 하니",
          "original_form": "포켓몬은 하니",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100033252.1.45",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100033252.1.46",
          "form": "얼마나 했니",
          "original_form": "얼마나 했니",
          "speaker_id": "2",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100033252.1.47",
          "form": "모 몰라... 기억이 안 나",
          "original_form": "모 몰라... 기억이 안 나",
          "speaker_id": "1",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100033252.1.48",
          "form": "빨리 포켓몬 마스터가 돼야 해",
          "original_form": "빨리 포켓몬 마스터가 돼야 해",
          "speaker_id": "2",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100033252.1.49",
          "form": "시른디",
          "original_form": "시른디",
          "speaker_id": "1",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100033252.1.50",
          "form": "포켓몬 마스터가 되어야",
          "original_form": "포켓몬 마스터가 되어야",
          "speaker_id": "2",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100033252.1.51",
          "form": "우리의 미래가 밝아져",
          "original_form": "우리의 미래가 밝아져",
          "speaker_id": "2",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100033252.1.52",
          "form": "이러네",
          "original_form": "이러네",
          "speaker_id": "1",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100033252.1.53",
          "form": "아무튼 중고거래는 참 좋아",
          "original_form": "아무튼 중고거래는 참 좋아",
          "speaker_id": "1",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100033252.1.54",
          "form": "오늘도 아까 말했던 중고거래 한 책 도착했어",
          "original_form": "오늘도 아까 말했던 중고거래 한 책 도착했어",
          "speaker_id": "1",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100033252.1.55",
          "form": "맞아 근데 사기도 많지",
          "original_form": "맞아 근데 사기도 많지",
          "speaker_id": "2",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100033252.1.56",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100033252.1.57",
          "form": "사실 거래할 때 조금 긴장했어",
          "original_form": "사실 거래할 때 조금 긴장했어",
          "speaker_id": "1",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100033252.1.58",
          "form": "아 대면이였어?",
          "original_form": "아 대면이였어?",
          "speaker_id": "2",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100033252.1.59",
          "form": "아니 택배야",
          "original_form": "아니 택배야",
          "speaker_id": "1",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100033252.1.60",
          "form": "멍미",
          "original_form": "멍미",
          "speaker_id": "2",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100033252.1.61",
          "form": "하지만 내 돈을 먼저 보냈기 때문에",
          "original_form": "하지만 내 돈을 먼저 보냈기 때문에",
          "speaker_id": "1",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100033252.1.62",
          "form": "약간 긴장했다고",
          "original_form": "약간 긴장했다고",
          "speaker_id": "1",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100033252.1.63",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100033252.1.64",
          "form": "벽돌 왔으면...",
          "original_form": "벽돌 왔으면...",
          "speaker_id": "2",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100033252.1.65",
          "form": "아니ㅋㅋㅋㅋㅋㅋ 같은 학교 학생이랑 거래한 거예요 ;",
          "original_form": "아니ㅋㅋㅋㅋㅋㅋ 같은 학교 학생이랑 거래한 거예요 ;",
          "speaker_id": "1",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100033252.1.66",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100033252.1.67",
          "form": "그리고 계좌번호 주고 이름 뜨니까 그래도 보낸 거지",
          "original_form": "그리고 계좌번호 주고 이름 뜨니까 그래도 보낸 거지",
          "speaker_id": "1",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100033252.1.68",
          "form": "그래 잘했다",
          "original_form": "그래 잘했다",
          "speaker_id": "2",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100033252.1.69",
          "form": "아 맞아 그리고 가지고 싶은 거 또 생각 났어",
          "original_form": "아 맞아 그리고 가지고 싶은 거 또 생각 났어",
          "speaker_id": "1",
          "time": "20210906 20:35"
        },
        {
