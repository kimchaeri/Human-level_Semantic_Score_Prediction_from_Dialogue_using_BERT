      "utterance": [
        {
          "id": "MDRW2100025433.1.1",
          "form": "오늘 점심은 뭘까요",
          "original_form": "오늘 점심은 뭘까요",
          "speaker_id": "1",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025433.1.2",
          "form": "곧점심시간이에요",
          "original_form": "곧점심시간이에요",
          "speaker_id": "2",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025433.1.3",
          "form": "저희는 병어랑 전어에여",
          "original_form": "저희는 병어랑 전어에여",
          "speaker_id": "2",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025433.1.4",
          "form": "아 별로네요",
          "original_form": "아 별로네요",
          "speaker_id": "1",
          "time": "20210901 11:16"
        },
        {
          "id": "MDRW2100025433.1.5",
          "form": "생선",
          "original_form": "생선",
          "speaker_id": "1",
          "time": "20210901 11:16"
        },
        {
          "id": "MDRW2100025433.1.6",
          "form": "오늘 좀 기대해봤는데",
          "original_form": "오늘 좀 기대해봤는데",
          "speaker_id": "1",
          "time": "20210901 11:16"
        },
        {
          "id": "MDRW2100025433.1.7",
          "form": "저희도 생선이네요",
          "original_form": "저희도 생선이네요",
          "speaker_id": "1",
          "time": "20210901 11:16"
        },
        {
          "id": "MDRW2100025433.1.8",
          "form": "그니깐요 저도생선안좋아하는데",
          "original_form": "그니깐요 저도생선안좋아하는데",
          "speaker_id": "2",
          "time": "20210901 11:18"
        },
        {
          "id": "MDRW2100025433.1.9",
          "form": "다른거먹을거없나찾아봐야겠어요",
          "original_form": "다른거먹을거없나찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025433.1.10",
          "form": "요즘 뭐맛있는 배달음식있나요ㅋㅋㅋ",
          "original_form": "요즘 뭐맛있는 배달음식있나요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025433.1.11",
          "form": "오늘은뭔가맛있는게시켜먹고싶네용",
          "original_form": "오늘은뭔가맛있는게시켜먹고싶네용",
          "speaker_id": "2",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025433.1.12",
          "form": "배달 좋죠",
          "original_form": "배달 좋죠",
          "speaker_id": "1",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025433.1.13",
          "form": "탕수육이 먹고싶네요",
          "original_form": "탕수육이 먹고싶네요",
          "speaker_id": "1",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025433.1.14",
          "form": "저희 회사는 구석에있어서 배달이 안돼요",
          "original_form": "저희 회사는 구석에있어서 배달이 안돼요",
          "speaker_id": "1",
          "time": "20210901 11:20"
        },
        {
          "id": "MDRW2100025433.1.15",
          "form": "ㅋㅋㅋ회사에서는못시켜먹고 집갈때시켜먹으려구요",
          "original_form": "ㅋㅋㅋ회사에서는못시켜먹고 집갈때시켜먹으려구요",
          "speaker_id": "2",
          "time": "20210901 11:22"
        },
        {
          "id": "MDRW2100025433.1.16",
          "form": "그래도 올만에 밥먹는거라 맛나게 먹었네요",
          "original_form": "그래도 올만에 밥먹는거라 맛나게 먹었네요",
          "speaker_id": "1",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100025433.1.17",
          "form": "저는오늘국이맛있더라구요고기국",
          "original_form": "저는오늘국이맛있더라구요고기국",
          "speaker_id": "2",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100025433.1.18",
          "form": "국에 밥먹었네요",
          "original_form": "국에 밥먹었네요",
          "speaker_id": "1",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100025433.1.19",
          "form": "맛나네요",
          "original_form": "맛나네요",
          "speaker_id": "1",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100025433.1.20",
          "form": "밥을먹으니 졸립네요",
          "original_form": "밥을먹으니 졸립네요",
          "speaker_id": "1",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100025433.1.21",
          "form": "이제 잠을좀자야겠어요",
          "original_form": "이제 잠을좀자야겠어요",
          "speaker_id": "2",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100025433.1.22",
          "form": "그래야겠어요",
          "original_form": "그래야겠어요",
          "speaker_id": "1",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100025433.1.23",
          "form": "오늘 집에가면 만두가 먹고싶지만",
          "original_form": "오늘 집에가면 만두가 먹고싶지만",
          "speaker_id": "1",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100025433.1.24",
          "form": "못먹겠지요",
          "original_form": "못먹겠지요",
          "speaker_id": "1",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100025433.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ왜요 집가서 만두 돌려먹으면대져",
          "original_form": "ㅋㅋㅋㅋㅋㅋ왜요 집가서 만두 돌려먹으면대져",
          "speaker_id": "2",
          "time": "20210901 12:53"
        },
        {
          "id": "MDRW2100025433.1.26",
          "form": "운동가요",
          "original_form": "운동가요",
          "speaker_id": "1",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025433.1.27",
          "form": "가기싫어요",
          "original_form": "가기싫어요",
          "speaker_id": "1",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025433.1.28",
          "form": "일찍 집가서 만두먹고싶어요",
          "original_form": "일찍 집가서 만두먹고싶어요",
          "speaker_id": "1",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025433.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 안돼요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 안돼요",
          "speaker_id": "2",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100025433.1.30",
          "form": "운동가야져",
          "original_form": "운동가야져",
          "speaker_id": "2",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025433.1.31",
          "form": "운동빡시게하고 만듀머구면대여ㅋㅋㅋ",
          "original_form": "운동빡시게하고 만듀머구면대여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025433.1.32",
          "form": "후..",
          "original_form": "후..",
          "speaker_id": "1",
          "time": "20210901 16:16"
        },
        {
          "id": "MDRW2100025433.1.33",
          "form": "그냥 맛난거 다 먹고싶어",
          "original_form": "그냥 맛난거 다 먹고싶어",
          "speaker_id": "1",
          "time": "20210901 16:16"
        },
        {
          "id": "MDRW2100025433.1.34",
          "form": "나는오늘초밥먹을꺼에요ㅋㅋㅋ",
          "original_form": "나는오늘초밥먹을꺼에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025433.1.35",
          "form": "좋겠네요",
          "original_form": "좋겠네요",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100025433.1.36",
          "form": "밤만쥬 갖고온거 잊고있었음",
          "original_form": "밤만쥬 갖고온거 잊고있었음",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100025433.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ밤만쥬맛있찌",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ밤만쥬맛있찌",
          "speaker_id": "2",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100025433.1.38",
          "form": "가는길에먹어여",
          "original_form": "가는길에먹어여",
          "speaker_id": "2",
          "time": "20210901 17:55"
        }
      ]
