      "utterance": [
        {
          "id": "MDRW2100030290.1.1",
          "form": "넌 싸이월드가 먼지 알아?",
          "original_form": "넌 싸이월드가 먼지 알아?",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030290.1.2",
          "form": "알지!! 카카오스토리나 인스타그램같은거잖아",
          "original_form": "알지!! 카카오스토리나 인스타그램같은거잖아",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030290.1.3",
          "form": "언니 싸이월드 해본적있어?",
          "original_form": "언니 싸이월드 해본적있어?",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030290.1.4",
          "form": "오 ㅋㅋ 나때두 옛날얘기엿는데",
          "original_form": "오 ㅋㅋ 나때두 옛날얘기엿는데",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030290.1.5",
          "form": "난 안해봣지",
          "original_form": "난 안해봣지",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030290.1.6",
          "form": "미니홈피 같은건 해봣어",
          "original_form": "미니홈피 같은건 해봣어",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030290.1.7",
          "form": "그렇구나 ㅋㅋ 아무튼 싸이월드가 부활한다는건 좋은거같아",
          "original_form": "그렇구나 ㅋㅋ 아무튼 싸이월드가 부활한다는건 좋은거같아",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030290.1.8",
          "form": "미니홈피는 나두 했어",
          "original_form": "미니홈피는 나두 했어",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030290.1.9",
          "form": "왜 그럿게생각하는데??",
          "original_form": "왜 그럿게생각하는데??",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030290.1.10",
          "form": "왜냐면 나이 많은사람들 추억을 다시",
          "original_form": "왜냐면 나이 많은사람들 추억을 다시",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030290.1.11",
          "form": "되살릴?수도잇구",
          "original_form": "되살릴?수도잇구",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030290.1.12",
          "form": "젊은 사람들두 싸이월드에 유입이 되면서",
          "original_form": "젊은 사람들두 싸이월드에 유입이 되면서",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030290.1.13",
          "form": "그렇지 ㅎㅎ 아예 없어지는것보단 낫다구 봐",
          "original_form": "그렇지 ㅎㅎ 아예 없어지는것보단 낫다구 봐",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030290.1.14",
          "form": "움.. 옛날감성만의 그",
          "original_form": "움.. 옛날감성만의 그",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030290.1.15",
          "form": "감성?",
          "original_form": "감성?",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030290.1.16",
          "form": "많이 유입됐으면 좋겟당 ㅠㅠ",
          "original_form": "많이 유입됐으면 좋겟당 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030290.1.17",
          "form": "옛날감성 좋잖어",
          "original_form": "옛날감성 좋잖어",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030290.1.18",
          "form": "맞아 없어지는건 슬펑 ㅜㅡㅜ",
          "original_form": "맞아 없어지는건 슬펑 ㅜㅡㅜ",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
