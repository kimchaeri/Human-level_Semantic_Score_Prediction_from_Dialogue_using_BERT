      "utterance": [
        {
          "id": "MDRW2100047554.1.1",
          "form": "나 오늘 택배로 캠핑용의자 받았다",
          "original_form": "나 오늘 택배로 캠핑용의자 받았다",
          "speaker_id": "2",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100047554.1.2",
          "form": "일부러 산거야?",
          "original_form": "일부러 산거야?",
          "speaker_id": "1",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100047554.1.3",
          "form": "저번에 포인트 소멸 때문에 스킨 한개 쌋는데 그게 당첨",
          "original_form": "저번에 포인트 소멸 때문에 스킨 한개 쌋는데 그게 당첨",
          "speaker_id": "2",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100047554.1.4",
          "form": "오 사은품이네...  좋겠다",
          "original_form": "오 사은품이네...  좋겠다",
          "speaker_id": "1",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100047554.1.5",
          "form": "런칭 이벤트로 당첨되면은 캠핑용의자 준다고 하더니 오늘 갑자기 택배가 와서 뭔가 했다",
          "original_form": "런칭 이벤트로 당첨되면은 캠핑용의자 준다고 하더니 오늘 갑자기 택배가 와서 뭔가 했다",
          "speaker_id": "2",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100047554.1.6",
          "form": "어떻는데? 방에서 사용할거야?",
          "original_form": "어떻는데? 방에서 사용할거야?",
          "speaker_id": "1",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100047554.1.7",
          "form": "비싼 캠핑용의자가 아니다 저렴이 버전",
          "original_form": "비싼 캠핑용의자가 아니다 저렴이 버전",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100047554.1.8",
          "form": "그럼 어따 쓰노?",
          "original_form": "그럼 어따 쓰노?",
          "speaker_id": "1",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100047554.1.9",
          "form": "당근에 팔려고 사진 찍어서 올렸음",
          "original_form": "당근에 팔려고 사진 찍어서 올렸음",
          "speaker_id": "2",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100047554.1.10",
          "form": "아 맞나 얼마에 올렸어?",
          "original_form": "아 맞나 얼마에 올렸어?",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100047554.1.11",
          "form": "새제품이라고 적어서 만원에 판매한다고 했는데 소식이 없당 ㅜ.ㅜ",
          "original_form": "새제품이라고 적어서 만원에 판매한다고 했는데 소식이 없당 ㅜ.ㅜ",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100047554.1.12",
          "form": "만원이면 싼데... 사용을 많이 안하니",
          "original_form": "만원이면 싼데... 사용을 많이 안하니",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100047554.1.13",
          "form": "엉 그래서 안사나보다 ㅋㅋㅋ",
          "original_form": "엉 그래서 안사나보다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:17"
        }
