      "utterance": [
        {
          "id": "MDRW2100015105.1.1",
          "form": "혹시 크로플이라고 아시나요? 디저트류에요",
          "original_form": "혹시 크로플이라고 아시나요? 디저트류에요",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015105.1.2",
          "form": "똑똑 계신가요",
          "original_form": "똑똑 계신가요",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015105.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015105.1.4",
          "form": "한번밖에안먹었지만잊을수없지요",
          "original_form": "한번밖에안먹었지만잊을수없지요",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015105.1.5",
          "form": "저도 참 맛있더라고요 크로플이",
          "original_form": "저도 참 맛있더라고요 크로플이",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015105.1.6",
          "form": "아이스크림올려진것도맛잇어요",
          "original_form": "아이스크림올려진것도맛잇어요",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015105.1.7",
          "form": "와플 비슷한건데 뭐가 다른거인지는 모르겠어요",
          "original_form": "와플 비슷한건데 뭐가 다른거인지는 모르겠어요",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015105.1.8",
          "form": "와플보다크로플",
          "original_form": "와플보다크로플",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015105.1.9",
          "form": "크로플은쫜득해요",
          "original_form": "크로플은쫜득해요",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015105.1.10",
          "form": "아 그런것같네요",
          "original_form": "아 그런것같네요",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015105.1.11",
          "form": "계피향이좋아요",
          "original_form": "계피향이좋아요",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015105.1.12",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015105.1.13",
          "form": "전 생크림올라간거먹었는데 진짜 맛있더라고요",
          "original_form": "전 생크림올라간거먹었는데 진짜 맛있더라고요",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015105.1.14",
          "form": "어디꺼드셨어요",
          "original_form": "어디꺼드셨어요",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015105.1.15",
          "form": "동성로에 이름은 잘 기억안나는데 민트로 도배된 카페였어요",
          "original_form": "동성로에 이름은 잘 기억안나는데 민트로 도배된 카페였어요",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015105.1.16",
          "form": "저는 메가커피요",
          "original_form": "저는 메가커피요",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015105.1.17",
          "form": "저렴하고맛잇어요",
          "original_form": "저렴하고맛잇어요",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015105.1.18",
          "form": "거기는 크로플하나에 얼마인가요,",
          "original_form": "거기는 크로플하나에 얼마인가요,",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015105.1.19",
          "form": "3500원이요아이스크림크로플",
          "original_form": "3500원이요아이스크림크로플",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015105.1.20",
          "form": "이야 정말 저렴하네요",
          "original_form": "이야 정말 저렴하네요",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015105.1.21",
          "form": "그냥은3000원이요",
          "original_form": "그냥은3000원이요",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015105.1.22",
          "form": "전 7000원주고 사먹었는데",
          "original_form": "전 7000원주고 사먹었는데",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015105.1.23",
          "form": "메가커피가저려미요",
          "original_form": "메가커피가저려미요",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015105.1.24",
          "form": "거기서 크로플두개사먹어야겠어요",
          "original_form": "거기서 크로플두개사먹어야겠어요",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015105.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015105.1.26",
          "form": "근데다른데는비쌀거예요",
          "original_form": "근데다른데는비쌀거예요",
          "speaker_id": "1",
          "time": "20210825 22:02"
        }
      ]
    }
