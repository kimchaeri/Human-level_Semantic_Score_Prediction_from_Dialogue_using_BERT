      "utterance": [
        {
          "id": "MDRW2100014588.1.1",
          "form": "쇼핑의 여왕님이신가요?? ㅋ",
          "original_form": "쇼핑의 여왕님이신가요?? ㅋ",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014588.1.2",
          "form": "매일매일 사지 않으세요?",
          "original_form": "매일매일 사지 않으세요?",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014588.1.3",
          "form": "전 매일 사지는 않는데 한번 사기 시작하면 계속 사요 ㅋㅋ",
          "original_form": "전 매일 사지는 않는데 한번 사기 시작하면 계속 사요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:07"
        },
        {
          "id": "MDRW2100014588.1.4",
          "form": "저도 스트레스 받을때 더 많이 사는 것 같아요",
          "original_form": "저도 스트레스 받을때 더 많이 사는 것 같아요",
          "speaker_id": "1",
          "time": "20210826 10:39"
        },
        {
          "id": "MDRW2100014588.1.5",
          "form": "신문물 뭐가 있나요?",
          "original_form": "신문물 뭐가 있나요?",
          "speaker_id": "2",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100014588.1.6",
          "form": "신문물 잘몰라여 이제 늙었어ㅠㅠㅋㅋ",
          "original_form": "신문물 잘몰라여 이제 늙었어ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100014588.1.7",
          "form": "원래 잘 아시지 않았나요?? ㅋ",
          "original_form": "원래 잘 아시지 않았나요?? ㅋ",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100014588.1.8",
          "form": "제가요...? 쇼핑녀긴 하지만 ㅋㅋㅋㅋㅋ",
          "original_form": "제가요...? 쇼핑녀긴 하지만 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100014588.1.9",
          "form": "70개 사신거 아니예요 반.찬.통.",
          "original_form": "70개 사신거 아니예요 반.찬.통.",
          "speaker_id": "2",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100014588.1.10",
          "form": "어머 들켰네.. 아까 정리하다 죽을맛... 제껀 16개에요",
          "original_form": "어머 들켰네.. 아까 정리하다 죽을맛... 제껀 16개에요",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100014588.1.11",
          "form": "17.5개였는데 누가 더 가져간거죠?",
          "original_form": "17.5개였는데 누가 더 가져간거죠?",
          "speaker_id": "2",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100014588.1.12",
          "form": "언제 나누기까지 ㅋㅋㅋ",
          "original_form": "언제 나누기까지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100014588.1.13",
          "form": "뒤끝있으시네요",
          "original_form": "뒤끝있으시네요",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100014588.1.14",
          "form": "ㅋㅋㅋ그 회사 언니 연락와선 직원이 실수로 수량제한 안해서 완전 깨졌다고 하더라구요",
          "original_form": "ㅋㅋㅋ그 회사 언니 연락와선 직원이 실수로 수량제한 안해서 완전 깨졌다고 하더라구요",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100014588.1.15",
          "form": "제가 못샀거든요 ㅋㅋㅋ",
          "original_form": "제가 못샀거든요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100014588.1.16",
          "form": "회사도 한사람이 70개 살줄은 몰랐겠죠",
          "original_form": "회사도 한사람이 70개 살줄은 몰랐겠죠",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100014588.1.17",
          "form": "플라스틱 안좋앙 안좋아",
          "original_form": "플라스틱 안좋앙 안좋아",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100014588.1.18",
          "form": "우유병 재질이라던데요",
          "original_form": "우유병 재질이라던데요",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100014588.1.19",
          "form": "ㅋㅋ우유병도 플라스틱이에요!",
          "original_form": "ㅋㅋ우유병도 플라스틱이에요!",
          "speaker_id": "1",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100014588.1.20",
          "form": "그런거예요?? 그래도 100원이면 재료값도 안나올것 같아요 ㅋ",
          "original_form": "그런거예요?? 그래도 100원이면 재료값도 안나올것 같아요 ㅋ",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100014588.1.21",
          "form": "택배비 하나내고 6박스에 왔으니 포장값 택배비도 안나와요 불항",
          "original_form": "택배비 하나내고 6박스에 왔으니 포장값 택배비도 안나와요 불항",
          "speaker_id": "1",
