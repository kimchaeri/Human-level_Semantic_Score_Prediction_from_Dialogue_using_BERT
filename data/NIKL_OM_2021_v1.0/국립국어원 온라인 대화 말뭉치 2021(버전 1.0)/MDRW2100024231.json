      "utterance": [
        {
          "id": "MDRW2100024231.1.1",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024231.1.2",
          "form": "그때 연예인들 샤기컷 울프컷이 유행이었었나요?",
          "original_form": "그때 연예인들 샤기컷 울프컷이 유행이었었나요?",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024231.1.3",
          "form": "그랫엇던거 같아용",
          "original_form": "그랫엇던거 같아용",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024231.1.4",
          "form": "이준기라",
          "original_form": "이준기라",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024231.1.5",
          "form": "그 유행이 아 ..",
          "original_form": "그 유행이 아 ..",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024231.1.6",
          "form": "이준기가 유행시켰어요",
          "original_form": "이준기가 유행시켰어요",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024231.1.7",
          "form": "이준기 배우님부터 시작된거에요?",
          "original_form": "이준기 배우님부터 시작된거에요?",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024231.1.8",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024231.1.9",
          "form": "저 아직도 그거 생각나네요",
          "original_form": "저 아직도 그거 생각나네요",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024231.1.10",
          "form": "동방신기부터인다",
          "original_form": "동방신기부터인다",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024231.1.11",
          "form": "미녀는 석류를 조아행~",
          "original_form": "미녀는 석류를 조아행~",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100024231.1.12",
          "form": "동방신기 샤기컷은 안 따라햇는데",
          "original_form": "동방신기 샤기컷은 안 따라햇는데",
          "speaker_id": "2",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100024231.1.13",
          "form": "이준기 샤기컷을 다들 따라햇던거같아요",
          "original_form": "이준기 샤기컷을 다들 따라햇던거같아요",
          "speaker_id": "2",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100024231.1.14",
          "form": "그 때 저 샤기컷 너무 하고 싶어서 머리 길렀는데",
          "original_form": "그 때 저 샤기컷 너무 하고 싶어서 머리 길렀는데",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100024231.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100024231.1.16",
          "form": "기를 때마다 미용사분이 그 꿈을 싹둑 자르셨어요",
          "original_form": "기를 때마다 미용사분이 그 꿈을 싹둑 자르셨어요",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100024231.1.17",
          "form": "흑역사남길뻔했어요",
          "original_form": "흑역사남길뻔했어요",
          "speaker_id": "2",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100024231.1.18",
          "form": "미용사님께 고마워해야됨",
          "original_form": "미용사님께 고마워해야됨",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024231.1.19",
          "form": "지금도 그 꿈을 이뤄보고 싶은걸요..",
          "original_form": "지금도 그 꿈을 이뤄보고 싶은걸요..",
          "speaker_id": "1",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024231.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024231.1.21",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024231.1.22",
          "form": "지금은 이룰수 있자나요",
          "original_form": "지금은 이룰수 있자나요",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024231.1.23",
          "form": "그때 했으면 지금 해보고 싶단 생각이 안들었을텐데 ㅠㅠ",
          "original_form": "그때 했으면 지금 해보고 싶단 생각이 안들었을텐데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024231.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024231.1.25",
          "form": "요즘엔 짧은 머리가 되게 유행(?)이잖아요",
          "original_form": "요즘엔 짧은 머리가 되게 유행(?)이잖아요",
          "speaker_id": "1",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024231.1.26",
          "form": "그래도 뭐 하고싶은 머리를 말리지는 않자나요",
          "original_form": "그래도 뭐 하고싶은 머리를 말리지는 않자나요",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024231.1.27",
          "form": "다들 투블럭을 하고잇지만",
          "original_form": "다들 투블럭을 하고잇지만",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024231.1.28",
          "form": "장발도 많아지고잇고",
          "original_form": "장발도 많아지고잇고",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024231.1.29",
          "form": "김지훈씨 장발 멋지던데",
          "original_form": "김지훈씨 장발 멋지던데",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100024231.1.30",
          "form": "ㅠㅠ제가 짧은 머리 어울려서 샤기컷은 못볼듯 하네용 흑흑",
          "original_form": "ㅠㅠ제가 짧은 머리 어울려서 샤기컷은 못볼듯 하네용 흑흑",
          "speaker_id": "1",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100024231.1.31",
          "form": "오~씽크홀 영화 감독님이셨네요",
          "original_form": "오~씽크홀 영화 감독님이셨네요",
          "speaker_id": "1",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100024231.1.32",
          "form": "오잉 진짜요??",
          "original_form": "오잉 진짜요??",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100024231.1.33",
          "form": "이 분 말씀하신거 아니에요?",
          "original_form": "이 분 말씀하신거 아니에요?",
          "speaker_id": "1",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100024231.1.34",
          "form": "아 아니에요 동명이인인가보네요",
          "original_form": "아 아니에요 동명이인인가보네요",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024231.1.35",
          "form": "배우 김지훈",
          "original_form": "배우 김지훈",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024231.1.36",
          "form": "고인 김지훈 가수님..? 은 장발 아니신거 같은데",
          "original_form": "고인 김지훈 가수님..? 은 장발 아니신거 같은데",
          "speaker_id": "1",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024231.1.37",
          "form": "ㅋㅋㅋㅋㅋ배우!!",
          "original_form": "ㅋㅋㅋㅋㅋ배우!!",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024231.1.38",
          "form": "혼내지마요 누나 ㅜㅜ",
          "original_form": "혼내지마요 누나 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024231.1.39",
          "form": "찾았는데 되게 잘어울리신다",
          "original_form": "찾았는데 되게 잘어울리신다",
          "speaker_id": "1",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024231.1.40",
          "form": "네 ㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
