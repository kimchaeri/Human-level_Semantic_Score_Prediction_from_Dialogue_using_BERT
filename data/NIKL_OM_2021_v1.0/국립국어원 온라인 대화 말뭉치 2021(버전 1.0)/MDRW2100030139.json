        {
          "id": "MDRW2100030139.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100030139.1.2",
          "form": "활 당기는것부터 힘들더라구요 전..",
          "original_form": "활 당기는것부터 힘들더라구요 전..",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100030139.1.3",
          "form": "ㅎㅎ...",
          "original_form": "ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100030139.1.4",
          "form": "늦은건 키즈모델뿐이랬어요...",
          "original_form": "늦은건 키즈모델뿐이랬어요...",
          "speaker_id": "1",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100030139.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100030139.1.6",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100030139.1.7",
          "form": "너무 웃겨욬ㅋㅋㅋㅋㅋ",
          "original_form": "너무 웃겨욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100030139.1.8",
          "form": "스포츠도 다 운동실력인데 관절 닳기전에 하면 다 할 수 있을거라고 믿어요",
          "original_form": "스포츠도 다 운동실력인데 관절 닳기전에 하면 다 할 수 있을거라고 믿어요",
          "speaker_id": "1",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100030139.1.9",
          "form": "화..화이팅 하세요!!",
          "original_form": "화..화이팅 하세요!!",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030139.1.10",
          "form": "응원하겠습니다 ㅎㅎ",
          "original_form": "응원하겠습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030139.1.11",
          "form": "배구도 진짜 짜릿하지 않았나요",
          "original_form": "배구도 진짜 짜릿하지 않았나요",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030139.1.12",
          "form": "터키전 할때랑 한일전할때",
          "original_form": "터키전 할때랑 한일전할때",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030139.1.13",
          "form": "네 맞아요 터키전도 그렇고 한일전도요!!",
          "original_form": "네 맞아요 터키전도 그렇고 한일전도요!!",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030139.1.14",
          "form": "배구 하는 거 보니까 배구모임 하고 싶더라구요...",
          "original_form": "배구 하는 거 보니까 배구모임 하고 싶더라구요...",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030139.1.15",
          "form": "ㅋㅋㅋ저두요 진짜",
          "original_form": "ㅋㅋㅋ저두요 진짜",
          "speaker_id": "2",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030139.1.16",
          "form": "물론 저는 배구를 너무 못해서 상상만..",
          "original_form": "물론 저는 배구를 너무 못해서 상상만..",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030139.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ당연히 저두요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ당연히 저두요",
          "speaker_id": "2",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030139.1.18",
          "form": "감독님도ㅠ너무 귀여우시지 않나요?ㅠㅠ",
          "original_form": "감독님도ㅠ너무 귀여우시지 않나요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030139.1.19",
          "form": "네ㅠㅜ 진심으로 한국 배구 아껴주는 것도 눈에 보이고...",
          "original_form": "네ㅠㅜ 진심으로 한국 배구 아껴주는 것도 눈에 보이고...",
          "speaker_id": "1",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030139.1.20",
          "form": "개개인의 능력을 끌어올려주고 팀능력 올리는거 위주로 잘해주시더라구요...",
          "original_form": "개개인의 능력을 끌어올려주고 팀능력 올리는거 위주로 잘해주시더라구요...",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100030139.1.21",
          "form": "감동이었어요 8ㅅ8",
          "original_form": "감동이었어요 8ㅅ8",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100030139.1.22",
          "form": "ㅠㅠㅠ 그리고 진짜 매의 눈...",
          "original_form": "ㅠㅠㅠ 그리고 진짜 매의 눈...",
          "speaker_id": "2",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100030139.1.23",
          "form": "라인 잡아내는게 예술이더라구요 진짜",
          "original_form": "라인 잡아내는게 예술이더라구요 진짜",
          "speaker_id": "2",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100030139.1.24",
          "form": "최공..",
          "original_form": "최공..",
          "speaker_id": "2",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100030139.1.25",
          "form": "유튜브 클립으로도 선수들이랑 감독이 잘지내는 훈훈한 모습 많은데",
          "original_form": "유튜브 클립으로도 선수들이랑 감독이 잘지내는 훈훈한 모습 많은데",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030139.1.26",
          "form": "계속 감독해주면 좋겠어요ㅠㅡㅠ",
          "original_form": "계속 감독해주면 좋겠어요ㅠㅡㅠ",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030139.1.27",
          "form": "ㅠㅠ그니까요",
          "original_form": "ㅠㅠ그니까요",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030139.1.28",
          "form": "김희진선수랑 감독이랑 있는 영상 보셨어요?",
          "original_form": "김희진선수랑 감독이랑 있는 영상 보셨어요?",
          "speaker_id": "2",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100030139.1.29",
          "form": "그것도 너무 귀엽고 웃겨요 둘다 ㅋㅋㅋㅋ",
          "original_form": "그것도 너무 귀엽고 웃겨요 둘다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:38"
        }
      ]
