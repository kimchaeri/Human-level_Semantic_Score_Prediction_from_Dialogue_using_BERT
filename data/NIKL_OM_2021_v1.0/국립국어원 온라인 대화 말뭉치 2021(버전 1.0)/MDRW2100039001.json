      "utterance": [
        {
          "id": "MDRW2100039001.1.1",
          "form": "내 친구중에",
          "original_form": "내 친구중에",
          "speaker_id": "2",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100039001.1.2",
          "form": "공무원이고",
          "original_form": "공무원이고",
          "speaker_id": "2",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100039001.1.3",
          "form": "거기 크기",
          "original_form": "거기 크기",
          "speaker_id": "1",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100039001.1.4",
          "form": "엄청착한애가",
          "original_form": "엄청착한애가",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100039001.1.5",
          "form": "있어",
          "original_form": "있어",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100039001.1.6",
          "form": "아까동아리도 같이하고",
          "original_form": "아까동아리도 같이하고",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100039001.1.7",
          "form": "과도같고",
          "original_form": "과도같고",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100039001.1.8",
          "form": "씨씨",
          "original_form": "씨씨",
          "speaker_id": "1",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100039001.1.9",
          "form": "그애가 사주볼따ㅗ",
          "original_form": "그애가 사주볼따ㅗ",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100039001.1.10",
          "form": "같이있었는데",
          "original_form": "같이있었는데",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100039001.1.11",
          "form": "사주 자주 보내요",
          "original_form": "사주 자주 보내요",
          "speaker_id": "1",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100039001.1.12",
          "form": "내가 그분이랑 소개팅한거",
          "original_form": "내가 그분이랑 소개팅한거",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100039001.1.13",
          "form": "알고있었어",
          "original_form": "알고있었어",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100039001.1.14",
          "form": "네 네",
          "original_form": "네 네",
          "speaker_id": "1",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100039001.1.15",
          "form": "그때 어머님이",
          "original_form": "그때 어머님이",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100039001.1.16",
          "form": "누님이",
          "original_form": "누님이",
          "speaker_id": "1",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100039001.1.17",
          "form": "그남자분 어머님",
          "original_form": "그남자분 어머님",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100039001.1.18",
          "form": "헐 어머님",
          "original_form": "헐 어머님",
          "speaker_id": "1",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100039001.1.19",
          "form": "나랑 걔랑 찍은 사진을",
          "original_form": "나랑 걔랑 찍은 사진을",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100039001.1.20",
          "form": "내친구말이야",
          "original_form": "내친구말이야",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100039001.1.21",
          "form": "홀 봤대",
          "original_form": "홀 봤대",
          "speaker_id": "1",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100039001.1.22",
          "form": "둘이서 찍은 사진을 봤는데",
          "original_form": "둘이서 찍은 사진을 봤는데",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100039001.1.23",
          "form": "사진을 봤는데",
          "original_form": "사진을 봤는데",
          "speaker_id": "1",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100039001.1.24",
          "form": "내친구를 더 맘에들어했어",
          "original_form": "내친구를 더 맘에들어했어",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100039001.1.25",
          "form": "근데 어쨌든",
          "original_form": "근데 어쨌든",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100039001.1.26",
          "form": "최악이다",
          "original_form": "최악이다",
          "speaker_id": "1",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100039001.1.27",
          "form": "나랑 그분이랑",
          "original_form": "나랑 그분이랑",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100039001.1.28",
          "form": "맨날 길 잘했네",
          "original_form": "맨날 길 잘했네",
          "speaker_id": "1",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100039001.1.29",
          "form": "소개만났고",
          "original_form": "소개만났고",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100039001.1.30",
          "form": "그분이 날 좋게보셨어",
          "original_form": "그분이 날 좋게보셨어",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100039001.1.31",
          "form": "내가 그만보자고 했을때",
          "original_form": "내가 그만보자고 했을때",
          "speaker_id": "2",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100039001.1.32",
          "form": "내앞에서는",
          "original_form": "내앞에서는",
          "speaker_id": "2",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100039001.1.33",
          "form": "슬퍼했겠다",
          "original_form": "슬퍼했겠다",
          "speaker_id": "1",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100039001.1.34",
          "form": "알겠다ㄱ하고",
          "original_form": "알겠다ㄱ하고",
          "speaker_id": "2",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100039001.1.35",
          "form": "뒤로 욕했나",
          "original_form": "뒤로 욕했나",
          "speaker_id": "1",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100039001.1.36",
          "form": "엄청 미련남아 했었나봐",
          "original_form": "엄청 미련남아 했었나봐",
          "speaker_id": "2",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100039001.1.37",
          "form": "아이구 어떡해",
          "original_form": "아이구 어떡해",
          "speaker_id": "1",
          "time": "20210903 01:00"
        }
      ]
