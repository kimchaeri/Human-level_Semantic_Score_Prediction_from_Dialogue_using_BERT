      "utterance": [
        {
          "id": "MDRW2100043310.1.1",
          "form": "결혼하셨어요?",
          "original_form": "결혼하셨어요?",
          "speaker_id": "1",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100043310.1.2",
          "form": "아니요 미혼이에요!",
          "original_form": "아니요 미혼이에요!",
          "speaker_id": "2",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100043310.1.3",
          "form": "결혼생각있으세요?",
          "original_form": "결혼생각있으세요?",
          "speaker_id": "1",
          "time": "20210907 17:25"
        },
        {
          "id": "MDRW2100043310.1.4",
          "form": "아직은 계획에 없어요",
          "original_form": "아직은 계획에 없어요",
          "speaker_id": "2",
          "time": "20210907 17:25"
        },
        {
          "id": "MDRW2100043310.1.5",
          "form": "저도 없어요",
          "original_form": "저도 없어요",
          "speaker_id": "1",
          "time": "20210907 17:25"
        },
        {
          "id": "MDRW2100043310.1.6",
          "form": "다 없는거같아요",
          "original_form": "다 없는거같아요",
          "speaker_id": "1",
          "time": "20210907 17:25"
        },
        {
          "id": "MDRW2100043310.1.7",
          "form": "연애는 하실 생각 있나요?",
          "original_form": "연애는 하실 생각 있나요?",
          "speaker_id": "2",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100043310.1.8",
          "form": "저 21살인데",
          "original_form": "저 21살인데",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100043310.1.9",
          "form": "모솔이에여",
          "original_form": "모솔이에여",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100043310.1.10",
          "form": "21살이면 뭐 그럴수도 있죠 ㅎㅎ",
          "original_form": "21살이면 뭐 그럴수도 있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100043310.1.11",
          "form": "이상형 같은 거 있나요?",
          "original_form": "이상형 같은 거 있나요?",
          "speaker_id": "2",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100043310.1.12",
          "form": "착하구 귀여운사람이요",
          "original_form": "착하구 귀여운사람이요",
          "speaker_id": "1",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100043310.1.13",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100043310.1.14",
          "form": "저는 대화가 잘 통하는거요 웃음코드 맞는 거랑",
          "original_form": "저는 대화가 잘 통하는거요 웃음코드 맞는 거랑",
          "speaker_id": "2",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100043310.1.15",
          "form": "아하 대화잘맞으면 좋아할수밖에없죠",
          "original_form": "아하 대화잘맞으면 좋아할수밖에없죠",
          "speaker_id": "1",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100043310.1.16",
          "form": "몇살이신가요",
          "original_form": "몇살이신가요",
          "speaker_id": "1",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100043310.1.17",
