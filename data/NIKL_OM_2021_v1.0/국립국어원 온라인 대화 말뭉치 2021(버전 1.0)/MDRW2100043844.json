      "utterance": [
        {
          "id": "MDRW2100043844.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043844.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043844.1.3",
          "form": "저 안그래도 이번에 코트를 사려고 벼르고 있습니다",
          "original_form": "저 안그래도 이번에 코트를 사려고 벼르고 있습니다",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043844.1.4",
          "form": "앗 코트요?",
          "original_form": "앗 코트요?",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043844.1.5",
          "form": "트렌치코트인가요 혹싀",
          "original_form": "트렌치코트인가요 혹싀",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043844.1.6",
          "form": "고민중이에요 ㅋㅋㅋㅋㅋ",
          "original_form": "고민중이에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043844.1.7",
          "form": "그냥 일반 코트 사고싶은데",
          "original_form": "그냥 일반 코트 사고싶은데",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043844.1.8",
          "form": "예쁜게 있을지",
          "original_form": "예쁜게 있을지",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043844.1.9",
          "form": "ㅋㅋㅋㅋ저는 저번에 역시즌 방송할 때 하나샀어요 겨울용...",
          "original_form": "ㅋㅋㅋㅋ저는 저번에 역시즌 방송할 때 하나샀어요 겨울용...",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100043844.1.10",
          "form": "오.... 저도 그래야하는디....",
          "original_form": "오.... 저도 그래야하는디....",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043844.1.11",
          "form": "역시즌 끝났갰죠....?",
          "original_form": "역시즌 끝났갰죠....?",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100043844.1.12",
          "form": "그러려나요ㅜㅜ? 한번 검색해보시면 나올 수도 있죠!",
          "original_form": "그러려나요ㅜㅜ? 한번 검색해보시면 나올 수도 있죠!",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043844.1.13",
          "form": "그래야겠어요! 검색왕 발동",
          "original_form": "그래야겠어요! 검색왕 발동",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100043844.1.14",
          "form": "알바하면서 여유가 있으니 후후후후",
          "original_form": "알바하면서 여유가 있으니 후후후후",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100043844.1.15",
          "form": "ㅋㅋㅋ요즘 구매하신 물건도 있나요?",
          "original_form": "ㅋㅋㅋ요즘 구매하신 물건도 있나요?",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100043844.1.16",
          "form": "저 이번에 일회용안대 종류별로 쟁였습니다 히히",
          "original_form": "저 이번에 일회용안대 종류별로 쟁였습니다 히히",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100043844.1.17",
          "form": "아 그 뜨끈해지는 안대 말씀하시는거죠?",
          "original_form": "아 그 뜨끈해지는 안대 말씀하시는거죠?",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100043844.1.18",
          "form": "오 맞아요!!",
          "original_form": "오 맞아요!!",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100043844.1.19",
          "form": "알고계시는군요",
          "original_form": "알고계시는군요",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100043844.1.20",
          "form": "저도 예전에 사서 썼었어요ㅋㅋ",
          "original_form": "저도 예전에 사서 썼었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100043844.1.21",
          "form": "잠이 솔솔 오더라구요ㅋㅋ",
          "original_form": "잠이 솔솔 오더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100043844.1.22",
          "form": "안구건조증에도 좋다더라구요!",
          "original_form": "안구건조증에도 좋다더라구요!",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100043844.1.23",
          "form": "그말듣고 대량구매(....)",
          "original_form": "그말듣고 대량구매(....)",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100043844.1.24",
          "form": "ㅋㅋㅋㅋ그거 다 쓰고나면 눈가가 촉촉해요 스팀한것마냥",
          "original_form": "ㅋㅋㅋㅋ그거 다 쓰고나면 눈가가 촉촉해요 스팀한것마냥",
          "speaker_id": "1",
