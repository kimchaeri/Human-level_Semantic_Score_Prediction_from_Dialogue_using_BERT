        },
        {
          "id": "MDRW2100043866.1.2",
          "form": "좋아합니다!!!",
          "original_form": "좋아합니다!!!",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043866.1.3",
          "form": "헉 치즈돈까스!!!!!",
          "original_form": "헉 치즈돈까스!!!!!",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043866.1.4",
          "form": "배달시켜드셨나요?!!",
          "original_form": "배달시켜드셨나요?!!",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043866.1.5",
          "form": "냉동된거 에어프라이어 돌려 먹었습니다",
          "original_form": "냉동된거 에어프라이어 돌려 먹었습니다",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043866.1.6",
          "form": "생선까스랑 함께여!!",
          "original_form": "생선까스랑 함께여!!",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043866.1.7",
          "form": "돈까스 진짜 너무 맛있는거 같아요ㅎㅎ",
          "original_form": "돈까스 진짜 너무 맛있는거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043866.1.8",
          "form": "쌀국수는 좋아하시나요??!",
          "original_form": "쌀국수는 좋아하시나요??!",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043866.1.9",
          "form": "쌀국수도 좋아합니다!!!!",
          "original_form": "쌀국수도 좋아합니다!!!!",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043866.1.10",
          "form": "맛있습니다!!!! 월남쌈 좋아하세요??",
          "original_form": "맛있습니다!!!! 월남쌈 좋아하세요??",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043866.1.11",
          "form": "네 엄청 좋아해요!!!!",
          "original_form": "네 엄청 좋아해요!!!!",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043866.1.12",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043866.1.13",
          "form": "엄청 좋아합니다",
          "original_form": "엄청 좋아합니다",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043866.1.14",
          "form": "집에서도 해먹어용!!",
          "original_form": "집에서도 해먹어용!!",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043866.1.15",
          "form": "우와 번거롭진않나요?",
          "original_form": "우와 번거롭진않나요?",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043866.1.16",
          "form": "그냥 썰기만 해도 되니까",
          "original_form": "그냥 썰기만 해도 되니까",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043866.1.17",
          "form": "간보는 요리보다 낫더라고요",
          "original_form": "간보는 요리보다 낫더라고요",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043866.1.18",
          "form": "나중에 저도 해먹어봐야겠네요",
          "original_form": "나중에 저도 해먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043866.1.19",
          "form": "샤브샤브는 어떠세요?",
          "original_form": "샤브샤브는 어떠세요?",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043866.1.20",
          "form": "샤브샤브도 당연히 좋아합니다!!!!!",
          "original_form": "샤브샤브도 당연히 좋아합니다!!!!!",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
