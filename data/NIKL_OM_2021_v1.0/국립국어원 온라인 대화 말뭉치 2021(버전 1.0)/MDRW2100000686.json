      "utterance": [
        {
          "id": "MDRW2100000686.1.1",
          "form": "책읽는거 좋아하시나요?",
          "original_form": "책읽는거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000686.1.2",
          "form": "네 책읽는거 좋아해요",
          "original_form": "네 책읽는거 좋아해요",
          "speaker_id": "2",
          "time": "20210819 21:01"
        },
        {
          "id": "MDRW2100000686.1.3",
          "form": "요즘에는 핸드폰으로도 읽을수있고 좋더라구요ㅎ",
          "original_form": "요즘에는 핸드폰으로도 읽을수있고 좋더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210819 21:26"
        },
        {
          "id": "MDRW2100000686.1.4",
          "form": "근데 만화책은 종이책으로 봐야재밌는거 같아요",
          "original_form": "근데 만화책은 종이책으로 봐야재밌는거 같아요",
          "speaker_id": "2",
          "time": "20210819 21:27"
        },
        {
          "id": "MDRW2100000686.1.5",
          "form": "맞아요!! 종이로 보는게 훨씬 재밌죠",
          "original_form": "맞아요!! 종이로 보는게 훨씬 재밌죠",
          "speaker_id": "1",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100000686.1.6",
          "form": "손가락에 침 묻혀가면서",
          "original_form": "손가락에 침 묻혀가면서",
          "speaker_id": "2",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100000686.1.7",
          "form": "라면도 먹고ㅎㅎ",
          "original_form": "라면도 먹고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100000686.1.8",
          "form": "배 깔고보는 것도 좋아요",
          "original_form": "배 깔고보는 것도 좋아요",
          "speaker_id": "2",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100000686.1.9",
          "form": "너무좋죠~ 요즘은 애보느라 책읽는시간도 없어서 아쉽네요ㅠ",
          "original_form": "너무좋죠~ 요즘은 애보느라 책읽는시간도 없어서 아쉽네요ㅠ",
          "speaker_id": "1",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100000686.1.10",
          "form": "폰으로는 어느 앱을 이용하시는거예요",
          "original_form": "폰으로는 어느 앱을 이용하시는거예요",
          "speaker_id": "2",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100000686.1.11",
          "form": "밀리의서재 무료로 한달ㅋㅋ해봤어요 ㅎ",
          "original_form": "밀리의서재 무료로 한달ㅋㅋ해봤어요 ㅎ",
          "speaker_id": "1",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100000686.1.12",
          "form": "아 전 카카오페이지 많이 이용해요",
          "original_form": "아 전 카카오페이지 많이 이용해요",
          "speaker_id": "2",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100000686.1.13",
          "form": "아~ 웹툰은ㅎ 네이버웹툰 봐요ㅎ",
          "original_form": "아~ 웹툰은ㅎ 네이버웹툰 봐요ㅎ",
          "speaker_id": "1",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100000686.1.14",
          "form": "요번에 새로 오픈한걸로 아는데 좋은가요",
          "original_form": "요번에 새로 오픈한걸로 아는데 좋은가요",
          "speaker_id": "2",
