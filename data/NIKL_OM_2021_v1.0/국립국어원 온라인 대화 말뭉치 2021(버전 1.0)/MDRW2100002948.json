      "utterance": [
        {
          "id": "MDRW2100002948.1.1",
          "form": "오늘은 어디 커피를 먹었니",
          "original_form": "오늘은 어디 커피를 먹었니",
          "speaker_id": "1",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002948.1.2",
          "form": "오늘도 여전히 지에스 커피를 마셨어",
          "original_form": "오늘도 여전히 지에스 커피를 마셨어",
          "speaker_id": "2",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002948.1.3",
          "form": "아빠가 커피를 사다주지 않았거든.",
          "original_form": "아빠가 커피를 사다주지 않았거든.",
          "speaker_id": "2",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002948.1.4",
          "form": "얼음타니까 시원한 맛에 먹을 만 해.",
          "original_form": "얼음타니까 시원한 맛에 먹을 만 해.",
          "speaker_id": "2",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002948.1.5",
          "form": "와 시원한 아이스커피",
          "original_form": "와 시원한 아이스커피",
          "speaker_id": "1",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002948.1.6",
          "form": "나는 선배가 커피를 사주셨어",
          "original_form": "나는 선배가 커피를 사주셨어",
          "speaker_id": "1",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002948.1.7",
          "form": "점심은 연구비로 먹고",
          "original_form": "점심은 연구비로 먹고",
          "speaker_id": "1",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002948.1.8",
          "form": "어떤 커피릉 마셨어?",
          "original_form": "어떤 커피릉 마셨어?",
          "speaker_id": "2",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002948.1.9",
          "form": "아무래도 얻어먹는거니까",
          "original_form": "아무래도 얻어먹는거니까",
          "speaker_id": "1",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002948.1.10",
          "form": "제일 싼 아메리카노지",
          "original_form": "제일 싼 아메리카노지",
          "speaker_id": "1",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002948.1.11",
          "form": "아니 내 말은 브랜드야",
          "original_form": "아니 내 말은 브랜드야",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002948.1.12",
          "form": "사실 제일 무난한거같아",
          "original_form": "사실 제일 무난한거같아",
          "speaker_id": "1",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002948.1.13",
          "form": "아하아하",
          "original_form": "아하아하",
          "speaker_id": "1",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002948.1.14",
          "form": "어떤 브랜드 커피를 마셨어?",
          "original_form": "어떤 브랜드 커피를 마셨어?",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002948.1.15",
          "form": "맞아 아아 무난하지",
          "original_form": "맞아 아아 무난하지",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002948.1.16",
          "form": "음 학교 앞에는 브랜드 커피가 없오서",
          "original_form": "음 학교 앞에는 브랜드 커피가 없오서",
          "speaker_id": "1",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002948.1.17",
          "form": "뜨아는 아니었지?",
          "original_form": "뜨아는 아니었지?",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002948.1.18",
          "form": "교내카페를 이용했구나",
          "original_form": "교내카페를 이용했구나",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002948.1.19",
          "form": "로컬 카페 커피를 마셨어",
          "original_form": "로컬 카페 커피를 마셨어",
          "speaker_id": "1",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002948.1.20",
          "form": "뜨아는 아무래도 이 날씨에",
          "original_form": "뜨아는 아무래도 이 날씨에",
          "speaker_id": "1",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002948.1.21",
          "form": "맛있었어?",
          "original_form": "맛있었어?",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002948.1.22",
          "form": "무난했지?",
          "original_form": "무난했지?",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002948.1.23",
          "form": "재앙아닐까?",
          "original_form": "재앙아닐까?",
          "speaker_id": "1",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002948.1.24",
          "form": "공짜커피는 언제나 맛있어",
          "original_form": "공짜커피는 언제나 맛있어",
          "speaker_id": "1",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002948.1.25",
          "form": "우리아버지는 뜨아를 드시는데",
          "original_form": "우리아버지는 뜨아를 드시는데",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002948.1.26",
          "form": "재앙을 드시고 계시는거니?",
          "original_form": "재앙을 드시고 계시는거니?",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002948.1.27",
          "form": "너 오늘 우리 오빠와 아빠를 한꺼번에 까는구나 !",
          "original_form": "너 오늘 우리 오빠와 아빠를 한꺼번에 까는구나 !",
          "speaker_id": "2",
          "time": "20210820 17:14"
        }
      ]
    }
  ]
