      "utterance": [
        {
          "id": "MDRW2100030903.1.1",
          "form": "군대갓다오셧어요?",
          "original_form": "군대갓다오셧어요?",
          "speaker_id": "1",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030903.1.2",
          "form": "앗 저는 군대를 다녀오지 않았어요",
          "original_form": "앗 저는 군대를 다녀오지 않았어요",
