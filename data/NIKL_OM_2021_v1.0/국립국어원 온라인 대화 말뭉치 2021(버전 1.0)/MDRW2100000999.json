      "utterance": [
        {
          "id": "MDRW2100000999.1.1",
          "form": "영화관에서 영화 못본지 오래되었어요ㅠㅠ",
          "original_form": "영화관에서 영화 못본지 오래되었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100000999.1.2",
          "form": "저두요 ㅠㅠ",
          "original_form": "저두요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100000999.1.3",
          "form": "영호ㅓ너무 보고싶어요",
          "original_form": "영호ㅓ너무 보고싶어요",
          "speaker_id": "2",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100000999.1.4",
          "form": "블랙위도우 개봉했는데 영화관서 못봐서...",
          "original_form": "블랙위도우 개봉했는데 영화관서 못봐서...",
          "speaker_id": "1",
          "time": "20210820 11:53"
        },
        {
          "id": "MDRW2100000999.1.5",
          "form": "아이피티비로 봐야할꺼같아요ㅠㅠ",
          "original_form": "아이피티비로 봐야할꺼같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 11:53"
        },
        {
          "id": "MDRW2100000999.1.6",
          "form": "저는 넷플릭스에 있는거 보려구여... 아무거나...",
          "original_form": "저는 넷플릭스에 있는거 보려구여... 아무거나...",
          "speaker_id": "2",
          "time": "20210820 11:54"
        },
        {
          "id": "MDRW2100000999.1.7",
          "form": "사람 없는 시간에 영화관 가볼까 했는데",
          "original_form": "사람 없는 시간에 영화관 가볼까 했는데",
          "speaker_id": "2",
          "time": "20210820 11:54"
        },
        {
          "id": "MDRW2100000999.1.8",
          "form": "그것도 걱정되긴하더라구요 ㅠㅠ",
          "original_form": "그것도 걱정되긴하더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 11:54"
        },
        {
          "id": "MDRW2100000999.1.9",
          "form": "그쵸..영화관이 많이 죽었어요...",
          "original_form": "그쵸..영화관이 많이 죽었어요...",
          "speaker_id": "1",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100000999.1.10",
          "form": "영화관에서 보는게 또 새로운데",
          "original_form": "영화관에서 보는게 또 새로운데",
          "speaker_id": "1",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100000999.1.11",
          "form": "맞아요ㅠㅠ 영화는 진짜 영화관에가서 봐야 느낌이 사는데",
          "original_form": "맞아요ㅠㅠ 영화는 진짜 영화관에가서 봐야 느낌이 사는데",
          "speaker_id": "2",
          "time": "20210820 12:01"
        },
        {
          "id": "MDRW2100000999.1.12",
          "form": "배우들 연기가 너무 좋더라구요",
          "original_form": "배우들 연기가 너무 좋더라구요",
          "speaker_id": "1",
          "time": "20210820 12:02"
        },
        {
          "id": "MDRW2100000999.1.13",
          "form": "맞아요 더 잘보이고",
          "original_form": "맞아요 더 잘보이고",
          "speaker_id": "2",
          "time": "20210820 12:05"
        },
        {
          "id": "MDRW2100000999.1.14",
          "form": "최근에는 세종문화회관서 연극을 봤거든요",
          "original_form": "최근에는 세종문화회관서 연극을 봤거든요",
          "speaker_id": "1",
          "time": "20210820 12:06"
        },
        {
          "id": "MDRW2100000999.1.15",
          "form": "완벽한 타인이라고 확실히 현장에서 보니까 생동감이 넘 좋아써요",
          "original_form": "완벽한 타인이라고 확실히 현장에서 보니까 생동감이 넘 좋아써요",
          "speaker_id": "1",
          "time": "20210820 12:07"
        },
        {
          "id": "MDRW2100000999.1.16",
          "form": "혹시 영화로 나왓던거 맞나요??",
          "original_form": "혹시 영화로 나왓던거 맞나요??",
          "speaker_id": "2",
          "time": "20210820 12:29"
        },
        {
          "id": "MDRW2100000999.1.17",
          "form": "아 맞아요! 그게 연극으로도 했어요!",
          "original_form": "아 맞아요! 그게 연극으로도 했어요!",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100000999.1.18",
          "form": "영화 보셨나요?",
          "original_form": "영화 보셨나요?",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100000999.1.19",
          "form": "영화봤어요!",
          "original_form": "영화봤어요!",
          "speaker_id": "2",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100000999.1.20",
          "form": "아주 흥미진진했어요",
          "original_form": "아주 흥미진진했어요",
          "speaker_id": "2",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100000999.1.21",
          "form": "그쵸 재밌더라구요 나중에 연극하면 보러가보세요",
          "original_form": "그쵸 재밌더라구요 나중에 연극하면 보러가보세요",
          "speaker_id": "1",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100000999.1.22",
          "form": "다 아는내용인데도",
          "original_form": "다 아는내용인데도",
          "speaker_id": "1",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100000999.1.23",
          "form": "그 흥미진진함이 살아있어요",
          "original_form": "그 흥미진진함이 살아있어요",
          "speaker_id": "1",
          "time": "20210820 14:10"
        }
      ]
    }
  ]
