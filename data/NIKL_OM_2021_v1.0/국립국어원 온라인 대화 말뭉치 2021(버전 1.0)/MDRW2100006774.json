        },
        {
          "id": "MDRW2100006774.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100006774.1.3",
          "form": "동물키우세요?",
          "original_form": "동물키우세요?",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100006774.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100006774.1.5",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100006774.1.6",
          "form": "네 고양이 좋아해요",
          "original_form": "네 고양이 좋아해요",
          "speaker_id": "1",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100006774.1.7",
          "form": "저는 조금무섭던대ㅠ",
          "original_form": "저는 조금무섭던대ㅠ",
          "speaker_id": "2",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100006774.1.8",
          "form": "좋아하시는 동물 있나요?",
          "original_form": "좋아하시는 동물 있나요?",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100006774.1.9",
          "form": "전 강아지!",
          "original_form": "전 강아지!",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100006774.1.10",
          "form": "오 혹시 키우고 계시나요?",
          "original_form": "오 혹시 키우고 계시나요?",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100006774.1.11",
          "form": "아뇨ㅠ",
          "original_form": "아뇨ㅠ",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100006774.1.12",
          "form": "사실 키울만큼은 아니에요ㅠ",
          "original_form": "사실 키울만큼은 아니에요ㅠ",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100006774.1.13",
          "form": "보는정도ㅠㅠ",
          "original_form": "보는정도ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100006774.1.14",
          "form": "저도 동물은 좋아하는데 반려동물로 키우고 싶을 정도로 좋아하는 동물은 없어요",
          "original_form": "저도 동물은 좋아하는데 반려동물로 키우고 싶을 정도로 좋아하는 동물은 없어요",
          "speaker_id": "1",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100006774.1.15",
          "form": "돈도 많이 들고",
          "original_form": "돈도 많이 들고",
          "speaker_id": "1",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100006774.1.16",
          "form": "책임감도",
          "original_form": "책임감도",
          "speaker_id": "2",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100006774.1.17",
          "form": "쉬운문제가 아니에요ㅠ",
          "original_form": "쉬운문제가 아니에요ㅠ",
          "speaker_id": "2",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100006774.1.18",
          "form": "그러게요 그래서 주로 동물관련 유튜브채널을 많이 봐요",
          "original_form": "그러게요 그래서 주로 동물관련 유튜브채널을 많이 봐요",
          "speaker_id": "1",
          "time": "20210908 09:10"
        }
      ]
