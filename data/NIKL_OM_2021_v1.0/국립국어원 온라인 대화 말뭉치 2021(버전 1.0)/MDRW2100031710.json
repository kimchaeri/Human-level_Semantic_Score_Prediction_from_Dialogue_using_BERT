      "utterance": [
        {
          "id": "MDRW2100031710.1.1",
          "form": "소고기국 먹었어요 간단하개",
          "original_form": "소고기국 먹었어요 간단하개",
          "speaker_id": "2",
          "time": "20210904 23:42"
        },
        {
          "id": "MDRW2100031710.1.2",
          "form": "올 해드신건거요",
          "original_form": "올 해드신건거요",
          "speaker_id": "1",
          "time": "20210904 23:42"
        },
        {
          "id": "MDRW2100031710.1.3",
          "form": "아뇨 집에서 냉동 갖고온거 해동해서요",
          "original_form": "아뇨 집에서 냉동 갖고온거 해동해서요",
          "speaker_id": "2",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100031710.1.4",
          "form": "저녁 뭐드셨어요",
          "original_form": "저녁 뭐드셨어요",
          "speaker_id": "2",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100031710.1.5",
          "form": "ㅋㅋ저는 점심은 열무된장덮밥 먹얶는데",
          "original_form": "ㅋㅋ저는 점심은 열무된장덮밥 먹얶는데",
          "speaker_id": "1",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100031710.1.6",
          "form": "저녁은 귀차나서 냉동치킨아요 ㅋㅋ",
          "original_form": "저녁은 귀차나서 냉동치킨아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100031710.1.7",
          "form": "점심 맛있었겠네요",
          "original_form": "점심 맛있었겠네요",
          "speaker_id": "2",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100031710.1.8",
          "form": "백종원씨꺼 따라햇네요 ㅋㅋ",
          "original_form": "백종원씨꺼 따라햇네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031710.1.9",
          "form": "저는 벌써 배고프네요",
          "original_form": "저는 벌써 배고프네요",
          "speaker_id": "2",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031710.1.10",
          "form": "친구가 열무를 줘서",
          "original_form": "친구가 열무를 줘서",
          "speaker_id": "1",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031710.1.11",
          "form": "악",
          "original_form": "악",
          "speaker_id": "1",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031710.1.12",
          "form": "토욜인데 야식안드셖나요",
          "original_form": "토욜인데 야식안드셖나요",
          "speaker_id": "1",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031710.1.13",
          "form": "친구도 결혼했나요",
          "original_form": "친구도 결혼했나요",
          "speaker_id": "2",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031710.1.14",
          "form": "아니욬ㅋㅋ",
          "original_form": "아니욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031710.1.15",
          "form": "농사지어요 오빠친구요",
          "original_form": "농사지어요 오빠친구요",
          "speaker_id": "1",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031710.1.16",
          "form": "요즘 체중 감량 하려고요",
          "original_form": "요즘 체중 감량 하려고요",
          "speaker_id": "2",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031710.1.17",
          "form": "저도 해여하는데 귀찮네요 누뮤",
          "original_form": "저도 해여하는데 귀찮네요 누뮤",
          "speaker_id": "1",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031710.1.18",
          "form": "아 대단한 친구네요",
          "original_form": "아 대단한 친구네요",
          "speaker_id": "2",
          "time": "20210904 23:44"
        }
      ]
