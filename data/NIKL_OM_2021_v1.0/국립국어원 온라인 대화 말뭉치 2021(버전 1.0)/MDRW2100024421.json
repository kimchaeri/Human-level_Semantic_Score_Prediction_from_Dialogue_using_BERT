      "utterance": [
        {
          "id": "MDRW2100024421.1.1",
          "form": "여행은 역시 먹는게 최고죠ㅋㅋ",
          "original_form": "여행은 역시 먹는게 최고죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 07:56"
        },
        {
          "id": "MDRW2100024421.1.2",
          "form": "그렇죠ㅋㅋ",
          "original_form": "그렇죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 07:56"
        },
        {
          "id": "MDRW2100024421.1.3",
          "form": "부산에 거대갈비라고 유명한대",
          "original_form": "부산에 거대갈비라고 유명한대",
          "speaker_id": "1",
          "time": "20210831 07:56"
        },
        {
          "id": "MDRW2100024421.1.4",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210831 07:56"
        },
        {
          "id": "MDRW2100024421.1.5",
          "form": "어제 거기 다녀왔어요",
          "original_form": "어제 거기 다녀왔어요",
          "speaker_id": "1",
          "time": "20210831 07:56"
        },
        {
          "id": "MDRW2100024421.1.6",
          "form": "맛있으셨나요?",
          "original_form": "맛있으셨나요?",
          "speaker_id": "2",
          "time": "20210831 07:57"
        },
        {
          "id": "MDRW2100024421.1.7",
          "form": "네 맛좋았어요  가격대가 있어서 점심특선으로 ㅎㅎ",
          "original_form": "네 맛좋았어요  가격대가 있어서 점심특선으로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 07:57"
        },
        {
          "id": "MDRW2100024421.1.8",
          "form": "기억해뒀다가 담에 가봐야겠어요ㅎㅎ",
          "original_form": "기억해뒀다가 담에 가봐야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 07:57"
        },
        {
          "id": "MDRW2100024421.1.9",
          "form": "아까 말씀하신 제천은 어떠셨어요?",
          "original_form": "아까 말씀하신 제천은 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210831 07:57"
        },
        {
          "id": "MDRW2100024421.1.10",
          "form": "어제 먹는데 진심이었던 하루",
          "original_form": "어제 먹는데 진심이었던 하루",
          "speaker_id": "1",
          "time": "20210831 07:57"
        },
        {
          "id": "MDRW2100024421.1.11",
          "form": "예쁘고 조용하고 좋았어요",
          "original_form": "예쁘고 조용하고 좋았어요",
          "speaker_id": "1",
          "time": "20210831 07:58"
        },
        {
          "id": "MDRW2100024421.1.12",
          "form": "저는 몰랐는데",
          "original_form": "저는 몰랐는데",
          "speaker_id": "1",
          "time": "20210831 07:58"
        },
        {
          "id": "MDRW2100024421.1.13",
          "form": "와이프가 부부의 세계 촬영했던 곳이라고 알려줬어요",
          "original_form": "와이프가 부부의 세계 촬영했던 곳이라고 알려줬어요",
          "speaker_id": "1",
          "time": "20210831 07:58"
        },
        {
          "id": "MDRW2100024421.1.14",
          "form": "노천탕들도 너무 좋고",
          "original_form": "노천탕들도 너무 좋고",
          "speaker_id": "1",
          "time": "20210831 07:59"
        },
        {
          "id": "MDRW2100024421.1.15",
          "form": "오??",
          "original_form": "오??",
          "speaker_id": "2",
          "time": "20210831 07:59"
        },
        {
          "id": "MDRW2100024421.1.16",
          "form": "여름에 갔는데 겨울에 가면 더 좋다고 하더라고요",
          "original_form": "여름에 갔는데 겨울에 가면 더 좋다고 하더라고요",
          "speaker_id": "1",
          "time": "20210831 07:59"
        },
        {
          "id": "MDRW2100024421.1.17",
          "form": "넘 궁금하네요! 부부의 세계 재밌게 봤는데",
          "original_form": "넘 궁금하네요! 부부의 세계 재밌게 봤는데",
          "speaker_id": "2",
          "time": "20210831 07:59"
        },
        {
          "id": "MDRW2100024421.1.18",
          "form": "이번 겨울에 노려봐야겠어요",
          "original_form": "이번 겨울에 노려봐야겠어요",
          "speaker_id": "2",
          "time": "20210831 07:59"
        },
        {
          "id": "MDRW2100024421.1.19",
          "form": "네네 좋아요 숲속에 숙소가 있어서 예쁘고 산책하기도 좋고",
          "original_form": "네네 좋아요 숲속에 숙소가 있어서 예쁘고 산책하기도 좋고",
          "speaker_id": "1",
          "time": "20210831 08:00"
        },
        {
          "id": "MDRW2100024421.1.20",
          "form": "가족들이랑 다같이 가기엔 괜찮나요?",
          "original_form": "가족들이랑 다같이 가기엔 괜찮나요?",
          "speaker_id": "2",
          "time": "20210831 08:00"
        },
        {
          "id": "MDRW2100024421.1.21",
          "form": "네 가족여행하기 좋은곳이에요",
          "original_form": "네 가족여행하기 좋은곳이에요",
          "speaker_id": "1",
          "time": "20210831 08:01"
        },
        {
          "id": "MDRW2100024421.1.22",
          "form": "name2님은 추천할만한 여행지 있나요?",
          "original_form": "&name2&님은 추천할만한 여행지 있나요?",
          "speaker_id": "1",
          "time": "20210831 08:01"
        },
        {
          "id": "MDRW2100024421.1.23",
          "form": "부모님이랑 가야겠어요ㅎㅎ",
          "original_form": "부모님이랑 가야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 08:01"
        },
        {
          "id": "MDRW2100024421.1.24",
          "form": "전 강릉을 자주 가는데",
          "original_form": "전 강릉을 자주 가는데",
          "speaker_id": "2",
          "time": "20210831 08:01"
        },
        {
          "id": "MDRW2100024421.1.25",
          "form": "뚜벅이 여행하기에도 넘 좋아요!",
          "original_form": "뚜벅이 여행하기에도 넘 좋아요!",
          "speaker_id": "2",
          "time": "20210831 08:01"
        },
        {
          "id": "MDRW2100024421.1.26",
          "form": "차있으면 더 좋겠지만요ㅎㅎ",
          "original_form": "차있으면 더 좋겠지만요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 08:02"
        }
      ]
