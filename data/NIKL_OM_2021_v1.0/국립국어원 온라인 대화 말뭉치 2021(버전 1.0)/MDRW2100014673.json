      "utterance": [
        {
          "id": "MDRW2100014673.1.1",
          "form": "게임 하시나요?",
          "original_form": "게임 하시나요?",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014673.1.2",
          "form": "안녕하세요 게임 좋아합니다",
          "original_form": "안녕하세요 게임 좋아합니다",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014673.1.3",
          "form": "혹시 엘소드라고 아세요?",
          "original_form": "혹시 엘소드라고 아세요?",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014673.1.4",
          "form": "오 그런 게임 하세요?",
          "original_form": "오 그런 게임 하세요?",
          "speaker_id": "1",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014673.1.5",
          "form": "오 해보신거 같은데",
          "original_form": "오 해보신거 같은데",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014673.1.6",
          "form": "아뇨 잘 몰라요",
          "original_form": "아뇨 잘 몰라요",
          "speaker_id": "1",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014673.1.7",
          "form": "엘소드에 대해 설명좀 해주세요",
          "original_form": "엘소드에 대해 설명좀 해주세요",
          "speaker_id": "1",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014673.1.8",
          "form": "아 엘소드 모르시는구나!",
          "original_form": "아 엘소드 모르시는구나!",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014673.1.9",
          "form": "넥슨의 알피쥐 게임으로",
          "original_form": "넥슨의 알피쥐 게임으로",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014673.1.10",
          "form": "10년이상이 지난 장수게임이죠",
          "original_form": "10년이상이 지난 장수게임이죠",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014673.1.11",
          "form": "전작으로는 그랜드체이스가 있구요",
          "original_form": "전작으로는 그랜드체이스가 있구요",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014673.1.12",
          "form": "오 근데 유명하진 않나봐요",
          "original_form": "오 근데 유명하진 않나봐요",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014673.1.13",
          "form": "정말 재밌습니다",
          "original_form": "정말 재밌습니다",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014673.1.14",
          "form": "그랜드체이스는 들어봤는데",
          "original_form": "그랜드체이스는 들어봤는데",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014673.1.15",
          "form": "해보세요",
          "original_form": "해보세요",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014673.1.16",
          "form": "엘소드? 이름부터 좀 구린거같아요",
          "original_form": "엘소드? 이름부터 좀 구린거같아요",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014673.1.17",
          "form": "좀 그렇긴해요",
          "original_form": "좀 그렇긴해요",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014673.1.18",
          "form": "게임 순위가 어떻게 되죠?",
          "original_form": "게임 순위가 어떻게 되죠?",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014673.1.19",
          "form": "50위 밖은 확실해요",
          "original_form": "50위 밖은 확실해요",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014673.1.20",
          "form": "로우바둑이포커가 항상 머리 위에 있던걸로",
          "original_form": "로우바둑이포커가 항상 머리 위에 있던걸로",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014673.1.21",
          "form": "기억해요",
          "original_form": "기억해요",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014673.1.22",
          "form": "엘소드 모르시면 다른 게임 이야기를 해볼까요?",
          "original_form": "엘소드 모르시면 다른 게임 이야기를 해볼까요?",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014673.1.23",
          "form": "카트라이더라고 아세요?",
          "original_form": "카트라이더라고 아세요?",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014673.1.24",
          "form": "포커보다 못한 게임을 하시는군요..",
          "original_form": "포커보다 못한 게임을 하시는군요..",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014673.1.25",
          "form": "이거모르면 간첩인데",
          "original_form": "이거모르면 간첩인데",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014673.1.26",
          "form": "오 알죠~ 마리오카트 표절작이잖아요",
          "original_form": "오 알죠~ 마리오카트 표절작이잖아요",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014673.1.27",
          "form": "저는 마리오카트보다 재밌다고 생각해요",
          "original_form": "저는 마리오카트보다 재밌다고 생각해요",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014673.1.28",
          "form": "닌텐도가 없으신가보네요",
          "original_form": "닌텐도가 없으신가보네요",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014673.1.29",
          "form": "아 있습니다",
          "original_form": "아 있습니다",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014673.1.30",
          "form": "전에는 친구들이랑",
          "original_form": "전에는 친구들이랑",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014673.1.31",
          "form": "네 마리오카트가 없으신가보네요",
          "original_form": "네 마리오카트가 없으신가보네요",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014673.1.32",
          "form": "모동숲도 같이 했는걸요",
          "original_form": "모동숲도 같이 했는걸요",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014673.1.33",
          "form": "마리오카트 있어요",
          "original_form": "마리오카트 있어요",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014673.1.34",
          "form": "재미없다고.",
          "original_form": "재미없다고.",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014673.1.35",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014673.1.36",
          "form": "닌텐도 있으시면 젤다 해보셨어요?",
          "original_form": "닌텐도 있으시면 젤다 해보셨어요?",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014673.1.37",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014673.1.38",
          "form": "안하면 닌텐도를 시작도 못한거라던데",
          "original_form": "안하면 닌텐도를 시작도 못한거라던데",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014673.1.39",
          "form": "아뇨 그런게임은 별로 안좋아해서요",
          "original_form": "아뇨 그런게임은 별로 안좋아해서요",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014673.1.40",
          "form": "알피지는",
          "original_form": "알피지는",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014673.1.41",
          "form": "엘소드로 충분해요",
          "original_form": "엘소드로 충분해요",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014673.1.42",
          "form": "엘소드 해보실래요?",
          "original_form": "엘소드 해보실래요?",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014673.1.43",
          "form": "저는 엘소드? 보다는 카트라이더가 낫다고 생각해요",
          "original_form": "저는 엘소드? 보다는 카트라이더가 낫다고 생각해요",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014673.1.44",
          "form": "카트라이더 아시면 크아도 아시겟네요?",
          "original_form": "카트라이더 아시면 크아도 아시겟네요?",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014673.1.45",
          "form": "그럼 카트라이더 합니다",
          "original_form": "그럼 카트라이더 합니다",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014673.1.46",
          "form": "아 초등학교시절에",
          "original_form": "아 초등학교시절에",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014673.1.47",
          "form": "많이했죠",
          "original_form": "많이했죠",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014673.1.48",
          "form": "저는 우니라는 캐릭터를",
          "original_form": "저는 우니라는 캐릭터를",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014673.1.49",
          "form": "제일 좋아해요",
          "original_form": "제일 좋아해요",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014673.1.50",
          "form": "오 저도 우니 좋아해요",
          "original_form": "오 저도 우니 좋아해요",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014673.1.51",
          "form": "근데 럭셔리 나오고부터는 럭마도 괜찮다고 생각해요",
          "original_form": "근데 럭셔리 나오고부터는 럭마도 괜찮다고 생각해요",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014673.1.52",
          "form": "그냥 마리드는 별론가요?",
          "original_form": "그냥 마리드는 별론가요?",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014673.1.53",
          "form": "이거 마리드가 들으면 상처받는데",
          "original_form": "이거 마리드가 들으면 상처받는데",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014673.1.54",
          "form": "솔직히 마리드는 너무 느리잖아요",
          "original_form": "솔직히 마리드는 너무 느리잖아요",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014673.1.55",
          "form": "그럼 폰게임은 뭐해보셨나요?",
          "original_form": "그럼 폰게임은 뭐해보셨나요?",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014673.1.56",
          "form": "쿠키런 킹덤 아시나요?",
          "original_form": "쿠키런 킹덤 아시나요?",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014673.1.57",
          "form": "요즘은 그것만 하고있어요",
          "original_form": "요즘은 그것만 하고있어요",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014673.1.58",
          "form": "오 저도 킹덤해요",
          "original_form": "오 저도 킹덤해요",
          "speaker_id": "1",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014673.1.59",
          "form": "쿠키 다 모으셨어요?",
          "original_form": "쿠키 다 모으셨어요?",
          "speaker_id": "1",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014673.1.60",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014673.1.61",
          "form": "승급은 못해지만",
          "original_form": "승급은 못해지만",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014673.1.62",
          "form": "다있는거 같아요",
          "original_form": "다있는거 같아요",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014673.1.63",
          "form": "오 무슨쿠키 제일 좋아하세요?",
          "original_form": "오 무슨쿠키 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014673.1.64",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014673.1.65",
          "form": "감초맛이요",
          "original_form": "감초맛이요",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014673.1.66",
          "form": "근데 아직 2성이라 좀 슬퍼요",
          "original_form": "근데 아직 2성이라 좀 슬퍼요",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014673.1.67",
          "form": "시커먼애 좋아하시네요",
