      "utterance": [
        {
          "id": "MDRW2100028652.1.1",
          "form": "좋아하는 음료수 잇어?",
          "original_form": "좋아하는 음료수 잇어?",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028652.1.2",
          "form": "나는 파워에이드랑",
          "original_form": "나는 파워에이드랑",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028652.1.3",
          "form": "토레타가 좋아",
          "original_form": "토레타가 좋아",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028652.1.4",
          "form": "언니뉸?",
          "original_form": "언니뉸?",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028652.1.5",
          "form": "파워에이드는 파란게 맛잇지",
          "original_form": "파워에이드는 파란게 맛잇지",
          "speaker_id": "2",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028652.1.6",
          "form": "난 커피 ㅎㅎ",
          "original_form": "난 커피 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028652.1.7",
          "form": "커피가짱이쥐",
          "original_form": "커피가짱이쥐",
          "speaker_id": "2",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028652.1.8",
          "form": "맞아 ㅎㅎ",
          "original_form": "맞아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028652.1.9",
          "form": "글쿠나..",
          "original_form": "글쿠나..",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028652.1.10",
          "form": "난 커피 먹으면ㅂ",
          "original_form": "난 커피 먹으면ㅂ",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028652.1.11",
          "form": "배가 아프더라구",
          "original_form": "배가 아프더라구",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028652.1.12",
          "form": "파워에이드 그리구",
          "original_form": "파워에이드 그리구",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028652.1.13",
          "form": "노란거랑 파란거 섞어먹어도 맛잇어.",
          "original_form": "노란거랑 파란거 섞어먹어도 맛잇어.",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028652.1.14",
          "form": "신기하당...",
          "original_form": "신기하당...",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028652.1.15",
          "form": "근본은 콜라랑 사이다를",
          "original_form": "근본은 콜라랑 사이다를",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028652.1.16",
          "form": "섞어먹는거지.",
          "original_form": "섞어먹는거지.",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028652.1.17",
          "form": "그럼 진짜 맛있어",
          "original_form": "그럼 진짜 맛있어",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028652.1.18",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028652.1.19",
          "form": "내일 해먹쟈",
          "original_form": "내일 해먹쟈",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028652.1.20",
          "form": "옛날엔 학교앞에서",
          "original_form": "옛날엔 학교앞에서",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028652.1.21",
          "form": "슬러시도 섞어먹엇는데...",
          "original_form": "슬러시도 섞어먹엇는데...",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028652.1.22",
          "form": "기억나?",
          "original_form": "기억나?",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028652.1.23",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028652.1.24",
          "form": "콜라랑",
          "original_form": "콜라랑",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028652.1.25",
          "form": "포도맛",
          "original_form": "포도맛",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028652.1.26",
          "form": "오렌지인가..",
          "original_form": "오렌지인가..",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028652.1.27",
          "form": "거기에",
          "original_form": "거기에",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028652.1.28",
          "form": "떡볶이까지 먹으면",
          "original_form": "떡볶이까지 먹으면",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028652.1.29",
          "form": "개꿀이지..",
          "original_form": "개꿀이지..",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028652.1.30",
          "form": "꼭 섞어달라고 햇엇지 ㅋㅋ",
          "original_form": "꼭 섞어달라고 햇엇지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028652.1.31",
          "form": "난 양념감자가 좋았어",
          "original_form": "난 양념감자가 좋았어",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028652.1.32",
          "form": "글쿤 ㅋㅋ",
          "original_form": "글쿤 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028652.1.33",
          "form": "나는 어묻",
          "original_form": "나는 어묻",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028652.1.34",
          "form": "어묵",
          "original_form": "어묵",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028652.1.35",
          "form": "어묵도 팔앗엇나...",
          "original_form": "어묵도 팔앗엇나...",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028652.1.36",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028652.1.37",
          "form": "500원이었지..",
          "original_form": "500원이었지..",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028652.1.38",
          "form": "엄청 싸다..",
          "original_form": "엄청 싸다..",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028652.1.39",
          "form": "맞아..",
          "original_form": "맞아..",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028652.1.40",
          "form": "떡볶이도",
          "original_form": "떡볶이도",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028652.1.41",
          "form": "커다란 컵에 500원이었는데 옛날엔..",
          "original_form": "커다란 컵에 500원이었는데 옛날엔..",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028652.1.42",
          "form": "그때로 돌아가구 싶당 ㅎㅎ",
          "original_form": "그때로 돌아가구 싶당 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028652.1.43",
          "form": "이젠 조그만 컵에 1500원인거보고",
          "original_form": "이젠 조그만 컵에 1500원인거보고",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028652.1.44",
          "form": "어이가없더라구",
          "original_form": "어이가없더라구",
          "speaker_id": "1",
          "time": "20210902 21:51"
        }
      ]
    }
  ]
