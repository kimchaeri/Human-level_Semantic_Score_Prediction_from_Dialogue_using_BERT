        },
        {
          "id": "MDRW2100001790.1.2",
          "form": "완전좋아해요!!!",
          "original_form": "완전좋아해요!!!",
          "speaker_id": "1",
          "time": "20210819 21:17"
        },
        {
          "id": "MDRW2100001790.1.3",
          "form": "모바일게임이나 PC게임이나ㅋㅋㅋ",
