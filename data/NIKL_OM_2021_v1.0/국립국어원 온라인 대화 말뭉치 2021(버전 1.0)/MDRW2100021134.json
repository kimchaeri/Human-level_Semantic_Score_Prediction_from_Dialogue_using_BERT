      "utterance": [
        {
          "id": "MDRW2100021134.1.1",
          "form": "반려동물있으신가요?",
          "original_form": "반려동물있으신가요?",
          "speaker_id": "1",
          "time": "20210828 16:14"
        },
        {
          "id": "MDRW2100021134.1.2",
          "form": "저는 반려동물 없어용 ㅠㅠ 키우시나요?",
          "original_form": "저는 반려동물 없어용 ㅠㅠ 키우시나요?",
          "speaker_id": "2",
          "time": "20210828 16:14"
        },
        {
          "id": "MDRW2100021134.1.3",
          "form": "네네 고앵이 한마리랑 같이 있어요~",
          "original_form": "네네 고앵이 한마리랑 같이 있어요~",
          "speaker_id": "1",
          "time": "20210828 16:14"
        },
        {
          "id": "MDRW2100021134.1.4",
          "form": "헉 너무 귀엽겠어요",
          "original_form": "헉 너무 귀엽겠어요",
          "speaker_id": "2",
          "time": "20210828 16:15"
        },
        {
          "id": "MDRW2100021134.1.5",
          "form": "네네 귀여워요~ㅋㅋ 불러도 안오지만요",
          "original_form": "네네 귀여워요~ㅋㅋ 불러도 안오지만요",
          "speaker_id": "1",
          "time": "20210828 16:16"
        },
        {
          "id": "MDRW2100021134.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:16"
        },
        {
          "id": "MDRW2100021134.1.7",
          "form": "치즈냥이인가요!?",
          "original_form": "치즈냥이인가요!?",
          "speaker_id": "2",
          "time": "20210828 16:16"
        },
        {
          "id": "MDRW2100021134.1.8",
          "form": "남자아이구 턱시도에요~",
          "original_form": "남자아이구 턱시도에요~",
          "speaker_id": "1",
          "time": "20210828 16:16"
        },
        {
          "id": "MDRW2100021134.1.9",
          "form": "어머 잘생긴걸로 정정할게요",
          "original_form": "어머 잘생긴걸로 정정할게요",
          "speaker_id": "2",
          "time": "20210828 16:17"
        },
        {
          "id": "MDRW2100021134.1.10",
          "form": "저는 고양이 안을 때 쭈욱 늘어나는게ㅠ너무 신기해요",
          "original_form": "저는 고양이 안을 때 쭈욱 늘어나는게ㅠ너무 신기해요",
          "speaker_id": "2",
          "time": "20210828 16:18"
        },
        {
          "id": "MDRW2100021134.1.11",
          "form": "네 저도 고양이 안다가 강아지안으면 너무 딱딱?하다고 해야되나 다르더라구요",
          "original_form": "네 저도 고양이 안다가 강아지안으면 너무 딱딱?하다고 해야되나 다르더라구요",
          "speaker_id": "1",
          "time": "20210828 16:18"
        },
        {
          "id": "MDRW2100021134.1.12",
          "form": "고양이는 부드러?운느낌이구",
          "original_form": "고양이는 부드러?운느낌이구",
          "speaker_id": "1",
          "time": "20210828 16:18"
        },
        {
          "id": "MDRW2100021134.1.13",
          "form": "진짜 ㅋㅋ 강아지랑 사는 사람이랑 고영이랑 사는 사람이랑 그 부분에서 서로 가장 어색하다고 하더라구요",
          "original_form": "진짜 ㅋㅋ 강아지랑 사는 사람이랑 고영이랑 사는 사람이랑 그 부분에서 서로 가장 어색하다고 하더라구요",
          "speaker_id": "2",
          "time": "20210828 16:20"
        },
        {
          "id": "MDRW2100021134.1.14",
          "form": "전 그저 둘 다 너무 부러운...",
          "original_form": "전 그저 둘 다 너무 부러운...",
          "speaker_id": "2",
          "time": "20210828 16:20"
        },
        {
          "id": "MDRW2100021134.1.15",
          "form": "ㅋㅋㅋ전 강아지는 좀 무서워하긴한데 그래도 귀엽긴해요",
          "original_form": "ㅋㅋㅋ전 강아지는 좀 무서워하긴한데 그래도 귀엽긴해요",
          "speaker_id": "1",
          "time": "20210828 16:21"
        },
        {
          "id": "MDRW2100021134.1.16",
          "form": "유튜브로 많이봐요",
          "original_form": "유튜브로 많이봐요",
          "speaker_id": "1",
          "time": "20210828 16:21"
        },
        {
          "id": "MDRW2100021134.1.17",
          "form": "저는 비염도 너무 심하고 집도 좁아서 좀 액정너머로 보는걸 많이 해요ㅠㅠ",
          "original_form": "저는 비염도 너무 심하고 집도 좁아서 좀 액정너머로 보는걸 많이 해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 16:22"
        }
      ]
