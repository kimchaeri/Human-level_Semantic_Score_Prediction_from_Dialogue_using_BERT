      "utterance": [
        {
          "id": "MDRW2100019505.1.1",
          "form": "어떤경기좋아하세요//!",
          "original_form": "어떤경기좋아하세요//!",
          "speaker_id": "1",
          "time": "20210827 19:54"
        },
        {
          "id": "MDRW2100019505.1.2",
          "form": "저는 축구나 올림픽 보는거좋아해요ㅎㅎ",
          "original_form": "저는 축구나 올림픽 보는거좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:57"
        },
        {
          "id": "MDRW2100019505.1.3",
          "form": "아하아 저는 야구보는거 좋아해요!",
          "original_form": "아하아 저는 야구보는거 좋아해요!",
          "speaker_id": "1",
          "time": "20210827 19:57"
        },
        {
          "id": "MDRW2100019505.1.4",
          "form": "축구는 응원하는 팀이 어디신가욤",
          "original_form": "축구는 응원하는 팀이 어디신가욤",
          "speaker_id": "1",
          "time": "20210827 19:57"
        },
        {
          "id": "MDRW2100019505.1.5",
          "form": "저 손흥민만 알아서ㅎㅎ 토트넘이요ㅎㅎ",
          "original_form": "저 손흥민만 알아서ㅎㅎ 토트넘이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019505.1.6",
          "form": "아ㅏㅇ 역시!!ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅏㅇ 역시!!ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019505.1.7",
          "form": "토트넘 체고,,,",
          "original_form": "토트넘 체고,,,",
          "speaker_id": "1",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019505.1.8",
          "form": "ㅋㅋㅋ야구 한국야구 보시나요? 아니면 해외야구 보시나요?",
          "original_form": "ㅋㅋㅋ야구 한국야구 보시나요? 아니면 해외야구 보시나요?",
          "speaker_id": "2",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019505.1.9",
          "form": "둘다요!",
          "original_form": "둘다요!",
          "speaker_id": "1",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019505.1.10",
          "form": "오 해외야구도 챙겨보시는구나 어디팬이세요?",
          "original_form": "오 해외야구도 챙겨보시는구나 어디팬이세요?",
          "speaker_id": "2",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019505.1.11",
          "form": "저는 토론토요!!!",
          "original_form": "저는 토론토요!!!",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019505.1.12",
          "form": "류뚱오기전부터 팬이었어여!!",
          "original_form": "류뚱오기전부터 팬이었어여!!",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019505.1.13",
          "form": "류뚱ㅋㅋㅋㅋ아 류현진 토론토소속이에요?",
          "original_form": "류뚱ㅋㅋㅋㅋ아 류현진 토론토소속이에요?",
          "speaker_id": "2",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019505.1.14",
          "form": "네네네!!ㅋㅋㅋㅋㅋ",
          "original_form": "네네네!!ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019505.1.15",
          "form": "정확합니당ㅎ",
          "original_form": "정확합니당ㅎ",
          "speaker_id": "1",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019505.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저는 야구를 잘 안봐서 la다저스인줄알았어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저는 야구를 잘 안봐서 la다저스인줄알았어요",
          "speaker_id": "2",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019505.1.17",
          "form": "아아아 이적한지",
          "original_form": "아아아 이적한지",
          "speaker_id": "1",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019505.1.18",
          "form": "조금시간이 흘렀어요!!",
          "original_form": "조금시간이 흘렀어요!!",
          "speaker_id": "1",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019505.1.19",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100019505.1.20",
          "form": "직관 가신적 있으세요?",
          "original_form": "직관 가신적 있으세요?",
          "speaker_id": "2",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100019505.1.21",
          "form": "국내는 거의매일갔어여!!!",
          "original_form": "국내는 거의매일갔어여!!!",
          "speaker_id": "1",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019505.1.22",
          "form": "와 부럽네요 저는 한번도 안 가봤어요ㅎㅎ",
          "original_form": "와 부럽네요 저는 한번도 안 가봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100019505.1.23",
          "form": "국내는 어디 팬이세요?",
          "original_form": "국내는 어디 팬이세요?",
          "speaker_id": "2",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100019505.1.24",
          "form": "축구장은요??!!",
          "original_form": "축구장은요??!!",
          "speaker_id": "1",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100019505.1.25",
          "form": "축구장은 여러번 가봤어요!!",
          "original_form": "축구장은 여러번 가봤어요!!",
          "speaker_id": "2",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100019505.1.26",
          "form": "특히 친선경기 할 때 친구들이랑 여러번 갔었어요!",
          "original_form": "특히 친선경기 할 때 친구들이랑 여러번 갔었어요!",
          "speaker_id": "2",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100019505.1.27",
          "form": "k리그는 안 봐서ㅎㅎ",
          "original_form": "k리그는 안 봐서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100019505.1.28",
          "form": "아아아!!!!",
          "original_form": "아아아!!!!",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019505.1.29",
          "form": "국내는 삼성이요!ㅋㅋㅋㅋ",
          "original_form": "국내는 삼성이요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019505.1.30",
          "form": "축구장은 저는 잘안가스",
          "original_form": "축구장은 저는 잘안가스",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019505.1.31",
          "form": "국대경기만 보는정도랄까",
          "original_form": "국대경기만 보는정도랄까",
          "speaker_id": "1",
