      "utterance": [
        {
          "id": "MDRW2100025360.1.1",
          "form": "반려동물은 좋아하세요?",
          "original_form": "반려동물은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 10:11"
        },
        {
          "id": "MDRW2100025360.1.2",
          "form": "네 저는 고양이를 키우고 있어요",
          "original_form": "네 저는 고양이를 키우고 있어요",
          "speaker_id": "2",
          "time": "20210901 10:13"
        },
        {
          "id": "MDRW2100025360.1.3",
          "form": "반려동물 좋아하시나요",
          "original_form": "반려동물 좋아하시나요",
          "speaker_id": "2",
          "time": "20210901 10:13"
        },
        {
          "id": "MDRW2100025360.1.4",
          "form": "저는 좋아하는데집에허",
          "original_form": "저는 좋아하는데집에허",
          "speaker_id": "1",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025360.1.5",
          "form": "털때문에 못키우게하네요 ㅠ",
          "original_form": "털때문에 못키우게하네요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025360.1.6",
          "form": "강아지파세요 고양이파세요",
          "original_form": "강아지파세요 고양이파세요",
          "speaker_id": "2",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100025360.1.7",
          "form": "전 가리지않아요 귀여우면 다 좋더라구요",
          "original_form": "전 가리지않아요 귀여우면 다 좋더라구요",
          "speaker_id": "1",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100025360.1.8",
          "form": "주변에 키우는거 보면 너무  부럽더라구요 ㅠ",
          "original_form": "주변에 키우는거 보면 너무  부럽더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 10:18"
        },
        {
          "id": "MDRW2100025360.1.9",
          "form": "저도 강아지 키워서 산책 한번 가보는게 꿈이에요",
          "original_form": "저도 강아지 키워서 산책 한번 가보는게 꿈이에요",
          "speaker_id": "2",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025360.1.10",
          "form": "독립하면 키웁시다",
          "original_form": "독립하면 키웁시다",
          "speaker_id": "2",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025360.1.11",
          "form": "고양이는 몇마리 키우시나요?",
          "original_form": "고양이는 몇마리 키우시나요?",
          "speaker_id": "1",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025360.1.12",
          "form": "저는 두마리 키워요",
          "original_form": "저는 두마리 키워요",
          "speaker_id": "2",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025360.1.13",
          "form": "아진짜요?? 성격은 어떤가요",
          "original_form": "아진짜요?? 성격은 어떤가요",
          "speaker_id": "1",
          "time": "20210901 10:24"
        },
        {
          "id": "MDRW2100025360.1.14",
          "form": "둘다 쫄보에요 사람오면 다숨어요",
          "original_form": "둘다 쫄보에요 사람오면 다숨어요",
          "speaker_id": "2",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100025360.1.15",
          "form": "보고싶네요 ㅠㅠ 이름이 먼가요",
          "original_form": "보고싶네요 ㅠㅠ 이름이 먼가요",
          "speaker_id": "1",
          "time": "20210901 10:27"
        },
        {
          "id": "MDRW2100025360.1.16",
          "form": "name3랑 name4이에요",
          "original_form": "&name3&랑 &name4&이에요",
          "speaker_id": "2",
          "time": "20210901 10:27"
        },
        {
          "id": "MDRW2100025360.1.17",
          "form": "고등어랑 흰냥이에요",
          "original_form": "고등어랑 흰냥이에요",
          "speaker_id": "2",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025360.1.18",
          "form": "헐 과자이름이네요 ㅋㅋㅋ",
          "original_form": "헐 과자이름이네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025360.1.19",
          "form": "근데 둘이 안닮은 형제에요",
          "original_form": "근데 둘이 안닮은 형제에요",
          "speaker_id": "2",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025360.1.20",
          "form": "헐 어쩌다가 키우게 되셨나요?",
          "original_form": "헐 어쩌다가 키우게 되셨나요?",
          "speaker_id": "1",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025360.1.21",
          "form": "집으로 어미냥이가 들어와서 키우게 됐어요",
          "original_form": "집으로 어미냥이가 들어와서 키우게 됐어요",
          "speaker_id": "2",
          "time": "20210901 10:28"
        }
