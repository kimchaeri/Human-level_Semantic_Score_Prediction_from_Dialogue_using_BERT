      "utterance": [
        {
          "id": "MDRW2100035953.1.1",
          "form": "내년까지요 ㅋㅋㅋㅋㅋ",
          "original_form": "내년까지요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:36"
        },
        {
          "id": "MDRW2100035953.1.2",
          "form": "우리 name3은 프레벨 독서 수업하는데",
          "original_form": "우리 &name3&은 프레벨 독서 수업하는데",
          "speaker_id": "2",
          "time": "20210830 12:36"
        },
        {
          "id": "MDRW2100035953.1.3",
          "form": "하면 재밌다고 하는데",
          "original_form": "하면 재밌다고 하는데",
          "speaker_id": "2",
          "time": "20210830 12:36"
        },
        {
          "id": "MDRW2100035953.1.4",
          "form": "하러 들어오라고 하기까지가 힘듦 ㅋㅋㅋㅋㅋ",
          "original_form": "하러 들어오라고 하기까지가 힘듦 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:36"
        },
        {
          "id": "MDRW2100035953.1.5",
          "form": "아 내년 ㅠ",
          "original_form": "아 내년 ㅠ",
          "speaker_id": "2",
          "time": "20210830 12:36"
        },
        {
          "id": "MDRW2100035953.1.6",
          "form": "ㅋㅋㅋㅋ뭔지 알꺼같네여~놀때믄 뭘 해준다 해도 안들아옴 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ뭔지 알꺼같네여~놀때믄 뭘 해준다 해도 안들아옴 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100035953.1.7",
          "form": "어 ㅋㅋㅋㅋㄱ 그래서 그만할까 몇번이나 물었는데",
          "original_form": "어 ㅋㅋㅋㅋㄱ 그래서 그만할까 몇번이나 물었는데",
          "speaker_id": "2",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100035953.1.8",
          "form": "수업하고나서는 또 재밌다고 계속 한다고 난리부르스...",
          "original_form": "수업하고나서는 또 재밌다고 계속 한다고 난리부르스...",
          "speaker_id": "2",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100035953.1.9",
          "form": "들어오라하면",
          "original_form": "들어오라하면",
          "speaker_id": "2",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100035953.1.10",
          "form": "지짜고...아놔....",
          "original_form": "지짜고...아놔....",
          "speaker_id": "2",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100035953.1.11",
          "form": "어떻게 집에 들어오게 만들어야 되죠??",
          "original_form": "어떻게 집에 들어오게 만들어야 되죠??",
          "speaker_id": "1",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100035953.1.12",
          "form": "근데 왜 우리도 놀다가 들어오면 참 싫잖아요ㅋㅋㅋㅋㅋ",
          "original_form": "근데 왜 우리도 놀다가 들어오면 참 싫잖아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100035953.1.13",
          "form": "똥 덜 닦은 거 같고",
          "original_form": "똥 덜 닦은 거 같고",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100035953.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋ 비유를 꼭",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋ 비유를 꼭",
          "speaker_id": "2",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100035953.1.15",
          "form": "똥에 해야되나 ㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋ",
          "original_form": "똥에 해야되나 ㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100035953.1.16",
          "form": "딱 똥 밖에 안 떠오르네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "딱 똥 밖에 안 떠오르네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035953.1.17",
          "form": "그래도 수업 좋아하니 다행입니다ㅋㅋ",
          "original_form": "그래도 수업 좋아하니 다행입니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035953.1.18",
