      "utterance": [
        {
          "id": "MDRW2100024082.1.1",
          "form": "문명특급 좋아하세요?",
          "original_form": "문명특급 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024082.1.2",
          "form": "네 ㅠㅠ 정말 좋아해요",
          "original_form": "네 ㅠㅠ 정말 좋아해요",
          "speaker_id": "2",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100024082.1.3",
          "form": "너무 즐거워요",
          "original_form": "너무 즐거워요",
          "speaker_id": "2",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100024082.1.4",
          "form": "저도 좋아해요.",
          "original_form": "저도 좋아해요.",
          "speaker_id": "1",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100024082.1.5",
          "form": "전에 숨듣명 컴눈명 할때",
          "original_form": "전에 숨듣명 컴눈명 할때",
          "speaker_id": "1",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100024082.1.6",
          "form": "ㅋㅋㅋㅋ큐ㅠㅠㅜㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ큐ㅠㅠㅜㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100024082.1.7",
          "form": "제가 딱 그 세대라",
          "original_form": "제가 딱 그 세대라",
          "speaker_id": "1",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100024082.1.8",
          "form": "와 저도요 ㅋㅋㅋㅋ어쩌면 비슷한 나이일수도 있겠어요 ㅎㅎ",
          "original_form": "와 저도요 ㅋㅋㅋㅋ어쩌면 비슷한 나이일수도 있겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100024082.1.9",
          "form": "진짜 즐겁죠 그시절을 같이 이야기하는것같고",
          "original_form": "진짜 즐겁죠 그시절을 같이 이야기하는것같고",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024082.1.10",
          "form": "미니홈피도 하셨나요?",
          "original_form": "미니홈피도 하셨나요?",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024082.1.11",
          "form": "아 했긴했는데",
          "original_form": "아 했긴했는데",
          "speaker_id": "1",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100024082.1.12",
          "form": "딱히 뭘 쓰진않았어요",
          "original_form": "딱히 뭘 쓰진않았어요",
          "speaker_id": "1",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100024082.1.13",
          "form": "학교에서 검사하는 일기도 안썼는데 미니홈피를 관리하기엔 전 너무 귀차니스트 ㅋㅋ",
          "original_form": "학교에서 검사하는 일기도 안썼는데 미니홈피를 관리하기엔 전 너무 귀차니스트 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100024082.1.14",
          "form": "그러셨군요...저는 정말 말이 많은사람이고 사담을 좋아해서 흑역사 열심히 남겼어욬ㅋㅋㅋ",
          "original_form": "그러셨군요...저는 정말 말이 많은사람이고 사담을 좋아해서 흑역사 열심히 남겼어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:30"
        },
        {
          "id": "MDRW2100024082.1.15",
          "form": "뮤지컬 좋아하세요?,,",
          "original_form": "뮤지컬 좋아하세요?,,",
          "speaker_id": "2",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100024082.1.16",
          "form": "사실 사랑합니다..  ㅋㅋ",
          "original_form": "사실 사랑합니다..  ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024082.1.17",
          "form": "달에 서너번은 보는듯요",
          "original_form": "달에 서너번은 보는듯요",
          "speaker_id": "1",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024082.1.18",
          "form": "와 너무 좋네요 저는 워낙 문화생활ㄱ 떨어진 곳이라 자주 못보지만 좋아하거든요",
          "original_form": "와 너무 좋네요 저는 워낙 문화생활ㄱ 떨어진 곳이라 자주 못보지만 좋아하거든요",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024082.1.19",
          "form": "이번달에도 가셔시나요??",
          "original_form": "이번달에도 가셔시나요??",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024082.1.20",
          "form": "네... ㅋㅋ",
          "original_form": "네... ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100024082.1.21",
          "form": "뭐 보셨어요? 저는 마리앙투아네트 보고싶어요 ㅠㅠ",
          "original_form": "뭐 보셨어요? 저는 마리앙투아네트 보고싶어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024082.1.22",
          "form": "레드북이랑 스프링어웨이크닝이요",
          "original_form": "레드북이랑 스프링어웨이크닝이요",
          "speaker_id": "1",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100024082.1.23",
          "form": "분장실이란 연극도 봤네요",
          "original_form": "분장실이란 연극도 봤네요",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024082.1.24",
          "form": "아, 저 마리앙투아네트 초연때 봤어요",
          "original_form": "아, 저 마리앙투아네트 초연때 봤어요",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024082.1.25",
          "form": "와 그러셨군요 ㅠㅠ 문화생활은 정말 좋은것 같아요 저도 보러가고 싶네요 정말 뮤지컬과 연극을 좋아하시나봐요!",
          "original_form": "와 그러셨군요 ㅠㅠ 문화생활은 정말 좋은것 같아요 저도 보러가고 싶네요 정말 뮤지컬과 연극을 좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024082.1.26",
          "form": "어쩌다보니 코가 꿰였죠 ㅎㅎ",
          "original_form": "어쩌다보니 코가 꿰였죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100024082.1.27",
          "form": "어릴때 영화는 영 시큰둥했는데",
          "original_form": "어릴때 영화는 영 시큰둥했는데",
          "speaker_id": "1",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100024082.1.28",
          "form": "무대취향이라 그랬나봐요 ㅋㅋㅋ",
          "original_form": "무대취향이라 그랬나봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:01"
        },
        {
          "id": "MDRW2100024082.1.29",
          "form": "그러시군요 저도 가서 보면 더 몰입도 되고 확 사로 잡호는게 있더라구요",
          "original_form": "그러시군요 저도 가서 보면 더 몰입도 되고 확 사로 잡호는게 있더라구요",
          "speaker_id": "2",
          "time": "20210831 13:09"
        }
      ]
