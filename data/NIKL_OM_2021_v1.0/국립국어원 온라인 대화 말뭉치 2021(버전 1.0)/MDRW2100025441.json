      "utterance": [
        {
          "id": "MDRW2100025441.1.1",
          "form": "가고싶은 여행지 있으세요?",
          "original_form": "가고싶은 여행지 있으세요?",
          "speaker_id": "1",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100025441.1.2",
          "form": "저는 국내여행도 제대로 못해봐서 서울도 가고싶고 제주도도 가고싶어요",
          "original_form": "저는 국내여행도 제대로 못해봐서 서울도 가고싶고 제주도도 가고싶어요",
          "speaker_id": "2",
          "time": "20210901 11:27"
        },
        {
          "id": "MDRW2100025441.1.3",
          "form": "어디로 여행 가고 싶으세요?",
          "original_form": "어디로 여행 가고 싶으세요?",
          "speaker_id": "2",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100025441.1.4",
          "form": "저두요 요즘 제주도가 너무 가보고싶어요",
          "original_form": "저두요 요즘 제주도가 너무 가보고싶어요",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100025441.1.5",
          "form": "돈가스 가게 가보고 싶어요 백종원",
          "original_form": "돈가스 가게 가보고 싶어요 백종원",
          "speaker_id": "1",
          "time": "20210901 11:29"
        },
        {
          "id": "MDRW2100025441.1.6",
          "form": "저도 먹어보고싶네요 아직도 먹기어렵다더라구요",
          "original_form": "저도 먹어보고싶네요 아직도 먹기어렵다더라구요",
          "speaker_id": "2",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025441.1.7",
          "form": "여전히 인기인가봐요",
          "original_form": "여전히 인기인가봐요",
          "speaker_id": "1",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025441.1.8",
          "form": "외국은 어디 가보고싶으신가요",
          "original_form": "외국은 어디 가보고싶으신가요",
          "speaker_id": "1",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025441.1.9",
          "form": "미국이요 볼게 많아보여요",
          "original_form": "미국이요 볼게 많아보여요",
          "speaker_id": "2",
          "time": "20210901 11:39"
        },
        {
          "id": "MDRW2100025441.1.10",
          "form": "와우 저도 가보고싶은데 영어가 딸려서",
          "original_form": "와우 저도 가보고싶은데 영어가 딸려서",
          "speaker_id": "1",
          "time": "20210901 11:40"
        },
        {
          "id": "MDRW2100025441.1.11",
          "form": "디즈니 랜드는 가보고 싶어요",
          "original_form": "디즈니 랜드는 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210901 11:40"
        },
        {
          "id": "MDRW2100025441.1.12",
          "form": "저도 영어때문에 엄두가 나질 않네요 디즈니랜드 가고싶어요",
          "original_form": "저도 영어때문에 엄두가 나질 않네요 디즈니랜드 가고싶어요",
          "speaker_id": "2",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100025441.1.13",
          "form": "다른 나라 가보고싶은데 있으세요?",
          "original_form": "다른 나라 가보고싶은데 있으세요?",
          "speaker_id": "2",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100025441.1.14",
          "form": "무서운거 잘못타는데 용기내서 여러게 타보고 싶어요",
          "original_form": "무서운거 잘못타는데 용기내서 여러게 타보고 싶어요",
          "speaker_id": "1",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100025441.1.15",
          "form": "일본이나 베이징 가보고싶어요",
          "original_form": "일본이나 베이징 가보고싶어요",
          "speaker_id": "1",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100025441.1.16",
          "form": "일본도 디즈니랜드",
          "original_form": "일본도 디즈니랜드",
          "speaker_id": "1",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100025441.1.17",
          "form": "재밌겠네요",
          "original_form": "재밌겠네요",
          "speaker_id": "2",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100025441.1.18",
          "form": "관광명소많아보이는데 먹거리도 그렇고 가면 되게 좋을것같아요",
          "original_form": "관광명소많아보이는데 먹거리도 그렇고 가면 되게 좋을것같아요",
          "speaker_id": "2",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025441.1.19",
          "form": "역시 외국가면 색다른 음식 먹는게 최고",
          "original_form": "역시 외국가면 색다른 음식 먹는게 최고",
          "speaker_id": "1",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025441.1.20",
          "form": "다른나라 어디 가보고싶으세요",
          "original_form": "다른나라 어디 가보고싶으세요",
          "speaker_id": "1",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025441.1.21",
          "form": "스위스요 물가가 비싸다고해서 못가겠지만 풍경이 워낙 좋다고해서요",
          "original_form": "스위스요 물가가 비싸다고해서 못가겠지만 풍경이 워낙 좋다고해서요",
          "speaker_id": "2",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025441.1.22",
          "form": "이야 사진으로만 봐도 평화롭던데요",
          "original_form": "이야 사진으로만 봐도 평화롭던데요",
          "speaker_id": "1",
          "time": "20210901 11:50"
        }
      ]
    }
