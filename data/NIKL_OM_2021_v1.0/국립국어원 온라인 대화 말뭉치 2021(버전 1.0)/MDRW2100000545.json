      "utterance": [
        {
          "id": "MDRW2100000545.1.1",
          "form": "스포츠 좋아해??",
          "original_form": "스포츠 좋아해??",
          "speaker_id": "1",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000545.1.2",
          "form": "보는 거 좋아해!",
          "original_form": "보는 거 좋아해!",
          "speaker_id": "2",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000545.1.3",
          "form": "이번에 올림픽도 너무 재밌었어",
          "original_form": "이번에 올림픽도 너무 재밌었어",
          "speaker_id": "2",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000545.1.4",
          "form": "난 배구랑 양궁만봐써",
          "original_form": "난 배구랑 양궁만봐써",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000545.1.5",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000545.1.6",
          "form": "거의 다 챙겨봤지!",
          "original_form": "거의 다 챙겨봤지!",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000545.1.7",
          "form": "양궁 진짜 재밌었지",
          "original_form": "양궁 진짜 재밌었지",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000545.1.8",
          "form": "높이뛰기는? 안 봤어?",
          "original_form": "높이뛰기는? 안 봤어?",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000545.1.9",
          "form": "아 높이뛰기!!결승만봐써",
          "original_form": "아 높이뛰기!!결승만봐써",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000545.1.10",
          "form": "진짜 대박이였음",
          "original_form": "진짜 대박이였음",
          "speaker_id": "1",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000545.1.11",
          "form": "진짜 서서 박수치면서 봤잖아",
          "original_form": "진짜 서서 박수치면서 봤잖아",
          "speaker_id": "2",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000545.1.12",
          "form": "운동하는 사람들 너무 멋있어",
          "original_form": "운동하는 사람들 너무 멋있어",
          "speaker_id": "2",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000545.1.13",
          "form": "그니까 나도 운동하고싶어졌자나",
          "original_form": "그니까 나도 운동하고싶어졌자나",
          "speaker_id": "1",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000545.1.14",
          "form": "몸 탄탄도 부럽고 열정도 부럽고!!",
          "original_form": "몸 탄탄도 부럽고 열정도 부럽고!!",
          "speaker_id": "2",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000545.1.15",
          "form": "마자 진짜ㅠㅠ\r\n넌 무슨운동해",
          "original_form": "마자 진짜ㅠㅠ\r\n넌 무슨운동해",
          "speaker_id": "1",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000545.1.16",
          "form": "나는 수영도 오래했고",
          "original_form": "나는 수영도 오래했고",
          "speaker_id": "2",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000545.1.17",
          "form": "필라테스 시작했는데 코로나 핑계로 안 간지 오백년..",
          "original_form": "필라테스 시작했는데 코로나 핑계로 안 간지 오백년..",
          "speaker_id": "2",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000545.1.18",
          "form": "우와 꾸준히하는구남",
          "original_form": "우와 꾸준히하는구남",
          "speaker_id": "1",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000545.1.19",
          "form": "아니야..안 간지 오백년",
          "original_form": "아니야..안 간지 오백년",
          "speaker_id": "2",
          "time": "20210819 17:54"
        },
        {
          "id": "MDRW2100000545.1.20",
          "form": "꾸준히가 아니지요",
          "original_form": "꾸준히가 아니지요",
          "speaker_id": "2",
          "time": "20210819 17:54"
        },
        {
          "id": "MDRW2100000545.1.21",
          "form": "다시 시작하려니 어유 엄두가 안 나",
          "original_form": "다시 시작하려니 어유 엄두가 안 나",
          "speaker_id": "2",
          "time": "20210819 17:54"
        },
        {
          "id": "MDRW2100000545.1.22",
          "form": "수영말한거여써ㅠ",
          "original_form": "수영말한거여써ㅠ",
          "speaker_id": "1",
          "time": "20210819 17:54"
        },
        {
          "id": "MDRW2100000545.1.23",
          "form": "요즘은 잘 못하지ㅠㅠ",
          "original_form": "요즘은 잘 못하지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000545.1.24",
          "form": "아하 수영은 맞아 이십년째 하고 있어",
          "original_form": "아하 수영은 맞아 이십년째 하고 있어",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000545.1.25",
          "form": "꾸준히 맞아 정답!",
          "original_form": "꾸준히 맞아 정답!",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000545.1.26",
          "form": "나도 필라테스하다가 코로나때문에 수업축소되고그러니까 예약도 어려워서",
          "original_form": "나도 필라테스하다가 코로나때문에 수업축소되고그러니까 예약도 어려워서",
          "speaker_id": "1",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000545.1.27",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000545.1.28",
          "form": "안 가게 되더라고",
          "original_form": "안 가게 되더라고",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000545.1.29",
          "form": "그냥 홈트해야지하고선 숨쉬기만한닼ㅋ",
          "original_form": "그냥 홈트해야지하고선 숨쉬기만한닼ㅋ",
          "speaker_id": "1",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100000545.1.30",
          "form": "홈트...",
          "original_form": "홈트...",
          "speaker_id": "2",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100000545.1.31",
          "form": "누가 옆에서 뭐라고 해야지 하는 사람이야 나는",
          "original_form": "누가 옆에서 뭐라고 해야지 하는 사람이야 나는",
          "speaker_id": "2",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100000545.1.32",
          "form": "혼자는...어려워",
          "original_form": "혼자는...어려워",
          "speaker_id": "2",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100000545.1.33",
          "form": "나도ㅠㅠ",
          "original_form": "나도ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 17:58"
        }
      ]
    }
  ]
