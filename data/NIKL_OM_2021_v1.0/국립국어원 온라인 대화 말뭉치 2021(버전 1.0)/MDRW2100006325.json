      "utterance": [
        {
          "id": "MDRW2100006325.1.1",
          "form": "집안일중에 뭘 제일 싫어하세요?",
          "original_form": "집안일중에 뭘 제일 싫어하세요?",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006325.1.2",
          "form": "저는 음...",
          "original_form": "저는 음...",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006325.1.3",
          "form": "빨래 개기요",
          "original_form": "빨래 개기요",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006325.1.4",
          "form": "빨래는 세탁기 건조는 건조기 설거지는 식세기 청소는 청소기",
          "original_form": "빨래는 세탁기 건조는 건조기 설거지는 식세기 청소는 청소기",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006325.1.5",
          "form": "그런데 빨래 개주는 기계가 없어요",
          "original_form": "그런데 빨래 개주는 기계가 없어요",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006325.1.6",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006325.1.7",
          "form": "저도 빨래개기가 제일싫어요",
          "original_form": "저도 빨래개기가 제일싫어요",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006325.1.8",
          "form": "그쵸 예쁘게도 안개지고",
          "original_form": "그쵸 예쁘게도 안개지고",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006325.1.9",
          "form": "너무 귀찮아요",
          "original_form": "너무 귀찮아요",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006325.1.10",
          "form": "name2님은 집안일 분담하시나요?",
          "original_form": "&name2&님은 집안일 분담하시나요?",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006325.1.11",
          "form": "아 저는 혼자 살아요",
          "original_form": "아 저는 혼자 살아요",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006325.1.12",
          "form": "아그럼 혼자다해야하는군여 힘들겠다",
          "original_form": "아그럼 혼자다해야하는군여 힘들겠다",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100006325.1.13",
          "form": "자취하세요?",
          "original_form": "자취하세요?",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100006325.1.14",
          "form": "네 ㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100006325.1.15",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100006325.1.16",
          "form": "혼자산지 오래되어 익숙해요",
          "original_form": "혼자산지 오래되어 익숙해요",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100006325.1.17",
          "form": "아니요 직장인입니다",
          "original_form": "아니요 직장인입니다",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100006325.1.18",
          "form": "아하 그렇구나",
          "original_form": "아하 그렇구나",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100006325.1.19",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100006325.1.20",
          "form": "저는 학생때 잠깐 자취하고 쭉 기숙사 살다가 19년부터 자취하다가",
          "original_form": "저는 학생때 잠깐 자취하고 쭉 기숙사 살다가 19년부터 자취하다가",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100006325.1.21",
          "form": "작년에 결혼해서 이제 둘이 살아요",
          "original_form": "작년에 결혼해서 이제 둘이 살아요",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100006325.1.22",
          "form": "오 저도 기숙사생활햇엇어요",
          "original_form": "오 저도 기숙사생활햇엇어요",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100006325.1.23",
          "form": "기숙사 오래 사셧으면 룸메 여러번 바뀌셧겟네요",
          "original_form": "기숙사 오래 사셧으면 룸메 여러번 바뀌셧겟네요",
