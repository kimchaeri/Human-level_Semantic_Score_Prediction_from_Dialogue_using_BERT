      "utterance": [
        {
          "id": "MDRW2100028259.1.1",
          "form": "만화잡지 많이 읽었는데",
          "original_form": "만화잡지 많이 읽었는데",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028259.1.2",
          "form": "전 만화보단 베스트셀러",
          "original_form": "전 만화보단 베스트셀러",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028259.1.3",
          "form": "잡지는 주로 미용실에서 봐요",
          "original_form": "잡지는 주로 미용실에서 봐요",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028259.1.4",
          "form": "그건 그냥 시간떼우기잖아요",
          "original_form": "그건 그냥 시간떼우기잖아요",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028259.1.5",
          "form": "전 소설이 그나마 잘 읽혀요",
          "original_form": "전 소설이 그나마 잘 읽혀요",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028259.1.6",
          "form": "책안보실거같은데....",
          "original_form": "책안보실거같은데....",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028259.1.7",
          "form": "조아하는 작가 있으세요?",
          "original_form": "조아하는 작가 있으세요?",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028259.1.8",
          "form": "음 앙드레코스톨라니요",
          "original_form": "음 앙드레코스톨라니요",
          "speaker_id": "1",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028259.1.9",
          "form": "전 하루키요",
          "original_form": "전 하루키요",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028259.1.10",
          "form": "오.. 일본작가네요",
          "original_form": "오.. 일본작가네요",
          "speaker_id": "1",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028259.1.11",
          "form": "넹 하루키 책은 에세이도 읽어봤어요",
          "original_form": "넹 하루키 책은 에세이도 읽어봤어요",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028259.1.12",
          "form": "네 저는 이십여년 전 상실의시대. 십여년전 일큐팔사정도",
          "original_form": "네 저는 이십여년 전 상실의시대. 십여년전 일큐팔사정도",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028259.1.13",
          "form": "책은 주로 사서 읽으시나요",
          "original_form": "책은 주로 사서 읽으시나요",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028259.1.14",
          "form": "네 제 돈으로는 안 사구요",
          "original_form": "네 제 돈으로는 안 사구요",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028259.1.15",
          "form": "저는 빌려서 읽어요",
          "original_form": "저는 빌려서 읽어요",
          "speaker_id": "2",
          "time": "20210902 21:53"
        }
