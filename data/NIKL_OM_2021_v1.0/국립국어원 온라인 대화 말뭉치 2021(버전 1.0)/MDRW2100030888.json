          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100030888.1.14",
          "form": "청소하시나요?",
          "original_form": "청소하시나요?",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100030888.1.15",
          "form": "저는 청소를 잘 안해요",
          "original_form": "저는 청소를 잘 안해요",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100030888.1.16",
          "form": "게으르신 편인가요?",
          "original_form": "게으르신 편인가요?",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100030888.1.17",
          "form": "네 뭐 그렇다면 그런거 같네요",
