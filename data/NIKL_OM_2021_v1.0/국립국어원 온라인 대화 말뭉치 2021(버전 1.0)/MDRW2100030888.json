        {
          "id": "MDRW2100030888.1.2",
          "form": "뭐가요?",
          "original_form": "뭐가요?",
          "speaker_id": "2",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030888.1.3",
          "form": "앗 아니에요",
          "original_form": "앗 아니에요",
          "speaker_id": "1",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030888.1.4",
          "form": "가족들이랑 같이 살고 계신가요",
          "original_form": "가족들이랑 같이 살고 계신가요",
          "speaker_id": "2",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100030888.1.5",
          "form": "아니요 저는 혼자 살아요",
          "original_form": "아니요 저는 혼자 살아요",
          "speaker_id": "1",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100030888.1.6",
          "form": "외로우시겠네요",
          "original_form": "외로우시겠네요",
          "speaker_id": "2",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100030888.1.7",
          "form": "네 시집 가려고요",
          "original_form": "네 시집 가려고요",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100030888.1.8",
          "form": "남자친구 분이 맘에 드시는군요",
          "original_form": "남자친구 분이 맘에 드시는군요",
          "speaker_id": "2",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100030888.1.9",
          "form": "집에서 보통 집안일은 하시나요",
          "original_form": "집에서 보통 집안일은 하시나요",
          "speaker_id": "2",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100030888.1.10",
          "form": "새로 구래서 할 거예요",
          "original_form": "새로 구래서 할 거예요",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100030888.1.11",
          "form": "오늘 대청소했어요",
          "original_form": "오늘 대청소했어요",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100030888.1.12",
          "form": "그렇군요",
          "original_form": "그렇군요",
