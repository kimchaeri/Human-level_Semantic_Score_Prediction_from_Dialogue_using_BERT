      "utterance": [
        {
          "id": "MDRW2100019571.1.1",
          "form": "학교다닐때로",
          "original_form": "학교다닐때로",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019571.1.2",
          "form": "돌아가고싶단생각하니",
          "original_form": "돌아가고싶단생각하니",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019571.1.3",
          "form": "음...학교생활 자체가 그립다기보단",
          "original_form": "음...학교생활 자체가 그립다기보단",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019571.1.4",
          "form": "그때로 돌아가서 선택을 바꾸고 싶은거라",
          "original_form": "그때로 돌아가서 선택을 바꾸고 싶은거라",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019571.1.5",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019571.1.6",
          "form": "언제로?",
          "original_form": "언제로?",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019571.1.7",
          "form": "고등학교?",
          "original_form": "고등학교?",
          "speaker_id": "2",
          "time": "20210828 12:21"
        },
        {
          "id": "MDRW2100019571.1.8",
          "form": "대학선택인가?",
          "original_form": "대학선택인가?",
          "speaker_id": "1",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100019571.1.9",
          "form": "과선택?",
          "original_form": "과선택?",
          "speaker_id": "1",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100019571.1.10",
          "form": "ㅇㅇ 그렇지",
          "original_form": "ㅇㅇ 그렇지",
          "speaker_id": "2",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100019571.1.11",
          "form": "너는 어떤데? 돌아가고 싶음?",
          "original_form": "너는 어떤데? 돌아가고 싶음?",
          "speaker_id": "2",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100019571.1.12",
          "form": "응 나도 대학이나 과",
          "original_form": "응 나도 대학이나 과",
          "speaker_id": "1",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100019571.1.13",
          "form": "바꾸고싶단생각함",
          "original_form": "바꾸고싶단생각함",
          "speaker_id": "1",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100019571.1.14",
          "form": "ㅋㅋㅋ아니면대학안가고",
          "original_form": "ㅋㅋㅋ아니면대학안가고",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100019571.1.15",
          "form": "공무원준비나..",
          "original_form": "공무원준비나..",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100019571.1.16",
          "form": "넘찌들었다",
          "original_form": "넘찌들었다",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100019571.1.17",
          "form": "예체능말고 다른 문이과",
          "original_form": "예체능말고 다른 문이과",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100019571.1.18",
          "form": "가보고싶단생각은 안해봄?",
          "original_form": "가보고싶단생각은 안해봄?",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100019571.1.19",
          "form": "음 글쎄 근데 또 막상 문이과 생각은 별로 안들긴함",
          "original_form": "음 글쎄 근데 또 막상 문이과 생각은 별로 안들긴함",
          "speaker_id": "2",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100019571.1.20",
          "form": "그냥 디자인 말고 서양화과 가고싶어 ㅋㅋㅋ",
          "original_form": "그냥 디자인 말고 서양화과 가고싶어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100019571.1.21",
          "form": "ㅋㅋㅋ오 신기하다",
          "original_form": "ㅋㅋㅋ오 신기하다",
          "speaker_id": "1",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100019571.1.22",
          "form": "이과 쪽은 너무 체질에 안맞고..",
          "original_form": "이과 쪽은 너무 체질에 안맞고..",
          "speaker_id": "2",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100019571.1.23",
          "form": "ㅋㅋㅋ맞지맞지",
          "original_form": "ㅋㅋㅋ맞지맞지",
          "speaker_id": "1",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100019571.1.24",
          "form": "문과라면 언어나 사학쪽이 관심 분야긴 한데",
          "original_form": "문과라면 언어나 사학쪽이 관심 분야긴 한데",
          "speaker_id": "2",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100019571.1.25",
          "form": "그런데는 취업이 어렵쟝",
          "original_form": "그런데는 취업이 어렵쟝",
          "speaker_id": "2",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100019571.1.26",
          "form": "으악ㅋㅋㅋ",
          "original_form": "으악ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100019571.1.27",
          "form": "찌들었어",
          "original_form": "찌들었어",
          "speaker_id": "1",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100019571.1.28",
          "form": "대학은 갔던거 후회안해?",
          "original_form": "대학은 갔던거 후회안해?",
          "speaker_id": "1",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100019571.1.29",
          "form": "대학갔던거 한참 후회하던 시기가 있었는데",
          "original_form": "대학갔던거 한참 후회하던 시기가 있었는데",
          "speaker_id": "2",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100019571.1.30",
          "form": "지금은 그냥 수긍했어",
          "original_form": "지금은 그냥 수긍했어",
          "speaker_id": "2",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100019571.1.31",
          "form": "그랬구만",
          "original_form": "그랬구만",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100019571.1.32",
          "form": "대학은 그래도 추억이라도 남은거 같아서..",
          "original_form": "대학은 그래도 추억이라도 남은거 같아서..",
          "speaker_id": "2",
          "time": "20210828 12:39"
        }
      ]
    }
  ]
