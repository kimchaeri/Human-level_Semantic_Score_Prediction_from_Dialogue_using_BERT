      "utterance": [
        {
          "id": "MDRW2100046494.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211021 21:06"
        },
        {
          "id": "MDRW2100046494.1.2",
          "form": "아침 드셨나요~?",
          "original_form": "아침 드셨나요~?",
          "speaker_id": "2",
          "time": "20211022 10:35"
        },
        {
          "id": "MDRW2100046494.1.3",
          "form": "아니요 아침대신 커피 마셨습니다",
          "original_form": "아니요 아침대신 커피 마셨습니다",
          "speaker_id": "1",
          "time": "20211022 10:37"
        },
        {
          "id": "MDRW2100046494.1.4",
          "form": "아침은 부담스러워서요",
          "original_form": "아침은 부담스러워서요",
          "speaker_id": "1",
          "time": "20211022 10:37"
        },
        {
          "id": "MDRW2100046494.1.5",
          "form": "헉 빈속에 커피마시면 안좋은데 ㅠㅠ",
          "original_form": "헉 빈속에 커피마시면 안좋은데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211022 10:41"
        },
        {
          "id": "MDRW2100046494.1.6",
          "form": "어떤 커피 좋아하세요?",
          "original_form": "어떤 커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20211022 10:41"
        },
        {
          "id": "MDRW2100046494.1.7",
          "form": "맥심 믹스커피가 최고죠",
          "original_form": "맥심 믹스커피가 최고죠",
          "speaker_id": "1",
          "time": "20211022 10:45"
        },
        {
          "id": "MDRW2100046494.1.8",
          "form": "달달하니",
          "original_form": "달달하니",
          "speaker_id": "1",
          "time": "20211022 10:45"
        },
        {
          "id": "MDRW2100046494.1.9",
          "form": "끊을수 없는 맛입니다",
          "original_form": "끊을수 없는 맛입니다",
          "speaker_id": "1",
          "time": "20211022 10:45"
        },
        {
          "id": "MDRW2100046494.1.10",
          "form": "아 직접 타드시는 군요 ㅋㅋㅋ",
          "original_form": "아 직접 타드시는 군요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211022 11:04"
        },
        {
          "id": "MDRW2100046494.1.11",
          "form": "요즘은 가루 커피도 종류가 진짜 많더라구요",
          "original_form": "요즘은 가루 커피도 종류가 진짜 많더라구요",
          "speaker_id": "2",
          "time": "20211022 11:04"
        },
        {
          "id": "MDRW2100046494.1.12",
          "form": "아메리카노는 안 드세요?",
          "original_form": "아메리카노는 안 드세요?",
          "speaker_id": "2",
          "time": "20211022 11:04"
        },
        {
          "id": "MDRW2100046494.1.13",
          "form": "아메리카노는 써서 안마셔요",
          "original_form": "아메리카노는 써서 안마셔요",
          "speaker_id": "1",
          "time": "20211023 00:36"
        },
        {
          "id": "MDRW2100046494.1.14",
          "form": "저는 오직 맥심입니다 ㅋㅋㅋ",
          "original_form": "저는 오직 맥심입니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211023 00:36"
        },
        {
          "id": "MDRW2100046494.1.15",
          "form": "ㅋㅋㅋㅋ회사 탕비실에 맥심이 항상 있나봐요",
          "original_form": "ㅋㅋㅋㅋ회사 탕비실에 맥심이 항상 있나봐요",
          "speaker_id": "2",
          "time": "20211023 00:47"
        },
        {
          "id": "MDRW2100046494.1.16",
          "form": "네 맥심 모카만 잔뜩 쌓여 있어요 남자들이 많은 회사라서 다들 맥심파입니다",
          "original_form": "네 맥심 모카만 잔뜩 쌓여 있어요 남자들이 많은 회사라서 다들 맥심파입니다",
          "speaker_id": "1",
          "time": "20211022 13:15"
        },
        {
          "id": "MDRW2100046494.1.17",
          "form": "그렇군요 ㅋㅋㅋㅋ 과자같은 건 잘 안 드세요?",
          "original_form": "그렇군요 ㅋㅋㅋㅋ 과자같은 건 잘 안 드세요?",
          "speaker_id": "2",
          "time": "20211022 13:56"
        },
        {
          "id": "MDRW2100046494.1.18",
          "form": "과자도 좋아합니다  과자 아이스크림 담배 술 아주 많이요",
          "original_form": "과자도 좋아합니다  과자 아이스크림 담배 술 아주 많이요",
          "speaker_id": "1",
          "time": "20211022 14:30"
        },
        {
          "id": "MDRW2100046494.1.19",
          "form": "헉 ㅋㅋㅋㅋ 그래도 건강 생각하면서 드세요!!!",
          "original_form": "헉 ㅋㅋㅋㅋ 그래도 건강 생각하면서 드세요!!!",
