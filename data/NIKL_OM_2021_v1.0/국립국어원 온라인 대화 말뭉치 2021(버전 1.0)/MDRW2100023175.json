      "utterance": [
        {
          "id": "MDRW2100023175.1.1",
          "form": "의사쌤이 저 고통을 잘 참는 편이라고 해주셧는데",
          "original_form": "의사쌤이 저 고통을 잘 참는 편이라고 해주셧는데",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023175.1.2",
          "form": "오 그래요~?",
          "original_form": "오 그래요~?",
          "speaker_id": "1",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023175.1.3",
          "form": "근데도 진짜 아프긴햇어요",
          "original_form": "근데도 진짜 아프긴햇어요",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023175.1.4",
          "form": "머 피부과에서 여드름 짜고 이럼 엄청 소리지르는 사람들도 잇는데",
          "original_form": "머 피부과에서 여드름 짜고 이럼 엄청 소리지르는 사람들도 잇는데",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023175.1.5",
          "form": "ㅠㅠ아 그거 진짜 무서워요",
          "original_form": "ㅠㅠ아 그거 진짜 무서워요",
          "speaker_id": "1",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023175.1.6",
          "form": "전 그냥 눈물로 고통을 삭히며 ㅠㅠ",
          "original_form": "전 그냥 눈물로 고통을 삭히며 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023175.1.7",
          "form": "저 위내시경 할 때도 수면 안했는데",
          "original_form": "저 위내시경 할 때도 수면 안했는데",
          "speaker_id": "1",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023175.1.8",
          "form": "이정도면 도전 가능할까요..?",
          "original_form": "이정도면 도전 가능할까요..?",
          "speaker_id": "1",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023175.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ네",
          "speaker_id": "2",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023175.1.10",
          "form": "근데 프락셀 영상 보니까 제가 한거 프락셀 아닌 거 같아요",
          "original_form": "근데 프락셀 영상 보니까 제가 한거 프락셀 아닌 거 같아요",
          "speaker_id": "1",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023175.1.11",
          "form": "저도 수면없이 위내시경햇는데 진짜 고통이던데",
          "original_form": "저도 수면없이 위내시경햇는데 진짜 고통이던데",
          "speaker_id": "2",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023175.1.12",
          "form": "피부과에서 저 속였나본데요 ㅠㅠ",
          "original_form": "피부과에서 저 속였나본데요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023175.1.13",
          "form": "오잉",
          "original_form": "오잉",
          "speaker_id": "2",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023175.1.14",
          "form": "그럼 뭐져",
          "original_form": "그럼 뭐져",
          "speaker_id": "2",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023175.1.15",
          "form": "모르겠어요 ㅜㅜ 고등학생이라고 암거나 했나봐요",
          "original_form": "모르겠어요 ㅜㅜ 고등학생이라고 암거나 했나봐요",
          "speaker_id": "1",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023175.1.16",
          "form": "요즘 기계가 새로 나온거 아닐가여",
          "original_form": "요즘 기계가 새로 나온거 아닐가여",
          "speaker_id": "2",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023175.1.17",
          "form": "그런가..ㅜㅜ",
          "original_form": "그런가..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023175.1.18",
          "form": "프락셀은 어우 진짜 못하겟다",
          "original_form": "프락셀은 어우 진짜 못하겟다",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023175.1.19",
          "form": "돈만 많으면 피부과 매일 다니고싶네요",
          "original_form": "돈만 많으면 피부과 매일 다니고싶네요",
          "speaker_id": "1",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023175.1.20",
          "form": "그러게여ㅋㅋㅋㅋ",
          "original_form": "그러게여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023175.1.21",
          "form": "화장품 뭐ㅜ이런거 다 부질없이",
          "original_form": "화장품 뭐ㅜ이런거 다 부질없이",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023175.1.22",
          "form": "돈은 돈대로 썼는데 개선 안되면...",
          "original_form": "돈은 돈대로 썼는데 개선 안되면...",
          "speaker_id": "1",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023175.1.23",
          "form": "말 그대로 돈낭비....",
          "original_form": "말 그대로 돈낭비....",
          "speaker_id": "1",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023175.1.24",
          "form": "유전과 피부과 이게 짱",
          "original_form": "유전과 피부과 이게 짱",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023175.1.25",
          "form": "선천적으로 피부 타고나신 분들 너무 부러워요",
          "original_form": "선천적으로 피부 타고나신 분들 너무 부러워요",
          "speaker_id": "1",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023175.1.26",
          "form": "저도 유전을 잘못받아서",
          "original_form": "저도 유전을 잘못받아서",
          "speaker_id": "2",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023175.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023175.1.28",
          "form": "왜이랗게 기름지게 태어난것인가 ㅠㅠㅠ",
          "original_form": "왜이랗게 기름지게 태어난것인가 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023175.1.29",
          "form": "저는 제 습관이 저를 이렇게 만들었어요 ㅜ",
          "original_form": "저는 제 습관이 저를 이렇게 만들었어요 ㅜ",
          "speaker_id": "1",
          "time": "20210829 22:33"
        }
      ]
    }
