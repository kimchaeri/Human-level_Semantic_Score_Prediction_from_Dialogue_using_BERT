      "utterance": [
        {
          "id": "MDRW2100004743.1.1",
          "form": "비가 많이오죠?",
          "original_form": "비가 많이오죠?",
          "speaker_id": "1",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004743.1.2",
          "form": "비 많이오죠?",
          "original_form": "비 많이오죠?",
          "speaker_id": "1",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004743.1.3",
          "form": "네 많이 왔다가 지금은 비가 그춌어요",
          "original_form": "네 많이 왔다가 지금은 비가 그춌어요",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004743.1.4",
          "form": "그쳤어요",
          "original_form": "그쳤어요",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004743.1.5",
          "form": "저희동네는 아직도 비가 많이와요ㅜㅜ",
          "original_form": "저희동네는 아직도 비가 많이와요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004743.1.6",
          "form": "저는 경기도이데 아침에 비가 엄청 오다가 지금은 그쳤어요",
          "original_form": "저는 경기도이데 아침에 비가 엄청 오다가 지금은 그쳤어요",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004743.1.7",
          "form": "아까는 진짜 비가 무섭게 내리더라고요",
          "original_form": "아까는 진짜 비가 무섭게 내리더라고요",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004743.1.8",
          "form": "와 진짜요 넘 비가많이와서 깜짝놀랬어요 가을장마라더니",
          "original_form": "와 진짜요 넘 비가많이와서 깜짝놀랬어요 가을장마라더니",
          "speaker_id": "1",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004743.1.9",
          "form": "오늘이 그나마 주말이라 집에있어서 다행이에요",
          "original_form": "오늘이 그나마 주말이라 집에있어서 다행이에요",
          "speaker_id": "2",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004743.1.10",
          "form": "날씨가 이럴땐 집이최고죠",
          "original_form": "날씨가 이럴땐 집이최고죠",
          "speaker_id": "2",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004743.1.11",
          "form": "네네 저도 어짜피 집에 있을 예정이라 ㅎㅎ 집콕했어요",
          "original_form": "네네 저도 어짜피 집에 있을 예정이라 ㅎㅎ 집콕했어요",
          "speaker_id": "1",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004743.1.12",
          "form": "비올땐 집에 있는게 좋아요 ㅎ",
          "original_form": "비올땐 집에 있는게 좋아요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004743.1.13",
          "form": "전아까 비와서 부침개도 해먹었어요",
          "original_form": "전아까 비와서 부침개도 해먹었어요",
          "speaker_id": "2",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004743.1.14",
          "form": "엇 저 지금 감자전 하는중이에요 ㅋㅋ 비올땐 부침개가 최고인듯해요ㅋㅋ",
          "original_form": "엇 저 지금 감자전 하는중이에요 ㅋㅋ 비올땐 부침개가 최고인듯해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004743.1.15",
          "form": "맞아요 ㅎ 비올땐  부침개죠",
          "original_form": "맞아요 ㅎ 비올땐  부침개죠",
          "speaker_id": "2",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100004743.1.16",
          "form": "전 저녁에도 부쳤어요",
          "original_form": "전 저녁에도 부쳤어요",
          "speaker_id": "2",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100004743.1.17",
          "form": "ㅋㅋ 내일도 먹을거같아요 ㅋㅋ",
          "original_form": "ㅋㅋ 내일도 먹을거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100004743.1.18",
          "form": "내일도 비온대요??",
          "original_form": "내일도 비온대요??",
          "speaker_id": "2",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100004743.1.19",
          "form": "저희동네는 비소식 있더라구요ㅠ 다음주도 ...",
          "original_form": "저희동네는 비소식 있더라구요ㅠ 다음주도 ...",
          "speaker_id": "1",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100004743.1.20",
          "form": "저희동네도요",
          "original_form": "저희동네도요",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100004743.1.21",
          "form": "다음주 내내 비소식이더라고요",
          "original_form": "다음주 내내 비소식이더라고요",
