      "utterance": [
        {
          "id": "MDRW2100036526.1.1",
          "form": "그니까욬ㅋㅋㅋㅋㅋ",
          "original_form": "그니까욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036526.1.2",
          "form": "억울억울 ㅎㅎㅎ",
          "original_form": "억울억울 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036526.1.3",
          "form": "차라리 갔다와서 바로 팔았으면 억울하지도 않지ㅠㅠ",
          "original_form": "차라리 갔다와서 바로 팔았으면 억울하지도 않지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036526.1.4",
          "form": "보관은 그렇게 다해놓고!!",
          "original_form": "보관은 그렇게 다해놓고!!",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036526.1.5",
          "form": "한번써서 진짜 깨끗한 그걸 헐값에 팔아놓고나니!!!",
          "original_form": "한번써서 진짜 깨끗한 그걸 헐값에 팔아놓고나니!!!",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036526.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ 그래도 8년 동안 갖고 있었어도 보관 잘 하셨나봐요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그래도 8년 동안 갖고 있었어도 보관 잘 하셨나봐요",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036526.1.7",
          "form": "",
          "original_form": "",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036526.1.8",
          "form": "중고로 다시 파신걸 보면 ㅎ",
          "original_form": "중고로 다시 파신걸 보면 ㅎ",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036526.1.9",
          "form": "그 배송온 박스랑 비닐 가지고 있어서 쓰고 고대로 넣어놨었거든요",
          "original_form": "그 배송온 박스랑 비닐 가지고 있어서 쓰고 고대로 넣어놨었거든요",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036526.1.10",
          "form": "오 ㅋㅋㅋㅋㅋ 박스까지....",
          "original_form": "오 ㅋㅋㅋㅋㅋ 박스까지....",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036526.1.11",
          "form": "진짜... 오버하면 새거였어요 완전 ㅠㅠ",
          "original_form": "진짜... 오버하면 새거였어요 완전 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036526.1.12",
          "form": "자리 많이 차지하셨었겠네요ㅠㅠ",
          "original_form": "자리 많이 차지하셨었겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036526.1.13",
          "form": "넼ㅋㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036526.1.14",
          "form": "그쳐ㅠ 산사람은 횡재였을듯",
          "original_form": "그쳐ㅠ 산사람은 횡재였을듯",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036526.1.15",
          "form": "그쳐......",
          "original_form": "그쳐......",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036526.1.16",
          "form": "유럽여행 가는거 같더라구여....",
          "original_form": "유럽여행 가는거 같더라구여....",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036526.1.17",
          "form": "직거래했는데 그 사람도 보더니 생각보다 너무 깨끗하다고",
          "original_form": "직거래했는데 그 사람도 보더니 생각보다 너무 깨끗하다고",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036526.1.18",
          "form": "가격 다시 물어보더라구요...",
          "original_form": "가격 다시 물어보더라구요...",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036526.1.19",
          "form": "그럼 좀 더 주시지ㅜㅜ",
          "original_form": "그럼 좀 더 주시지ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036526.1.20",
          "form": "꺼이꺼이 ㅋㅋㅋ",
          "original_form": "꺼이꺼이 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036526.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 너무웃기네요 ㅎㅎ 전 중고파는데 제가 젤 싸게 내놨는데\n돈 들고와놓고 깍아달라고 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 너무웃기네요 ㅎㅎ 전 중고파는데 제가 젤 싸게 내놨는데\n돈 들고와놓고 깍아달라고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036526.1.22",
          "form": "누가깍아줘요 그돈 꺼내놓고",
          "original_form": "누가깍아줘요 그돈 꺼내놓고",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036526.1.23",
          "form": "이야기하면 ㅎㅎ 안되용! 하니 쿨하게 주시고 가심 ㅋㅋ 당근 한번씩 하면 잼나요",
          "original_form": "이야기하면 ㅎㅎ 안되용! 하니 쿨하게 주시고 가심 ㅋㅋ 당근 한번씩 하면 잼나요",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036526.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ저도 요즘은 연락이 없는데 가끔씩 판매하러 나가거든요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도 요즘은 연락이 없는데 가끔씩 판매하러 나가거든요",
          "speaker_id": "2",
          "time": "20210831 00:11"
        }
      ]
