      "utterance": [
        {
          "id": "MDRW2100039217.1.1",
          "form": "넹 ㅜㅠ 집 꾸미고싶어용 ㅜㅜㅜ",
          "original_form": "넹 ㅜㅠ 집 꾸미고싶어용 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 14:07"
        },
        {
          "id": "MDRW2100039217.1.2",
          "form": "오늘의 집 쓰시나요?",
          "original_form": "오늘의 집 쓰시나요?",
          "speaker_id": "2",
          "time": "20210903 14:07"
        },
        {
          "id": "MDRW2100039217.1.3",
          "form": "처음 자취 시작할때 썼었어요",
          "original_form": "처음 자취 시작할때 썼었어요",
          "speaker_id": "1",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100039217.1.4",
          "form": "그거보다보면 다들 진짜 센스 쩔어요",
          "original_form": "그거보다보면 다들 진짜 센스 쩔어요",
          "speaker_id": "1",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100039217.1.5",
          "form": "저도 한번 깔아볼까봐요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 한번 깔아볼까봐요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100039217.1.6",
          "form": "ㅋㅋㅋname2님 깔면 또 엄청 사고싶은거 많아지실것같아요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ&name2&님 깔면 또 엄청 사고싶은거 많아지실것같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100039217.1.7",
          "form": "그리구 후기보다보면 다 팔랑거려요 ㅋㅋㅋ",
          "original_form": "그리구 후기보다보면 다 팔랑거려요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100039217.1.8",
          "form": "그니깐여 ㅜㅜ 돈 펑펑쓸까봐 못깔겠네요 지금 사고싶은건",
          "original_form": "그니깐여 ㅜㅜ 돈 펑펑쓸까봐 못깔겠네요 지금 사고싶은건",
          "speaker_id": "2",
          "time": "20210903 16:18"
        },
        {
          "id": "MDRW2100039217.1.9",
          "form": "좀 지저분한거 덮을 큰 천이랑",
          "original_form": "좀 지저분한거 덮을 큰 천이랑",
          "speaker_id": "2",
          "time": "20210903 16:18"
        },
        {
          "id": "MDRW2100039217.1.10",
          "form": "그위에 둘 반신거울...",
          "original_form": "그위에 둘 반신거울...",
          "speaker_id": "2",
          "time": "20210903 16:18"
        },
        {
          "id": "MDRW2100039217.1.11",
          "form": "아 지저분한거가 단단한 무언가에요?",
          "original_form": "아 지저분한거가 단단한 무언가에요?",
          "speaker_id": "1",
          "time": "20210903 16:29"
        },
        {
          "id": "MDRW2100039217.1.12",
          "form": "딱 필요한거만 사기에는 괜찮을것같은데 ㅋㅋㅋ",
          "original_form": "딱 필요한거만 사기에는 괜찮을것같은데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039217.1.13",
          "form": "가서 다른거에 눈돌리지말아요 ㅋㅋㅋ",
          "original_form": "가서 다른거에 눈돌리지말아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039217.1.14",
          "form": "네네 예전에 산 서랍장인데",
          "original_form": "네네 예전에 산 서랍장인데",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039217.1.15",
          "form": "싸구려거든요",
          "original_form": "싸구려거든요",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039217.1.16",
          "form": "그래서 걍 가리고 싶어요",
          "original_form": "그래서 걍 가리고 싶어요",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039217.1.17",
          "form": "이쁜 린넨천 같은거 살까싶네요",
          "original_form": "이쁜 린넨천 같은거 살까싶네요",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039217.1.18",
          "form": "아항ㅋㅋㅋ 그러게요 천은 예쁜거 많더라구요 ㅋㅋㅋㅋ",
          "original_form": "아항ㅋㅋㅋ 그러게요 천은 예쁜거 많더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100039217.1.19",
          "form": "이러다가 서랍장도 새로 사실까봐 무서운데 ㅋㅋㅋ",
          "original_form": "이러다가 서랍장도 새로 사실까봐 무서운데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100039217.1.20",
          "form": "기존꺼 버리는 게 귀찮네요 ㅠㅠ 귀차니즘.. 핫 ㅋㅋㅌ",
          "original_form": "기존꺼 버리는 게 귀찮네요 ㅠㅠ 귀차니즘.. 핫 ㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100039217.1.21",
          "form": "맞아요 가구는 버릴때도 뭐 신청해서 스티커 붙이고 버려야되서.. 넘 귀찮아요",
          "original_form": "맞아요 가구는 버릴때도 뭐 신청해서 스티커 붙이고 버려야되서.. 넘 귀찮아요",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100039217.1.22",
          "form": "맞아요ㅜㅜ 흑흑 대충살죠 뭐",
          "original_form": "맞아요ㅜㅜ 흑흑 대충살죠 뭐",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100039217.1.23",
          "form": "근데 오늘보니까..제 옷이 참 많긴하네요",
          "original_form": "근데 오늘보니까..제 옷이 참 많긴하네요",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100039217.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039217.1.25",
          "form": "아니 그것을 지금 깨닳으신건가욤!ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니 그것을 지금 깨닳으신건가욤!ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039217.1.26",
          "form": "제 옷은 제가 정리한다고 놔두라고 했는데 지금 진빠져서 누워있어요",
          "original_form": "제 옷은 제가 정리한다고 놔두라고 했는데 지금 진빠져서 누워있어요",
          "speaker_id": "2",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039217.1.27",
          "form": "이런 옷도 있었나 싶구요",
          "original_form": "이런 옷도 있었나 싶구요",
          "speaker_id": "2",
          "time": "20210903 16:54"
