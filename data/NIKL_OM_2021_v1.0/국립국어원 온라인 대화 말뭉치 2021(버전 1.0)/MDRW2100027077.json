      "utterance": [
        {
          "id": "MDRW2100027077.1.1",
          "form": "지금은 컴터 사고싶더라구요",
          "original_form": "지금은 컴터 사고싶더라구요",
          "speaker_id": "2",
          "time": "20210902 09:05"
        },
        {
          "id": "MDRW2100027077.1.2",
          "form": "앗 저두요",
          "original_form": "앗 저두요",
          "speaker_id": "1",
          "time": "20210902 09:05"
        },
        {
          "id": "MDRW2100027077.1.3",
          "form": "노트북이랑 데스크탑 따로 필요한 거 같아요 ㅋㅋㅋㅋ",
          "original_form": "노트북이랑 데스크탑 따로 필요한 거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:05"
        },
        {
          "id": "MDRW2100027077.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 09:05"
        },
        {
          "id": "MDRW2100027077.1.5",
          "form": "저는 데스크탑살려구요",
          "original_form": "저는 데스크탑살려구요",
          "speaker_id": "2",
          "time": "20210902 09:05"
        },
        {
          "id": "MDRW2100027077.1.6",
          "form": "오오 봐두신 모델 있으세요?",
          "original_form": "오오 봐두신 모델 있으세요?",
          "speaker_id": "1",
          "time": "20210902 09:06"
        },
        {
          "id": "MDRW2100027077.1.7",
          "form": "견적맞춰서",
          "original_form": "견적맞춰서",
          "speaker_id": "2",
          "time": "20210902 09:08"
        },
        {
          "id": "MDRW2100027077.1.8",
          "form": "하는중입니다",
          "original_form": "하는중입니다",
          "speaker_id": "2",
          "time": "20210902 09:08"
        },
        {
          "id": "MDRW2100027077.1.9",
          "form": "요즘은 조립이  조터라구요",
          "original_form": "요즘은 조립이  조터라구요",
          "speaker_id": "2",
          "time": "20210902 09:08"
        },
        {
          "id": "MDRW2100027077.1.10",
          "form": "음 맞아요 견적이 중요하죠",
          "original_form": "음 맞아요 견적이 중요하죠",
          "speaker_id": "1",
          "time": "20210902 09:08"
        },
        {
          "id": "MDRW2100027077.1.11",
          "form": "조립이 조금 더 저렴하죠?",
          "original_form": "조립이 조금 더 저렴하죠?",
          "speaker_id": "1",
          "time": "20210902 09:08"
        },
        {
          "id": "MDRW2100027077.1.12",
          "form": "다비슷한거같아요",
          "original_form": "다비슷한거같아요",
          "speaker_id": "2",
          "time": "20210902 09:12"
        },
        {
          "id": "MDRW2100027077.1.13",
          "form": "무슨 부품을 고르냐에 따라..ㅎ",
          "original_form": "무슨 부품을 고르냐에 따라..ㅎ",
          "speaker_id": "2",
          "time": "20210902 09:12"
        },
        {
          "id": "MDRW2100027077.1.14",
          "form": "앗 그렇군요...",
          "original_form": "앗 그렇군요...",
          "speaker_id": "1",
          "time": "20210902 09:12"
        },
        {
          "id": "MDRW2100027077.1.15",
          "form": "컴퓨터를 잘 몰라서,,",
          "original_form": "컴퓨터를 잘 몰라서,,",
          "speaker_id": "1",
          "time": "20210902 09:12"
        },
        {
          "id": "MDRW2100027077.1.16",
          "form": "대학교 진학할 때 컴공을",
          "original_form": "대학교 진학할 때 컴공을",
          "speaker_id": "1",
          "time": "20210902 09:13"
        },
        {
          "id": "MDRW2100027077.1.17",
          "form": "아예 안중에도 두지 않은게 후회될 때가 있어요!",
          "original_form": "아예 안중에도 두지 않은게 후회될 때가 있어요!",
          "speaker_id": "1",
          "time": "20210902 09:13"
        },
        {
          "id": "MDRW2100027077.1.18",
          "form": "자도 친구들한테 물아보면서 하요",
          "original_form": "자도 친구들한테 물아보면서 하요",
          "speaker_id": "2",
          "time": "20210902 09:18"
        },
        {
          "id": "MDRW2100027077.1.19",
          "form": "역시 집단지성이..!",
          "original_form": "역시 집단지성이..!",
          "speaker_id": "1",
          "time": "20210902 09:21"
        },
        {
          "id": "MDRW2100027077.1.20",
          "form": "데스크탑으로 주로 작업하시나요?",
          "original_form": "데스크탑으로 주로 작업하시나요?",
          "speaker_id": "1",
          "time": "20210902 09:21"
        },
        {
          "id": "MDRW2100027077.1.21",
          "form": "아니요 게임할려규 ㅎㅎ",
          "original_form": "아니요 게임할려규 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100027077.1.22",
          "form": "오오 무슨 게임 좋아하시는데요?!",
          "original_form": "오오 무슨 게임 좋아하시는데요?!",
          "speaker_id": "1",
          "time": "20210902 09:38"
        },
        {
          "id": "MDRW2100027077.1.23",
          "form": "하긴 게임하려면",
          "original_form": "하긴 게임하려면",
          "speaker_id": "1",
          "time": "20210902 09:38"
        },
        {
          "id": "MDRW2100027077.1.24",
          "form": "데스크탑이 짱이죠!!!!",
          "original_form": "데스크탑이 짱이죠!!!!",
          "speaker_id": "1",
          "time": "20210902 09:38"
        },
        {
          "id": "MDRW2100027077.1.25",
          "form": "저 배그랑 옵치 이런거해요",
          "original_form": "저 배그랑 옵치 이런거해요",
          "speaker_id": "2",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100027077.1.26",
          "form": "우와 재밌는거 하시네요!!!",
          "original_form": "우와 재밌는거 하시네요!!!",
          "speaker_id": "1",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100027077.1.27",
          "form": "옵치는 어떤 캐릭터로 하세요?",
          "original_form": "옵치는 어떤 캐릭터로 하세요?",
          "speaker_id": "1",
          "time": "20210902 11:56"
        }
      ]
