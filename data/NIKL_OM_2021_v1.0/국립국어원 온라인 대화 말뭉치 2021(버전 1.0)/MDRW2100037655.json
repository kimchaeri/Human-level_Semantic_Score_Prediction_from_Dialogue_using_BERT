      "utterance": [
        {
          "id": "MDRW2100037655.1.1",
          "form": "족발도 너무 맛있더라고요 맛있는 집은",
          "original_form": "족발도 너무 맛있더라고요 맛있는 집은",
          "speaker_id": "2",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100037655.1.2",
          "form": "저는 뼈를 좋아해요 ㅋㅋㅋ",
          "original_form": "저는 뼈를 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:19"
        },
        {
          "id": "MDRW2100037655.1.3",
          "form": "뼈에 붙은 살 말씀하시는 거예요?",
          "original_form": "뼈에 붙은 살 말씀하시는 거예요?",
          "speaker_id": "2",
          "time": "20210901 14:19"
        },
        {
          "id": "MDRW2100037655.1.4",
          "form": "네네 !",
          "original_form": "네네 !",
          "speaker_id": "1",
          "time": "20210901 14:19"
        },
        {
          "id": "MDRW2100037655.1.5",
          "form": "아 맛있죠 ㅎㅎ",
          "original_form": "아 맛있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 14:20"
        },
        {
          "id": "MDRW2100037655.1.6",
          "form": "그게 원래족발아닌가요? 족발시키면 주로오는 건 다리쪽 살 같던데요 진짜 족발은 아주조금만 있어요",
          "original_form": "그게 원래족발아닌가요? 족발시키면 주로오는 건 다리쪽 살 같던데요 진짜 족발은 아주조금만 있어요",
          "speaker_id": "2",
          "time": "20210901 14:20"
        },
        {
          "id": "MDRW2100037655.1.7",
          "form": "맞아요 ㅎㅎ 앞다리살이 뒷다리살보다 더 맛있는데",
          "original_form": "맞아요 ㅎㅎ 앞다리살이 뒷다리살보다 더 맛있는데",
          "speaker_id": "1",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100037655.1.8",
          "form": "보통 더 비싸더라구여",
          "original_form": "보통 더 비싸더라구여",
          "speaker_id": "1",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100037655.1.9",
          "form": "가격 정해진거 보면 참 정확한거같아요 정확해서 슬픕니다",
          "original_form": "가격 정해진거 보면 참 정확한거같아요 정확해서 슬픕니다",
          "speaker_id": "2",
          "time": "20210901 14:23"
        },
        {
          "id": "MDRW2100037655.1.10",
          "form": "마자요 ㅋㅋㅋ",
          "original_form": "마자요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100037655.1.11",
          "form": "근데 또 비싼데 맛없으면",
          "original_form": "근데 또 비싼데 맛없으면",
          "speaker_id": "1",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100037655.1.12",
          "form": "더 슬퍼요 ㅋㅋ",
          "original_form": "더 슬퍼요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100037655.1.13",
          "form": "비싼데 맛없었던거 이야기해주세요~",
          "original_form": "비싼데 맛없었던거 이야기해주세요~",
          "speaker_id": "2",
          "time": "20210901 14:28"
        },
        {
          "id": "MDRW2100037655.1.14",
          "form": "전 그 멕시코 음식들이 저랑 안맞아요 ㅎㅎ ㅜㅜ",
          "original_form": "전 그 멕시코 음식들이 저랑 안맞아요 ㅎㅎ ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100037655.1.15",
          "form": "아아 그러시군요",
          "original_form": "아아 그러시군요",
          "speaker_id": "2",
          "time": "20210901 14:40"
        },
        {
          "id": "MDRW2100037655.1.16",
          "form": "네 안좋아하는거 있으세요?",
          "original_form": "네 안좋아하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210901 14:46"
        },
        {
          "id": "MDRW2100037655.1.17",
          "form": "향신료가 안맞는거 잘 못먹겠어요 동남아 음식같은거요",
          "original_form": "향신료가 안맞는거 잘 못먹겠어요 동남아 음식같은거요",
          "speaker_id": "2",
