        },
        {
          "id": "MDRW2100015519.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015519.1.3",
          "form": "당근 애용하세요?",
          "original_form": "당근 애용하세요?",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015519.1.4",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015519.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015519.1.6",
          "form": "저는 가끔 중고물건 팔거나 나눔할 때 사용해요",
          "original_form": "저는 가끔 중고물건 팔거나 나눔할 때 사용해요",
          "speaker_id": "1",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015519.1.7",
          "form": "중고거래는 잘 안하세요?",
          "original_form": "중고거래는 잘 안하세요?",
          "speaker_id": "1",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015519.1.8",
          "form": "네네 거의 진짜가끔?",
          "original_form": "네네 거의 진짜가끔?",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100015519.1.9",
          "form": "아예안쓰는물건 팔때정도요ㅋㅋㅋ",
          "original_form": "아예안쓰는물건 팔때정도요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100015519.1.10",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100015519.1.11",
          "form": "쇼핑은 자주 하시는 편이세요?",
          "original_form": "쇼핑은 자주 하시는 편이세요?",
          "speaker_id": "1",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100015519.1.12",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210826 13:41"
        },
        {
          "id": "MDRW2100015519.1.13",
          "form": "거의 안하는편이죠ㅋㅋㅋ",
          "original_form": "거의 안하는편이죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:41"
        },
        {
          "id": "MDRW2100015519.1.14",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210826 13:41"
        },
        {
          "id": "MDRW2100015519.1.15",
          "form": "저도 잘 안해요",
          "original_form": "저도 잘 안해요",
          "speaker_id": "1",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100015519.1.16",
          "form": "생필품이나 인터넷으로 사는정도",
          "original_form": "생필품이나 인터넷으로 사는정도",
          "speaker_id": "1",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100015519.1.17",
          "form": "과일이나 식재료는 코스트코에서 사고",
          "original_form": "과일이나 식재료는 코스트코에서 사고",
          "speaker_id": "1",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100015519.1.18",
          "form": "코스트코세권이신가보네요?!",
          "original_form": "코스트코세권이신가보네요?!",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100015519.1.19",
          "form": "차타고 20분정도 가야해요",
          "original_form": "차타고 20분정도 가야해요",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100015519.1.20",
          "form": "식재료나 생필품은 어떻게 구입하세요?",
          "original_form": "식재료나 생필품은 어떻게 구입하세요?",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100015519.1.21",
          "form": "주로 동네마트요ㅋㅋㅋ",
          "original_form": "주로 동네마트요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100015519.1.22",
          "form": "아님 쓱이요",
          "original_form": "아님 쓱이요",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100015519.1.23",
          "form": "직접 장보는 편이세요?",
          "original_form": "직접 장보는 편이세요?",
          "speaker_id": "1",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100015519.1.24",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100015519.1.25",
          "form": "제가보죠ㅋㅋㅋ",
          "original_form": "제가보죠ㅋㅋㅋ",
