      "utterance": [
        {
          "id": "MDRW2100012619.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012619.1.2",
          "form": "예전에 좋아 했었어요",
          "original_form": "예전에 좋아 했었어요",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012619.1.3",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012619.1.4",
          "form": "전 게임이랑은 인연이 없어서",
          "original_form": "전 게임이랑은 인연이 없어서",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012619.1.5",
          "form": "어렸을때 조금하고 아예 안했어요",
          "original_form": "어렸을때 조금하고 아예 안했어요",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012619.1.6",
          "form": "해보려고해도 최대 2시간이더라구요",
          "original_form": "해보려고해도 최대 2시간이더라구요",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012619.1.7",
          "form": "어떤 게임이었나요?",
          "original_form": "어떤 게임이었나요?",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100012619.1.8",
          "form": "오버위치요",
          "original_form": "오버위치요",
          "speaker_id": "2",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100012619.1.9",
          "form": "그게 한창 유행하던 때가 있어서",
          "original_form": "그게 한창 유행하던 때가 있어서",
          "speaker_id": "2",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100012619.1.10",
          "form": "친구랑 가서 해봤는데",
          "original_form": "친구랑 가서 해봤는데",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100012619.1.11",
          "form": "친구들은 재밌게 하더라구요",
          "original_form": "친구들은 재밌게 하더라구요",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100012619.1.12",
          "form": "오버워치 해본적은 없지만",
          "original_form": "오버워치 해본적은 없지만",
          "speaker_id": "1",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100012619.1.13",
          "form": "재밌어보여요",
          "original_form": "재밌어보여요",
          "speaker_id": "1",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100012619.1.14",
          "form": "근데 하는건 어렵더라구요",
          "original_form": "근데 하는건 어렵더라구요",
          "speaker_id": "2",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100012619.1.15",
          "form": "지금은 인기가 시들해졌지만",
          "original_form": "지금은 인기가 시들해졌지만",
          "speaker_id": "2",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100012619.1.16",
          "form": "소소하게 하시는 분들이 많더라구요",
          "original_form": "소소하게 하시는 분들이 많더라구요",
          "speaker_id": "2",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100012619.1.17",
          "form": "캐릭터들도 늘고",
          "original_form": "캐릭터들도 늘고",
          "speaker_id": "2",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100012619.1.18",
          "form": "맞아요 신경써야할게 많아보이는 게임이예요",
          "original_form": "맞아요 신경써야할게 많아보이는 게임이예요",
          "speaker_id": "1",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100012619.1.19",
          "form": "좋아하는 게임 스트리머있으세요?",
          "original_form": "좋아하는 게임 스트리머있으세요?",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100012619.1.20",
          "form": "아뇨 딱히 없어요",
          "original_form": "아뇨 딱히 없어요",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100012619.1.21",
          "form": "name1은 있으신가요?",
          "original_form": "&name1&은 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100012619.1.22",
          "form": "짬타수아 많이 봤었어요",
          "original_form": "짬타수아 많이 봤었어요",
          "speaker_id": "1",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100012619.1.23",
          "form": "오 이런분도 있으시군요",
          "original_form": "오 이런분도 있으시군요",
          "speaker_id": "2",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100012619.1.24",
          "form": "뭔가 호쾌해보이세요",
          "original_form": "뭔가 호쾌해보이세요",
          "speaker_id": "2",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100012619.1.25",
          "form": "시원시원한 스타일",
          "original_form": "시원시원한 스타일",
          "speaker_id": "2",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100012619.1.26",
          "form": "네 텐션이 엄청나요ㅋㅋ",
