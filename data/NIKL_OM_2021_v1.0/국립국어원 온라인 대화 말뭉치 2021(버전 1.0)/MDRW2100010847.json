      "utterance": [
        {
          "id": "MDRW2100010847.1.1",
          "form": "음 제 야구 이야기하죠 뭐 ㅋㄱ",
          "original_form": "음 제 야구 이야기하죠 뭐 ㅋㄱ",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010847.1.2",
          "form": "name1님은 리액션의 달인이시니까 ㅋㅋ",
          "original_form": "&name1&님은 리액션의 달인이시니까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010847.1.3",
          "form": "우린 할쑤이써요! ㅋㅋㅋ",
          "original_form": "우린 할쑤이써요! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010847.1.4",
          "form": "네 해보셔요",
          "original_form": "네 해보셔요",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010847.1.5",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010847.1.6",
          "form": "달리죠",
          "original_form": "달리죠",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010847.1.7",
          "form": "대학때는 야구티켓이 2천원이어서",
          "original_form": "대학때는 야구티켓이 2천원이어서",
          "speaker_id": "2",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010847.1.8",
          "form": "오 정말 싸네요",
          "original_form": "오 정말 싸네요",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010847.1.9",
          "form": "그당시에도 되게 싼가격이었어요 ㄲ",
          "original_form": "그당시에도 되게 싼가격이었어요 ㄲ",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010847.1.10",
          "form": "전 저번에 공짜라 얼만지 몰라요 ㅜㅜ",
          "original_form": "전 저번에 공짜라 얼만지 몰라요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010847.1.11",
          "form": "그렇죠 엄청싸요",
          "original_form": "그렇죠 엄청싸요",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010847.1.12",
          "form": "심지어 교양을 생활체육의 이해 이런거 들으면",
          "original_form": "심지어 교양을 생활체육의 이해 이런거 들으면",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010847.1.13",
          "form": "네 막주나요?",
          "original_form": "네 막주나요?",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010847.1.14",
          "form": "야구 한번보고오는게",
          "original_form": "야구 한번보고오는게",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010847.1.15",
          "form": "리포트 대신이었어요",
          "original_form": "리포트 대신이었어요",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010847.1.16",
          "form": "지금 생각해보면 참 공부에는 도움 1도 안되는 ㅋㅋㅋ",
          "original_form": "지금 생각해보면 참 공부에는 도움 1도 안되는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010847.1.17",
          "form": "그런거 있죠 그럼 신나는데요",
          "original_form": "그런거 있죠 그럼 신나는데요",
          "speaker_id": "1",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010847.1.18",
          "form": "안되죠 원래 안되는건 잼나요 ㅋㅋ",
          "original_form": "안되죠 원래 안되는건 잼나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010847.1.19",
          "form": "실컷 보셨겠어요?",
          "original_form": "실컷 보셨겠어요?",
          "speaker_id": "1",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010847.1.20",
          "form": "그랬어요 아련하네",
          "original_form": "그랬어요 아련하네",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010847.1.21",
          "form": "결국 그과목은근데",
          "original_form": "결국 그과목은근데",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010847.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010847.1.23",
          "form": "중간기말 망처서 씨쁠떴어요 ㅋㅋ",
          "original_form": "중간기말 망처서 씨쁠떴어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010847.1.24",
          "form": "ㅋㅋ저 순간 뿜었어요",
          "original_form": "ㅋㅋ저 순간 뿜었어요",
          "speaker_id": "1",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010847.1.25",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010847.1.26",
          "form": "어쩔수 없어요",
          "original_form": "어쩔수 없어요",
          "speaker_id": "1",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010847.1.27",
          "form": "공부는 안하고 야구만보러다님",
          "original_form": "공부는 안하고 야구만보러다님",
          "speaker_id": "2",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010847.1.28",
          "form": "다 그렇게돼요",
          "original_form": "다 그렇게돼요",
          "speaker_id": "1",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010847.1.29",
          "form": "아뇨 이 이야기는 여기서 끝나지아나요",
          "original_form": "아뇨 이 이야기는 여기서 끝나지아나요",
          "speaker_id": "2",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010847.1.30",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010847.1.31",
          "form": "넘 빠지셨네요 ㅋㅋ 뭐든 다 중간이 없어요 저도",
          "original_form": "넘 빠지셨네요 ㅋㅋ 뭐든 다 중간이 없어요 저도",
          "speaker_id": "1",
          "time": "20210823 22:34"
        }
      ]
