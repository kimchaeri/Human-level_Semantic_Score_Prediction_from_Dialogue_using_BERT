      "utterance": [
        {
          "id": "MDRW2100033677.1.1",
          "form": "깁콘쓰면 싫어하던데",
          "original_form": "깁콘쓰면 싫어하던데",
          "speaker_id": "2",
          "time": "20210824 22:38"
        },
        {
          "id": "MDRW2100033677.1.2",
          "form": "여기는 그런거 없어요ㅎㅎ",
          "original_form": "여기는 그런거 없어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100033677.1.3",
          "form": "좋은 사장님이네요",
          "original_form": "좋은 사장님이네요",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100033677.1.4",
          "form": "네~  나이조금?있으신데 잘해주시더라구요",
          "original_form": "네~  나이조금?있으신데 잘해주시더라구요",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100033677.1.5",
          "form": "행사있으면 미리알려주시고",
          "original_form": "행사있으면 미리알려주시고",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100033677.1.6",
          "form": "그런분 넘 좋아요",
          "original_form": "그런분 넘 좋아요",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100033677.1.7",
          "form": "낼도 또하시게요?",
          "original_form": "낼도 또하시게요?",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100033677.1.8",
          "form": "음 과자가 많아서ㅠㅠ  아이스크림은 실결제 있는데",
          "original_form": "음 과자가 많아서ㅠㅠ  아이스크림은 실결제 있는데",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100033677.1.9",
          "form": "재고있으면?도오전?",
          "original_form": "재고있으면?도오전?",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100033677.1.10",
          "form": "과자말곤 살만한거없어요?",
          "original_form": "과자말곤 살만한거없어요?",
          "speaker_id": "2",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033677.1.11",
          "form": "아이스크림요",
          "original_form": "아이스크림요",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033677.1.12",
          "form": "그파격세일?",
          "original_form": "그파격세일?",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033677.1.13",
          "form": "칸에서 사야",
          "original_form": "칸에서 사야",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033677.1.14",
          "form": "이득인듯",
          "original_form": "이득인듯",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033677.1.15",
          "form": "아이스크림은이래저래 해봤는데 4천안되고3천만되요ㅠ",
          "original_form": "아이스크림은이래저래 해봤는데 4천안되고3천만되요ㅠ",
          "speaker_id": "1",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100033677.1.16",
          "form": "아스크림 씨유서 하드는 10개 4천원이던데요",
          "original_form": "아스크림 씨유서 하드는 10개 4천원이던데요",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100033677.1.17",
          "form": "한달내내인지 몰겠는데",
          "original_form": "한달내내인지 몰겠는데",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100033677.1.18",
          "form": "막대아니고 콘 쮸쮸바종류여요",
          "original_form": "막대아니고 콘 쮸쮸바종류여요",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033677.1.19",
          "form": "낼은 살거있나봐야겠네요",
          "original_form": "낼은 살거있나봐야겠네요",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100033677.1.20",
          "form": "네 배달비까지 받으면 핫딜입니다ㅋ",
          "original_form": "네 배달비까지 받으면 핫딜입니다ㅋ",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100033677.1.21",
          "form": "그러게요 우친은 근데안해봐서",
          "original_form": "그러게요 우친은 근데안해봐서",
          "speaker_id": "2",
          "time": "20210824 23:09"
        }
      ]
