      "utterance": [
        {
          "id": "MDRW2100005342.1.1",
          "form": "와 오늘 날씨 뭐냐",
          "original_form": "와 오늘 날씨 뭐냐",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005342.1.2",
          "form": "짱 덥지 ?? 해가 쨍쨍해",
          "original_form": "짱 덥지 ?? 해가 쨍쨍해",
          "speaker_id": "2",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005342.1.3",
          "form": "덥고 습하고 비오고 난리임",
          "original_form": "덥고 습하고 비오고 난리임",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005342.1.4",
          "form": "아 오늘 비도 왔어 ??",
          "original_form": "아 오늘 비도 왔어 ??",
          "speaker_id": "2",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005342.1.5",
          "form": "응 아침에 비 엄청 왔잖아",
          "original_form": "응 아침에 비 엄청 왔잖아",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005342.1.6",
          "form": "밖을 안나가서 몰랐네",
          "original_form": "밖을 안나가서 몰랐네",
          "speaker_id": "2",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005342.1.7",
          "form": "자랑이다.\r\n근데 내일 또 비온다는데. 가을장마래",
          "original_form": "자랑이다.\r\n근데 내일 또 비온다는데. 가을장마래",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005342.1.8",
          "form": "벌써 가을이라고 ??",
          "original_form": "벌써 가을이라고 ??",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005342.1.9",
          "form": "아직 너무 덥지 않나",
          "original_form": "아직 너무 덥지 않나",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005342.1.10",
          "form": "점심에는 덥긴한데 저녁은 춥던데??",
          "original_form": "점심에는 덥긴한데 저녁은 춥던데??",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005342.1.11",
          "form": "요즘 밖을 안나가서 시원해진 것도 몰랐네",
          "original_form": "요즘 밖을 안나가서 시원해진 것도 몰랐네",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005342.1.12",
          "form": "그러니까 밖에좀 나가. 요즘 미세먼지도 좋아서 나가기 딱이야",
          "original_form": "그러니까 밖에좀 나가. 요즘 미세먼지도 좋아서 나가기 딱이야",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005342.1.13",
          "form": "오 미세먼지도 좋아졌구나 !",
          "original_form": "오 미세먼지도 좋아졌구나 !",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005342.1.14",
          "form": "응응 근데 비와서 망함",
          "original_form": "응응 근데 비와서 망함",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005342.1.15",
          "form": "웃기네",
          "original_form": "웃기네",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005342.1.16",
          "form": "비오고 해서 날이 좀 쌀쌀해졌나보군",
          "original_form": "비오고 해서 날이 좀 쌀쌀해졌나보군",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005342.1.17",
          "form": "맞아 그래서 가을 옷 준비해야해",
          "original_form": "맞아 그래서 가을 옷 준비해야해",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005342.1.18",
          "form": "그치 나도 가을 옷 사야하는데",
          "original_form": "그치 나도 가을 옷 사야하는데",
          "speaker_id": "2",
          "time": "20210821 21:57"
        }
      ]
