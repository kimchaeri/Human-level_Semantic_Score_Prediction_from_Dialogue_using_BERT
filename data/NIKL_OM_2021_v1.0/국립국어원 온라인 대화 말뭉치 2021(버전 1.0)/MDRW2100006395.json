      "utterance": [
        {
          "id": "MDRW2100006395.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100006395.1.2",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100006395.1.3",
          "form": "여자분? 남자분 ?",
          "original_form": "여자분? 남자분 ?",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100006395.1.4",
          "form": "여자에요 ㅋㅋㅋ",
          "original_form": "여자에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100006395.1.5",
          "form": "요즘 디피라는 드라마가 하더라구요?",
          "original_form": "요즘 디피라는 드라마가 하더라구요?",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100006395.1.6",
          "form": "디피 보셨어요?",
          "original_form": "디피 보셨어요?",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006395.1.7",
          "form": "진짜 재밌어요",
          "original_form": "진짜 재밌어요",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006395.1.8",
          "form": "재밌다기보단 흥미로워요",
          "original_form": "재밌다기보단 흥미로워요",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006395.1.9",
          "form": "슬프기도하구요",
          "original_form": "슬프기도하구요",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006395.1.10",
          "form": "아뇨 아직 보진 않았는데",
          "original_form": "아뇨 아직 보진 않았는데",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006395.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006395.1.12",
          "form": "고증이 잘됐다고 하더라구요",
          "original_form": "고증이 잘됐다고 하더라구요",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006395.1.13",
          "form": "맞아요 ㅠㅠ 꼭보세요",
          "original_form": "맞아요 ㅠㅠ 꼭보세요",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006395.1.14",
          "form": "군대폭력이지만",
          "original_form": "군대폭력이지만",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006395.1.15",
          "form": "주말쯤에 보려고 벼르고 있어요",
          "original_form": "주말쯤에 보려고 벼르고 있어요",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006395.1.16",
          "form": "전 요즘 학교폭력 성폭력 다 다를거없다고봐요",
          "original_form": "전 요즘 학교폭력 성폭력 다 다를거없다고봐요",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006395.1.17",
          "form": "끝에선 울었어요 ㅠㅠ",
          "original_form": "끝에선 울었어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006395.1.18",
          "form": "아 그렇죠 무엇이든 폭력은 금지되어야하는 거죠",
          "original_form": "아 그렇죠 무엇이든 폭력은 금지되어야하는 거죠",
          "speaker_id": "2",
          "time": "20210907 23:57"
        }
