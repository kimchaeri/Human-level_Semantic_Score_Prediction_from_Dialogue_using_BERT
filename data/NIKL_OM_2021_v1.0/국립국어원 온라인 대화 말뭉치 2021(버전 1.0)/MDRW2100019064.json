      "utterance": [
        {
          "id": "MDRW2100019064.1.1",
          "form": "그건 또 뭠가요?",
          "original_form": "그건 또 뭠가요?",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019064.1.2",
          "form": "이민기랑 이유영 나오는건데",
          "original_form": "이민기랑 이유영 나오는건데",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019064.1.3",
          "form": "저도 아직 뭔 내용인지..",
          "original_form": "저도 아직 뭔 내용인지..",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019064.1.4",
          "form": "드라마에요?",
          "original_form": "드라마에요?",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019064.1.5",
          "form": "네 김강우도 나와요",
          "original_form": "네 김강우도 나와요",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019064.1.6",
          "form": "김강우.. ㅎㅎ",
          "original_form": "김강우.. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019064.1.7",
          "form": "전 좀 안 맞더라고요",
          "original_form": "전 좀 안 맞더라고요",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019064.1.8",
          "form": "아 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019064.1.9",
          "form": "맞아요 김강우",
          "original_form": "맞아요 김강우",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019064.1.10",
          "form": "좀 흥행 실패하는..",
          "original_form": "좀 흥행 실패하는..",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019064.1.11",
          "form": "네 ㅎㅎ 왜그럴까요",
          "original_form": "네 ㅎㅎ 왜그럴까요",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019064.1.12",
          "form": "그니가요 ㅋㅋ",
          "original_form": "그니가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019064.1.13",
          "form": "그냥 오는 대본 다 하는 것 같은..",
          "original_form": "그냥 오는 대본 다 하는 것 같은..",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019064.1.14",
          "form": "그럴 수도 있어요 ㅎㅎ",
          "original_form": "그럴 수도 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019064.1.15",
          "form": "그리고 약간 다크한 느낌?",
          "original_form": "그리고 약간 다크한 느낌?",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019064.1.16",
          "form": "간신.. 보셨어요?",
          "original_form": "간신.. 보셨어요?",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019064.1.17",
          "form": "네 저 그거 ㅋㅋㅋ",
          "original_form": "네 저 그거 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019064.1.18",
          "form": "뭣모르고 엄마랑 봤다가 ㅋㅋㅋㅋ",
          "original_form": "뭣모르고 엄마랑 봤다가 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019064.1.19",
          "form": "헉 ㅋㅋㅋㅋㅋ",
          "original_form": "헉 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019064.1.20",
          "form": "네 재앙인데요",
          "original_form": "네 재앙인데요",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019064.1.21",
          "form": "네 완전 식겁했어요",
          "original_form": "네 완전 식겁했어요",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019064.1.22",
          "form": "이유영 좋아하세요?",
          "original_form": "이유영 좋아하세요?",
