      "utterance": [
        {
          "id": "MDRW2100033324.1.1",
          "form": "꽃게 킬러라서",
          "original_form": "꽃게 킬러라서",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100033324.1.2",
          "form": "연안부두 자주가다가",
          "original_form": "연안부두 자주가다가",
          "speaker_id": "2",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100033324.1.3",
          "form": "요즘은 소래포구",
          "original_form": "요즘은 소래포구",
          "speaker_id": "2",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100033324.1.4",
          "form": "아 소래포구 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 소래포구 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100033324.1.5",
          "form": "아 꽃게도 먹고싶네요ㅠㅠ",
          "original_form": "아 꽃게도 먹고싶네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100033324.1.6",
          "form": "게장 좋아해요?",
          "original_form": "게장 좋아해요?",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100033324.1.7",
          "form": "넹ㅋㅋ게장 좋아해요",
          "original_form": "넹ㅋㅋ게장 좋아해요",
          "speaker_id": "1",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100033324.1.8",
          "form": "저는 간장보다 양념입니당ㅋㅋ",
          "original_form": "저는 간장보다 양념입니당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100033324.1.9",
          "form": "양념좋아하시는구나",
          "original_form": "양념좋아하시는구나",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100033324.1.10",
          "form": "매운거 좋아하세요?",
          "original_form": "매운거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100033324.1.11",
          "form": "맵찔인데 그래도 매콤한거 좋아요:)",
          "original_form": "맵찔인데 그래도 매콤한거 좋아요:)",
          "speaker_id": "1",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100033324.1.12",
          "form": "name2님은 간장파세요?",
          "original_form": "&name2&님은 간장파세요?",
          "speaker_id": "1",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100033324.1.13",
          "form": "전 둘다 좋아해요",
          "original_form": "전 둘다 좋아해요",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100033324.1.14",
          "form": "안가립니다 ㅋㅋ",
          "original_form": "안가립니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100033324.1.15",
          "form": "게딱지에 밥비벼먹고싶어요",
          "original_form": "게딱지에 밥비벼먹고싶어요",
          "speaker_id": "2",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100033324.1.16",
          "form": "아 맛있겠다ㅜㅜ\r\n아는맛은 못참쥐",
          "original_form": "아 맛있겠다ㅜㅜ\r\n아는맛은 못참쥐",
          "speaker_id": "1",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100033324.1.17",
          "form": "킹크랩도 먹고싶고",
          "original_form": "킹크랩도 먹고싶고",
          "speaker_id": "2",
          "time": "20210906 23:17"
        },
        {
          "id": "MDRW2100033324.1.18",
          "form": "대게는 많이 못먹겠더라구요?",
          "original_form": "대게는 많이 못먹겠더라구요?",
          "speaker_id": "2",
          "time": "20210906 23:17"
        },
        {
          "id": "MDRW2100033324.1.19",
          "form": "저는 없어서 못먹겄던데요",
          "original_form": "저는 없어서 못먹겄던데요",
          "speaker_id": "1",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100033324.1.20",
          "form": "게들은ㅋㅋㅋㅋㅋ",
          "original_form": "게들은ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100033324.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100033324.1.22",
          "form": "꽃게가 짱이에요",
          "original_form": "꽃게가 짱이에요",
          "speaker_id": "2",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100033324.1.23",
          "form": "요새 키로에 얼마나 하려나요 ㅋㅋ",
          "original_form": "요새 키로에 얼마나 하려나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100033324.1.24",
          "form": "살아있는건 비쌀거에요",
          "original_form": "살아있는건 비쌀거에요",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100033324.1.25",
          "form": "찬바람불면 꽃게가 맛있을때라는데",
          "original_form": "찬바람불면 꽃게가 맛있을때라는데",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100033324.1.26",
          "form": "가을 꽃게!ㅋㅋ",
          "original_form": "가을 꽃게!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:33"
        }
      ]
    }
