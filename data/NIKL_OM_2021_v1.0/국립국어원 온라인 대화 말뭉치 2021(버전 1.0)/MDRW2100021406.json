      "utterance": [
        {
          "id": "MDRW2100021406.1.1",
          "form": "ㅎㅎ 우리 어렸을 때 많이 얼려 먹었잖아",
          "original_form": "ㅎㅎ 우리 어렸을 때 많이 얼려 먹었잖아",
          "speaker_id": "2",
          "time": "20210828 18:37"
        },
        {
          "id": "MDRW2100021406.1.2",
          "form": "학교 앞에서 많이 팔았지",
          "original_form": "학교 앞에서 많이 팔았지",
          "speaker_id": "1",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100021406.1.3",
          "form": "쿨피스 중에서 무슨 맛 좋아해?",
          "original_form": "쿨피스 중에서 무슨 맛 좋아해?",
          "speaker_id": "2",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100021406.1.4",
          "form": "난 복숭아 맛 ㅎㅎ",
          "original_form": "난 복숭아 맛 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100021406.1.5",
          "form": "나도 그런데 ㅋ",
          "original_form": "나도 그런데 ㅋ",
          "speaker_id": "2",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100021406.1.6",
          "form": "파인애플이랑 다른 맛도 있던데 난 복숭아 맛이 최고야",
          "original_form": "파인애플이랑 다른 맛도 있던데 난 복숭아 맛이 최고야",
          "speaker_id": "1",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100021406.1.7",
          "form": "음료수 중에서 쿨피스가 제일 쌀걸? ㅋ",
          "original_form": "음료수 중에서 쿨피스가 제일 쌀걸? ㅋ",
          "speaker_id": "2",
          "time": "20210828 18:39"
        },
        {
          "id": "MDRW2100021406.1.8",
          "form": "아마도 ㅎㅎ 큰 거 한 병에 천원이면 사니까",
          "original_form": "아마도 ㅎㅎ 큰 거 한 병에 천원이면 사니까",
          "speaker_id": "1",
          "time": "20210828 18:39"
        },
        {
          "id": "MDRW2100021406.1.9",
          "form": "진짜 싸다~ 맛있는데",
          "original_form": "진짜 싸다~ 맛있는데",
          "speaker_id": "2",
          "time": "20210828 18:39"
        },
        {
          "id": "MDRW2100021406.1.10",
          "form": "매운 거 먹을 때 같이 마시면 특히 맛있는 것 같아",
          "original_form": "매운 거 먹을 때 같이 마시면 특히 맛있는 것 같아",
          "speaker_id": "1",
          "time": "20210828 18:39"
        },
        {
          "id": "MDRW2100021406.1.11",
          "form": "달달해서 매운 맛이 많이 중화되는듯",
          "original_form": "달달해서 매운 맛이 많이 중화되는듯",
          "speaker_id": "2",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100021406.1.12",
          "form": "맞아~ 그래서 더 맛있나봐",
          "original_form": "맞아~ 그래서 더 맛있나봐",
          "speaker_id": "1",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100021406.1.13",
          "form": "여러 개 사놔~ 애들 간식이랑 먹게 ㅎㅎ",
          "original_form": "여러 개 사놔~ 애들 간식이랑 먹게 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100021406.1.14",
          "form": "아랫집은 맨날 맛있는 거 해먹나봐 ㅎㅎ",
          "original_form": "아랫집은 맨날 맛있는 거 해먹나봐 ㅎㅎ",
          "speaker_id": "1",
