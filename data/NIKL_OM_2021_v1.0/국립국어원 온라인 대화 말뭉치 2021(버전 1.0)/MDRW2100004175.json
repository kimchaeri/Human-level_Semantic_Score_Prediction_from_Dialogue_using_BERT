      "utterance": [
        {
          "id": "MDRW2100004175.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004175.1.2",
          "form": "안녕하세요 여행 최근에 다녀오신적있나요?",
          "original_form": "안녕하세요 여행 최근에 다녀오신적있나요?",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004175.1.3",
          "form": "아니요 코로나때문에 못갔어요ㅜㅜ",
          "original_form": "아니요 코로나때문에 못갔어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004175.1.4",
          "form": "name2님은 여행 다녀오셨어요?",
          "original_form": "&name2&님은 여행 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004175.1.5",
          "form": "아쉽네요ㅠㅠ저는 속초로 잠깐 다녀왔어요",
          "original_form": "아쉽네요ㅠㅠ저는 속초로 잠깐 다녀왔어요",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004175.1.6",
          "form": "제주도에 가고 싶었는데~ㅜㅜ",
          "original_form": "제주도에 가고 싶었는데~ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004175.1.7",
          "form": "오 속초 다녀오셨구나",
          "original_form": "오 속초 다녀오셨구나",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004175.1.8",
          "form": "속초 여행 어떠셨어요?",
          "original_form": "속초 여행 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004175.1.9",
          "form": "네 친척이 있어서 조심스럽게 다녀왔어요ㅎㅎ 바다는 엄청 좋더라구요",
          "original_form": "네 친척이 있어서 조심스럽게 다녀왔어요ㅎㅎ 바다는 엄청 좋더라구요",
          "speaker_id": "2",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004175.1.10",
          "form": "바다ㅠㅠ 밤바다 가보고싶어요",
          "original_form": "바다ㅠㅠ 밤바다 가보고싶어요",
          "speaker_id": "1",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004175.1.11",
          "form": "코로나 풀리면 여행 어디로 가보고싶으세요?",
          "original_form": "코로나 풀리면 여행 어디로 가보고싶으세요?",
          "speaker_id": "1",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004175.1.12",
          "form": "제주도인가요?",
          "original_form": "제주도인가요?",
          "speaker_id": "1",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004175.1.13",
          "form": "네 여름에만 느낄수있는 밤바다가 좋은데",
          "original_form": "네 여름에만 느낄수있는 밤바다가 좋은데",
          "speaker_id": "2",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004175.1.14",
          "form": "네 제주도도 가고싶고 동남아도 가고싶어요",
          "original_form": "네 제주도도 가고싶고 동남아도 가고싶어요",
          "speaker_id": "2",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004175.1.15",
          "form": "동남아가 여행하기 그렇게 좋다고 들었어요",
          "original_form": "동남아가 여행하기 그렇게 좋다고 들었어요",
          "speaker_id": "1",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004175.1.16",
          "form": "열대과일도 싸고",
          "original_form": "열대과일도 싸고",
          "speaker_id": "1",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004175.1.17",
          "form": "네 날씨좋은게 제일 좋더라구요 바다랑ㅜㅜ",
          "original_form": "네 날씨좋은게 제일 좋더라구요 바다랑ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004175.1.18",
          "form": "동남아만의 그 감성 찰랑찰랑 롱원피스 입고 가보고싶어요!ㅜㅜ",
          "original_form": "동남아만의 그 감성 찰랑찰랑 롱원피스 입고 가보고싶어요!ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004175.1.19",
          "form": "맞아요 여행가면 새로운 옷도 입어보구ㅎㅎ",
          "original_form": "맞아요 여행가면 새로운 옷도 입어보구ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004175.1.20",
          "form": "정말 좋죠... 여행 얘기하니까 여행 가고싶어요",
          "original_form": "정말 좋죠... 여행 얘기하니까 여행 가고싶어요",
          "speaker_id": "1",
          "time": "20210821 15:36"
        }
      ]
