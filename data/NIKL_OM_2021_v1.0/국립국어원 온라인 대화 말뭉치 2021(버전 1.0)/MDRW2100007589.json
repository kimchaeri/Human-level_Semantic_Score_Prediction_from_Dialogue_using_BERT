      "utterance": [
        {
          "id": "MDRW2100007589.1.1",
          "form": "주말마다 가는게쉽지않은데\r\n약간 내비치는게 애기보고싶어해서",
          "original_form": "주말마다 가는게쉽지않은데\r\n약간 내비치는게 애기보고싶어해서",
          "speaker_id": "2",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007589.1.2",
          "form": "주말에는 나도집에서 쉬고 낮잠도자고",
          "original_form": "주말에는 나도집에서 쉬고 낮잠도자고",
          "speaker_id": "2",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007589.1.3",
          "form": "안하니까 그런가보다 싶으신거같아요",
          "original_form": "안하니까 그런가보다 싶으신거같아요",
          "speaker_id": "1",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007589.1.4",
          "form": "그러고싶은데 요즘파업할까 생각중이예요",
          "original_form": "그러고싶은데 요즘파업할까 생각중이예요",
          "speaker_id": "2",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007589.1.5",
          "form": "주말마다 못가요 ㅎ",
          "original_form": "주말마다 못가요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007589.1.6",
          "form": "애기 6개월때부터 지금 13개월 다 되가는데 매주갔어요..",
          "original_form": "애기 6개월때부터 지금 13개월 다 되가는데 매주갔어요..",
          "speaker_id": "2",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007589.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ제 의지가 아니라는게",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ제 의지가 아니라는게",
          "speaker_id": "2",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007589.1.8",
          "form": "친정은 3달에 1번씩 가요ㅠㅠ",
          "original_form": "친정은 3달에 1번씩 가요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007589.1.9",
          "form": "저도 시댁이랑 30분거리 빠르면 20분인데",
          "original_form": "저도 시댁이랑 30분거리 빠르면 20분인데",
          "speaker_id": "1",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007589.1.10",
          "form": "나도 엄마있는데",
          "original_form": "나도 엄마있는데",
          "speaker_id": "2",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007589.1.11",
          "form": "한달에 한번가요",
          "original_form": "한달에 한번가요",
          "speaker_id": "1",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007589.1.12",
          "form": "부럽네요.. 애기 예뻐해주시는건 감사한데",
          "original_form": "부럽네요.. 애기 예뻐해주시는건 감사한데",
          "speaker_id": "2",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007589.1.13",
          "form": "어쩔땐 애들이랑 아빠만보내요",
          "original_form": "어쩔땐 애들이랑 아빠만보내요",
          "speaker_id": "1",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007589.1.14",
          "form": "애기만 놓고오고싶어요",
          "original_form": "애기만 놓고오고싶어요",
          "speaker_id": "2",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007589.1.15",
          "form": "ㅋㅋㅋㅋ아빠도 놓고와야겠군요",
          "original_form": "ㅋㅋㅋㅋ아빠도 놓고와야겠군요",
          "speaker_id": "2",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007589.1.16",
          "form": "네 큰아들도 놓고와야되요",
          "original_form": "네 큰아들도 놓고와야되요",
          "speaker_id": "1",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007589.1.17",
          "form": "일있다고 좀델고가라고 했더니",
          "original_form": "일있다고 좀델고가라고 했더니",
          "speaker_id": "1",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007589.1.18",
          "form": "가서 놀다오더라구요",
          "original_form": "가서 놀다오더라구요",
          "speaker_id": "1",
          "time": "20210822 03:40"
        },
        {
          "id": "MDRW2100007589.1.19",
          "form": "여기는 아빠동네예요\r\n저는 타지에 혼자온거라 2년동안 너무 외로워해서\r\n언니가 근처로 이사왔어요\r\n언니가 있으니 그나마 퇴근하고나 주말에 잠깐보는데 놀 시간을 안주네요",
          "original_form": "여기는 아빠동네예요\r\n저는 타지에 혼자온거라 2년동안 너무 외로워해서\r\n언니가 근처로 이사왔어요\r\n언니가 있으니 그나마 퇴근하고나 주말에 잠깐보는데 놀 시간을 안주네요",
          "speaker_id": "2",
          "time": "20210822 03:40"
        },
        {
          "id": "MDRW2100007589.1.20",
          "form": "그리고 한번씩 나만의 시간을 좀 달라고 대놓고 이야기해요",
          "original_form": "그리고 한번씩 나만의 시간을 좀 달라고 대놓고 이야기해요",
          "speaker_id": "1",
          "time": "20210822 03:40"
        },
        {
          "id": "MDRW2100007589.1.21",
          "form": "자기도 일한데요ㅋㅋ",
          "original_form": "자기도 일한데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:40"
        },
        {
          "id": "MDRW2100007589.1.22",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "2",
          "time": "20210822 03:40"
        },
        {
          "id": "MDRW2100007589.1.23",
          "form": "자기도 안놀고 일한데요",
          "original_form": "자기도 안놀고 일한데요",
          "speaker_id": "2",
          "time": "20210822 03:41"
        },
        {
          "id": "MDRW2100007589.1.24",
          "form": "그럼 신랑이 애델고 시댁갔다와요",
          "original_form": "그럼 신랑이 애델고 시댁갔다와요",
          "speaker_id": "1",
          "time": "20210822 03:41"
        },
        {
          "id": "MDRW2100007589.1.25",
          "form": "저보고 애기데리고 시댁 갔다오라고말한적도 있어요ㅜㅜ",
          "original_form": "저보고 애기데리고 시댁 갔다오라고말한적도 있어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 03:41"
        },
        {
          "id": "MDRW2100007589.1.26",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210822 03:41"
        },
        {
          "id": "MDRW2100007589.1.27",
          "form": "넘하네요",
          "original_form": "넘하네요",
          "speaker_id": "1",
          "time": "20210822 03:41"
        },
        {
          "id": "MDRW2100007589.1.28",
          "form": "이번주에는 친정갈줄알았는데ㅠㅠ",
          "original_form": "이번주에는 친정갈줄알았는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 03:42"
        },
        {
          "id": "MDRW2100007589.1.29",
          "form": "신랑은 애들 시댁 델다놓고 영화보러가자 이러던데 ㅎㅎ",
          "original_form": "신랑은 애들 시댁 델다놓고 영화보러가자 이러던데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 03:42"
        },
        {
          "id": "MDRW2100007589.1.30",
          "form": "처음부터 매주가셔서 길들여진듯요",
          "original_form": "처음부터 매주가셔서 길들여진듯요",
          "speaker_id": "1",
          "time": "20210822 03:42"
        },
        {
          "id": "MDRW2100007589.1.31",
          "form": "맞아요ㅋㅋ 거기다 남편은 토요일오전도 일해요ㅠ",
          "original_form": "맞아요ㅋㅋ 거기다 남편은 토요일오전도 일해요ㅠ",
          "speaker_id": "2",
          "time": "20210822 03:42"
        },
        {
          "id": "MDRW2100007589.1.32",
          "form": "그거끝나고 바로시댁가는데",
          "original_form": "그거끝나고 바로시댁가는데",
          "speaker_id": "2",
          "time": "20210822 03:42"
        },
        {
          "id": "MDRW2100007589.1.33",
          "form": "자기도 약간 힘든가봐요",
          "original_form": "자기도 약간 힘든가봐요",
          "speaker_id": "2",
          "time": "20210822 03:43"
        },
        {
          "id": "MDRW2100007589.1.34",
          "form": "안가고 집에서 있으면 전화오는데",
          "original_form": "안가고 집에서 있으면 전화오는데",
          "speaker_id": "2",
          "time": "20210822 03:43"
        },
        {
          "id": "MDRW2100007589.1.35",
          "form": "거짓말하고 쉴때도 있어요",
          "original_form": "거짓말하고 쉴때도 있어요",
          "speaker_id": "2",
          "time": "20210822 03:43"
        },
        {
          "id": "MDRW2100007589.1.36",
          "form": "슬슬 안가면안되냐고 해봐요",
          "original_form": "슬슬 안가면안되냐고 해봐요",
          "speaker_id": "1",
          "time": "20210822 03:43"
        },
        {
          "id": "MDRW2100007589.1.37",
          "form": "어디 가야된다고 ㅎㅎ",
          "original_form": "어디 가야된다고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 03:43"
        },
        {
          "id": "MDRW2100007589.1.38",
          "form": "애기랑 둘이 보내야겠어요 ㅋㅋ",
          "original_form": "애기랑 둘이 보내야겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:44"
        },
        {
          "id": "MDRW2100007589.1.39",
          "form": "내일해볼게요",
          "original_form": "내일해볼게요",
          "speaker_id": "2",
          "time": "20210822 03:44"
        },
        {
          "id": "MDRW2100007589.1.40",
          "form": "그게 젤좋아요",
          "original_form": "그게 젤좋아요",
          "speaker_id": "1",
          "time": "20210822 03:44"
        }
      ]
    }
