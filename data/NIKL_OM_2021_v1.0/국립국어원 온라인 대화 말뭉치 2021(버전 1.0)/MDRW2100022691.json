      "utterance": [
        {
          "id": "MDRW2100022691.1.1",
          "form": "오오 신기하네용",
          "original_form": "오오 신기하네용",
          "speaker_id": "2",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022691.1.2",
          "form": "넷플이나 왓챠 같은거",
          "original_form": "넷플이나 왓챠 같은거",
          "speaker_id": "1",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022691.1.3",
          "form": "구독했어요?",
          "original_form": "구독했어요?",
          "speaker_id": "1",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022691.1.4",
          "form": "네네 저 넷플 구독중이에요!",
          "original_form": "네네 저 넷플 구독중이에요!",
          "speaker_id": "2",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022691.1.5",
          "form": "오 ㅎㅎ 넷플에선 주로 어떤 장르 보세요?",
          "original_form": "오 ㅎㅎ 넷플에선 주로 어떤 장르 보세요?",
          "speaker_id": "1",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022691.1.6",
          "form": "티빙은 네이버플러십때문에보구요",
          "original_form": "티빙은 네이버플러십때문에보구요",
          "speaker_id": "2",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022691.1.7",
          "form": "아 티빙 ㅋㅋㅋ 공짜로 보시겠네요",
          "original_form": "아 티빙 ㅋㅋㅋ 공짜로 보시겠네요",
          "speaker_id": "1",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022691.1.8",
          "form": "네네 환승연애재밋던데요?",
          "original_form": "네네 환승연애재밋던데요?",
          "speaker_id": "2",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022691.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ보세요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ보세요?",
          "speaker_id": "2",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022691.1.10",
          "form": "아 ㅋㅋ 며칠 전에 누가 추천해줬어요",
          "original_form": "아 ㅋㅋ 며칠 전에 누가 추천해줬어요",
          "speaker_id": "1",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022691.1.11",
          "form": "그거랑 비슷한 프로그램도 하나 더 추천해줬는데 돌싱글즈였나?",
          "original_form": "그거랑 비슷한 프로그램도 하나 더 추천해줬는데 돌싱글즈였나?",
          "speaker_id": "1",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022691.1.12",
          "form": "ㅋㅋㅋㅋ재밋어요 추천추천",
          "original_form": "ㅋㅋㅋㅋ재밋어요 추천추천",
          "speaker_id": "2",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022691.1.13",
          "form": "아 그것도 요즘 많이보나봐요",
          "original_form": "아 그것도 요즘 많이보나봐요",
          "speaker_id": "2",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022691.1.14",
          "form": "그거 커플 바꿔서 데이트 하는 내용 맞죠?",
          "original_form": "그거 커플 바꿔서 데이트 하는 내용 맞죠?",
          "speaker_id": "1",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022691.1.15",
          "form": "진짜 돌싱나와서하능거라덩뎈ㅋㅋㅋ",
          "original_form": "진짜 돌싱나와서하능거라덩뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022691.1.16",
          "form": "아 그거는 체인지데이즈에요!",
          "original_form": "아 그거는 체인지데이즈에요!",
          "speaker_id": "2",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022691.1.17",
          "form": "ㅋㅋㅋㅋ그런 포맷들이 요즘 유행이더라구요",
          "original_form": "ㅋㅋㅋㅋ그런 포맷들이 요즘 유행이더라구요",
          "speaker_id": "1",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022691.1.18",
          "form": "앗 체인지데이즈 ㅋㅋㅋ",
          "original_form": "앗 체인지데이즈 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022691.1.19",
          "form": "그럼 환승연애는 뭐에요?",
          "original_form": "그럼 환승연애는 뭐에요?",
          "speaker_id": "1",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022691.1.20",
          "form": "네네 ㅋㅋㅋㅋ하트시그널이 워낙 핫햇어서",
          "original_form": "네네 ㅋㅋㅋㅋ하트시그널이 워낙 핫햇어서",
          "speaker_id": "2",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022691.1.21",
          "form": "그거는 헤어진 연인들 나와요!",
          "original_form": "그거는 헤어진 연인들 나와요!",
          "speaker_id": "2",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100022691.1.22",
          "form": "헉...",
          "original_form": "헉...",
          "speaker_id": "1",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100022691.1.23",
          "form": "첨에 막장이라고 생각햇능데 ㅋㅋㅋㅋㅋ",
          "original_form": "첨에 막장이라고 생각햇능데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100022691.1.24",
          "form": "그럼 다시 재결합...?",
          "original_form": "그럼 다시 재결합...?",
          "speaker_id": "1",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100022691.1.25",
          "form": "재밌어요 꼳보세요 ㅠㅠ",
          "original_form": "재밌어요 꼳보세요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100022691.1.26",
          "form": "재결합 목적인 사람도 잇고 겸사겸사인거같아요",
          "original_form": "재결합 목적인 사람도 잇고 겸사겸사인거같아요",
          "speaker_id": "2",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100022691.1.27",
          "form": "전 만약에 전애인이 우리 저런 프로그램 출연하자 하면",
          "original_form": "전 만약에 전애인이 우리 저런 프로그램 출연하자 하면",
          "speaker_id": "1",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100022691.1.28",
          "form": "저 못나갈 것 같아요 ㅋㅋㅋ",
          "original_form": "저 못나갈 것 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100022691.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ진짜그건신기하긴해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ진짜그건신기하긴해요",
          "speaker_id": "2",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100022691.1.30",
          "form": "진짜 용기 대단하다",
          "original_form": "진짜 용기 대단하다",
          "speaker_id": "1",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100022691.1.31",
          "form": "잘 헤어진사람들같기도하고",
          "original_form": "잘 헤어진사람들같기도하고",
          "speaker_id": "2",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100022691.1.32",
          "form": "어느정도 홍보목적도잇능거같구",
          "original_form": "어느정도 홍보목적도잇능거같구",
          "speaker_id": "2",
          "time": "20210829 17:06"
        },
        {
          "id": "MDRW2100022691.1.33",
          "form": "전 아직 넷플 못본 거 많아서 하나하나 보고 있어요",
          "original_form": "전 아직 넷플 못본 거 많아서 하나하나 보고 있어요",
          "speaker_id": "1",
          "time": "20210829 17:06"
        }
      ]
    }
