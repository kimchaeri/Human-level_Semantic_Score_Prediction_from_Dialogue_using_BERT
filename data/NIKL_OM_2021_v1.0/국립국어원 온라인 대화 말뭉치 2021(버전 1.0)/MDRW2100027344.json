      "utterance": [
        {
          "id": "MDRW2100027344.1.1",
          "form": "끊이질않죠",
          "original_form": "끊이질않죠",
          "speaker_id": "2",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100027344.1.2",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100027344.1.3",
          "form": "카레 맛있게",
          "original_form": "카레 맛있게",
          "speaker_id": "2",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100027344.1.4",
          "form": "만드는법아세요?",
          "original_form": "만드는법아세요?",
          "speaker_id": "2",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100027344.1.5",
          "form": "우유요!",
          "original_form": "우유요!",
          "speaker_id": "1",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100027344.1.6",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027344.1.7",
          "form": "매운카레에 우유 넣으면 부드러워진대요",
          "original_form": "매운카레에 우유 넣으면 부드러워진대요",
          "speaker_id": "1",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027344.1.8",
          "form": "전커피가루 넣음 맛있다는말",
          "original_form": "전커피가루 넣음 맛있다는말",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027344.1.9",
          "form": "들어봤어요",
          "original_form": "들어봤어요",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027344.1.10",
          "form": "헐?",
          "original_form": "헐?",
          "speaker_id": "1",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027344.1.11",
          "form": "새롭네요",
          "original_form": "새롭네요",
          "speaker_id": "1",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027344.1.12",
          "form": "몰랐나요 담에 시도해보세요",
          "original_form": "몰랐나요 담에 시도해보세요",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027344.1.13",
          "form": "그럴게요!",
          "original_form": "그럴게요!",
          "speaker_id": "1",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027344.1.14",
          "form": "하이라이스",
          "original_form": "하이라이스",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027344.1.15",
          "form": "좋아하나요",
          "original_form": "좋아하나요",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027344.1.16",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027344.1.17",
          "form": "카레재료 남았는데",
          "original_form": "카레재료 남았는데",
          "speaker_id": "2",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100027344.1.18",
          "form": "또 카레만들순없어요",
          "original_form": "또 카레만들순없어요",
          "speaker_id": "2",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100027344.1.19",
          "form": "하이라이스해먹어야겠어요",
          "original_form": "하이라이스해먹어야겠어요",
          "speaker_id": "2",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100027344.1.20",
          "form": "불족발 먹고싶네요",
          "original_form": "불족발 먹고싶네요",
          "speaker_id": "2",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100027344.1.21",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100027344.1.22",
          "form": "요리잘하시나봐요",
          "original_form": "요리잘하시나봐요",
          "speaker_id": "1",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100027344.1.23",
          "form": "족발좋아하세요?",
          "original_form": "족발좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100027344.1.24",
          "form": "뚝딱뚝딱",
          "original_form": "뚝딱뚝딱",
          "speaker_id": "1",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100027344.1.25",
          "form": "못해요 쉬운요리라서 해요",
          "original_form": "못해요 쉬운요리라서 해요",
          "speaker_id": "2",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100027344.1.26",
          "form": "족발좋아해요",
          "original_form": "족발좋아해요",
          "speaker_id": "1",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100027344.1.27",
          "form": "부산진족발",
          "original_form": "부산진족발",
          "speaker_id": "2",
          "time": "20210902 11:41"
        },
        {
          "id": "MDRW2100027344.1.28",
          "form": "족발은 서대였는데 이젠 모르겠네요",
          "original_form": "족발은 서대였는데 이젠 모르겠네요",
          "speaker_id": "1",
          "time": "20210902 11:41"
        }
