      "utterance": [
        {
          "id": "MDRW2100041128.1.1",
          "form": "맥도날드 햄버거 좋아하세요?",
          "original_form": "맥도날드 햄버거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 17:36"
        },
        {
          "id": "MDRW2100041128.1.2",
          "form": "네!!!",
          "original_form": "네!!!",
          "speaker_id": "2",
          "time": "20210905 17:37"
        },
        {
          "id": "MDRW2100041128.1.3",
          "form": "어제 가서 스낵랩먹고왔어오",
          "original_form": "어제 가서 스낵랩먹고왔어오",
          "speaker_id": "2",
          "time": "20210905 17:37"
        },
        {
          "id": "MDRW2100041128.1.4",
          "form": "오 스낵랩 맛있어요? 한 번도 못 먹어 봤어요",
          "original_form": "오 스낵랩 맛있어요? 한 번도 못 먹어 봤어요",
          "speaker_id": "1",
          "time": "20210905 17:37"
        },
        {
          "id": "MDRW2100041128.1.5",
          "form": "한개는 간에 기별도안가지만",
          "original_form": "한개는 간에 기별도안가지만",
          "speaker_id": "2",
          "time": "20210905 17:38"
        },
        {
          "id": "MDRW2100041128.1.6",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210905 17:38"
        },
        {
          "id": "MDRW2100041128.1.7",
          "form": "맥도날드 앱 쿠폰도 좋더라고요",
          "original_form": "맥도날드 앱 쿠폰도 좋더라고요",
          "speaker_id": "1",
          "time": "20210905 17:38"
        },
        {
          "id": "MDRW2100041128.1.8",
          "form": "오 저도그거써요",
          "original_form": "오 저도그거써요",
          "speaker_id": "2",
          "time": "20210905 17:38"
        },
        {
          "id": "MDRW2100041128.1.9",
          "form": "할인좋아요",
          "original_form": "할인좋아요",
          "speaker_id": "2",
          "time": "20210905 17:38"
        },
        {
          "id": "MDRW2100041128.1.10",
          "form": "매주 올라오는 거 중에 세일 제품 먹으면 꽤 싸서 좋아요",
          "original_form": "매주 올라오는 거 중에 세일 제품 먹으면 꽤 싸서 좋아요",
          "speaker_id": "1",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100041128.1.11",
          "form": "이번주 불고기버거 쿠폰은 이미 썼답니다!ㅋㅋㅋ",
          "original_form": "이번주 불고기버거 쿠폰은 이미 썼답니다!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100041128.1.12",
          "form": "맛있겠어요 불고기 버거",
          "original_form": "맛있겠어요 불고기 버거",
          "speaker_id": "1",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100041128.1.13",
          "form": "좋아하시는군요!",
          "original_form": "좋아하시는군요!",
          "speaker_id": "2",
          "time": "20210905 17:40"
        },
        {
          "id": "MDRW2100041128.1.14",
          "form": "불고기버거 맛있어요",
          "original_form": "불고기버거 맛있어요",
          "speaker_id": "1",
          "time": "20210905 17:40"
        },
        {
          "id": "MDRW2100041128.1.15",
          "form": "다른건 어떤버거좋아하세요?",
          "original_form": "다른건 어떤버거좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 17:40"
        },
        {
          "id": "MDRW2100041128.1.16",
          "form": "슈비버거랑 빅맥 좋아해요 ㅎㅎ",
          "original_form": "슈비버거랑 빅맥 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 17:42"
        },
