      "utterance": [
        {
          "id": "MDRW2100047594.1.1",
          "form": "키우고 싶은 동물 있으세요?!",
          "original_form": "키우고 싶은 동물 있으세요?!",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100047594.1.2",
          "form": "저 강아지요!",
          "original_form": "저 강아지요!",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100047594.1.3",
          "form": "너무 귀여워요ㅎ",
          "original_form": "너무 귀여워요ㅎ",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100047594.1.4",
          "form": "강아지 귀엽죠 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "강아지 귀엽죠 ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100047594.1.5",
          "form": "어떤 매력이 좋으세요",
          "original_form": "어떤 매력이 좋으세요",
          "speaker_id": "2",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100047594.1.6",
          "form": "복실복실한것도 귀엽고 사람 잘 따르는것도요",
          "original_form": "복실복실한것도 귀엽고 사람 잘 따르는것도요",
          "speaker_id": "1",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100047594.1.7",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100047594.1.8",
          "form": "하지만 전",
          "original_form": "하지만 전",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100047594.1.9",
          "form": "고양이를 키우고싶어요",
          "original_form": "고양이를 키우고싶어요",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100047594.1.10",
          "form": "고양이도 귀엽죠",
          "original_form": "고양이도 귀엽죠",
          "speaker_id": "1",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100047594.1.11",
          "form": "키우기 까다로울거 같아요",
          "original_form": "키우기 까다로울거 같아요",
          "speaker_id": "1",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100047594.1.12",
          "form": "맞아요 ㅠ ㅠㅠㅠ",
          "original_form": "맞아요 ㅠ ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100047594.1.13",
          "form": "근데 너무 귀여워서..",
          "original_form": "근데 너무 귀여워서..",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100047594.1.14",
          "form": "털도 많이 빠진다던데요",
          "original_form": "털도 많이 빠진다던데요",
          "speaker_id": "1",
          "time": "20210827 07:36"
        },
        {
          "id": "MDRW2100047594.1.15",
          "form": "아 그거 알고있어요 ㅠㅠ",
          "original_form": "아 그거 알고있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 07:46"
        },
        {
          "id": "MDRW2100047594.1.16",
          "form": "관리하기 힘들것 같아요",
          "original_form": "관리하기 힘들것 같아요",
          "speaker_id": "1",
          "time": "20210827 08:27"
        },
        {
          "id": "MDRW2100047594.1.17",
          "form": "ㅋㅋ ㅋㅋ 근데 오히려 목욕도 안하고",
          "original_form": "ㅋㅋ ㅋㅋ 근데 오히려 목욕도 안하고",
          "speaker_id": "2",
          "time": "20210827 09:44"
        },
        {
          "id": "MDRW2100047594.1.18",
          "form": "그래서 털만 괜찮으면 뭐!!",
          "original_form": "그래서 털만 괜찮으면 뭐!!",
          "speaker_id": "2",
          "time": "20210827 09:44"
        },
        {
          "id": "MDRW2100047594.1.19",
          "form": "털이 큰 문제네요ㅋㅋㅋ",
          "original_form": "털이 큰 문제네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 09:45"
        },
        {
          "id": "MDRW2100047594.1.20",
          "form": "ㅋㅋㅋㅋㅋ네 ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ네 ㅠ",
          "speaker_id": "2",
          "time": "20210827 09:45"
        },
        {
          "id": "MDRW2100047594.1.21",
          "form": "근데 어느동물을 키워도",
          "original_form": "근데 어느동물을 키워도",
          "speaker_id": "2",
          "time": "20210827 09:45"
        },
        {
          "id": "MDRW2100047594.1.22",
          "form": "털은 빠지니까요..",
          "original_form": "털은 빠지니까요..",
