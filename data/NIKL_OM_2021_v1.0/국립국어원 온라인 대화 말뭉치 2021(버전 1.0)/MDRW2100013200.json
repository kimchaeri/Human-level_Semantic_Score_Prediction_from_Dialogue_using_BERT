      "utterance": [
        {
          "id": "MDRW2100013200.1.1",
          "form": "그냥 먹을만한 정도? 쿡방 채널 레시피 보고 이것저것 따라하고 있어요.",
          "original_form": "그냥 먹을만한 정도? 쿡방 채널 레시피 보고 이것저것 따라하고 있어요.",
          "speaker_id": "2",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013200.1.2",
          "form": "오오...!! 너무 멋져요",
          "original_form": "오오...!! 너무 멋져요",
          "speaker_id": "1",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013200.1.3",
          "form": "쿡방 같은 거 보면",
          "original_form": "쿡방 같은 거 보면",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013200.1.4",
          "form": "뭔가 할 수 있을 거 같은데",
          "original_form": "뭔가 할 수 있을 거 같은데",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013200.1.5",
          "form": "백종원 채널도 레시피 정리가 잘 되어있어요",
          "original_form": "백종원 채널도 레시피 정리가 잘 되어있어요",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013200.1.6",
          "form": "막상 실천하려니 몸이 안따라주더라구요",
          "original_form": "막상 실천하려니 몸이 안따라주더라구요",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013200.1.7",
          "form": "아 그쵸 백파더",
          "original_form": "아 그쵸 백파더",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013200.1.8",
          "form": "하루한끼 채널 레시피는 진짜 쉬운 거 많아요",
          "original_form": "하루한끼 채널 레시피는 진짜 쉬운 거 많아요",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013200.1.9",
          "form": "오 그런가요?! 이따 찾아봐야겠어요",
          "original_form": "오 그런가요?! 이따 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100013200.1.10",
          "form": "하나 추천해주실 수 있나요 ㅎㅎ",
          "original_form": "하나 추천해주실 수 있나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100013200.1.11",
          "form": "조회수 9천 8백 대 계란볶음밥 추천이요 ㅋㅋ",
          "original_form": "조회수 9천 8백 대 계란볶음밥 추천이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013200.1.12",
          "form": "와 9천 8백",
          "original_form": "와 9천 8백",
          "speaker_id": "1",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100013200.1.13",
          "form": "계란볶음밥 크게 도전해봐야겠네요",
          "original_form": "계란볶음밥 크게 도전해봐야겠네요",
          "speaker_id": "1",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100013200.1.14",
          "form": "계란 카레도 따라해봤는데 맛있었어요",
          "original_form": "계란 카레도 따라해봤는데 맛있었어요",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100013200.1.15",
          "form": "계란 카레랑 계란 볶음밥",
          "original_form": "계란 카레랑 계란 볶음밥",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013200.1.16",
          "form": "좋습니다 ㅎㅋㅋㅋㅋㅋㅋ 계란은 집에ㅣ 많은ㄱ",
          "original_form": "좋습니다 ㅎㅋㅋㅋㅋㅋㅋ 계란은 집에ㅣ 많은ㄱ",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013200.1.17",
          "form": "실컷 해봐야겠어요",
          "original_form": "실컷 해봐야겠어요",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013200.1.18",
          "form": "혹시 넷플릭스 보세요?",
          "original_form": "혹시 넷플릭스 보세요?",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013200.1.19",
          "form": "영상 짧으니까 보기 편하실 거에요 ㅋㅋ",
          "original_form": "영상 짧으니까 보기 편하실 거에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013200.1.20",
          "form": "넷플릭스 한 번도 안 써봤어요. 어때요?",
          "original_form": "넷플릭스 한 번도 안 써봤어요. 어때요?",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100013200.1.21",
          "form": "저는 한달 무료만 써봤는데",
          "original_form": "저는 한달 무료만 써봤는데",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100013200.1.22",
          "form": "사실 제 취향은 아니었어요 선택을 잘 못해서",
          "original_form": "사실 제 취향은 아니었어요 선택을 잘 못해서",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100013200.1.23",
          "form": "킹덤만 보고 끝났던 거 같습니다 ㅋㅋㅋㅋㅠㅠ",
          "original_form": "킹덤만 보고 끝났던 거 같습니다 ㅋㅋㅋㅋㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100013200.1.24",
          "form": "저도 결제하면 돈낭비 될까봐요ㅋㅋ",
          "original_form": "저도 결제하면 돈낭비 될까봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100013200.1.25",
          "form": "넷플릭스... 되게 비싸가지고",
          "original_form": "넷플릭스... 되게 비싸가지고",
          "speaker_id": "1",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100013200.1.26",
          "form": "특히 더 그런 거 같아요",
          "original_form": "특히 더 그런 거 같아요",
          "speaker_id": "1",
          "time": "20210825 13:36"
        }
      ]
