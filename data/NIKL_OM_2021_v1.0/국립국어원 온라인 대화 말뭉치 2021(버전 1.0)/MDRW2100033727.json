      "utterance": [
        {
          "id": "MDRW2100033727.1.1",
          "form": "너 뭐 산다고했더라?",
          "original_form": "너 뭐 산다고했더라?",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033727.1.2",
          "form": "암막커튼!",
          "original_form": "암막커튼!",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033727.1.3",
          "form": "아아 암막",
          "original_form": "아아 암막",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033727.1.4",
          "form": "좋지",
          "original_form": "좋지",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033727.1.5",
          "form": "근데 이게 생각보다 무겁더라구",
          "original_form": "근데 이게 생각보다 무겁더라구",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033727.1.6",
          "form": "어쩔 수 없어",
          "original_form": "어쩔 수 없어",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033727.1.7",
          "form": "내 커튼봉이 버틸 수 있을까?",
          "original_form": "내 커튼봉이 버틸 수 있을까?",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033727.1.8",
          "form": "햇빛을 확실히 가려야하거든",
          "original_form": "햇빛을 확실히 가려야하거든",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033727.1.9",
          "form": "당연하지.",
          "original_form": "당연하지.",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033727.1.10",
          "form": "이쑤시개가 아닌이상 버틸 수 있어.",
          "original_form": "이쑤시개가 아닌이상 버틸 수 있어.",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033727.1.11",
          "form": "ㅠㅠ아무래더 다이소에서 봉을 사서",
          "original_form": "ㅠㅠ아무래더 다이소에서 봉을 사서",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033727.1.12",
          "form": "커튼봉도 산거야?",
          "original_form": "커튼봉도 산거야?",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033727.1.13",
          "form": "조금 불안해",
          "original_form": "조금 불안해",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033727.1.14",
          "form": "아아 고민하더니 결국 샀구나",
          "original_form": "아아 고민하더니 결국 샀구나",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033727.1.15",
          "form": "응 5000원에 구매했어",
          "original_form": "응 5000원에 구매했어",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033727.1.16",
          "form": "커튼은 쿠팡에서 살거야?",
          "original_form": "커튼은 쿠팡에서 살거야?",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033727.1.17",
          "form": "그냥 쿠팡에서 다 구매하지 왜 다이소에서 힘들게 산거야?",
          "original_form": "그냥 쿠팡에서 다 구매하지 왜 다이소에서 힘들게 산거야?",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033727.1.18",
          "form": "응 아마도? 쿠팡에 커튼봉이 더 비싸더라",
          "original_form": "응 아마도? 쿠팡에 커튼봉이 더 비싸더라",
          "speaker_id": "1",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100033727.1.19",
          "form": "다이소 상품권이라도 있는거야?",
          "original_form": "다이소 상품권이라도 있는거야?",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100033727.1.20",
          "form": "쿠팡에서는 거의 만원정도해",
          "original_form": "쿠팡에서는 거의 만원정도해",
          "speaker_id": "1",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100033727.1.21",
          "form": "그렇구나.",
          "original_form": "그렇구나.",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100033727.1.22",
          "form": "쿠팡이라고 다 싼건 아니더라구.",
          "original_form": "쿠팡이라고 다 싼건 아니더라구.",
          "speaker_id": "1",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100033727.1.23",
          "form": "나는 쿠팡에서 샀는데 괜찮았오",
          "original_form": "나는 쿠팡에서 샀는데 괜찮았오",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100033727.1.24",
          "form": "그렇구나 참고할께!",
          "original_form": "그렇구나 참고할께!",
          "speaker_id": "1",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100033727.1.25",
          "form": "너는 어떤 이불을 써?",
          "original_form": "너는 어떤 이불을 써?",
          "speaker_id": "1",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100033727.1.26",
          "form": "이불을 새로 장만할까 고민중이야",
          "original_form": "이불을 새로 장만할까 고민중이야",
          "speaker_id": "1",
          "time": "20210825 00:10"
        }
      ]
    }
