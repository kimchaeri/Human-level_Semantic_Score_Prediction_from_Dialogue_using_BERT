      "utterance": [
        {
          "id": "MDRW2100018650.1.1",
          "form": "뭐 좋아하세요 음식 ㅎㅎ",
          "original_form": "뭐 좋아하세요 음식 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018650.1.2",
          "form": "저는 회랑 해산물류 다좋아해요",
          "original_form": "저는 회랑 해산물류 다좋아해요",
          "speaker_id": "1",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018650.1.3",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 09:57"
        },
        {
          "id": "MDRW2100018650.1.4",
          "form": "저 연어회!",
          "original_form": "저 연어회!",
          "speaker_id": "2",
          "time": "20210827 10:00"
        },
        {
          "id": "MDRW2100018650.1.5",
          "form": "연어 좋죠!",
          "original_form": "연어 좋죠!",
          "speaker_id": "1",
          "time": "20210827 10:02"
        },
        {
          "id": "MDRW2100018650.1.6",
          "form": "아침은 드셨어요?",
          "original_form": "아침은 드셨어요?",
          "speaker_id": "1",
          "time": "20210827 10:02"
        },
        {
          "id": "MDRW2100018650.1.7",
          "form": "아니요 ㅎㅎ 뒹굴뒹굴중이라",
          "original_form": "아니요 ㅎㅎ 뒹굴뒹굴중이라",
          "speaker_id": "2",
          "time": "20210827 10:08"
        },
        {
          "id": "MDRW2100018650.1.8",
          "form": "배고프시면 드시려나봐요 뭐 먹고싶으세요?",
          "original_form": "배고프시면 드시려나봐요 뭐 먹고싶으세요?",
          "speaker_id": "1",
          "time": "20210827 10:17"
        },
        {
          "id": "MDRW2100018650.1.9",
          "form": "이따 요거트 먹을까해요",
          "original_form": "이따 요거트 먹을까해요",
          "speaker_id": "2",
          "time": "20210827 10:24"
        },
        {
          "id": "MDRW2100018650.1.10",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 10:24"
        },
        {
          "id": "MDRW2100018650.1.11",
          "form": "저는 시리얼먹었어요",
          "original_form": "저는 시리얼먹었어요",
          "speaker_id": "1",
          "time": "20210827 10:24"
        },
        {
          "id": "MDRW2100018650.1.12",
          "form": "점심은 국밥먹을까해요",
          "original_form": "점심은 국밥먹을까해요",
          "speaker_id": "1",
          "time": "20210827 10:24"
        },
        {
          "id": "MDRW2100018650.1.13",
          "form": "국밥좋아하시나요?",
          "original_form": "국밥좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100018650.1.14",
          "form": "순대국이요",
          "original_form": "순대국이요",
          "speaker_id": "2",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100018650.1.15",
