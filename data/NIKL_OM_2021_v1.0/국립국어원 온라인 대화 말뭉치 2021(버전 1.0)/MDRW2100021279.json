      "utterance": [
        {
          "id": "MDRW2100021279.1.1",
          "form": "맞아요 그 사이에 동계도 잇고",
          "original_form": "맞아요 그 사이에 동계도 잇고",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021279.1.2",
          "form": "맞아요 너무 기대돼요",
          "original_form": "맞아요 너무 기대돼요",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021279.1.3",
          "form": "내년에 엄청 많더라구요 뭐가",
          "original_form": "내년에 엄청 많더라구요 뭐가",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021279.1.4",
          "form": "내년에 또 다른 행사가 잇나요?",
          "original_form": "내년에 또 다른 행사가 잇나요?",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021279.1.5",
          "form": "그 아시안게인이랑 월드컵도요!ㅋㅋ",
          "original_form": "그 아시안게인이랑 월드컵도요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021279.1.6",
          "form": "아 헐 월드컵이 내년이네요",
          "original_form": "아 헐 월드컵이 내년이네요",
          "speaker_id": "2",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100021279.1.7",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100021279.1.8",
          "form": "그쵸!너무 기대돼요",
          "original_form": "그쵸!너무 기대돼요",
          "speaker_id": "1",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100021279.1.9",
          "form": "내년에 진짜 끊임없이 경기들이 잇겟네요 아시안게임까지",
          "original_form": "내년에 진짜 끊임없이 경기들이 잇겟네요 아시안게임까지",
          "speaker_id": "2",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100021279.1.10",
          "form": "아시안게임은 개최지가 어디엿죠?",
          "original_form": "아시안게임은 개최지가 어디엿죠?",
          "speaker_id": "2",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100021279.1.11",
          "form": "모르겠어요 그거까진 안 찾아봐서 ㅠㅋㅋㅋㅋ",
          "original_form": "모르겠어요 그거까진 안 찾아봐서 ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021279.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ어쨋든 기대되네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ어쨋든 기대되네요",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021279.1.13",
          "form": "월드컵은 까맣게 잊고잇엇는데 내년이엇네요",
          "original_form": "월드컵은 까맣게 잊고잇엇는데 내년이엇네요",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021279.1.14",
          "form": "저두요 ㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021279.1.15",
          "form": "시간이 정말 빨리 가죠",
          "original_form": "시간이 정말 빨리 가죠",
          "speaker_id": "1",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021279.1.16",
          "form": "그러게요 ㅋㅋㅋㅋㅋ2022년 ㅋㅋㅋㅋ숫자부터 생소해요",
          "original_form": "그러게요 ㅋㅋㅋㅋㅋ2022년 ㅋㅋㅋㅋ숫자부터 생소해요",
          "speaker_id": "2",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021279.1.17",
          "form": "와 그러게요",
          "original_form": "와 그러게요",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021279.1.18",
          "form": "몇년에 한번씩 이런 경기해주니 활력소가 되는거같아요",
          "original_form": "몇년에 한번씩 이런 경기해주니 활력소가 되는거같아요",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021279.1.19",
          "form": "맞아요 그게 스포츠의 매력인것같아요",
          "original_form": "맞아요 그게 스포츠의 매력인것같아요",
