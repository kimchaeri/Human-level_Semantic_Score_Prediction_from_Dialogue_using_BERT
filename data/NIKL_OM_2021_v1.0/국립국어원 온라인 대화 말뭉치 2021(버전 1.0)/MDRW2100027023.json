      "utterance": [
        {
          "id": "MDRW2100027023.1.1",
          "form": "오늘날씨 어때요?",
          "original_form": "오늘날씨 어때요?",
          "speaker_id": "1",
          "time": "20210902 07:30"
        },
        {
          "id": "MDRW2100027023.1.2",
          "form": "아직 안나가봤어요",
          "original_form": "아직 안나가봤어요",
          "speaker_id": "2",
          "time": "20210902 07:30"
        },
        {
          "id": "MDRW2100027023.1.3",
          "form": "어제까지만해도 비 왕창 왔었는데",
          "original_form": "어제까지만해도 비 왕창 왔었는데",
          "speaker_id": "1",
          "time": "20210902 07:48"
        },
        {
          "id": "MDRW2100027023.1.4",
          "form": "오늘 비안와서 다행이에요",
          "original_form": "오늘 비안와서 다행이에요",
          "speaker_id": "1",
          "time": "20210902 07:49"
        },
        {
          "id": "MDRW2100027023.1.5",
          "form": "음 흐린 날씨네요 비는 오는건지 안 오는 건지 애매",
          "original_form": "음 흐린 날씨네요 비는 오는건지 안 오는 건지 애매",
          "speaker_id": "2",
          "time": "20210902 07:59"
        },
        {
          "id": "MDRW2100027023.1.6",
          "form": "비안왔으면좋겠어요...",
          "original_form": "비안왔으면좋겠어요...",
          "speaker_id": "1",
          "time": "20210902 08:00"
        },
        {
          "id": "MDRW2100027023.1.7",
          "form": "비많이 올때 우산쓰고나갔다가",
          "original_form": "비많이 올때 우산쓰고나갔다가",
          "speaker_id": "1",
          "time": "20210902 08:00"
        },
        {
          "id": "MDRW2100027023.1.8",
          "form": "우산잃어버려서",
          "original_form": "우산잃어버려서",
          "speaker_id": "1",
          "time": "20210902 08:00"
        },
        {
          "id": "MDRW2100027023.1.9",
          "form": "비 홀딱 다맞았어요",
          "original_form": "비 홀딱 다맞았어요",
          "speaker_id": "1",
          "time": "20210902 08:00"
        },
        {
          "id": "MDRW2100027023.1.10",
          "form": "ㅡㅅㅡ",
          "original_form": "ㅡㅅㅡ",
          "speaker_id": "1",
          "time": "20210902 08:01"
        },
        {
          "id": "MDRW2100027023.1.11",
          "form": "비가살짝오네요ㅜ",
          "original_form": "비가살짝오네요ㅜ",
          "speaker_id": "2",
          "time": "20210902 08:01"
        },
        {
          "id": "MDRW2100027023.1.12",
          "form": "비오면안돼요!!!!",
          "original_form": "비오면안돼요!!!!",
          "speaker_id": "1",
          "time": "20210902 08:01"
        },
        {
          "id": "MDRW2100027023.1.13",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210902 08:03"
        },
        {
          "id": "MDRW2100027023.1.14",
          "form": "우산잃어버리는징크스가 있어서.....",
          "original_form": "우산잃어버리는징크스가 있어서.....",
          "speaker_id": "1",
          "time": "20210902 08:03"
        },
        {
          "id": "MDRW2100027023.1.15",
          "form": "비 만오면은....",
          "original_form": "비 만오면은....",
          "speaker_id": "1",
          "time": "20210902 08:03"
        },
        {
          "id": "MDRW2100027023.1.16",
          "form": "아하 우산 잘 챙기세요ㅋ",
          "original_form": "아하 우산 잘 챙기세요ㅋ",
          "speaker_id": "2",
          "time": "20210902 08:04"
        },
        {
          "id": "MDRW2100027023.1.17",
          "form": "우산잘챙겨야해요ㅜㅜ",
          "original_form": "우산잘챙겨야해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 08:05"
        },
        {
          "id": "MDRW2100027023.1.18",
          "form": "잉?",
          "original_form": "잉?",
          "speaker_id": "1",
          "time": "20210902 08:06"
        },
        {
          "id": "MDRW2100027023.1.19",
          "form": "우산 빌려줬는데",
          "original_form": "우산 빌려줬는데",
          "speaker_id": "1",
          "time": "20210902 08:06"
        },
        {
          "id": "MDRW2100027023.1.20",
          "form": "출근할때갖다줄려고",
          "original_form": "출근할때갖다줄려고",
          "speaker_id": "1",
          "time": "20210902 08:06"
        },
        {
          "id": "MDRW2100027023.1.21",
          "form": "꽉잡고있어요",
          "original_form": "꽉잡고있어요",
          "speaker_id": "1",
          "time": "20210902 08:06"
        },
        {
          "id": "MDRW2100027023.1.22",
          "form": "누구를 빌려줘요?",
          "original_form": "누구를 빌려줘요?",
          "speaker_id": "2",
          "time": "20210902 08:07"
        },
        {
          "id": "MDRW2100027023.1.23",
          "form": "직원이 저한테 우산빌려줬어요",
          "original_form": "직원이 저한테 우산빌려줬어요",
          "speaker_id": "1",
          "time": "20210902 08:08"
        },
        {
          "id": "MDRW2100027023.1.24",
          "form": "비맞을까봐서요",
          "original_form": "비맞을까봐서요",
          "speaker_id": "1",
          "time": "20210902 08:08"
        },
        {
          "id": "MDRW2100027023.1.25",
          "form": "오 인기쟁이시네요 비 많이와요?",
          "original_form": "오 인기쟁이시네요 비 많이와요?",
          "speaker_id": "2",
          "time": "20210902 08:17"
        }
      ]
