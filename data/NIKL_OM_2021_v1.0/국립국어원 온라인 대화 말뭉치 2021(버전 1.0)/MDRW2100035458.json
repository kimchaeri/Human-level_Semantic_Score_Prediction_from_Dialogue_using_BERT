      "utterance": [
        {
          "id": "MDRW2100035458.1.1",
          "form": "저녁메뉴 정하셨나요",
          "original_form": "저녁메뉴 정하셨나요",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100035458.1.2",
          "form": "아직요 고르셨어요?",
          "original_form": "아직요 고르셨어요?",
          "speaker_id": "1",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100035458.1.3",
          "form": "저는 냉동해둔 카레 데워먹으려구요",
          "original_form": "저는 냉동해둔 카레 데워먹으려구요",
          "speaker_id": "2",
          "time": "20210829 18:16"
        },
        {
          "id": "MDRW2100035458.1.4",
          "form": "저는 케이크 먹으려구요!",
          "original_form": "저는 케이크 먹으려구요!",
          "speaker_id": "1",
          "time": "20210829 18:30"
        },
        {
          "id": "MDRW2100035458.1.5",
          "form": "사놓은게 있는걸 까먹었어요",
          "original_form": "사놓은게 있는걸 까먹었어요",
          "speaker_id": "1",
          "time": "20210829 18:30"
        },
        {
          "id": "MDRW2100035458.1.6",
          "form": "와 저는 먹는건 왜 까먹어지지 않는건지...ㅋ 무슨 케이크인가요?",
          "original_form": "와 저는 먹는건 왜 까먹어지지 않는건지...ㅋ 무슨 케이크인가요?",
          "speaker_id": "2",
          "time": "20210829 19:09"
        },
        {
          "id": "MDRW2100035458.1.7",
          "form": "치즈랑 고구마요!",
          "original_form": "치즈랑 고구마요!",
          "speaker_id": "1",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100035458.1.8",
          "form": "냉장고에 안넣어서 버릴뻔했어요ㅡㅠ",
          "original_form": "냉장고에 안넣어서 버릴뻔했어요ㅡㅠ",
          "speaker_id": "1",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100035458.1.9",
          "form": "여러가지네요! 직접 구매하신건가요?",
          "original_form": "여러가지네요! 직접 구매하신건가요?",
          "speaker_id": "2",
          "time": "20210829 19:47"
        },
        {
          "id": "MDRW2100035458.1.10",
          "form": "넹!",
          "original_form": "넹!",
          "speaker_id": "1",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100035458.1.11",
          "form": "후식 드시나요?",
          "original_form": "후식 드시나요?",
          "speaker_id": "1",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100035458.1.12",
          "form": "네 밥먹고 꼭 뭐가 더 먹고싶어요 ㅠ",
          "original_form": "네 밥먹고 꼭 뭐가 더 먹고싶어요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 19:57"
        },
        {
          "id": "MDRW2100035458.1.13",
          "form": "저두요ㅜ",
          "original_form": "저두요ㅜ",
          "speaker_id": "1",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100035458.1.14",
          "form": "수박 먹을까 싶어요ㅋㅋ",
          "original_form": "수박 먹을까 싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100035458.1.15",
          "form": "오 아직 수박 맛이 괜찮나요?",
          "original_form": "오 아직 수박 맛이 괜찮나요?",
          "speaker_id": "2",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100035458.1.16",
          "form": "생각보다 괜찮더라구요!",
          "original_form": "생각보다 괜찮더라구요!",
          "speaker_id": "1",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100035458.1.17",
          "form": "시원하니 맛났어요~",
          "original_form": "시원하니 맛났어요~",
          "speaker_id": "1",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100035458.1.18",
          "form": "그러시군요 요즘은 복숭아를 많이 먹네용",
          "original_form": "그러시군요 요즘은 복숭아를 많이 먹네용",
