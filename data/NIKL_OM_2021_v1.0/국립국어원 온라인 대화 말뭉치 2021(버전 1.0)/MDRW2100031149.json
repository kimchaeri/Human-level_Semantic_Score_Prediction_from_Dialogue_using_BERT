      "utterance": [
        {
          "id": "MDRW2100031149.1.1",
          "form": "밥은 드셨나요?",
          "original_form": "밥은 드셨나요?",
          "speaker_id": "2",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100031149.1.2",
          "form": "네 닭미역국이요",
          "original_form": "네 닭미역국이요",
          "speaker_id": "1",
          "time": "20210904 22:09"
        },
        {
          "id": "MDRW2100031149.1.3",
          "form": "맛있었겠어요",
          "original_form": "맛있었겠어요",
          "speaker_id": "2",
          "time": "20210904 22:11"
        },
        {
          "id": "MDRW2100031149.1.4",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:11"
        },
        {
          "id": "MDRW2100031149.1.5",
          "form": "내일은 뭐드실건가요?",
          "original_form": "내일은 뭐드실건가요?",
          "speaker_id": "2",
          "time": "20210904 22:11"
        },
        {
          "id": "MDRW2100031149.1.6",
          "form": "내일 오징어초무침해보려고요",
          "original_form": "내일 오징어초무침해보려고요",
          "speaker_id": "1",
          "time": "20210904 22:12"
        },
        {
          "id": "MDRW2100031149.1.7",
          "form": "오징어 너모. 맛있을것같아요",
          "original_form": "오징어 너모. 맛있을것같아요",
          "speaker_id": "2",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100031149.1.8",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "2",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100031149.1.9",
          "form": "저는 오징어 튀김도 좋아하고무침도 좋아해요 ㅎㅎ",
          "original_form": "저는 오징어 튀김도 좋아하고무침도 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100031149.1.10",
          "form": "오징어 맛있죸ㅋ\r\n저녁 모드셨어요",
          "original_form": "오징어 맛있죸ㅋ\r\n저녁 모드셨어요",
          "speaker_id": "1",
          "time": "20210904 22:16"
        },
        {
          "id": "MDRW2100031149.1.11",
          "form": "저는 집밥 먹었어요",
          "original_form": "저는 집밥 먹었어요",
          "speaker_id": "2",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100031149.1.12",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100031149.1.13",
          "form": "배달음식도 좋아하시나요?",
          "original_form": "배달음식도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100031149.1.14",
          "form": "고기류 잘시켜먹어여ㅋ",
          "original_form": "고기류 잘시켜먹어여ㅋ",
          "speaker_id": "1",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100031149.1.15",
          "form": "저도 족발이나 회 좋아해요",
          "original_form": "저도 족발이나 회 좋아해요",
          "speaker_id": "2",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100031149.1.16",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100031149.1.17",
          "form": "족발 보쌈 좋아해요",
          "original_form": "족발 보쌈 좋아해요",
          "speaker_id": "1",
          "time": "20210904 22:19"
        },
        {
          "id": "MDRW2100031149.1.18",
          "form": "그렇군요 ㅎㅎ 족발이 먹고싶네요",
          "original_form": "그렇군요 ㅎㅎ 족발이 먹고싶네요",
          "speaker_id": "2",
          "time": "20210905 10:18"
        },
        {
          "id": "MDRW2100031149.1.19",
          "form": "아침드셨나요?",
          "original_form": "아침드셨나요?",
          "speaker_id": "2",
          "time": "20210905 10:19"
        },
        {
          "id": "MDRW2100031149.1.20",
          "form": "네 토스트요",
          "original_form": "네 토스트요",
          "speaker_id": "1",
          "time": "20210905 10:19"
        },
        {
          "id": "MDRW2100031149.1.21",
          "form": "그러시군요 토스트 맛있었겠어요",
          "original_form": "그러시군요 토스트 맛있었겠어요",
          "speaker_id": "2",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031149.1.22",
          "form": "어떤 재료를 넣으셨나요?",
          "original_form": "어떤 재료를 넣으셨나요?",
          "speaker_id": "2",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031149.1.23",
          "form": "프렌치토스트에 샐러드따로요ㅋ",
          "original_form": "프렌치토스트에 샐러드따로요ㅋ",
          "speaker_id": "1",
          "time": "20210905 10:26"
        }
      ]
