      "utterance": [
        {
          "id": "MDRW2100035655.1.1",
          "form": "그러게용ㅋㅋㅋㅋㅋ",
          "original_form": "그러게용ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100035655.1.2",
          "form": "아침 드셨나요?",
          "original_form": "아침 드셨나요?",
          "speaker_id": "1",
          "time": "20210831 06:52"
        },
        {
          "id": "MDRW2100035655.1.3",
          "form": "네 오늘 아침 시리얼 먹었어요 ㅋㅋ",
          "original_form": "네 오늘 아침 시리얼 먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:10"
        },
        {
          "id": "MDRW2100035655.1.4",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 18:10"
        },
        {
          "id": "MDRW2100035655.1.5",
          "form": "이제 먹으려구용!!",
          "original_form": "이제 먹으려구용!!",
          "speaker_id": "1",
          "time": "20210831 18:20"
        },
        {
          "id": "MDRW2100035655.1.6",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210831 18:20"
        },
        {
          "id": "MDRW2100035655.1.7",
          "form": "저도 이제 먹으려고 준비중이에요!!",
          "original_form": "저도 이제 먹으려고 준비중이에요!!",
          "speaker_id": "2",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100035655.1.8",
          "form": "오오 어떤거 드세요??",
          "original_form": "오오 어떤거 드세요??",
          "speaker_id": "1",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100035655.1.9",
          "form": "그냥 집에 있는 반찬에 먹으려구요",
          "original_form": "그냥 집에 있는 반찬에 먹으려구요",
          "speaker_id": "2",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100035655.1.10",
          "form": "name1님은 뭐 드세요?",
          "original_form": "&name1&님은 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100035655.1.11",
          "form": "아항~",
          "original_form": "아항~",
          "speaker_id": "1",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100035655.1.12",
          "form": "저는 김치전에 된장국이요!",
          "original_form": "저는 김치전에 된장국이요!",
          "speaker_id": "1",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100035655.1.13",
          "form": "와 맛있겠네요 제가 전 중에서 제일 좋아하는 게 김치전이거든요",
          "original_form": "와 맛있겠네요 제가 전 중에서 제일 좋아하는 게 김치전이거든요",
          "speaker_id": "2",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100035655.1.14",
          "form": "오늘 비도 오는데 먹고 싶네요 ㅋㅋ",
          "original_form": "오늘 비도 오는데 먹고 싶네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100035655.1.15",
          "form": "오오 name2님도 좋아하시는군요!",
          "original_form": "오오 &name2&님도 좋아하시는군요!",
          "speaker_id": "1",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100035655.1.16",
          "form": "오징어도 넣었슴닷!",
          "original_form": "오징어도 넣었슴닷!",
          "speaker_id": "1",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100035655.1.17",
          "form": "그쵸~  날이 이러니 엄청 생각나더라구요",
          "original_form": "그쵸~  날이 이러니 엄청 생각나더라구요",
          "speaker_id": "1",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100035655.1.18",
          "form": "와 오징어라니 탁월하시네요 ㅋㅋㅋ",
          "original_form": "와 오징어라니 탁월하시네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100035655.1.19",
          "form": "그쵸! 진짜 오징어 너무 맛있구 감자도 넣구ㅋㅋㅋ",
          "original_form": "그쵸! 진짜 오징어 너무 맛있구 감자도 넣구ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100035655.1.20",
          "form": "이것저것 넣어서 먹슴닷!",
          "original_form": "이것저것 넣어서 먹슴닷!",
          "speaker_id": "1",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100035655.1.21",
          "form": "역시 엄마표가 최고에요!",
          "original_form": "역시 엄마표가 최고에요!",
          "speaker_id": "1",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100035655.1.22",
          "form": "진짜 맛있을 것 같아요!!",
          "original_form": "진짜 맛있을 것 같아요!!",
          "speaker_id": "2",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100035655.1.23",
          "form": "아하 맞아요 엄마표 최고",
          "original_form": "아하 맞아요 엄마표 최고",
          "speaker_id": "2",
          "time": "20210831 18:27"
        },
        {
          "id": "MDRW2100035655.1.24",
          "form": "제가 하면 반죽에서부터 망삘이 흘러넘치는데 말이죠 ㅋㄱㄱ",
          "original_form": "제가 하면 반죽에서부터 망삘이 흘러넘치는데 말이죠 ㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210831 18:27"
        },
        {
          "id": "MDRW2100035655.1.25",
          "form": "저두용ㅋㅋㅋ",
          "original_form": "저두용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:27"
        },
        {
          "id": "MDRW2100035655.1.26",
          "form": "역시 엄마표 요리가 최고에요",
          "original_form": "역시 엄마표 요리가 최고에요",
          "speaker_id": "2",
          "time": "20210831 18:28"
        },
        {
          "id": "MDRW2100035655.1.27",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210831 18:28"
        },
        {
          "id": "MDRW2100035655.1.28",
          "form": "못따라가죵",
          "original_form": "못따라가죵",
          "speaker_id": "1",
          "time": "20210831 18:28"
        },
        {
          "id": "MDRW2100035655.1.29",
          "form": "손맛이 진짜 크으",
          "original_form": "손맛이 진짜 크으",
          "speaker_id": "1",
          "time": "20210831 18:28"
        },
        {
          "id": "MDRW2100035655.1.30",
          "form": "맞아요 진짜 특히 전이나 국같은 류는 엄마들이 계량도 대충해도 잘하시잖아요",
          "original_form": "맞아요 진짜 특히 전이나 국같은 류는 엄마들이 계량도 대충해도 잘하시잖아요",
          "speaker_id": "2",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100035655.1.31",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210831 18:32"
        },
        {
          "id": "MDRW2100035655.1.32",
          "form": "저는 맞춰서해도 간당간당인데",
          "original_form": "저는 맞춰서해도 간당간당인데",
          "speaker_id": "1",
          "time": "20210831 18:32"
        },
        {
          "id": "MDRW2100035655.1.33",
          "form": "요리 못하고 자취하고 있는 저는 맛있는 전을 먹으려면 배달밖에 방법이 없네요 ㅋㅋㅋ",
          "original_form": "요리 못하고 자취하고 있는 저는 맛있는 전을 먹으려면 배달밖에 방법이 없네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:39"
        }
      ]
