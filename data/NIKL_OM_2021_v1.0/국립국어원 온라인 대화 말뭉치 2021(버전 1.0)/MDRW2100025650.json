      "utterance": [
        {
          "id": "MDRW2100025650.1.1",
          "form": "아직도 그러는듯",
          "original_form": "아직도 그러는듯",
          "speaker_id": "2",
          "time": "20210901 14:59"
        },
        {
          "id": "MDRW2100025650.1.2",
          "form": "작은극회전문돌면 제작사쪽에서 비웃더라고",
          "original_form": "작은극회전문돌면 제작사쪽에서 비웃더라고",
          "speaker_id": "1",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025650.1.3",
          "form": "코로나 때 관련자들이 떠드는거 들어보면",
          "original_form": "코로나 때 관련자들이 떠드는거 들어보면",
          "speaker_id": "2",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025650.1.4",
          "form": "회전문들 무시하고",
          "original_form": "회전문들 무시하고",
          "speaker_id": "2",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025650.1.5",
          "form": "아.......",
          "original_form": "아.......",
          "speaker_id": "1",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025650.1.6",
          "form": "그래서 또 삔또 상하고",
          "original_form": "그래서 또 삔또 상하고",
          "speaker_id": "2",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025650.1.7",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㅋ진짜 정신을 아직도 못차리는구나",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㅋ진짜 정신을 아직도 못차리는구나",
          "speaker_id": "1",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025650.1.8",
          "form": "한참 젠가 볼 때라",
          "original_form": "한참 젠가 볼 때라",
          "speaker_id": "2",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025650.1.9",
          "form": "연뮤갤이랑 연뮤카테 봤는데",
          "original_form": "연뮤갤이랑 연뮤카테 봤는데",
          "speaker_id": "2",
          "time": "20210901 15:01"
        },
        {
          "id": "MDRW2100025650.1.10",
          "form": "그래서 플 몇번 왔던거 기억함",
          "original_form": "그래서 플 몇번 왔던거 기억함",
          "speaker_id": "2",
          "time": "20210901 15:01"
        },
        {
          "id": "MDRW2100025650.1.11",
          "form": "저는 갤은 너무 말이 심해서",
          "original_form": "저는 갤은 너무 말이 심해서",
          "speaker_id": "1",
          "time": "20210901 15:01"
        },
        {
          "id": "MDRW2100025650.1.12",
          "form": "잘안보지만",
          "original_form": "잘안보지만",
          "speaker_id": "1",
          "time": "20210901 15:01"
        },
        {
          "id": "MDRW2100025650.1.13",
          "form": "카테까지그런거면 말다했군",
          "original_form": "카테까지그런거면 말다했군",
          "speaker_id": "1",
          "time": "20210901 15:01"
        },
        {
          "id": "MDRW2100025650.1.14",
          "form": "아니 왜 소비자를 무시행",
          "original_form": "아니 왜 소비자를 무시행",
          "speaker_id": "1",
          "time": "20210901 15:03"
        },
        {
          "id": "MDRW2100025650.1.15",
          "form": "어디나 경솔한 관계자들이 입 터는게",
          "original_form": "어디나 경솔한 관계자들이 입 터는게",
          "speaker_id": "2",
          "time": "20210901 15:03"
        },
        {
          "id": "MDRW2100025650.1.16",
          "form": "참 문제야",
          "original_form": "참 문제야",
          "speaker_id": "2",
          "time": "20210901 15:03"
        },
        {
          "id": "MDRW2100025650.1.17",
          "form": "그니까 월급이 우리지갑에서 나오는데",
          "original_form": "그니까 월급이 우리지갑에서 나오는데",
          "speaker_id": "1",
          "time": "20210901 15:03"
        },
        {
          "id": "MDRW2100025650.1.18",
          "form": "머글극도 그게 한번에 머글극 된거 겠냐고",
          "original_form": "머글극도 그게 한번에 머글극 된거 겠냐고",
          "speaker_id": "2",
          "time": "20210901 15:03"
        },
        {
          "id": "MDRW2100025650.1.19",
          "form": "ㅋㅋㅋㄱㅋㅋ 으으음....",
          "original_form": "ㅋㅋㅋㄱㅋㅋ 으으음....",
          "speaker_id": "1",
          "time": "20210901 15:03"
        },
        {
          "id": "MDRW2100025650.1.20",
          "form": "입소문이 뭐 뚝딱 되는건 아니니께",
          "original_form": "입소문이 뭐 뚝딱 되는건 아니니께",
          "speaker_id": "2",
          "time": "20210901 15:04"
        },
        {
          "id": "MDRW2100025650.1.21",
          "form": "머글극이랑 아닌거 차이가있나",
          "original_form": "머글극이랑 아닌거 차이가있나",
          "speaker_id": "1",
          "time": "20210901 15:04"
        },
        {
          "id": "MDRW2100025650.1.22",
          "form": "회전문극도 어차피 관객이보러오는건데",
          "original_form": "회전문극도 어차피 관객이보러오는건데",
          "speaker_id": "1",
          "time": "20210901 15:05"
        }
      ]
