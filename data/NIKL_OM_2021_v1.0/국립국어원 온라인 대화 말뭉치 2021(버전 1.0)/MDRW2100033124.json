      "utterance": [
        {
          "id": "MDRW2100033124.1.1",
          "form": "진짜 완전 밤이네",
          "original_form": "진짜 완전 밤이네",
          "speaker_id": "2",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033124.1.2",
          "form": "금방 쏟아지겠다",
          "original_form": "금방 쏟아지겠다",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033124.1.3",
          "form": "이번에 비오면 완전 소나기처럼 올듯",
          "original_form": "이번에 비오면 완전 소나기처럼 올듯",
          "speaker_id": "2",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033124.1.4",
          "form": "ㅇㅇ 이건 부슬부슬 내릴 하늘이 아니야",
          "original_form": "ㅇㅇ 이건 부슬부슬 내릴 하늘이 아니야",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033124.1.5",
          "form": "요즘엔 한 번 비오면 시원하게 오더만",
          "original_form": "요즘엔 한 번 비오면 시원하게 오더만",
          "speaker_id": "2",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100033124.1.6",
          "form": "창문 닫을 준비 해야겠어 ㅎㅎ",
          "original_form": "창문 닫을 준비 해야겠어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100033124.1.7",
          "form": "ㅎㅎ 요이땅 하게?",
          "original_form": "ㅎㅎ 요이땅 하게?",
          "speaker_id": "2",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100033124.1.8",
          "form": "글치 후두둑 내리면 뛰어가야지",
          "original_form": "글치 후두둑 내리면 뛰어가야지",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100033124.1.9",
          "form": "오늘은 바람도 꽤 시원하네",
          "original_form": "오늘은 바람도 꽤 시원하네",
          "speaker_id": "2",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100033124.1.10",
          "form": "바람이 시원한 정도가 아니고 완전 세구만",
          "original_form": "바람이 시원한 정도가 아니고 완전 세구만",
          "speaker_id": "1",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100033124.1.11",
          "form": "그래? 아까 편의점 잠깐 갈 때는 시원하던데 ㅎㅎ",
          "original_form": "그래? 아까 편의점 잠깐 갈 때는 시원하던데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100033124.1.12",
          "form": "ㅎㅎ 신랑 있는 곳이랑 바람 세기가 다른가보네",
          "original_form": "ㅎㅎ 신랑 있는 곳이랑 바람 세기가 다른가보네",
          "speaker_id": "1",
          "time": "20210906 16:59"
        },
        {
          "id": "MDRW2100033124.1.13",
          "form": "아.. 나 퇴근할 때는 비 안내렸으면",
          "original_form": "아.. 나 퇴근할 때는 비 안내렸으면",
