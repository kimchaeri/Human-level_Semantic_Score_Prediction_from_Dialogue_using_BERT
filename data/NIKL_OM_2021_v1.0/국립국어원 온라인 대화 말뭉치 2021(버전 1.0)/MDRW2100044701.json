      "utterance": [
        {
          "id": "MDRW2100044701.1.1",
          "form": "네 과일 좋아해요",
          "original_form": "네 과일 좋아해요",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044701.1.2",
          "form": "어떤과일 가장 좋아하세요?",
          "original_form": "어떤과일 가장 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044701.1.3",
          "form": "주로 껍질 깎지 않고 먹을 수 있는 과일이요",
          "original_form": "주로 껍질 깎지 않고 먹을 수 있는 과일이요",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044701.1.4",
          "form": "딸기 귤",
          "original_form": "딸기 귤",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044701.1.5",
          "form": "그리고 여름과일들 좋아해요",
          "original_form": "그리고 여름과일들 좋아해요",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044701.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ저는 수박이랑 귤이요!!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저는 수박이랑 귤이요!!!",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044701.1.7",
          "form": "진짜 수박 너무사랑해요ㅠ",
          "original_form": "진짜 수박 너무사랑해요ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044701.1.8",
          "form": "수박도 좋아요",
          "original_form": "수박도 좋아요",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044701.1.9",
          "form": "깎기가 귀찮지만 ㅠㅠㅠ",
          "original_form": "깎기가 귀찮지만 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044701.1.10",
          "form": "여름에 수박 한 통 썰어놓고 먹음",
          "original_form": "여름에 수박 한 통 썰어놓고 먹음",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044701.1.11",
          "form": "행복하죠",
          "original_form": "행복하죠",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044701.1.12",
          "form": "수박 때문에 여름에 음쓰 진짜 자주 버리러가요 ㅋㅋㅋㅋ",
          "original_form": "수박 때문에 여름에 음쓰 진짜 자주 버리러가요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044701.1.13",
          "form": "껍질ㅋㅋㅋㅋ",
          "original_form": "껍질ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044701.1.14",
          "form": "맞아요 미리 썰어두면 마음든든",
          "original_form": "맞아요 미리 썰어두면 마음든든",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044701.1.15",
          "form": "네 맞아요 쓰레기는 많이 나오는데",
          "original_form": "네 맞아요 쓰레기는 많이 나오는데",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044701.1.16",
          "form": "미리 한통 가득 썰어두면 진짜 뿌듯하죠",
          "original_form": "미리 한통 가득 썰어두면 진짜 뿌듯하죠",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044701.1.17",
          "form": "맞아요 ㅋㅋㅋ통에 착착 넣어서 냉장고에 탁 넣어두면",
          "original_form": "맞아요 ㅋㅋㅋ통에 착착 넣어서 냉장고에 탁 넣어두면",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044701.1.18",
          "form": "진짜 맘이 든든ㅋㅋㅋ",
          "original_form": "진짜 맘이 든든ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044701.1.19",
          "form": "네 더울때 수분보충도 되고",
          "original_form": "네 더울때 수분보충도 되고",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044701.1.20",
          "form": "맞아요ㅠㅋㅋㅋ그치만 여름에 수박때문에 살찌더라구요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "맞아요ㅠㅋㅋㅋ그치만 여름에 수박때문에 살찌더라구요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044701.1.21",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044701.1.22",
          "form": "당분도 많고",
          "original_form": "당분도 많고",
          "speaker_id": "2",
