        },
        {
          "id": "MDRW2100008369.1.2",
          "form": "name1 님도 좋아하새요?",
          "original_form": "&name1& 님도 좋아하새요?",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008369.1.3",
          "form": "네 햄버거 자주먹어요",
          "original_form": "네 햄버거 자주먹어요",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008369.1.4",
          "form": "어디브랜드 좋아하시나요",
          "original_form": "어디브랜드 좋아하시나요",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008369.1.5",
          "form": "저는 맥날 파예요",
          "original_form": "저는 맥날 파예요",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008369.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008369.1.7",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008369.1.8",
          "form": "맥날 가장 좋아해요",
          "original_form": "맥날 가장 좋아해요",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008369.1.9",
          "form": "어떤 메뉴 즐겨드시나요",
          "original_form": "어떤 메뉴 즐겨드시나요",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008369.1.10",
          "form": "저는 상하이 좋아해요",
          "original_form": "저는 상하이 좋아해요",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008369.1.11",
          "form": "저는 빅맥이요!",
          "original_form": "저는 빅맥이요!",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008369.1.12",
          "form": "치즈 들어간게 너무 맛있어요",
          "original_form": "치즈 들어간게 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008369.1.13",
          "form": "사이트 메뉴는 뭐가 제일 좋으세요?",
          "original_form": "사이트 메뉴는 뭐가 제일 좋으세요?",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008369.1.14",
          "form": "사이드 감자튀김 치즈스틱 전부좋아요",
          "original_form": "사이드 감자튀김 치즈스틱 전부좋아요",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008369.1.15",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008369.1.16",
          "form": "저는 감자튀김 너무 좋아해요",
          "original_form": "저는 감자튀김 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008369.1.17",
          "form": "어떤종류의 감자튀김 좋아하세요",
          "original_form": "어떤종류의 감자튀김 좋아하세요",
          "speaker_id": "1",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008369.1.18",
          "form": "전에는 대용량으로 사서 에어프라이기로 튀겨먹고 햤어요 ㅎㅎ",
          "original_form": "전에는 대용량으로 사서 에어프라이기로 튀겨먹고 햤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008369.1.19",
          "form": "맘터 감튀가 재일 맛있더라고요",
          "original_form": "맘터 감튀가 재일 맛있더라고요",
          "speaker_id": "2",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008369.1.20",
          "form": "케이준감튀요?",
          "original_form": "케이준감튀요?",
          "speaker_id": "1",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008369.1.21",
          "form": "확실히 양념되어있는게 매력있더라구요",
          "original_form": "확실히 양념되어있는게 매력있더라구요",
          "speaker_id": "1",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008369.1.22",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008369.1.23",
          "form": "짠걸 되게 좋아해서",
          "original_form": "짠걸 되게 좋아해서",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008369.1.24",
          "form": "마라탕은 드셔보셨나요?",
          "original_form": "마라탕은 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008369.1.25",
          "form": "마라탕 안먹어봤어요ㅠㅠ",
          "original_form": "마라탕 안먹어봤어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008369.1.26",
          "form": "마라탕은 어떤가요",
          "original_form": "마라탕은 어떤가요",
          "speaker_id": "1",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008369.1.27",
          "form": "맵고 짜고 맛있어요!",
          "original_form": "맵고 짜고 맛있어요!",
          "speaker_id": "2",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008369.1.28",
          "form": "건강 나빠지는 맛이에요 ㅎㅎ",
          "original_form": "건강 나빠지는 맛이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008369.1.29",
          "form": "자극적이라는 소리군요",
          "original_form": "자극적이라는 소리군요",
          "speaker_id": "1",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008369.1.30",
          "form": "사실 그런게 맛있어요ㅋㅋ",
