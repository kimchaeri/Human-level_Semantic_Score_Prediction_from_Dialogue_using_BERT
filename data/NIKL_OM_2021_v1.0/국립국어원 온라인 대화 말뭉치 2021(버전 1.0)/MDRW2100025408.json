      "utterance": [
        {
          "id": "MDRW2100025408.1.1",
          "form": "요즘 유튜브 뭐볼거있나여",
          "original_form": "요즘 유튜브 뭐볼거있나여",
          "speaker_id": "1",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100025408.1.2",
          "form": "전 재태크랑 고양이 관련으로 봐요ㅎ",
          "original_form": "전 재태크랑 고양이 관련으로 봐요ㅎ",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025408.1.3",
          "form": "아 요즘 재태크가 또 난리져",
          "original_form": "아 요즘 재태크가 또 난리져",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025408.1.4",
          "form": "다들 월급오르기보다 내삼성주식 오르는거 좋다고",
          "original_form": "다들 월급오르기보다 내삼성주식 오르는거 좋다고",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025408.1.5",
          "form": "하더라구요ㅋㅋ",
          "original_form": "하더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025408.1.6",
          "form": "전 없어서ㅋㅋ",
          "original_form": "전 없어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025408.1.7",
          "form": "ㅋㅋㅋ 저도 투자할돈이없어서 시작도못하고있어요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 저도 투자할돈이없어서 시작도못하고있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025408.1.8",
          "form": "제 음식에 투자하고 있어서ㅋㅋㅋ",
          "original_form": "제 음식에 투자하고 있어서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025408.1.9",
          "form": "즐겨보는 채널있으신가요?",
          "original_form": "즐겨보는 채널있으신가요?",
          "speaker_id": "2",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025408.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저는 먹방이나 브이로그같은거많이봐요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저는 먹방이나 브이로그같은거많이봐요",
          "speaker_id": "1",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025408.1.11",
          "form": "먹방보면 배고프지 않나요?",
          "original_form": "먹방보면 배고프지 않나요?",
          "speaker_id": "2",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025408.1.12",
          "form": "맞아요 배고프지만 대리만족ㅋㅋㅋ",
          "original_form": "맞아요 배고프지만 대리만족ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025408.1.13",
          "form": "전 못참고 야식먹을거 같아요ㅜㅜ",
          "original_form": "전 못참고 야식먹을거 같아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 11:26"
        },
        {
          "id": "MDRW2100025408.1.14",
          "form": "요즘 야식도뭔가먹을게없더라구요",
          "original_form": "요즘 야식도뭔가먹을게없더라구요",
          "speaker_id": "1",
          "time": "20210901 11:29"
        },
        {
          "id": "MDRW2100025408.1.15",
          "form": "세상에나..",
          "original_form": "세상에나..",
          "speaker_id": "2",
          "time": "20210901 11:30"
        },
        {
          "id": "MDRW2100025408.1.16",
          "form": "얼마나 많은데요",
          "original_form": "얼마나 많은데요",
          "speaker_id": "2",
          "time": "20210901 11:30"
        },
        {
          "id": "MDRW2100025408.1.17",
          "form": "먹태사랑합니다",
          "original_form": "먹태사랑합니다",
          "speaker_id": "2",
          "time": "20210901 11:30"
        },
        {
          "id": "MDRW2100025408.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ먹태맛있쪄 그거만먹을수가없잖아요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ먹태맛있쪄 그거만먹을수가없잖아요ㅋㅋㅋ",
