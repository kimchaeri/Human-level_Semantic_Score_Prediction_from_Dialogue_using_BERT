        },
        {
          "id": "MDRW2100009240.1.3",
          "form": "요즘 날씨 많이풀렸죠 ㅎ",
          "original_form": "요즘 날씨 많이풀렸죠 ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009240.1.4",
          "form": "네 이제 좀 시원해요",
          "original_form": "네 이제 좀 시원해요",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009240.1.5",
          "form": "가을옷 준비좀 해야겟어용",
          "original_form": "가을옷 준비좀 해야겟어용",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009240.1.6",
          "form": "그러게요 가을도 금새지나갈꺼같아요",
          "original_form": "그러게요 가을도 금새지나갈꺼같아요",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009240.1.7",
          "form": "이제 우리나라도 이계절이잖아요 ㅠㅠ",
          "original_form": "이제 우리나라도 이계절이잖아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100009240.1.8",
          "form": "그니까요 곧겨울ㅠ",
          "original_form": "그니까요 곧겨울ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009240.1.9",
          "form": "요즘 어떤스탈로 입고다니세요?",
          "original_form": "요즘 어떤스탈로 입고다니세요?",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
