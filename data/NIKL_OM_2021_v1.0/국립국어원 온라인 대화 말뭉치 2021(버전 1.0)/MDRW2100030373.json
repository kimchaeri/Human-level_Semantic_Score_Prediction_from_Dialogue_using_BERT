      "utterance": [
        {
          "id": "MDRW2100030373.1.1",
          "form": "게임 좀 하시나여?",
          "original_form": "게임 좀 하시나여?",
          "speaker_id": "1",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100030373.1.2",
          "form": "모바겜만 좀 해요",
          "original_form": "모바겜만 좀 해요",
          "speaker_id": "2",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100030373.1.3",
          "form": "뭐하세요??쿠킹덤?",
          "original_form": "뭐하세요??쿠킹덤?",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030373.1.4",
          "form": "넼 최근엔 쿠킹덤이랑 로오히 두개 하고 있어요",
          "original_form": "넼 최근엔 쿠킹덤이랑 로오히 두개 하고 있어요",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100030373.1.5",
          "form": "오... 게임 주제로 대화하는거 괜찮으신가요?",
          "original_form": "오... 게임 주제로 대화하는거 괜찮으신가요?",
          "speaker_id": "1",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100030373.1.6",
          "form": "네 괜찮아요 ㅋㅋ",
          "original_form": "네 괜찮아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030373.1.7",
          "form": "쿠킹덤에서 좋아하는 캐릭터가 있으신가요?",
          "original_form": "쿠킹덤에서 좋아하는 캐릭터가 있으신가요?",
          "speaker_id": "1",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030373.1.8",
          "form": "로오히는 광고가 멋졌던거로 기억해요",
          "original_form": "로오히는 광고가 멋졌던거로 기억해요",
          "speaker_id": "1",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030373.1.9",
          "form": "쿠킹덤에 바다요정 쿠키 좋아하고 있어요",
          "original_form": "쿠킹덤에 바다요정 쿠키 좋아하고 있어요",
          "speaker_id": "2",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030373.1.10",
          "form": "저도 로오히 지하철 광고로 올라왔던 게 정말 멋있었다고 생각해요 :D",
          "original_form": "저도 로오히 지하철 광고로 올라왔던 게 정말 멋있었다고 생각해요 :D",
          "speaker_id": "2",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030373.1.11",
          "form": "그 걷는게 너무 멋있었어요",
          "original_form": "그 걷는게 너무 멋있었어요",
          "speaker_id": "1",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030373.1.12",
          "form": "바다요정!",
          "original_form": "바다요정!",
          "speaker_id": "1",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030373.1.13",
          "form": "쿠키런때부터 좋아했어요",
          "original_form": "쿠키런때부터 좋아했어요",
          "speaker_id": "1",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030373.1.14",
          "form": "한번도 얻은적은 없지만...",
          "original_form": "한번도 얻은적은 없지만...",
          "speaker_id": "1",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030373.1.15",
          "form": "지방 살아서 직접 못봤던 게 너무 아쉬웠어요ㅜ",
          "original_form": "지방 살아서 직접 못봤던 게 너무 아쉬웠어요ㅜ",
          "speaker_id": "2",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030373.1.16",
          "form": "앗.. 혹시 지금은 쿠키런이나 쿠킹덤 안하시나요?",
          "original_form": "앗.. 혹시 지금은 쿠키런이나 쿠킹덤 안하시나요?",
          "speaker_id": "2",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030373.1.17",
          "form": "지금은 게임을 접었어요 ㅎㅎㅎ",
          "original_form": "지금은 게임을 접었어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:38"
        },
        {
          "id": "MDRW2100030373.1.18",
          "form": "앟 그러시구나 ㅎㅎ",
          "original_form": "앟 그러시구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030373.1.19",
          "form": "이전에도 즐겨하신 게임이 있으신가요?",
          "original_form": "이전에도 즐겨하신 게임이 있으신가요?",
          "speaker_id": "1",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100030373.1.20",
          "form": "전엔 오버워치나 롤은 종종 했었는데 접은지 좀 오래됐어요;",
          "original_form": "전엔 오버워치나 롤은 종종 했었는데 접은지 좀 오래됐어요;",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030373.1.21",
          "form": "와 롤도 하셨었구나",
          "original_form": "와 롤도 하셨었구나",
          "speaker_id": "1",
          "time": "20210904 00:50"
        }
      ]
