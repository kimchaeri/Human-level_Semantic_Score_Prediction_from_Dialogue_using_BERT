      "utterance": [
        {
          "id": "MDRW2100040098.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210904 09:46"
        },
        {
          "id": "MDRW2100040098.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210904 09:52"
        },
        {
          "id": "MDRW2100040098.1.3",
          "form": "선선한 아침이네요",
          "original_form": "선선한 아침이네요",
          "speaker_id": "2",
          "time": "20210904 09:52"
        },
        {
          "id": "MDRW2100040098.1.4",
          "form": "네 완연한 가을 날씨에요:)",
          "original_form": "네 완연한 가을 날씨에요:)",
          "speaker_id": "1",
          "time": "20210904 09:56"
        },
        {
          "id": "MDRW2100040098.1.5",
          "form": "저는 방금 백신접종 맞고와서",
          "original_form": "저는 방금 백신접종 맞고와서",
          "speaker_id": "2",
          "time": "20210904 10:26"
        },
        {
          "id": "MDRW2100040098.1.6",
          "form": "오늘 좀 쉬려고해요 ㅋㅋ",
          "original_form": "오늘 좀 쉬려고해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:26"
        },
        {
          "id": "MDRW2100040098.1.7",
          "form": "헛 전 어제 ㅋㅋ \r\n그래도 오랜만에 날씨가 좋으니 가볍게 산책할려구요 ㅎㅎ",
          "original_form": "헛 전 어제 ㅋㅋ \r\n그래도 오랜만에 날씨가 좋으니 가볍게 산책할려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100040098.1.8",
          "form": "오 좀 괜찮으신가봐요",
          "original_form": "오 좀 괜찮으신가봐요",
          "speaker_id": "2",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100040098.1.9",
          "form": "네네 주사맞은 팔이 좀 아프긴 한데 컨디션은 괜찮아요 ㅎㅎ",
          "original_form": "네네 주사맞은 팔이 좀 아프긴 한데 컨디션은 괜찮아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100040098.1.10",
          "form": "저 2차맞고 왔는데 2차는 좀 아프다 들어서 그냥 하루종일 누워있으려고요ㅎㅎ",
          "original_form": "저 2차맞고 왔는데 2차는 좀 아프다 들어서 그냥 하루종일 누워있으려고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100040098.1.11",
          "form": "맞아요 전 1차라 조금 양호한가봐요",
          "original_form": "맞아요 전 1차라 조금 양호한가봐요",
          "speaker_id": "1",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100040098.1.12",
          "form": "넹 선선해서 산책하기 참 좋은 날씨겠어요",
          "original_form": "넹 선선해서 산책하기 참 좋은 날씨겠어요",
          "speaker_id": "2",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100040098.1.13",
          "form": "제몫까지 잘 다녀와주세요ㅎㅎㅎ ㅠ",
          "original_form": "제몫까지 잘 다녀와주세요ㅎㅎㅎ ㅠ",
          "speaker_id": "2",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100040098.1.14",
          "form": "2차는 쉬어야해요 ㅎㅎ 오늘 날이 좋아서 외출했을때 괜찮았을거 같은데ㅎㅎ",
          "original_form": "2차는 쉬어야해요 ㅎㅎ 오늘 날이 좋아서 외출했을때 괜찮았을거 같은데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100040098.1.15",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100040098.1.16",
          "form": "불편한곳은 없으시죠?",
          "original_form": "불편한곳은 없으시죠?",
          "speaker_id": "1",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100040098.1.17",
          "form": "넹 아직은 세시간 정도 지나서 그런지",
          "original_form": "넹 아직은 세시간 정도 지나서 그런지",
          "speaker_id": "2",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100040098.1.18",
          "form": "별 이상이 없어요 좀 뻐근함 정도?",
          "original_form": "별 이상이 없어요 좀 뻐근함 정도?",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100040098.1.19",
          "form": "다행이에요 ",
          "original_form": "다행이에요 ",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100040098.1.20",
          "form": "ㅋㅋㅋ더 아파지기전에 먹을거 사러 편의점 나들이 해야겠어요",
          "original_form": "ㅋㅋㅋ더 아파지기전에 먹을거 사러 편의점 나들이 해야겠어요",
          "speaker_id": "2",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100040098.1.21",
          "form": "좋은생각입니당ㅋㅋㅋㅋ",
          "original_form": "좋은생각입니당ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100040098.1.22",
          "form": "그것도 산책이에요ㅋㅋㅋㅋ",
          "original_form": "그것도 산책이에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:34"
        }
      ]
    }
