        {
          "id": "MDRW2100029487.1.1",
          "form": "당근마켓 자주 이용하시나요?",
          "original_form": "당근마켓 자주 이용하시나요?",
          "speaker_id": "2",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100029487.1.2",
          "form": "당근 하긴 하는데",
          "original_form": "당근 하긴 하는데",
          "speaker_id": "1",
          "time": "20210903 09:41"
        },
        {
          "id": "MDRW2100029487.1.3",
          "form": "팔기만 해요",
          "original_form": "팔기만 해요",
          "speaker_id": "1",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100029487.1.4",
          "form": "저는 팔기도하고 무료받기도하고 ㅋ",
          "original_form": "저는 팔기도하고 무료받기도하고 ㅋ",
          "speaker_id": "2",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100029487.1.5",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100029487.1.6",
          "form": "어떤거 파시나요?",
          "original_form": "어떤거 파시나요?",
          "speaker_id": "2",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100029487.1.7",
          "form": "공기청정기요 최근에",
          "original_form": "공기청정기요 최근에",
          "speaker_id": "1",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100029487.1.8",
          "form": "큰거파셨네요",
          "original_form": "큰거파셨네요",
          "speaker_id": "2",
          "time": "20210903 10:15"
        },
        {
          "id": "MDRW2100029487.1.9",
          "form": "차량용이라소형이에요",
          "original_form": "차량용이라소형이에요",
          "speaker_id": "1",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100029487.1.10",
          "form": "바로 팔리나요?",
          "original_form": "바로 팔리나요?",
          "speaker_id": "2",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100029487.1.11",
