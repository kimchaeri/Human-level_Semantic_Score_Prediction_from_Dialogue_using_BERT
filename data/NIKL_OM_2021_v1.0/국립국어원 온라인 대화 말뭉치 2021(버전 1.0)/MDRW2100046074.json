          "speaker_id": "2",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046074.1.3",
          "form": "어디 브랜드야",
          "original_form": "어디 브랜드야",
          "speaker_id": "1",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046074.1.4",
          "form": "샘표꺼",
          "original_form": "샘표꺼",
          "speaker_id": "2",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046074.1.5",
          "form": "참기름 어떤 요리에 주로 써",
          "original_form": "참기름 어떤 요리에 주로 써",
          "speaker_id": "1",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046074.1.6",
          "form": "볶음밥에 정말 좋아",
          "original_form": "볶음밥에 정말 좋아",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046074.1.7",
          "form": "나도 김치 볶음밥 에 자주 뿌려먹어",
          "original_form": "나도 김치 볶음밥 에 자주 뿌려먹어",
          "speaker_id": "1",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046074.1.8",
          "form": "맞아 기름종류도다양해",
          "original_form": "맞아 기름종류도다양해",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046074.1.9",
          "form": "들기름도 있지",
          "original_form": "들기름도 있지",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046074.1.10",
          "form": "들기름이 더 고소한것같아",
          "original_form": "들기름이 더 고소한것같아",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046074.1.11",
          "form": "들기름이 몸에도 좋대",
          "original_form": "들기름이 몸에도 좋대",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046074.1.12",
          "form": "맞아 저온압축이 몸에좋다더라",
          "original_form": "맞아 저온압축이 몸에좋다더라",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046074.1.13",
          "form": "요리 뭐해서 먹을거야 오늘",
          "original_form": "요리 뭐해서 먹을거야 오늘",
          "speaker_id": "1",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046074.1.14",
          "form": "난 오늘 계란볶음밥해먹을거야",
          "original_form": "난 오늘 계란볶음밥해먹을거야",
          "speaker_id": "2",
          "time": "20210908 10:15"
        },
