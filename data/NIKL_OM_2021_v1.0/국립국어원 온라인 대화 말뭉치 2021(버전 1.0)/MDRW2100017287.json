      "utterance": [
        {
          "id": "MDRW2100017287.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017287.1.2",
          "form": "이제 먹으러 가는 길이에요",
          "original_form": "이제 먹으러 가는 길이에요",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017287.1.3",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017287.1.4",
          "form": "저는 보쌈 먹었어요",
          "original_form": "저는 보쌈 먹었어요",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017287.1.5",
          "form": "헐 완전 맛있는 거 드셨네요",
          "original_form": "헐 완전 맛있는 거 드셨네요",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017287.1.6",
          "form": "시켜먹을건데 급 보쌈 땡기네요",
          "original_form": "시켜먹을건데 급 보쌈 땡기네요",
          "speaker_id": "2",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017287.1.7",
          "form": "ㅎㅎ저녁메뉴 추천입니다",
          "original_form": "ㅎㅎ저녁메뉴 추천입니다",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017287.1.8",
          "form": "단백질이 풍부한 식사메뉴",
          "original_form": "단백질이 풍부한 식사메뉴",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017287.1.9",
          "form": "크크 보쌈 시켰어요 감사합니다",
          "original_form": "크크 보쌈 시켰어요 감사합니다",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017287.1.10",
          "form": "ㅎㅎ저녁메뉴 정해드렸네요",
          "original_form": "ㅎㅎ저녁메뉴 정해드렸네요",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017287.1.11",
          "form": "맛있게 드세요~",
          "original_form": "맛있게 드세요~",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017287.1.12",
          "form": "네ㅋㅋㅋ 운동을 격하게 하다와서 고기 먹어야겠네요",
          "original_form": "네ㅋㅋㅋ 운동을 격하게 하다와서 고기 먹어야겠네요",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017287.1.13",
          "form": "오 운동하고 오셨구나",
          "original_form": "오 운동하고 오셨구나",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017287.1.14",
          "form": "저는 오늘 운동은 안하고 든든히 먹기만ㅎㅎ",
          "original_form": "저는 오늘 운동은 안하고 든든히 먹기만ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017287.1.15",
          "form": "저도 그게 어제였는데 양심이 찔려서 그만",
          "original_form": "저도 그게 어제였는데 양심이 찔려서 그만",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017287.1.16",
          "form": "아아 저도 오늘 식사 든든히 했으니 내일은 운동해야겠어요",
          "original_form": "아아 저도 오늘 식사 든든히 했으니 내일은 운동해야겠어요",
          "speaker_id": "1",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017287.1.17",
          "form": "ㅋㅋㅋ보쌈은 근데 단백질이니까 건강한 음식 마자요",
          "original_form": "ㅋㅋㅋ보쌈은 근데 단백질이니까 건강한 음식 마자요",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017287.1.18",
          "form": "그쵸?",
          "original_form": "그쵸?",
          "speaker_id": "1",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017287.1.19",
          "form": "단백질 덩어리",
          "original_form": "단백질 덩어리",
          "speaker_id": "1",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017287.1.20",
          "form": "밥이나 면은 안먹었으니 건강식이었다고 생각합니다",
          "original_form": "밥이나 면은 안먹었으니 건강식이었다고 생각합니다",
          "speaker_id": "1",
          "time": "20210826 18:43"
        }
      ]
