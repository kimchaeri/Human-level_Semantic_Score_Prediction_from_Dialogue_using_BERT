        {
          "id": "MDRW2100001085.1.6",
          "form": "저도ㅠㅠ",
          "original_form": "저도ㅠㅠ",
          "speaker_id": "2",
          "time": "20210723 15:44"
        },
        {
          "id": "MDRW2100001085.1.7",
          "form": "겨울이 더 좋아요~",
          "original_form": "겨울이 더 좋아요~",
          "speaker_id": "2",
          "time": "20210723 15:45"
        },
        {
          "id": "MDRW2100001085.1.8",
          "form": "저도요! 전 가을이 젤 좋아요",
          "original_form": "저도요! 전 가을이 젤 좋아요",
          "speaker_id": "1",
          "time": "20210723 15:45"
        },
        {
          "id": "MDRW2100001085.1.9",
          "form": "시원하고 벌레도 없고 ㅎㅎㅎㅎ",
          "original_form": "시원하고 벌레도 없고 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210723 15:45"
        },
        {
          "id": "MDRW2100001085.1.10",
          "form": "저도 가을 좋아요!",
          "original_form": "저도 가을 좋아요!",
          "speaker_id": "2",
          "time": "20210723 15:46"
        },
        {
          "id": "MDRW2100001085.1.11",
          "form": "그런데 겨울옷이 전 예쁜것같더라구요",
          "original_form": "그런데 겨울옷이 전 예쁜것같더라구요",
          "speaker_id": "2",
          "time": "20210723 15:47"
        },
        {
          "id": "MDRW2100001085.1.12",
          "form": "아 맞아요 전 코트 좋아해요!!",
          "original_form": "아 맞아요 전 코트 좋아해요!!",
          "speaker_id": "1",
          "time": "20210723 15:48"
        },
        {
          "id": "MDRW2100001085.1.13",
          "form": "name2 님은 좋아하는 옷 스타일이 어떻게 되세요!",
          "original_form": "&name2& 님은 좋아하는 옷 스타일이 어떻게 되세요!",
          "speaker_id": "1",
          "time": "20210723 15:48"
        },
        {
          "id": "MDRW2100001085.1.14",
          "form": "저는 편한거 좋아해요",
          "original_form": "저는 편한거 좋아해요",
          "speaker_id": "2",
          "time": "20210723 15:48"
        },
        {
