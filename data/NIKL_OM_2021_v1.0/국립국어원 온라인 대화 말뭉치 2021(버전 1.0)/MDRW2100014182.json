      "utterance": [
        {
          "id": "MDRW2100014182.1.1",
          "form": "사탄님이 기뻐하시는 게 분명한 날씨",
          "original_form": "사탄님이 기뻐하시는 게 분명한 날씨",
          "speaker_id": "2",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014182.1.2",
          "form": "거긴 아직 바람 많이 불어요?",
          "original_form": "거긴 아직 바람 많이 불어요?",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014182.1.3",
          "form": "비도 바람도 와르르입니다",
          "original_form": "비도 바람도 와르르입니다",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014182.1.4",
          "form": "창문도 와르르",
          "original_form": "창문도 와르르",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014182.1.5",
          "form": "헐 무섭네요 저희는 낮부터 평화로운데 왜지???",
          "original_form": "헐 무섭네요 저희는 낮부터 평화로운데 왜지???",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014182.1.6",
          "form": "잠와죽겠다잠와죽겠다",
          "original_form": "잠와죽겠다잠와죽겠다",
          "speaker_id": "2",
          "time": "20210826 04:04"
        },
        {
          "id": "MDRW2100014182.1.7",
          "form": "와 아까 뉴스보니까",
          "original_form": "와 아까 뉴스보니까",
          "speaker_id": "2",
          "time": "20210826 04:04"
        },
        {
          "id": "MDRW2100014182.1.8",
          "form": "도로가 물에 잠겼더라고요",
          "original_form": "도로가 물에 잠겼더라고요",
          "speaker_id": "2",
          "time": "20210826 04:04"
        },
        {
          "id": "MDRW2100014182.1.9",
          "form": "지하도 절대 드가지말라카고",
          "original_form": "지하도 절대 드가지말라카고",
          "speaker_id": "2",
          "time": "20210826 04:04"
        },
        {
          "id": "MDRW2100014182.1.10",
          "form": "내일 출근 어케 하냐",
          "original_form": "내일 출근 어케 하냐",
          "speaker_id": "2",
          "time": "20210826 04:04"
        },
        {
          "id": "MDRW2100014182.1.11",
          "form": "헐....오늘괜찮았나요????",
          "original_form": "헐....오늘괜찮았나요????",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100014182.1.12",
          "form": "무사히출근하셨는지...",
          "original_form": "무사히출근하셨는지...",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100014182.1.13",
          "form": "오늘은 쨍쨍하더라고요???",
          "original_form": "오늘은 쨍쨍하더라고요???",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100014182.1.14",
          "form": "알수없는 반도날씨...",
          "original_form": "알수없는 반도날씨...",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100014182.1.15",
          "form": "지금 땀 쫙쫙 빼고 있습니다",
          "original_form": "지금 땀 쫙쫙 빼고 있습니다",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100014182.1.16",
          "form": "와....저희쪽도 새벽에 비엄청오다가 아침되니까 쨍쨍하더라고요",
          "original_form": "와....저희쪽도 새벽에 비엄청오다가 아침되니까 쨍쨍하더라고요",
          "speaker_id": "1",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100014182.1.17",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "1",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100014182.1.18",
          "form": "과메기 되는 중입니다",
          "original_form": "과메기 되는 중입니다",
          "speaker_id": "2",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100014182.1.19",
          "form": "진짜 개더워",
          "original_form": "진짜 개더워",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100014182.1.20",
          "form": "날 익혀먹어봤자",
          "original_form": "날 익혀먹어봤자",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100014182.1.21",
          "form": "과메깅ㄴ간",
          "original_form": "과메깅ㄴ간",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100014182.1.22",
          "form": "병만 얻을텐데",
          "original_form": "병만 얻을텐데",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100014182.1.23",
          "form": "과메기인간",
          "original_form": "과메기인간",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100014182.1.24",
          "form": "요즘도",
          "original_form": "요즘도",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100014182.1.25",
          "form": "반팔입어야해요?",
          "original_form": "반팔입어야해요?",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100014182.1.26",
          "form": "아이스크림 먹고싶다...",
          "original_form": "아이스크림 먹고싶다...",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100014182.1.27",
          "form": "나시입는데요",
          "original_form": "나시입는데요",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100014182.1.28",
          "form": "그정도구나",
          "original_form": "그정도구나",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100014182.1.29",
          "form": "집에서 빤스랑 나시만 입고",
          "original_form": "집에서 빤스랑 나시만 입고",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100014182.1.30",
          "form": "에어컨 트는데요",
          "original_form": "에어컨 트는데요",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100014182.1.31",
          "form": "빨리 겨울돼서",
          "original_form": "빨리 겨울돼서",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100014182.1.32",
          "form": "털잠옷입고싶어요",
          "original_form": "털잠옷입고싶어요",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100014182.1.33",
          "form": "겨울 낮 너무 짧아서 싫어요",
          "original_form": "겨울 낮 너무 짧아서 싫어요",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100014182.1.34",
          "form": "롱패딩도",
          "original_form": "롱패딩도",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100014182.1.35",
          "form": "딴건 다 좋은데",
          "original_form": "딴건 다 좋은데",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100014182.1.36",
          "form": "낮이 긴 겨울이 필요해요",
          "original_form": "낮이 긴 겨울이 필요해요",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100014182.1.37",
          "form": "겨울낮이짧아서좋아요",
          "original_form": "겨울낮이짧아서좋아요",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100014182.1.38",
          "form": "밤이길어야해",
          "original_form": "밤이길어야해",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100014182.1.39",
          "form": "끔찍해요",
          "original_form": "끔찍해요",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100014182.1.40",
          "form": "우울한 계절",
          "original_form": "우울한 계절",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100014182.1.41",
          "form": "그냥..그냥..그러면행복해요",
          "original_form": "그냥..그냥..그러면행복해요",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100014182.1.42",
          "form": "퇴근할때 해떠있는 게 좋다고요",
          "original_form": "퇴근할때 해떠있는 게 좋다고요",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100014182.1.43",
          "form": "해지고 퇴근하면",
          "original_form": "해지고 퇴근하면",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100014182.1.44",
          "form": "그건맞는말이지만",
          "original_form": "그건맞는말이지만",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100014182.1.45",
          "form": "완전 화가 나요",
          "original_form": "완전 화가 나요",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100014182.1.46",
          "form": "전 집에만있으니까요",
          "original_form": "전 집에만있으니까요",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100014182.1.47",
          "form": "완전 화가 나요",
          "original_form": "완전 화가 나요",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100014182.1.48",
          "form": "그렇구나...",
          "original_form": "그렇구나...",
          "speaker_id": "1",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100014182.1.49",
          "form": "저도 화낼수있는사람이되면좋을텐데",
          "original_form": "저도 화낼수있는사람이되면좋을텐데",
          "speaker_id": "1",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100014182.1.50",
          "form": "말이에요...",
          "original_form": "말이에요...",
          "speaker_id": "1",
          "time": "20210826 17:09"
        }
      ]
    }
