        },
        {
          "id": "MDRW2100041857.1.2",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "2",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100041857.1.3",
          "form": "배달 자주 시켜드시나요?",
          "original_form": "배달 자주 시켜드시나요?",
          "speaker_id": "2",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041857.1.4",
          "form": "주1.2회정도요",
          "original_form": "주1.2회정도요",
          "speaker_id": "1",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100041857.1.5",
          "form": "주로 어떤거 시켜드시나요?",
          "original_form": "주로 어떤거 시켜드시나요?",
          "speaker_id": "2",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100041857.1.6",
          "form": "치킨 자주먹고  피자도 종종",
          "original_form": "치킨 자주먹고  피자도 종종",
          "speaker_id": "1",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100041857.1.7",
          "form": "시켜먹어요",
          "original_form": "시켜먹어요",
          "speaker_id": "1",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100041857.1.8",
          "form": "오 저도 치킨 피자 자주시켜먹어요",
          "original_form": "오 저도 치킨 피자 자주시켜먹어요",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100041857.1.9",
          "form": "어떤 치킨 좋아하세요?",
          "original_form": "어떤 치킨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100041857.1.10",
          "form": "교촌치킨이나 네네요~",
          "original_form": "교촌치킨이나 네네요~",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100041857.1.11",
          "form": "둘다 맛있죠 저도 교촌 자주 시켜먹어요 허니콤보ㅎㅎ",
          "original_form": "둘다 맛있죠 저도 교촌 자주 시켜먹어요 허니콤보ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100041857.1.12",
          "form": "콤보가 최고ㅎ",
          "original_form": "콤보가 최고ㅎ",
          "speaker_id": "1",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100041857.1.13",
          "form": "그쳐 교촌은 콤보가 젤 맛있어요",
          "original_form": "그쳐 교촌은 콤보가 젤 맛있어요",
          "speaker_id": "2",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100041857.1.14",
          "form": "맞아용 저는 레드콤보ㅠㅠ",
          "original_form": "맞아용 저는 레드콤보ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:00"
        },
        {
          "id": "MDRW2100041857.1.15",
          "form": "말하니 먹고싶네여ㅠ",
          "original_form": "말하니 먹고싶네여ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:00"
        },
        {
          "id": "MDRW2100041857.1.16",
          "form": "그러니까요.. 혹시 교촌 후라이드 드셔보셨나요?",
          "original_form": "그러니까요.. 혹시 교촌 후라이드 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210906 21:01"
        },
        {
          "id": "MDRW2100041857.1.17",
          "form": "궁금은 한데 한번도 먹어본적은 없네요",
          "original_form": "궁금은 한데 한번도 먹어본적은 없네요",
          "speaker_id": "2",
          "time": "20210906 21:01"
        },
        {
          "id": "MDRW2100041857.1.18",
          "form": "아니요 저도 후라이드는ㅋㅇ교촌서는 안먹어봤어요",
          "original_form": "아니요 저도 후라이드는ㅋㅇ교촌서는 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210906 21:02"
        },
        {
          "id": "MDRW2100041857.1.19",
          "form": "담에 한번 먹어봐야겠어요",
          "original_form": "담에 한번 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210906 21:04"
        }
      ]
