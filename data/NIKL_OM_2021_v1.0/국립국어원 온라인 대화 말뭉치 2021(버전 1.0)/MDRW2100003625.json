        {
          "id": "MDRW2100003625.1.1",
          "form": "피엑스는 어때요",
          "original_form": "피엑스는 어때요",
          "speaker_id": "1",
          "time": "20210821 09:09"
        },
        {
          "id": "MDRW2100003625.1.2",
          "form": "ㅋㅋ 군대얘기는 참...",
          "original_form": "ㅋㅋ 군대얘기는 참...",
          "speaker_id": "2",
          "time": "20210821 09:09"
        },
        {
          "id": "MDRW2100003625.1.3",
          "form": "끝이 없어요 ㅋㅋㅋ",
          "original_form": "끝이 없어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:09"
        },
        {
          "id": "MDRW2100003625.1.4",
          "form": "피엑스는 어때요",
          "original_form": "피엑스는 어때요",
          "speaker_id": "1",
          "time": "20210821 09:09"
        },
        {
          "id": "MDRW2100003625.1.5",
          "form": "피엑스는",
          "original_form": "피엑스는",
          "speaker_id": "2",
          "time": "20210821 09:09"
        },
        {
          "id": "MDRW2100003625.1.6",
          "form": "물건이 싸서 좋아요",
          "original_form": "물건이 싸서 좋아요",
          "speaker_id": "2",
          "time": "20210821 09:09"
        },
        {
          "id": "MDRW2100003625.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:09"
        },
        {
          "id": "MDRW2100003625.1.8",
          "form": "술이 싸서?",
          "original_form": "술이 싸서?",
          "speaker_id": "2",
          "time": "20210821 09:09"
        },
        {
          "id": "MDRW2100003625.1.9",
          "form": "거기서 제일 좋았던건 뭐에요",
          "original_form": "거기서 제일 좋았던건 뭐에요",
          "speaker_id": "1",
          "time": "20210821 09:09"
        },
        {
          "id": "MDRW2100003625.1.10",
          "form": "술,",
          "original_form": "술,",
          "speaker_id": "1",
          "time": "20210821 09:09"
        },
        {
          "id": "MDRW2100003625.1.11",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210821 09:09"
        },
        {
          "id": "MDRW2100003625.1.12",
          "form": "군대에서 술 먹어도 되나요?",
          "original_form": "군대에서 술 먹어도 되나요?",
          "speaker_id": "1",
          "time": "20210821 09:10"
        },
        {
          "id": "MDRW2100003625.1.13",
          "form": "아는게 없어서",
          "original_form": "아는게 없어서",
          "speaker_id": "1",
          "time": "20210821 09:10"
        },
        {
          "id": "MDRW2100003625.1.14",
          "form": "가끔먹고",
          "original_form": "가끔먹고",
          "speaker_id": "2",
          "time": "20210821 09:10"
        },
        {
          "id": "MDRW2100003625.1.15",
          "form": "휴가나갈때",
          "original_form": "휴가나갈때",
          "speaker_id": "2",
          "time": "20210821 09:10"
        },
        {
          "id": "MDRW2100003625.1.16",
          "form": "사가지고 나가기도 하고",
          "original_form": "사가지고 나가기도 하고",
          "speaker_id": "2",
          "time": "20210821 09:10"
        },
        {
          "id": "MDRW2100003625.1.17",
          "form": "군대는 면세라서",
          "original_form": "군대는 면세라서",
          "speaker_id": "2",
          "time": "20210821 09:10"
        },
        {
          "id": "MDRW2100003625.1.18",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210821 09:10"
        },
        {
          "id": "MDRW2100003625.1.19",
          "form": "술이 싸고",
          "original_form": "술이 싸고",
          "speaker_id": "2",
          "time": "20210821 09:10"
        },
        {
          "id": "MDRW2100003625.1.20",
          "form": "면세구나",
          "original_form": "면세구나",
          "speaker_id": "1",
          "time": "20210821 09:10"
        },
        {
          "id": "MDRW2100003625.1.21",
          "form": "저때는 담배가",
          "original_form": "저때는 담배가",
          "speaker_id": "2",
          "time": "20210821 09:10"
        },
        {
          "id": "MDRW2100003625.1.22",
          "form": "1보루에 오천원도 안했었어요",
          "original_form": "1보루에 오천원도 안했었어요",
          "speaker_id": "2",
          "time": "20210821 09:10"
        },
        {
          "id": "MDRW2100003625.1.23",
          "form": "너무 옛날얘긴가.....",
          "original_form": "너무 옛날얘긴가.....",
          "speaker_id": "2",
          "time": "20210821 09:11"
        },
        {
          "id": "MDRW2100003625.1.24",
          "form": "지금은",
          "original_form": "지금은",
          "speaker_id": "1",
          "time": "20210821 09:11"
        },
        {
          "id": "MDRW2100003625.1.25",
          "form": "비싼가",
          "original_form": "비싼가",
          "speaker_id": "1",
          "time": "20210821 09:11"
        },
        {
          "id": "MDRW2100003625.1.26",
          "form": "예전에 2500원이다 4500원시대가왔으니",
          "original_form": "예전에 2500원이다 4500원시대가왔으니",
          "speaker_id": "1",
          "time": "20210821 09:11"
        },
        {
          "id": "MDRW2100003625.1.27",
          "form": "지금 한갑에 4500",
          "original_form": "지금 한갑에 4500",
          "speaker_id": "2",
          "time": "20210821 09:11"
        },
        {
          "id": "MDRW2100003625.1.28",
          "form": "군대에서 면세담배 아예 안주고",
          "original_form": "군대에서 면세담배 아예 안주고",
          "speaker_id": "2",
          "time": "20210821 09:11"
        },
        {
          "id": "MDRW2100003625.1.29",
