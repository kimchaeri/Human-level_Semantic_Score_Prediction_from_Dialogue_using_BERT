        {
          "id": "MDRW2100041292.1.1",
          "form": "분위기는 대존맛",
          "original_form": "분위기는 대존맛",
          "speaker_id": "2",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100041292.1.2",
          "form": "분위기맛이지뭨ㅋㅋㅋㅋㅋㅋ",
          "original_form": "분위기맛이지뭨ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100041292.1.3",
          "form": "정말...",
          "original_form": "정말...",
          "speaker_id": "1",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100041292.1.4",
          "form": "자본의 맛",
          "original_form": "자본의 맛{emoji:☆}",
          "speaker_id": "1",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100041292.1.5",
          "form": "아까워서 사진 오만장 찍어야겠네",
          "original_form": "아까워서 사진 오만장 찍어야겠네",
          "speaker_id": "1",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100041292.1.6",
          "form": "양은 많았어?",
          "original_form": "양은 많았어?",
          "speaker_id": "1",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100041292.1.7",
          "form": "응응 ㅋㅋㅋ",
          "original_form": "응응 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100041292.1.8",
          "form": "많고 이쁘고 맛은있는데",
          "original_form": "많고 이쁘고 맛은있는데",
          "speaker_id": "2",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041292.1.9",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041292.1.10",
          "form": "8ㅅ8",
          "original_form": "8ㅅ8",
          "speaker_id": "1",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041292.1.11",
          "form": "몇인분이야?!",
          "original_form": "몇인분이야?!",
          "speaker_id": "1",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041292.1.12",
          "form": "빵도아니고 과일+ 얼음이 6만원인건",
          "original_form": "빵도아니고 과일+ 얼음이 6만원인건",
          "speaker_id": "2",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041292.1.13",
          "form": "4인분은 되어야하는거 아니냐구",
          "original_form": "4인분은 되어야하는거 아니냐구",
          "speaker_id": "1",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041292.1.14",
          "form": "허허",
          "original_form": "허허",
          "speaker_id": "1",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041292.1.15",
          "form": "ㄱㄱㅋㅋ2인분가격",
          "original_form": "ㄱㄱㅋㅋ2인분가격",
          "speaker_id": "2",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041292.1.16",
          "form": "거기에...시럽?!",
          "original_form": "거기에...시럽?!",
