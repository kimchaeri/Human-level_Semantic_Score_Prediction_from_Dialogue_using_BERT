      "utterance": [
        {
          "id": "MDRW2100004993.1.1",
          "form": "종교있어요?",
          "original_form": "종교있어요?",
          "speaker_id": "1",
          "time": "20210821 19:47"
        },
        {
          "id": "MDRW2100004993.1.2",
          "form": "하나가져 볼까요",
          "original_form": "하나가져 볼까요",
          "speaker_id": "2",
          "time": "20210821 20:06"
        },
        {
          "id": "MDRW2100004993.1.3",
          "form": "엨",
          "original_form": "엨",
          "speaker_id": "1",
          "time": "20210821 20:07"
        },
        {
          "id": "MDRW2100004993.1.4",
          "form": "무슨 종교에 관심이 있는데요",
          "original_form": "무슨 종교에 관심이 있는데요",
          "speaker_id": "1",
          "time": "20210821 20:07"
        },
        {
          "id": "MDRW2100004993.1.5",
          "form": "불교가 1위 \r\n기독교가2위",
          "original_form": "불교가 1위 \r\n기독교가2위",
          "speaker_id": "2",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100004993.1.6",
          "form": "추천좀",
          "original_form": "추천좀",
          "speaker_id": "2",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100004993.1.7",
          "form": "헐... 노래좋아하면 기독교",
          "original_form": "헐... 노래좋아하면 기독교",
          "speaker_id": "1",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100004993.1.8",
          "form": "추천한다",
          "original_form": "추천한다",
          "speaker_id": "1",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100004993.1.9",
          "form": "기독교면",
          "original_form": "기독교면",
          "speaker_id": "2",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100004993.1.10",
          "form": "사랑노래만 불러야되잖아",
          "original_form": "사랑노래만 불러야되잖아",
          "speaker_id": "2",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100004993.1.11",
          "form": "찬송가 꽤 들을만해",
          "original_form": "찬송가 꽤 들을만해",
          "speaker_id": "1",
          "time": "20210821 20:11"
        },
        {
          "id": "MDRW2100004993.1.12",
          "form": "립싱크 하면 되지",
          "original_form": "립싱크 하면 되지",
          "speaker_id": "1",
          "time": "20210821 20:11"
        },
        {
          "id": "MDRW2100004993.1.13",
          "form": "립싱크하면 천벌받을텐데",
          "original_form": "립싱크하면 천벌받을텐데",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100004993.1.14",
          "form": "너 불교지",
          "original_form": "너 불교지",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100004993.1.15",
          "form": "나 무교임",
          "original_form": "나 무교임",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004993.1.16",
          "form": "천벌 얼척쓰",
          "original_form": "천벌 얼척쓰",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004993.1.17",
          "form": "넌 기독교 아님?",
          "original_form": "넌 기독교 아님?",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004993.1.18",
          "form": "ㅇㅇ  나도 무교임",
          "original_form": "ㅇㅇ  나도 무교임",
          "speaker_id": "2",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004993.1.19",
          "form": "넌 왜 무교임",
          "original_form": "넌 왜 무교임",
          "speaker_id": "2",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100004993.1.20",
          "form": "한자 좋아하면 불교나 하셈",
          "original_form": "한자 좋아하면 불교나 하셈",
          "speaker_id": "1",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100004993.1.21",
          "form": "중, 고 모두 기독학교 나왔는데 광신하는 건 맹독이라는 생각이 들던데",
          "original_form": "중, 고 모두 기독학교 나왔는데 광신하는 건 맹독이라는 생각이 들던데",
          "speaker_id": "1",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100004993.1.22",
          "form": "성경 많이 읽어봄?",
          "original_form": "성경 많이 읽어봄?",
          "speaker_id": "2",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100004993.1.23",
          "form": "학원에서 주기도문 영어로 외워보긴함",
          "original_form": "학원에서 주기도문 영어로 외워보긴함",
          "speaker_id": "1",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100004993.1.24",
          "form": "성경은 안읽고 걍 종교수업있음",
          "original_form": "성경은 안읽고 걍 종교수업있음",
          "speaker_id": "1",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100004993.1.25",
          "form": "종교수업이면  \r\n하느님 업적 외우고 시험보는거임?",
          "original_form": "종교수업이면  \r\n하느님 업적 외우고 시험보는거임?",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100004993.1.26",
          "form": "아뇨 그냥 예배 드림",
          "original_form": "아뇨 그냥 예배 드림",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100004993.1.27",
          "form": "교회 안가봄?",
          "original_form": "교회 안가봄?",
          "speaker_id": "1",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100004993.1.28",
          "form": "어릴때 치킨준다고 갔다가\r\n성경책 줘서  빤스런한 기억밖에 없음",
          "original_form": "어릴때 치킨준다고 갔다가\r\n성경책 줘서  빤스런한 기억밖에 없음",
          "speaker_id": "2",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100004993.1.29",
          "form": "올 순수하지 않았네",
          "original_form": "올 순수하지 않았네",
          "speaker_id": "1",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100004993.1.30",
          "form": "교회는 뭔가 뇌물주는 거 같아서 별로였음",
          "original_form": "교회는 뭔가 뇌물주는 거 같아서 별로였음",
          "speaker_id": "1",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100004993.1.31",
          "form": "난 중학교까지 순수해서 다님",
          "original_form": "난 중학교까지 순수해서 다님",
          "speaker_id": "1",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100004993.1.32",
          "form": "고등학교때는 타락해서 안다님?\r\n왜 타락함",
          "original_form": "고등학교때는 타락해서 안다님?\r\n왜 타락함",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100004993.1.33",
          "form": "그만 다니고 싶은데 전도사가 집에도 찾아오더라",
          "original_form": "그만 다니고 싶은데 전도사가 집에도 찾아오더라",
          "speaker_id": "1",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100004993.1.34",
          "form": "아 별로였다",
          "original_form": "아 별로였다",
          "speaker_id": "1",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100004993.1.35",
          "form": "와 무섭네",
          "original_form": "와 무섭네",
          "speaker_id": "2",
          "time": "20210821 20:39"
        }
      ]
    }
