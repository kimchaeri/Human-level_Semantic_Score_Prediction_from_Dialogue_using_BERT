      "utterance": [
        {
          "id": "MDRW2100046096.1.1",
          "form": "학교 다닐 때",
          "original_form": "학교 다닐 때",
          "speaker_id": "2",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100046096.1.2",
          "form": "통학 하셨어요?",
          "original_form": "통학 하셨어요?",
          "speaker_id": "2",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100046096.1.3",
          "form": "넼ㅋㅋㅋ저는",
          "original_form": "넼ㅋㅋㅋ저는",
          "speaker_id": "1",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100046096.1.4",
          "form": "근처살아서",
          "original_form": "근처살아서",
          "speaker_id": "1",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100046096.1.5",
          "form": "그러면 자취?",
          "original_form": "그러면 자취?",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046096.1.6",
          "form": "아뇨 본가요",
          "original_form": "아뇨 본가요",
          "speaker_id": "1",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046096.1.7",
          "form": "대학교원서쓸때",
          "original_form": "대학교원서쓸때",
          "speaker_id": "1",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046096.1.8",
          "form": "아예 집근처대학으로썼어요",
          "original_form": "아예 집근처대학으로썼어요",
          "speaker_id": "1",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046096.1.9",
          "form": "전 타지갈생각없었어서",
          "original_form": "전 타지갈생각없었어서",
          "speaker_id": "1",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046096.1.10",
          "form": "좋겠어요",
          "original_form": "좋겠어요",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046096.1.11",
          "form": "전 1시간 거리..",
          "original_form": "전 1시간 거리..",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046096.1.12",
          "form": "에이 그정도면 괜찮죠",
          "original_form": "에이 그정도면 괜찮죠",
          "speaker_id": "1",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046096.1.13",
          "form": "편도 세시간걸리는 친구도있었어욬ㅋㅋ",
          "original_form": "편도 세시간걸리는 친구도있었어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046096.1.14",
          "form": "그정도면",
          "original_form": "그정도면",
          "speaker_id": "2",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046096.1.15",
          "form": "자취 해야겠어요",
          "original_form": "자취 해야겠어요",
          "speaker_id": "2",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046096.1.16",
          "form": "학교는 어때요??",
          "original_form": "학교는 어때요??",
          "speaker_id": "1",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046096.1.17",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "2",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046096.1.18",
          "form": "언덕많고",
          "original_form": "언덕많고",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046096.1.19",
          "form": "언덕이 많아요",
          "original_form": "언덕이 많아요",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046096.1.20",
          "form": "앜ㅋㅋㅋㅋ저희도요",
          "original_form": "앜ㅋㅋㅋㅋ저희도요",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046096.1.21",
          "form": "진짜 정문에서 후문가려면",
          "original_form": "진짜 정문에서 후문가려면",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046096.1.22",
          "form": "언덕을 한참...하...",
          "original_form": "언덕을 한참...하...",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046096.1.23",
          "form": "정문 통과해서",
          "original_form": "정문 통과해서",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046096.1.24",
          "form": "십분걸어야지",
          "original_form": "십분걸어야지",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046096.1.25",
          "form": "건물 나와요",
          "original_form": "건물 나와요",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046096.1.26",
          "form": "ㅋㅋ,,ㅋ",
          "original_form": "ㅋㅋ,,ㅋ",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046096.1.27",
          "form": "우린 그정돈 아니었...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "우린 그정돈 아니었...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046096.1.28",
          "form": "건물은요?",
          "original_form": "건물은요?",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046096.1.29",
          "form": "다 들어가 본 거는",
          "original_form": "다 들어가 본 거는",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046096.1.30",
          "form": "아니지만",
          "original_form": "아니지만",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046096.1.31",
          "form": "별로예요",
          "original_form": "별로예요",
          "speaker_id": "2",
          "time": "20210908 10:24"
        }
      ]
    }
  ]
