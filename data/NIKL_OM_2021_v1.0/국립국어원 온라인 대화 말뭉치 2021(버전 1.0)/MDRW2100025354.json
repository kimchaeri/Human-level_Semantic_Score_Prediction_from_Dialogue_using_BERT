        {
          "id": "MDRW2100025354.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210901 10:06"
        },
        {
          "id": "MDRW2100025354.1.2",
          "form": "했습니다!",
          "original_form": "했습니다!",
          "speaker_id": "2",
          "time": "20210901 10:07"
        },
        {
          "id": "MDRW2100025354.1.3",
          "form": "아침드셨나요?",
          "original_form": "아침드셨나요?",
          "speaker_id": "2",
          "time": "20210901 10:07"
        },
        {
          "id": "MDRW2100025354.1.4",
          "form": "시리얼이오",
          "original_form": "시리얼이오",
          "speaker_id": "1",
          "time": "20210901 10:07"
        },
        {
          "id": "MDRW2100025354.1.5",
          "form": "씨리얼 어떤 브랜드 제일 좋아하세요?",
          "original_form": "씨리얼 어떤 브랜드 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 10:07"
        },
        {
          "id": "MDRW2100025354.1.6",
          "form": "호랑이 기운이여 ㅋㅋ",
          "original_form": "호랑이 기운이여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100025354.1.7",
          "form": "거기꺼 맛있죠",
          "original_form": "거기꺼 맛있죠",
          "speaker_id": "2",
          "time": "20210901 10:18"
        },
        {
          "id": "MDRW2100025354.1.8",
          "form": "저도 그거 제일 좋아해요",
          "original_form": "저도 그거 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210901 10:18"
        },
        {
          "id": "MDRW2100025354.1.9",
          "form": "달달하니",
          "original_form": "달달하니",
          "speaker_id": "1",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025354.1.10",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025354.1.11",
          "form": "아침에 밥보다 씨리얼 주로 드세요??",
          "original_form": "아침에 밥보다 씨리얼 주로 드세요??",
          "speaker_id": "2",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025354.1.12",
          "form": "네 간단히요",
          "original_form": "네 간단히요",
          "speaker_id": "1",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025354.1.13",
          "form": "밥 드실때는 주로 해드시나요??? 시켜드시나요??",
          "original_form": "밥 드실때는 주로 해드시나요??? 시켜드시나요??",
          "speaker_id": "2",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025354.1.14",
          "form": "시켜요 요리를 잘 못해요",
          "original_form": "시켜요 요리를 잘 못해요",
          "speaker_id": "1",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025354.1.15",
          "form": "어떤걸 제일 잘 시켜드세요??",
          "original_form": "어떤걸 제일 잘 시켜드세요??",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025354.1.16",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "1",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025354.1.17",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
