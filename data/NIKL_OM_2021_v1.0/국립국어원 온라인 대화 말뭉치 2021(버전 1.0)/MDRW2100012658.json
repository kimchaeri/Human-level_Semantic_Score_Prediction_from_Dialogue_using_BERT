      "utterance": [
        {
          "id": "MDRW2100012658.1.1",
          "form": "빨래해야하는데 요즘 비가많이 오네요ㅠ",
          "original_form": "빨래해야하는데 요즘 비가많이 오네요ㅠ",
          "speaker_id": "1",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100012658.1.2",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210825 10:31"
        },
        {
          "id": "MDRW2100012658.1.3",
          "form": "여름에 비 많이오면 빨래 눅눅하죠",
          "original_form": "여름에 비 많이오면 빨래 눅눅하죠",
          "speaker_id": "2",
          "time": "20210825 10:31"
        },
        {
          "id": "MDRW2100012658.1.4",
          "form": "그나마 여름 옷이 얇아 빨래가 잘 마르는 편이라 다행이에요",
          "original_form": "그나마 여름 옷이 얇아 빨래가 잘 마르는 편이라 다행이에요",
          "speaker_id": "2",
          "time": "20210825 10:32"
        },
        {
          "id": "MDRW2100012658.1.5",
          "form": "그죠! 수건같은것만 조심하면 쿰쿰한 냄새 안나더라구요",
          "original_form": "그죠! 수건같은것만 조심하면 쿰쿰한 냄새 안나더라구요",
          "speaker_id": "1",
          "time": "20210825 10:56"
        },
        {
          "id": "MDRW2100012658.1.6",
          "form": "요즘은 실내빨래전용 세제가나오기도하구요",
          "original_form": "요즘은 실내빨래전용 세제가나오기도하구요",
          "speaker_id": "1",
          "time": "20210825 10:56"
        },
        {
          "id": "MDRW2100012658.1.7",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210825 12:58"
        },
        {
          "id": "MDRW2100012658.1.8",
          "form": "신기하네요 그런게 있는줄도 몰랐어요",
          "original_form": "신기하네요 그런게 있는줄도 몰랐어요",
          "speaker_id": "2",
          "time": "20210825 12:58"
        },
        {
          "id": "MDRW2100012658.1.9",
          "form": "수건 진짜 안마르죠",
          "original_form": "수건 진짜 안마르죠",
          "speaker_id": "2",
          "time": "20210825 12:58"
        },
        {
          "id": "MDRW2100012658.1.10",
          "form": "덜 마른 수건 쓰는것도 찝찝하고",
          "original_form": "덜 마른 수건 쓰는것도 찝찝하고",
          "speaker_id": "2",
          "time": "20210825 12:58"
        },
        {
          "id": "MDRW2100012658.1.11",
          "form": "여름은 힘들어요ㅋㅋㅋ",
          "original_form": "여름은 힘들어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:59"
        },
        {
          "id": "MDRW2100012658.1.12",
          "form": "그죠 ㅋㅋㅋ이럴때마다 제습기는 어찌나사고싶은지",
          "original_form": "그죠 ㅋㅋㅋ이럴때마다 제습기는 어찌나사고싶은지",
          "speaker_id": "1",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100012658.1.13",
          "form": "제일 싫은 집안일이 있나요?",
          "original_form": "제일 싫은 집안일이 있나요?",
          "speaker_id": "1",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100012658.1.14",
          "form": "전 화장실청소요",
          "original_form": "전 화장실청소요",
          "speaker_id": "2",
          "time": "20210825 19:01"
        },
        {
          "id": "MDRW2100012658.1.15",
          "form": "아무리해봐도 익숙해지지않아요",
          "original_form": "아무리해봐도 익숙해지지않아요",
          "speaker_id": "2",
          "time": "20210825 19:01"
        },
        {
          "id": "MDRW2100012658.1.16",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:01"
        },
        {
          "id": "MDRW2100012658.1.17",
          "form": "쭈그려 앉아 해야해서 더 힘들기도하구요",
          "original_form": "쭈그려 앉아 해야해서 더 힘들기도하구요",
          "speaker_id": "2",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100012658.1.18",
          "form": "아 저두요! 비위가 약해서 더더욱 싫어요",
          "original_form": "아 저두요! 비위가 약해서 더더욱 싫어요",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100012658.1.19",
          "form": "못해도 주에 한번은 해야하는데 힘들죠...ㅠ",
          "original_form": "못해도 주에 한번은 해야하는데 힘들죠...ㅠ",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100012658.1.20",
          "form": "맞아요 화장실이 또 다른방들보다",
          "original_form": "맞아요 화장실이 또 다른방들보다",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100012658.1.21",
          "form": "습해서 더 힘든 것 같아요",
          "original_form": "습해서 더 힘든 것 같아요",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100012658.1.22",
          "form": "환기가 안되는 느낌이라ㅜ",
          "original_form": "환기가 안되는 느낌이라ㅜ",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100012658.1.23",
          "form": "맞아요 그래서 저는 매번 유리닦이로 물기를 제거해요",
          "original_form": "맞아요 그래서 저는 매번 유리닦이로 물기를 제거해요",
          "speaker_id": "1",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100012658.1.24",
          "form": "부지런하시네요!",
          "original_form": "부지런하시네요!",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100012658.1.25",
          "form": "물기제거 해야 곰팡이도 잘 안생기고 그렇겠네요",
          "original_form": "물기제거 해야 곰팡이도 잘 안생기고 그렇겠네요",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100012658.1.26",
          "form": "저도 한번 써봐야겠어요",
          "original_form": "저도 한번 써봐야겠어요",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100012658.1.27",
          "form": "한번써보세요 추천드려요",
          "original_form": "한번써보세요 추천드려요",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100012658.1.28",
          "form": "물기 잘안마르는 화장실이셨으면 더욱 효과 보실거예요",
          "original_form": "물기 잘안마르는 화장실이셨으면 더욱 효과 보실거예요",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100012658.1.29",
          "form": "오 종류가 많네요",
          "original_form": "오 종류가 많네요",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100012658.1.30",
          "form": "개인적으로 이케아제품 추천드려요",
          "original_form": "개인적으로 이케아제품 추천드려요",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100012658.1.31",
          "form": "가성비 최고",
          "original_form": "가성비 최고",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100012658.1.32",
          "form": "이케아 바로 딱 떴는데",
          "original_form": "이케아 바로 딱 떴는데",
          "speaker_id": "2",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100012658.1.33",
          "form": "역시 좋은가보군요",
          "original_form": "역시 좋은가보군요",
          "speaker_id": "2",
          "time": "20210825 21:04"
        }
      ]
    }
