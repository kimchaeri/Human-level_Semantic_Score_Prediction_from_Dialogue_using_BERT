        {
          "id": "MDRW2100038643.1.1",
          "form": "공연좋아하세요?",
          "original_form": "공연좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 18:10"
        },
        {
          "id": "MDRW2100038643.1.2",
          "form": "안녕하세요 네~",
          "original_form": "안녕하세요 네~",
          "speaker_id": "2",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100038643.1.3",
          "form": "어떤 종류 좋아하세요?",
          "original_form": "어떤 종류 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100038643.1.4",
          "form": "뮤지컬이나 연극 좋아해요",
          "original_form": "뮤지컬이나 연극 좋아해요",
          "speaker_id": "2",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100038643.1.5",
          "form": "아 가장 좋았던 공연 뭐예요?",
          "original_form": "아 가장 좋았던 공연 뭐예요?",
          "speaker_id": "1",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100038643.1.6",
          "form": "저는 태일이라고 소극장 공연있는데 그게 가장 좋았어요",
          "original_form": "저는 태일이라고 소극장 공연있는데 그게 가장 좋았어요",
          "speaker_id": "2",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100038643.1.7",
          "form": "공연 어떤거 좋아하세요?",
          "original_form": "공연 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100038643.1.8",
          "form": "아 저는 가리지않고 유명하고\r\n 좋다는 것들ㅋㅋ 위주로 보는거같아요",
          "original_form": "아 저는 가리지않고 유명하고\r\n 좋다는 것들ㅋㅋ 위주로 보는거같아요",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100038643.1.9",
          "form": "어떤거 보셨어요?",
          "original_form": "어떤거 보셨어요?",
          "speaker_id": "2",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100038643.1.10",
          "form": "뮤지컬 위키드좋았어요",
          "original_form": "뮤지컬 위키드좋았어요",
          "speaker_id": "1",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100038643.1.11",
          "form": "태일 찾아봐야겠네요",
          "original_form": "태일 찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100038643.1.12",
          "form": "저도 위키드 보고싶었는데 코로나때문에 결국 못갔어요",
          "original_form": "저도 위키드 보고싶었는데 코로나때문에 결국 못갔어요",
          "speaker_id": "2",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100038643.1.13",
          "form": "공연 재미있어요! 맘마미아는 영화가 훨씬 좋았어요",
          "original_form": "공연 재미있어요! 맘마미아는 영화가 훨씬 좋았어요",
          "speaker_id": "1",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100038643.1.14",
          "form": "대극장공연 위주로 좋아하시는군용",
          "original_form": "대극장공연 위주로 좋아하시는군용",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100038643.1.15",
          "form": "회사 문화비였어요ㅋㅋ이젠 못가지만ㅜㅠ",
          "original_form": "회사 문화비였어요ㅋㅋ이젠 못가지만ㅜㅠ",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100038643.1.16",
          "form": "회사돈 플렉스",
          "original_form": "회사돈 플렉스",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100038643.1.17",
          "form": "헉 너무 좋은 회사네요ㅡ.!!!!!",
          "original_form": "헉 너무 좋은 회사네요ㅡ.!!!!!",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100038643.1.18",
          "form": "퇴사하고ㅋㅋ지금 회사는 국물도 없어요ㅋㅋ 지금은 내돈내본",
          "original_form": "퇴사하고ㅋㅋ지금 회사는 국물도 없어요ㅋㅋ 지금은 내돈내본",
          "speaker_id": "1",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100038643.1.19",
          "form": "아하..ㅎㅎ",
          "original_form": "아하..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100038643.1.20",
          "form": "저도 내돈내본이요 ㅎㅎ",
          "original_form": "저도 내돈내본이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100038643.1.21",
          "form": "더 신중하게 고르게 돼요ㅋㅋ",
          "original_form": "더 신중하게 고르게 돼요ㅋㅋ",
