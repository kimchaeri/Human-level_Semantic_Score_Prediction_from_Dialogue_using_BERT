        },
        {
          "id": "MDRW2100017145.1.2",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100017145.1.3",
          "form": "곧 퇴근입니다..",
          "original_form": "곧 퇴근입니다..",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017145.1.4",
          "form": "재택하고싶어요 ㅠㅠ",
          "original_form": "재택하고싶어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017145.1.5",
          "form": "저두요 ㅎㅎ",
          "original_form": "저두요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017145.1.6",
          "form": "재택이 아예 없으신가요??",
          "original_form": "재택이 아예 없으신가요??",
          "speaker_id": "1",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017145.1.7",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017145.1.8",
          "form": "ㅠㅠ 재택하는 친구들 너무 부러워요",
          "original_form": "ㅠㅠ 재택하는 친구들 너무 부러워요",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017145.1.9",
          "form": "저두 없어요...ㅠㅠ",
          "original_form": "저두 없어요...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017145.1.10",
          "form": "친구들 중에 저만 없어요ㅠ",
          "original_form": "친구들 중에 저만 없어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017145.1.11",
          "form": "어떤 일하세요~?",
          "original_form": "어떤 일하세요~?",
          "speaker_id": "2",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017145.1.12",
          "form": "저는 컨설팅회사 다니고 있어요",
          "original_form": "저는 컨설팅회사 다니고 있어요",
          "speaker_id": "1",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017145.1.13",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017145.1.14",
          "form": "저는 병원에서 일해요 ㅋㅋㅋ",
          "original_form": "저는 병원에서 일해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017145.1.15",
          "form": "컨설팅도 종류 많지않나요??",
          "original_form": "컨설팅도 종류 많지않나요??",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017145.1.16",
          "form": "에너지쪽 컨설팅 하고 있어요!",
          "original_form": "에너지쪽 컨설팅 하고 있어요!",
          "speaker_id": "1",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017145.1.17",
          "form": "퇴근 7분 전이네요",
          "original_form": "퇴근 7분 전이네요",
          "speaker_id": "1",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017145.1.18",
          "form": "신발 갈아신으셨나요?",
          "original_form": "신발 갈아신으셨나요?",
