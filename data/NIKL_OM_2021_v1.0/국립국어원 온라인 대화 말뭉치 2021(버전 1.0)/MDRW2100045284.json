      "utterance": [
        {
          "id": "MDRW2100045284.1.1",
          "form": "물리듣는중",
          "original_form": "물리듣는중",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045284.1.2",
          "form": "30분남앗어요",
          "original_form": "30분남앗어요",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045284.1.3",
          "form": "하 그거 이배속 안되는거 좀 많이 노답이에요",
          "original_form": "하 그거 이배속 안되는거 좀 많이 노답이에요",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045284.1.4",
          "form": "그니까요 세상왜케바뀐거죠",
          "original_form": "그니까요 세상왜케바뀐거죠",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045284.1.5",
          "form": "오호 세상이 바뀌기까지",
          "original_form": "오호 세상이 바뀌기까지",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045284.1.6",
          "form": "아 졸린데 인강 하",
          "original_form": "아 졸린데 인강 하",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045284.1.7",
          "form": "듣지마요",
          "original_form": "듣지마요",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045284.1.8",
          "form": "내일로 미뤄요",
          "original_form": "내일로 미뤄요",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045284.1.9",
          "form": "인강은 미리미리듣기",
          "original_form": "인강은 미리미리듣기",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045284.1.10",
          "form": "그러기엔 이미 어제부터 밀려놔서요",
          "original_form": "그러기엔 이미 어제부터 밀려놔서요",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045284.1.11",
          "form": "진짜 내일은 열심히 살아볼게요",
          "original_form": "진짜 내일은 열심히 살아볼게요",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045284.1.12",
          "form": "야간학교다니시냐고요",
          "original_form": "야간학교다니시냐고요",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045284.1.13",
          "form": "그런가봐요야행성이네",
          "original_form": "그런가봐요야행성이네",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045284.1.14",
          "form": "너심야씨는 뭐 수업 안들어여?",
          "original_form": "너심야씨는 뭐 수업 안들어여?",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045284.1.15",
          "form": "지금요 ???",
          "original_form": "지금요 ???",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045284.1.16",
          "form": "일단 오늘 수업은 포기하고 누웠어요",
          "original_form": "일단 오늘 수업은 포기하고 누웠어요",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045284.1.17",
          "form": "밀린거 산더미",
          "original_form": "밀린거 산더미",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045284.1.18",
          "form": "네 지금 안들어여?",
          "original_form": "네 지금 안들어여?",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045284.1.19",
          "form": "출튀안해여?",
          "original_form": "출튀안해여?",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045284.1.20",
          "form": "출튀조차 귀찮아여",
          "original_form": "출튀조차 귀찮아여",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045284.1.21",
          "form": "오늘 그래도 유기도 듣고 예창도 듣고 채플도 들었어요",
          "original_form": "오늘 그래도 유기도 듣고 예창도 듣고 채플도 들었어요",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045284.1.22",
          "form": "칭찬해줘",
          "original_form": "칭찬해줘",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045284.1.23",
          "form": "오ㅑ케 부지런해요?",
          "original_form": "오ㅑ케 부지런해요?",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045284.1.24",
          "form": "전 산더미로 밀렸능데",
          "original_form": "전 산더미로 밀렸능데",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045284.1.25",
          "form": "저는 오늘한거 예창 채플 출튀",
          "original_form": "저는 오늘한거 예창 채플 출튀",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045284.1.26",
          "form": "산더미로 밀렸어요",
          "original_form": "산더미로 밀렸어요",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045284.1.27",
          "form": "물2 출튀중",
          "original_form": "물2 출튀중",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045284.1.28",
          "form": "출튀는 안좋아요",
          "original_form": "출튀는 안좋아요",
          "speaker_id": "1",
          "time": "20210908 01:47"
        }
      ]
