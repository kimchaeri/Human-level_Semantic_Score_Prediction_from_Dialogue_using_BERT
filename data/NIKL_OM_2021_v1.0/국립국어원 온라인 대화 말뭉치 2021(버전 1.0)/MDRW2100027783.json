      "utterance": [
        {
          "id": "MDRW2100027783.1.1",
          "form": "오늘 점심 뭐 먹었니",
          "original_form": "오늘 점심 뭐 먹었니",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027783.1.2",
          "form": "나 오늘 점심으로 남은 순두부 찌개",
          "original_form": "나 오늘 점심으로 남은 순두부 찌개",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027783.1.3",
          "form": "나는 피자스틱 먹었어",
          "original_form": "나는 피자스틱 먹었어",
          "speaker_id": "1",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027783.1.4",
          "form": "청년피자에서 나온건데 그냥 피자맛을 빵에 바른느낌?",
          "original_form": "청년피자에서 나온건데 그냥 피자맛을 빵에 바른느낌?",
          "speaker_id": "1",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027783.1.5",
          "form": "그치만 난 역시 피자보다 치킨파야",
          "original_form": "그치만 난 역시 피자보다 치킨파야",
          "speaker_id": "1",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027783.1.6",
          "form": "난 피자파인데",
          "original_form": "난 피자파인데",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027783.1.7",
          "form": "헐 피자하니까 생각났는데",
          "original_form": "헐 피자하니까 생각났는데",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027783.1.8",
          "form": "오뚜기가 낸 냉동피자 완전 맛있어",
          "original_form": "오뚜기가 낸 냉동피자 완전 맛있어",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027783.1.9",
          "form": "강추",
          "original_form": "강추",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027783.1.10",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "1",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027783.1.11",
          "form": "하긴 냉동피자도 요즘 괜찮더라",
          "original_form": "하긴 냉동피자도 요즘 괜찮더라",
          "speaker_id": "1",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027783.1.12",
          "form": "난 요즘 햄버거도 끌려",
          "original_form": "난 요즘 햄버거도 끌려",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027783.1.13",
          "form": "맥도날드 좋아",
          "original_form": "맥도날드 좋아",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027783.1.14",
          "form": "냉동피자 요즘 퀄리티 쩔어",
          "original_form": "냉동피자 요즘 퀄리티 쩔어",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027783.1.15",
          "form": "고르곤졸라 피자에는 치즈크러스트가",
          "original_form": "고르곤졸라 피자에는 치즈크러스트가",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027783.1.16",
          "form": "일반 피자에는 콘소메크러스트가 들어있어",
          "original_form": "일반 피자에는 콘소메크러스트가 들어있어",
          "speaker_id": "2",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027783.1.17",
          "form": "정말 피자에 진심이구나",
          "original_form": "정말 피자에 진심이구나",
          "speaker_id": "1",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027783.1.18",
          "form": "나 원래 맘스터치 포테이토 휠렛버거 좋아했는데 단종이고",
          "original_form": "나 원래 맘스터치 포테이토 휠렛버거 좋아했는데 단종이고",
          "speaker_id": "1",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027783.1.19",
          "form": "버거킹 머쉬룸와퍼 좋아했는데 단종이고",
          "original_form": "버거킹 머쉬룸와퍼 좋아했는데 단종이고",
          "speaker_id": "1",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027783.1.20",
          "form": "맥도날드 좋아하니까 동네 매장이 없어",
          "original_form": "맥도날드 좋아하니까 동네 매장이 없어",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027783.1.21",
          "form": "난 머쉬룸 와파 안먹어봤어 맛있나",
          "original_form": "난 머쉬룸 와파 안먹어봤어 맛있나",
          "speaker_id": "2",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027783.1.22",
          "form": "내가 버섯을 많이 안좋아해가지구",
          "original_form": "내가 버섯을 많이 안좋아해가지구",
          "speaker_id": "2",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027783.1.23",
          "form": "난  버섯은 그냥저냥인데",
          "original_form": "난  버섯은 그냥저냥인데",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027783.1.24",
          "form": "그 소스가 좋았어",
          "original_form": "그 소스가 좋았어",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027783.1.25",
          "form": "난 음식을 거의 소스맛을 중시해서",
          "original_form": "난 음식을 거의 소스맛을 중시해서",
          "speaker_id": "1",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027783.1.26",
          "form": "이삭토스트도 그래서 가끔 생각나",
          "original_form": "이삭토스트도 그래서 가끔 생각나",
          "speaker_id": "1",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027783.1.27",
          "form": "이삭토스트 맛있지",
          "original_form": "이삭토스트 맛있지",
          "speaker_id": "2",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027783.1.28",
          "form": "나 대학교때 맨날 먹은듯",
          "original_form": "나 대학교때 맨날 먹은듯",
          "speaker_id": "2",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027783.1.29",
          "form": "나 이삭토스트의 키위소스를.조아해",
          "original_form": "나 이삭토스트의 키위소스를.조아해",
          "speaker_id": "2",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027783.1.30",
          "form": "나도 안양살때는 자주 먹었는데",
          "original_form": "나도 안양살때는 자주 먹었는데",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027783.1.31",
          "form": "집에서 걸어서 5분? 거리였거든",
          "original_form": "집에서 걸어서 5분? 거리였거든",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027783.1.32",
          "form": "이삭토스트에 베이글도 맛있었어",
          "original_form": "이삭토스트에 베이글도 맛있었어",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027783.1.33",
          "form": "나도 집앞에 이삭 초딩때 있었다가 사라지고 최근에 다시 생겼어",
          "original_form": "나도 집앞에 이삭 초딩때 있었다가 사라지고 최근에 다시 생겼어",
          "speaker_id": "2",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027783.1.34",
          "form": "나는 거기서 맨날 뭐먹었더라 먹던거만 먹었어",
          "original_form": "나는 거기서 맨날 뭐먹었더라 먹던거만 먹었어",
          "speaker_id": "2",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027783.1.35",
          "form": "치즈감자햄인다",
          "original_form": "치즈감자햄인다",
          "speaker_id": "2",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027783.1.36",
          "form": "감자 들어간 건 다 맛있어",
          "original_form": "감자 들어간 건 다 맛있어",
          "speaker_id": "1",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027783.1.37",
          "form": "난 사실 뼈해장국이나 감자탕 감자 먹으려고 시켜",
          "original_form": "난 사실 뼈해장국이나 감자탕 감자 먹으려고 시켜",
          "speaker_id": "1",
          "time": "20210902 15:20"
        }
      ]
