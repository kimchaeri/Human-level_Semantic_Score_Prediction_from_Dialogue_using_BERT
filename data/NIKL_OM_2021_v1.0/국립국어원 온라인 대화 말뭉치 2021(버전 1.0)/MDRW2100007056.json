      "utterance": [
        {
          "id": "MDRW2100007056.1.1",
          "form": "안녕하세요~^-^",
          "original_form": "안녕하세요~^-^",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007056.1.2",
          "form": "안녕하세요^^..",
          "original_form": "안녕하세요^^..",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007056.1.3",
          "form": "반려동물",
          "original_form": "반려동물",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007056.1.4",
          "form": "있으신가요",
          "original_form": "있으신가요",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007056.1.5",
          "form": "아니요..",
          "original_form": "아니요..",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007056.1.6",
          "form": "있으신가요",
          "original_form": "있으신가요",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007056.1.7",
          "form": "본가에서",
          "original_form": "본가에서",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007056.1.8",
          "form": "앵무새키워요",
          "original_form": "앵무새키워요",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007056.1.9",
          "form": "앵무새 귀엽죠",
          "original_form": "앵무새 귀엽죠",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007056.1.10",
          "form": "저도 매일 앵무새 유튜브 영상을 본답니다",
          "original_form": "저도 매일 앵무새 유튜브 영상을 본답니다",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007056.1.11",
          "form": "말하는 앵무새 유튜브 영상이 많이 올라오더라고요",
          "original_form": "말하는 앵무새 유튜브 영상이 많이 올라오더라고요",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007056.1.12",
          "form": "저희앵무새는 말을못허지만 귀여워요",
          "original_form": "저희앵무새는 말을못허지만 귀여워요",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007056.1.13",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007056.1.14",
          "form": "한번 보고싶네요",
          "original_form": "한번 보고싶네요",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007056.1.15",
          "form": "키우고싶은 동물 있나요",
          "original_form": "키우고싶은 동물 있나요",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007056.1.16",
          "form": "키우고싶은 동물이야 많죠",
          "original_form": "키우고싶은 동물이야 많죠",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007056.1.17",
          "form": "고양이도 귀엽고 강아지도 귀엽고 앵무새도",
          "original_form": "고양이도 귀엽고 강아지도 귀엽고 앵무새도",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007056.1.18",
          "form": "저는 고양이가 좋아요",
          "original_form": "저는 고양이가 좋아요",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007056.1.19",
          "form": "키워주세요",
          "original_form": "키워주세요",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007056.1.20",
          "form": "놀러갈래요",
          "original_form": "놀러갈래요",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007056.1.21",
          "form": "네 한 10년뒤에 오시면",
          "original_form": "네 한 10년뒤에 오시면",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007056.1.22",
          "form": "그때쯤 보실 수 있을거같네요",
          "original_form": "그때쯤 보실 수 있을거같네요",
          "speaker_id": "1",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007056.1.23",
          "form": "10년 너무 먼데요",
          "original_form": "10년 너무 먼데요",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007056.1.24",
          "form": "더빨리안될까요",
          "original_form": "더빨리안될까요",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007056.1.25",
          "form": "그럼 한 7년",
          "original_form": "그럼 한 7년",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007056.1.26",
          "form": "돈이 있어야 책임도 질 수 있는거니까 그정도는 벌어야될거같내요",
          "original_form": "돈이 있어야 책임도 질 수 있는거니까 그정도는 벌어야될거같내요",
          "speaker_id": "1",
          "time": "20210822 00:44"
        }
      ]
