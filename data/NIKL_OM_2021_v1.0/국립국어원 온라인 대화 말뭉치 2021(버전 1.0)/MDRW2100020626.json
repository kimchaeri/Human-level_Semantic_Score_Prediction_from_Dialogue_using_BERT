      "utterance": [
        {
          "id": "MDRW2100020626.1.1",
          "form": "무섭다기보다는",
          "original_form": "무섭다기보다는",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020626.1.2",
          "form": "일단 남주를 볼때마다 병크가 생각나고",
          "original_form": "일단 남주를 볼때마다 병크가 생각나고",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020626.1.3",
          "form": "스토리 자체가 좀 찝찝~하고",
          "original_form": "스토리 자체가 좀 찝찝~하고",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020626.1.4",
          "form": "기분 나쁘고 그래요ㅠ",
          "original_form": "기분 나쁘고 그래요ㅠ",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020626.1.5",
          "form": "아 맞아요 병크 터졌죠",
          "original_form": "아 맞아요 병크 터졌죠",
          "speaker_id": "2",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020626.1.6",
          "form": "이런 그렇군요 ..",
          "original_form": "이런 그렇군요 ..",
          "speaker_id": "2",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020626.1.7",
          "form": "근데 왜 그렇게 인기가 많았었던 걸까요 ??",
          "original_form": "근데 왜 그렇게 인기가 많았었던 걸까요 ??",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020626.1.8",
          "form": "병크있으면 전 집중을 잘 못하는 타입이라ㅠㅠㅠ",
          "original_form": "병크있으면 전 집중을 잘 못하는 타입이라ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020626.1.9",
          "form": "그러게용",
          "original_form": "그러게용",
          "speaker_id": "1",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020626.1.10",
          "form": "스토리가 되게.. 좀 불편하다고 해야할까용",
          "original_form": "스토리가 되게.. 좀 불편하다고 해야할까용",
          "speaker_id": "1",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020626.1.11",
          "form": "맞아요 저도 병크터지면 집중 못해요 ...",
          "original_form": "맞아요 저도 병크터지면 집중 못해요 ...",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020626.1.12",
          "form": "근데 작가인가 감독인가 보는 사람으로 하여금 불편함을 느끼게 하는게",
          "original_form": "근데 작가인가 감독인가 보는 사람으로 하여금 불편함을 느끼게 하는게",
          "speaker_id": "1",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020626.1.13",
          "form": "의도라고 하더라구요",
          "original_form": "의도라고 하더라구요",
          "speaker_id": "1",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020626.1.14",
          "form": "아 .... 아 .....",
          "original_form": "아 .... 아 .....",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020626.1.15",
          "form": "성공?ㅋㅋㅋ했네요",
          "original_form": "성공?ㅋㅋㅋ했네요",
          "speaker_id": "1",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020626.1.16",
          "form": "그럼 그 의도가",
          "original_form": "그럼 그 의도가",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020626.1.17",
          "form": "그렇네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그렇네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020626.1.18",
          "form": "저 기생출 볼 때도 살짝 그런 느낌이었어요",
          "original_form": "저 기생출 볼 때도 살짝 그런 느낌이었어요",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020626.1.19",
          "form": "어머 기생충이요",
          "original_form": "어머 기생충이요",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020626.1.20",
          "form": "아하ㅠㅠ",
          "original_form": "아하ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020626.1.21",
          "form": "어쩐지",
          "original_form": "어쩐지",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020626.1.22",
          "form": "기생충 그거",
          "original_form": "기생충 그거",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020626.1.23",
          "form": "초반에 보다가 음... 하고 껐는데",
          "original_form": "초반에 보다가 음... 하고 껐는데",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020626.1.24",
          "form": "이것도 그런류인가바여",
          "original_form": "이것도 그런류인가바여",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020626.1.25",
          "form": "비슷한 결일 것 같네요",
          "original_form": "비슷한 결일 것 같네요",
          "speaker_id": "2",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020626.1.26",
          "form": "근데 기생충은 쓸데없이 야시꾸리한 장면들이 들어가있어요",
          "original_form": "근데 기생충은 쓸데없이 야시꾸리한 장면들이 들어가있어요",
          "speaker_id": "2",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020626.1.27",
          "form": "그래서 굳이 이걸 넣었어야싶나 궁금했는데",
          "original_form": "그래서 굳이 이걸 넣었어야싶나 궁금했는데",
          "speaker_id": "2",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020626.1.28",
          "form": "그 소파에서...",
          "original_form": "그 소파에서...",
          "speaker_id": "1",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020626.1.29",
          "form": "읍읍...",
          "original_form": "읍읍...",
          "speaker_id": "1",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020626.1.30",
          "form": "감독 인터뷰를 보니까",
          "original_form": "감독 인터뷰를 보니까",
          "speaker_id": "2",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020626.1.31",
          "form": "네네 ...",
          "original_form": "네네 ...",
          "speaker_id": "2",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020626.1.32",
          "form": "그것도 그렇고",
          "original_form": "그것도 그렇고",
          "speaker_id": "2",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020626.1.33",
          "form": "그 장면을 어찌어찌하다가 클립으로...봤어요...ㅋㅋㅋㅋ",
          "original_form": "그 장면을 어찌어찌하다가 클립으로...봤어요...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020626.1.34",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:38"
        }
