      "utterance": [
        {
          "id": "MDRW2100009358.1.1",
          "form": "코로나 끝나면 어디갈래?",
          "original_form": "코로나 끝나면 어디갈래?",
          "speaker_id": "2",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009358.1.2",
          "form": "우리 다음 여행지 발리였나? ㅋㅋㅋㅋ",
          "original_form": "우리 다음 여행지 발리였나? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009358.1.3",
          "form": "웅ㅋ",
          "original_form": "웅ㅋ",
          "speaker_id": "2",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009358.1.4",
          "form": "혹시 벌써 계획 완료? ㅎㅎ",
          "original_form": "혹시 벌써 계획 완료? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009358.1.5",
          "form": "아닝..ㅜ",
          "original_form": "아닝..ㅜ",
          "speaker_id": "2",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009358.1.6",
          "form": "믿고 가는 name2투어",
          "original_form": "믿고 가는 &name2&투어",
          "speaker_id": "1",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009358.1.7",
          "form": "근데 발리 입국허용인가?",
          "original_form": "근데 발리 입국허용인가?",
          "speaker_id": "2",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009358.1.8",
          "form": "사이판은 가능하단 이야기 들었는데ㅋ",
          "original_form": "사이판은 가능하단 이야기 들었는데ㅋ",
          "speaker_id": "2",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009358.1.9",
          "form": "연기됐대 ㅠ",
          "original_form": "연기됐대 ㅠ",
          "speaker_id": "1",
          "time": "20210822 22:59"
        },
        {
          "id": "MDRW2100009358.1.10",
          "form": "7~8월 외국인 관광 재개 목표했는데",
          "original_form": "7~8월 외국인 관광 재개 목표했는데",
          "speaker_id": "1",
          "time": "20210822 22:59"
        },
        {
          "id": "MDRW2100009358.1.11",
          "form": "사이판도?ㅜ",
          "original_form": "사이판도?ㅜ",
          "speaker_id": "2",
          "time": "20210822 22:59"
        },
        {
          "id": "MDRW2100009358.1.12",
          "form": "아,발리?",
          "original_form": "아,발리?",
          "speaker_id": "2",
          "time": "20210822 22:59"
        },
        {
          "id": "MDRW2100009358.1.13",
          "form": "웅 발리는 연기됐고, 사이판은 입국중!",
          "original_form": "웅 발리는 연기됐고, 사이판은 입국중!",
          "speaker_id": "1",
          "time": "20210822 23:00"
        },
        {
          "id": "MDRW2100009358.1.14",
          "form": "사이판에 여행객이 많지 않아서 전세 느낌난대 ㅋㅋ",
          "original_form": "사이판에 여행객이 많지 않아서 전세 느낌난대 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009358.1.15",
          "form": "ㅋㅋ내년에 우째되는지 봐야겠네",
          "original_form": "ㅋㅋ내년에 우째되는지 봐야겠네",
          "speaker_id": "2",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009358.1.16",
          "form": "크 완전 좋겠다 관광객별로 없어서",
          "original_form": "크 완전 좋겠다 관광객별로 없어서",
          "speaker_id": "1",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009358.1.17",
          "form": "내년에 휴일 118일",
          "original_form": "내년에 휴일 118일",
          "speaker_id": "1",
          "time": "20210822 23:02"
        },
        {
