      "utterance": [
        {
          "id": "MDRW2100015808.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 01:21"
        },
        {
          "id": "MDRW2100015808.1.2",
          "form": "안녕하세요~ 좋아하시는 작가있나요",
          "original_form": "안녕하세요~ 좋아하시는 작가있나요",
          "speaker_id": "2",
          "time": "20210826 01:21"
        },
        {
          "id": "MDRW2100015808.1.3",
          "form": "아니요 name2님은요?",
          "original_form": "아니요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015808.1.4",
          "form": "저는 기욤 뮈소책을 많이읽었었어요",
          "original_form": "저는 기욤 뮈소책을 많이읽었었어요",
          "speaker_id": "2",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015808.1.5",
          "form": "책 좋아하시나요?",
          "original_form": "책 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015808.1.6",
          "form": "어느정도는요 ㅎㅎ최근엔 달라구트의 꿈백화점을 봤어요",
          "original_form": "어느정도는요 ㅎㅎ최근엔 달라구트의 꿈백화점을 봤어요",
          "speaker_id": "2",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015808.1.7",
          "form": "어떤 내용이에요?",
          "original_form": "어떤 내용이에요?",
          "speaker_id": "1",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015808.1.8",
          "form": "우리나라 소설인데 밤에 우리가 꾸는 꿈이 상점서 사는거라는 내용이에요",
          "original_form": "우리나라 소설인데 밤에 우리가 꾸는 꿈이 상점서 사는거라는 내용이에요",
          "speaker_id": "2",
          "time": "20210826 01:32"
        },
        {
          "id": "MDRW2100015808.1.9",
          "form": "오 흥미진진하네요!",
          "original_form": "오 흥미진진하네요!",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015808.1.10",
          "form": "정말 재밌어요 ㅎㅎㅎ인기있는 꿈은 하늘을 나는 꿈이에요",
          "original_form": "정말 재밌어요 ㅎㅎㅎ인기있는 꿈은 하늘을 나는 꿈이에요",
          "speaker_id": "2",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100015808.1.11",
          "form": "왜 인기가 있나요?",
          "original_form": "왜 인기가 있나요?",
          "speaker_id": "1",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015808.1.12",
          "form": "스트레스가 풀리는 기분이라 그런가봐요 ㅎㅎㅎ 전 여행책도좋아해요",
          "original_form": "스트레스가 풀리는 기분이라 그런가봐요 ㅎㅎㅎ 전 여행책도좋아해요",
          "speaker_id": "2",
          "time": "20210826 01:38"
        },
        {
          "id": "MDRW2100015808.1.13",
          "form": "혹시 미니멀유목민 책 읽어보셨나요?",
          "original_form": "혹시 미니멀유목민 책 읽어보셨나요?",
          "speaker_id": "1",
          "time": "20210826 01:40"
        },
        {
          "id": "MDRW2100015808.1.14",
          "form": "엇 아니요",
          "original_form": "엇 아니요",
          "speaker_id": "2",
