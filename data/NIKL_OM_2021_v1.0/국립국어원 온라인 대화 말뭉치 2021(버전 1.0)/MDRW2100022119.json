        },
        {
          "id": "MDRW2100022119.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210829 08:10"
        },
        {
          "id": "MDRW2100022119.1.3",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 08:10"
        },
        {
          "id": "MDRW2100022119.1.4",
          "form": "네 좋아합니다 name2님은 좋아하시나요",
          "original_form": "네 좋아합니다 &name2&님은 좋아하시나요",
          "speaker_id": "1",
          "time": "20210829 08:10"
        },
        {
          "id": "MDRW2100022119.1.5",
          "form": "저두요 ㅋㅋ",
          "original_form": "저두요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 08:11"
        },
        {
          "id": "MDRW2100022119.1.6",
          "form": "어느 곳이 제일 좋으셨어요?",
          "original_form": "어느 곳이 제일 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210829 08:11"
        },
        {
          "id": "MDRW2100022119.1.7",
          "form": "전 부산좋았어요",
          "original_form": "전 부산좋았어요",
          "speaker_id": "1",
          "time": "20210829 08:11"
        },
        {
          "id": "MDRW2100022119.1.8",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 08:11"
        },
        {
          "id": "MDRW2100022119.1.9",
          "form": "부산 좋죠!",
          "original_form": "부산 좋죠!",
          "speaker_id": "2",
          "time": "20210829 08:16"
        },
        {
          "id": "MDRW2100022119.1.10",
          "form": "스무살때 친구들이랑 내일로 하면서 들렀던 기억이 나요 ㅋㅋ",
          "original_form": "스무살때 친구들이랑 내일로 하면서 들렀던 기억이 나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 08:16"
        },
        {
          "id": "MDRW2100022119.1.11",
          "form": "내일로 어땠어요",
          "original_form": "내일로 어땠어요",
          "speaker_id": "1",
          "time": "20210829 08:16"
        },
        {
          "id": "MDRW2100022119.1.12",
          "form": "신청하고싶긴 한데 코로나가 터져서..ㅎ",
          "original_form": "신청하고싶긴 한데 코로나가 터져서..ㅎ",
          "speaker_id": "1",
          "time": "20210829 08:17"
        },
        {
          "id": "MDRW2100022119.1.13",
          "form": "재밌어요 ㅋㅋ 저는 이제 까마득하지만",
          "original_form": "재밌어요 ㅋㅋ 저는 이제 까마득하지만",
          "speaker_id": "2",
          "time": "20210829 08:17"
        },
        {
          "id": "MDRW2100022119.1.14",
          "form": "진짜 그 나이에만 할 수 있는 경험이었던 것 같아요 ㅋㅋ",
          "original_form": "진짜 그 나이에만 할 수 있는 경험이었던 것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 08:17"
        },
        {
          "id": "MDRW2100022119.1.15",
          "form": "그래도 추억 하나쯤 생기는거라",
          "original_form": "그래도 추억 하나쯤 생기는거라",
          "speaker_id": "1",
          "time": "20210829 08:17"
        },
        {
          "id": "MDRW2100022119.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 08:17"
        },
        {
          "id": "MDRW2100022119.1.17",
          "form": "숙소도 없이 기차에서 자고 ㅋㅋㅋ",
          "original_form": "숙소도 없이 기차에서 자고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 08:17"
        },
        {
          "id": "MDRW2100022119.1.18",
          "form": "우와 그래도되는거에요?",
          "original_form": "우와 그래도되는거에요?",
          "speaker_id": "1",
          "time": "20210829 08:17"
        },
        {
          "id": "MDRW2100022119.1.19",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "1",
          "time": "20210829 08:17"
        },
        {
          "id": "MDRW2100022119.1.20",
          "form": "내일로 어디어디 가보셨어요?",
          "original_form": "내일로 어디어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210829 08:18"
        },
        {
          "id": "MDRW2100022119.1.21",
          "form": "경상도쪽이랑 강원도쪽으로 갔던 것 같아요 ㅋㅋㅋ",
          "original_form": "경상도쪽이랑 강원도쪽으로 갔던 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 08:19"
        },
        {
          "id": "MDRW2100022119.1.22",
          "form": "어디가 가장 좋았나요",
          "original_form": "어디가 가장 좋았나요",
          "speaker_id": "1",
          "time": "20210829 08:20"
        },
        {
          "id": "MDRW2100022119.1.23",
          "form": "경상 강원라인이라니 너무 재밌었을거같아요",
          "original_form": "경상 강원라인이라니 너무 재밌었을거같아요",
          "speaker_id": "1",
