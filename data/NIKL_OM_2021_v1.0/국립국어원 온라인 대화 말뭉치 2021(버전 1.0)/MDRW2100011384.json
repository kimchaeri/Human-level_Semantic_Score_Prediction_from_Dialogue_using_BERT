      "utterance": [
        {
          "id": "MDRW2100011384.1.1",
          "form": "최근에 사신것있나요?",
          "original_form": "최근에 사신것있나요?",
          "speaker_id": "1",
          "time": "20210824 11:54"
        },
        {
          "id": "MDRW2100011384.1.2",
          "form": "최근에 닌텐도 스위치 동물의 숲 게임을 샀어요",
          "original_form": "최근에 닌텐도 스위치 동물의 숲 게임을 샀어요",
          "speaker_id": "2",
          "time": "20210824 11:55"
        },
        {
          "id": "MDRW2100011384.1.3",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210824 12:16"
        },
        {
          "id": "MDRW2100011384.1.4",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "1",
          "time": "20210824 12:16"
        },
        {
          "id": "MDRW2100011384.1.5",
          "form": "네 소소하게 할만해요 자기전에 십분이십분정도 하기좋아요",
          "original_form": "네 소소하게 할만해요 자기전에 십분이십분정도 하기좋아요",
          "speaker_id": "2",
          "time": "20210824 12:17"
        },
        {
          "id": "MDRW2100011384.1.6",
          "form": "누누님은 최근에 사신거 있나요?",
          "original_form": "누누님은 최근에 사신거 있나요?",
          "speaker_id": "2",
          "time": "20210824 12:17"
        },
        {
          "id": "MDRW2100011384.1.7",
          "form": "저 운동화요ㅋㅋㅋ",
          "original_form": "저 운동화요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:23"
        },
        {
          "id": "MDRW2100011384.1.8",
          "form": "운동화 어디꺼 사셨나요?",
          "original_form": "운동화 어디꺼 사셨나요?",
          "speaker_id": "2",
          "time": "20210824 12:23"
        },
        {
          "id": "MDRW2100011384.1.9",
          "form": "나이키요!",
          "original_form": "나이키요!",
          "speaker_id": "1",
          "time": "20210824 12:29"
        },
        {
          "id": "MDRW2100011384.1.10",
          "form": "인터넷으로 사셨나요?",
          "original_form": "인터넷으로 사셨나요?",
          "speaker_id": "2",
          "time": "20210824 12:35"
        },
        {
          "id": "MDRW2100011384.1.11",
          "form": "네네 롯데몰인가 어디서구매했던거같네요ㅋㅋㅋ",
          "original_form": "네네 롯데몰인가 어디서구매했던거같네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100011384.1.12",
          "form": "오즈위고요",
          "original_form": "오즈위고요",
          "speaker_id": "1",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100011384.1.13",
          "form": "어때요 만족하세요?",
          "original_form": "어때요 만족하세요?",
          "speaker_id": "2",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100011384.1.14",
          "form": "ㅋㅋㅋ네네 귀여워요",
          "original_form": "ㅋㅋㅋ네네 귀여워요",
          "speaker_id": "1",
          "time": "20210824 12:47"
        },
        {
          "id": "MDRW2100011384.1.15",
          "form": "사시고싶으신거있나요?",
          "original_form": "사시고싶으신거있나요?",
          "speaker_id": "1",
          "time": "20210824 12:47"
        },
        {
          "id": "MDRW2100011384.1.16",
          "form": "제습기요!",
          "original_form": "제습기요!",
          "speaker_id": "2",
          "time": "20210824 12:52"
        },
        {
          "id": "MDRW2100011384.1.17",
          "form": "특히 오늘같은날 제습기 뽐뿌생겨요",
          "original_form": "특히 오늘같은날 제습기 뽐뿌생겨요",
          "speaker_id": "2",
          "time": "20210824 12:52"
        },
        {
          "id": "MDRW2100011384.1.18",
          "form": "제습기좋네요",
          "original_form": "제습기좋네요",
          "speaker_id": "1",
          "time": "20210824 12:56"
        },
        {
          "id": "MDRW2100011384.1.19",
          "form": "저는 가습기가 사고싶어요ㅋㅋㅋ",
          "original_form": "저는 가습기가 사고싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:56"
        }
      ]
