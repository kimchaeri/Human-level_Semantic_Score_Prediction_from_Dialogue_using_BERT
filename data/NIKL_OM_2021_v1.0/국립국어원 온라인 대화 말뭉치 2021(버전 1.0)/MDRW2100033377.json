      "utterance": [
        {
          "id": "MDRW2100033377.1.1",
          "form": "공복에 잠드셨나여?",
          "original_form": "공복에 잠드셨나여?",
          "speaker_id": "2",
          "time": "20210907 06:24"
        },
        {
          "id": "MDRW2100033377.1.2",
          "form": "네:) 공복 상태로 잤어요ㅋㅋ",
          "original_form": "네:) 공복 상태로 잤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 07:55"
        },
        {
          "id": "MDRW2100033377.1.3",
          "form": "이제 아침드세요",
          "original_form": "이제 아침드세요",
          "speaker_id": "2",
          "time": "20210907 08:20"
        },
        {
          "id": "MDRW2100033377.1.4",
          "form": "아침 ㅜㅜ네 먹도록 하겠습니다",
          "original_form": "아침 ㅜㅜ네 먹도록 하겠습니다",
          "speaker_id": "1",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100033377.1.5",
          "form": "점심드셨나요?",
          "original_form": "점심드셨나요?",
          "speaker_id": "2",
          "time": "20210907 17:19"
        },
        {
          "id": "MDRW2100033377.1.6",
          "form": "아뇽ㅋㅋ",
          "original_form": "아뇽ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100033377.1.7",
          "form": "조금전에 집에 오면서\r\n빠바에서 빵 사왔어용",
          "original_form": "조금전에 집에 오면서\r\n빠바에서 빵 사왔어용",
          "speaker_id": "1",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100033377.1.8",
          "form": "단식하세요?",
          "original_form": "단식하세요?",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100033377.1.9",
          "form": "왜케 안드세요",
          "original_form": "왜케 안드세요",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100033377.1.10",
          "form": "아니요ㅠ",
          "original_form": "아니요ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100033377.1.11",
          "form": "요새 왜케 식욕이 없을까요",
          "original_form": "요새 왜케 식욕이 없을까요",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100033377.1.12",
          "form": "걍 쉬고만 싶어요",
          "original_form": "걍 쉬고만 싶어요",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100033377.1.13",
          "form": "저녁드셨어요?",
          "original_form": "저녁드셨어요?",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100033377.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100033377.1.15",
          "form": "먹었죠",
          "original_form": "먹었죠",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100033377.1.16",
          "form": "어디 아프세요?",
          "original_form": "어디 아프세요?",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100033377.1.17",
          "form": "아니요ㅠ 건강해요",
          "original_form": "아니요ㅠ 건강해요",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100033377.1.18",
          "form": "오늘은 엄마가\r\n뭐 맛있는거 해주셨나용",
          "original_form": "오늘은 엄마가\r\n뭐 맛있는거 해주셨나용",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100033377.1.19",
          "form": "가을타요?",
          "original_form": "가을타요?",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100033377.1.20",
          "form": "가을타네",
          "original_form": "가을타네",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100033377.1.21",
          "form": "맞죠?",
          "original_form": "맞죠?",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100033377.1.22",
          "form": "오늘은 그냥 치킨먹었어요",
          "original_form": "오늘은 그냥 치킨먹었어요",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100033377.1.23",
          "form": "ㅋㅋ맞아요\r\n가을타는거 정답!",
          "original_form": "ㅋㅋ맞아요\r\n가을타는거 정답!",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100033377.1.24",
          "form": "어디 치킨드셨어요?",
          "original_form": "어디 치킨드셨어요?",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100033377.1.25",
          "form": "푸라닭이요",
          "original_form": "푸라닭이요",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100033377.1.26",
          "form": "고독하십니까",
          "original_form": "고독하십니까",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100033377.1.27",
          "form": "꺄아 블랙알리오?",
          "original_form": "꺄아 블랙알리오?",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100033377.1.28",
          "form": "안고독한데영ㅋㅋㅋㅋㅋ",
          "original_form": "안고독한데영ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100033377.1.29",
          "form": "걍 센치해짐 가을되면 ㅋㅋ",
          "original_form": "걍 센치해짐 가을되면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100033377.1.30",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:12"
        }
      ]
    }
