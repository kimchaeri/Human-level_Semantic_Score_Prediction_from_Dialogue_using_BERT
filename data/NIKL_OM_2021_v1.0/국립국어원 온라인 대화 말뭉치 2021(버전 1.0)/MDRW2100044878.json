      "utterance": [
        {
          "id": "MDRW2100044878.1.1",
          "form": "아이몇명이세요?ㅋ",
          "original_form": "아이몇명이세요?ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044878.1.2",
          "form": "딸아이 1명요.",
          "original_form": "딸아이 1명요.",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044878.1.3",
          "form": "와 부러워요",
          "original_form": "와 부러워요",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044878.1.4",
          "form": "아들맘입니다ㅜㅜ",
          "original_form": "아들맘입니다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044878.1.5",
          "form": "딸은 원피스 예쁘게 ㅎㅎ 입겠네요",
          "original_form": "딸은 원피스 예쁘게 ㅎㅎ 입겠네요",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044878.1.6",
          "form": "네.\r\n내일 입힐 옷 준비해야하는데 아직 준비 전이네요\r\n  ㅎ\r\n아이 신발은 뭐 신기세요?",
          "original_form": "네.\r\n내일 입힐 옷 준비해야하는데 아직 준비 전이네요\r\n  ㅎ\r\n아이 신발은 뭐 신기세요?",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044878.1.7",
          "form": "3살이라 아기상어 샌달이요ㅋㅋㅋ",
          "original_form": "3살이라 아기상어 샌달이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044878.1.8",
          "form": "집안일도와줘요 딸이?",
          "original_form": "집안일도와줘요 딸이?",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044878.1.9",
          "form": "아이가 7세인데요.\r\n자기가 어지른 것은 좀 치우네요\r\n\r\n3세면 손이 많이 가겠어요.",
          "original_form": "아이가 7세인데요.\r\n자기가 어지른 것은 좀 치우네요\r\n\r\n3세면 손이 많이 가겠어요.",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044878.1.10",
          "form": "네네 맘에 안들면 소리지르고\r\n남편도 집안일많이도와주나요?",
          "original_form": "네네 맘에 안들면 소리지르고\r\n남편도 집안일많이도와주나요?",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044878.1.11",
          "form": "집안일은 제 일이 아니라\r\n남편이 도와주는 게 아니라\r\n함께 합니다. ㅎ\r\n분담했어요.\r\n님은요?",
          "original_form": "집안일은 제 일이 아니라\r\n남편이 도와주는 게 아니라\r\n함께 합니다. ㅎ\r\n분담했어요.\r\n님은요?",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044878.1.12",
          "form": "와 그말진짜멋있네요\r\n저도 써먹어야지",
          "original_form": "와 그말진짜멋있네요\r\n저도 써먹어야지",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044878.1.13",
          "form": "저는 7대3정도 하고",
          "original_form": "저는 7대3정도 하고",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044878.1.14",
          "form": "아들목욕은 꼭 아빠가시켜서 좋아요ㅋㅋ",
          "original_form": "아들목욕은 꼭 아빠가시켜서 좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044878.1.15",
          "form": "아.\r\n목욕도 일인데요.\r\n잘됐네요. ㅎ\r\n아이 크면 공부도 남편분에게 맡기세요.ㅎ",
          "original_form": "아.\r\n목욕도 일인데요.\r\n잘됐네요. ㅎ\r\n아이 크면 공부도 남편분에게 맡기세요.ㅎ",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044878.1.16",
          "form": "오 공부는 전혀 아직 생각도안했는데",
          "original_form": "오 공부는 전혀 아직 생각도안했는데",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044878.1.17",
          "form": "책도 아빠가 잘읽어주나요?",
          "original_form": "책도 아빠가 잘읽어주나요?",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044878.1.18",
          "form": "책은 남편보다 제가 더 읽어주고 있는 것 같은데\r\n귀찮네요. ㅋ 책을 읽는데 책 속 그림을 보면서 퀴즈도 내고 자꾸 딴지를 걸어요.ㅎ",
          "original_form": "책은 남편보다 제가 더 읽어주고 있는 것 같은데\r\n귀찮네요. ㅋ 책을 읽는데 책 속 그림을 보면서 퀴즈도 내고 자꾸 딴지를 걸어요.ㅎ",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044878.1.19",
          "form": "어머 이뻐라! ㅎㅎ 딸이 그런게좋은듯요ㅋ",
          "original_form": "어머 이뻐라! ㅎㅎ 딸이 그런게좋은듯요ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044878.1.20",
          "form": "잠은 일찍자요 아기?",
          "original_form": "잠은 일찍자요 아기?",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100044878.1.21",
          "form": "들쑥날쑥해요. \r\n지금은 10시쯤 자나봐요.",
          "original_form": "들쑥날쑥해요. \r\n지금은 10시쯤 자나봐요.",
          "speaker_id": "2",
          "time": "20210908 00:37"
        }
      ]
    }
