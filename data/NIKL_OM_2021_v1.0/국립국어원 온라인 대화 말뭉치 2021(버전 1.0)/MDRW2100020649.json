        },
        {
          "id": "MDRW2100020649.1.2",
          "form": "name1님은 편의점가면 뭐 드세요",
          "original_form": "&name1&님은 편의점가면 뭐 드세요",
          "speaker_id": "2",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020649.1.3",
          "form": "전.. 과자 집어와요 음료랑",
          "original_form": "전.. 과자 집어와요 음료랑",
          "speaker_id": "1",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020649.1.4",
          "form": "과자 좋아하는거 있으세요",
          "original_form": "과자 좋아하는거 있으세요",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020649.1.5",
          "form": "포카칩...",
          "original_form": "포카칩...",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020649.1.6",
          "form": "옳다",
          "original_form": "옳다",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020649.1.7",
          "form": "감자과자는 언제나 옳다",
          "original_form": "감자과자는 언제나 옳다",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020649.1.8",
          "form": "인정하십니까",
          "original_form": "인정하십니까",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020649.1.9",
          "form": "꼬깔콘... 인절미과자... 츄러스..",
          "original_form": "꼬깔콘... 인절미과자... 츄러스..",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020649.1.10",
          "form": "그쵸 맛없는걸 못봤어요",
          "original_form": "그쵸 맛없는걸 못봤어요",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020649.1.11",
          "form": "꼬깔콘도 완전좋아",
          "original_form": "꼬깔콘도 완전좋아",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020649.1.12",
          "form": "단짠단짠으로 골고루 섞어와야해요",
          "original_form": "단짠단짠으로 골고루 섞어와야해요",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020649.1.13",
          "form": "그럼 포카칩 초록 파랑",
          "original_form": "그럼 포카칩 초록 파랑",
          "speaker_id": "1",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020649.1.14",
          "form": "전 오리지날인 파랑",
          "original_form": "전 오리지날인 파랑",
          "speaker_id": "1",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020649.1.15",
          "form": "아 맛차이를 못느끼는데",
          "original_form": "아 맛차이를 못느끼는데",
          "speaker_id": "2",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020649.1.16",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020649.1.17",
          "form": "두개 무슨 차이죠!",
          "original_form": "두개 무슨 차이죠!",
          "speaker_id": "2",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020649.1.18",
          "form": "어니언과 소금의 맛을",
          "original_form": "어니언과 소금의 맛을",
          "speaker_id": "1",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020649.1.19",
          "form": "못느끼셨나요",
          "original_form": "못느끼셨나요",
          "speaker_id": "1",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020649.1.20",
          "form": "초록이 어니언이에요",
          "original_form": "초록이 어니언이에요",
