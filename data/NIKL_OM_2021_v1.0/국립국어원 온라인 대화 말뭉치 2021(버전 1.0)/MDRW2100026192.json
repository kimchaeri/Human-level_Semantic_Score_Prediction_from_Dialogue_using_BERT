      "utterance": [
        {
          "id": "MDRW2100026192.1.1",
          "form": "요즘 사고싶은게 뭐에요?",
          "original_form": "요즘 사고싶은게 뭐에요?",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100026192.1.2",
          "form": "만년필 사고싶어요!",
          "original_form": "만년필 사고싶어요!",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026192.1.3",
          "form": "사고싶은거 있나요??",
          "original_form": "사고싶은거 있나요??",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026192.1.4",
          "form": "오!! 만녈필 예전에 갖고싶었는데",
          "original_form": "오!! 만녈필 예전에 갖고싶었는데",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026192.1.5",
          "form": "저는 요즘 플스 갖고싶더라고요",
          "original_form": "저는 요즘 플스 갖고싶더라고요",
          "speaker_id": "1",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026192.1.6",
          "form": "게임 좋아하시나봐요!",
          "original_form": "게임 좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100026192.1.7",
          "form": "저요! 그닥 열심히는 안하는데 가끔 꽂히는게 있어요",
          "original_form": "저요! 그닥 열심히는 안하는데 가끔 꽂히는게 있어요",
          "speaker_id": "1",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100026192.1.8",
          "form": "플스 갖고싶은게 단지 저스트댄스를 하고싶다는 이유...",
          "original_form": "플스 갖고싶은게 단지 저스트댄스를 하고싶다는 이유...",
          "speaker_id": "1",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100026192.1.9",
          "form": "좋은데요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "좋은데요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026192.1.10",
          "form": "저는 옛날에 파이널판타지가 너무 하고싶어서 플스를 썼었는데 지금은 어디 있는지도 모르겠어요 ㅎㅎ",
          "original_form": "저는 옛날에 파이널판타지가 너무 하고싶어서 플스를 썼었는데 지금은 어디 있는지도 모르겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026192.1.11",
          "form": "너무 옛날꺼라 저스트댄스는 플레이가 안 되겠죠 ㅎㅎ",
          "original_form": "너무 옛날꺼라 저스트댄스는 플레이가 안 되겠죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026192.1.12",
          "form": "아 그게 오래되면 안되나요?",
          "original_form": "아 그게 오래되면 안되나요?",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026192.1.13",
          "form": "사실 이러다가도 금방 식을듯요",
          "original_form": "사실 이러다가도 금방 식을듯요",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026192.1.14",
          "form": "한번씩 잠결에 쇼핑하기도 해요 ㅎ",
          "original_form": "한번씩 잠결에 쇼핑하기도 해요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026192.1.15",
          "form": "잠결에 ㅋㅋㅋㅋㅋㅋㅋ 잠결에 주로 뭘 구매하시나요 ㅋㅋㅋㅋㅋ",
          "original_form": "잠결에 ㅋㅋㅋㅋㅋㅋㅋ 잠결에 주로 뭘 구매하시나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026192.1.16",
          "form": "홧김에 지르시나요??",
          "original_form": "홧김에 지르시나요??",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026192.1.17",
          "form": "ㅋㅋㅋㅋ 저도 모르게 뭔가 샀더라고요",
          "original_form": "ㅋㅋㅋㅋ 저도 모르게 뭔가 샀더라고요",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026192.1.18",
          "form": "그때그때 달라요ㅋㅋㅋ",
          "original_form": "그때그때 달라요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026192.1.19",
          "form": "저번에는 보니까 뭘 샀더라...무슨 양은도시락을 샀더라고요",
          "original_form": "저번에는 보니까 뭘 샀더라...무슨 양은도시락을 샀더라고요",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026192.1.20",
          "form": "꿈에서 배가 고프셨던 걸까요 ㅋㅋㅋㅋ",
          "original_form": "꿈에서 배가 고프셨던 걸까요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100026192.1.21",
          "form": "아침에 깜짝깜짝 놀라겠어요 ㅋㅋㅋㅋ",
          "original_form": "아침에 깜짝깜짝 놀라겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100026192.1.22",
          "form": "ㅋㅋㅋㅋ분홍소세지가 먹고싶었긴했어요",
          "original_form": "ㅋㅋㅋㅋ분홍소세지가 먹고싶었긴했어요",
          "speaker_id": "1",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100026192.1.23",
          "form": "옛날 도시락처럼 해먹고싶었나봐요",
          "original_form": "옛날 도시락처럼 해먹고싶었나봐요",
          "speaker_id": "1",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100026192.1.24",
          "form": "네ㅋㅋㅋ 가끔 그러면 별걸 다 샀네 싶어요",
          "original_form": "네ㅋㅋㅋ 가끔 그러면 별걸 다 샀네 싶어요",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026192.1.25",
          "form": "옛날 도시락 흔들어 먹으니까 진짜 맛있던데",
          "original_form": "옛날 도시락 흔들어 먹으니까 진짜 맛있던데",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026192.1.26",
          "form": "사고 나서 해보셨나요??",
          "original_form": "사고 나서 해보셨나요??",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026192.1.27",
          "form": "ㅋㅋㅋㅋㅋ아직이요",
          "original_form": "ㅋㅋㅋㅋㅋ아직이요",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026192.1.28",
          "form": "사놓고 까먹었어요 ㅎ",
          "original_form": "사놓고 까먹었어요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026192.1.29",
          "form": "요번 주말에 해먹어봐야겠어요",
          "original_form": "요번 주말에 해먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210901 21:30"
        }
      ]
