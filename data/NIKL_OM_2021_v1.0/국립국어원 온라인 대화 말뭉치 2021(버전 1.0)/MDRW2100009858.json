      "utterance": [
        {
          "id": "MDRW2100009858.1.1",
          "form": "여름다 갔는데 베스킨어떤거 드셔보셨나요",
          "original_form": "여름다 갔는데 베스킨어떤거 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009858.1.2",
          "form": "와~ 맛있겠네요ㅎ",
          "original_form": "와~ 맛있겠네요ㅎ",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009858.1.3",
          "form": "저희는 거의 같은데 초코나무숲\r\n아몬드봉봉 엄마는외계인 슈팅스타 이런거좋아해요",
          "original_form": "저희는 거의 같은데 초코나무숲\r\n아몬드봉봉 엄마는외계인 슈팅스타 이런거좋아해요",
          "speaker_id": "2",
          "time": "20210823 11:01"
        },
        {
          "id": "MDRW2100009858.1.4",
          "form": "저랑 취향이 비슷하시네요",
          "original_form": "저랑 취향이 비슷하시네요",
          "speaker_id": "1",
          "time": "20210823 11:02"
        },
        {
          "id": "MDRW2100009858.1.5",
          "form": "저도 신것보다 고소하고 단게 좋아요",
          "original_form": "저도 신것보다 고소하고 단게 좋아요",
          "speaker_id": "1",
          "time": "20210823 11:02"
        },
        {
          "id": "MDRW2100009858.1.6",
          "form": "저는 신상나오면 사먹는편인데",
          "original_form": "저는 신상나오면 사먹는편인데",
          "speaker_id": "1",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009858.1.7",
          "form": "레인보우는 셔보여서ㅜ 안먹었어요",
          "original_form": "레인보우는 셔보여서ㅜ 안먹었어요",
          "speaker_id": "1",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009858.1.8",
          "form": "레인보우 샤베틍ᆢ?",
          "original_form": "레인보우 샤베틍ᆢ?",
          "speaker_id": "2",
          "time": "20210823 11:06"
        },
        {
          "id": "MDRW2100009858.1.9",
          "form": "그거말고 먼가 신상이 있던데 까먹었네요",
          "original_form": "그거말고 먼가 신상이 있던데 까먹었네요",
          "speaker_id": "1",
          "time": "20210823 11:07"
        },
        {
          "id": "MDRW2100009858.1.10",
          "form": "담달 신상인 맥심모카골드도 궁금하네요",
          "original_form": "담달 신상인 맥심모카골드도 궁금하네요",
          "speaker_id": "1",
          "time": "20210823 11:07"
        },
        {
          "id": "MDRW2100009858.1.11",
          "form": "아ㅋㅋ신상은 안먹어봤는데ㅎ 맥심은 맛있을것같아요~ 커피맛",
          "original_form": "아ㅋㅋ신상은 안먹어봤는데ㅎ 맥심은 맛있을것같아요~ 커피맛",
          "speaker_id": "2",
          "time": "20210823 11:10"
        },
        {
          "id": "MDRW2100009858.1.12",
          "form": "가을엔 커피죠..",
          "original_form": "가을엔 커피죠..",
          "speaker_id": "1",
          "time": "20210823 11:11"
        },
        {
          "id": "MDRW2100009858.1.13",
          "form": "베스킨이 멀좀아네요",
          "original_form": "베스킨이 멀좀아네요",
          "speaker_id": "1",
          "time": "20210823 11:11"
        },
        {
          "id": "MDRW2100009858.1.14",
          "form": "ㅋㅋㅋ 갑자기 초코나무숲이 안보이더라구요",
          "original_form": "ㅋㅋㅋ 갑자기 초코나무숲이 안보이더라구요",
          "speaker_id": "2",
          "time": "20210823 11:12"
        },
        {
          "id": "MDRW2100009858.1.15",
          "form": "요 앞전에 못먹었어요ㅠ",
          "original_form": "요 앞전에 못먹었어요ㅠ",
          "speaker_id": "2",
          "time": "20210823 11:12"
        },
        {
          "id": "MDRW2100009858.1.16",
          "form": "안돼~~",
          "original_form": "안돼~~",
          "speaker_id": "1",
          "time": "20210823 11:13"
        },
        {
          "id": "MDRW2100009858.1.17",
          "form": "name2이님하고 야기하니 갑자기 또 초코나무숲이 먹어보고싶네요 ㅋㅋ",
          "original_form": "&name2&이님하고 야기하니 갑자기 또 초코나무숲이 먹어보고싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 11:13"
        },
        {
          "id": "MDRW2100009858.1.18",
          "form": "배스킨하고 던킨에서 이번에 보드게임 콜라보 나온거 보셨어요?",
          "original_form": "배스킨하고 던킨에서 이번에 보드게임 콜라보 나온거 보셨어요?",
          "speaker_id": "2",
          "time": "20210823 11:14"
        },
        {
          "id": "MDRW2100009858.1.19",
          "form": "두둥! 아니요",
          "original_form": "두둥! 아니요",
          "speaker_id": "1",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009858.1.20",
          "form": "찾아봐야겠어요",
          "original_form": "찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009858.1.21",
          "form": "모두의마블 콜라보네요",
          "original_form": "모두의마블 콜라보네요",
          "speaker_id": "1",
          "time": "20210823 11:16"
        },
        {
          "id": "MDRW2100009858.1.22",
          "form": "맞아요ㅡ배스킨께 이쁘던데 그래서 살까말까 하고있어요 아이스크림도 먹고 일석이조",
          "original_form": "맞아요ㅡ배스킨께 이쁘던데 그래서 살까말까 하고있어요 아이스크림도 먹고 일석이조",
          "speaker_id": "2",
          "time": "20210823 11:17"
        },
        {
          "id": "MDRW2100009858.1.23",
          "form": "찾아보니까 넘 이쁘네요 저두 겜 했었었는데",
          "original_form": "찾아보니까 넘 이쁘네요 저두 겜 했었었는데",
          "speaker_id": "1",
          "time": "20210823 11:19"
        },
        {
          "id": "MDRW2100009858.1.24",
          "form": "집에서 놀기좋네",
          "original_form": "집에서 놀기좋네",
          "speaker_id": "1",
          "time": "20210823 11:19"
        }
      ]
    }
