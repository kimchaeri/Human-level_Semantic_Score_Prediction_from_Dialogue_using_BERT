      "utterance": [
        {
          "id": "MDRW2100015828.1.1",
          "form": "이런주제를 얘기해야겠니? ㅋㅋㅋㅋㅋ",
          "original_form": "이런주제를 얘기해야겠니? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:32"
        },
        {
          "id": "MDRW2100015828.1.2",
          "form": "저는 아직 비혼주의입니다만 어떠신가요",
          "original_form": "저는 아직 비혼주의입니다만 어떠신가요",
          "speaker_id": "2",
          "time": "20210826 01:32"
        },
        {
          "id": "MDRW2100015828.1.3",
          "form": "저는 꿈에 지디가 나와서요",
          "original_form": "저는 꿈에 지디가 나와서요",
          "speaker_id": "1",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015828.1.4",
          "form": "지디랑 학교에서 연애했더니",
          "original_form": "지디랑 학교에서 연애했더니",
          "speaker_id": "1",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015828.1.5",
          "form": "설렘이 생겼어요",
          "original_form": "설렘이 생겼어요",
          "speaker_id": "1",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015828.1.6",
          "form": "지디무슨일이야.",
          "original_form": "지디무슨일이야.",
          "speaker_id": "2",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015828.1.7",
          "form": "지디 실화냐.",
          "original_form": "지디 실화냐.",
          "speaker_id": "2",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015828.1.8",
          "form": "제말이요ㅋㅋ",
          "original_form": "제말이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015828.1.9",
          "form": "나오늘 지디 노래듣고싶었는데 1년정거장 ㅋㅋㅋ",
          "original_form": "나오늘 지디 노래듣고싶었는데 1년정거장 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015828.1.10",
          "form": "아니 지디 제니랑 연애한다며",
          "original_form": "아니 지디 제니랑 연애한다며",
          "speaker_id": "2",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015828.1.11",
          "form": "응응 그렇더라규",
          "original_form": "응응 그렇더라규",
          "speaker_id": "1",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015828.1.12",
          "form": "내남자였는게",
          "original_form": "내남자였는게",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015828.1.13",
          "form": "꿈에서",
          "original_form": "꿈에서",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015828.1.14",
          "form": "아니 좀 나이차 나지않나?",
          "original_form": "아니 좀 나이차 나지않나?",
          "speaker_id": "2",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015828.1.15",
          "form": "나이차나는 연애어떠싶니까",
          "original_form": "나이차나는 연애어떠싶니까",
          "speaker_id": "2",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015828.1.16",
          "form": "띠동갑까지는",
          "original_form": "띠동갑까지는",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015828.1.17",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015828.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015828.1.19",
          "form": "위로 아래로? ㅋㅋㅋ",
          "original_form": "위로 아래로? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100015828.1.20",
          "form": "아 급 고민이네ㅋㅋㅋㅋ",
          "original_form": "아 급 고민이네ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100015828.1.21",
          "form": "ㅋㅋㅋㅋ고민할일이야?ㅋㅋㅋ아래띠동갑이면 와 풋풋하네",
          "original_form": "ㅋㅋㅋㅋ고민할일이야?ㅋㅋㅋ아래띠동갑이면 와 풋풋하네",
          "speaker_id": "2",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015828.1.22",
          "form": "위아래 둘다 좋다야",
          "original_form": "위아래 둘다 좋다야",
          "speaker_id": "1",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015828.1.23",
          "form": "아니다 풋풋이아니라 너무어무어려",
          "original_form": "아니다 풋풋이아니라 너무어무어려",
          "speaker_id": "2",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015828.1.24",
          "form": "절로 웃음이 나옴ㅋㅋㅋ",
          "original_form": "절로 웃음이 나옴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015828.1.25",
          "form": "내제자들나이야...",
          "original_form": "내제자들나이야...",
          "speaker_id": "2",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015828.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015828.1.27",
          "form": "그건아닌거같어...",
          "original_form": "그건아닌거같어...",
          "speaker_id": "2",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015828.1.28",
          "form": "연애는 괜찮지않아?ㅋㅋㅋㅋ",
          "original_form": "연애는 괜찮지않아?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015828.1.29",
          "form": "난좀 좀 좀 그래...ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "난좀 좀 좀 그래...ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015828.1.30",
          "form": "어른스러운 연애",
          "original_form": "어른스러운 연애",
          "speaker_id": "1",
          "time": "20210826 01:38"
        },
        {
          "id": "MDRW2100015828.1.31",
          "form": "해보고싶다",
          "original_form": "해보고싶다",
          "speaker_id": "1",
          "time": "20210826 01:38"
        },
        {
          "id": "MDRW2100015828.1.32",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:38"
        },
        {
          "id": "MDRW2100015828.1.33",
          "form": "어른스러운연애가대체뭔겨",
          "original_form": "어른스러운연애가대체뭔겨",
          "speaker_id": "2",
          "time": "20210826 01:38"
        },
        {
          "id": "MDRW2100015828.1.34",
          "form": "서로에게 얽매이지 않은",
          "original_form": "서로에게 얽매이지 않은",
          "speaker_id": "1",
          "time": "20210826 01:39"
        },
        {
          "id": "MDRW2100015828.1.35",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:39"
        },
        {
          "id": "MDRW2100015828.1.36",
          "form": "근데 그게 그냥 권태로워진거일수도있지않나?",
          "original_form": "근데 그게 그냥 권태로워진거일수도있지않나?",
          "speaker_id": "2",
          "time": "20210826 01:40"
        },
        {
          "id": "MDRW2100015828.1.37",
          "form": "아냐 내 로망이야",
          "original_form": "아냐 내 로망이야",
          "speaker_id": "1",
          "time": "20210826 01:41"
        },
        {
          "id": "MDRW2100015828.1.38",
          "form": "불타오르지만 구속하지 않는",
          "original_form": "불타오르지만 구속하지 않는",
          "speaker_id": "1",
          "time": "20210826 01:41"
        },
        {
          "id": "MDRW2100015828.1.39",
          "form": "아니왜 막 계속계속 설레는 부부들도있다잖어 ㅋㅋ",
          "original_form": "아니왜 막 계속계속 설레는 부부들도있다잖어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:41"
        },
        {
          "id": "MDRW2100015828.1.40",
          "form": "시간이 덜지나서",
          "original_form": "시간이 덜지나서",
          "speaker_id": "1",
          "time": "20210826 01:42"
        },
        {
          "id": "MDRW2100015828.1.41",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:42"
        },
        {
          "id": "MDRW2100015828.1.42",
          "form": "구속이 아예 없을 수있나?",
          "original_form": "구속이 아예 없을 수있나?",
          "speaker_id": "2",
          "time": "20210826 01:42"
        },
        {
          "id": "MDRW2100015828.1.43",
          "form": "구속이 어떤거야?",
          "original_form": "구속이 어떤거야?",
          "speaker_id": "1",
          "time": "20210826 01:43"
        },
        {
          "id": "MDRW2100015828.1.44",
          "form": "니가생각하는구속은뭔데? ㅋㅋㅋ 난음 뭐 연락을 누구랑하는지 확인하는정도...? 약속을 누구랑하는지 이런거",
          "original_form": "니가생각하는구속은뭔데? ㅋㅋㅋ 난음 뭐 연락을 누구랑하는지 확인하는정도...? 약속을 누구랑하는지 이런거",
          "speaker_id": "2",
          "time": "20210826 01:44"
        },
        {
          "id": "MDRW2100015828.1.45",
          "form": "그것도 구속이야?????",
          "original_form": "그것도 구속이야?????",
          "speaker_id": "1",
          "time": "20210826 01:45"
        },
        {
          "id": "MDRW2100015828.1.46",
          "form": "관심아니냐ㅋㅋㅋ",
          "original_form": "관심아니냐ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:45"
        },
        {
          "id": "MDRW2100015828.1.47",
          "form": "그런가? ㅋㅋㅋㅋㅋ 아 근데 저정도는 그럴수있다 이거지",
          "original_form": "그런가? ㅋㅋㅋㅋㅋ 아 근데 저정도는 그럴수있다 이거지",
          "speaker_id": "2",
          "time": "20210826 01:46"
        },
        {
          "id": "MDRW2100015828.1.48",
          "form": "응응 난 옷입는거 친구랑만나는거",
          "original_form": "응응 난 옷입는거 친구랑만나는거",
          "speaker_id": "1",
          "time": "20210826 01:47"
        },
        {
          "id": "MDRW2100015828.1.49",
          "form": "하루종일 연락하는거",
          "original_form": "하루종일 연락하는거",
          "speaker_id": "1",
          "time": "20210826 01:47"
        },
        {
          "id": "MDRW2100015828.1.50",
          "form": "진짜 구속임ㅋ",
          "original_form": "진짜 구속임ㅋ",
          "speaker_id": "1",
          "time": "20210826 01:47"
        },
        {
          "id": "MDRW2100015828.1.51",
          "form": "근데 다들 어느정도의 구속은 있는듯 ㅋㅋ정도의 차이지",
          "original_form": "근데 다들 어느정도의 구속은 있는듯 ㅋㅋ정도의 차이지",
          "speaker_id": "2",
          "time": "20210826 01:48"
        },
        {
          "id": "MDRW2100015828.1.52",
          "form": "ㅋㅋㅋ맞앙",
          "original_form": "ㅋㅋㅋ맞앙",
          "speaker_id": "1",
          "time": "20210826 01:48"
        }
      ]
