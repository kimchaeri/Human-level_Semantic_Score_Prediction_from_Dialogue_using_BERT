      "utterance": [
        {
          "id": "MDRW2100008507.1.1",
          "form": "저는 대학교때 과제 대충해가는 놈팽이었어요",
          "original_form": "저는 대학교때 과제 대충해가는 놈팽이었어요",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008507.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 저는 직전까지 미루다미루다 밤새서 해가는...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 저는 직전까지 미루다미루다 밤새서 해가는...",
          "speaker_id": "2",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008507.1.3",
          "form": "아 저와같은 부류시군요",
          "original_form": "아 저와같은 부류시군요",
          "speaker_id": "1",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100008507.1.4",
          "form": "지금도 일할때 그러고있어요",
          "original_form": "지금도 일할때 그러고있어요",
          "speaker_id": "1",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100008507.1.5",
          "form": "넘나 일하기싫네용ㅋㅋㅋㅋㅋㅋ",
          "original_form": "넘나 일하기싫네용ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100008507.1.6",
          "form": "네 일도 그래요 ㅋㅋㅋ 근데 팀플이면 눈치보여서 먼저 해요",
          "original_form": "네 일도 그래요 ㅋㅋㅋ 근데 팀플이면 눈치보여서 먼저 해요",
          "speaker_id": "2",
          "time": "20210822 18:28"
        },
        {
          "id": "MDRW2100008507.1.7",
          "form": "감시자가 필요한 스타일..",
          "original_form": "감시자가 필요한 스타일..",
          "speaker_id": "2",
          "time": "20210822 18:28"
        },
        {
          "id": "MDRW2100008507.1.8",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210822 18:28"
        },
        {
          "id": "MDRW2100008507.1.9",
          "form": "누가 있어야 성실해져요ㅋㅋㅋ",
          "original_form": "누가 있어야 성실해져요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:28"
        },
        {
          "id": "MDRW2100008507.1.10",
          "form": "스스로는 힘들어ㅜ",
          "original_form": "스스로는 힘들어ㅜ",
          "speaker_id": "1",
          "time": "20210822 18:28"
        },
        {
          "id": "MDRW2100008507.1.11",
          "form": "도서관 다닐때 중도처럼 탁 트인데서 공부해야 됐어요..",
          "original_form": "도서관 다닐때 중도처럼 탁 트인데서 공부해야 됐어요..",
          "speaker_id": "2",
          "time": "20210822 18:29"
        },
        {
          "id": "MDRW2100008507.1.12",
          "form": "고등학교 선생님이 관리해주던 버릇이 안빠지나봐요",
          "original_form": "고등학교 선생님이 관리해주던 버릇이 안빠지나봐요",
          "speaker_id": "1",
          "time": "20210822 18:29"
        },
        {
          "id": "MDRW2100008507.1.13",
          "form": "누가 봐야 하는 ㅋㅋㅋㅋ",
          "original_form": "누가 봐야 하는 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:29"
        },
        {
          "id": "MDRW2100008507.1.14",
          "form": "저는 도서관가면 자는데ㅋㅋ ㅋ",
          "original_form": "저는 도서관가면 자는데ㅋㅋ ㅋ",
          "speaker_id": "1",
          "time": "20210822 18:30"
        },
        {
          "id": "MDRW2100008507.1.15",
          "form": "누가봐도 잠..",
          "original_form": "누가봐도 잠..",
          "speaker_id": "1",
          "time": "20210822 18:30"
        },
        {
          "id": "MDRW2100008507.1.16",
          "form": "책보면 잠와요ㅜㅜ도파민중독인가봐요",
          "original_form": "책보면 잠와요ㅜㅜ도파민중독인가봐요",
          "speaker_id": "1",
          "time": "20210822 18:30"
        },
        {
          "id": "MDRW2100008507.1.17",
          "form": "공부도 적당히 소음 있어야 잘 되더라고요 ㅋㅋㅋ",
          "original_form": "공부도 적당히 소음 있어야 잘 되더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008507.1.18",
          "form": "시험공부도 미뤄서 하셨나요? ㅋㅋㅋ",
          "original_form": "시험공부도 미뤄서 하셨나요? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008507.1.19",
          "form": "저도 백색소음 원해요ㅋㅋ",
          "original_form": "저도 백색소음 원해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:33"
        },
        {
          "id": "MDRW2100008507.1.20",
          "form": "셤공부 당일 하는거죠",
          "original_form": "셤공부 당일 하는거죠",
          "speaker_id": "1",
          "time": "20210822 18:34"
        },
        {
          "id": "MDRW2100008507.1.21",
          "form": "이러고 망함",
          "original_form": "이러고 망함",
          "speaker_id": "1",
          "time": "20210822 18:34"
        },
        {
          "id": "MDRW2100008507.1.22",
          "form": "저도 한꺼번에 하다가 새벽에 좀만 자야지 하고 자요 그리고 다음날 후회 ㅋㅋㅋ",
          "original_form": "저도 한꺼번에 하다가 새벽에 좀만 자야지 하고 자요 그리고 다음날 후회 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:57"
        },
        {
          "id": "MDRW2100008507.1.23",
          "form": "지금 저와같군요 ㅋㅋㅋㅜㅜㅜㅜㅜㅜㅜㅜㅜ",
          "original_form": "지금 저와같군요 ㅋㅋㅋㅜㅜㅜㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 18:58"
        },
        {
          "id": "MDRW2100008507.1.24",
          "form": "좀 현명하게 살고싶네요ㅋㅋㅋㅋ",
          "original_form": "좀 현명하게 살고싶네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:58"
        },
        {
          "id": "MDRW2100008507.1.25",
          "form": "미리미리 계획 세워서 공부하고 제출하고 이런 분들 너무 신기하고 존경스러워요 ㅎㅎ",
          "original_form": "미리미리 계획 세워서 공부하고 제출하고 이런 분들 너무 신기하고 존경스러워요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008507.1.26",
          "form": "저도 그렇습니다 성실한분  눈치보며",
          "original_form": "저도 그렇습니다 성실한분  눈치보며",
          "speaker_id": "1",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008507.1.27",
          "form": "일을 착착 해놓고싶네요",
          "original_form": "일을 착착 해놓고싶네요",
          "speaker_id": "1",
          "time": "20210822 19:22"
        }
      ]
    }
