      "utterance": [
        {
          "id": "MDRW2100007179.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007179.1.2",
          "form": "여행 좋아하는데\r\n코시국 이후에 가본적이 없네요~",
          "original_form": "여행 좋아하는데\r\n코시국 이후에 가본적이 없네요~",
          "speaker_id": "2",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100007179.1.3",
          "form": "name1이님은 가장 최근에\r\n여행 다녀오신게 언제였나요?",
          "original_form": "&name1&이님은 가장 최근에\r\n여행 다녀오신게 언제였나요?",
          "speaker_id": "2",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100007179.1.4",
          "form": "저요?국내여행은 얼마 안됐는데...  해외는 4년?",
          "original_form": "저요?국내여행은 얼마 안됐는데...  해외는 4년?",
          "speaker_id": "1",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100007179.1.5",
          "form": "국내여행은 어디 다녀오셨어요?",
          "original_form": "국내여행은 어디 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210822 11:20"
        },
        {
          "id": "MDRW2100007179.1.6",
          "form": "저는 국내도 19년 가을이 마지막이었네요",
          "original_form": "저는 국내도 19년 가을이 마지막이었네요",
          "speaker_id": "2",
          "time": "20210822 11:21"
        },
        {
          "id": "MDRW2100007179.1.7",
          "form": "국내는 최근에도 갔어요 ㅋㅋ 제주도",
          "original_form": "국내는 최근에도 갔어요 ㅋㅋ 제주도",
          "speaker_id": "1",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007179.1.8",
          "form": "오호 제주도!!!!  \r\n제주도는 여전한가요? ㅋㅋ",
          "original_form": "오호 제주도!!!!  \r\n제주도는 여전한가요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100007179.1.9",
          "form": "네 사람많고 다 그대로 ㅋㅋㅋ",
          "original_form": "네 사람많고 다 그대로 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100007179.1.10",
          "form": "사실 저는 고등학교때 간 게\r\n마지막이라서 너무 궁금해요ㅋㅋ",
          "original_form": "사실 저는 고등학교때 간 게\r\n마지막이라서 너무 궁금해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007179.1.11",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "1",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007179.1.12",
          "form": "고등학교때가 마지막이라구요?",
          "original_form": "고등학교때가 마지막이라구요?",
          "speaker_id": "1",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007179.1.13",
          "form": "네ㅋㅋ 제주도는\r\n그때가 마지막이네요",
          "original_form": "네ㅋㅋ 제주도는\r\n그때가 마지막이네요",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007179.1.14",
          "form": "전 고딩때 못가고 성인되서 3번갔어요",
          "original_form": "전 고딩때 못가고 성인되서 3번갔어요",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007179.1.15",
          "form": "신행은 어디 갔다오셨어요?",
          "original_form": "신행은 어디 갔다오셨어요?",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007179.1.16",
          "form": "저는 성인되고는 가본적이 없네용",
          "original_form": "저는 성인되고는 가본적이 없네용",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007179.1.17",
          "form": "신행은 하와이로 다녀왔었어요:)",
          "original_form": "신행은 하와이로 다녀왔었어요:)",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007179.1.18",
          "form": "하와이 좋죠?",
          "original_form": "하와이 좋죠?",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007179.1.19",
          "form": "가고싶네요",
          "original_form": "가고싶네요",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007179.1.20",
          "form": "네! 너무 좋았어요ㅋㅋ",
          "original_form": "네! 너무 좋았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007179.1.21",
          "form": "(그때의 내가 좋았던건가?ㅋㅋㅋ)",
          "original_form": "(그때의 내가 좋았던건가?ㅋㅋㅋ)",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007179.1.22",
          "form": "해외여행도 그게 마지막?",
          "original_form": "해외여행도 그게 마지막?",
          "speaker_id": "1",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100007179.1.23",
          "form": "아니요 ㅋㅋ 괌이 마지막이요",
          "original_form": "아니요 ㅋㅋ 괌이 마지막이요",
          "speaker_id": "2",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100007179.1.24",
          "form": "휴양지로 다니셨군요",
          "original_form": "휴양지로 다니셨군요",
          "speaker_id": "1",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100007179.1.25",
          "form": "코로나무서워서 신행도 해외로 못가니...ㅜㅜ",
