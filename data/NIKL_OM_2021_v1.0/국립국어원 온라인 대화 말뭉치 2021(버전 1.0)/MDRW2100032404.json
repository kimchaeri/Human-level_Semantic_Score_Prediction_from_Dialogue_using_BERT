      "utterance": [
        {
          "id": "MDRW2100032404.1.1",
          "form": "맛있겠다 밥도둑",
          "original_form": "맛있겠다 밥도둑",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100032404.1.2",
          "form": "간장게장이 좋아 양념게장이 좋아?",
          "original_form": "간장게장이 좋아 양념게장이 좋아?",
          "speaker_id": "1",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100032404.1.3",
          "form": "나는 양념게장",
          "original_form": "나는 양념게장",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100032404.1.4",
          "form": "간장게장은 별로야",
          "original_form": "간장게장은 별로야",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100032404.1.5",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100032404.1.6",
          "form": "간장게장 껍데기에 밥 비벼먹으면 맛있는데",
          "original_form": "간장게장 껍데기에 밥 비벼먹으면 맛있는데",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032404.1.7",
          "form": "아 그건 좋아 ㅋㅋㅋ 참기름 쪼록",
          "original_form": "아 그건 좋아 ㅋㅋㅋ 참기름 쪼록",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032404.1.8",
          "form": "양념 게장 맛있지",
          "original_form": "양념 게장 맛있지",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032404.1.9",
          "form": "양념에 밥 쓱싹 비벼서",
          "original_form": "양념에 밥 쓱싹 비벼서",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032404.1.10",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032404.1.11",
          "form": "김에 밥 싸서 먹으면",
          "original_form": "김에 밥 싸서 먹으면",
          "speaker_id": "1",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100032404.1.12",
          "form": "전복장은 맛있어?",
          "original_form": "전복장은 맛있어?",
          "speaker_id": "2",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100032404.1.13",
          "form": "나도 아직 안먹어봐서",
          "original_form": "나도 아직 안먹어봐서",
          "speaker_id": "1",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100032404.1.14",
          "form": "전복도 장으로 만들구나",
          "original_form": "전복도 장으로 만들구나",
          "speaker_id": "2",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100032404.1.15",
          "form": "쫀득한 전복맛이지 않을까 ㅋㅋ",
          "original_form": "쫀득한 전복맛이지 않을까 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100032404.1.16",
          "form": "쫀득 달콤 짭짤",
          "original_form": "쫀득 달콤 짭짤",
          "speaker_id": "2",
          "time": "20210905 17:04"
        }
