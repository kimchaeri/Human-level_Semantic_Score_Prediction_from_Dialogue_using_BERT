      "utterance": [
        {
          "id": "MDRW2100006231.1.1",
          "form": "춘천여행",
          "original_form": "춘천여행",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100006231.1.2",
          "form": "사실 몇 년 돼서 잘 기억은 안 나고",
          "original_form": "사실 몇 년 돼서 잘 기억은 안 나고",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006231.1.3",
          "form": "닭갈비 먹은 기억만 나네요 ㅋㅋㅋ",
          "original_form": "닭갈비 먹은 기억만 나네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006231.1.4",
          "form": "춘천가서 드신건가요?",
          "original_form": "춘천가서 드신건가요?",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006231.1.5",
          "form": "몇 년 전에 춘천 여행 가서 먹었어요",
          "original_form": "몇 년 전에 춘천 여행 가서 먹었어요",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006231.1.6",
          "form": "여행가신 곳 중에 가장 기억에 남는 곳 있으신가요?",
          "original_form": "여행가신 곳 중에 가장 기억에 남는 곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100006231.1.7",
          "form": "아일랜드요!",
          "original_form": "아일랜드요!",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006231.1.8",
          "form": "좀 독특한 곳이긴 한데",
          "original_form": "좀 독특한 곳이긴 한데",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006231.1.9",
          "form": "자연환경도 좋고 사람들도 친절하고 좋았어요",
          "original_form": "자연환경도 좋고 사람들도 친절하고 좋았어요",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100006231.1.10",
          "form": "인종차별 같은 건 전혀 없었나요?",
          "original_form": "인종차별 같은 건 전혀 없었나요?",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006231.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006231.1.12",
          "form": "관광지로 거의 안 알려진 곳이라",
          "original_form": "관광지로 거의 안 알려진 곳이라",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006231.1.13",
          "form": "시골마을 그런 느낌이었어요",
          "original_form": "시골마을 그런 느낌이었어요",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006231.1.14",
          "form": "마을에 사람도 거의 없고 ㅋㅋㅋ",
          "original_form": "마을에 사람도 거의 없고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006231.1.15",
          "form": "그럼 기념품도 사셨나요?",
          "original_form": "그럼 기념품도 사셨나요?",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100006231.1.16",
          "form": "자석 같은거 몇 개 산 거 같아요",
          "original_form": "자석 같은거 몇 개 산 거 같아요",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100006231.1.17",
          "form": "돈이 안아까울만큼 기념할만 한가요?",
          "original_form": "돈이 안아까울만큼 기념할만 한가요?",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100006231.1.18",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006231.1.19",
          "form": "주로 풍경모양 자석인가요?",
          "original_form": "주로 풍경모양 자석인가요?",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006231.1.20",
          "form": "스켈리그 마이클이라고",
          "original_form": "스켈리그 마이클이라고",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006231.1.21",
          "form": "바위섬에 가서",
          "original_form": "바위섬에 가서",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006231.1.22",
          "form": "그 섬 모양이랑 섬에 사는 새 모양 자석 샀어요",
          "original_form": "그 섬 모양이랑 섬에 사는 새 모양 자석 샀어요",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006231.1.23",
          "form": "오 귀여울 것 같아요",
          "original_form": "오 귀여울 것 같아요",
          "speaker_id": "1",
          "time": "20210907 22:20"
        }
      ]
