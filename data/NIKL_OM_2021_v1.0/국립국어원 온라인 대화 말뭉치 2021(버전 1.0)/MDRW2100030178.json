      "utterance": [
        {
          "id": "MDRW2100030178.1.1",
          "form": "아 몰랐던걸 여기서 알아갑니다",
          "original_form": "아 몰랐던걸 여기서 알아갑니다",
          "speaker_id": "1",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030178.1.2",
          "form": "스벅 안되나봐요",
          "original_form": "스벅 안되나봐요",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030178.1.3",
          "form": "이케아도",
          "original_form": "이케아도",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030178.1.4",
          "form": "세계적인 기업이니까",
          "original_form": "세계적인 기업이니까",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030178.1.5",
          "form": "취지를 생각하면 당연하네요",
          "original_form": "취지를 생각하면 당연하네요",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030178.1.6",
          "form": "대형마트가 안되는거랑 같은거같아요",
          "original_form": "대형마트가 안되는거랑 같은거같아요",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030178.1.7",
          "form": "그렇죠 어디어디안되는지도 알아야할거같네요",
          "original_form": "그렇죠 어디어디안되는지도 알아야할거같네요",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030178.1.8",
          "form": "저번엔 서울 스벅은 됐다고하던데",
          "original_form": "저번엔 서울 스벅은 됐다고하던데",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030178.1.9",
          "form": "아마 동백전사용가능한매장은되는거같아요",
          "original_form": "아마 동백전사용가능한매장은되는거같아요",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030178.1.10",
          "form": "이번엔 안되면 좋은거같아요",
          "original_form": "이번엔 안되면 좋은거같아요",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030178.1.11",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030178.1.12",
          "form": "이번엔 그냥 외식과 시장에 쓸려구요",
          "original_form": "이번엔 그냥 외식과 시장에 쓸려구요",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030178.1.13",
          "form": "시장은 카드안되는곳도 임ㅅ잖아요",
          "original_form": "시장은 카드안되는곳도 임ㅅ잖아요",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030178.1.14",
          "form": "온누리로 다 받나요?",
          "original_form": "온누리로 다 받나요?",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030178.1.15",
          "form": "아.................",
          "original_form": "아.................",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030178.1.16",
          "form": "선불카드도 카드로치겠죠?",
          "original_form": "선불카드도 카드로치겠죠?",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030178.1.17",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030178.1.18",
          "form": "으헝헝",
          "original_form": "으헝헝",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030178.1.19",
          "form": "카드기계가 있어야하니까요",
          "original_form": "카드기계가 있어야하니까요",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030178.1.20",
          "form": "그래도 도전해봅니다",
          "original_form": "그래도 도전해봅니다",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030178.1.21",
          "form": "아닛 왜 ㅠㅠ카드 ㅠㅠㅠ아니......장사하기싫니 ㅠㅠㅠㅠㅠ 헝헝",
          "original_form": "아닛 왜 ㅠㅠ카드 ㅠㅠㅠ아니......장사하기싫니 ㅠㅠㅠㅠㅠ 헝헝",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030178.1.22",
          "form": "뭐가사고싶었나요",
          "original_form": "뭐가사고싶었나요",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030178.1.23",
          "form": "무...",
          "original_form": "무...",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030178.1.24",
          "form": "무우..?",
          "original_form": "무우..?",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030178.1.25",
          "form": "정말 무",
          "original_form": "정말 무",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030178.1.26",
          "form": "깍두기 담굴려구요",
          "original_form": "깍두기 담굴려구요",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030178.1.27",
          "form": "10월에",
          "original_form": "10월에",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030178.1.28",
          "form": "햇무로",
          "original_form": "햇무로",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030178.1.29",
          "form": "아하 무거운데 어떻게 들고와요?",
          "original_form": "아하 무거운데 어떻게 들고와요?",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030178.1.30",
          "form": "차 끌고가여죠",
          "original_form": "차 끌고가여죠",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030178.1.31",
          "form": "못들고와요",
          "original_form": "못들고와요",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030178.1.32",
          "form": "구루마도",
          "original_form": "구루마도",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030178.1.33",
          "form": "끌어야대요",
          "original_form": "끌어야대요",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030178.1.34",
          "form": "근데 다른 업체도 국민지원금 제외되었어야하지않을까요",
          "original_form": "근데 다른 업체도 국민지원금 제외되었어야하지않을까요",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030178.1.35",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030178.1.36",
          "form": "어디가있지..",
          "original_form": "어디가있지..",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030178.1.37",
          "form": "이케아는 자주가세요?",
          "original_form": "이케아는 자주가세요?",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030178.1.38",
          "form": "한번 가보고싶긴한데",
          "original_form": "한번 가보고싶긴한데",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030178.1.39",
          "form": "너무 멀어서",
          "original_form": "너무 멀어서",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030178.1.40",
          "form": "생겨도 안가봤네요",
          "original_form": "생겨도 안가봤네요",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030178.1.41",
          "form": "가본친구말론",
          "original_form": "가본친구말론",
          "speaker_id": "1",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030178.1.42",
          "form": "정말 넓다던데",
          "original_form": "정말 넓다던데",
          "speaker_id": "1",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030178.1.43",
          "form": "길도 잃는대요",
          "original_form": "길도 잃는대요",
          "speaker_id": "1",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030178.1.44",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "2",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100030178.1.45",
          "form": "스타벅스는 자주가나요?",
          "original_form": "스타벅스는 자주가나요?",
          "speaker_id": "2",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100030178.1.46",
          "form": "네 ㅎㅎ 맛나서 자주 가용",
          "original_form": "네 ㅎㅎ 맛나서 자주 가용",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100030178.1.47",
          "form": "name2님은 닉넴만봐도",
          "original_form": "&name2&님은 닉넴만봐도",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100030178.1.48",
          "form": "자주갈거같아요!",
          "original_form": "자주갈거같아요!",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100030178.1.49",
          "form": "피지오잖아요~~",
          "original_form": "피지오잖아요~~",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100030178.1.50",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100030178.1.51",
          "form": "그렇지만 국민지원금으로 스타벅스가 제외된건 이해가 되요",
          "original_form": "그렇지만 국민지원금으로 스타벅스가 제외된건 이해가 되요",
          "speaker_id": "2",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100030178.1.52",
          "form": "맞아요 이해되용!",
          "original_form": "맞아요 이해되용!",
          "speaker_id": "1",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100030178.1.53",
          "form": "이시국에도 스타벅스는 장사가 잘되니까요",
          "original_form": "이시국에도 스타벅스는 장사가 잘되니까요",
          "speaker_id": "2",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100030178.1.54",
          "form": "완전 찬성합니다",
