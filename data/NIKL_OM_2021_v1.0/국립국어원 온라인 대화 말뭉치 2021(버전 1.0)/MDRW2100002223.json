      "utterance": [
        {
          "id": "MDRW2100002223.1.1",
          "form": "넷플에서 제일 재미있게 본거 뭐야",
          "original_form": "넷플에서 제일 재미있게 본거 뭐야",
          "speaker_id": "1",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002223.1.2",
          "form": "나는 아무래도 굿플레이스지.",
          "original_form": "나는 아무래도 굿플레이스지.",
          "speaker_id": "2",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002223.1.3",
          "form": "너는 루시퍼였나?",
          "original_form": "너는 루시퍼였나?",
          "speaker_id": "2",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002223.1.4",
          "form": "아니면 브나나?",
          "original_form": "아니면 브나나?",
          "speaker_id": "2",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002223.1.5",
          "form": "아니면 모팸?",
          "original_form": "아니면 모팸?",
          "speaker_id": "2",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002223.1.6",
          "form": "음 전부 막상막하야",
          "original_form": "음 전부 막상막하야",
          "speaker_id": "1",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002223.1.7",
          "form": "나는 굿플레이스를 정말 재미있게 봐서 관련 엠디를 사모으고 싶었어.",
          "original_form": "나는 굿플레이스를 정말 재미있게 봐서 관련 엠디를 사모으고 싶었어.",
          "speaker_id": "2",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002223.1.8",
          "form": "근데 밥친구로 보기에는 브나나인거같아",
          "original_form": "근데 밥친구로 보기에는 브나나인거같아",
          "speaker_id": "1",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002223.1.9",
          "form": "밥친구로 정말 딱이야.",
          "original_form": "밥친구로 정말 딱이야.",
          "speaker_id": "2",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002223.1.10",
          "form": "티셔츠같은거잇나?",
          "original_form": "티셔츠같은거잇나?",
          "speaker_id": "1",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002223.1.11",
          "form": "주인공들이 정말 재치넘쳐.",
          "original_form": "주인공들이 정말 재치넘쳐.",
          "speaker_id": "2",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002223.1.12",
          "form": "나는 프렌즈도 좋아해.",
          "original_form": "나는 프렌즈도 좋아해.",
          "speaker_id": "2",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002223.1.13",
          "form": "외국인들 티셔츠 굿즈 잘만들던데",
          "original_form": "외국인들 티셔츠 굿즈 잘만들던데",
          "speaker_id": "1",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002223.1.14",
          "form": "공식 엠디는 거의 없는 거 같더라.",
          "original_form": "공식 엠디는 거의 없는 거 같더라.",
          "speaker_id": "2",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002223.1.15",
          "form": "그리고 나는 구린 티셔츠는 직구할 생각이 없어,",
          "original_form": "그리고 나는 구린 티셔츠는 직구할 생각이 없어,",
          "speaker_id": "2",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002223.1.16",
          "form": "프렌즈는 이번에 스파오랑 콜라보했잔항",
          "original_form": "프렌즈는 이번에 스파오랑 콜라보했잔항",
          "speaker_id": "1",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002223.1.17",
          "form": "잖아",
          "original_form": "잖아",
          "speaker_id": "1",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002223.1.18",
          "form": "집에 가정용티셔츠는 많아.",
          "original_form": "집에 가정용티셔츠는 많아.",
          "speaker_id": "2",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002223.1.19",
          "form": "그거 실물로 봤는데 너무 크더라",
          "original_form": "그거 실물로 봤는데 너무 크더라",
          "speaker_id": "2",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002223.1.20",
          "form": "잠옷행이었어.",
          "original_form": "잠옷행이었어.",
          "speaker_id": "2",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002223.1.21",
          "form": "건조기에 5번 돌리면 작아질듯",
          "original_form": "건조기에 5번 돌리면 작아질듯",
          "speaker_id": "1",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002223.1.22",
          "form": "근데 또 그 때는 프렌즈에 잠시 식었을 때라 그저그래써",
          "original_form": "근데 또 그 때는 프렌즈에 잠시 식었을 때라 그저그래써",
          "speaker_id": "2",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002223.1.23",
          "form": "프렌즈는 끝까지봤니?",
          "original_form": "프렌즈는 끝까지봤니?",
          "speaker_id": "1",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002223.1.24",
          "form": "스파오 옷을 그렇게 모욕하는거니.",
          "original_form": "스파오 옷을 그렇게 모욕하는거니.",
          "speaker_id": "2",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002223.1.25",
          "form": "프렌즈 시즌이 너무 많아.",
          "original_form": "프렌즈 시즌이 너무 많아.",
          "speaker_id": "2",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002223.1.26",
          "form": "사실이니까",
          "original_form": "사실이니까",
          "speaker_id": "1",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002223.1.27",
          "form": "하지만 엄청 많ㅇ ㅣ봤어.",
          "original_form": "하지만 엄청 많ㅇ ㅣ봤어.",
          "speaker_id": "2",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002223.1.28",
          "form": "브루클린나인나인은 미국에서",
          "original_form": "브루클린나인나인은 미국에서",
          "speaker_id": "1",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002223.1.29",
          "form": "마지막시즌 방영하더라",
          "original_form": "마지막시즌 방영하더라",
          "speaker_id": "1",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002223.1.30",
          "form": "굿플레이스같은 드라마가  또 나온다면 나는 또 빠질거야.",
          "original_form": "굿플레이스같은 드라마가  또 나온다면 나는 또 빠질거야.",
          "speaker_id": "2",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002223.1.31",
          "form": "아쉽구나.",
          "original_form": "아쉽구나.",
          "speaker_id": "2",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002223.1.32",
          "form": "넷플은 언제 시즌6을 보여줄까",
          "original_form": "넷플은 언제 시즌6을 보여줄까",
          "speaker_id": "1",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002223.1.33",
          "form": "ㅂ나나는 정말 명작이야.",
          "original_form": "ㅂ나나는 정말 명작이야.",
          "speaker_id": "2",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002223.1.34",
          "form": "셜록도 한 때 많이 봤어.",
          "original_form": "셜록도 한 때 많이 봤어.",
          "speaker_id": "2",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002223.1.35",
          "form": "굿플레이스 남주가 조금만 더 잘생겼더라면...",
          "original_form": "굿플레이스 남주가 조금만 더 잘생겼더라면...",
          "speaker_id": "1",
          "time": "20210820 00:50"
        }
      ]
