      "utterance": [
        {
          "id": "MDRW2100015468.1.1",
          "form": "e이신가봐요",
          "original_form": "e이신가봐요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015468.1.2",
          "form": "엠비티아이 I형이",
          "original_form": "엠비티아이 I형이",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015468.1.3",
          "form": "아뇨 저 I입니다",
          "original_form": "아뇨 저 I입니다",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015468.1.4",
          "form": "I형이 따로 먼저 만나자한다? 이건 백프로다.",
          "original_form": "I형이 따로 먼저 만나자한다? 이건 백프로다.",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015468.1.5",
          "form": "뭐 도와달람서 만나자 한다?????",
          "original_form": "뭐 도와달람서 만나자 한다?????",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015468.1.6",
          "form": "이건 ..!!",
          "original_form": "이건 ..!!",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015468.1.7",
          "form": "그런거군요",
          "original_form": "그런거군요",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015468.1.8",
          "form": "ㅇㄴ 선다방 보시나요",
          "original_form": "ㅇㄴ 선다방 보시나요",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015468.1.9",
          "form": "아니요 그게뭐죠",
          "original_form": "아니요 그게뭐죠",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015468.1.10",
          "form": "선다방이라고 소개팅하는 프로그램",
          "original_form": "선다방이라고 소개팅하는 프로그램",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015468.1.11",
          "form": "자주보시나봐요",
          "original_form": "자주보시나봐요",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015468.1.12",
          "form": "조곤조곤한 남녀 둘이 나와서 소개팅하는 프로그램이요",
          "original_form": "조곤조곤한 남녀 둘이 나와서 소개팅하는 프로그램이요",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015468.1.13",
          "form": "아뇨 그거 알고리즘으로 떳다가",
          "original_form": "아뇨 그거 알고리즘으로 떳다가",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015468.1.14",
          "form": "지금 알고리즘에 연애만 뜨네여..?",
          "original_form": "지금 알고리즘에 연애만 뜨네여..?",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015468.1.15",
          "form": "어때요 재밌던가요",
          "original_form": "어때요 재밌던가요",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015468.1.16",
          "form": "잼슴니다",
          "original_form": "잼슴니다",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015468.1.17",
          "form": "근데 연애하고싶어진다기보다는",
          "original_form": "근데 연애하고싶어진다기보다는",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015468.1.18",
          "form": "어떤점이",
          "original_form": "어떤점이",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015468.1.19",
          "form": "드라마보는것같아요",
          "original_form": "드라마보는것같아요",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015468.1.20",
          "form": "아ㅋㅋㅋㄱㅋ",
          "original_form": "아ㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015468.1.21",
          "form": "재밌겠어요",
          "original_form": "재밌겠어요",
          "speaker_id": "2",
          "time": "20210825 23:13"
        }
      ]
