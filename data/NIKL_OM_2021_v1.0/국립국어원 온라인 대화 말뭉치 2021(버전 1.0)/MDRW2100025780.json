        },
        {
          "id": "MDRW2100025780.1.2",
          "form": "안녕하세용!!",
          "original_form": "안녕하세용!!",
          "speaker_id": "2",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100025780.1.3",
          "form": "저녁메뉴 뭐에요?",
          "original_form": "저녁메뉴 뭐에요?",
          "speaker_id": "2",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100025780.1.4",
          "form": "저 아직 안정했어요",
          "original_form": "저 아직 안정했어요",
          "speaker_id": "1",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100025780.1.5",
          "form": "뭐 드실건가여?",
          "original_form": "뭐 드실건가여?",
          "speaker_id": "1",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100025780.1.6",
          "form": "저녁은 가볍게 두부랑 달걀 구워서 먹을까싶어여 점심에 피자를먹어서ㅜㅜ",
          "original_form": "저녁은 가볍게 두부랑 달걀 구워서 먹을까싶어여 점심에 피자를먹어서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025780.1.7",
          "form": "오 좋네여",
          "original_form": "오 좋네여",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025780.1.8",
          "form": "점심에",
          "original_form": "점심에",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025780.1.9",
          "form": "무슨 피자 드셨나요",
          "original_form": "무슨 피자 드셨나요",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025780.1.10",
          "form": "피자 알볼로 이벤트하길래 시켜먹었어요ㅎㅎㅎ",
          "original_form": "피자 알볼로 이벤트하길래 시켜먹었어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025780.1.11",
          "form": "오 거기 맛있어요?",
          "original_form": "오 거기 맛있어요?",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100025780.1.12",
          "form": "페이코 이벤트인가 그건가요?",
          "original_form": "페이코 이벤트인가 그건가요?",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100025780.1.13",
          "form": "네!! 80퍼 할인해주더라구여",
          "original_form": "네!! 80퍼 할인해주더라구여",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025780.1.14",
          "form": "뭔가..굉장히 건강한 맛이에요",
          "original_form": "뭔가..굉장히 건강한 맛이에요",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025780.1.15",
          "form": "성공하셨군요 !!!!!",
          "original_form": "성공하셨군요 !!!!!",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025780.1.16",
          "form": "맛은 그냥그렁가여ㅋㅋㅋㅋㅋ?",
          "original_form": "맛은 그냥그렁가여ㅋㅋㅋㅋㅋ?",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025780.1.17",
          "form": "시키기 힘들긴했어요ㅋㅋㅋ저는 자극적인 맛을 조아하는데..\r\n너무 건강해서..ㅠㅠ",
          "original_form": "시키기 힘들긴했어요ㅋㅋㅋ저는 자극적인 맛을 조아하는데..\r\n너무 건강해서..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025780.1.18",
          "form": "아 그래도 80퍼로 먹었으니까요!!",
          "original_form": "아 그래도 80퍼로 먹었으니까요!!",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025780.1.19",
          "form": "맞아요 오천원내고 먹었으니 이득입니닷!!",
          "original_form": "맞아요 오천원내고 먹었으니 이득입니닷!!",
          "speaker_id": "2",
          "time": "20210901 20:13"
        },
        {
          "id": "MDRW2100025780.1.20",
          "form": "진짜 저렴하네요ㅋㅋㅋㅋㅋㅋ",
