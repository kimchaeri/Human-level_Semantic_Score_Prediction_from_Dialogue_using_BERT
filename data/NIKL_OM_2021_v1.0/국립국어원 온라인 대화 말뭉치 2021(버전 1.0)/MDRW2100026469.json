      "utterance": [
        {
          "id": "MDRW2100026469.1.1",
          "form": "볶은양파.. 좋습니다 추천",
          "original_form": "볶은양파.. 좋습니다 추천",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026469.1.2",
          "form": "읭 샌드위치에 볶은양파요??",
          "original_form": "읭 샌드위치에 볶은양파요??",
          "speaker_id": "1",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026469.1.3",
          "form": "다른거랑 같이넣으면 괜찮을거같긴하네용",
          "original_form": "다른거랑 같이넣으면 괜찮을거같긴하네용",
          "speaker_id": "1",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026469.1.4",
          "form": "제입엔 좋았는데 괜찮을지ㅋㅋㅋ",
          "original_form": "제입엔 좋았는데 괜찮을지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026469.1.5",
          "form": "아 혹시 동아리 하셨어요?",
          "original_form": "아 혹시 동아리 하셨어요?",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026469.1.6",
          "form": "저는 대학때는 농구동아리... 1일..",
          "original_form": "저는 대학때는 농구동아리... 1일..",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026469.1.7",
          "form": "ㅎㅎㅎ먹어게요",
          "original_form": "ㅎㅎㅎ먹어게요",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026469.1.8",
          "form": "저도 동아리는..그냥 기웃기웃대다 말았어요ㅋㅋ",
          "original_form": "저도 동아리는..그냥 기웃기웃대다 말았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026469.1.9",
          "form": "음악동아리 베이스만 만져보고...",
          "original_form": "음악동아리 베이스만 만져보고...",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026469.1.10",
          "form": "와 음악동아리",
          "original_form": "와 음악동아리",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026469.1.11",
          "form": "요새 슈퍼밴드 보는데 베이스 멋져요",
          "original_form": "요새 슈퍼밴드 보는데 베이스 멋져요",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026469.1.12",
          "form": "정말 만져보고 두들기다만 나왔어요ㅋㅋㅋ",
          "original_form": "정말 만져보고 두들기다만 나왔어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026469.1.13",
          "form": "그래도 시작이 반이니까요!",
          "original_form": "그래도 시작이 반이니까요!",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026469.1.14",
          "form": "그때이후로 베이스 구경한지도 오래됐네요ㅎ",
          "original_form": "그때이후로 베이스 구경한지도 오래됐네요ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026469.1.15",
          "form": "수강신청할땐 잘하셨나요",
          "original_form": "수강신청할땐 잘하셨나요",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026469.1.16",
          "form": "전 나름 티켓팅 종류는 잘하는 편이라ㅋㅋ",
          "original_form": "전 나름 티켓팅 종류는 잘하는 편이라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026469.1.17",
          "form": "항상 괜찮았어요",
          "original_form": "항상 괜찮았어요",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026469.1.18",
          "form": "오우ㅋㅋㅋ",
          "original_form": "오우ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026469.1.19",
          "form": "하긴 콘서트같은게 피터지죠",
          "original_form": "하긴 콘서트같은게 피터지죠",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026469.1.20",
          "form": "근데 수강신청 잘해도ㅋㅋ 강의가 문제라..",
          "original_form": "근데 수강신청 잘해도ㅋㅋ 강의가 문제라..",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026469.1.21",
          "form": "좋대서 신청했는데 망한 경우도 많았던 거 같아요",
          "original_form": "좋대서 신청했는데 망한 경우도 많았던 거 같아요",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026469.1.22",
          "form": "ㅋㅋㅋ그래서 강의평가도 꼭 봐줘야해요",
          "original_form": "ㅋㅋㅋ그래서 강의평가도 꼭 봐줘야해요",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026469.1.23",
          "form": "진짜 수업이 이게 뭐냐 적고싶지만",
          "original_form": "진짜 수업이 이게 뭐냐 적고싶지만",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026469.1.24",
          "form": "강의평가에는 좋았습니다...",
          "original_form": "강의평가에는 좋았습니다...",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026469.1.25",
          "form": "나만당할수없다인가요ㅋㅋㅋㅋ",
          "original_form": "나만당할수없다인가요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:16"
        }
      ]
