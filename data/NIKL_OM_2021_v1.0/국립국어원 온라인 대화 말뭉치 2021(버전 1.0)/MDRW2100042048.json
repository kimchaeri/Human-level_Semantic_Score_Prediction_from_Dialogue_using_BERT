      "utterance": [
        {
          "id": "MDRW2100042048.1.1",
          "form": "식사는 하셨어요??",
          "original_form": "식사는 하셨어요??",
          "speaker_id": "1",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100042048.1.2",
          "form": "넹넹 방금 연어덮밥 먹고왓어요 ㅋㅋㅋ",
          "original_form": "넹넹 방금 연어덮밥 먹고왓어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100042048.1.3",
          "form": "맛난거 드셨네요!!!",
          "original_form": "맛난거 드셨네요!!!",
          "speaker_id": "1",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100042048.1.4",
          "form": "넹 백신 맞고는 좀 든든하게 영양섭취중이에여",
          "original_form": "넹 백신 맞고는 좀 든든하게 영양섭취중이에여",
          "speaker_id": "2",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100042048.1.5",
          "form": "ㅋㅋ 부작용이 식욕폭발이라고ㅋㅋ",
          "original_form": "ㅋㅋ 부작용이 식욕폭발이라고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100042048.1.6",
          "form": "넼ㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100042048.1.7",
          "form": "전 부침개 먹습니다",
          "original_form": "전 부침개 먹습니다",
          "speaker_id": "1",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100042048.1.8",
          "form": "영양보충이라는 핑계의 부작용",
          "original_form": "영양보충이라는 핑계의 부작용",
          "speaker_id": "2",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100042048.1.9",
          "form": "오 부침개 맛잇겟네용",
          "original_form": "오 부침개 맛잇겟네용",
          "speaker_id": "2",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100042048.1.10",
          "form": "파전인가옷",
          "original_form": "파전인가옷",
          "speaker_id": "2",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100042048.1.11",
          "form": "부추전이요! ㅋㅋ",
          "original_form": "부추전이요! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100042048.1.12",
          "form": "간식은 따로 안드세요?",
          "original_form": "간식은 따로 안드세요?",
          "speaker_id": "1",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100042048.1.13",
          "form": "넼ㅋㅋ이제 배불러요",
          "original_form": "넼ㅋㅋ이제 배불러요",
          "speaker_id": "2",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100042048.1.14",
          "form": "사실 빙수먹고 저녁먹어서",
          "original_form": "사실 빙수먹고 저녁먹어서",
          "speaker_id": "2",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100042048.1.15",
          "form": "ㅋㅋ원래 간식을 먼저 먹어야한대요ㅋㅋ",
          "original_form": "ㅋㅋ원래 간식을 먼저 먹어야한대요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:17"
        },
        {
          "id": "MDRW2100042048.1.16",
          "form": "ㅋㅋㅋㅋㅋ그런가요",
          "original_form": "ㅋㅋㅋㅋㅋ그런가요",
