      "utterance": [
        {
          "id": "MDRW2100010183.1.1",
          "form": "안녕하세요\r\n티비 프로그램 챙겨보시는 거 있나요?",
          "original_form": "안녕하세요\r\n티비 프로그램 챙겨보시는 거 있나요?",
          "speaker_id": "1",
          "time": "20210823 15:18"
        },
        {
          "id": "MDRW2100010183.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210823 15:21"
        },
        {
          "id": "MDRW2100010183.1.3",
          "form": "저는 왓챠랑 유투브로 챙겨봐요",
          "original_form": "저는 왓챠랑 유투브로 챙겨봐요",
          "speaker_id": "2",
          "time": "20210823 15:21"
        },
        {
          "id": "MDRW2100010183.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210823 15:21"
        },
        {
          "id": "MDRW2100010183.1.5",
          "form": "아~  다시보기로 보시는거에요?",
          "original_form": "아~  다시보기로 보시는거에요?",
          "speaker_id": "1",
          "time": "20210823 15:21"
        },
        {
          "id": "MDRW2100010183.1.6",
          "form": "그냥 보이는거봐요",
          "original_form": "그냥 보이는거봐요",
          "speaker_id": "2",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010183.1.7",
          "form": "세계속으로같은 프로그램은 다 볼수있어요",
          "original_form": "세계속으로같은 프로그램은 다 볼수있어요",
          "speaker_id": "2",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010183.1.8",
          "form": "아~그러시구나~! 예능은 안 좋아하세요?",
          "original_form": "아~그러시구나~! 예능은 안 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010183.1.9",
          "form": "아 좋아하죠",
          "original_form": "아 좋아하죠",
          "speaker_id": "2",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100010183.1.10",
          "form": "무한도전",
          "original_form": "무한도전",
          "speaker_id": "2",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100010183.1.11",
          "form": "요즘 예능에 올림픽 영웅들이 자주 나오더라고요",
          "original_form": "요즘 예능에 올림픽 영웅들이 자주 나오더라고요",
          "speaker_id": "1",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100010183.1.12",
          "form": "맛있는녀석들 운동뚱",
          "original_form": "맛있는녀석들 운동뚱",
          "speaker_id": "2",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100010183.1.13",
          "form": "오 넘좋아요",
          "original_form": "오 넘좋아요",
          "speaker_id": "2",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100010183.1.14",
          "form": "아~ 맛있는 녀석들은 저도 자주 봐요",
          "original_form": "아~ 맛있는 녀석들은 저도 자주 봐요",
          "speaker_id": "1",
          "time": "20210823 15:24"
        },
        {
          "id": "MDRW2100010183.1.15",
          "form": "저대신 많이 먹어줘서 좋아요 ㅋ",
          "original_form": "저대신 많이 먹어줘서 좋아요 ㅋ",
          "speaker_id": "2",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010183.1.16",
          "form": "연경님 많이나오시면좋겠어요",
          "original_form": "연경님 많이나오시면좋겠어요",
          "speaker_id": "2",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010183.1.17",
          "form": "아~예능에서 아직 연경님은 안 나오신 것 같아요",
          "original_form": "아~예능에서 아직 연경님은 안 나오신 것 같아요",
          "speaker_id": "1",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010183.1.18",
          "form": "아는형님에 나왔던거같은데",
          "original_form": "아는형님에 나왔던거같은데",
          "speaker_id": "2",
          "time": "20210823 15:26"
        },
        {
          "id": "MDRW2100010183.1.19",
          "form": "아직 펜싱밖에 안 나온것 같아요",
          "original_form": "아직 펜싱밖에 안 나온것 같아요",
          "speaker_id": "1",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100010183.1.20",
          "form": "유투브채널이 있어서 구독했어요",
          "original_form": "유투브채널이 있어서 구독했어요",
          "speaker_id": "2",
          "time": "20210823 15:28"
        },
        {
          "id": "MDRW2100010183.1.21",
          "form": "아는형님 유투브요?",
          "original_form": "아는형님 유투브요?",
          "speaker_id": "1",
          "time": "20210823 15:29"
        },
        {
          "id": "MDRW2100010183.1.22",
          "form": "식빵언니 김연경 유투브요",
          "original_form": "식빵언니 김연경 유투브요",
          "speaker_id": "2",
          "time": "20210823 15:29"
        },
        {
          "id": "MDRW2100010183.1.23",
          "form": "아~그렇구나~ㅋㅋ 그럼 이제 곧 아는형님에도 나오나보네요",
          "original_form": "아~그렇구나~ㅋㅋ 그럼 이제 곧 아는형님에도 나오나보네요",
          "speaker_id": "1",
          "time": "20210823 15:29"
        },
        {
          "id": "MDRW2100010183.1.24",
          "form": "그게 언제방송분인지 잘 모르겠어요 같이배구하는건 봤는데..",
          "original_form": "그게 언제방송분인지 잘 모르겠어요 같이배구하는건 봤는데..",
          "speaker_id": "2",
