      "utterance": [
        {
          "id": "MDRW2100009471.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009471.1.2",
          "form": "자주 입는 옷 스타일이 뭐예요?",
          "original_form": "자주 입는 옷 스타일이 뭐예요?",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009471.1.3",
          "form": "안녕하세요~ 저는 편하게 입는 스타일 좋아해요",
          "original_form": "안녕하세요~ 저는 편하게 입는 스타일 좋아해요",
          "speaker_id": "2",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009471.1.4",
          "form": "와이드나 슬랙스에 셔츠요 ..!",
          "original_form": "와이드나 슬랙스에 셔츠요 ..!",
          "speaker_id": "2",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009471.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009471.1.6",
          "form": "저는 거기서 더 편하게 입어요",
          "original_form": "저는 거기서 더 편하게 입어요",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009471.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009471.1.8",
          "form": "스포티한 거 좋아해요",
          "original_form": "스포티한 거 좋아해요",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009471.1.9",
          "form": "편한게 좋더라구요~",
          "original_form": "편한게 좋더라구요~",
          "speaker_id": "2",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009471.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009471.1.11",
          "form": "어떤 브랜드 자주 입으시나요?",
          "original_form": "어떤 브랜드 자주 입으시나요?",
          "speaker_id": "2",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009471.1.12",
          "form": "음 스포츠 브랜드가 괜찮은데",
          "original_form": "음 스포츠 브랜드가 괜찮은데",
          "speaker_id": "1",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009471.1.13",
          "form": "전 휠라 디자인 좋아해요",
          "original_form": "전 휠라 디자인 좋아해요",
          "speaker_id": "1",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009471.1.14",
          "form": "근데 초등학교 가니까",
          "original_form": "근데 초등학교 가니까",
          "speaker_id": "1",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009471.1.15",
          "form": "6학년 애들이랑 제 운동화가 같더라고요",
          "original_form": "6학년 애들이랑 제 운동화가 같더라고요",
          "speaker_id": "1",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009471.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009471.1.17",
          "form": "어떤 브랜드 좋아하세요?",
          "original_form": "어떤 브랜드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009471.1.18",
          "form": "저도 휠라운동화 좋아해요ㅋㅋ",
          "original_form": "저도 휠라운동화 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009471.1.19",
          "form": "저는 아디다스 좋아해요! 무신사에서 브랜드 옷 자주 구입하구요",
          "original_form": "저는 아디다스 좋아해요! 무신사에서 브랜드 옷 자주 구입하구요",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009471.1.20",
          "form": "그런데 학생들이랑 겹치는 경우가 많더라구요..ㅎㅎ",
          "original_form": "그런데 학생들이랑 겹치는 경우가 많더라구요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009471.1.21",
          "form": "오 아디다스 저도요",
          "original_form": "오 아디다스 저도요",
          "speaker_id": "1",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009471.1.22",
          "form": "특히 바지가 겹쳐요",
          "original_form": "특히 바지가 겹쳐요",
          "speaker_id": "1",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009471.1.23",
          "form": "패션에 돈 많이 쓰시는 편이세요?",
          "original_form": "패션에 돈 많이 쓰시는 편이세요?",
          "speaker_id": "1",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009471.1.24",
          "form": "많이 사는 편은 아닌데 브랜드 옷 가격대가 있다보니까 한번살때 많이 쓰게되네요..",
          "original_form": "많이 사는 편은 아닌데 브랜드 옷 가격대가 있다보니까 한번살때 많이 쓰게되네요..",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009471.1.25",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009471.1.26",
          "form": "바지랑 셔츠도 뭔가 저렴한건 오래못입어서 한번사서 오래입어요!",
          "original_form": "바지랑 셔츠도 뭔가 저렴한건 오래못입어서 한번사서 오래입어요!",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009471.1.27",
          "form": "음 저도 그런 것 같아요",
          "original_form": "음 저도 그런 것 같아요",
          "speaker_id": "1",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009471.1.28",
          "form": "보세는 안 사고 주로 브랜드를 사거든요",
          "original_form": "보세는 안 사고 주로 브랜드를 사거든요",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009471.1.29",
          "form": "근데 많이는 안 사고",
          "original_form": "근데 많이는 안 사고",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009471.1.30",
          "form": "대신 그 돈을 다른 취미에 써요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "대신 그 돈을 다른 취미에 써요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009471.1.31",
          "form": "그것도 좋네요 브랜드옷은 사면 오래입으니까요!!",
          "original_form": "그것도 좋네요 브랜드옷은 사면 오래입으니까요!!",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009471.1.32",
          "form": "운동도 하시나요?",
          "original_form": "운동도 하시나요?",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009471.1.33",
          "form": "해야하는데.......",
          "original_form": "해야하는데.......",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009471.1.34",
          "form": "안 하고 있어요",
          "original_form": "안 하고 있어요",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009471.1.35",
          "form": "체육관은 재미없어서 싫어하고",
          "original_form": "체육관은 재미없어서 싫어하고",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009471.1.36",
          "form": "수영이나 테니스 배드민턴 같은 스포츠 좋아하거든요",
          "original_form": "수영이나 테니스 배드민턴 같은 스포츠 좋아하거든요",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009471.1.37",
          "form": "근데 코로나도 있고",
          "original_form": "근데 코로나도 있고",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009471.1.38",
          "form": "너무 덥기도 해서요",
          "original_form": "너무 덥기도 해서요",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009471.1.39",
          "form": "평소에 운동 하세요?",
          "original_form": "평소에 운동 하세요?",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009471.1.40",
          "form": "저도 요즘엔 홈트하고있어요!",
          "original_form": "저도 요즘엔 홈트하고있어요!",
          "speaker_id": "2",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100009471.1.41",
          "form": "저도 배드민턴 좋아해서 가끔 공원에 치러가요ㅎㅎ",
          "original_form": "저도 배드민턴 좋아해서 가끔 공원에 치러가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100009471.1.42",
          "form": "오 홈트",
          "original_form": "오 홈트",
          "speaker_id": "1",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100009471.1.43",
          "form": "홈트 하신 지 얼마나 되셨어요?",
          "original_form": "홈트 하신 지 얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210823 00:18"
        },
        {
          "id": "MDRW2100009471.1.44",
          "form": "대단해요!!!",
          "original_form": "대단해요!!!",
          "speaker_id": "1",
          "time": "20210823 00:18"
        },
        {
          "id": "MDRW2100009471.1.45",
          "form": "한지는 한달정도밖에 안됐어요 ..! 다이어트 목적이긴한데 천천히 빼려고 해요ㅎㅎ",
          "original_form": "한지는 한달정도밖에 안됐어요 ..! 다이어트 목적이긴한데 천천히 빼려고 해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:24"
        },
        {
          "id": "MDRW2100009471.1.46",
          "form": "훌륭한 마인드예요",
          "original_form": "훌륭한 마인드예요",
          "speaker_id": "1",
          "time": "20210823 00:28"
        },
        {
          "id": "MDRW2100009471.1.47",
          "form": "다이어트는 길게 보고 해야 건강한 것 같아요",
          "original_form": "다이어트는 길게 보고 해야 건강한 것 같아요",
          "speaker_id": "1",
          "time": "20210823 00:28"
        },
        {
          "id": "MDRW2100009471.1.48",
          "form": "챙겨보시는 스포츠 리그 있으세요?",
          "original_form": "챙겨보시는 스포츠 리그 있으세요?",
          "speaker_id": "1",
          "time": "20210823 00:28"
        },
        {
          "id": "MDRW2100009471.1.49",
          "form": "스포츠는 잘 안챙겨보는데 올림픽은 챙겨봤어요ㅎㅎ",
          "original_form": "스포츠는 잘 안챙겨보는데 올림픽은 챙겨봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:32"
        }
      ]
