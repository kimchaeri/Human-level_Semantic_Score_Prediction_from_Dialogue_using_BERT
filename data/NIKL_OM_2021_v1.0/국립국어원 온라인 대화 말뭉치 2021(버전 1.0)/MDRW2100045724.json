      "utterance": [
        {
          "id": "MDRW2100045724.1.1",
          "form": "학교 밖에 나가면 진짴ㅋㅋㅋ",
          "original_form": "학교 밖에 나가면 진짴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:05"
        },
        {
          "id": "MDRW2100045724.1.2",
          "form": "차밖에 없어요",
          "original_form": "차밖에 없어요",
          "speaker_id": "1",
          "time": "20210908 06:05"
        },
        {
          "id": "MDRW2100045724.1.3",
          "form": "구냥 도로!!!!",
          "original_form": "구냥 도로!!!!",
          "speaker_id": "1",
          "time": "20210908 06:05"
        },
        {
          "id": "MDRW2100045724.1.4",
          "form": "진짜 강제 학식생활이었군요ㅋㅋ.ㅋ..ㅠㅠㅠㅜ",
          "original_form": "진짜 강제 학식생활이었군요ㅋㅋ.ㅋ..ㅠㅠㅠㅜ",
          "speaker_id": "2",
          "time": "20210908 06:06"
        },
        {
          "id": "MDRW2100045724.1.5",
          "form": "학교 편의점은 없었나요?",
          "original_form": "학교 편의점은 없었나요?",
          "speaker_id": "2",
          "time": "20210908 06:06"
        },
        {
          "id": "MDRW2100045724.1.6",
          "form": "넼ㅋㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:06"
        },
        {
          "id": "MDRW2100045724.1.7",
          "form": "편의점도 있기는 했는데",
          "original_form": "편의점도 있기는 했는데",
          "speaker_id": "1",
          "time": "20210908 06:07"
        },
        {
          "id": "MDRW2100045724.1.8",
          "form": "조금만 늦으면 다 털려있었서 자주 이용은 못했어요ㅠㅠ",
          "original_form": "조금만 늦으면 다 털려있었서 자주 이용은 못했어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:07"
        },
        {
          "id": "MDRW2100045724.1.9",
          "form": "다들 한마음 한 뜻이었나봐요ㅌㅋㅌㅌㅌㅋㅋㅋㅋ",
          "original_form": "다들 한마음 한 뜻이었나봐요ㅌㅋㅌㅌㅌㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:08"
        },
        {
          "id": "MDRW2100045724.1.10",
          "form": "편의점 돈 많이 벌었겠는데요",
          "original_form": "편의점 돈 많이 벌었겠는데요",
          "speaker_id": "2",
          "time": "20210908 06:08"
        },
        {
          "id": "MDRW2100045724.1.11",
          "form": "그랬을 것 같아욬ㅋㅋㅋㅋㅋ",
          "original_form": "그랬을 것 같아욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:09"
        },
        {
          "id": "MDRW2100045724.1.12",
          "form": "맛없는 학식보단 편의점 삼김이었우니까욬ㅋㅋ",
          "original_form": "맛없는 학식보단 편의점 삼김이었우니까욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:09"
        },
        {
          "id": "MDRW2100045724.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ삼김보다 구린 학식",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ삼김보다 구린 학식",
          "speaker_id": "2",
          "time": "20210908 06:09"
        },
        {
          "id": "MDRW2100045724.1.14",
          "form": "진짜루욬ㅋㅋㅋㅋㅋ",
          "original_form": "진짜루욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:10"
        },
        {
          "id": "MDRW2100045724.1.15",
          "form": "학교를 왜 구딴식으로 ㅂㄷㅂㄷ...",
          "original_form": "학교를 왜 구딴식으로 ㅂㄷㅂㄷ...",
          "speaker_id": "1",
          "time": "20210908 06:10"
        },
        {
          "id": "MDRW2100045724.1.16",
          "form": "맛없었던 학식메뉴가 떠오르시나요?",
          "original_form": "맛없었던 학식메뉴가 떠오르시나요?",
          "speaker_id": "2",
          "time": "20210908 06:10"
        },
        {
          "id": "MDRW2100045724.1.17",
          "form": "돈까스",
          "original_form": "돈까스",
          "speaker_id": "1",
          "time": "20210908 06:10"
        },
        {
          "id": "MDRW2100045724.1.18",
          "form": "돈까스요!!!",
          "original_form": "돈까스요!!!",
          "speaker_id": "1",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100045724.1.19",
          "form": "돈꺼스가 맛이 없었다니까요!!!",
          "original_form": "돈꺼스가 맛이 없었다니까요!!!",
          "speaker_id": "1",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100045724.1.20",
          "form": "이럴수가",
          "original_form": "이럴수가",
          "speaker_id": "2",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100045724.1.21",
          "form": "어떻게 돈까스가 맛이 없어요",
          "original_form": "어떻게 돈까스가 맛이 없어요",
          "speaker_id": "2",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100045724.1.22",
          "form": "믿기지 않지만 그거시자로 사실입니다.",
          "original_form": "믿기지 않지만 그거시자로 사실입니다.",
          "speaker_id": "1",
          "time": "20210908 06:13"
        },
        {
          "id": "MDRW2100045724.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:14"
        },
        {
          "id": "MDRW2100045724.1.24",
          "form": "동네 분식집 경양식 돈까스도 맛있는 법 아닌가요 원래",
          "original_form": "동네 분식집 경양식 돈까스도 맛있는 법 아닌가요 원래",
          "speaker_id": "2",
          "time": "20210908 06:14"
        },
        {
          "id": "MDRW2100045724.1.25",
          "form": "어케 그걸 맛없게 만들까요",
          "original_form": "어케 그걸 맛없게 만들까요",
          "speaker_id": "2",
          "time": "20210908 06:14"
        },
        {
          "id": "MDRW2100045724.1.26",
          "form": "묘사하자면 어땠어요?",
          "original_form": "묘사하자면 어땠어요?",
          "speaker_id": "2",
          "time": "20210908 06:14"
        },
        {
          "id": "MDRW2100045724.1.27",
          "form": "어...그거보다 맛없다고 하면 될까요?",
          "original_form": "어...그거보다 맛없다고 하면 될까요?",
          "speaker_id": "1",
          "time": "20210908 06:15"
        },
        {
          "id": "MDRW2100045724.1.28",
          "form": "학교다닐때 미니돈가스같은 느낌?",
          "original_form": "학교다닐때 미니돈가스같은 느낌?",
          "speaker_id": "1",
          "time": "20210908 06:15"
        },
        {
          "id": "MDRW2100045724.1.29",
          "form": "이상하게 맛이 빠진 느낌인가요 ㅋㅋㅋㅋ",
          "original_form": "이상하게 맛이 빠진 느낌인가요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:16"
        },
        {
          "id": "MDRW2100045724.1.30",
          "form": "고기맛이 고긴가....?싶은",
          "original_form": "고기맛이 고긴가....?싶은",
          "speaker_id": "2",
          "time": "20210908 06:16"
        },
        {
          "id": "MDRW2100045724.1.31",
          "form": "네넼ㅋㅋ그 있잖아요",
          "original_form": "네넼ㅋㅋ그 있잖아요",
          "speaker_id": "1",
          "time": "20210908 06:16"
        },
        {
          "id": "MDRW2100045724.1.32",
          "form": "이게 고긴가? 근데 왜이리 딱딱하지? 싶은거요",
          "original_form": "이게 고긴가? 근데 왜이리 딱딱하지? 싶은거요",
          "speaker_id": "1",
          "time": "20210908 06:17"
        },
        {
          "id": "MDRW2100045724.1.33",
          "form": "ㅌㅋㅋㅋㅋㅋ끔찍했군요.......",
          "original_form": "ㅌㅋㅋㅋㅋㅋ끔찍했군요.......",
          "speaker_id": "2",
          "time": "20210908 06:17"
        },
        {
          "id": "MDRW2100045724.1.34",
          "form": "뭔가 포를 튀긴 느낌",
          "original_form": "뭔가 포를 튀긴 느낌",
          "speaker_id": "2",
          "time": "20210908 06:17"
        },
        {
          "id": "MDRW2100045724.1.35",
          "form": "꼭 고기 다진거에 밀가루 많아 섞은 느낌이요!",
          "original_form": "꼭 고기 다진거에 밀가루 많아 섞은 느낌이요!",
          "speaker_id": "1",
          "time": "20210908 06:18"
        },
        {
          "id": "MDRW2100045724.1.36",
          "form": "아 맞아요 구려요...",
          "original_form": "아 맞아요 구려요...",
          "speaker_id": "2",
          "time": "20210908 06:19"
        },
        {
          "id": "MDRW2100045724.1.37",
          "form": "열라 정열적으로 튀기는건가",
          "original_form": "열라 정열적으로 튀기는건가",
          "speaker_id": "2",
          "time": "20210908 06:19"
        },
        {
          "id": "MDRW2100045724.1.38",
          "form": "왜 그리 딱딱한지 몰라요",
          "original_form": "왜 그리 딱딱한지 몰라요",
          "speaker_id": "2",
          "time": "20210908 06:19"
        },
        {
          "id": "MDRW2100045724.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:19"
        },
        {
          "id": "MDRW2100045724.1.40",
          "form": "학교에서 쓰는거라고 싼걸 사서 그런거 아닐까 싶어요ㅠㅠ",
          "original_form": "학교에서 쓰는거라고 싼걸 사서 그런거 아닐까 싶어요ㅠㅠ",
