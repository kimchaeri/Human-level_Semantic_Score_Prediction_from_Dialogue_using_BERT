      "utterance": [
        {
          "id": "MDRW2100018522.1.1",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 07:37"
        },
        {
          "id": "MDRW2100018522.1.2",
          "form": "자주 마셔요 카페도 가는데 요즘 캔음료 되게 잘나와서 캔커피도 자주마시구요",
          "original_form": "자주 마셔요 카페도 가는데 요즘 캔음료 되게 잘나와서 캔커피도 자주마시구요",
          "speaker_id": "1",
          "time": "20210827 07:37"
        },
        {
          "id": "MDRW2100018522.1.3",
          "form": "name2님요?",
          "original_form": "&name2&님요?",
          "speaker_id": "1",
          "time": "20210827 07:37"
        },
        {
          "id": "MDRW2100018522.1.4",
          "form": "저는 커피는 못마시고 카페가는거 좋아하고",
          "original_form": "저는 커피는 못마시고 카페가는거 좋아하고",
          "speaker_id": "2",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100018522.1.5",
          "form": "다른 음료 스무디같은거 좋아해요 ㅡㅎㅎ",
          "original_form": "다른 음료 스무디같은거 좋아해요 ㅡㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100018522.1.6",
          "form": "스무디 종류맛잇죠",
          "original_form": "스무디 종류맛잇죠",
          "speaker_id": "1",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100018522.1.7",
          "form": "어떤 스무디좋아하시나요",
          "original_form": "어떤 스무디좋아하시나요",
          "speaker_id": "1",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100018522.1.8",
          "form": "블루베리 스무디를 제일 좋아해요!",
          "original_form": "블루베리 스무디를 제일 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100018522.1.9",
          "form": "초코 프라푸치노도 맛있어요호",
          "original_form": "초코 프라푸치노도 맛있어요호",
          "speaker_id": "2",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100018522.1.10",
          "form": "어떤 커피 좋아하세요?",
          "original_form": "어떤 커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100018522.1.11",
          "form": "맛있는걸 잘 알고계시네요ㅎ",
          "original_form": "맛있는걸 잘 알고계시네요ㅎ",
          "speaker_id": "1",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100018522.1.12",
          "form": "제가 좀 먹짱이라ㅋㅋㅋ",
          "original_form": "제가 좀 먹짱이라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100018522.1.13",
          "form": "저는 쓴건 안좋아해서 라떼마셔요",
          "original_form": "저는 쓴건 안좋아해서 라떼마셔요",
          "speaker_id": "1",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100018522.1.14",
          "form": "그럼 모카류도 좋아하시겠네용",
          "original_form": "그럼 모카류도 좋아하시겠네용",
          "speaker_id": "2",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100018522.1.15",
          "form": "마끼아또나ㅋㅋ",
          "original_form": "마끼아또나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100018522.1.16",
          "form": "마끼아또 좋아해요",
          "original_form": "마끼아또 좋아해요",
          "speaker_id": "1",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100018522.1.17",
          "form": "모카는 뭔가 향에비에 맛이 못따라오는 느낌이라",
          "original_form": "모카는 뭔가 향에비에 맛이 못따라오는 느낌이라",
          "speaker_id": "1",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100018522.1.18",
          "form": "오홍 향은 진짜 좋긴해요 ㅡㅋㅋㅋ",
          "original_form": "오홍 향은 진짜 좋긴해요 ㅡㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100018522.1.19",
          "form": "향에서 기대한 맛이 안나긴하죠",
          "original_form": "향에서 기대한 맛이 안나긴하죠",
          "speaker_id": "2",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100018522.1.20",
          "form": "맞아요 디저트는 뭐 드시나요",
          "original_form": "맞아요 디저트는 뭐 드시나요",
          "speaker_id": "1",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100018522.1.21",
          "form": "에그타르트랑 스콘 좋아해요!",
          "original_form": "에그타르트랑 스콘 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100018522.1.22",
          "form": "푸딩도요!",
          "original_form": "푸딩도요!",
          "speaker_id": "2",
          "time": "20210827 07:41"
        }
      ]
