      "utterance": [
        {
          "id": "MDRW2100013774.1.1",
          "form": "운동 하는 거 있으세요?",
          "original_form": "운동 하는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013774.1.2",
          "form": "아니요ㅎ",
          "original_form": "아니요ㅎ",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013774.1.3",
          "form": "있으신가요?",
          "original_form": "있으신가요?",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013774.1.4",
          "form": "스트레칭 하는 게 다네요",
          "original_form": "스트레칭 하는 게 다네요",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013774.1.5",
          "form": "수영 배우고 싶어요",
          "original_form": "수영 배우고 싶어요",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013774.1.6",
          "form": "수영 할 줄 아세요?",
          "original_form": "수영 할 줄 아세요?",
          "speaker_id": "1",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013774.1.7",
          "form": "아니요ㅠ",
          "original_form": "아니요ㅠ",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013774.1.8",
          "form": "물에뜰줄모른답니다",
          "original_form": "물에뜰줄모른답니다",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013774.1.9",
          "form": "저도 맥주병이에요ㅋㅋ",
          "original_form": "저도 맥주병이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013774.1.10",
          "form": "저도 수영배우려다가",
          "original_form": "저도 수영배우려다가",
          "speaker_id": "2",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100013774.1.11",
          "form": "코로나가ㅠ",
          "original_form": "코로나가ㅠ",
          "speaker_id": "2",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100013774.1.12",
          "form": "마찬가지입니다ㅠㅠ 코로나 백신은 맞으셨어요?",
          "original_form": "마찬가지입니다ㅠㅠ 코로나 백신은 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100013774.1.13",
          "form": "아직이요ㅠ",
          "original_form": "아직이요ㅠ",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100013774.1.14",
          "form": "미루다가 신청을안했네요",
          "original_form": "미루다가 신청을안했네요",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100013774.1.15",
          "form": "저는 내일모레 맞아요",
          "original_form": "저는 내일모레 맞아요",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100013774.1.16",
          "form": "뭘로맞으시나요?",
          "original_form": "뭘로맞으시나요?",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100013774.1.17",
          "form": "화이자 맞아요",
          "original_form": "화이자 맞아요",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100013774.1.18",
          "form": "name2님은 맞을 거면 뭘로 맞고 싶으세요",
          "original_form": "&name2&님은 맞을 거면 뭘로 맞고 싶으세요",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100013774.1.19",
          "form": "저도 화이자요ㅋㅋㅋㅋ",
          "original_form": "저도 화이자요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100013774.1.20",
          "form": "신청을 미루다아직도안맞았네요",
          "original_form": "신청을 미루다아직도안맞았네요",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100013774.1.21",
          "form": "그래도 마스크 잘 쓰고 다니심 안전할 거예요",
          "original_form": "그래도 마스크 잘 쓰고 다니심 안전할 거예요",
