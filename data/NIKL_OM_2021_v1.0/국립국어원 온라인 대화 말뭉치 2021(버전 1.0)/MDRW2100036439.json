      "utterance": [
        {
          "id": "MDRW2100036439.1.1",
          "form": "그러게요 만나지도 못하니...ㅠ",
          "original_form": "그러게요 만나지도 못하니...ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036439.1.2",
          "form": "백신맞으셨어요?",
          "original_form": "백신맞으셨어요?",
          "speaker_id": "1",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036439.1.3",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036439.1.4",
          "form": "저는 지난 금요일에 화이자 1차를 맞았어요!",
          "original_form": "저는 지난 금요일에 화이자 1차를 맞았어요!",
          "speaker_id": "2",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036439.1.5",
          "form": "맞으셨구나!",
          "original_form": "맞으셨구나!",
          "speaker_id": "1",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036439.1.6",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "1",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036439.1.7",
          "form": "여기저기서 부작용 이야기를 많이 들어서 겁났었는데",
          "original_form": "여기저기서 부작용 이야기를 많이 들어서 겁났었는데",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036439.1.8",
          "form": "저는 아직까지 아무렇지도 않네요 ㅎㅎ",
          "original_form": "저는 아직까지 아무렇지도 않네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036439.1.9",
          "form": "팔도 안아프셨어요?",
          "original_form": "팔도 안아프셨어요?",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036439.1.10",
          "form": "주사 맞은 부위가 좀 간지럽고 아팠던 것 외에는 괜찮았어요!",
          "original_form": "주사 맞은 부위가 좀 간지럽고 아팠던 것 외에는 괜찮았어요!",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036439.1.11",
          "form": "이틀동안은 아팠는데",
          "original_form": "이틀동안은 아팠는데",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036439.1.12",
          "form": "그래도 움직이는 데 지장은 없었어요",
          "original_form": "그래도 움직이는 데 지장은 없었어요",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036439.1.13",
          "form": "name1님은 맞으셨나요?",
          "original_form": "&name1&님은 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100036439.1.14",
          "form": "룸메보면 엄청 피곤해하던데",
          "original_form": "룸메보면 엄청 피곤해하던데",
          "speaker_id": "1",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100036439.1.15",
          "form": "괜찮으셨나요",
          "original_form": "괜찮으셨나요",
          "speaker_id": "1",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100036439.1.16",
          "form": "네 저는 괜찮았어요",
          "original_form": "네 저는 괜찮았어요",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036439.1.17",
          "form": "사람마다 개인차가 있나보더라구요",
          "original_form": "사람마다 개인차가 있나보더라구요",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036439.1.18",
          "form": "저는 내일 맞을 예정이예요",
          "original_form": "저는 내일 맞을 예정이예요",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036439.1.19",
          "form": "잘 맞고 오세요 무리하지 마시고",
          "original_form": "잘 맞고 오세요 무리하지 마시고",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036439.1.20",
          "form": "저는 타이레놀도 안먹었답니다",
          "original_form": "저는 타이레놀도 안먹었답니다",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036439.1.21",
          "form": "맞기전에도 조심하라고해서 운동도 못했어요...",
          "original_form": "맞기전에도 조심하라고해서 운동도 못했어요...",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036439.1.22",
          "form": "또 한 일주일 못하겠죠...",
          "original_form": "또 한 일주일 못하겠죠...",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036439.1.23",
          "form": "헉 최대한 조심하고 계시네요",
          "original_form": "헉 최대한 조심하고 계시네요",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036439.1.24",
          "form": "잘 하고 계셔요!",
          "original_form": "잘 하고 계셔요!",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036439.1.25",
          "form": "사실 저는 일주일 안됐는데",
          "original_form": "사실 저는 일주일 안됐는데",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036439.1.26",
          "form": "돈 낸 게 너무 아까워서 오늘 그냥 다녀왔답니다",
          "original_form": "돈 낸 게 너무 아까워서 오늘 그냥 다녀왔답니다",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036439.1.27",
          "form": "헉 다녀오셨군요",
          "original_form": "헉 다녀오셨군요",
          "speaker_id": "1",
          "time": "20210830 23:25"
        }
      ]
    }
