        {
          "id": "MDRW2100032144.1.1",
          "form": "쇼핑은 해도해도 끝이없어요",
          "original_form": "쇼핑은 해도해도 끝이없어요",
          "speaker_id": "2",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100032144.1.2",
          "form": "오늘도 신 발 샀어요",
          "original_form": "오늘도 신 발 샀어요",
          "speaker_id": "1",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100032144.1.3",
          "form": "신 발이요?",
          "original_form": "신 발이요?",
          "speaker_id": "2",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100032144.1.4",
          "form": "어디서샀어요?",
          "original_form": "어디서샀어요?",
          "speaker_id": "2",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100032144.1.5",
          "form": "애기꺼 하나 제 꺼 하나",
          "original_form": "애기꺼 하나 제 꺼 하나",
          "speaker_id": "1",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100032144.1.6",
          "form": "신발",
          "original_form": "신발",
          "speaker_id": "1",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100032144.1.7",
          "form": "애기꺼는 휠라 꾸미 라이트?",
          "original_form": "애기꺼는 휠라 꾸미 라이트?",
          "speaker_id": "1",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100032144.1.8",
          "form": "그거랑ㅋㅋ뉴발란스574요ㅋ",
          "original_form": "그거랑ㅋㅋ뉴발란스574요ㅋ",
          "speaker_id": "1",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100032144.1.9",
          "form": "꾸미라이트는 4컬레째 사는 것 같아요ㅋ",
          "original_form": "꾸미라이트는 4컬레째 사는 것 같아요ㅋ",
          "speaker_id": "1",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100032144.1.10",
          "form": "어머두개나샀군요",
          "original_form": "어머두개나샀군요",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100032144.1.11",
          "form": "휠라꾸미는 진리죠^^",
          "original_form": "휠라꾸미는 진리죠^^",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100032144.1.12",
          "form": "벌써4켤레라니ㅋㅋㅋ",
          "original_form": "벌써4켤레라니ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100032144.1.13",
