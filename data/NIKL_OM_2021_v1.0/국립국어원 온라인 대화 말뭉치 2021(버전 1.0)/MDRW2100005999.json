        },
        {
          "id": "MDRW2100005999.1.3",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100005999.1.4",
          "form": "우산쓰기 넘싫은데 ㅠㅠ",
          "original_form": "우산쓰기 넘싫은데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100005999.1.5",
          "form": "그러게 운동가야 하는데 비가 계속 오네",
          "original_form": "그러게 운동가야 하는데 비가 계속 오네",
          "speaker_id": "2",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100005999.1.6",
          "form": "오늘 계속 오는가",
          "original_form": "오늘 계속 오는가",
          "speaker_id": "1",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100005999.1.7",
          "form": "아마도 그럴 듯",
          "original_form": "아마도 그럴 듯",
          "speaker_id": "2",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100005999.1.8",
          "form": "ㅠㅠ운동은 내일 가야겠다 그럼..",
          "original_form": "ㅠㅠ운동은 내일 가야겠다 그럼..",
          "speaker_id": "1",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100005999.1.9",
          "form": "어쩔 수 없지",
          "original_form": "어쩔 수 없지",
          "speaker_id": "2",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100005999.1.10",
          "form": "우산 쓰고 걷는 거 넘힘들어",
          "original_form": "우산 쓰고 걷는 거 넘힘들어",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100005999.1.11",
          "form": "그래   물 밟는 것도 짜증",
          "original_form": "그래   물 밟는 것도 짜증",
          "speaker_id": "2",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100005999.1.12",
          "form": "웅..ㅋㅋ 신발 다 젖는당",
          "original_form": "웅..ㅋㅋ 신발 다 젖는당",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100005999.1.13",
          "form": "집에 있자  그냥",
          "original_form": "집에 있자  그냥",
          "speaker_id": "2",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100005999.1.14",
          "form": "웅웅 ㅠㅠ안나가고 쉬어야겠다",
          "original_form": "웅웅 ㅠㅠ안나가고 쉬어야겠다",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100005999.1.15",
          "form": "오늘 계속 오는 듯",
