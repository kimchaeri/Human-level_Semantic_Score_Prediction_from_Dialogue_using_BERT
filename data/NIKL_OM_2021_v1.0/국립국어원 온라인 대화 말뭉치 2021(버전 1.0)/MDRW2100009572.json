          "time": "20210823 00:50"
        },
        {
          "id": "MDRW2100009572.1.13",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "1",
          "time": "20210823 00:50"
        },
        {
          "id": "MDRW2100009572.1.14",
          "form": "전 푸딩 올라간 빙수",
          "original_form": "전 푸딩 올라간 빙수",
          "speaker_id": "1",
          "time": "20210823 00:50"
        },
        {
          "id": "MDRW2100009572.1.15",
          "form": "드라마에서 한 번 보고",
          "original_form": "드라마에서 한 번 보고",
          "speaker_id": "1",
          "time": "20210823 00:50"
        },
        {
          "id": "MDRW2100009572.1.16",
          "form": "크",
          "original_form": "크",
          "speaker_id": "2",
          "time": "20210823 00:50"
        },
        {
          "id": "MDRW2100009572.1.17",
          "form": "아직도 아른 거려요",
          "original_form": "아직도 아른 거려요",
          "speaker_id": "1",
          "time": "20210823 00:51"
        },
        {
          "id": "MDRW2100009572.1.18",
          "form": "대만에 대왕연어초밥도 유명",
          "original_form": "대만에 대왕연어초밥도 유명",
          "speaker_id": "2",
          "time": "20210823 00:51"
        },
        {
          "id": "MDRW2100009572.1.19",
          "form": "대만 진짜 가보고 싶다",
          "original_form": "대만 진짜 가보고 싶다",
          "speaker_id": "1",
          "time": "20210823 00:51"
        },
        {
          "id": "MDRW2100009572.1.20",
          "form": "연어ㅜㅜㅜㅜ",
          "original_form": "연어ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 00:51"
        },
        {
          "id": "MDRW2100009572.1.21",
          "form": "홍콩도요",
          "original_form": "홍콩도요",
          "speaker_id": "1",
          "time": "20210823 00:51"
        },
        {
          "id": "MDRW2100009572.1.22",
          "form": "니가간",
          "original_form": "니가간",
          "speaker_id": "2",
          "time": "20210823 00:51"
        },
        {
          "id": "MDRW2100009572.1.23",
          "form": "그 박물관이",
          "original_form": "그 박물관이",
          "speaker_id": "2",
          "time": "20210823 00:51"
        },
        {
          "id": "MDRW2100009572.1.24",
          "form": "뭐였지?",
          "original_form": "뭐였지?",
          "speaker_id": "2",
          "time": "20210823 00:51"
        },
        {
          "id": "MDRW2100009572.1.25",
          "form": "대만국립박물관이요",
          "original_form": "대만국립박물관이요",
          "speaker_id": "1",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009572.1.26",
          "form": "잊지 않아요",
          "original_form": "잊지 않아요",
          "speaker_id": "1",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009572.1.27",
          "form": "1관에는 배추도 있어요",
          "original_form": "1관에는 배추도 있어요",
          "speaker_id": "1",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009572.1.28",
          "form": "ㅋㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009572.1.29",
          "form": "옥으로 만든",
          "original_form": "옥으로 만든",
          "speaker_id": "1",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009572.1.30",
          "form": "반드시 다시 가고 만다",
          "original_form": "반드시 다시 가고 만다",
          "speaker_id": "1",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009572.1.31",
          "form": "배추 나도 봄",
          "original_form": "배추 나도 봄",
          "speaker_id": "2",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009572.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009572.1.33",
          "form": "홍콩",
          "original_form": "홍콩",
          "speaker_id": "2",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009572.1.34",
          "form": "아는게 없음",
          "original_form": "아는게 없음",
          "speaker_id": "2",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009572.1.35",
          "form": "안 가봐서",
          "original_form": "안 가봐서",
          "speaker_id": "2",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009572.1.36",
          "form": "홍콩이",
          "original_form": "홍콩이",
          "speaker_id": "1",
          "time": "20210823 00:56"
        },
        {
          "id": "MDRW2100009572.1.37",
          "form": "약간 평양 냉면인가봐요",
          "original_form": "약간 평양 냉면인가봐요",
          "speaker_id": "1",
          "time": "20210823 00:56"
        },
        {
          "id": "MDRW2100009572.1.38",
          "form": "가면 진짜 별 거 없대요",
          "original_form": "가면 진짜 별 거 없대요",
          "speaker_id": "1",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.39",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.40",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.41",
          "form": "특별한 게 없대요",
          "original_form": "특별한 게 없대요",
          "speaker_id": "1",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.42",
          "form": "음식은 맛있음?",
          "original_form": "음식은 맛있음?",
          "speaker_id": "2",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.43",
          "form": "근데 절반은 다시 안 가",
          "original_form": "근데 절반은 다시 안 가",
          "speaker_id": "1",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.44",
          "form": "그러고",
          "original_form": "그러고",
          "speaker_id": "1",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.45",
          "form": "그게 제일 중요해",
          "original_form": "그게 제일 중요해",
          "speaker_id": "2",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.46",
          "form": "나머지 반은",
          "original_form": "나머지 반은",
          "speaker_id": "1",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.47",
          "form": "겨울마다 그리워해요",
          "original_form": "겨울마다 그리워해요",
          "speaker_id": "1",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.48",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.49",
          "form": "도시니까 yes",
          "original_form": "도시니까 yes",
          "speaker_id": "1",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.50",
          "form": "겨울에 그리워할만하지",
          "original_form": "겨울에 그리워할만하지",
          "speaker_id": "2",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.51",
          "form": "그 속담 알아요?",
          "original_form": "그 속담 알아요?",
          "speaker_id": "1",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.52",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.53",
          "form": "중국인들은 나무와 돌 빼고 다 먹는다고",
          "original_form": "중국인들은 나무와 돌 빼고 다 먹는다고",
          "speaker_id": "1",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.54",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.55",
          "form": "한국은 나무껍질도 먹는데",
          "original_form": "한국은 나무껍질도 먹는데",
          "speaker_id": "1",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009572.1.56",
          "form": "부자는 안 먹지만",
          "original_form": "부자는 안 먹지만",
          "speaker_id": "1",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009572.1.57",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009572.1.58",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009572.1.59",
          "form": "홍콩이 왜 그립냐고 물으면",
          "original_form": "홍콩이 왜 그립냐고 물으면",
          "speaker_id": "1",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009572.1.60",
          "form": "친구들이 하나같이",
          "original_form": "친구들이 하나같이",
          "speaker_id": "1",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009572.1.61",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009572.1.62",
          "form": "그 온도....",
          "original_form": "그 온도....",
          "speaker_id": "1",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009572.1.63",
          "form": "그 습도....",
          "original_form": "그 습도....",
          "speaker_id": "1",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009572.1.64",
          "form": "그 조명....",
          "original_form": "그 조명....",
          "speaker_id": "1",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009572.1.65",
          "form": "그 분위기...",
          "original_form": "그 분위기...",
          "speaker_id": "1",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009572.1.66",
          "form": "ㅋㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009572.1.67",
          "form": "대체 뭔지 궁금해요",
          "original_form": "대체 뭔지 궁금해요",
          "speaker_id": "1",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009572.1.68",
          "form": "어떤 느낌인지 알 거같애",
          "original_form": "어떤 느낌인지 알 거같애",
          "speaker_id": "2",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009572.1.69",
          "form": "해운대 밤거리를 걷다보면",
          "original_form": "해운대 밤거리를 걷다보면",
          "speaker_id": "2",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009572.1.70",
          "form": "여기가 한국인가?",
          "original_form": "여기가 한국인가?",
          "speaker_id": "2",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009572.1.71",
          "form": "홍콩같은데? 라는 생각이 들때가있음",
          "original_form": "홍콩같은데? 라는 생각이 들때가있음",
          "speaker_id": "2",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009572.1.72",
          "form": "그런 느낌인가",
          "original_form": "그런 느낌인가",
          "speaker_id": "2",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009572.1.73",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009572.1.74",
          "form": "궁금해요",
          "original_form": "궁금해요",
          "speaker_id": "1",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009572.1.75",
          "form": "가보면 알게 되겠지",
          "original_form": "가보면 알게 되겠지",
          "speaker_id": "2",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009572.1.76",
          "form": "홍콩에",
          "original_form": "홍콩에",
          "speaker_id": "2",
          "time": "20210823 00:59"
