      "utterance": [
        {
          "id": "MDRW2100009572.1.1",
          "form": "가고싶은 지역이나",
          "original_form": "가고싶은 지역이나",
          "speaker_id": "1",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009572.1.2",
          "form": "먹고 싶은 지역 특산물 말해볼까요",
          "original_form": "먹고 싶은 지역 특산물 말해볼까요",
          "speaker_id": "1",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009572.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009572.1.4",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009572.1.5",
          "form": "나나나 대만",
          "original_form": "나나나 대만",
          "speaker_id": "2",
          "time": "20210823 00:48"
        },
        {
