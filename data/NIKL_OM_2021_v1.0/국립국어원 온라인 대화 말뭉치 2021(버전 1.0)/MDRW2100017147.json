      "utterance": [
        {
          "id": "MDRW2100017147.1.1",
          "form": "ㅋㅋㅋㅋ생선구이드시러가세요",
          "original_form": "ㅋㅋㅋㅋ생선구이드시러가세요",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017147.1.2",
          "form": "ㅋㅋㅋ아니요 오늘은 안갈래요",
          "original_form": "ㅋㅋㅋ아니요 오늘은 안갈래요",
          "speaker_id": "1",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017147.1.3",
          "form": "쳇 그럼유",
          "original_form": "쳇 그럼유",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017147.1.4",
          "form": "샤브샤브",
          "original_form": "샤브샤브",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017147.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017147.1.6",
          "form": "헐 샤브샤브 땡긴다! ㅋㅋ",
          "original_form": "헐 샤브샤브 땡긴다! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017147.1.7",
          "form": "지난주 샤브샤브 맛있게 해먹었나요?",
          "original_form": "지난주 샤브샤브 맛있게 해먹었나요?",
          "speaker_id": "1",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017147.1.8",
          "form": "괜찮앗늠 !!",
          "original_form": "괜찮앗늠 !!",
          "speaker_id": "2",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017147.1.9",
          "form": "백만송이버섯도 넣어먹어봄",
          "original_form": "백만송이버섯도 넣어먹어봄",
          "speaker_id": "2",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100017147.1.10",
          "form": "아주 요리사군요!!!",
          "original_form": "아주 요리사군요!!!",
          "speaker_id": "1",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100017147.1.11",
          "form": "청경채 배추 숙주 불고기감고기나샤브용고기 모듬버섯 백만송이버섯 이렇게산거같아",
          "original_form": "청경채 배추 숙주 불고기감고기나샤브용고기 모듬버섯 백만송이버섯 이렇게산거같아",
          "speaker_id": "2",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100017147.1.12",
          "form": "모듬버섯에 표고 목이 느타리 팽이 이렇게잇엇나?",
          "original_form": "모듬버섯에 표고 목이 느타리 팽이 이렇게잇엇나?",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017147.1.13",
          "form": "난 팽이 버섯 젤 좋아!!!",
          "original_form": "난 팽이 버섯 젤 좋아!!!",
          "speaker_id": "1",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017147.1.14",
          "form": "식감 중시파",
          "original_form": "식감 중시파",
          "speaker_id": "1",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017147.1.15",
          "form": "맞아 ㅋㅋㅋㅋ",
          "original_form": "맞아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100017147.1.16",
          "form": "또먹고싶다 고기가좀에러엇음",
          "original_form": "또먹고싶다 고기가좀에러엇음",
          "speaker_id": "2",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100017147.1.17",
          "form": "헉! 고기가 에러라니 ㅠㅠ 마트가 잘못했구만",
          "original_form": "헉! 고기가 에러라니 ㅠㅠ 마트가 잘못했구만",
          "speaker_id": "1",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017147.1.18",
          "form": "월남쌈싸서 땅콩소스에 찍어먹고싶다",
          "original_form": "월남쌈싸서 땅콩소스에 찍어먹고싶다",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017147.1.19",
          "form": "와 ㅋㅋㅋㅋ 진짜맛잇겟당",
          "original_form": "와 ㅋㅋㅋㅋ 진짜맛잇겟당",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017147.1.20",
          "form": "ㅋㅋ크으ㅋㅋ 암튼 난 바깥음식을 넘 좋아하는거같아",
          "original_form": "ㅋㅋ크으ㅋㅋ 암튼 난 바깥음식을 넘 좋아하는거같아",
          "speaker_id": "1",
          "time": "20210826 17:54"
        },
        {
          "id": "MDRW2100017147.1.21",
          "form": "나듀염 ㅋㅋㅋ 아 머핀에 휴유증으로 매운음식이땡긴당",
          "original_form": "나듀염 ㅋㅋㅋ 아 머핀에 휴유증으로 매운음식이땡긴당",
          "speaker_id": "2",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017147.1.22",
          "form": "ㅋㅋㅋ요즘 마라탕에 대한 애정은 식었나요?",
          "original_form": "ㅋㅋㅋ요즘 마라탕에 대한 애정은 식었나요?",
          "speaker_id": "1",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017147.1.23",
          "form": "급 고민되는데 뭔가",
          "original_form": "급 고민되는데 뭔가",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017147.1.24",
