      "utterance": [
        {
          "id": "MDRW2100040021.1.1",
          "form": "냉면육수 한사발 들이키고싶네요",
          "original_form": "냉면육수 한사발 들이키고싶네요",
          "speaker_id": "1",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100040021.1.2",
          "form": "ㅋㅋㅋ...",
          "original_form": "ㅋㅋㅋ...",
          "speaker_id": "1",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100040021.1.3",
          "form": "저도 늦게 확인했네요 오늘 점심에 냉면은 아니어도 면을 먹어야겠어요!!",
          "original_form": "저도 늦게 확인했네요 오늘 점심에 냉면은 아니어도 면을 먹어야겠어요!!",
          "speaker_id": "2",
          "time": "20210904 11:42"
        },
        {
          "id": "MDRW2100040021.1.4",
          "form": "ㅋㅋ오오 저도 면먹을까용 ㅎㅎㅎㅎ",
          "original_form": "ㅋㅋ오오 저도 면먹을까용 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100040021.1.5",
          "form": "아침에 볶음밥 먹었어요",
          "original_form": "아침에 볶음밥 먹었어요",
          "speaker_id": "1",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100040021.1.6",
          "form": "저는 토달볶 먹었어요 ㅎㅎ",
          "original_form": "저는 토달볶 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040021.1.7",
          "form": "짜파게티 먹으려고요!",
          "original_form": "짜파게티 먹으려고요!",
          "speaker_id": "2",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040021.1.8",
          "form": "꺄 너무맛있었겠어요",
          "original_form": "꺄 너무맛있었겠어요",
          "speaker_id": "1",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040021.1.9",
          "form": "토마토 좋아하는 가족 요기있는뎈ㅋ",
          "original_form": "토마토 좋아하는 가족 요기있는뎈ㅋ",
          "speaker_id": "1",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040021.1.10",
          "form": "짜파게티!!!",
          "original_form": "짜파게티!!!",
          "speaker_id": "1",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040021.1.11",
          "form": "아침에 볶음밥먹다가",
          "original_form": "아침에 볶음밥먹다가",
          "speaker_id": "1",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040021.1.12",
          "form": "아..짜장이랑 같이 먹고싶당 했는뎋ㅋㅋ",
          "original_form": "아..짜장이랑 같이 먹고싶당 했는뎋ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040021.1.13",
          "form": "ㅋㅋ 지금 볶음밥 드셨대서 아 짜파게티 먹고 싶다고 생각했어요",
          "original_form": "ㅋㅋ 지금 볶음밥 드셨대서 아 짜파게티 먹고 싶다고 생각했어요",
          "speaker_id": "2",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040021.1.14",
          "form": "먹어야지!!ㅎㅎ",
          "original_form": "먹어야지!!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040021.1.15",
          "form": "ㅋㅋㅋㅋ 맛나겠어요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ 맛나겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040021.1.16",
          "form": "저희집은",
          "original_form": "저희집은",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040021.1.17",
          "form": "먹을건 바로 실천해요",
          "original_form": "먹을건 바로 실천해요",
          "speaker_id": "2",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040021.1.18",
          "form": "비빔면만 있어요",
          "original_form": "비빔면만 있어요",
          "speaker_id": "1",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100040021.1.19",
          "form": "아 비빔면도 맛있죠",
          "original_form": "아 비빔면도 맛있죠",
          "speaker_id": "2",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100040021.1.20",
          "form": "ㅋㅋㅋ드시는것에 진심이시군요",
          "original_form": "ㅋㅋㅋ드시는것에 진심이시군요{emoji:♥}",
          "speaker_id": "1",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100040021.1.21",
          "form": "이제 추워지기 전에 비빔면 드시면 맛있죠",
          "original_form": "이제 추워지기 전에 비빔면 드시면 맛있죠",
