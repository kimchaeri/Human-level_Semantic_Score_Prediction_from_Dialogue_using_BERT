      "utterance": [
        {
          "id": "MDRW2100002100.1.1",
          "form": "우리 앵무새 장난감 편애가 심하다",
          "original_form": "우리 앵무새 장난감 편애가 심하다",
          "speaker_id": "2",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002100.1.2",
          "form": "어떤 장난감 좋아하는데??",
          "original_form": "어떤 장난감 좋아하는데??",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002100.1.3",
          "form": "물고뜯는 수세미 하나는 안갖고논다",
          "original_form": "물고뜯는 수세미 하나는 안갖고논다",
          "speaker_id": "2",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002100.1.4",
          "form": "사다리는 너무 타고 다녀서 늘어졌다",
          "original_form": "사다리는 너무 타고 다녀서 늘어졌다",
          "speaker_id": "2",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002100.1.5",
          "form": "수세미도 장난감으로 사줬나",
          "original_form": "수세미도 장난감으로 사줬나",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002100.1.6",
          "form": "장난감공도 하루면 고장난다",
          "original_form": "장난감공도 하루면 고장난다",
          "speaker_id": "2",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002100.1.7",
          "form": "진짜 수세미는 아니제",
          "original_form": "진짜 수세미는 아니제",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002100.1.8",
          "form": "그 수세미 식물로 만든거ㅠ 있다",
          "original_form": "그 수세미 식물로 만든거ㅠ 있다",
          "speaker_id": "2",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002100.1.9",
          "form": "수세미라는 식물로 만든.. 뜯는 장난감",
          "original_form": "수세미라는 식물로 만든.. 뜯는 장난감",
          "speaker_id": "2",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002100.1.10",
          "form": "아아 ㅠㅠ 오해할뻔",
          "original_form": "아아 ㅠㅠ 오해할뻔",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002100.1.11",
          "form": "간식도 편식이 심하다",
          "original_form": "간식도 편식이 심하다",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002100.1.12",
          "form": "깃털영양간식 잘 안먹는다",
          "original_form": "깃털영양간식 잘 안먹는다",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002100.1.13",
          "form": "그럼 안좋아하는건 뭐야",
          "original_form": "그럼 안좋아하는건 뭐야",
          "speaker_id": "1",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002100.1.14",
          "form": "깃털영양간식!",
          "original_form": "깃털영양간식!",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002100.1.15",
          "form": "앵이 영양제 뭐 하나 되게 비릿하더라",
          "original_form": "앵이 영양제 뭐 하나 되게 비릿하더라",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002100.1.16",
          "form": "안돼...깃털에 윤기가 촤르르",
          "original_form": "안돼...깃털에 윤기가 촤르르",
          "speaker_id": "1",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002100.1.17",
          "form": "흘러야지 ㅠ",
          "original_form": "흘러야지 ㅠ",
          "speaker_id": "1",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002100.1.18",
          "form": "쇠냄새 나더라",
          "original_form": "쇠냄새 나더라",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002100.1.19",
          "form": "니도 느낄수있을정도?",
          "original_form": "니도 느낄수있을정도?",
          "speaker_id": "1",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002100.1.20",
          "form": "맞아 근데 깃털 윤기가 문제가 아니라 털이 숭숭 빠져",
          "original_form": "맞아 근데 깃털 윤기가 문제가 아니라 털이 숭숭 빠져",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002100.1.21",
          "form": "ㅠㅠ 그럼 더 안돼",
          "original_form": "ㅠㅠ 그럼 더 안돼",
          "speaker_id": "1",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002100.1.22",
          "form": "어ㅠ 나도 먹기싫어서 앵이 물에는 못타주고 밥에 타준다",
          "original_form": "어ㅠ 나도 먹기싫어서 앵이 물에는 못타주고 밥에 타준다",
          "speaker_id": "2",
          "time": "20210819 23:39"
        }
      ]
