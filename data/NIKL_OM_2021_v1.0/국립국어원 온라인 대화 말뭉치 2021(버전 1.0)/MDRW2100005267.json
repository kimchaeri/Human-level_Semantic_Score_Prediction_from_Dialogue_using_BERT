      "utterance": [
        {
          "id": "MDRW2100005267.1.1",
          "form": "네! 스팀이 세일 많이 하는 걸로 유명해요",
          "original_form": "네! 스팀이 세일 많이 하는 걸로 유명해요",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005267.1.2",
          "form": "평소에 30퍼 이런 건 일상이고",
          "original_form": "평소에 30퍼 이런 건 일상이고",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005267.1.3",
          "form": "스타듀밸리 같은 건 찜 해두면",
          "original_form": "스타듀밸리 같은 건 찜 해두면",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005267.1.4",
          "form": "메일로 세일할 때 알려줘요",
          "original_form": "메일로 세일할 때 알려줘요",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005267.1.5",
          "form": "그럼 막 50퍼 넘고",
          "original_form": "그럼 막 50퍼 넘고",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005267.1.6",
          "form": "헐 대박!!!!! 당장 당장 가입해야겠어요",
          "original_form": "헐 대박!!!!! 당장 당장 가입해야겠어요",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005267.1.7",
          "form": "저 예전에 마인크래프트 진짜 좋아했어서",
          "original_form": "저 예전에 마인크래프트 진짜 좋아했어서",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005267.1.8",
          "form": "스듀도 엄청 기대중이거든요!",
          "original_form": "스듀도 엄청 기대중이거든요!",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005267.1.9",
          "form": "name2님도 마크 하셨었어요?",
          "original_form": "&name2&님도 마크 하셨었어요?",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005267.1.10",
          "form": "네! 저 스듀 한 다음에 마크 했는데",
          "original_form": "네! 저 스듀 한 다음에 마크 했는데",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005267.1.11",
          "form": "마크도 재밌더라고요ㅠㅠㅠㅠ",
          "original_form": "마크도 재밌더라고요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005267.1.12",
          "form": "헐 그쵸! 마크 초딩 게임이라고 막 하는데",
          "original_form": "헐 그쵸! 마크 초딩 게임이라고 막 하는데",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005267.1.13",
          "form": "전... 이 나이 먹고도 재밌게 해요...",
          "original_form": "전... 이 나이 먹고도 재밌게 해요...",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005267.1.14",
          "form": "저도... 이 나이 먹고도 재밌게 해요",
          "original_form": "저도... 이 나이 먹고도 재밌게 해요",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005267.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 나이가 무슨 상관이겠어요 게임만 재밌으면 대지 히히",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 나이가 무슨 상관이겠어요 게임만 재밌으면 대지 히히",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005267.1.16",
          "form": "미니 게임도 좋아하세요? 막 스도쿠 같은 게임이요",
          "original_form": "미니 게임도 좋아하세요? 막 스도쿠 같은 게임이요",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005267.1.17",
          "form": "맞져 ㅎㅎ",
          "original_form": "맞져 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005267.1.18",
          "form": "미니게림 좋아해요",
          "original_form": "미니게림 좋아해요",
