      "utterance": [
        {
          "id": "MDRW2100045299.1.1",
          "form": "이것저것 많이해서 스킬레벨이 올라야",
          "original_form": "이것저것 많이해서 스킬레벨이 올라야",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045299.1.2",
          "form": "캐릭레벨이 오르는거예요",
          "original_form": "캐릭레벨이 오르는거예요",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045299.1.3",
          "form": "몬스터잡기는 크게 상관없어요",
          "original_form": "몬스터잡기는 크게 상관없어요",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045299.1.4",
          "form": "아하 그렇군요... 그러면 좀 노가다 형이네요 ㅎ..",
          "original_form": "아하 그렇군요... 그러면 좀 노가다 형이네요 ㅎ..",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045299.1.5",
          "form": "그래서 초반에는 돈주고 기술도 많이 배우면 좋아요",
          "original_form": "그래서 초반에는 돈주고 기술도 많이 배우면 좋아요",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045299.1.6",
          "form": "골드가.. 애들이...",
          "original_form": "골드가.. 애들이...",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045299.1.7",
          "form": "이래서 다들 소매치기를 하나봐요",
          "original_form": "이래서 다들 소매치기를 하나봐요",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045299.1.8",
          "form": "돈은 그냥 몹죽인거 파밍하면 금방 벌어요",
          "original_form": "돈은 그냥 몹죽인거 파밍하면 금방 벌어요",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045299.1.9",
          "form": "쓸곳도 의외로 없어요",
          "original_form": "쓸곳도 의외로 없어요",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045299.1.10",
          "form": "제조가 늘 더 좋아서",
          "original_form": "제조가 늘 더 좋아서",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045299.1.11",
          "form": "인챈도 많이하고",
          "original_form": "인챈도 많이하고",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045299.1.12",
          "form": "제련도하고",
          "original_form": "제련도하고",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045299.1.13",
          "form": "연금도하고",
          "original_form": "연금도하고",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045299.1.14",
          "form": "마법쓰면서 칼도쓰고",
          "original_form": "마법쓰면서 칼도쓰고",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045299.1.15",
          "form": "맞아서 방어숙련도 올리고",
          "original_form": "맞아서 방어숙련도 올리고",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045299.1.16",
          "form": "은신소매치기로 은신이랑 소매치기올리고",
          "original_form": "은신소매치기로 은신이랑 소매치기올리고",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045299.1.17",
          "form": "그럼되요",
          "original_form": "그럼되요",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045299.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045299.1.19",
          "form": "지금 좀 정통 마법사 노리고 있어서",
          "original_form": "지금 좀 정통 마법사 노리고 있어서",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045299.1.20",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045299.1.21",
          "form": "검은 좀 별로네요",
          "original_form": "검은 좀 별로네요",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045299.1.22",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045299.1.23",
          "form": "그럼 마법을",
          "original_form": "그럼 마법을",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045299.1.24",
          "form": "종류별로 다써요",
          "original_form": "종류별로 다써요",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045299.1.25",
          "form": "연금술도 좀 하고 있는데",
          "original_form": "연금술도 좀 하고 있는데",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045299.1.26",
          "form": "변화",
          "original_form": "변화",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045299.1.27",
          "form": "랑",
          "original_form": "랑",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045299.1.28",
          "form": "모르는 이상한 음식들 먹을 때마다",
          "original_form": "모르는 이상한 음식들 먹을 때마다",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045299.1.29",
          "form": "은신법사 괜찮아요",
          "original_form": "은신법사 괜찮아요",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.30",
          "form": "으.. 역겨워서 먹기 싫은데",
          "original_form": "으.. 역겨워서 먹기 싫은데",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.31",
          "form": "제가 아니니까 그냥 먹여요",
          "original_form": "제가 아니니까 그냥 먹여요",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.32",
          "form": "은신은 뎀지 높여줘서",
          "original_form": "은신은 뎀지 높여줘서",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.33",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.34",
          "form": "은신법사? 아 !",
          "original_form": "은신법사? 아 !",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.35",
          "form": "거임발가락먹기",
          "original_form": "거임발가락먹기",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.36",
          "form": "메지카가 딸려서 힘든데 그렇게 해야겠어요",
          "original_form": "메지카가 딸려서 힘든데 그렇게 해야겠어요",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.37",
          "form": "념",
          "original_form": "념",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.38",
          "form": "거미알 먹기...",
          "original_form": "거미알 먹기...",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.39",
          "form": "은신하고 숨으며",
          "original_form": "은신하고 숨으며",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.40",
          "form": "메지카도 금방차요",
          "original_form": "메지카도 금방차요",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.41",
          "form": "공숨공숨 반복!",
          "original_form": "공숨공숨 반복!",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.42",
          "form": "아 진짜요??",
          "original_form": "아 진짜요??",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.43",
          "form": "전투가 풀려서 그런가요?",
          "original_form": "전투가 풀려서 그런가요?",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.44",
          "form": "대신 팔로워 잘 챙기시구 닥돌 안하게",
          "original_form": "대신 팔로워 잘 챙기시구 닥돌 안하게",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.45",
          "form": "내내",
          "original_form": "내내",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.46",
          "form": "맞어요",
          "original_form": "맞어요",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045299.1.47",
          "form": "아.. 정통마법사는",
          "original_form": "아.. 정통마법사는",
          "speaker_id": "2",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045299.1.48",
          "form": "숨지 않기",
          "original_form": "숨지 않기",
          "speaker_id": "2",
          "time": "20210908 01:48"
        },
