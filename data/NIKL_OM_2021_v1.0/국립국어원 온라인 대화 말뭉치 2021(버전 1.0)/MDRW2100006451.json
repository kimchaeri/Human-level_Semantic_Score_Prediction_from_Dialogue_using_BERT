      "utterance": [
        {
          "id": "MDRW2100006451.1.1",
          "form": "중고거래 해보셨나요",
          "original_form": "중고거래 해보셨나요",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100006451.1.2",
          "form": "네네 해봤어요",
          "original_form": "네네 해봤어요",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100006451.1.3",
          "form": "어디서 하시나요?",
          "original_form": "어디서 하시나요?",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100006451.1.4",
          "form": "전 그냥 에스엔에스에서 팔앗어요",
          "original_form": "전 그냥 에스엔에스에서 팔앗어요",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100006451.1.5",
          "form": "그러셨군요 당근은 안하시나요",
          "original_form": "그러셨군요 당근은 안하시나요",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100006451.1.6",
          "form": "당근은 깔았다가 너무 그것만 보고 있어서 지웠어요ㅋㅋㅋㅋㅋ",
          "original_form": "당근은 깔았다가 너무 그것만 보고 있어서 지웠어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100006451.1.7",
          "form": "사지도 않지만 물건 구경하는게 재밌더라고요",
          "original_form": "사지도 않지만 물건 구경하는게 재밌더라고요",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100006451.1.8",
          "form": "제가 그래요...ㅎㅎ",
          "original_form": "제가 그래요...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100006451.1.9",
          "form": "ㅎㅎ 거래 하신 적은 있나요?",
          "original_form": "ㅎㅎ 거래 하신 적은 있나요?",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100006451.1.10",
          "form": "네 거래도 종종 합니다 ㅎㅎ",
          "original_form": "네 거래도 종종 합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100006451.1.11",
          "form": "주로 사시는 쪽인가요?",
          "original_form": "주로 사시는 쪽인가요?",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100006451.1.12",
          "form": "주로 파는쪽인듯요.가끔 사구요",
          "original_form": "주로 파는쪽인듯요.가끔 사구요",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100006451.1.13",
          "form": "머 파셨나요?",
          "original_form": "머 파셨나요?",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100006451.1.14",
          "form": "캠핑용품 육아용품이요 ㅎ",
          "original_form": "캠핑용품 육아용품이요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100006451.1.15",
          "form": "다 고가일 것 같아요",
          "original_form": "다 고가일 것 같아요",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100006451.1.16",
          "form": "당근은 고가는 팔리지 않는듯해요..ㅎㅎ",
          "original_form": "당근은 고가는 팔리지 않는듯해요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006451.1.17",
          "form": "아앗.... 그치만 캠핑용품은 거의 비싸던데요ㅠㅠ",
          "original_form": "아앗.... 그치만 캠핑용품은 거의 비싸던데요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006451.1.18",
          "form": "그러긴 한데 원가 생각하면 되팔땐 별로 못받는듯요..ㅎㅎ",
          "original_form": "그러긴 한데 원가 생각하면 되팔땐 별로 못받는듯요..ㅎㅎ",
