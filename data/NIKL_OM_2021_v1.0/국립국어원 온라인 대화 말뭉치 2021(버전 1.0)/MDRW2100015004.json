      "utterance": [
        {
          "id": "MDRW2100015004.1.1",
          "form": "맞아요ㅠㅠㅠㅠ 디싱이라도 제발 내줬음 좋겠어요",
          "original_form": "맞아요ㅠㅠㅠㅠ 디싱이라도 제발 내줬음 좋겠어요",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100015004.1.2",
          "form": "음방 안나오더라두. . 애들 목소리로 된 신곡 듣고싶어요",
          "original_form": "음방 안나오더라두. . 애들 목소리로 된 신곡 듣고싶어요",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015004.1.3",
          "form": "맞아요...단체곡 제발",
          "original_form": "맞아요...단체곡 제발",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015004.1.4",
          "form": "콘서트도 너무 가고싶은데 코로나때문에 못가는게 너무 슬퍼요 ㅠㅠ",
          "original_form": "콘서트도 너무 가고싶은데 코로나때문에 못가는게 너무 슬퍼요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015004.1.5",
          "form": "그니까요ㅠㅠㅠ",
          "original_form": "그니까요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015004.1.6",
          "form": "팬미팅도 못하고",
          "original_form": "팬미팅도 못하고",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100015004.1.7",
          "form": "눈물ㅜㅠ",
          "original_form": "눈물ㅜㅠ",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100015004.1.8",
          "form": "나졸급팬 2 열어조라조..ㅠㅠㅠ",
          "original_form": "나졸급팬 2 열어조라조..ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100015004.1.9",
          "form": "돌아오면 팬미팅하기로 약속했는뎁ㅜ ㅜㅠ",
          "original_form": "돌아오면 팬미팅하기로 약속했는뎁ㅜ ㅜㅠ",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100015004.1.10",
          "form": "나쁜바이러스자식!!",
          "original_form": "나쁜바이러스자식!!",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100015004.1.11",
          "form": "ㅠㅠㅠㅠ 이렇게 역병이 창궐하게될줄은 꿈에도 몰랐죠..",
          "original_form": "ㅠㅠㅠㅠ 이렇게 역병이 창궐하게될줄은 꿈에도 몰랐죠..",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100015004.1.12",
          "form": "콘서트 하면 가장 보고싶은 무대가 뭐에요?",
          "original_form": "콘서트 하면 가장 보고싶은 무대가 뭐에요?",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100015004.1.13",
          "form": "저는 그동안 안했던 오에스티 무대 해줬으면 좋겠어요 ㅠㅠ",
          "original_form": "저는 그동안 안했던 오에스티 무대 해줬으면 좋겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100015004.1.14",
          "form": "얼웨이즈 오픈이나 환상그녀같은거요 ㅠㅠ 라이브 한 번도 못들어본 곡들!",
          "original_form": "얼웨이즈 오픈이나 환상그녀같은거요 ㅠㅠ 라이브 한 번도 못들어본 곡들!",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100015004.1.15",
          "form": "헐 너무좋아요....무대안해본 노래들 다 해줬음 좋겠어요",
          "original_form": "헐 너무좋아요....무대안해본 노래들 다 해줬음 좋겠어요",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015004.1.16",
          "form": "성규온콘때도 드라마랑 뷰리풀 들어서 좋았는데ㅜㅜ",
          "original_form": "성규온콘때도 드라마랑 뷰리풀 들어서 좋았는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015004.1.17",
          "form": "그런 써프라이즈ㅠㅠ",
          "original_form": "그런 써프라이즈ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015004.1.18",
          "form": "아 맞아요ㅠㅠㅠ",
          "original_form": "아 맞아요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015004.1.19",
          "form": "그리고 솔로곡도 서로 바꿔서 부르기 해줬음 좋겠어요",
          "original_form": "그리고 솔로곡도 서로 바꿔서 부르기 해줬음 좋겠어요",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015004.1.20",
          "form": "황홀했죠.. ㅎㅎ",
          "original_form": "황홀했죠.. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100015004.1.21",
          "form": "그것도 재밌을거같아서ㅎㅎ",
          "original_form": "그것도 재밌을거같아서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100015004.1.22",
          "form": "오 진짜 재밌을 것 같아요",
          "original_form": "오 진짜 재밌을 것 같아요",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100015004.1.23",
          "form": "성규나 우현이가 동우 노래 부르면 ㅋㅋㅋㅋㅋ",
          "original_form": "성규나 우현이가 동우 노래 부르면 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100015004.1.24",
          "form": "ㅋㅋㅋㅋㅋ",
