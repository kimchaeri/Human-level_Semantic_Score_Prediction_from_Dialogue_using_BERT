        },
        {
          "id": "MDRW2100025015.1.2",
          "form": "네다섯개 모아놔여",
          "original_form": "네다섯개 모아놔여",
          "speaker_id": "2",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100025015.1.3",
          "form": "일반핫도그양 될것같아요",
          "original_form": "일반핫도그양 될것같아요",
          "speaker_id": "2",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100025015.1.4",
          "form": "두번도아니고 한입에 먹을수있는",
          "original_form": "두번도아니고 한입에 먹을수있는",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100025015.1.5",
          "form": "그렇게 따지면 딱히 다이어트용도 아닌..",
          "original_form": "그렇게 따지면 딱히 다이어트용도 아닌..",
          "speaker_id": "2",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100025015.1.6",
          "form": "제가 핫도그 귀신이라",
          "original_form": "제가 핫도그 귀신이라",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100025015.1.7",
          "form": "아앗 귀신이나타낫다",
          "original_form": "아앗 귀신이나타낫다",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100025015.1.8",
          "form": "고메핫도그 사서먹는데",
          "original_form": "고메핫도그 사서먹는데",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100025015.1.9",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100025015.1.10",
          "form": "그거 너무 맛있죠",
          "original_form": "그거 너무 맛있죠",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100025015.1.11",
          "form": "그거빵맛이랑 비슷해요",
          "original_form": "그거빵맛이랑 비슷해요",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100025015.1.12",
          "form": "핫도그 좋아하시면",
          "original_form": "핫도그 좋아하시면",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100025015.1.13",
          "form": "그쵸 ㅠㅠ 핫도그 너무 맛있어요",
          "original_form": "그쵸 ㅠㅠ 핫도그 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100025015.1.14",
          "form": "쟌슨빌 핫도그도 드셔보세요 아주그냥..죽여줘요..",
          "original_form": "쟌슨빌 핫도그도 드셔보세요 아주그냥..죽여줘요..",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100025015.1.15",
          "form": "아.... 알죠알죠",
          "original_form": "아.... 알죠알죠",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100025015.1.16",
          "form": "최고",
          "original_form": "최고",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100025015.1.17",
          "form": "그거는소시지가 진짜 맛있는거같아요",
          "original_form": "그거는소시지가 진짜 맛있는거같아요",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
