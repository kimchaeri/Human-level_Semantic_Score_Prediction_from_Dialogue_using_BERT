      "utterance": [
        {
          "id": "MDRW2100034662.1.1",
          "form": "이 자격증 따놓으면 취업에 도움이되긴할까",
          "original_form": "이 자격증 따놓으면 취업에 도움이되긴할까",
          "speaker_id": "1",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100034662.1.2",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100034662.1.3",
          "form": "당연되지",
          "original_form": "당연되지",
          "speaker_id": "2",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100034662.1.4",
          "form": "엑셀중요함",
          "original_form": "엑셀중요함",
          "speaker_id": "2",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100034662.1.5",
          "form": "운전면허도 따고나성",
          "original_form": "운전면허도 따고나성",
          "speaker_id": "1",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100034662.1.6",
          "form": "운전안하면 아무소용없는데",
          "original_form": "운전안하면 아무소용없는데",
          "speaker_id": "1",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100034662.1.7",
          "form": "그 평벙한 사무직에 취업을 하고싶다 하하",
          "original_form": "그 평벙한 사무직에 취업을 하고싶다 하하",
          "speaker_id": "1",
          "time": "20210828 17:13"
        },
        {
          "id": "MDRW2100034662.1.8",
          "form": "ㅋㅋ그거이 참 어렵네",
          "original_form": "ㅋㅋ그거이 참 어렵네",
          "speaker_id": "2",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100034662.1.9",
          "form": "운전면허 딴지 10년장롱",
          "original_form": "운전면허 딴지 10년장롱",
          "speaker_id": "2",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100034662.1.10",
          "form": "저거도 따고 안쓰면 까먹을거같긴한데",
          "original_form": "저거도 따고 안쓰면 까먹을거같긴한데",
          "speaker_id": "1",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100034662.1.11",
          "form": "그래도 와 엑셀이런기능도 있구나",
          "original_form": "그래도 와 엑셀이런기능도 있구나",
          "speaker_id": "1",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100034662.1.12",
          "form": "이런것도 알게는됨",
          "original_form": "이런것도 알게는됨",
          "speaker_id": "1",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100034662.1.13",
          "form": "엑셀마니 씀",
          "original_form": "엑셀마니 씀",
          "speaker_id": "2",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100034662.1.14",
          "form": "유투브에도 강좌마늠",
          "original_form": "유투브에도 강좌마늠",
          "speaker_id": "2",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100034662.1.15",
          "form": "뭔가 수영처럼",
          "original_form": "뭔가 수영처럼",
          "speaker_id": "1",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100034662.1.16",
          "form": "당장 안써도",
          "original_form": "당장 안써도",
          "speaker_id": "1",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100034662.1.17",
          "form": "취업하연",
          "original_form": "취업하연",
          "speaker_id": "1",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100034662.1.18",
          "form": "하면",
          "original_form": "하면",
          "speaker_id": "1",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100034662.1.19",
          "form": "취업하믄 쓰게됨",
          "original_form": "취업하믄 쓰게됨",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034662.1.20",
          "form": "다시또 동영상보면",
          "original_form": "다시또 동영상보면",
          "speaker_id": "1",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034662.1.21",
          "form": "금방 떠올리겠져",
          "original_form": "금방 떠올리겠져",
          "speaker_id": "1",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034662.1.22",
          "form": "ㅇ  잘찾아보믄 있어",
          "original_form": "ㅇ  잘찾아보믄 있어",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034662.1.23",
          "form": "아그때 이런거 있었지 하면서",
          "original_form": "아그때 이런거 있었지 하면서",
          "speaker_id": "1",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034662.1.24",
          "form": "엑셀 배워두면 잘쓴당",
          "original_form": "엑셀 배워두면 잘쓴당",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034662.1.25",
          "form": "진짜라궁",
          "original_form": "진짜라궁",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034662.1.26",
          "form": "ㅇㅇ거기서 자기소개서나 이력서 써오면 봐준대요",
          "original_form": "ㅇㅇ거기서 자기소개서나 이력서 써오면 봐준대요",
          "speaker_id": "1",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100034662.1.27",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100034662.1.28",
          "form": "그거 도움되겠네",
