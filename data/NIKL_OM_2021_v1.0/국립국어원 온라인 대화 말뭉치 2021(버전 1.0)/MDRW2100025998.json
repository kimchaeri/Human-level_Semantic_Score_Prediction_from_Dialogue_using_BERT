      "utterance": [
        {
          "id": "MDRW2100025998.1.1",
          "form": "요즘 갖고싶은 거 있으세요?",
          "original_form": "요즘 갖고싶은 거 있으세요?",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025998.1.2",
          "form": "요즘 가을옷이나 가방 운동화사고싶네요ㅋㅋ",
          "original_form": "요즘 가을옷이나 가방 운동화사고싶네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025998.1.3",
          "form": "가을옷은 어떤 종류로 사고싶으세요?",
          "original_form": "가을옷은 어떤 종류로 사고싶으세요?",
          "speaker_id": "2",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025998.1.4",
          "form": "옷은 늘 없어요ㅋㅋㅋㅋㅋㅋ작년에 뭐입었는지ㅋㅋㄱㄲㅋㅋ",
          "original_form": "옷은 늘 없어요ㅋㅋㅋㅋㅋㅋ작년에 뭐입었는지ㅋㅋㄱㄲㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025998.1.5",
          "form": "전 가을 원피스 입고싶은데 마땅한 게 없다라고요",
          "original_form": "전 가을 원피스 입고싶은데 마땅한 게 없다라고요",
          "speaker_id": "2",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025998.1.6",
          "form": "한번씩 아울렛가는데ㅋㅋ",
          "original_form": "한번씩 아울렛가는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025998.1.7",
          "form": "또 가볼까싶어요ㅋㅋ",
          "original_form": "또 가볼까싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025998.1.8",
          "form": "맞아요 옷은 항상 해바뀌면 뭐 입었는지 모르겠어요ㅋㅋㅋㅋ",
          "original_form": "맞아요 옷은 항상 해바뀌면 뭐 입었는지 모르겠어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025998.1.9",
          "form": "아울렛 좋죠",
          "original_form": "아울렛 좋죠",
          "speaker_id": "2",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025998.1.10",
          "form": "네 그렇죠ㅋ",
          "original_form": "네 그렇죠ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025998.1.11",
          "form": "저도 어머니랑 이번에 아울렛 가려는데",
          "original_form": "저도 어머니랑 이번에 아울렛 가려는데",
          "speaker_id": "2",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025998.1.12",
          "form": "네네ㅋㅋ",
          "original_form": "네네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025998.1.13",
          "form": "아 그러시구나ㅋㅋ",
          "original_form": "아 그러시구나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025998.1.14",
          "form": "쇼핑하기엔 아울렛이 가격도 그렇고 참 좋아요",
          "original_form": "쇼핑하기엔 아울렛이 가격도 그렇고 참 좋아요",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025998.1.15",
          "form": "맞아요. 가격도 좋고",
          "original_form": "맞아요. 가격도 좋고",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025998.1.16",
          "form": "옷 재질도좋구요ㅋ",
          "original_form": "옷 재질도좋구요ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025998.1.17",
          "form": "이월상품이라고",
          "original_form": "이월상품이라고",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025998.1.18",
          "form": "가격 떨이로 나왔는데",
          "original_form": "가격 떨이로 나왔는데",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025998.1.19",
          "form": "네네ㅋ",
          "original_form": "네네ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025998.1.20",
          "form": "좋은 상품인 고 건지면",
          "original_form": "좋은 상품인 고 건지면",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
