      "utterance": [
        {
          "id": "MDRW2100001051.1.1",
          "form": "나 다이어트 해야하는데",
          "original_form": "나 다이어트 해야하는데",
          "speaker_id": "1",
          "time": "20210722 16:57"
        },
        {
          "id": "MDRW2100001051.1.2",
          "form": "지금 몇키로야?",
          "original_form": "지금 몇키로야?",
          "speaker_id": "2",
          "time": "20210722 16:58"
        },
        {
          "id": "MDRW2100001051.1.3",
          "form": "지금 나 72",
          "original_form": "지금 나 72",
          "speaker_id": "1",
          "time": "20210722 16:58"
        },
        {
          "id": "MDRW2100001051.1.4",
          "form": "키는?",
          "original_form": "키는?",
          "speaker_id": "2",
          "time": "20210722 16:58"
        },
        {
          "id": "MDRW2100001051.1.5",
          "form": "178!",
          "original_form": "178!",
          "speaker_id": "1",
          "time": "20210722 16:58"
        },
        {
          "id": "MDRW2100001051.1.6",
          "form": "헉 적당하지 않나?",
          "original_form": "헉 적당하지 않나?",
          "speaker_id": "2",
          "time": "20210722 16:59"
        },
        {
          "id": "MDRW2100001051.1.7",
          "form": "그러긴한데...",
          "original_form": "그러긴한데...",
          "speaker_id": "1",
          "time": "20210722 16:59"
        },
        {
          "id": "MDRW2100001051.1.8",
          "form": "근육이 별로없어?",
          "original_form": "근육이 별로없어?",
          "speaker_id": "2",
          "time": "20210722 16:59"
        },
        {
          "id": "MDRW2100001051.1.9",
          "form": "근육도 없고....",
          "original_form": "근육도 없고....",
          "speaker_id": "1",
          "time": "20210722 17:00"
        },
        {
          "id": "MDRW2100001051.1.10",
          "form": "그래서 방학이니까 헬스 다니려고!!",
          "original_form": "그래서 방학이니까 헬스 다니려고!!",
          "speaker_id": "1",
          "time": "20210722 17:00"
        },
        {
          "id": "MDRW2100001051.1.11",
          "form": "오 나도 헬스장 다녀",
          "original_form": "오 나도 헬스장 다녀",
          "speaker_id": "2",
          "time": "20210722 17:01"
        },
        {
          "id": "MDRW2100001051.1.12",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210722 17:01"
        },
        {
          "id": "MDRW2100001051.1.13",
          "form": "다니면 무슨운동 해야해??",
          "original_form": "다니면 무슨운동 해야해??",
          "speaker_id": "1",
          "time": "20210722 17:01"
        },
        {
          "id": "MDRW2100001051.1.14",
          "form": "난아예 몰라서..",
          "original_form": "난아예 몰라서..",
          "speaker_id": "1",
          "time": "20210722 17:01"
        },
        {
          "id": "MDRW2100001051.1.15",
          "form": "나도 잘 몰라서 pt했어",
          "original_form": "나도 잘 몰라서 pt했어",
          "speaker_id": "2",
          "time": "20210722 17:02"
        },
        {
          "id": "MDRW2100001051.1.16",
          "form": "아하 나도",
          "original_form": "아하 나도",
          "speaker_id": "1",
          "time": "20210722 17:02"
        },
        {
          "id": "MDRW2100001051.1.17",
          "form": "pt받을까??",
          "original_form": "pt받을까??",
          "speaker_id": "1",
          "time": "20210722 17:03"
        },
        {
          "id": "MDRW2100001051.1.18",
          "form": "용어들은 잘 모르는데 등근육 운동 위주로해",
          "original_form": "용어들은 잘 모르는데 등근육 운동 위주로해",
          "speaker_id": "2",
          "time": "20210722 17:03"
        },
        {
          "id": "MDRW2100001051.1.19",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210722 17:03"
        },
        {
          "id": "MDRW2100001051.1.20",
          "form": "난 초보자라면",
          "original_form": "난 초보자라면",
          "speaker_id": "2",
          "time": "20210722 17:03"
        },
        {
          "id": "MDRW2100001051.1.21",
          "form": "하는게 좋다고 생각해",
          "original_form": "하는게 좋다고 생각해",
          "speaker_id": "2",
          "time": "20210722 17:03"
        },
        {
          "id": "MDRW2100001051.1.22",
          "form": "열심히 해여겠다..",
          "original_form": "열심히 해여겠다..",
          "speaker_id": "1",
          "time": "20210722 17:03"
        },
        {
          "id": "MDRW2100001051.1.23",
          "form": "꾸준히가는게 가장 힘들긴해",
          "original_form": "꾸준히가는게 가장 힘들긴해",
          "speaker_id": "2",
          "time": "20210722 17:04"
        },
        {
          "id": "MDRW2100001051.1.24",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "1",
          "time": "20210722 17:04"
        },
        {
          "id": "MDRW2100001051.1.25",
          "form": "진짜 꾸준히 하는게 어렵지...",
          "original_form": "진짜 꾸준히 하는게 어렵지...",
