      "utterance": [
        {
          "id": "MDRW2100034480.1.1",
          "form": "만두 쪄먹었어요!",
          "original_form": "만두 쪄먹었어요!",
          "speaker_id": "2",
          "time": "20210828 13:35"
        },
        {
          "id": "MDRW2100034480.1.2",
          "form": "어떤 만두 드셨나요??",
          "original_form": "어떤 만두 드셨나요??",
          "speaker_id": "1",
          "time": "20210828 13:35"
        },
        {
          "id": "MDRW2100034480.1.3",
          "form": "얇은피만두 고기랑 김치 먹었어요 ㅋㅋㅋㅋ",
          "original_form": "얇은피만두 고기랑 김치 먹었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100034480.1.4",
          "form": "단무지도 같이 사서 먹으니 꿀맛이더라구여",
          "original_form": "단무지도 같이 사서 먹으니 꿀맛이더라구여",
          "speaker_id": "2",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100034480.1.5",
          "form": "단무지랑 꿀조합이죠 ㅠ",
          "original_form": "단무지랑 꿀조합이죠 ㅠ",
          "speaker_id": "1",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100034480.1.6",
          "form": "저도 먹고싶네요ㅋㅋㅋ",
          "original_form": "저도 먹고싶네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100034480.1.7",
          "form": "점심 뭐드셧어요?",
          "original_form": "점심 뭐드셧어요?",
          "speaker_id": "2",
          "time": "20210828 13:38"
        },
        {
          "id": "MDRW2100034480.1.8",
          "form": "저는 닭갈비요 ㅋㅋㅋ",
          "original_form": "저는 닭갈비요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100034480.1.9",
          "form": "어제시켜먹고 남아서 해치웠어요",
          "original_form": "어제시켜먹고 남아서 해치웠어요",
          "speaker_id": "1",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100034480.1.10",
          "form": "크 닭갈비 치즈추가하셧나요!",
          "original_form": "크 닭갈비 치즈추가하셧나요!",
          "speaker_id": "2",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100034480.1.11",
          "form": "하 아쉽게도 아니요 ㅠㅜ",
          "original_form": "하 아쉽게도 아니요 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100034480.1.12",
          "form": "원래 치즈엄청 좋아했는데 요샌 다이어터라 그거라도 덜어요ㅠㅋㅋㅋㅋ",
          "original_form": "원래 치즈엄청 좋아했는데 요샌 다이어터라 그거라도 덜어요ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100034480.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ치즈 칼로리 많이나가나요...? 조금 올라가니까 괜찮지않을까요....",
          "original_form": "ㅋㅋㅋㅋㅋㅋ치즈 칼로리 많이나가나요...? 조금 올라가니까 괜찮지않을까요....",
          "speaker_id": "2",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100034480.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저도 치즈를 좋아해서욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저도 치즈를 좋아해서욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100034480.1.15",
          "form": "하근데또 조금이면 감질맛나서..",
          "original_form": "하근데또 조금이면 감질맛나서..",
          "speaker_id": "1",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100034480.1.16",
          "form": "지금도 치즈마카롱 먹어요 ㅋㅋㅋㅋ",
          "original_form": "지금도 치즈마카롱 먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100034480.1.17",
          "form": "크 너무 맛잇겟믄데요 ㅠㅜㅜㅠ",
          "original_form": "크 너무 맛잇겟믄데요 ㅠㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034480.1.18",
          "form": "가까이에 마카롱집이 없어서 아쉬운데 다행같기도하고",
          "original_form": "가까이에 마카롱집이 없어서 아쉬운데 다행같기도하고",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034480.1.19",
          "form": "반반이네요 ㅋㅋㅋ",
          "original_form": "반반이네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034480.1.20",
          "form": "전 전에 근처 마카롱집 배달시켜서 망했는데",
          "original_form": "전 전에 근처 마카롱집 배달시켜서 망했는데",
          "speaker_id": "1",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100034480.1.21",
          "form": "아이디어스에서 시켰더니 너무 맛있어요",
          "original_form": "아이디어스에서 시켰더니 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100034480.1.22",
          "form": "오 아이디어스 어느상품이에요? 추천해주세요!",
          "original_form": "오 아이디어스 어느상품이에요? 추천해주세요!",
          "speaker_id": "2",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100034480.1.23",
          "form": "안그래두 토스할인해서 이용해볼까했거든요",
          "original_form": "안그래두 토스할인해서 이용해볼까했거든요",
          "speaker_id": "2",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100034480.1.24",
          "form": "오 저도 그걸로 먹었어요!",
          "original_form": "오 저도 그걸로 먹었어요!",
          "speaker_id": "1",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100034480.1.25",
          "form": "디디얌 먹었는데 생각보단 쏘쏘였고",
          "original_form": "디디얌 먹었는데 생각보단 쏘쏘였고",
          "speaker_id": "1",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100034480.1.26",
          "form": "타이니디저트? 에서",
          "original_form": "타이니디저트? 에서",
          "speaker_id": "1",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100034480.1.27",
          "form": "크림브륄레 마카롱 너무 맛있어요!! 인생마카롱입니다",
          "original_form": "크림브륄레 마카롱 너무 맛있어요!! 인생마카롱입니다",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100034480.1.28",
          "form": "오오오오 좀따가 주문해야겟어욬ㅋㅋㅋㅋㅋ",
          "original_form": "오오오오 좀따가 주문해야겟어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100034480.1.29",
          "form": "강추합니다!!",
          "original_form": "강추합니다!!",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100034480.1.30",
          "form": "저 마카롱 그렇게 안 좋아하는데 눈이 띠용해졌어요",
          "original_form": "저 마카롱 그렇게 안 좋아하는데 눈이 띠용해졌어요",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100034480.1.31",
          "form": "의외로 살게없어서 그냥 놔둿엇거든욬ㅋㅋㅋ",
          "original_form": "의외로 살게없어서 그냥 놔둿엇거든욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100034480.1.32",
          "form": "쫀득이 아니라 쫜득~~",
          "original_form": "쫀득이 아니라 쫜득~~",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100034480.1.33",
          "form": "ㅋㅋㅋㅋ 저도 아이디어스 첨 이용해봤어요",
          "original_form": "ㅋㅋㅋㅋ 저도 아이디어스 첨 이용해봤어요",
          "speaker_id": "1",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100034480.1.34",
          "form": "엄청 고민했어여",
          "original_form": "엄청 고민했어여",
