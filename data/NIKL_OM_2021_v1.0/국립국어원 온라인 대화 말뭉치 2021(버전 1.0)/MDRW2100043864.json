      "utterance": [
        {
          "id": "MDRW2100043864.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043864.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043864.1.3",
          "form": "저녁은 드셨나요ㅎㅎ",
          "original_form": "저녁은 드셨나요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043864.1.4",
          "form": "넵넵",
          "original_form": "넵넵",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043864.1.5",
          "form": "닭볶음탕 먹었어요",
          "original_form": "닭볶음탕 먹었어요",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043864.1.6",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043864.1.7",
          "form": "오.. 맛있는거 드셨네요ㅎㅎ",
          "original_form": "오.. 맛있는거 드셨네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043864.1.8",
          "form": "저는 김치찌개 해먹으려다 부엌에 전구가 나가서 그냥 가볍게먹었어요",
          "original_form": "저는 김치찌개 해먹으려다 부엌에 전구가 나가서 그냥 가볍게먹었어요",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043864.1.9",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043864.1.10",
          "form": "지금은 고치셨나요?",
          "original_form": "지금은 고치셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043864.1.11",
          "form": "자취방이라 내일 집주인분께 연락드리려구요ㅎㅎ",
          "original_form": "자취방이라 내일 집주인분께 연락드리려구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043864.1.12",
          "form": "영 갑갑하네요",
          "original_form": "영 갑갑하네요",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043864.1.13",
          "form": "아이구 그러시겠어요ㅠㅠ",
          "original_form": "아이구 그러시겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043864.1.14",
          "form": "어떤 메뉴드셨나요?",
          "original_form": "어떤 메뉴드셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043864.1.15",
          "form": "찌개 해먹으려던 고기  그냥 구워먹었네요 ㅎㅎ",
          "original_form": "찌개 해먹으려던 고기  그냥 구워먹었네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043864.1.16",
          "form": "아무래도 구이가 간단해서요",
          "original_form": "아무래도 구이가 간단해서요",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043864.1.17",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043864.1.18",
          "form": "고기는",
          "original_form": "고기는",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043864.1.19",
          "form": "모두가 옳ㅈㅅ",
          "original_form": "모두가 옳ㅈㅅ",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043864.1.20",
          "form": "옳죠",
          "original_form": "옳죠",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043864.1.21",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043864.1.22",
          "form": "구워먹으면 그릇도 많이 안나오고..",
          "original_form": "구워먹으면 그릇도 많이 안나오고..",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043864.1.23",
          "form": "설겆이 덜해도 되고!",
          "original_form": "설겆이 덜해도 되고!",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043864.1.24",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043864.1.25",
          "form": "만약 에프가 있다면",
          "original_form": "만약 에프가 있다면",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043864.1.26",
          "form": "심지어 그건 그냥 넣기만 하면 끝이에요...",
          "original_form": "심지어 그건 그냥 넣기만 하면 끝이에요...",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043864.1.27",
          "form": "너무 간단..",
          "original_form": "너무 간단..",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043864.1.28",
          "form": "크..  맞아요",
          "original_form": "크..  맞아요",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043864.1.29",
          "form": "에프는 진짜 신기하더라구요 넘편하구",
          "original_form": "에프는 진짜 신기하더라구요 넘편하구",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043864.1.30",
          "form": "맞아요 진짜 혁신이에요",
          "original_form": "맞아요 진짜 혁신이에요",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043864.1.31",
          "form": "사람은 그냥 놀기만 하면 되고ㅠㅠ",
          "original_form": "사람은 그냥 놀기만 하면 되고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:48"
        }
      ]
    }
  ]
