        },
        {
          "id": "MDRW2100036337.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036337.1.30",
          "form": "감탄했습니다 20대 재테크",
          "original_form": "감탄했습니다 20대 재테크",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036337.1.31",
