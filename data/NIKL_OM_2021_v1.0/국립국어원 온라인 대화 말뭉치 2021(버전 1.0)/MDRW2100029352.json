      "utterance": [
        {
          "id": "MDRW2100029352.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210903 02:31"
        },
        {
          "id": "MDRW2100029352.1.2",
          "form": "안녕하세요.",
          "original_form": "안녕하세요.",
          "speaker_id": "1",
          "time": "20210903 02:32"
        },
        {
          "id": "MDRW2100029352.1.3",
          "form": "다이소에 맞는사이즈의 케이스가 있엇나보네요 ?",
          "original_form": "다이소에 맞는사이즈의 케이스가 있엇나보네요 ?",
          "speaker_id": "1",
          "time": "20210903 02:32"
        },
        {
          "id": "MDRW2100029352.1.4",
          "form": "아 케이스는 다이소에서 구매한게 아니에요",
          "original_form": "아 케이스는 다이소에서 구매한게 아니에요",
          "speaker_id": "2",
          "time": "20210903 02:32"
        },
        {
          "id": "MDRW2100029352.1.5",
          "form": "어디서 샀어요 ?",
          "original_form": "어디서 샀어요 ?",
          "speaker_id": "1",
          "time": "20210903 02:32"
        },
        {
          "id": "MDRW2100029352.1.6",
          "form": "인터넷으로요",
          "original_form": "인터넷으로요",
          "speaker_id": "2",
          "time": "20210903 02:33"
        },
        {
          "id": "MDRW2100029352.1.7",
          "form": "몇개 삿어요?",
          "original_form": "몇개 삿어요?",
          "speaker_id": "1",
          "time": "20210903 02:33"
        },
        {
          "id": "MDRW2100029352.1.8",
          "form": "케이서 원플원으로 하나 샀지만 2개가 왔죠",
          "original_form": "케이서 원플원으로 하나 샀지만 2개가 왔죠",
          "speaker_id": "2",
          "time": "20210903 02:33"
        },
        {
          "id": "MDRW2100029352.1.9",
          "form": "케이서가 아니라 케이스에요",
          "original_form": "케이서가 아니라 케이스에요",
          "speaker_id": "2",
          "time": "20210903 02:34"
        },
        {
          "id": "MDRW2100029352.1.10",
          "form": "그거 하나는 잘 모셔둿다가 많이 변색될때 바꿔요",
          "original_form": "그거 하나는 잘 모셔둿다가 많이 변색될때 바꿔요",
          "speaker_id": "1",
          "time": "20210903 02:34"
        },
        {
          "id": "MDRW2100029352.1.11",
          "form": "네 그럴려고 하나는 잘 모셔뒀는데 어디다 뒀는지 기억이 안 나네요ㅠㅠ",
          "original_form": "네 그럴려고 하나는 잘 모셔뒀는데 어디다 뒀는지 기억이 안 나네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 02:34"
        },
        {
          "id": "MDRW2100029352.1.12",
          "form": "집에 투명케이스가 있는데 갈색이 되어버렸어요",
          "original_form": "집에 투명케이스가 있는데 갈색이 되어버렸어요",
          "speaker_id": "1",
          "time": "20210903 02:34"
        },
        {
          "id": "MDRW2100029352.1.13",
          "form": "어머나.....충격이네요",
          "original_form": "어머나.....충격이네요",
          "speaker_id": "2",
          "time": "20210903 02:35"
        },
        {
          "id": "MDRW2100029352.1.14",
          "form": "이것때문에 안경도 투명으로 안삿어요",
          "original_form": "이것때문에 안경도 투명으로 안삿어요",
          "speaker_id": "1",
          "time": "20210903 02:35"
        },
        {
          "id": "MDRW2100029352.1.15",
          "form": "투명도 잘 구매해야겠어요 \n아니면 저렴한걸로 자주 바꿔도 되고",
          "original_form": "투명도 잘 구매해야겠어요 \n아니면 저렴한걸로 자주 바꿔도 되고",
          "speaker_id": "2",
          "time": "20210903 02:35"
        },
        {
          "id": "MDRW2100029352.1.16",
          "form": "아 투명테를 사려고 하시다가 안 하셨군요",
          "original_form": "아 투명테를 사려고 하시다가 안 하셨군요",
          "speaker_id": "2",
          "time": "20210903 02:36"
        },
        {
          "id": "MDRW2100029352.1.17",
          "form": "하얀애들은 언제든지 변색이되죠\n그래서 햇빛쬐면 대부분 누렇게되고\n여러모로 처치곤란이라  옷도 흰색안사요",
          "original_form": "하얀애들은 언제든지 변색이되죠\n그래서 햇빛쬐면 대부분 누렇게되고\n여러모로 처치곤란이라  옷도 흰색안사요",
          "speaker_id": "1",
