      "utterance": [
        {
          "id": "MDRW2100015071.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015071.1.2",
          "form": "반갑습니당",
          "original_form": "반갑습니당",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015071.1.3",
          "form": "안녕하새용 동물 키우눈거 있으신가요??",
          "original_form": "안녕하새용 동물 키우눈거 있으신가요??",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015071.1.4",
          "form": "동물은 예전에 키웠었어요",
          "original_form": "동물은 예전에 키웠었어요",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015071.1.5",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015071.1.6",
          "form": "뭐 키우셨어요??",
          "original_form": "뭐 키우셨어요??",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015071.1.7",
          "form": "그..열대어요",
          "original_form": "그..열대어요",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015071.1.8",
          "form": "물고기",
          "original_form": "물고기",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015071.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ우와 완전 예상 외에여 열대어면 되게 쪼꼬만하고 화려한 그런건가용",
          "original_form": "ㅋㅋㅋㅋㅋㅋ우와 완전 예상 외에여 열대어면 되게 쪼꼬만하고 화려한 그런건가용",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015071.1.10",
          "form": "어..네에ㅜㅜ",
          "original_form": "어..네에ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015071.1.11",
          "form": "무슨 동물키우셨어용?",
          "original_form": "무슨 동물키우셨어용?",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015071.1.12",
          "form": "전 동물 키워본적 없어요ㅜㅜ",
          "original_form": "전 동물 키워본적 없어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015071.1.13",
          "form": "키우고 싶은건 있는데",
          "original_form": "키우고 싶은건 있는데",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015071.1.14",
          "form": "앵무새",
          "original_form": "앵무새",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015071.1.15",
          "form": "엉? 진짜요?",
          "original_form": "엉? 진짜요?",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015071.1.16",
          "form": "저맞춰볼래용",
          "original_form": "저맞춰볼래용",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015071.1.17",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015071.1.18",
          "form": "전혀아니네요",
          "original_form": "전혀아니네요",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015071.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015071.1.20",
          "form": "갱얼쥐일줄",
          "original_form": "갱얼쥐일줄",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015071.1.21",
          "form": "ㅋㅋㄱㄲ",
          "original_form": "ㅋㅋㄱㄲ",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015071.1.22",
          "form": "아 ㅋㅋㅋㅋㅋ웃겨 유튜브 보니까 귀엽더라구여 ㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ웃겨 유튜브 보니까 귀엽더라구여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015071.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015071.1.24",
          "form": "지송합니닷",
          "original_form": "지송합니닷",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015071.1.25",
          "form": "저의편견이었네용",
          "original_form": "저의편견이었네용",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015071.1.26",
          "form": "ㅋㅋㅋㅋ갠찬아여 유튭에 태식이라고 하얀 앵무새 있는데 넘나 귀엽드라요",
          "original_form": "ㅋㅋㅋㅋ갠찬아여 유튭에 태식이라고 하얀 앵무새 있는데 넘나 귀엽드라요",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015071.1.27",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015071.1.28",
          "form": "태식이?",
          "original_form": "태식이?",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015071.1.29",
          "form": "밥도 묵고",
          "original_form": "밥도 묵고",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015071.1.30",
          "form": "나쁜말아닌데ㅜㅜ",
          "original_form": "나쁜말아닌데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015071.1.31",
          "form": "영화대사 패러디인데",
          "original_form": "영화대사 패러디인데",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015071.1.32",
          "form": "안가네요ㅜ",
          "original_form": "안가네요ㅜ",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015071.1.33",
          "form": "막 말도 하나요?",
          "original_form": "막 말도 하나요?",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015071.1.34",
          "form": "네 노래부르고 대답하구 물건 던지면 물어오고",
          "original_form": "네 노래부르고 대답하구 물건 던지면 물어오고",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015071.1.35",
          "form": "아구 귀여워",
          "original_form": "아구 귀여워",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015071.1.36",
          "form": "진짜 넘무 귀엽습니당",
          "original_form": "진짜 넘무 귀엽습니당",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015071.1.37",
          "form": "노래 ..",
          "original_form": "노래 ..",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015071.1.38",
          "form": "전 음치입니다",
          "original_form": "전 음치입니다",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015071.1.39",
          "form": "태식이도 음치던데..",
          "original_form": "태식이도 음치던데..",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015071.1.40",
          "form": "헐..",
          "original_form": "헐..",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015071.1.41",
          "form": "앗 님이 앵무새랑 동급이란건 아니에여!!",
          "original_form": "앗 님이 앵무새랑 동급이란건 아니에여!!",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015071.1.42",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015071.1.43",
          "form": "제가앵무새가 되는건..",
          "original_form": "제가앵무새가 되는건..",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015071.1.44",
          "form": "크읍",
          "original_form": "크읍",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015071.1.45",
          "form": "맞는거 같은데요",
          "original_form": "맞는거 같은데요",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015071.1.46",
          "form": "울어드릴까요 앵무",
          "original_form": "울어드릴까요 앵무",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015071.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015071.1.48",
          "form": "앵무새가 되사면 안돼용",
          "original_form": "앵무새가 되사면 안돼용",
          "speaker_id": "2",
          "time": "20210825 21:53"
        }
      ]
