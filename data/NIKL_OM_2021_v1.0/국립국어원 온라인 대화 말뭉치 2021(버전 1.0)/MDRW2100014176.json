        },
        {
          "id": "MDRW2100014176.1.2",
          "form": "안녕하세요~~어떤음식좋아하세요?",
          "original_form": "안녕하세요~~어떤음식좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014176.1.3",
          "form": "ㅋㅋㅋ지금은",
          "original_form": "ㅋㅋㅋ지금은",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014176.1.4",
          "form": "항정살이 먹고싶네요",
          "original_form": "항정살이 먹고싶네요",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014176.1.5",
          "form": "ㅋㅋㅋ고기는 늘 옳아요",
          "original_form": "ㅋㅋㅋ고기는 늘 옳아요",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014176.1.6",
          "form": "맥주vs소주?",
          "original_form": "맥주vs소주?",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014176.1.7",
          "form": "맥주!",
          "original_form": "맥주!",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014176.1.8",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014176.1.9",
          "form": "저도요ㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014176.1.10",
          "form": "저는 지금은 모히또를 먹고있어요 ㅎㅎㅅ",
          "original_form": "저는 지금은 모히또를 먹고있어요 ㅎㅎㅅ",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100014176.1.11",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100014176.1.12",
          "form": "혼자서요?",
          "original_form": "혼자서요?",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100014176.1.13",
          "form": "ㅋㅋㅋㅋ친구랑 집에서 먹고있어요  코로나라 이시간엔 하는데가없어요",
          "original_form": "ㅋㅋㅋㅋ친구랑 집에서 먹고있어요  코로나라 이시간엔 하는데가없어요",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100014176.1.14",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100014176.1.15",
          "form": "9시면 닫죠",
          "original_form": "9시면 닫죠",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100014176.1.16",
          "form": "코로나로 요리가 느는것같아요 ㅋㅋ라임과 애플민트에 사이다만 타서먹어도 맛있네요",
          "original_form": "코로나로 요리가 느는것같아요 ㅋㅋ라임과 애플민트에 사이다만 타서먹어도 맛있네요",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100014176.1.17",
          "form": "ㅋㅋㅋ 좋네요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 좋네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100014176.1.18",
          "form": "전 하이볼좋아해요",
          "original_form": "전 하이볼좋아해요",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100014176.1.19",
          "form": "오오 술이 세신가요?",
          "original_form": "오오 술이 세신가요?",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100014176.1.20",
          "form": "전혀요ㅋㅋㅋㅋ",
          "original_form": "전혀요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100014176.1.21",
          "form": "하이볼이 진짜맛있어요",
          "original_form": "하이볼이 진짜맛있어요",
