      "utterance": [
        {
          "id": "MDRW2100024826.1.1",
          "form": "소고기는 정말 어떤식으로 먹어도 맛있는것같아요",
          "original_form": "소고기는 정말 어떤식으로 먹어도 맛있는것같아요",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024826.1.2",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024826.1.3",
          "form": "그냥 구워먹어도 맛있어요",
          "original_form": "그냥 구워먹어도 맛있어요",
          "speaker_id": "2",
          "time": "20210831 19:14"
        },
        {
          "id": "MDRW2100024826.1.4",
          "form": "좋아하는 부위 있으세요?",
          "original_form": "좋아하는 부위 있으세요?",
          "speaker_id": "2",
          "time": "20210831 19:14"
        },
        {
          "id": "MDRW2100024826.1.5",
          "form": "아니요 특별히 없어요!",
          "original_form": "아니요 특별히 없어요!",
          "speaker_id": "1",
          "time": "20210831 19:15"
        },
        {
          "id": "MDRW2100024826.1.6",
          "form": "근데 전 특히 육회로 먹었을때 더 맛있다고 느껴요",
          "original_form": "근데 전 특히 육회로 먹었을때 더 맛있다고 느껴요",
          "speaker_id": "1",
          "time": "20210831 19:15"
        },
        {
          "id": "MDRW2100024826.1.7",
          "form": "육회 맛있죠!",
          "original_form": "육회 맛있죠!",
          "speaker_id": "2",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100024826.1.8",
          "form": "혁명이에요ㅋㅋㅋ",
          "original_form": "혁명이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:17"
        },
        {
          "id": "MDRW2100024826.1.9",
          "form": "육회비빔밥 육회냉면 육회막국수 다 맛있어요",
          "original_form": "육회비빔밥 육회냉면 육회막국수 다 맛있어요",
          "speaker_id": "1",
          "time": "20210831 19:21"
        },
        {
          "id": "MDRW2100024826.1.10",
          "form": "맞아요! 육사시미도 드셔보셨나요?",
          "original_form": "맞아요! 육사시미도 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210831 19:22"
        },
        {
          "id": "MDRW2100024826.1.11",
          "form": "아니요아니요!!!",
          "original_form": "아니요아니요!!!",
          "speaker_id": "1",
          "time": "20210831 19:23"
        },
        {
          "id": "MDRW2100024826.1.12",
          "form": "맛있나요???",
          "original_form": "맛있나요???",
          "speaker_id": "1",
          "time": "20210831 19:23"
        },
        {
          "id": "MDRW2100024826.1.13",
          "form": "저도 아직 안 먹어봤어요ㅎㅎ 먹어본 친구가 육회보다 더 맛있다고 해서 다음에 한 번 먹어보려고요!",
          "original_form": "저도 아직 안 먹어봤어요ㅎㅎ 먹어본 친구가 육회보다 더 맛있다고 해서 다음에 한 번 먹어보려고요!",
          "speaker_id": "2",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100024826.1.14",
          "form": "헐 대박 저도요",
          "original_form": "헐 대박 저도요",
          "speaker_id": "1",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100024826.1.15",
          "form": "저희 둘 다 취향에 맞으면 좋겠네요ㅋㅋㅋ",
          "original_form": "저희 둘 다 취향에 맞으면 좋겠네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100024826.1.16",
          "form": "회도 좋아하시나요?",
          "original_form": "회도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100024826.1.17",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210831 19:27"
        },
        {
          "id": "MDRW2100024826.1.18",
          "form": "회 짱 좋아해요",
          "original_form": "회 짱 좋아해요",
          "speaker_id": "1",
          "time": "20210831 19:27"
        },
        {
          "id": "MDRW2100024826.1.19",
          "form": "전 초장에 푹 담궈먹는답니다",
          "original_form": "전 초장에 푹 담궈먹는답니다",
          "speaker_id": "1",
          "time": "20210831 19:27"
        },
        {
          "id": "MDRW2100024826.1.20",
          "form": "저도 초장 제일 좋아해요ㅎㅎ 요즘엔 와사비 간장이랑도 먹고 있는데 역시 초장이 제일 맛있더라고요ㅎㅎ",
          "original_form": "저도 초장 제일 좋아해요ㅎㅎ 요즘엔 와사비 간장이랑도 먹고 있는데 역시 초장이 제일 맛있더라고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 19:29"
        },
        {
          "id": "MDRW2100024826.1.21",
          "form": "맞아요 회엔 역시 초장이죠 ",
          "original_form": "맞아요 회엔 역시 초장이죠 ",
          "speaker_id": "1",
          "time": "20210831 19:29"
        }
      ]
