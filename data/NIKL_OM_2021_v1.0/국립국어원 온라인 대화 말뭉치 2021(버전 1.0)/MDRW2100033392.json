      "utterance": [
        {
          "id": "MDRW2100033392.1.1",
          "form": "이제 카드 재발급 되었으니 또 무얼 사실 생각 입니까??",
          "original_form": "이제 카드 재발급 되었으니 또 무얼 사실 생각 입니까??",
          "speaker_id": "2",
          "time": "20210907 03:13"
        },
        {
          "id": "MDRW2100033392.1.2",
          "form": "애들 옷???",
          "original_form": "애들 옷???",
          "speaker_id": "2",
          "time": "20210907 03:13"
        },
        {
          "id": "MDRW2100033392.1.3",
          "form": "이제는 애들 옷 사야지..",
          "original_form": "이제는 애들 옷 사야지..",
          "speaker_id": "1",
          "time": "20210907 03:13"
        },
        {
          "id": "MDRW2100033392.1.4",
          "form": "가을 이잖아... 훅 추워졌던데??",
          "original_form": "가을 이잖아... 훅 추워졌던데??",
          "speaker_id": "1",
          "time": "20210907 03:13"
        },
        {
          "id": "MDRW2100033392.1.5",
          "form": "그렇긴 하드라.. 오늘 아침에는 좀 추웠어",
          "original_form": "그렇긴 하드라.. 오늘 아침에는 좀 추웠어",
          "speaker_id": "2",
          "time": "20210907 03:13"
        },
        {
          "id": "MDRW2100033392.1.6",
          "form": "그니까... 재난지원금 어제 신청했지??",
          "original_form": "그니까... 재난지원금 어제 신청했지??",
          "speaker_id": "1",
          "time": "20210907 03:13"
        },
        {
          "id": "MDRW2100033392.1.7",
          "form": "어 신청했지!!",
          "original_form": "어 신청했지!!",
          "speaker_id": "2",
          "time": "20210907 03:13"
        },
        {
          "id": "MDRW2100033392.1.8",
          "form": "오늘 그거 들어오면 가을 긴팔 사줘야지..",
          "original_form": "오늘 그거 들어오면 가을 긴팔 사줘야지..",
          "speaker_id": "1",
          "time": "20210907 03:14"
        },
        {
          "id": "MDRW2100033392.1.9",
          "form": "키 많이 커서 봄에 입던옷들 작겠던데??",
          "original_form": "키 많이 커서 봄에 입던옷들 작겠던데??",
          "speaker_id": "1",
          "time": "20210907 03:14"
        },
        {
          "id": "MDRW2100033392.1.10",
          "form": "여보는 뭐 안사???",
          "original_form": "여보는 뭐 안사???",
          "speaker_id": "2",
          "time": "20210907 03:14"
        },
        {
          "id": "MDRW2100033392.1.11",
          "form": "나???? 난 복직할때 사야지..",
          "original_form": "나???? 난 복직할때 사야지..",
          "speaker_id": "1",
          "time": "20210907 03:15"
        },
        {
          "id": "MDRW2100033392.1.12",
          "form": "+살도 빼고....",
          "original_form": "+살도 빼고....",
          "speaker_id": "1",
          "time": "20210907 03:15"
        },
        {
          "id": "MDRW2100033392.1.13",
          "form": "겨울에 살라고??",
          "original_form": "겨울에 살라고??",
          "speaker_id": "2",
          "time": "20210907 03:15"
        },
        {
          "id": "MDRW2100033392.1.14",
          "form": "어어 겨울에 사야지",
          "original_form": "어어 겨울에 사야지",
          "speaker_id": "1",
          "time": "20210907 03:15"
        },
        {
          "id": "MDRW2100033392.1.15",
          "form": "가을에도 좀 사지 왜??",
          "original_form": "가을에도 좀 사지 왜??",
          "speaker_id": "2",
          "time": "20210907 03:15"
        },
        {
          "id": "MDRW2100033392.1.16",
          "form": "다이어트하고 사겠습니다.",
          "original_form": "다이어트하고 사겠습니다.",
          "speaker_id": "1",
          "time": "20210907 03:16"
        },
        {
          "id": "MDRW2100033392.1.17",
          "form": "ㅋㅋㅋ겨울에도 못사는거 아니고?",
          "original_form": "ㅋㅋㅋ겨울에도 못사는거 아니고?",
