      "utterance": [
        {
          "id": "MDRW2100037893.1.1",
          "form": "부산은 날씨 어때?",
          "original_form": "부산은 날씨 어때?",
          "speaker_id": "1",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100037893.1.2",
          "form": "여기 비오다 말았어",
          "original_form": "여기 비오다 말았어",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037893.1.3",
          "form": "지금은 그친듯?",
          "original_form": "지금은 그친듯?",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037893.1.4",
          "form": "오 다행이다",
          "original_form": "오 다행이다",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037893.1.5",
          "form": "비오면 싫어",
          "original_form": "비오면 싫어",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037893.1.6",
          "form": "아니 아까 밥 먹으러 나갔는데 갑자기 비 와서",
          "original_form": "아니 아까 밥 먹으러 나갔는데 갑자기 비 와서",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037893.1.7",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037893.1.8",
          "form": "비 맞으면서 집에 왔어",
          "original_form": "비 맞으면서 집에 왔어",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037893.1.9",
          "form": "맞아 비 오는 날은... 싫어...",
          "original_form": "맞아 비 오는 날은... 싫어...",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037893.1.10",
          "form": "ㅇㄴ 잘 씻었니",
          "original_form": "ㅇㄴ 잘 씻었니",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037893.1.11",
          "form": "요즘 비는 더러워",
          "original_form": "요즘 비는 더러워",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100037893.1.12",
          "form": "나는 비오는 날... 관절이 쑤셔서 별로야",
          "original_form": "나는 비오는 날... 관절이 쑤셔서 별로야",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100037893.1.13",
          "form": "근데 비오면 시원하긴하더라 그건 좋았어 미세먼지도 적고",
          "original_form": "근데 비오면 시원하긴하더라 그건 좋았어 미세먼지도 적고",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100037893.1.14",
          "form": "맞아 집 오자마자...는 아니고 밥 먹기 전에 씻었어",
          "original_form": "맞아 집 오자마자...는 아니고 밥 먹기 전에 씻었어",
          "speaker_id": "2",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100037893.1.15",
          "form": "맞어 비 오는 날은 시원하긴 해 집 안에만 있다면 딱 좋지",
          "original_form": "맞어 비 오는 날은 시원하긴 해 집 안에만 있다면 딱 좋지",
          "speaker_id": "2",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100037893.1.16",
          "form": "멋져 수우는 깔끔해",
          "original_form": "멋져 수우는 깔끔해",
          "speaker_id": "1",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100037893.1.17",
          "form": "ㅋㅋㅋㅋㅋ맞아 집에만 있다면.",
          "original_form": "ㅋㅋㅋㅋㅋ맞아 집에만 있다면.",
          "speaker_id": "1",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100037893.1.18",
          "form": "아니 여기 어제 비왔는데",
          "original_form": "아니 여기 어제 비왔는데",
          "speaker_id": "1",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100037893.1.19",
          "form": "집에만 있다면 무슨 날씨든 좋다",
          "original_form": "집에만 있다면 무슨 날씨든 좋다",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100037893.1.20",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100037893.1.21",
          "form": "ㄹㅇ 많이 오는거야",
          "original_form": "ㄹㅇ 많이 오는거야",
          "speaker_id": "1",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100037893.1.22",
          "form": "비맞아서 축축해졌어",
          "original_form": "비맞아서 축축해졌어",
          "speaker_id": "1",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100037893.1.23",
          "form": "이런",
          "original_form": "이런",
          "speaker_id": "2",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100037893.1.24",
          "form": "고생 많았어 가경....",
          "original_form": "고생 많았어 가경....",
          "speaker_id": "2",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100037893.1.25",
          "form": "ㄹㅇ 화났음",
          "original_form": "ㄹㅇ 화났음",
          "speaker_id": "1",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100037893.1.26",
          "form": "화났음",
          "original_form": "화났음",
          "speaker_id": "1",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100037893.1.27",
          "form": "맞아 나 고생많았어",
          "original_form": "맞아 나 고생많았어",
          "speaker_id": "1",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100037893.1.28",
          "form": "그치만 시원해서 나쁘진 않았던것 같어",
          "original_form": "그치만 시원해서 나쁘진 않았던것 같어",
          "speaker_id": "1",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100037893.1.29",
          "form": "가경이 나쁘지 않았다면 좋아...",
          "original_form": "가경이 나쁘지 않았다면 좋아...",
          "speaker_id": "2",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100037893.1.30",
          "form": "기쁘다",
          "original_form": "기쁘다",
          "speaker_id": "1",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100037893.1.31",
          "form": "헐 가경 지금 부산에 호우주의보래",
          "original_form": "헐 가경 지금 부산에 호우주의보래",
          "speaker_id": "2",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100037893.1.32",
          "form": "수우는 싫어하는 계절 있오?",
          "original_form": "수우는 싫어하는 계절 있오?",
          "speaker_id": "1",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100037893.1.33",
          "form": "아니 어이없네",
          "original_form": "아니 어이없네",
          "speaker_id": "1",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100037893.1.34",
          "form": "빨리 하늘에 정권지르기 실시",
          "original_form": "빨리 하늘에 정권지르기 실시",
          "speaker_id": "1",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100037893.1.35",
          "form": "수우는 여름이 싫다",
          "original_form": "수우는 여름이 싫다",
          "speaker_id": "2",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100037893.1.36",
          "form": "그치만 미디어에서 너무 미화를 많이 해서 나쁘진 않은 거 같아 하지만 싫어",
          "original_form": "그치만 미디어에서 너무 미화를 많이 해서 나쁘진 않은 거 같아 하지만 싫어",
          "speaker_id": "2",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100037893.1.37",
          "form": "아~ 뭔지 알지",
          "original_form": "아~ 뭔지 알지",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100037893.1.38",
          "form": "미디어 여름은 좋지만 현실 여름은 나도 싫어",
          "original_form": "미디어 여름은 좋지만 현실 여름은 나도 싫어",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100037893.1.39",
          "form": "하지만 여름이라는 단어에서 오는... 낭만은 있다고 생각해",
          "original_form": "하지만 여름이라는 단어에서 오는... 낭만은 있다고 생각해",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100037893.1.40",
          "form": "맞아 여름은... 낭만만 남은 계절이라고 생각해",
          "original_form": "맞아 여름은... 낭만만 남은 계절이라고 생각해",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037893.1.41",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 내실은 남지 않은.",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 내실은 남지 않은.",
          "speaker_id": "1",
          "time": "20210901 20:39"
        }
      ]
    }
  ]
