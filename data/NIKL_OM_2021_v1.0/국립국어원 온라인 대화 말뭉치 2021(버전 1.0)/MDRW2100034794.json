      "utterance": [
        {
          "id": "MDRW2100034794.1.1",
          "form": "그거 시즌2도 나오나요??",
          "original_form": "그거 시즌2도 나오나요??",
          "speaker_id": "1",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100034794.1.2",
          "form": "네네! 이번에 또 촬영한다고 하던데",
          "original_form": "네네! 이번에 또 촬영한다고 하던데",
          "speaker_id": "2",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100034794.1.3",
          "form": "언제 개봉하는지는 모르겠어용",
          "original_form": "언제 개봉하는지는 모르겠어용",
          "speaker_id": "2",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100034794.1.4",
          "form": "오오 2 나오기전에 봐야겠네요 ㅎㅎ",
          "original_form": "오오 2 나오기전에 봐야겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034794.1.5",
          "form": "킹덤은 보셨어요?",
          "original_form": "킹덤은 보셨어요?",
          "speaker_id": "2",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034794.1.6",
          "form": "네 ! 그거때문에 넷플 가입했어요 ㅋㅋ",
          "original_form": "네 ! 그거때문에 넷플 가입했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100034794.1.7",
          "form": "앗 저두요ㅋㅋㅋ",
          "original_form": "앗 저두요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034794.1.8",
          "form": "좀비물 넘 좋아해서",
          "original_form": "좀비물 넘 좋아해서",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034794.1.9",
          "form": "맞아요 ㅋㅋㅋ 사람들이 재밌디ㅏ서 봤는데",
          "original_form": "맞아요 ㅋㅋㅋ 사람들이 재밌디ㅏ서 봤는데",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034794.1.10",
          "form": "거의 인생작이에요 ㅋㅋ",
          "original_form": "거의 인생작이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034794.1.11",
          "form": "김은희 작가 천재인가 싶을정도로ㅋㅋㅋㅋ",
          "original_form": "김은희 작가 천재인가 싶을정도로ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100034794.1.12",
          "form": "혹시 오컬트물 좋아하세요?",
          "original_form": "혹시 오컬트물 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100034794.1.13",
          "form": "오오 첨 듣는데 혹시 어떤 장르에요?",
          "original_form": "오오 첨 듣는데 혹시 어떤 장르에요?",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100034794.1.14",
          "form": "그",
          "original_form": "그",
          "speaker_id": "2",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034794.1.15",
          "form": "검은사제들이나",
          "original_form": "검은사제들이나",
          "speaker_id": "2",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034794.1.16",
          "form": "ocn 드라마 손더게스트 아세용?",
          "original_form": "ocn 드라마 손더게스트 아세용?",
          "speaker_id": "2",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034794.1.17",
          "form": "아아 알죠알죠 !",
          "original_form": "아아 알죠알죠 !",
          "speaker_id": "1",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034794.1.18",
          "form": "저 손더게스트 진짜 너무 재밌게 봤어요",
          "original_form": "저 손더게스트 진짜 너무 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034794.1.19",
          "form": "헐 저는 아직 못봤는데 ㅜㅜ",
          "original_form": "헐 저는 아직 못봤는데 ㅜㅜ",
