      "utterance": [
        {
          "id": "MDRW2100029326.1.1",
          "form": "영양제드시나요",
          "original_form": "영양제드시나요",
          "speaker_id": "1",
          "time": "20210903 01:34"
        },
        {
          "id": "MDRW2100029326.1.2",
          "form": "사다놓기만 하고 안먹어요ㅋㅋㅋ",
          "original_form": "사다놓기만 하고 안먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029326.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029326.1.4",
          "form": "ㅋㅋㅋ ㅋ저도귀찮아서..",
          "original_form": "ㅋㅋㅋ ㅋ저도귀찮아서..",
          "speaker_id": "1",
          "time": "20210903 01:36"
        },
        {
          "id": "MDRW2100029326.1.5",
          "form": "머사다놓셧어요",
          "original_form": "머사다놓셧어요",
          "speaker_id": "1",
          "time": "20210903 01:36"
        },
        {
          "id": "MDRW2100029326.1.6",
          "form": "종합비타민이랑 칼슘 마그네슘이요ㅋㅋㅋ",
          "original_form": "종합비타민이랑 칼슘 마그네슘이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:36"
        },
        {
          "id": "MDRW2100029326.1.7",
          "form": "ㅋㅋㅋㅋ 유산균 효과 젤 조은거같아요 !",
          "original_form": "ㅋㅋㅋㅋ 유산균 효과 젤 조은거같아요 !",
          "speaker_id": "1",
          "time": "20210903 01:37"
        },
        {
          "id": "MDRW2100029326.1.8",
          "form": "칼슘 마그네슘은 왜용?",
          "original_form": "칼슘 마그네슘은 왜용?",
          "speaker_id": "1",
          "time": "20210903 01:37"
        },
        {
          "id": "MDRW2100029326.1.9",
          "form": "저 골다공증 위험군이라고 나와서요ㅠㅜ",
          "original_form": "저 골다공증 위험군이라고 나와서요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210903 01:37"
        },
        {
          "id": "MDRW2100029326.1.10",
          "form": "아 유산균",
          "original_form": "아 유산균",
          "speaker_id": "2",
          "time": "20210903 01:37"
        },
        {
          "id": "MDRW2100029326.1.11",
          "form": "어떤거 드셨어요??",
          "original_form": "어떤거 드셨어요??",
          "speaker_id": "2",
          "time": "20210903 01:37"
        },
        {
          "id": "MDRW2100029326.1.12",
          "form": "음... 재로우?랑 걍 그때그때 잇는거사는데용",
          "original_form": "음... 재로우?랑 걍 그때그때 잇는거사는데용",
          "speaker_id": "1",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100029326.1.13",
          "form": "확실히 좀 느껴져요 뭘 먹든 ㅋㅋㅋ",
          "original_form": "확실히 좀 느껴져요 뭘 먹든 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100029326.1.14",
          "form": "옹 아무거나 먹어도 효과가 있나요??",
          "original_form": "옹 아무거나 먹어도 효과가 있나요??",
          "speaker_id": "2",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100029326.1.15",
          "form": "아 골다공증 ㅠ",
          "original_form": "아 골다공증 ㅠ",
          "speaker_id": "1",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100029326.1.16",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "2",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100029326.1.17",
          "form": "음... 제가먹은건 다 갠찮앗어요! ㅋㅋ",
          "original_form": "음... 제가먹은건 다 갠찮앗어요! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029326.1.18",
          "form": "상태 안조을때 먹으면 화장실갓어요..",
          "original_form": "상태 안조을때 먹으면 화장실갓어요..",
          "speaker_id": "1",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029326.1.19",
          "form": "장 민감성인데 계속먹으면 그거도 나아지고..",
          "original_form": "장 민감성인데 계속먹으면 그거도 나아지고..",
          "speaker_id": "1",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029326.1.20",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029326.1.21",
          "form": "신경성인가요?",
          "original_form": "신경성인가요?",
          "speaker_id": "2",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100029326.1.22",
          "form": "그건몰겟어요 ㅠ 식습관문젠가",
          "original_form": "그건몰겟어요 ㅠ 식습관문젠가",
          "speaker_id": "1",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100029326.1.23",
          "form": "밀가루 이런게 안좋대요 장에...",
          "original_form": "밀가루 이런게 안좋대요 장에...",
          "speaker_id": "1",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100029326.1.24",
          "form": "그렇다고 하더라고요....",
          "original_form": "그렇다고 하더라고요....",
          "speaker_id": "2",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100029326.1.25",
          "form": "그런데 밀가루가 제일 맛있잖아오.....",
          "original_form": "그런데 밀가루가 제일 맛있잖아오.....",
          "speaker_id": "2",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100029326.1.26",
          "form": "그니까요...........",
          "original_form": "그니까요...........",
          "speaker_id": "1",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100029326.1.27",
          "form": "그냥 먹고..유산균먹고....ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 먹고..유산균먹고....ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:40"
