      "utterance": [
        {
          "id": "MDRW2100036726.1.1",
          "form": "악 배고파요ㅜㅜ",
          "original_form": "악 배고파요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100036726.1.2",
          "form": "다이어트중인데ㅜㅜ",
          "original_form": "다이어트중인데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100036726.1.3",
          "form": "아침안드셨나여?ㅋ",
          "original_form": "아침안드셨나여?ㅋ",
          "speaker_id": "1",
          "time": "20210831 10:35"
        },
        {
          "id": "MDRW2100036726.1.4",
          "form": "레디톡 48시간을 마시고 있어요 ㅋㅋ",
          "original_form": "레디톡 48시간을 마시고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100036726.1.5",
          "form": "효과는요,ㅋ",
          "original_form": "효과는요,ㅋ",
          "speaker_id": "1",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100036726.1.6",
          "form": "이게 이쪼꼬만 과채주스를 ㅋㅋ하루 다섯번 먹으래요",
          "original_form": "이게 이쪼꼬만 과채주스를 ㅋㅋ하루 다섯번 먹으래요",
          "speaker_id": "2",
          "time": "20210831 10:56"
        },
        {
          "id": "MDRW2100036726.1.7",
          "form": "켁 맛은요?",
          "original_form": "켁 맛은요?",
          "speaker_id": "1",
          "time": "20210831 10:59"
        },
        {
          "id": "MDRW2100036726.1.8",
          "form": "맛은 좋아요",
          "original_form": "맛은 좋아요",
          "speaker_id": "2",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100036726.1.9",
          "form": "진짜 그냥 상콤하고 맛있어요 ㅋㅋㅋ",
          "original_form": "진짜 그냥 상콤하고 맛있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:11"
        },
        {
          "id": "MDRW2100036726.1.10",
          "form": "아그럼 먹을만 하네요",
          "original_form": "아그럼 먹을만 하네요",
          "speaker_id": "1",
          "time": "20210831 11:16"
        },
        {
          "id": "MDRW2100036726.1.11",
          "form": "맛은 좋아요 근데 배가 고파요ㅜㅜ",
          "original_form": "맛은 좋아요 근데 배가 고파요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 11:18"
        },
        {
          "id": "MDRW2100036726.1.12",
          "form": "다른건일절안먹고요?",
          "original_form": "다른건일절안먹고요?",
          "speaker_id": "1",
          "time": "20210831 11:19"
        },
        {
          "id": "MDRW2100036726.1.13",
          "form": "일단 이틀은요",
          "original_form": "일단 이틀은요",
          "speaker_id": "2",
          "time": "20210831 11:21"
        },
        {
          "id": "MDRW2100036726.1.14",
          "form": "와~~~48시간동안",
          "original_form": "와~~~48시간동안",
          "speaker_id": "1",
          "time": "20210831 11:21"
        },
        {
          "id": "MDRW2100036726.1.15",
          "form": "이 제품 이름이 레디톡 48시간 ㅋㅋ",
          "original_form": "이 제품 이름이 레디톡 48시간 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:21"
        },
        {
          "id": "MDRW2100036726.1.16",
          "form": "인데 ㅋㅋ진짜 컨셉에 충실하네요",
          "original_form": "인데 ㅋㅋ진짜 컨셉에 충실하네요",
          "speaker_id": "2",
          "time": "20210831 11:22"
        },
        {
          "id": "MDRW2100036726.1.17",
          "form": "첨해보시는거에여?",
          "original_form": "첨해보시는거에여?",
          "speaker_id": "1",
          "time": "20210831 11:22"
        },
        {
          "id": "MDRW2100036726.1.18",
          "form": "이 제품은 처음 먹어봐요",
          "original_form": "이 제품은 처음 먹어봐요",
          "speaker_id": "2",
          "time": "20210831 11:26"
        },
        {
          "id": "MDRW2100036726.1.19",
          "form": "무슨 디톡스인가",
          "original_form": "무슨 디톡스인가",
          "speaker_id": "2",
          "time": "20210831 11:26"
        },
        {
          "id": "MDRW2100036726.1.20",
          "form": "하는건 일단 첨해보네여",
          "original_form": "하는건 일단 첨해보네여",
          "speaker_id": "2",
          "time": "20210831 11:27"
        }
