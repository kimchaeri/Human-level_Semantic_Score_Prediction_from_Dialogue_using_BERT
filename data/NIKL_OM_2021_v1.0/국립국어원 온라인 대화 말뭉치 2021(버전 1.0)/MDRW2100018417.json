        },
        {
          "id": "MDRW2100018417.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210827 01:31"
        },
        {
          "id": "MDRW2100018417.1.3",
          "form": "대학생이신가요?",
          "original_form": "대학생이신가요?",
          "speaker_id": "2",
          "time": "20210827 01:31"
        },
        {
          "id": "MDRW2100018417.1.4",
          "form": "넵 대학생이에요",
          "original_form": "넵 대학생이에요",
          "speaker_id": "1",
          "time": "20210827 01:31"
        },
        {
          "id": "MDRW2100018417.1.5",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "2",
          "time": "20210827 01:31"
        },
        {
          "id": "MDRW2100018417.1.6",
          "form": "10대로 나오던데",
          "original_form": "10대로 나오던데",
          "speaker_id": "1",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100018417.1.7",
          "form": "고등학생 아니신가요",
          "original_form": "고등학생 아니신가요",
          "speaker_id": "1",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100018417.1.8",
          "form": "엥 저 10대로 나와요?",
          "original_form": "엥 저 10대로 나와요?",
          "speaker_id": "2",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100018417.1.9",
          "form": "네 10대던데",
          "original_form": "네 10대던데",
          "speaker_id": "1",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100018417.1.10",
          "form": "이제 갓 20살이신가봐요",
          "original_form": "이제 갓 20살이신가봐요",
          "speaker_id": "1",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100018417.1.11",
          "form": "만 나이로 뜨는거같은데",
          "original_form": "만 나이로 뜨는거같은데",
          "speaker_id": "1",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100018417.1.12",
          "form": "아아 그렇구나 몰랐네요ㅠㅠ",
          "original_form": "아아 그렇구나 몰랐네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100018417.1.13",
          "form": "네 갓 스무살입니다",
          "original_form": "네 갓 스무살입니다",
          "speaker_id": "2",
          "time": "20210827 01:33"
        },
        {
          "id": "MDRW2100018417.1.14",
          "form": "1학년이세요?",
          "original_form": "1학년이세요?",
          "speaker_id": "1",
          "time": "20210827 01:33"
        },
        {
          "id": "MDRW2100018417.1.15",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210827 01:33"
        },
        {
          "id": "MDRW2100018417.1.16",
          "form": "전공이 어떻게되시나요",
          "original_form": "전공이 어떻게되시나요",
          "speaker_id": "1",
          "time": "20210827 01:33"
        },
        {
          "id": "MDRW2100018417.1.17",
          "form": "저는 어문전공입니다 ㅎㅎ",
          "original_form": "저는 어문전공입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 01:33"
        },
        {
          "id": "MDRW2100018417.1.18",
          "form": "전공이 어떻게 되세요?",
          "original_form": "전공이 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210827 01:33"
        },
        {
          "id": "MDRW2100018417.1.19",
          "form": "저는 사회복지학과입니다",
          "original_form": "저는 사회복지학과입니다",
          "speaker_id": "1",
          "time": "20210827 01:33"
        },
        {
          "id": "MDRW2100018417.1.20",
          "form": "어문전공이면",
          "original_form": "어문전공이면",
          "speaker_id": "1",
          "time": "20210827 01:34"
        },
        {
          "id": "MDRW2100018417.1.21",
          "form": "국어나 영어 이쪽인건가요?",
          "original_form": "국어나 영어 이쪽인건가요?",
          "speaker_id": "1",
          "time": "20210827 01:34"
        },
        {
          "id": "MDRW2100018417.1.22",
          "form": "아니먼 외국어",
          "original_form": "아니먼 외국어",
          "speaker_id": "1",
          "time": "20210827 01:34"
        },
        {
          "id": "MDRW2100018417.1.23",
          "form": "아뇨! 외국어요",
          "original_form": "아뇨! 외국어요",
          "speaker_id": "2",
          "time": "20210827 01:34"
        },
        {
          "id": "MDRW2100018417.1.24",
          "form": "오호 어떤 외국어죠",
          "original_form": "오호 어떤 외국어죠",
          "speaker_id": "1",
          "time": "20210827 01:34"
        },
        {
          "id": "MDRW2100018417.1.25",
          "form": "러시아어요",
          "original_form": "러시아어요",
          "speaker_id": "2",
          "time": "20210827 01:34"
        },
        {
          "id": "MDRW2100018417.1.26",
          "form": "오.. 러시아어",
          "original_form": "오.. 러시아어",
          "speaker_id": "1",
          "time": "20210827 01:35"
        },
        {
          "id": "MDRW2100018417.1.27",
          "form": "멋있네요",
          "original_form": "멋있네요",
          "speaker_id": "1",
          "time": "20210827 01:35"
        },
        {
          "id": "MDRW2100018417.1.28",
          "form": "ㅋㅋ 그런가요",
          "original_form": "ㅋㅋ 그런가요",
          "speaker_id": "2",
          "time": "20210827 01:35"
        },
        {
          "id": "MDRW2100018417.1.29",
          "form": "외국어 잘하는 사람들보면",
          "original_form": "외국어 잘하는 사람들보면",
          "speaker_id": "1",
          "time": "20210827 01:35"
        },
        {
          "id": "MDRW2100018417.1.30",
          "form": "신기한데",
          "original_form": "신기한데",
          "speaker_id": "1",
          "time": "20210827 01:35"
        },
        {
          "id": "MDRW2100018417.1.31",
          "form": "저도 잘하고 싶네요ㅠㅠ",
          "original_form": "저도 잘하고 싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 01:35"
        },
        {
          "id": "MDRW2100018417.1.32",
          "form": "학과까지 외국관련이면",
          "original_form": "학과까지 외국관련이면",
          "speaker_id": "1",
          "time": "20210827 01:35"
        },
        {
          "id": "MDRW2100018417.1.33",
          "form": "러시아어 아는거 하나있어요",
          "original_form": "러시아어 아는거 하나있어요",
          "speaker_id": "1",
          "time": "20210827 01:36"
        },
        {
          "id": "MDRW2100018417.1.34",
          "form": "네 뭐죠",
          "original_form": "네 뭐죠",
          "speaker_id": "2",
          "time": "20210827 01:36"
        },
        {
          "id": "MDRW2100018417.1.35",
          "form": "욕으로 나오네요",
          "original_form": "욕으로 나오네요",
          "speaker_id": "1",
          "time": "20210827 01:36"
        },
        {
          "id": "MDRW2100018417.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:36"
        },
        {
          "id": "MDRW2100018417.1.37",
          "form": "스파시",
          "original_form": "스파시",
          "speaker_id": "1",
          "time": "20210827 01:36"
        },
        {
          "id": "MDRW2100018417.1.38",
          "form": "설마 고마워",
          "original_form": "설마 고마워",
          "speaker_id": "2",
          "time": "20210827 01:36"
        },
        {
          "id": "MDRW2100018417.1.39",
          "form": "바입니다",
          "original_form": "바입니다",
          "speaker_id": "1",
          "time": "20210827 01:36"
        },
        {
          "id": "MDRW2100018417.1.40",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:36"
        },
        {
          "id": "MDRW2100018417.1.41",
          "form": "러시아어 잘하세요?",
          "original_form": "러시아어 잘하세요?",
          "speaker_id": "1",
          "time": "20210827 01:36"
        },
        {
          "id": "MDRW2100018417.1.42",
          "form": "다들 그말은 잘알더라구요ㅋㅋ",
          "original_form": "다들 그말은 잘알더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:36"
        },
        {
          "id": "MDRW2100018417.1.43",
          "form": "아직음 잘못해요ㅎㅎ 그냥 기초만",
          "original_form": "아직음 잘못해요ㅎㅎ 그냥 기초만",
          "speaker_id": "2",
          "time": "20210827 01:37"
        },
        {
          "id": "MDRW2100018417.1.44",
          "form": "어문학과쪽에 아는 사람이없어서 그런데",
          "original_form": "어문학과쪽에 아는 사람이없어서 그런데",
          "speaker_id": "1",
          "time": "20210827 01:37"
        },
        {
          "id": "MDRW2100018417.1.45",
          "form": "러시아 언어를 배우는건가요",
          "original_form": "러시아 언어를 배우는건가요",
          "speaker_id": "1",
          "time": "20210827 01:37"
        },
        {
          "id": "MDRW2100018417.1.46",
          "form": "아니면 러시아 역사를 배우는건가요?",
          "original_form": "아니면 러시아 역사를 배우는건가요?",
          "speaker_id": "1",
          "time": "20210827 01:37"
        },
        {
          "id": "MDRW2100018417.1.47",
          "form": "네네 언어를 배우는거에용",
          "original_form": "네네 언어를 배우는거에용",
          "speaker_id": "2",
          "time": "20210827 01:37"
        },
        {
          "id": "MDRW2100018417.1.48",
          "form": "아 그러면 대학과정 4년동안",
          "original_form": "아 그러면 대학과정 4년동안",
          "speaker_id": "1",
          "time": "20210827 01:37"
        },
        {
          "id": "MDRW2100018417.1.49",
          "form": "러시아어만..?",
          "original_form": "러시아어만..?",
          "speaker_id": "1",
          "time": "20210827 01:38"
        },
        {
          "id": "MDRW2100018417.1.50",
          "form": "그런데 전공 중에 역사나 문화같은 것도 있고? 선택할 수 있을걸요",
          "original_form": "그런데 전공 중에 역사나 문화같은 것도 있고? 선택할 수 있을걸요",
          "speaker_id": "2",
          "time": "20210827 01:38"
        },
        {
          "id": "MDRW2100018417.1.51",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210827 01:38"
        },
        {
          "id": "MDRW2100018417.1.52",
          "form": "그래도 졸업할때쯤",
          "original_form": "그래도 졸업할때쯤",
          "speaker_id": "1",
          "time": "20210827 01:38"
        },
        {
          "id": "MDRW2100018417.1.53",
          "form": "나중에는 뭐 통번역하는 것도 배우고 그런것 같아요",
          "original_form": "나중에는 뭐 통번역하는 것도 배우고 그런것 같아요",
          "speaker_id": "2",
          "time": "20210827 01:38"
        },
        {
          "id": "MDRW2100018417.1.54",
          "form": "러시아어 마스터하실거같네요",
          "original_form": "러시아어 마스터하실거같네요",
          "speaker_id": "1",
          "time": "20210827 01:38"
        },
        {
          "id": "MDRW2100018417.1.55",
          "form": "ㅋㅋ정말 그랬으면 좋겠어요",
          "original_form": "ㅋㅋ정말 그랬으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210827 01:38"
        },
        {
          "id": "MDRW2100018417.1.56",
          "form": "올해 1학년이면",
          "original_form": "올해 1학년이면",
          "speaker_id": "1",
          "time": "20210827 01:39"
        },
        {
          "id": "MDRW2100018417.1.57",
          "form": "코로나때문에 비대면수업으로 하셨나요?",
          "original_form": "코로나때문에 비대면수업으로 하셨나요?",
          "speaker_id": "1",
          "time": "20210827 01:39"
        },
        {
          "id": "MDRW2100018417.1.58",
          "form": "네네 전부 비대면으로 했어요ㅠㅠ",
          "original_form": "네네 전부 비대면으로 했어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 01:39"
        },
        {
          "id": "MDRW2100018417.1.59",
          "form": "그래서 아직 친구가 없네요ㅋㅋ",
          "original_form": "그래서 아직 친구가 없네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:39"
        },
        {
          "id": "MDRW2100018417.1.60",
          "form": "헉.. ㅋㅋㅋ",
          "original_form": "헉.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:40"
        },
        {
          "id": "MDRW2100018417.1.61",
          "form": "어떻게 그럼 동기들이랑 친해지는건지",
          "original_form": "어떻게 그럼 동기들이랑 친해지는건지",
