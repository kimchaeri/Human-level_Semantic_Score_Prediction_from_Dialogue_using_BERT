      "utterance": [
        {
          "id": "MDRW2100018124.1.1",
          "form": "좋아하시는 음식 있으시나요",
          "original_form": "좋아하시는 음식 있으시나요",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100018124.1.2",
          "form": "저는 닭갈비 좋아해요",
          "original_form": "저는 닭갈비 좋아해요",
          "speaker_id": "1",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100018124.1.3",
          "form": "매운 닭갈비를 좋아하시나요",
          "original_form": "매운 닭갈비를 좋아하시나요",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018124.1.4",
          "form": "저는 매운거 먹으면 속이쓰려서요 ㅠ",
          "original_form": "저는 매운거 먹으면 속이쓰려서요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018124.1.5",
          "form": "매운거좋아하세요?",
          "original_form": "매운거좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018124.1.6",
          "form": "저는 매운거 싫어합니다 ㅋㅋ",
          "original_form": "저는 매운거 싫어합니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018124.1.7",
          "form": "속쓰리거나 그런건 없는데",
          "original_form": "속쓰리거나 그런건 없는데",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018124.1.8",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018124.1.9",
          "form": "먹는데서 고통을 느낀다는게 싫어서..",
          "original_form": "먹는데서 고통을 느낀다는게 싫어서..",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018124.1.10",
          "form": "저랑 반대네요 ㅋㅋㅋ 전 먹을땐 오히려 괜찮아요",
          "original_form": "저랑 반대네요 ㅋㅋㅋ 전 먹을땐 오히려 괜찮아요",
          "speaker_id": "1",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018124.1.11",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018124.1.12",
          "form": "저는 닭갈비보다",
          "original_form": "저는 닭갈비보다",
          "speaker_id": "2",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100018124.1.13",
          "form": "치킨좋아해요",
          "original_form": "치킨좋아해요",
          "speaker_id": "2",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100018124.1.14",
          "form": "치킨도 맛있죠",
          "original_form": "치킨도 맛있죠",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018124.1.15",
          "form": "어느브랜드요?",
          "original_form": "어느브랜드요?",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018124.1.16",
          "form": "요새 먹어본 치킨중에",
          "original_form": "요새 먹어본 치킨중에",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018124.1.17",
          "form": "60계치킨 호랑이치킨이 맛있더라구요",
          "original_form": "60계치킨 호랑이치킨이 맛있더라구요",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018124.1.18",
          "form": "오 그거 궁금했는데",
          "original_form": "오 그거 궁금했는데",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018124.1.19",
          "form": "마늘맛인가요?",
          "original_form": "마늘맛인가요?",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018124.1.20",
          "form": "마늘맛까진 아니고",
          "original_form": "마늘맛까진 아니고",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018124.1.21",
          "form": "마늘향이 조금있고 달달한 뿌링클같더라구여",
          "original_form": "마늘향이 조금있고 달달한 뿌링클같더라구여",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018124.1.22",
          "form": "뿌링클이요?? 치즈맛이 나나요??",
          "original_form": "뿌링클이요?? 치즈맛이 나나요??",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018124.1.23",
          "form": "개인적으로 뿌셔뿌셔 스프맛이라 느꼈습니다",
          "original_form": "개인적으로 뿌셔뿌셔 스프맛이라 느꼈습니다",
          "speaker_id": "2",
          "time": "20210826 22:57"
        }
      ]
