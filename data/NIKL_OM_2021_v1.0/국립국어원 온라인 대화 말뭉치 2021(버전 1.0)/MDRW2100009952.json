      "utterance": [
        {
          "id": "MDRW2100009952.1.1",
          "form": "스포츠 좋아하시나요?",
          "original_form": "스포츠 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 12:09"
        },
        {
          "id": "MDRW2100009952.1.2",
          "form": "음 요즘은 좀 시들해요 어떠세요",
          "original_form": "음 요즘은 좀 시들해요 어떠세요",
          "speaker_id": "2",
          "time": "20210823 12:20"
        },
        {
          "id": "MDRW2100009952.1.3",
          "form": "저는 원래 크게 관심이 없어요",
          "original_form": "저는 원래 크게 관심이 없어요",
          "speaker_id": "1",
          "time": "20210823 12:21"
        },
        {
          "id": "MDRW2100009952.1.4",
          "form": "탁구는 좀 했었는데 안 하고",
          "original_form": "탁구는 좀 했었는데 안 하고",
          "speaker_id": "1",
          "time": "20210823 12:21"
        },
        {
          "id": "MDRW2100009952.1.5",
          "form": "야구는 종 봤었는데 안 보고",
          "original_form": "야구는 종 봤었는데 안 보고",
          "speaker_id": "1",
          "time": "20210823 12:21"
        },
        {
          "id": "MDRW2100009952.1.6",
          "form": "그러네요",
          "original_form": "그러네요",
          "speaker_id": "1",
          "time": "20210823 12:21"
        },
        {
          "id": "MDRW2100009952.1.7",
          "form": "야구 한때 좋아했었어요",
          "original_form": "야구 한때 좋아했었어요",
          "speaker_id": "2",
          "time": "20210823 12:32"
        },
        {
          "id": "MDRW2100009952.1.8",
          "form": "삼성 라이온즈 팬",
          "original_form": "삼성 라이온즈 팬",
          "speaker_id": "2",
          "time": "20210823 12:32"
        },
        {
          "id": "MDRW2100009952.1.9",
          "form": "오호 저는 기아팬입니다",
          "original_form": "오호 저는 기아팬입니다",
          "speaker_id": "1",
          "time": "20210823 12:34"
        },
        {
          "id": "MDRW2100009952.1.10",
          "form": "요즘은 쓱 응원ㅋ",
          "original_form": "요즘은 쓱 응원ㅋ",
          "speaker_id": "1",
          "time": "20210823 12:34"
        },
        {
          "id": "MDRW2100009952.1.11",
          "form": "근데 좀 실망을 해서",
          "original_form": "근데 좀 실망을 해서",
          "speaker_id": "2",
          "time": "20210823 12:36"
        },
        {
          "id": "MDRW2100009952.1.12",
          "form": "잉 왜 실망을?",
          "original_form": "잉 왜 실망을?",
          "speaker_id": "1",
          "time": "20210823 13:09"
        },
        {
          "id": "MDRW2100009952.1.13",
          "form": "그들의 인성에 실망했달까",
          "original_form": "그들의 인성에 실망했달까",
          "speaker_id": "2",
          "time": "20210823 14:00"
        },
        {
          "id": "MDRW2100009952.1.14",
          "form": "아..이번 사건때문에요?",
          "original_form": "아..이번 사건때문에요?",
          "speaker_id": "1",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100009952.1.15",
          "form": "그거 말고도 전부터 도박이니 뭐니",
          "original_form": "그거 말고도 전부터 도박이니 뭐니",
          "speaker_id": "2",
          "time": "20210823 16:31"
        },
        {
          "id": "MDRW2100009952.1.16",
          "form": "엇 도박은 뭔 일인지 모르것네요",
          "original_form": "엇 도박은 뭔 일인지 모르것네요",
          "speaker_id": "1",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100009952.1.17",
          "form": "잊을만하면 터지는거 같아요 ㅋ",
          "original_form": "잊을만하면 터지는거 같아요 ㅋ",
          "speaker_id": "2",
          "time": "20210823 17:27"
        },
        {
          "id": "MDRW2100009952.1.18",
          "form": "야구 외적인 요인에 실망을 하셨네요",
          "original_form": "야구 외적인 요인에 실망을 하셨네요",
          "speaker_id": "1",
          "time": "20210823 17:38"
        }
      ]
