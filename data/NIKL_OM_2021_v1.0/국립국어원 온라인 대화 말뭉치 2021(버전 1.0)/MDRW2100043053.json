      "utterance": [
        {
          "id": "MDRW2100043053.1.1",
          "form": "가고싶은 여행지가 있나요",
          "original_form": "가고싶은 여행지가 있나요",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043053.1.2",
          "form": "저는 이탈리아요",
          "original_form": "저는 이탈리아요",
          "speaker_id": "2",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100043053.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100043053.1.4",
          "form": "전 따뜻한 나라로 가고 싶어요",
          "original_form": "전 따뜻한 나라로 가고 싶어요",
          "speaker_id": "1",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043053.1.5",
          "form": "예를 들면요?",
          "original_form": "예를 들면요?",
          "speaker_id": "2",
          "time": "20210907 16:38"
        },
        {
          "id": "MDRW2100043053.1.6",
          "form": "바로셀로나 . 이탈리아 따뜻하지 않을까요",
          "original_form": "바로셀로나 . 이탈리아 따뜻하지 않을까요",
          "speaker_id": "1",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043053.1.7",
          "form": "전 추운게 싫어요",
          "original_form": "전 추운게 싫어요",
          "speaker_id": "1",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043053.1.8",
          "form": "그럼 발리나 세부같은 곳도 좋아하시나요?",
          "original_form": "그럼 발리나 세부같은 곳도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100043053.1.9",
          "form": "그런곳도 좋겠네요",
          "original_form": "그런곳도 좋겠네요",
          "speaker_id": "1",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100043053.1.10",
          "form": "여행가보신 적 있으신가요?",
          "original_form": "여행가보신 적 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 16:51"
        },
        {
          "id": "MDRW2100043053.1.11",
          "form": "아니요\r\n가보고 싶은 소망 이네요",
          "original_form": "아니요\r\n가보고 싶은 소망 이네요",
          "speaker_id": "1",
          "time": "20210907 16:54"
        },
        {
          "id": "MDRW2100043053.1.12",
          "form": "다른 나라 가보신곳이 어디있나요",
          "original_form": "다른 나라 가보신곳이 어디있나요",
          "speaker_id": "1",
          "time": "20210907 16:54"
        },
        {
          "id": "MDRW2100043053.1.13",
          "form": "국내 여행도 없어신가요?",
          "original_form": "국내 여행도 없어신가요?",
          "speaker_id": "2",
          "time": "20210907 16:54"
        },
        {
          "id": "MDRW2100043053.1.14",
          "form": "제주도 이쁘고 좋아요",
          "original_form": "제주도 이쁘고 좋아요",
          "speaker_id": "1",
          "time": "20210907 16:56"
        },
        {
          "id": "MDRW2100043053.1.15",
          "form": "제주도 가보신건가요?",
          "original_form": "제주도 가보신건가요?",
          "speaker_id": "2",
          "time": "20210907 17:50"
        },
        {
          "id": "MDRW2100043053.1.16",
