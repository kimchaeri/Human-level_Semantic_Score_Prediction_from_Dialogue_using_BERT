      "utterance": [
        {
          "id": "MDRW2100004861.1.1",
          "form": "산리오 러버스.클럽 티셔츠.38000원.",
          "original_form": "산리오 러버스.클럽 티셔츠.38000원.",
          "speaker_id": "2",
          "time": "20210821 18:55"
        },
        {
          "id": "MDRW2100004861.1.2",
          "form": "너는 뭐샀어?",
          "original_form": "너는 뭐샀어?",
          "speaker_id": "2",
          "time": "20210821 18:57"
        },
        {
          "id": "MDRW2100004861.1.3",
          "form": "나는 마이멜로디 머리핀. 9000원.",
          "original_form": "나는 마이멜로디 머리핀. 9000원.",
          "speaker_id": "1",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004861.1.4",
          "form": "값어치 제대로인듯",
          "original_form": "값어치 제대로인듯",
          "speaker_id": "1",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004861.1.5",
          "form": "나도 그거 쿠로미 사고싶었는데",
          "original_form": "나도 그거 쿠로미 사고싶었는데",
          "speaker_id": "2",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004861.1.6",
          "form": "나중에 또 가자 그땐 고민좀 해봐야지",
          "original_form": "나중에 또 가자 그땐 고민좀 해봐야지",
          "speaker_id": "2",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004861.1.7",
          "form": "좋아 9월에 가서 사자",
          "original_form": "좋아 9월에 가서 사자",
          "speaker_id": "1",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004861.1.8",
          "form": "그리고 좀 천천히 즐겨서 디저트도 사먹자",
          "original_form": "그리고 좀 천천히 즐겨서 디저트도 사먹자",
          "speaker_id": "1",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004861.1.9",
          "form": "좋아 우리 앉아서 수다막떨자",
          "original_form": "좋아 우리 앉아서 수다막떨자",
          "speaker_id": "2",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004861.1.10",
          "form": "좋아 가는 길은 힘들어도 가면 기분 좋아서 또 막 잊잖아",
          "original_form": "좋아 가는 길은 힘들어도 가면 기분 좋아서 또 막 잊잖아",
          "speaker_id": "1",
          "time": "20210821 19:30"
        },
        {
          "id": "MDRW2100004861.1.11",
          "form": "그래놓고 오기 싫어서 뭐 사먹고",
          "original_form": "그래놓고 오기 싫어서 뭐 사먹고",
          "speaker_id": "1",
          "time": "20210821 19:30"
        },
        {
          "id": "MDRW2100004861.1.12",
          "form": "만약에 맛없어도 욕하고 기억 미화도 되고",
          "original_form": "만약에 맛없어도 욕하고 기억 미화도 되고",
          "speaker_id": "1",
          "time": "20210821 19:31"
        },
        {
          "id": "MDRW2100004861.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아맞아\r\n오면 걍 그래도 재밌었다~^^\r\n이럼 맛있었다~^^이러고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아맞아\r\n오면 걍 그래도 재밌었다~^^\r\n이럼 맛있었다~^^이러고",
          "speaker_id": "2",
          "time": "20210821 19:31"
        },
        {
          "id": "MDRW2100004861.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ이거만 무한반복 학습능력이 없는 듯",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ이거만 무한반복 학습능력이 없는 듯",
          "speaker_id": "1",
          "time": "20210821 19:31"
        },
        {
          "id": "MDRW2100004861.1.15",
          "form": "그래도 좋지 뭐 ㅎㅎ\r\n혹시 사고싶은 거 있어?",
          "original_form": "그래도 좋지 뭐 ㅎㅎ\r\n혹시 사고싶은 거 있어?",
          "speaker_id": "2",
          "time": "20210821 19:31"
        },
        {
          "id": "MDRW2100004861.1.16",
          "form": "나 옷 사고싶어",
          "original_form": "나 옷 사고싶어",
          "speaker_id": "1",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004861.1.17",
          "form": "버즈 케이스도 사고싶고 폰 케이스더 사고싶어",
          "original_form": "버즈 케이스도 사고싶고 폰 케이스더 사고싶어",
