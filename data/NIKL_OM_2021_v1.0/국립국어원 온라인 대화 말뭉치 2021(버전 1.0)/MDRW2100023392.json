      "utterance": [
        {
          "id": "MDRW2100023392.1.1",
          "form": "딸을 위해서 걱정하는건데",
          "original_form": "딸을 위해서 걱정하는건데",
          "speaker_id": "2",
          "time": "20210830 01:20"
        },
        {
          "id": "MDRW2100023392.1.2",
          "form": "아 아내분도 원하는거에요?",
          "original_form": "아 아내분도 원하는거에요?",
          "speaker_id": "1",
          "time": "20210830 01:20"
        },
        {
          "id": "MDRW2100023392.1.3",
          "form": "참 말안들어요ㅎ",
          "original_form": "참 말안들어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:20"
        },
        {
          "id": "MDRW2100023392.1.4",
          "form": "전 안하고싶은데",
          "original_form": "전 안하고싶은데",
          "speaker_id": "2",
          "time": "20210830 01:20"
        },
        {
          "id": "MDRW2100023392.1.5",
          "form": "아내는 이미결정내렸어요ㅎ 완강하구요ㅎ",
          "original_form": "아내는 이미결정내렸어요ㅎ 완강하구요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:20"
        },
        {
          "id": "MDRW2100023392.1.6",
          "form": "복잡하네요",
          "original_form": "복잡하네요",
          "speaker_id": "1",
          "time": "20210830 01:20"
        },
        {
          "id": "MDRW2100023392.1.7",
          "form": "글죠ㅍ",
          "original_form": "글죠ㅍ",
          "speaker_id": "2",
          "time": "20210830 01:20"
        },
        {
          "id": "MDRW2100023392.1.8",
          "form": "복잡한데 사람일이라ㅎㅎ",
          "original_form": "복잡한데 사람일이라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023392.1.9",
          "form": "내맘대로 안되더라구요ㅎ",
          "original_form": "내맘대로 안되더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023392.1.10",
          "form": "하기사 나도 복잡한데 ㅎㅎㅎ",
          "original_form": "하기사 나도 복잡한데 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023392.1.11",
          "form": "왜요?ㅎㅎ",
          "original_form": "왜요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023392.1.12",
          "form": "그마음 이래 함요 ㅎㅎㅎ",
          "original_form": "그마음 이래 함요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023392.1.13",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023392.1.14",
          "form": "이해 ㅎㅎㅎ",
          "original_form": "이해 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023392.1.15",
          "form": "아아ㅎ",
          "original_form": "아아ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023392.1.16",
          "form": "내마음 같지 않다는거욮ㅎ ㅎ",
          "original_form": "내마음 같지 않다는거욮ㅎ ㅎ",
          "speaker_id": "1",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023392.1.17",
          "form": "그래서저도ㅎ이제는",
          "original_form": "그래서저도ㅎ이제는",
          "speaker_id": "2",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023392.1.18",
          "form": "글죠ㅎ 사람일인데ㅎ 어떻게 딱 맞겠어요ㅎ",
          "original_form": "글죠ㅎ 사람일인데ㅎ 어떻게 딱 맞겠어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:22"
        },
        {
          "id": "MDRW2100023392.1.19",
          "form": "포기한상태?ㅎ 하고싶은대로 하게 냅두는것밖에없어요ㅎ",
          "original_form": "포기한상태?ㅎ 하고싶은대로 하게 냅두는것밖에없어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:22"
        },
        {
          "id": "MDRW2100023392.1.20",
          "form": "다들 그렁게  가정을 이루면 다 행복한지 알았어요",
          "original_form": "다들 그렁게  가정을 이루면 다 행복한지 알았어요",
          "speaker_id": "1",
          "time": "20210830 01:22"
        },
        {
          "id": "MDRW2100023392.1.21",
          "form": "어릴땐  ㅎㅎ",
          "original_form": "어릴땐  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:22"
        },
        {
          "id": "MDRW2100023392.1.22",
          "form": "이혼하면 저는 좋죠ㅎ 빚도 갚을수있고 아기도 월10회 이상 볼수가 있고 아내 잔소리도 안들어도되고",
          "original_form": "이혼하면 저는 좋죠ㅎ 빚도 갚을수있고 아기도 월10회 이상 볼수가 있고 아내 잔소리도 안들어도되고",
          "speaker_id": "2",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100023392.1.23",
          "form": "ㅎㅎ 어릴땐 그랬나봐요ㅎ",
          "original_form": "ㅎㅎ 어릴땐 그랬나봐요ㅎ",
