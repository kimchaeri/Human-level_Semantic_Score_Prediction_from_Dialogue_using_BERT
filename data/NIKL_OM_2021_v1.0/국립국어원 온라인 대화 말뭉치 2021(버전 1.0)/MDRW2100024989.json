      "utterance": [
        {
          "id": "MDRW2100024989.1.1",
          "form": "요즘 다이어트 열심히해?",
          "original_form": "요즘 다이어트 열심히해?",
          "speaker_id": "1",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100024989.1.2",
          "form": "아니 오늘부터 열심히 하기로 햇어",
          "original_form": "아니 오늘부터 열심히 하기로 햇어",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100024989.1.3",
          "form": "다이어트 요즘 어때",
          "original_form": "다이어트 요즘 어때",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100024989.1.4",
          "form": "내일 1일이잖아 내일부터하려구^^*",
          "original_form": "내일 1일이잖아 내일부터하려구^^*",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024989.1.5",
          "form": "인정 ㅋㅋ 9월에는 정말...감량해야지....",
          "original_form": "인정 ㅋㅋ 9월에는 정말...감량해야지....",
          "speaker_id": "2",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024989.1.6",
          "form": "필라테스 다시 다니는 건 어때",
          "original_form": "필라테스 다시 다니는 건 어때",
          "speaker_id": "2",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024989.1.7",
          "form": "엄청 힘들던데 그거",
          "original_form": "엄청 힘들던데 그거",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100024989.1.8",
          "form": "마스크쓰고 절대 못할거같애..",
          "original_form": "마스크쓰고 절대 못할거같애..",
          "speaker_id": "1",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100024989.1.9",
          "form": "아맞다 필라테스 시작햇댓나?",
          "original_form": "아맞다 필라테스 시작햇댓나?",
          "speaker_id": "1",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100024989.1.10",
          "form": "아니 한번 갔었는데",
          "original_form": "아니 한번 갔었는데",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100024989.1.11",
          "form": "너무 힘들어서 다시 피티로...",
          "original_form": "너무 힘들어서 다시 피티로...",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100024989.1.12",
          "form": "ㅋㅋㅋㅋㅋ기구필테였어?",
          "original_form": "ㅋㅋㅋㅋㅋ기구필테였어?",
          "speaker_id": "1",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100024989.1.13",
          "form": "어엉ㅋㅋㅋ리포머",
          "original_form": "어엉ㅋㅋㅋ리포머",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100024989.1.14",
          "form": "단어 오랜망에 듣는다...",
          "original_form": "단어 오랜망에 듣는다...",
          "speaker_id": "1",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100024989.1.15",
          "form": "자아있는 어깨..",
          "original_form": "자아있는 어깨..",
          "speaker_id": "1",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100024989.1.16",
          "form": "ㅋㅋㅋ아 진짜 내 웃음벨 ㅠ",
          "original_form": "ㅋㅋㅋ아 진짜 내 웃음벨 ㅠ",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100024989.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100024989.1.18",
          "form": "내 어깨도 자아 강하더라",
          "original_form": "내 어깨도 자아 강하더라",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100024989.1.19",
          "form": "피티는 마스크쓰고 괜챤?",
          "original_form": "피티는 마스크쓰고 괜챤?",
          "speaker_id": "1",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100024989.1.20",
          "form": "숨 좀 차면 안경 뿌옇긴 한데",
          "original_form": "숨 좀 차면 안경 뿌옇긴 한데",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100024989.1.21",
          "form": "그래도 숨 고르라고 시간을 좀 주거나",
          "original_form": "그래도 숨 고르라고 시간을 좀 주거나",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100024989.1.22",
          "form": "아니면 그냥 내 맘대로 쉬어 ㅋㅋ",
          "original_form": "아니면 그냥 내 맘대로 쉬어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100024989.1.23",
          "form": "ㄷㄷㄷ힌들거같다",
          "original_form": "ㄷㄷㄷ힌들거같다",
          "speaker_id": "1",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100024989.1.24",
          "form": "나는 내일부터 매일운동챌린지해야겟어",
          "original_form": "나는 내일부터 매일운동챌린지해야겟어",
          "speaker_id": "1",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100024989.1.25",
          "form": "새마음으로...",
          "original_form": "새마음으로...",
          "speaker_id": "1",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100024989.1.26",
          "form": "어떤 운동",
          "original_form": "어떤 운동",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100024989.1.27",
          "form": "스트레칭이라도 매일할려고..",
          "original_form": "스트레칭이라도 매일할려고..",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100024989.1.28",
          "form": "홈트할려구",
          "original_form": "홈트할려구",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100024989.1.29",
          "form": "마스크쓰고는 진짜 못할거가텨",
          "original_form": "마스크쓰고는 진짜 못할거가텨",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100024989.1.30",
          "form": "ㅋㅋ힘들긴 하당ㅇ",
          "original_form": "ㅋㅋ힘들긴 하당ㅇ",
          "speaker_id": "2",
          "time": "20210831 21:57"
        }
      ]
