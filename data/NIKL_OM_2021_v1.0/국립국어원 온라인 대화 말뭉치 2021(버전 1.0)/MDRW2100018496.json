      "utterance": [
        {
          "id": "MDRW2100018496.1.1",
          "form": "그건  지금 보다가 잠시 중단했는데",
          "original_form": "그건  지금 보다가 잠시 중단했는데",
          "speaker_id": "2",
          "time": "20210827 06:31"
        },
        {
          "id": "MDRW2100018496.1.2",
          "form": "2화까지 봤거든요",
          "original_form": "2화까지 봤거든요",
          "speaker_id": "2",
          "time": "20210827 06:31"
        },
        {
          "id": "MDRW2100018496.1.3",
          "form": "아하 이거 보면 알마치라고",
          "original_form": "아하 이거 보면 알마치라고",
          "speaker_id": "1",
          "time": "20210827 06:31"
        },
        {
          "id": "MDRW2100018496.1.4",
          "form": "네넵!",
          "original_form": "네넵!",
          "speaker_id": "2",
          "time": "20210827 06:31"
        },
        {
          "id": "MDRW2100018496.1.5",
          "form": "PPL 나오는데 먹고싶어요ㅠㅠㅋㅋㅋ",
          "original_form": "PPL 나오는데 먹고싶어요ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:31"
        },
        {
          "id": "MDRW2100018496.1.6",
          "form": "앜ㅋㅋ혹시 피자는 아니죠?",
          "original_form": "앜ㅋㅋ혹시 피자는 아니죠?",
          "speaker_id": "2",
          "time": "20210827 06:31"
        },
        {
          "id": "MDRW2100018496.1.7",
          "form": "이것도 반응이 좋아서 그런지 시즌2 나온대요",
          "original_form": "이것도 반응이 좋아서 그런지 시즌2 나온대요",
          "speaker_id": "1",
          "time": "20210827 06:31"
        },
        {
          "id": "MDRW2100018496.1.8",
          "form": "넹넹 치킨이에용 노랑통닭!",
          "original_form": "넹넹 치킨이에용 노랑통닭!",
          "speaker_id": "1",
          "time": "20210827 06:32"
        },
        {
          "id": "MDRW2100018496.1.9",
          "form": "그 제가 지금 2화까지만 봐서 음식이 피자밖에 안나왔어요~!ㅋㅋㅋ",
          "original_form": "그 제가 지금 2화까지만 봐서 음식이 피자밖에 안나왔어요~!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 06:32"
        },
        {
          "id": "MDRW2100018496.1.10",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "2",
          "time": "20210827 06:32"
        },
        {
          "id": "MDRW2100018496.1.11",
          "form": "아항!ㅋㅋㅋ",
          "original_form": "아항!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:32"
        },
        {
          "id": "MDRW2100018496.1.12",
          "form": "매점터는것도 나오죵ㅋㅋ",
          "original_form": "매점터는것도 나오죵ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:32"
        },
        {
          "id": "MDRW2100018496.1.13",
          "form": "뭔지알겠다 알싸한~~마늘통닭",
          "original_form": "뭔지알겠다 알싸한~~마늘통닭",
          "speaker_id": "2",
          "time": "20210827 06:32"
        },
        {
          "id": "MDRW2100018496.1.14",
          "form": "이거 말씀하시는거죠ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이거 말씀하시는거죠ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 06:32"
        },
        {
          "id": "MDRW2100018496.1.15",
          "form": "네네ㅋㅋㅋ 알싸~~한ㅋㅋ",
          "original_form": "네네ㅋㅋㅋ 알싸~~한ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:32"
        },
        {
          "id": "MDRW2100018496.1.16",
          "form": "맞아요ㅋㅋㅋ 이것도 진짜 재밌게 봤어요",
          "original_form": "맞아요ㅋㅋㅋ 이것도 진짜 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210827 06:32"
        },
        {
          "id": "MDRW2100018496.1.17",
          "form": "저는 이런류 좋아해서",
          "original_form": "저는 이런류 좋아해서",
          "speaker_id": "1",
          "time": "20210827 06:32"
        },
        {
          "id": "MDRW2100018496.1.18",
          "form": "저 그거 한번도 먹어본적없는데 재재랑 유해진때문에",
          "original_form": "저 그거 한번도 먹어본적없는데 재재랑 유해진때문에",
          "speaker_id": "2",
          "time": "20210827 06:32"
        },
        {
          "id": "MDRW2100018496.1.19",
          "form": "크라임씬도 좋아해요",
          "original_form": "크라임씬도 좋아해요",
