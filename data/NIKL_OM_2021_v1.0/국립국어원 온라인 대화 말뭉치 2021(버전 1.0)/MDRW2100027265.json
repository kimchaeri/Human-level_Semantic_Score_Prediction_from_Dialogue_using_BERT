      "utterance": [
        {
          "id": "MDRW2100027265.1.1",
          "form": "묘즘은복숭아요",
          "original_form": "묘즘은복숭아요",
          "speaker_id": "2",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027265.1.2",
          "form": "백도 계속 먹었고",
          "original_form": "백도 계속 먹었고",
          "speaker_id": "2",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027265.1.3",
          "form": "최근엔 황도샀어요",
          "original_form": "최근엔 황도샀어요",
          "speaker_id": "2",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027265.1.4",
          "form": "딱볶인가요",
          "original_form": "딱볶인가요",
          "speaker_id": "1",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027265.1.5",
          "form": "딱복",
          "original_form": "딱복",
          "speaker_id": "1",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027265.1.6",
          "form": "저놀복좋아하지만",
          "original_form": "저놀복좋아하지만",
          "speaker_id": "2",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100027265.1.7",
          "form": "자두도 좋구요",
          "original_form": "자두도 좋구요",
          "speaker_id": "1",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100027265.1.8",
          "form": "맛이좋으면딱복도 좋아요",
          "original_form": "맛이좋으면딱복도 좋아요",
          "speaker_id": "2",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100027265.1.9",
          "form": "새콤달콤하죠",
          "original_form": "새콤달콤하죠",
          "speaker_id": "2",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100027265.1.10",
          "form": "참외는 별로에요",
          "original_form": "참외는 별로에요",
          "speaker_id": "1",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100027265.1.11",
          "form": "수박을 많이못먹었어요",
          "original_form": "수박을 많이못먹었어요",
          "speaker_id": "2",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100027265.1.12",
          "form": "수박도 맛있죠",
          "original_form": "수박도 맛있죠",
          "speaker_id": "1",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100027265.1.13",
          "form": "화채먹고십네요",
          "original_form": "화채먹고십네요",
          "speaker_id": "2",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100027265.1.14",
          "form": "전참외도 잘먹어요",
          "original_form": "전참외도 잘먹어요",
          "speaker_id": "2",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100027265.1.15",
          "form": "수박은 커서 냉장고에 안들어가요",
          "original_form": "수박은 커서 냉장고에 안들어가요",
          "speaker_id": "1",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100027265.1.16",
          "form": "딸기가 젤 좋아요",
          "original_form": "딸기가 젤 좋아요",
          "speaker_id": "1",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100027265.1.17",
          "form": "설빙에서",
          "original_form": "설빙에서",
          "speaker_id": "2",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100027265.1.18",
          "form": "곧 배랑 감 나오겠어요",
          "original_form": "곧 배랑 감 나오겠어요",
          "speaker_id": "1",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100027265.1.19",
          "form": "딸기프리미엄사드세요",
          "original_form": "딸기프리미엄사드세요",
          "speaker_id": "2",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100027265.1.20",
          "form": "팥안들어가고",
          "original_form": "팥안들어가고",
          "speaker_id": "2",
          "time": "20210902 11:00"
        },
        {
