      "utterance": [
        {
          "id": "MDRW2100002937.1.1",
          "form": "기억나니?",
          "original_form": "기억나니?",
          "speaker_id": "1",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002937.1.2",
          "form": "어떤것?",
          "original_form": "어떤것?",
          "speaker_id": "2",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002937.1.3",
          "form": "나 최근에 시내에서 이상한 봉사단체를 만났어",
          "original_form": "나 최근에 시내에서 이상한 봉사단체를 만났어",
          "speaker_id": "1",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002937.1.4",
          "form": "최근이라기엔 몇년 됐지만..",
          "original_form": "최근이라기엔 몇년 됐지만..",
          "speaker_id": "1",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002937.1.5",
          "form": "오 뭐라고 말걸던데??",
          "original_form": "오 뭐라고 말걸던데??",
          "speaker_id": "2",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002937.1.6",
          "form": "나보고 시각장애인을 위한 책 제작에 도움을 달라고 하더라.",
          "original_form": "나보고 시각장애인을 위한 책 제작에 도움을 달라고 하더라.",
          "speaker_id": "1",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002937.1.7",
          "form": "내가 바빠서 집에서 녹음해주겠다고 하니",
          "original_form": "내가 바빠서 집에서 녹음해주겠다고 하니",
          "speaker_id": "1",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002937.1.8",
          "form": "부득불 자기 아지트로 나를 이끌려했어",
          "original_form": "부득불 자기 아지트로 나를 이끌려했어",
          "speaker_id": "1",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002937.1.9",
          "form": "나는 단호히 거절했단다",
          "original_form": "나는 단호히 거절했단다",
          "speaker_id": "1",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002937.1.10",
          "form": "대신 번호는 넘겨주었지. 난 정말 봉사를 도와주고싶었어.",
          "original_form": "대신 번호는 넘겨주었지. 난 정말 봉사를 도와주고싶었어.",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002937.1.11",
          "form": "아니 그런 파렴치한 사람이",
          "original_form": "아니 그런 파렴치한 사람이",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002937.1.12",
          "form": "정말 사이비 종교였단다",
          "original_form": "정말 사이비 종교였단다",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002937.1.13",
          "form": "신천지같은 것일까?",
          "original_form": "신천지같은 것일까?",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002937.1.14",
          "form": "종교를 강요하는 단체는 정말 별로인 것 같아",
          "original_form": "종교를 강요하는 단체는 정말 별로인 것 같아",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002937.1.15",
          "form": "비슷한 단체일듯",
          "original_form": "비슷한 단체일듯",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002937.1.16",
          "form": "나중에 이상한 이벤트라며 전화가 오던데 정말 이상했어.",
          "original_form": "나중에 이상한 이벤트라며 전화가 오던데 정말 이상했어.",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002937.1.17",
          "form": "ㅠㅠ개인정보를 다른곳에도 팔아넘겼나봐",
          "original_form": "ㅠㅠ개인정보를 다른곳에도 팔아넘겼나봐",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002937.1.18",
          "form": "어떻게 봉사활동을 빙자한 포교활동을 할까?",
          "original_form": "어떻게 봉사활동을 빙자한 포교활동을 할까?",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002937.1.19",
          "form": "큰 벌을 받을거야.",
          "original_form": "큰 벌을 받을거야.",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002937.1.20",
          "form": "너는 혹시 가입한 봉사단체가 있니?",
          "original_form": "너는 혹시 가입한 봉사단체가 있니?",
          "speaker_id": "1",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002937.1.21",
          "form": "양심이 없는 사람들이 모인 단체인가봐",
          "original_form": "양심이 없는 사람들이 모인 단체인가봐",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002937.1.22",
          "form": "나는 딱히 없어",
          "original_form": "나는 딱히 없어",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002937.1.23",
          "form": "맞아. 기분이 좋지 않았어.",
          "original_form": "맞아. 기분이 좋지 않았어.",
          "speaker_id": "1",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002937.1.24",
          "form": "나도 앖어.",
          "original_form": "나도 앖어.",
          "speaker_id": "1",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002937.1.25",
          "form": "봉사는 내가 여유로울때 하고싶오",
          "original_form": "봉사는 내가 여유로울때 하고싶오",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002937.1.26",
          "form": "지금은 학업만으로도 ㅠㅠ",
          "original_form": "지금은 학업만으로도 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 16:50"
        }
      ]
