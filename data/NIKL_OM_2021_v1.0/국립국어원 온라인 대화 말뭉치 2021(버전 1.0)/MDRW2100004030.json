      "utterance": [
        {
          "id": "MDRW2100004030.1.1",
          "form": "물냉이 좋아 비냉이 좋아?",
          "original_form": "물냉이 좋아 비냉이 좋아?",
          "speaker_id": "2",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004030.1.2",
          "form": "물냉이 좋아 비냉이 좋아?",
          "original_form": "물냉이 좋아 비냉이 좋아?",
          "speaker_id": "2",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004030.1.3",
          "form": "나는 비냉!",
          "original_form": "나는 비냉!",
          "speaker_id": "1",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004030.1.4",
          "form": "나는 물냉이 더 좋은데 ㅎㅎ",
          "original_form": "나는 물냉이 더 좋은데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004030.1.5",
          "form": "비빔냉면에 육수 추가해서",
          "original_form": "비빔냉면에 육수 추가해서",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004030.1.6",
          "form": "먹으면 좋아",
          "original_form": "먹으면 좋아",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004030.1.7",
          "form": "오 맛잘알!",
          "original_form": "오 맛잘알!",
          "speaker_id": "2",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004030.1.8",
          "form": "비빔냉면으로 먹다가 중간에 육수 부으면",
          "original_form": "비빔냉면으로 먹다가 중간에 육수 부으면",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004030.1.9",
          "form": "비냉하고 물냉 둘 다 먹네 ㅋㅋ",
          "original_form": "비냉하고 물냉 둘 다 먹네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004030.1.10",
          "form": "응 좋아 맛있어 ㅋㅋㅋ",
          "original_form": "응 좋아 맛있어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004030.1.11",
          "form": "육쌈냉면 먹어봤어?",
          "original_form": "육쌈냉면 먹어봤어?",
          "speaker_id": "2",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004030.1.12",
          "form": "응 갈비 비빔 냉면에 싸먹으면 완전 맛있어",
          "original_form": "응 갈비 비빔 냉면에 싸먹으면 완전 맛있어",
          "speaker_id": "1",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004030.1.13",
          "form": "꿀맛이지",
          "original_form": "꿀맛이지",
          "speaker_id": "2",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004030.1.14",
          "form": "냉면 먹고 싶다",
          "original_form": "냉면 먹고 싶다",
          "speaker_id": "2",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004030.1.15",
          "form": "회냉면도 맛있어",
          "original_form": "회냉면도 맛있어",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004030.1.16",
          "form": "배달 시켜 먹어야겠다",
          "original_form": "배달 시켜 먹어야겠다",
          "speaker_id": "2",
          "time": "20210821 14:58"
        }
