      "utterance": [
        {
          "id": "MDRW2100043174.1.1",
          "form": "오늘점심뭐드셨어요",
          "original_form": "오늘점심뭐드셨어요",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043174.1.2",
          "form": "제육볶음이요",
          "original_form": "제육볶음이요",
          "speaker_id": "2",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043174.1.3",
          "form": "사드셨나요 만드셨나여",
          "original_form": "사드셨나요 만드셨나여",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043174.1.4",
          "form": "사먹었어요",
          "original_form": "사먹었어요",
          "speaker_id": "2",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100043174.1.5",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100043174.1.6",
          "form": "전 버거킹 햄버거요 ㅎㅎㅎ",
          "original_form": "전 버거킹 햄버거요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 16:11"
        },
        {
          "id": "MDRW2100043174.1.7",
          "form": "맛있겠네요ㅋㅋ",
          "original_form": "맛있겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:13"
        },
        {
          "id": "MDRW2100043174.1.8",
          "form": "전 버거킹에선 머쉬룸와퍼나 치즈와퍼 자주 먹어요",
          "original_form": "전 버거킹에선 머쉬룸와퍼나 치즈와퍼 자주 먹어요",
          "speaker_id": "2",
          "time": "20210907 16:14"
        },
        {
          "id": "MDRW2100043174.1.9",
          "form": "ㅎㅎ전 새우들어간 아빠상어랑 몬스타x 잘먹어요",
          "original_form": "ㅎㅎ전 새우들어간 아빠상어랑 몬스타x 잘먹어요",
          "speaker_id": "1",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100043174.1.10",
          "form": "햄버거 최애브랜드 어디세요",
          "original_form": "햄버거 최애브랜드 어디세요",
          "speaker_id": "1",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100043174.1.11",
          "form": "맘스터치요ㅋㅋ",
          "original_form": "맘스터치요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100043174.1.12",
          "form": "ㅋㅋㅋㅋㅋ저도 맘터가 최애에요",
          "original_form": "ㅋㅋㅋㅋㅋ저도 맘터가 최애에요",
          "speaker_id": "1",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100043174.1.13",
          "form": "맘터 어떤버거여",
          "original_form": "맘터 어떤버거여",
          "speaker_id": "1",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100043174.1.14",
          "form": "싸이버거요. 패티두껍고 넘 짜지않아서 좋아요",
          "original_form": "싸이버거요. 패티두껍고 넘 짜지않아서 좋아요",
          "speaker_id": "2",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100043174.1.15",
          "form": "저두요 ㅜㅜ싸이버거 넘맛있어요",
          "original_form": "저두요 ㅜㅜ싸이버거 넘맛있어요",
          "speaker_id": "1",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100043174.1.16",
          "form": "닭다리살좋이하시나봐여",
          "original_form": "닭다리살좋이하시나봐여",
          "speaker_id": "1",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100043174.1.17",
          "form": "네 맛있잖아요ㅋㅋ 치킨메뉴도 가끔 시켜먹어요",
          "original_form": "네 맛있잖아요ㅋㅋ 치킨메뉴도 가끔 시켜먹어요",
          "speaker_id": "2",
          "time": "20210907 18:39"
        },
        {
          "id": "MDRW2100043174.1.18",
          "form": "ㅋㅋㅋ저도요 그래서 치킨시킬때 콤보로먹어요",
          "original_form": "ㅋㅋㅋ저도요 그래서 치킨시킬때 콤보로먹어요",
