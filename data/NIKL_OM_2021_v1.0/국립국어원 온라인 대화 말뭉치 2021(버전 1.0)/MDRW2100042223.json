      "utterance": [
        {
          "id": "MDRW2100042223.1.1",
          "form": "여행 또 가고싶은 곳 있으세요?",
          "original_form": "여행 또 가고싶은 곳 있으세요?",
          "speaker_id": "1",
          "time": "20210906 20:44"
        },
        {
          "id": "MDRW2100042223.1.2",
          "form": "미국가보고싶어요!",
          "original_form": "미국가보고싶어요!",
          "speaker_id": "2",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100042223.1.3",
          "form": "아니면 이탈리아!",
          "original_form": "아니면 이탈리아!",
          "speaker_id": "2",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042223.1.4",
          "form": "저도!!! 미국",
          "original_form": "저도!!! 미국",
          "speaker_id": "1",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042223.1.5",
          "form": "그리고 포르투갈 가보고싶어요!",
          "original_form": "그리고 포르투갈 가보고싶어요!",
          "speaker_id": "1",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042223.1.6",
          "form": "와 마자요",
          "original_form": "와 마자요",
          "speaker_id": "2",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042223.1.7",
          "form": "막 가성비좋다그래가주구",
          "original_form": "막 가성비좋다그래가주구",
          "speaker_id": "2",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042223.1.8",
          "form": "네!!! 음식도 맛있대요!!",
          "original_form": "네!!! 음식도 맛있대요!!",
          "speaker_id": "1",
          "time": "20210906 20:49"
        },
        {
          "id": "MDRW2100042223.1.9",
          "form": "터키도 가고싶었는데요ㅜㅠ",
          "original_form": "터키도 가고싶었는데요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210906 20:49"
        },
        {
          "id": "MDRW2100042223.1.10",
          "form": "아 저도 가고싶었는데ㅜㅜ",
          "original_form": "아 저도 가고싶었는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042223.1.11",
          "form": "거기도 은근 분쟁?지역인거같더라구오",
          "original_form": "거기도 은근 분쟁?지역인거같더라구오",
          "speaker_id": "2",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042223.1.12",
          "form": "거기 진짜 너무 위험해졌어요ㅜ",
          "original_form": "거기 진짜 너무 위험해졌어요ㅜ",
          "speaker_id": "1",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042223.1.13",
          "form": "살아생전에 갈수있을지...",
          "original_form": "살아생전에 갈수있을지...",
          "speaker_id": "1",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042223.1.14",
          "form": "백종원아저씨때문에 카이막먹으러 가보고싶었는데ㅜㅜ",
          "original_form": "백종원아저씨때문에 카이막먹으러 가보고싶었는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042223.1.15",
          "form": "아 저도! 그 영상 봤어요!",
          "original_form": "아 저도! 그 영상 봤어요!",
          "speaker_id": "1",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100042223.1.16",
          "form": "한국에도 팔긴한대요..ㅜㅠ",
          "original_form": "한국에도 팔긴한대요..ㅜㅠ",
          "speaker_id": "1",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100042223.1.17",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100042223.1.18",
          "form": "그래도 현지직접가보고싶었는데ㅜㅜ",
          "original_form": "그래도 현지직접가보고싶었는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100042223.1.19",
          "form": "어쩌다이래됐는지모르겠어요ㅜㅜ",
          "original_form": "어쩌다이래됐는지모르겠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100042223.1.20",
          "form": "그니까요 터키 패키지 예약했다 취소했었는데.... 그때가 마지막 기회....ㅜ",
          "original_form": "그니까요 터키 패키지 예약했다 취소했었는데.... 그때가 마지막 기회....ㅜ",
          "speaker_id": "1",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100042223.1.21",
          "form": "아정말요??",
          "original_form": "아정말요??",
          "speaker_id": "2",
          "time": "20210906 20:58"
        },
        {
          "id": "MDRW2100042223.1.22",
          "form": "아까워라ㅜㅜㅜ",
          "original_form": "아까워라ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 20:58"
        },
        {
          "id": "MDRW2100042223.1.23",
          "form": "그리고 바로 코로나.... 이렇게 길어질줄 몰랐어요",
          "original_form": "그리고 바로 코로나.... 이렇게 길어질줄 몰랐어요",
          "speaker_id": "1",
          "time": "20210906 20:58"
        }
      ]
