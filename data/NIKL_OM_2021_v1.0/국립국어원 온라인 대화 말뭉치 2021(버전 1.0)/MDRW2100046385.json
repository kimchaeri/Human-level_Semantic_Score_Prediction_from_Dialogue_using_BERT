      "utterance": [
        {
          "id": "MDRW2100046385.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20211020 16:28"
        },
        {
          "id": "MDRW2100046385.1.2",
          "form": "오늘 점심 뭐 드셨어요?ㅎㅎ",
          "original_form": "오늘 점심 뭐 드셨어요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20211020 16:30"
        },
        {
          "id": "MDRW2100046385.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211020 16:34"
        },
        {
          "id": "MDRW2100046385.1.4",
          "form": "저는 오늘",
          "original_form": "저는 오늘",
          "speaker_id": "2",
          "time": "20211020 16:34"
        },
        {
          "id": "MDRW2100046385.1.5",
          "form": "닭볶음탕을 먹었습니닷",
          "original_form": "닭볶음탕을 먹었습니닷",
          "speaker_id": "2",
          "time": "20211020 16:34"
        },
        {
          "id": "MDRW2100046385.1.6",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20211020 16:34"
        },
        {
          "id": "MDRW2100046385.1.7",
          "form": "저는 치즈김밥먹었어요!",
          "original_form": "저는 치즈김밥먹었어요!",
          "speaker_id": "1",
          "time": "20211020 16:35"
        },
        {
          "id": "MDRW2100046385.1.8",
          "form": "우왕 치즈김밥",
          "original_form": "우왕 치즈김밥",
          "speaker_id": "2",
          "time": "20211020 16:35"
        },
        {
          "id": "MDRW2100046385.1.9",
          "form": "김밥만 드신건가요?",
          "original_form": "김밥만 드신건가요?",
          "speaker_id": "2",
          "time": "20211020 16:35"
        },
        {
          "id": "MDRW2100046385.1.10",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20211020 16:35"
        },
        {
          "id": "MDRW2100046385.1.11",
          "form": "네 김밥이랑 어묵먹었어요",
          "original_form": "네 김밥이랑 어묵먹었어요",
          "speaker_id": "1",
          "time": "20211020 16:35"
        },
        {
          "id": "MDRW2100046385.1.12",
          "form": "날이 많이 추워졌네요",
          "original_form": "날이 많이 추워졌네요",
          "speaker_id": "1",
          "time": "20211020 16:35"
        },
        {
          "id": "MDRW2100046385.1.13",
          "form": "감기조심하세요!",
          "original_form": "감기조심하세요!",
          "speaker_id": "1",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046385.1.14",
          "form": "우와 김밥에 어묵 ㅠㅠ",
          "original_form": "우와 김밥에 어묵 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046385.1.15",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "2",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046385.1.16",
          "form": "갑자기 분식 땡기네요",
          "original_form": "갑자기 분식 땡기네요",
          "speaker_id": "2",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046385.1.17",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046385.1.18",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046385.1.19",
          "form": "네네 님두요!!!",
          "original_form": "네네 님두요!!!",
          "speaker_id": "2",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046385.1.20",
          "form": "이제 정말 춥더라구요...",
          "original_form": "이제 정말 춥더라구요...",
          "speaker_id": "2",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046385.1.21",
          "form": "분식 맛있죠",
          "original_form": "분식 맛있죠",
          "speaker_id": "1",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046385.1.22",
          "form": "ㅎㅎㅎ 사실",
          "original_form": "ㅎㅎㅎ 사실",
          "speaker_id": "2",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046385.1.23",
          "form": "분식이 진짜 안질려요",
          "original_form": "분식이 진짜 안질려요",
          "speaker_id": "2",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046385.1.24",
          "form": "하핫",
          "original_form": "하핫",
          "speaker_id": "2",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046385.1.25",
          "form": "최애 분식 메뉴가 있으싱가요",
          "original_form": "최애 분식 메뉴가 있으싱가요",
          "speaker_id": "2",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046385.1.26",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046385.1.27",
          "form": "저 떡볶이요",
          "original_form": "저 떡볶이요",
          "speaker_id": "1",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046385.1.28",
          "form": "쫄면도ㅋㅋ",
          "original_form": "쫄면도ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046385.1.29",
          "form": "뭐 제일 좋아하세요?",
          "original_form": "뭐 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046385.1.30",
          "form": "캬 쫄면 미쳤죠",
          "original_form": "캬 쫄면 미쳤죠",
          "speaker_id": "2",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046385.1.31",
          "form": "저는 분식집가면",
          "original_form": "저는 분식집가면",
          "speaker_id": "2",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046385.1.32",
          "form": "어묵이랑.....",
          "original_form": "어묵이랑.....",
          "speaker_id": "2",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046385.1.33",
          "form": "튀김...",
          "original_form": "튀김...",
          "speaker_id": "2",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046385.1.34",
          "form": "튀김 ㅠㅠ",
          "original_form": "튀김 ㅠㅠ",
          "speaker_id": "1",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046385.1.35",
          "form": "가장 좋아하는",
          "original_form": "가장 좋아하는",
          "speaker_id": "2",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046385.1.36",
          "form": "튀김이 뭐에요?",
          "original_form": "튀김이 뭐에요?",
          "speaker_id": "2",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046385.1.37",
          "form": "저는 야채튀김이랑 야끼만두 ㅠㅠㅠ",
          "original_form": "저는 야채튀김이랑 야끼만두 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046385.1.38",
          "form": "저는 단호박튀김이랑 김말이요 ㅋㅋ",
          "original_form": "저는 단호박튀김이랑 김말이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 16:41"
        },
        {
          "id": "MDRW2100046385.1.39",
          "form": "야끼만두도 맛있죠ㅠㅠ",
          "original_form": "야끼만두도 맛있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20211020 16:41"
        },
        {
          "id": "MDRW2100046385.1.40",
          "form": "하... 걔네도 근본이죠 ㅠ\r\n",
          "original_form": "하... 걔네도 근본이죠 ㅠ\r\n",
