      "utterance": [
        {
          "id": "MDRW2100003996.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003996.1.2",
          "form": "반려동물 키우시나요",
          "original_form": "반려동물 키우시나요",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003996.1.3",
          "form": "아니요 예전에 키워본 적은 있어요!",
          "original_form": "아니요 예전에 키워본 적은 있어요!",
          "speaker_id": "2",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003996.1.4",
          "form": "그런가요? 뭐 키워보셨어요?",
          "original_form": "그런가요? 뭐 키워보셨어요?",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003996.1.5",
          "form": "저는 강아지요",
          "original_form": "저는 강아지요",
          "speaker_id": "2",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003996.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003996.1.7",
          "form": "넘 귀엽죠",
          "original_form": "넘 귀엽죠",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003996.1.8",
          "form": "사실 전 안 키우는데 궁금해서 여쭤봤어요",
          "original_form": "사실 전 안 키우는데 궁금해서 여쭤봤어요",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003996.1.9",
          "form": "초코푸들이었는데 진짜 귀여웠어요",
          "original_form": "초코푸들이었는데 진짜 귀여웠어요",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003996.1.10",
          "form": "혹시 그러면 나중에 반려동물 키워볼 생각은 있으세요?",
          "original_form": "혹시 그러면 나중에 반려동물 키워볼 생각은 있으세요?",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003996.1.11",
          "form": "잘 모르겠어요",
          "original_form": "잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100003996.1.12",
          "form": "돈 드는 건 그래도 감당할 수 있을 것 같은데",
          "original_form": "돈 드는 건 그래도 감당할 수 있을 것 같은데",
          "speaker_id": "1",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100003996.1.13",
          "form": "제가 생명을 키울만한 위인은 안 되는 것 같아서",
          "original_form": "제가 생명을 키울만한 위인은 안 되는 것 같아서",
          "speaker_id": "1",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100003996.1.14",
          "form": "많이 힘들어요?",
          "original_form": "많이 힘들어요?",
          "speaker_id": "1",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100003996.1.15",
          "form": "그러면 그냥 동물 귀여워만 하시는건가요?",
          "original_form": "그러면 그냥 동물 귀여워만 하시는건가요?",
          "speaker_id": "2",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100003996.1.16",
          "form": "아 그거는 저도 그래요.. 누군가를 책임지는 게 어려워서",
          "original_form": "아 그거는 저도 그래요.. 누군가를 책임지는 게 어려워서",
          "speaker_id": "2",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100003996.1.17",
          "form": "네 저는 동물 영상 많이 보죠 대신... ㅋㅋ",
          "original_form": "네 저는 동물 영상 많이 보죠 대신... ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100003996.1.18",
          "form": "친구가 개를 새로 키우는데",
          "original_form": "친구가 개를 새로 키우는데",
          "speaker_id": "1",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100003996.1.19",
          "form": "아직 아기라서 말을 안 듣는 것 같더라고요",
          "original_form": "아직 아기라서 말을 안 듣는 것 같더라고요",
          "speaker_id": "1",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100003996.1.20",
          "form": "아기들은 다 그렇잖아요",
          "original_form": "아기들은 다 그렇잖아요",
          "speaker_id": "2",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100003996.1.21",
          "form": "사람도 그러는데 강아지라고 뭐가 다르겠어요",
          "original_form": "사람도 그러는데 강아지라고 뭐가 다르겠어요",
          "speaker_id": "2",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100003996.1.22",
          "form": "그것도 그렇네요",
          "original_form": "그것도 그렇네요",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100003996.1.23",
          "form": "세나개도 가끔씩 보는데 강아지들이랑 사람이랑 키우는 법이 다르지 않은 것 같아요",
          "original_form": "세나개도 가끔씩 보는데 강아지들이랑 사람이랑 키우는 법이 다르지 않은 것 같아요",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100003996.1.24",
          "form": "맞아요 다 훈련도 필요하고 사랑도 필요하고",
          "original_form": "맞아요 다 훈련도 필요하고 사랑도 필요하고",
          "speaker_id": "2",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100003996.1.25",
          "form": "강아지를 제일 좋아하시나요?",
          "original_form": "강아지를 제일 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100003996.1.26",
          "form": "앗 아뇨아뇨 ㅋㅋㅋㅋ 예전엔 강아지 좋아햇는데 지금은 고양이가 더 좋긴해요",
          "original_form": "앗 아뇨아뇨 ㅋㅋㅋㅋ 예전엔 강아지 좋아햇는데 지금은 고양이가 더 좋긴해요",
          "speaker_id": "1",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100003996.1.27",
          "form": "제가 좋아하는 고양이채널이 있어서",
          "original_form": "제가 좋아하는 고양이채널이 있어서",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100003996.1.28",
          "form": "아~ 고양이도 매력있죠 어떤 채널 좋아하세요?",
          "original_form": "아~ 고양이도 매력있죠 어떤 채널 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100003996.1.29",
          "form": "크집사네랑 하하하님이랑 아리네 좋아해요^_^",
          "original_form": "크집사네랑 하하하님이랑 아리네 좋아해요^_^",
