      "utterance": [
        {
          "id": "MDRW2100004342.1.1",
          "form": "혹시 반려동물 있으신가요?ㅎ",
          "original_form": "혹시 반려동물 있으신가요?ㅎ",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004342.1.2",
          "form": "아니요 ㅠ없어요",
          "original_form": "아니요 ㅠ없어요",
          "speaker_id": "2",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004342.1.3",
          "form": "랜선 이모입니다",
          "original_form": "랜선 이모입니다",
          "speaker_id": "2",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004342.1.4",
          "form": "ㅋㅋㅋ큐ㅠㅠ아 보시는 동물유튜브 채널 있나요?",
          "original_form": "ㅋㅋㅋ큐ㅠㅠ아 보시는 동물유튜브 채널 있나요?",
          "speaker_id": "1",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004342.1.5",
          "form": "너무 많죠!!",
          "original_form": "너무 많죠!!",
          "speaker_id": "2",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004342.1.6",
          "form": "크집사님 고양이들 보고",
          "original_form": "크집사님 고양이들 보고",
          "speaker_id": "2",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004342.1.7",
          "form": "새덕후님의 새 보고 ㅋㅋㅋ",
          "original_form": "새덕후님의 새 보고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004342.1.8",
          "form": "name1님은 반려동물 있으신가요?",
          "original_form": "&name1&님은 반려동물 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004342.1.9",
          "form": "헉 ㅠㅜ 저도 크집사님 구독합니다..",
          "original_form": "헉 ㅠㅜ 저도 크집사님 구독합니다..",
          "speaker_id": "1",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004342.1.10",
          "form": "없어요 흡.. 제 능력이 부족해서",
          "original_form": "없어요 흡.. 제 능력이 부족해서",
          "speaker_id": "1",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004342.1.11",
          "form": "아앗 ㅠㅠ",
          "original_form": "아앗 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004342.1.12",
          "form": "맞아요 반려동물 키우려면 여러 여건이 되어야지 데려올 수 있죠",
          "original_form": "맞아요 반려동물 키우려면 여러 여건이 되어야지 데려올 수 있죠",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004342.1.13",
          "form": "그쵸그쵸 돈도필요하구 넓은 집이나 산책갈 시간도 보장이되야하구",
          "original_form": "그쵸그쵸 돈도필요하구 넓은 집이나 산책갈 시간도 보장이되야하구",
          "speaker_id": "1",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004342.1.14",
          "form": "맞아요 언제 병원으로 가야할 일이 생길지모르니 병원비도 따로 저금해야하고",
          "original_form": "맞아요 언제 병원으로 가야할 일이 생길지모르니 병원비도 따로 저금해야하고",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004342.1.15",
          "form": "네네 그래서 랜선집사로 만족하고있어요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "네네 그래서 랜선집사로 만족하고있어요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004342.1.16",
          "form": "새유튜버는 누구예요? 궁금 ㅎㅎ",
          "original_form": "새유튜버는 누구예요? 궁금 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004342.1.17",
          "form": "새덕후 님이신데",
          "original_form": "새덕후 님이신데",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004342.1.18",
          "form": "야생 새를 영상으로 찍으시는데 재밌어요",
          "original_form": "야생 새를 영상으로 찍으시는데 재밌어요",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004342.1.19",
          "form": "라이브 채널도 운영하세요",
          "original_form": "라이브 채널도 운영하세요",
          "speaker_id": "2",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004342.1.20",
          "form": "오~ 구경해봐야겠아요",
          "original_form": "오~ 구경해봐야겠아요",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004342.1.21",
          "form": "동물영상보면 힐링되죠 ㅎㅎ",
          "original_form": "동물영상보면 힐링되죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:31"
