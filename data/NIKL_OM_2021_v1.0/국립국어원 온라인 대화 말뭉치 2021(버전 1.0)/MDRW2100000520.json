      "utterance": [
        {
          "id": "MDRW2100000520.1.1",
          "form": "요새 날씨가 좀 선선해졌네요~",
          "original_form": "요새 날씨가 좀 선선해졌네요~",
          "speaker_id": "1",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000520.1.2",
          "form": "절기무시못하겠어요 입추지나니 바로 선선해지는거같죠?",
          "original_form": "절기무시못하겠어요 입추지나니 바로 선선해지는거같죠?",
          "speaker_id": "2",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000520.1.3",
          "form": "네~ 넘 신기해요!ㅋㅋㅋ",
          "original_form": "네~ 넘 신기해요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000520.1.4",
          "form": "잘땐 이제 좀 춥더라구요",
          "original_form": "잘땐 이제 좀 춥더라구요",
          "speaker_id": "1",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000520.1.5",
          "form": "저두요 에어컨없이는 죽을것같더니 어젠 선풍기켜놔도 춥더라구요",
          "original_form": "저두요 에어컨없이는 죽을것같더니 어젠 선풍기켜놔도 춥더라구요",
          "speaker_id": "2",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000520.1.6",
          "form": "이런 날씨 넘 좋은데 갑자기 겨울올꺼같아요 ㅜㅠ",
          "original_form": "이런 날씨 넘 좋은데 갑자기 겨울올꺼같아요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000520.1.7",
          "form": "마자요 ㅠㅠ 딱 이런 날씨로 1년 내내 가면 좋겠는데 ㅠㅠ",
          "original_form": "마자요 ㅠㅠ 딱 이런 날씨로 1년 내내 가면 좋겠는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000520.1.8",
          "form": "글쵸 그럼 옷정리도 안해도되고..\n곧 옷장 또 뒤집어야할것같아요",
          "original_form": "글쵸 그럼 옷정리도 안해도되고..\n곧 옷장 또 뒤집어야할것같아요",
          "speaker_id": "2",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000520.1.9",
          "form": "아 그러네여 ㅠㅠ 계절 바뀔때마다 일이에요~",
          "original_form": "아 그러네여 ㅠㅠ 계절 바뀔때마다 일이에요~",
          "speaker_id": "1",
          "time": "20210819 16:39"
        },
        {
          "id": "MDRW2100000520.1.10",
          "form": "그러고보니 주말부터 담주내내 비 그림이네요\n장마인건가요?",
          "original_form": "그러고보니 주말부터 담주내내 비 그림이네요\n장마인건가요?",
          "speaker_id": "2",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000520.1.11",
          "form": "헛! 요새 비가 좀 많이 오네요 ㅠㅠ",
          "original_form": "헛! 요새 비가 좀 많이 오네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000520.1.12",
          "form": "늦은 장마이려나요 ㅠ",
          "original_form": "늦은 장마이려나요 ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000520.1.13",
          "form": "아 죄송요 \n전화가와서...요즘은 스콜성 비도 많은거같아요",
          "original_form": "아 죄송요 \n전화가와서...요즘은 스콜성 비도 많은거같아요",
          "speaker_id": "2",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000520.1.14",
          "form": "비소식도 없었는데 갑자기 하늘이 까맣더니 확내리고 그치고 하더라구요",
          "original_form": "비소식도 없었는데 갑자기 하늘이 까맣더니 확내리고 그치고 하더라구요",
          "speaker_id": "2",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000520.1.15",
          "form": "에고 맞아요 점점 동남아같은 날씨 ㅠ.ㅠ",
          "original_form": "에고 맞아요 점점 동남아같은 날씨 ㅠ.ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000520.1.16",
          "form": "맞아요 동남아 날씨 딱 그거예요",
          "original_form": "맞아요 동남아 날씨 딱 그거예요",
          "speaker_id": "2",
          "time": "20210819 16:56"
        },
        {
          "id": "MDRW2100000520.1.17",
          "form": "그쵸 ㅠㅋㅋ이런거 환경오염때문에 더 그런가봐요 ..",
          "original_form": "그쵸 ㅠㅋㅋ이런거 환경오염때문에 더 그런가봐요 ..",
          "speaker_id": "1",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000520.1.18",
          "form": "내일부터 비온다던데 그러기엔 지금 하늘이 너무 쨍쨍하네요",
          "original_form": "내일부터 비온다던데 그러기엔 지금 하늘이 너무 쨍쨍하네요",
          "speaker_id": "2",
          "time": "20210820 10:18"
        }
      ]
