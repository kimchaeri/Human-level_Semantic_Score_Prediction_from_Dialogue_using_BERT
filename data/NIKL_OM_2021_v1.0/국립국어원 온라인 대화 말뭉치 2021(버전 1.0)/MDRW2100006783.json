      "utterance": [
        {
          "id": "MDRW2100006783.1.1",
          "form": "이제 앞으로 위드코로나가 되면 무엇을 가장 하고 싶나요?",
          "original_form": "이제 앞으로 위드코로나가 되면 무엇을 가장 하고 싶나요?",
          "speaker_id": "1",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100006783.1.2",
          "form": "모임을 가지고 싶네요",
          "original_form": "모임을 가지고 싶네요",
          "speaker_id": "2",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100006783.1.3",
          "form": "저는 여행을 하고 싶습니다",
          "original_form": "저는 여행을 하고 싶습니다",
          "speaker_id": "1",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100006783.1.4",
          "form": "그동안 모임을 너무 안가졌어요",
          "original_form": "그동안 모임을 너무 안가졌어요",
          "speaker_id": "2",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100006783.1.5",
          "form": "여행도 좋죠",
          "original_form": "여행도 좋죠",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100006783.1.6",
          "form": "그렇죠 모임을 하면 밥을 먹어야 하는데",
          "original_form": "그렇죠 모임을 하면 밥을 먹어야 하는데",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100006783.1.7",
          "form": "같이 밥먹기도 쉽지 않고",
          "original_form": "같이 밥먹기도 쉽지 않고",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100006783.1.8",
          "form": "차 마시기도 쉽지 않고",
          "original_form": "차 마시기도 쉽지 않고",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100006783.1.9",
          "form": "식당에서 워낙 걸리니... ㅠ",
          "original_form": "식당에서 워낙 걸리니... ㅠ",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100006783.1.10",
          "form": "그동안 좀 그랬지요",
          "original_form": "그동안 좀 그랬지요",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100006783.1.11",
          "form": "여행은 일단 혼자라도 갈 수 있으니",
          "original_form": "여행은 일단 혼자라도 갈 수 있으니",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100006783.1.12",
          "form": "그래도 조심해야죠",
          "original_form": "그래도 조심해야죠",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100006783.1.13",
          "form": "마스크 쓰고라도 이제 가볼까 합니다",
          "original_form": "마스크 쓰고라도 이제 가볼까 합니다",
          "speaker_id": "1",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100006783.1.14",
          "form": "그동안은 그 조차 조심스러워서 못하고 있었죠",
          "original_form": "그동안은 그 조차 조심스러워서 못하고 있었죠",
          "speaker_id": "1",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100006783.1.15",
          "form": "혼자도 가시네요",
          "original_form": "혼자도 가시네요",
          "speaker_id": "2",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100006783.1.16",
          "form": "기분전환겸 가기도 하지요",
          "original_form": "기분전환겸 가기도 하지요",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100006783.1.17",
          "form": "전 혼자는 아직 안 가봤어요",
          "original_form": "전 혼자는 아직 안 가봤어요",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100006783.1.18",
          "form": "저는 가까운데 등산하기 좋은 산이 있답니다",
          "original_form": "저는 가까운데 등산하기 좋은 산이 있답니다",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100006783.1.19",
          "form": "혼자서 산행을 하기도 했는데",
          "original_form": "혼자서 산행을 하기도 했는데",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100006783.1.20",
          "form": "작년부터는 산도 위험하다고 안가고 있습니다",
          "original_form": "작년부터는 산도 위험하다고 안가고 있습니다",
          "speaker_id": "1",
          "time": "20210908 09:21"
        }
