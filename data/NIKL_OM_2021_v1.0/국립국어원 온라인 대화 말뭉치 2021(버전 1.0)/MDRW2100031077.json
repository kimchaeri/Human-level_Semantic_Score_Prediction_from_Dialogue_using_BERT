      "utterance": [
        {
          "id": "MDRW2100031077.1.1",
          "form": "일본 여행가고 싶다",
          "original_form": "일본 여행가고 싶다",
          "speaker_id": "1",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100031077.1.2",
          "form": "너랑 옛날에 도쿄 짧게 다녀왔잖아 그때 좋았는뎅",
          "original_form": "너랑 옛날에 도쿄 짧게 다녀왔잖아 그때 좋았는뎅",
          "speaker_id": "1",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100031077.1.3",
          "form": "나두 그때 소소한 재미가 있었는데",
          "original_form": "나두 그때 소소한 재미가 있었는데",
          "speaker_id": "2",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100031077.1.4",
          "form": "교훈도 많이 얻었었지",
          "original_form": "교훈도 많이 얻었었지",
          "speaker_id": "2",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100031077.1.5",
          "form": "ㅋㅋㅋㅋ어떤 교훈???",
          "original_form": "ㅋㅋㅋㅋ어떤 교훈???",
          "speaker_id": "1",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100031077.1.6",
          "form": "무조건 가는 길을 적어가자",
          "original_form": "무조건 가는 길을 적어가자",
          "speaker_id": "2",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100031077.1.7",
          "form": "종이지도를 그려서 랜드마크 그려놓고",
          "original_form": "종이지도를 그려서 랜드마크 그려놓고",
          "speaker_id": "2",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100031077.1.8",
          "form": "ㅋㅋㅋ아니 근데 생각해보면 우리 그때 스마트폰도 없었는데",
          "original_form": "ㅋㅋㅋ아니 근데 생각해보면 우리 그때 스마트폰도 없었는데",
          "speaker_id": "1",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100031077.1.9",
          "form": "그 정도면 길 잘 찾은거 아님?",
          "original_form": "그 정도면 길 잘 찾은거 아님?",
          "speaker_id": "1",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100031077.1.10",
          "form": "아냐 맛폰 있었어 그때",
          "original_form": "아냐 맛폰 있었어 그때",
          "speaker_id": "2",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100031077.1.11",
          "form": "너 타블렛 피씨 들고갔잖아",
          "original_form": "너 타블렛 피씨 들고갔잖아",
          "speaker_id": "2",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100031077.1.12",
          "form": "진짜야...? 그럼 당시에 해외로밍을 안해서 그랬다고 하자",
          "original_form": "진짜야...? 그럼 당시에 해외로밍을 안해서 그랬다고 하자",
          "speaker_id": "1",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100031077.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ 아니 너무 웃겨",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 아니 너무 웃겨",
          "speaker_id": "2",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100031077.1.14",
          "form": "지금 다시 간다치면 로밍을 제대로 하지 않을까...?",
          "original_form": "지금 다시 간다치면 로밍을 제대로 하지 않을까...?",
          "speaker_id": "1",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100031077.1.15",
          "form": "우리 나중에 다시 리벤지 도쿄여행을 가자꾸나",
          "original_form": "우리 나중에 다시 리벤지 도쿄여행을 가자꾸나",
          "speaker_id": "1",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100031077.1.16",
          "form": "그렇겠지? 나 유럽갔을땐 그냥 로밍 함",
          "original_form": "그렇겠지? 나 유럽갔을땐 그냥 로밍 함",
          "speaker_id": "2",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100031077.1.17",
          "form": "근데 도쿄 말고 난 오사카가고 싶다",
          "original_form": "근데 도쿄 말고 난 오사카가고 싶다",
          "speaker_id": "1",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100031077.1.18",
          "form": "마자 나도 오사카",
          "original_form": "마자 나도 오사카",
          "speaker_id": "2",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100031077.1.19",
          "form": "우리 해리포터 보러가야지",
          "original_form": "우리 해리포터 보러가야지",
          "speaker_id": "2",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100031077.1.20",
          "form": "이유 : 유니버셜스튜디오 가고싶듬",
          "original_form": "이유 : 유니버셜스튜디오 가고싶듬",
          "speaker_id": "1",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100031077.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞어 해리포터성 진짜 잘되어 잇대",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞어 해리포터성 진짜 잘되어 잇대",
          "speaker_id": "1",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100031077.1.22",
          "form": "친구가 다녀와서 사진 보여줬거든? 진짜 영화랑 똑같음",
          "original_form": "친구가 다녀와서 사진 보여줬거든? 진짜 영화랑 똑같음",
          "speaker_id": "1",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100031077.1.23",
          "form": "재미있겠다 ㅋㅋㅋ 가보고싶다. 언제 코로나 풀리나",
          "original_form": "재미있겠다 ㅋㅋㅋ 가보고싶다. 언제 코로나 풀리나",
          "speaker_id": "2",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100031077.1.24",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100031077.1.25",
          "form": "다이에건 앨리도 완전 똑같이 만들엇다고 함",
          "original_form": "다이에건 앨리도 완전 똑같이 만들엇다고 함",
          "speaker_id": "1",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100031077.1.26",
          "form": "뭐 오타쿠샵도 오사카에 잇겟지",
          "original_form": "뭐 오타쿠샵도 오사카에 잇겟지",
          "speaker_id": "1",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100031077.1.27",
          "form": "오 그거 진짜 궁금한데 파는것도 비슷하려나",
          "original_form": "오 그거 진짜 궁금한데 파는것도 비슷하려나",
          "speaker_id": "2",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100031077.1.28",
          "form": "오사카 가보고싶구나 간절하다",
          "original_form": "오사카 가보고싶구나 간절하다",
          "speaker_id": "1",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100031077.1.29",
          "form": "버터맥주도 판다던데?",
          "original_form": "버터맥주도 판다던데?",
          "speaker_id": "1",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100031077.1.30",
          "form": "후기 읽어봄",
          "original_form": "후기 읽어봄",
          "speaker_id": "1",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100031077.1.31",
          "form": "버터맥주맛없다고 소문나서",
          "original_form": "버터맥주맛없다고 소문나서",
          "speaker_id": "2",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100031077.1.32",
          "form": "별로 먹고싶지 않음",
          "original_form": "별로 먹고싶지 않음",
          "speaker_id": "2",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100031077.1.33",
          "form": "아니 그거 맛없냐고ㅠㅠ 책에서 진짜 맛잇게 묘사되서 먹어보고 싶엇건만",
          "original_form": "아니 그거 맛없냐고ㅠㅠ 책에서 진짜 맛잇게 묘사되서 먹어보고 싶엇건만",
          "speaker_id": "1",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100031077.1.34",
          "form": "나의 동심ㅠㅠ",
          "original_form": "나의 동심ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100031077.1.35",
          "form": "맞아... 나도 무슨맛일까 기대했는데",
          "original_form": "맞아... 나도 무슨맛일까 기대했는데",
          "speaker_id": "2",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100031077.1.36",
          "form": "그래두 맛없어도 경험은 해보고싶구낭",
          "original_form": "그래두 맛없어도 경험은 해보고싶구낭",
          "speaker_id": "1",
          "time": "20210904 16:27"
        }
      ]
