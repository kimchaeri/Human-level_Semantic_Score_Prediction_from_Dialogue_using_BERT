      "utterance": [
        {
          "id": "MDRW2100036445.1.1",
          "form": "이게뭐지싶어요..",
          "original_form": "이게뭐지싶어요..",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036445.1.2",
          "form": "의사들의 삶",
          "original_form": "의사들의 삶",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036445.1.3",
          "form": "여기도 사람사는 곳이다",
          "original_form": "여기도 사람사는 곳이다",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036445.1.4",
          "form": "교훈주려는 느낌",
          "original_form": "교훈주려는 느낌",
          "speaker_id": "1",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100036445.1.5",
          "form": "교훈주려는 거",
          "original_form": "교훈주려는 거",
          "speaker_id": "1",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100036445.1.6",
          "form": "진짜 공감이요",
          "original_form": "진짜 공감이요",
          "speaker_id": "1",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100036445.1.7",
          "form": "네 그냥 자연스럽게 말해도 되는데",
          "original_form": "네 그냥 자연스럽게 말해도 되는데",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036445.1.8",
          "form": "굳이 인위적으로",
          "original_form": "굳이 인위적으로",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036445.1.9",
          "form": "가장 중요한 순간이 언제였어?",
          "original_form": "가장 중요한 순간이 언제였어?",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036445.1.10",
          "form": "응급의학과 강조해주고ㅋㅋㅋㄱ",
          "original_form": "응급의학과 강조해주고ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036445.1.11",
          "form": "다른 작가들도 마찬가지지맘",
          "original_form": "다른 작가들도 마찬가지지맘",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100036445.1.12",
          "form": "너무 뻔한 클리셰를 자주써서ㅋㅋㅋㅋ",
          "original_form": "너무 뻔한 클리셰를 자주써서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100036445.1.13",
          "form": "장기기증편에서",
          "original_form": "장기기증편에서",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100036445.1.14",
          "form": "정문 경호원분 등장하자마자 느낌이 왔어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "정문 경호원분 등장하자마자 느낌이 왔어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100036445.1.15",
          "form": "악ㅋㅈㄲㄱㅋㄱㅈㅋ저도..",
          "original_form": "악ㅋㅈㄲㄱㅋㄱㅈㅋ저도..",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036445.1.16",
          "form": "이게모야싶고...",
          "original_form": "이게모야싶고...",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100036445.1.17",
          "form": "송화랑 익준이 이어지는거",
          "original_form": "송화랑 익준이 이어지는거",
          "speaker_id": "1",
          "time": "20210831 00:21"
        },
        {
          "id": "MDRW2100036445.1.18",
          "form": "어떻게 생각하세요?",
          "original_form": "어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210831 00:22"
        },
        {
          "id": "MDRW2100036445.1.19",
          "form": "작가는 마음정한거같은데 송화가아깝지않나요..ㅋㅋㄱㅋ",
          "original_form": "작가는 마음정한거같은데 송화가아깝지않나요..ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210831 09:14"
        },
        {
          "id": "MDRW2100036445.1.20",
          "form": "당연하죠...",
          "original_form": "당연하죠...",
          "speaker_id": "1",
          "time": "20210831 09:21"
        },
        {
          "id": "MDRW2100036445.1.21",
          "form": "물론 송화도 예전에 마음있던건 알겠지만 ㅜㅜ",
          "original_form": "물론 송화도 예전에 마음있던건 알겠지만 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 09:21"
        },
        {
          "id": "MDRW2100036445.1.22",
          "form": "그리고 뭐 친구로서 가깝게지내다가 꽁냥대고 데이트스러운?그런게 없는데",
          "original_form": "그리고 뭐 친구로서 가깝게지내다가 꽁냥대고 데이트스러운?그런게 없는데",
          "speaker_id": "1",
          "time": "20210831 09:22"
        },
        {
          "id": "MDRW2100036445.1.23",
          "form": "갑자기 잘되는건..ㅋㅋ",
          "original_form": "갑자기 잘되는건..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:22"
        },
        {
          "id": "MDRW2100036445.1.24",
          "form": "그니까요 급 고백하고 흔들고 과거 보여주고 그게모예요ㅋㅋㅋ심지어 돌싱에 애아빤데...",
          "original_form": "그니까요 급 고백하고 흔들고 과거 보여주고 그게모예요ㅋㅋㅋ심지어 돌싱에 애아빤데...",
          "speaker_id": "2",
          "time": "20210831 11:05"
        },
        {
          "id": "MDRW2100036445.1.25",
          "form": "안 나오는건지 모르겠는데",
          "original_form": "안 나오는건지 모르겠는데",
          "speaker_id": "1",
          "time": "20210901 06:18"
        },
        {
          "id": "MDRW2100036445.1.26",
          "form": "송화랑 우주 같이 만나는것도 저번에 캠핑이 처음아니였어요?",
          "original_form": "송화랑 우주 같이 만나는것도 저번에 캠핑이 처음아니였어요?",
          "speaker_id": "1",
          "time": "20210901 06:18"
        },
        {
          "id": "MDRW2100036445.1.27",
          "form": "물론 둘이 좋아하는거긴한데 그래두ㅋㅋㅋ애가있는데",
          "original_form": "물론 둘이 좋아하는거긴한데 그래두ㅋㅋㅋ애가있는데",
          "speaker_id": "1",
          "time": "20210901 06:18"
        },
        {
          "id": "MDRW2100036445.1.28",
          "form": "그때 그 우주아플때 송화가 간호해주러 오긴왔어용!  자주봤다고 쳐도 송화가 아깝습니다.. ㅠㅡㅠㅋㅋㅋ",
          "original_form": "그때 그 우주아플때 송화가 간호해주러 오긴왔어용!  자주봤다고 쳐도 송화가 아깝습니다.. ㅠㅡㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100036445.1.29",
          "form": "아 맞다! 그때",
          "original_form": "아 맞다! 그때",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100036445.1.30",
          "form": "익준이바빠서 왔었지",
          "original_form": "익준이바빠서 왔었지",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100036445.1.31",
          "form": "저는 겨울이랑 안정원도 뭔가별로ㅜ",
          "original_form": "저는 겨울이랑 안정원도 뭔가별로ㅜ",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100036445.1.32",
          "form": "네 갑분키스..ㅋㅋㅋㅋ",
          "original_form": "네 갑분키스..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100036445.1.33",
          "form": "모지했다니까오...ㅋㅋㄱ",
          "original_form": "모지했다니까오...ㅋㅋㄱ",
