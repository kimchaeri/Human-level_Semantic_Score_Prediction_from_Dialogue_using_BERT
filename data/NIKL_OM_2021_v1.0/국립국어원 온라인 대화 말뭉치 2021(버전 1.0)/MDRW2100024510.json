      "utterance": [
        {
          "id": "MDRW2100024510.1.1",
          "form": "언제할라고??",
          "original_form": "언제할라고??",
          "speaker_id": "2",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100024510.1.2",
          "form": "지금 보니깐",
          "original_form": "지금 보니깐",
          "speaker_id": "1",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100024510.1.3",
          "form": "200원 차이있네",
          "original_form": "200원 차이있네",
          "speaker_id": "1",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100024510.1.4",
          "form": "11605원이야 ㅋㅋ",
          "original_form": "11605원이야 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100024510.1.5",
          "form": "켁 그때는 니가 500원 들었다고 한거 같은데",
          "original_form": "켁 그때는 니가 500원 들었다고 한거 같은데",
          "speaker_id": "2",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100024510.1.6",
          "form": "지금 10원인가 또 떨어졌다.",
          "original_form": "지금 10원인가 또 떨어졌다.",
          "speaker_id": "1",
          "time": "20210831 12:38"
        },
        {
          "id": "MDRW2100024510.1.7",
          "form": "433주 사고 팔고했는데",
          "original_form": "433주 사고 팔고했는데",
          "speaker_id": "1",
          "time": "20210831 12:38"
        },
        {
          "id": "MDRW2100024510.1.8",
          "form": "-2665원에다가 수수료500원 더 드네",
          "original_form": "-2665원에다가 수수료500원 더 드네",
          "speaker_id": "1",
          "time": "20210831 12:39"
        },
        {
          "id": "MDRW2100024510.1.9",
          "form": "일단 팔리는것도 433주 걸어놨는데 조금씩 조금씩 다 팔려서 지금 매도다됬는데",
          "original_form": "일단 팔리는것도 433주 걸어놨는데 조금씩 조금씩 다 팔려서 지금 매도다됬는데",
          "speaker_id": "1",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100024510.1.10",
          "form": "일단 500만원 거래는 완료했엉",
          "original_form": "일단 500만원 거래는 완료했엉",
          "speaker_id": "1",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100024510.1.11",
          "form": "그러면500만원에 수수료가 3천원인거기ㅡ",
          "original_form": "그러면500만원에 수수료가 3천원인거기ㅡ",
          "speaker_id": "2",
          "time": "20210831 13:48"
        },
        {
          "id": "MDRW2100024510.1.12",
          "form": "웅 그렇네잉 ㅋㅋㅋ",
          "original_form": "웅 그렇네잉 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:48"
        },
        {
          "id": "MDRW2100024510.1.13",
          "form": "손해본거랑 수수료가3천?",
          "original_form": "손해본거랑 수수료가3천?",
          "speaker_id": "2",
          "time": "20210831 13:48"
        },
        {
          "id": "MDRW2100024510.1.14",
          "form": "오할만하네",
          "original_form": "오할만하네",
          "speaker_id": "2",
          "time": "20210831 13:48"
        },
        {
          "id": "MDRW2100024510.1.15",
          "form": "변동 크게 없나?ㅋㅋ",
          "original_form": "변동 크게 없나?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:48"
        },
        {
          "id": "MDRW2100024510.1.16",
          "form": "근데 ㅋㅋ 팔릴때 하도 매도 수량이 많으니깐",
          "original_form": "근데 ㅋㅋ 팔릴때 하도 매도 수량이 많으니깐",
          "speaker_id": "1",
          "time": "20210831 13:49"
        },
        {
          "id": "MDRW2100024510.1.17",
          "form": "긴장되긴하더라 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "긴장되긴하더라 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:49"
        },
        {
          "id": "MDRW2100024510.1.18",
          "form": "아 할까",
          "original_form": "아 할까",
          "speaker_id": "2",
          "time": "20210831 13:49"
        },
        {
          "id": "MDRW2100024510.1.19",
          "form": "금액 변동 있긴하네 ㅋㅋ",
          "original_form": "금액 변동 있긴하네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:49"
        },
        {
          "id": "MDRW2100024510.1.20",
          "form": "훅내려가나",
          "original_form": "훅내려가나",
          "speaker_id": "2",
          "time": "20210831 13:49"
        },
        {
          "id": "MDRW2100024510.1.21",
          "form": "훅ㄴㅐ려가지는 않는데 일단 차이는 쪼매있는듯 ㅋ",
          "original_form": "훅ㄴㅐ려가지는 않는데 일단 차이는 쪼매있는듯 ㅋ",
          "speaker_id": "1",
          "time": "20210831 13:49"
        },
        {
          "id": "MDRW2100024510.1.22",
          "form": "니는 한단계 밑에???",
          "original_form": "니는 한단계 밑에???",
