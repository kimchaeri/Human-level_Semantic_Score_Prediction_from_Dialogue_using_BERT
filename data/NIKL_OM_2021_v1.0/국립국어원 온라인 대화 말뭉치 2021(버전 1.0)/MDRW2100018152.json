      "utterance": [
        {
          "id": "MDRW2100018152.1.1",
          "form": "유튭은 뭐보시나요?",
          "original_form": "유튭은 뭐보시나요?",
          "speaker_id": "1",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018152.1.2",
          "form": "저 영화 리뷰 같은 거 본 다음에 보통 와챠나 넷플로  플 스토리.감상해요",
          "original_form": "저 영화 리뷰 같은 거 본 다음에 보통 와챠나 넷플로  플 스토리.감상해요",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018152.1.3",
          "form": "아무래도 안 맞을수도 있으니까 약간은 스포 당한 상태로 보게 되더라거요",
          "original_form": "아무래도 안 맞을수도 있으니까 약간은 스포 당한 상태로 보게 되더라거요",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018152.1.4",
          "form": "리뷰를 먼저보시는구나",
          "original_form": "리뷰를 먼저보시는구나",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018152.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018152.1.6",
          "form": "영화아무것도 모르고보면",
          "original_form": "영화아무것도 모르고보면",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018152.1.7",
          "form": "취향 안맞았을때 너무 아쉬우니까",
          "original_form": "취향 안맞았을때 너무 아쉬우니까",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018152.1.8",
          "form": "요즘은 그런것도 좋은거 같아요",
          "original_form": "요즘은 그런것도 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018152.1.9",
          "form": "name1은 취향이뭐에요?",
          "original_form": "&name1&은 취향이뭐에요?",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018152.1.10",
          "form": "여운긴 영화요",
          "original_form": "여운긴 영화요",
          "speaker_id": "1",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018152.1.11",
          "form": "새드엔딩 덕후...",
          "original_form": "새드엔딩 덕후...",
          "speaker_id": "1",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018152.1.12",
          "form": "세드 엔딩 추천 부탁드려요 와 넘 좋아요",
          "original_form": "세드 엔딩 추천 부탁드려요 와 넘 좋아요",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018152.1.13",
          "form": "가장따듯한색 블루 이런거요?",
          "original_form": "가장따듯한색 블루 이런거요?",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018152.1.14",
          "form": "가따블 좋죠 프랑스영화라",
          "original_form": "가따블 좋죠 프랑스영화라",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018152.1.15",
          "form": "감정따라가기 힘들었지만요...",
          "original_form": "감정따라가기 힘들었지만요...",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018152.1.16",
          "form": "음 지금기억나는건",
          "original_form": "음 지금기억나는건",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018152.1.17",
          "form": "왕의남자 불한당...?",
          "original_form": "왕의남자 불한당...?",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018152.1.18",
          "form": "센치로",
          "original_form": "센치로",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018152.1.19",
          "form": "유명한것만 생각나네요",
          "original_form": "유명한것만 생각나네요",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018152.1.20",
          "form": "아앗 저 최근에 불한당 봤어요",
          "original_form": "아앗 저 최근에 불한당 봤어요",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018152.1.21",
          "form": "살짝 치였죠... 불한당... 초반씬에서 총이 사람의 죄책감을 없애줬다하잖아요",
          "original_form": "살짝 치였죠... 불한당... 초반씬에서 총이 사람의 죄책감을 없애줬다하잖아요",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018152.1.22",
          "form": "근데 현수가 마지막에 그 나쁜놈 죽일때 자기 손으로 목을 조르잖아요 그게 너무 애뜻했어요",
          "original_form": "근데 현수가 마지막에 그 나쁜놈 죽일때 자기 손으로 목을 조르잖아요 그게 너무 애뜻했어요",
          "speaker_id": "2",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018152.1.23",
          "form": "어떤방법보다 직접적인방법으로 죽이죠...",
          "original_form": "어떤방법보다 직접적인방법으로 죽이죠...",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018152.1.24",
          "form": "그때 나오는 음악좋아해요",
          "original_form": "그때 나오는 음악좋아해요",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018152.1.25",
          "form": "내가 감아볼게. 하는 것도 마음에 들지 않아요?",
          "original_form": "내가 감아볼게. 하는 것도 마음에 들지 않아요?",
          "speaker_id": "2",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018152.1.26",
          "form": "여기서 치인 사람 누구냐고요. 저요",
          "original_form": "여기서 치인 사람 누구냐고요. 저요",
          "speaker_id": "2",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018152.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018152.1.28",
          "form": "저도 한참 좋아했었죠",
          "original_form": "저도 한참 좋아했었죠",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018152.1.29",
          "form": "하나하나 해석해놓으신분들도 많아서",
          "original_form": "하나하나 해석해놓으신분들도 많아서",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018152.1.30",
          "form": "그거보시면 더 재밌으실건데 보셨을거 같기도해요ㅋㅋㅋㅋ",
          "original_form": "그거보시면 더 재밌으실건데 보셨을거 같기도해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018152.1.31",
          "form": "맞아요 저 그런거 다 읽어요. 남이 해 놓은 다채로은 해석이나 숨겨진 메타포 보고 다시 치이는 편이에요",
          "original_form": "맞아요 저 그런거 다 읽어요. 남이 해 놓은 다채로은 해석이나 숨겨진 메타포 보고 다시 치이는 편이에요",
          "speaker_id": "2",
          "time": "20210826 23:37"
        }
      ]
    }
