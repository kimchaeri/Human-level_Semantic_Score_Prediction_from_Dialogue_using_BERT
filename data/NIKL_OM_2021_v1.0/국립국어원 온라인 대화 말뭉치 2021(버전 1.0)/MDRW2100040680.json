      "utterance": [
        {
          "id": "MDRW2100040680.1.1",
          "form": "앗 대화제출했더니 name1님 말씀하싱거 못봤으요 ㅠㅠ",
          "original_form": "앗 대화제출했더니 &name1&님 말씀하싱거 못봤으요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100040680.1.2",
          "form": "아니 뭐했다고 벌써 일욜이랍니까....? 참내....",
          "original_form": "아니 뭐했다고 벌써 일욜이랍니까....? 참내....",
          "speaker_id": "2",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100040680.1.3",
          "form": "오늘 이태원에서 재밌게 놀다오세요!!!",
          "original_form": "오늘 이태원에서 재밌게 놀다오세요!!!",
          "speaker_id": "2",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100040680.1.4",
          "form": "헤헤 저도 제가 무슨 말을 했는지 기억이 안나네요 ><",
          "original_form": "헤헤 저도 제가 무슨 말을 했는지 기억이 안나네요 ><",
          "speaker_id": "1",
          "time": "20210905 10:03"
        },
        {
          "id": "MDRW2100040680.1.5",
          "form": "감사해요!!!!",
          "original_form": "감사해요!!!!",
          "speaker_id": "1",
          "time": "20210905 10:03"
        },
        {
          "id": "MDRW2100040680.1.6",
          "form": "저는 11시 미사를... 다녀올거랍니다,,, 쿨럭!",
          "original_form": "저는 11시 미사를... 다녀올거랍니다,,, 쿨럭!",
          "speaker_id": "1",
          "time": "20210905 10:03"
        },
        {
          "id": "MDRW2100040680.1.7",
          "form": "주일이군요! 좋은 말씀있으면 공유해주세요 ㅋㅋㅋㅋㅋ",
          "original_form": "주일이군요! 좋은 말씀있으면 공유해주세요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:28"
        },
        {
          "id": "MDRW2100040680.1.8",
          "form": "으하하하하",
          "original_form": "으하하하하",
          "speaker_id": "1",
          "time": "20210905 11:43"
        },
        {
          "id": "MDRW2100040680.1.9",
          "form": "방금 성체도 모셨어요!",
          "original_form": "방금 성체도 모셨어요!",
          "speaker_id": "1",
          "time": "20210905 11:43"
        },
        {
          "id": "MDRW2100040680.1.10",
          "form": "밀가루로 만든,,, 예수님의,,,",
          "original_form": "밀가루로 만든,,, 예수님의,,,",
          "speaker_id": "1",
          "time": "20210905 11:43"
        },
        {
          "id": "MDRW2100040680.1.11",
          "form": "바디...",
          "original_form": "바디...{emoji:☆}",
          "speaker_id": "1",
          "time": "20210905 11:43"
        },
        {
          "id": "MDRW2100040680.1.12",
          "form": "와 저 그거 한번도 못모셔봣는데",
          "original_form": "와 저 그거 한번도 못모셔봣는데",
          "speaker_id": "2",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100040680.1.13",
          "form": "너무 궁금하고 부러웟어요",
          "original_form": "너무 궁금하고 부러웟어요",
          "speaker_id": "2",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100040680.1.14",
          "form": "아 진짜요?!",
          "original_form": "아 진짜요?!",
          "speaker_id": "1",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100040680.1.15",
          "form": "그냥 밀가루 맛인데 씹으면 안되는",
          "original_form": "그냥 밀가루 맛인데 씹으면 안되는",
          "speaker_id": "1",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100040680.1.16",
          "form": "빵이에욬ㅋㅋㅋㅋㅋ",
          "original_form": "빵이에욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100040680.1.17",
          "form": "ㅌㅋㅋㅋ헐 씹으면 안되는구나",
          "original_form": "ㅌㅋㅋㅋ헐 씹으면 안되는구나",
          "speaker_id": "2",
          "time": "20210905 12:59"
        },
        {
          "id": "MDRW2100040680.1.18",
          "form": "녹여먹으라는 말은 들었는디",
          "original_form": "녹여먹으라는 말은 들었는디",
          "speaker_id": "2",
          "time": "20210905 12:59"
        },
        {
          "id": "MDRW2100040680.1.19",
          "form": "맞이요,,, 쿄쿄쿄",
          "original_form": "맞이요,,, 쿄쿄쿄",
          "speaker_id": "1",
          "time": "20210905 13:07"
        },
        {
          "id": "MDRW2100040680.1.20",
          "form": "name2님 점심 드셨나요?!",
          "original_form": "&name2&님 점심 드셨나요?!",
          "speaker_id": "1",
          "time": "20210905 13:07"
        },
        {
          "id": "MDRW2100040680.1.21",
          "form": "네 저 아점 먹었습니담 어제 남은 똠양꿍에 밥이랑해서 ㅋㅋㅋ",
          "original_form": "네 저 아점 먹었습니담 어제 남은 똠양꿍에 밥이랑해서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:09"
        },
        {
          "id": "MDRW2100040680.1.22",
          "form": "name1님 점심약속이신가요?",
          "original_form": "&name1&님 점심약속이신가요?",
          "speaker_id": "2",
          "time": "20210905 13:09"
        },
