      "utterance": [
        {
          "id": "MDRW2100022182.1.1",
          "form": "마인크래프트 재밌나요?",
          "original_form": "마인크래프트 재밌나요?",
          "speaker_id": "1",
          "time": "20210829 10:38"
        },
        {
          "id": "MDRW2100022182.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022182.1.3",
          "form": "오랜만에 하니까 재밌더라구요",
          "original_form": "오랜만에 하니까 재밌더라구요",
          "speaker_id": "2",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022182.1.4",
          "form": "어떤거 하셨나요?",
          "original_form": "어떤거 하셨나요?",
          "speaker_id": "1",
          "time": "20210829 10:48"
        },
        {
          "id": "MDRW2100022182.1.5",
          "form": "저는 주로 집 짓거나 주민 이랑 마을 찾아다녔어요",
          "original_form": "저는 주로 집 짓거나 주민 이랑 마을 찾아다녔어요",
          "speaker_id": "1",
          "time": "20210829 10:48"
        },
        {
          "id": "MDRW2100022182.1.6",
          "form": "지옥도 갔구요",
          "original_form": "지옥도 갔구요",
          "speaker_id": "1",
          "time": "20210829 10:48"
        },
        {
          "id": "MDRW2100022182.1.7",
          "form": "그냥 집 짓고 땅굴 파고 그랬죠",
          "original_form": "그냥 집 짓고 땅굴 파고 그랬죠",
          "speaker_id": "2",
          "time": "20210829 10:48"
        },
        {
          "id": "MDRW2100022182.1.8",
          "form": "엄청큰 동굴이 있어서 구경하고 그랬어요",
          "original_form": "엄청큰 동굴이 있어서 구경하고 그랬어요",
          "speaker_id": "2",
          "time": "20210829 10:49"
        },
        {
          "id": "MDRW2100022182.1.9",
          "form": "퐁퐁두두는 어때??",
          "original_form": "퐁퐁두두는 어때??",
          "speaker_id": "2",
          "time": "20210829 10:50"
        },
        {
          "id": "MDRW2100022182.1.10",
          "form": "오 그런거 재밌는데",
          "original_form": "오 그런거 재밌는데",
          "speaker_id": "1",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022182.1.11",
          "form": "퐁퐁두두ㅋㅋㅋ 날짜 바꿔서 해야되겠다 좀 있다가",
          "original_form": "퐁퐁두두ㅋㅋㅋ 날짜 바꿔서 해야되겠다 좀 있다가",
          "speaker_id": "1",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022182.1.12",
          "form": "오늘 질문 하고 와야겠어~!",
          "original_form": "오늘 질문 하고 와야겠어~!",
          "speaker_id": "1",
          "time": "20210829 11:00"
        },
        {
          "id": "MDRW2100022182.1.13",
          "form": "헐 나 날짜바꿔서",
          "original_form": "헐 나 날짜바꿔서",
          "speaker_id": "1",
          "time": "20210829 11:00"
        },
        {
          "id": "MDRW2100022182.1.14",
          "form": "질문이 안나오나봐ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "질문이 안나오나봐ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:00"
        },
        {
          "id": "MDRW2100022182.1.15",
          "form": "날짜 원래대로 돌렸는데 질문이 없대..ㅋ..",
          "original_form": "날짜 원래대로 돌렸는데 질문이 없대..ㅋ..",
          "speaker_id": "1",
          "time": "20210829 11:01"
        },
        {
          "id": "MDRW2100022182.1.16",
          "form": "헐ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:04"
        },
        {
          "id": "MDRW2100022182.1.17",
          "form": "그럼 며칠간 계속 안 오나??",
          "original_form": "그럼 며칠간 계속 안 오나??",
          "speaker_id": "2",
          "time": "20210829 11:04"
        },
        {
          "id": "MDRW2100022182.1.18",
          "form": "그니까 난 망했어",
          "original_form": "그니까 난 망했어",
          "speaker_id": "1",
          "time": "20210829 11:04"
        },
        {
          "id": "MDRW2100022182.1.19",
          "form": "어제 9월 몇일까지한지 기억이 안나거든",
          "original_form": "어제 9월 몇일까지한지 기억이 안나거든",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022182.1.20",
          "form": "ㅋㅋㅋㅋㅋ그냥 찍어봐야겠다",
          "original_form": "ㅋㅋㅋㅋㅋ그냥 찍어봐야겠다",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022182.1.21",
          "form": "너는? 질문했나?",
          "original_form": "너는? 질문했나?",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022182.1.22",
          "form": "ㅇㅇ 난 오늘 했지",
          "original_form": "ㅇㅇ 난 오늘 했지",
          "speaker_id": "2",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100022182.1.23",
          "form": "아까 버스에서",
          "original_form": "아까 버스에서",
          "speaker_id": "2",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100022182.1.24",
          "form": "퐁퐁이 많이 큼??",
          "original_form": "퐁퐁이 많이 큼??",
          "speaker_id": "2",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100022182.1.25",
          "form": "오 ㅋㅋㅋㅋ 그냥 어제 본 그 나무 크기야",
          "original_form": "오 ㅋㅋㅋㅋ 그냥 어제 본 그 나무 크기야",
          "speaker_id": "1",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100022182.1.26",
          "form": "얼른 키워야지",
          "original_form": "얼른 키워야지",
          "speaker_id": "1",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022182.1.27",
          "form": "모바일게임 재밌는거 없나",
          "original_form": "모바일게임 재밌는거 없나",
          "speaker_id": "1",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022182.1.28",
          "form": "글쎄다",
          "original_form": "글쎄다",
          "speaker_id": "2",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022182.1.29",
          "form": "루미큐브",
          "original_form": "루미큐브",
          "speaker_id": "2",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022182.1.30",
          "form": "나 요새 퐁퐁두두밖에 안 해",
          "original_form": "나 요새 퐁퐁두두밖에 안 해",
          "speaker_id": "2",
          "time": "20210829 11:09"
        },
        {
          "id": "MDRW2100022182.1.31",
          "form": "루미큐브ㅋㅋㅋㅋㅋ아직 하고 있구나",
          "original_form": "루미큐브ㅋㅋㅋㅋㅋ아직 하고 있구나",
          "speaker_id": "1",
          "time": "20210829 11:09"
        },
        {
          "id": "MDRW2100022182.1.32",
          "form": "나는 무슨 방탈출 게임 깔았었는데 재밌더라",
          "original_form": "나는 무슨 방탈출 게임 깔았었는데 재밌더라",
          "speaker_id": "1",
          "time": "20210829 11:09"
        },
        {
          "id": "MDRW2100022182.1.33",
          "form": "아니 루미 안 하는데",
          "original_form": "아니 루미 안 하는데",
          "speaker_id": "2",
          "time": "20210829 11:09"
        },
        {
          "id": "MDRW2100022182.1.34",
          "form": "가끔 하다가 빡치면 삭제하고ㅋㅋㅋㅋ",
          "original_form": "가끔 하다가 빡치면 삭제하고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100022182.1.35",
          "form": "폰으로 방탈출을 해??",
          "original_form": "폰으로 방탈출을 해??",
          "speaker_id": "2",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100022182.1.36",
          "form": "ㅇㅇㅇ게임 엄청 많아",
          "original_form": "ㅇㅇㅇ게임 엄청 많아",
          "speaker_id": "1",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022182.1.37",
          "form": "아무튼 재밌었음 호호",
          "original_form": "아무튼 재밌었음 호호",
          "speaker_id": "1",
          "time": "20210829 11:14"
        }
      ]
    }
