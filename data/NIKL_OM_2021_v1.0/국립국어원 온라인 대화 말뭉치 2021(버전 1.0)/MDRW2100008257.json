      "utterance": [
        {
          "id": "MDRW2100008257.1.1",
          "form": "안녕하새요~",
          "original_form": "안녕하새요~",
          "speaker_id": "1",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100008257.1.2",
          "form": "그쪽 날씨 어때요",
          "original_form": "그쪽 날씨 어때요",
          "speaker_id": "1",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100008257.1.3",
          "form": "오늘은 맑아요",
          "original_form": "오늘은 맑아요",
          "speaker_id": "2",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100008257.1.4",
          "form": "날씨어떤가요",
          "original_form": "날씨어떤가요",
          "speaker_id": "2",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100008257.1.5",
          "form": "엄청 맑아요",
          "original_form": "엄청 맑아요",
          "speaker_id": "1",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100008257.1.6",
          "form": "어제까지 시원했는대",
          "original_form": "어제까지 시원했는대",
          "speaker_id": "1",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008257.1.7",
          "form": "오늘은 또 덥내요",
          "original_form": "오늘은 또 덥내요",
          "speaker_id": "1",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008257.1.8",
          "form": "헉 어제 비안왔나요?",
          "original_form": "헉 어제 비안왔나요?",
          "speaker_id": "2",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008257.1.9",
          "form": "저 사는 곳은 엄청 퍼부었는데",
          "original_form": "저 사는 곳은 엄청 퍼부었는데",
          "speaker_id": "2",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008257.1.10",
          "form": "비 엄청왔다가 그쳐서",
          "original_form": "비 엄청왔다가 그쳐서",
          "speaker_id": "1",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008257.1.11",
          "form": "그 이후로는 내내 시원했어요",
          "original_form": "그 이후로는 내내 시원했어요",
          "speaker_id": "1",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008257.1.12",
          "form": "오히려 추워서ㅎㅎ",
          "original_form": "오히려 추워서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008257.1.13",
          "form": "근데 오늘은 바람이 안불어요 ㅠㅠ",
          "original_form": "근데 오늘은 바람이 안불어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008257.1.14",
          "form": "더우시겠어요ㅠㅠ",
          "original_form": "더우시겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008257.1.15",
          "form": "어떤 계절을 좋아하시나요",
          "original_form": "어떤 계절을 좋아하시나요",
          "speaker_id": "2",
          "time": "20210822 14:40"
        },
        {
          "id": "MDRW2100008257.1.16",
          "form": "저는 봄 가을이 좋아요",
          "original_form": "저는 봄 가을이 좋아요",
          "speaker_id": "1",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008257.1.17",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008257.1.18",
          "form": "저는 늦 여름이요",
          "original_form": "저는 늦 여름이요",
          "speaker_id": "2",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008257.1.19",
          "form": "봄 가을이 좋은 이유가 있나요",
          "original_form": "봄 가을이 좋은 이유가 있나요",
          "speaker_id": "2",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008257.1.20",
          "form": "바람이 선선한게 좋아요",
          "original_form": "바람이 선선한게 좋아요",
          "speaker_id": "1",
          "time": "20210822 14:43"
        },
        {
          "id": "MDRW2100008257.1.21",
          "form": "그리고 봄은 꽃이 예쁘고 가을은 과일이 맛있잖아요",
          "original_form": "그리고 봄은 꽃이 예쁘고 가을은 과일이 맛있잖아요",
          "speaker_id": "1",
          "time": "20210822 14:43"
        },
        {
          "id": "MDRW2100008257.1.22",
          "form": "앗 저도 그이유에서 늦여름이 좋아요",
          "original_form": "앗 저도 그이유에서 늦여름이 좋아요",
          "speaker_id": "2",
          "time": "20210822 14:43"
        },
        {
          "id": "MDRW2100008257.1.23",
          "form": "아아 늦여름도 선선해서 좋죠",
          "original_form": "아아 늦여름도 선선해서 좋죠",
          "speaker_id": "1",
          "time": "20210822 14:44"
        },
        {
          "id": "MDRW2100008257.1.24",
          "form": "요즘은 잘 모르겠긴한대 ㅋㅋ ㅜㅜ",
          "original_form": "요즘은 잘 모르겠긴한대 ㅋㅋ ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 14:44"
        },
        {
          "id": "MDRW2100008257.1.25",
          "form": "이상기후 때문에 그런가봐요ㅠ",
          "original_form": "이상기후 때문에 그런가봐요ㅠ",
          "speaker_id": "2",
          "time": "20210822 14:44"
        },
        {
          "id": "MDRW2100008257.1.26",
          "form": "약간 계절의 경계가 모호해진 느낌",
          "original_form": "약간 계절의 경계가 모호해진 느낌",
          "speaker_id": "2",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008257.1.27",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008257.1.28",
          "form": "너무여름 빡! 겨울 빡!",
          "original_form": "너무여름 빡! 겨울 빡!",
          "speaker_id": "1",
          "time": "20210822 14:46"
        }
      ]
    }
  ]
