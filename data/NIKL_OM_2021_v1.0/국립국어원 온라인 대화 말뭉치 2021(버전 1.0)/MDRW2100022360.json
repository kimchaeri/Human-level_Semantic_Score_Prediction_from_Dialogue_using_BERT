      "utterance": [
        {
          "id": "MDRW2100022360.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022360.1.2",
          "form": "안녕하세요! 맛점하셨나요?",
          "original_form": "안녕하세요! 맛점하셨나요?",
          "speaker_id": "2",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022360.1.3",
          "form": "저는 아직 안먹었어요.",
          "original_form": "저는 아직 안먹었어요.",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022360.1.4",
          "form": "배달앱에서 알아보는 중이에요",
          "original_form": "배달앱에서 알아보는 중이에요",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022360.1.5",
          "form": "저도 저녁 고민중입니다",
          "original_form": "저도 저녁 고민중입니다",
          "speaker_id": "2",
          "time": "20210829 13:25"
        },
        {
          "id": "MDRW2100022360.1.6",
          "form": "지금 점심인데 해외에 거주하시나봐요?",
          "original_form": "지금 점심인데 해외에 거주하시나봐요?",
          "speaker_id": "1",
          "time": "20210829 13:25"
        },
        {
          "id": "MDRW2100022360.1.7",
          "form": "점심은 이미 먹어서ㅋㅋ 이제 저녁 고민이요ㅋㅋ",
          "original_form": "점심은 이미 먹어서ㅋㅋ 이제 저녁 고민이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022360.1.8",
          "form": "아 ㅋㅋ 그렇구나 저녁엔 역시 삼겹살을(?)",
          "original_form": "아 ㅋㅋ 그렇구나 저녁엔 역시 삼겹살을(?)",
          "speaker_id": "1",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022360.1.9",
          "form": "삼겹살ㅋㅋ 그것도 좀 땡기네요 점심 결정하셨나요?",
          "original_form": "삼겹살ㅋㅋ 그것도 좀 땡기네요 점심 결정하셨나요?",
          "speaker_id": "2",
          "time": "20210829 13:30"
        },
        {
          "id": "MDRW2100022360.1.10",
          "form": "저 선택장애라 ㅋㅋ 한번 고르려면 시간 꽤 걸려요 최소 한시간은ㅋㅋ...",
          "original_form": "저 선택장애라 ㅋㅋ 한번 고르려면 시간 꽤 걸려요 최소 한시간은ㅋㅋ...",
          "speaker_id": "1",
          "time": "20210829 13:32"
        },
        {
          "id": "MDRW2100022360.1.11",
          "form": "ㅋㅋ 저도 그래요ㅜㅠ 게다가 배민은 선택지가 너무 많아요",
          "original_form": "ㅋㅋ 저도 그래요ㅜㅠ 게다가 배민은 선택지가 너무 많아요",
          "speaker_id": "2",
          "time": "20210829 13:32"
        },
        {
          "id": "MDRW2100022360.1.12",
          "form": "전 요기요 써서 ㅋㅋ 배민보단 요기요가 더 혜택이 좋더라구요",
          "original_form": "전 요기요 써서 ㅋㅋ 배민보단 요기요가 더 혜택이 좋더라구요",
          "speaker_id": "1",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100022360.1.13",
          "form": "아 저 요즘 요기요로 파바ㅋㅋ 엄청 샀어요ㅋㅋ",
          "original_form": "아 저 요즘 요기요로 파바ㅋㅋ 엄청 샀어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:34"
        },
        {
          "id": "MDRW2100022360.1.14",
          "form": "ㅋㅋㅋㅋ요기요 파바 ㅋㅋㅋ 요기요 덕분에 빵은 실컷 먹어보네요",
          "original_form": "ㅋㅋㅋㅋ요기요 파바 ㅋㅋㅋ 요기요 덕분에 빵은 실컷 먹어보네요",
          "speaker_id": "1",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100022360.1.15",
          "form": "그니까요 너무 사는게 아닌가싶어서 자제하는데 안사면 손해같고요ㅎㅎ",
          "original_form": "그니까요 너무 사는게 아닌가싶어서 자제하는데 안사면 손해같고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100022360.1.16",
          "form": "뭐 천원단위로 준다는데 꼭 해야죠 ㅋㅋ 어차피 합당하게 사는거니",
          "original_form": "뭐 천원단위로 준다는데 꼭 해야죠 ㅋㅋ 어차피 합당하게 사는거니",
          "speaker_id": "1",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022360.1.17",
          "form": "ㅋㅋ 슈퍼클럽인가? 가입하셨나요?",
          "original_form": "ㅋㅋ 슈퍼클럽인가? 가입하셨나요?",
          "speaker_id": "2",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022360.1.18",
          "form": "ㅋㅋㅋ 슈클 국룰이죠",
          "original_form": "ㅋㅋㅋ 슈클 국룰이죠",
          "speaker_id": "1",
