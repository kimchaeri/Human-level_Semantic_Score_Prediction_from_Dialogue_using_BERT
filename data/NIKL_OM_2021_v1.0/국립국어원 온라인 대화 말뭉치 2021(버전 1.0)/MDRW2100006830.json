      "utterance": [
        {
          "id": "MDRW2100006830.1.1",
          "form": "저도 8월 다가고 먹어서 아쉬웠어요...",
          "original_form": "저도 8월 다가고 먹어서 아쉬웠어요...",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100006830.1.2",
          "form": "꼬북칩츄러스맛 맛있죠",
          "original_form": "꼬북칩츄러스맛 맛있죠",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100006830.1.3",
          "form": "또 좋아하는 과자 있으신가요??",
          "original_form": "또 좋아하는 과자 있으신가요??",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100006830.1.4",
          "form": "저는 썬칩좋아해요",
          "original_form": "저는 썬칩좋아해요",
          "speaker_id": "1",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100006830.1.5",
          "form": "저 썬칩은 안 먹어봤어요..",
          "original_form": "저 썬칩은 안 먹어봤어요..",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100006830.1.6",
          "form": "어떤 맛인가요??",
          "original_form": "어떤 맛인가요??",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100006830.1.7",
          "form": "약간 짜고 단 맛이에요 ㅋㅋㅋㅋ  어떤과자좋아하세요?",
          "original_form": "약간 짜고 단 맛이에요 ㅋㅋㅋㅋ  어떤과자좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100006830.1.8",
          "form": "전 그냥 꼬북칩 젤 좋아해요!",
          "original_form": "전 그냥 꼬북칩 젤 좋아해요!",
          "speaker_id": "2",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100006830.1.9",
          "form": "그리고 꼬깔콘 매콤달콤도 맛있더라고요",
          "original_form": "그리고 꼬깔콘 매콤달콤도 맛있더라고요",
          "speaker_id": "2",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100006830.1.10",
          "form": "오! 저도꼬깔콘 매운거좋아해요",
          "original_form": "오! 저도꼬깔콘 매운거좋아해요",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100006830.1.11",
          "form": "맛있죠",
          "original_form": "맛있죠",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006830.1.12",
          "form": "점심은 뭐 드실건가요??",
          "original_form": "점심은 뭐 드실건가요??",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006830.1.13",
          "form": "저는 오늘은 순두부찌개먹을거에요!",
          "original_form": "저는 오늘은 순두부찌개먹을거에요!",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006830.1.14",
          "form": "와 맛있겠네요...",
          "original_form": "와 맛있겠네요...",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100006830.1.15",
          "form": "어떤거드실건가요?",
          "original_form": "어떤거드실건가요?",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006830.1.16",
          "form": "저 김치볶음밥이요!!",
          "original_form": "저 김치볶음밥이요!!",
          "speaker_id": "2",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100006830.1.17",
          "form": "오 요리해서드시나요?",
          "original_form": "오 요리해서드시나요?",
          "speaker_id": "1",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100006830.1.18",
          "form": "아뇽 사먹으려고요 ㅋㅋㅋ",
          "original_form": "아뇽 사먹으려고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100006830.1.19",
          "form": "매번 점심메뉴 어떻게고르세요?",
          "original_form": "매번 점심메뉴 어떻게고르세요?",
          "speaker_id": "1",
          "time": "20210908 10:40"
        }
