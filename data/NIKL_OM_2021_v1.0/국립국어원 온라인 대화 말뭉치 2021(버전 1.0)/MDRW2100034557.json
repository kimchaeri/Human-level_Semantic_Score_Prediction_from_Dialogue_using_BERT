      "utterance": [
        {
          "id": "MDRW2100034557.1.1",
          "form": "허리피기딱 하고다니기만 해도 도움좀되는거같아요",
          "original_form": "허리피기딱 하고다니기만 해도 도움좀되는거같아요",
          "speaker_id": "2",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100034557.1.2",
          "form": "바른자세 중요한데 어렵더라구요ㅠ",
          "original_form": "바른자세 중요한데 어렵더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100034557.1.3",
          "form": "코어근육을 키워야하는 걸까요?ㅋㅋ",
          "original_form": "코어근육을 키워야하는 걸까요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100034557.1.4",
          "form": "넘 중요하죠ㅋㅋㅋ",
          "original_form": "넘 중요하죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:09"
        },
        {
          "id": "MDRW2100034557.1.5",
          "form": "등과 배근육이 둘다있어야 허리펴진대요ㅋ",
          "original_form": "등과 배근육이 둘다있어야 허리펴진대요ㅋ",
          "speaker_id": "2",
          "time": "20210828 15:09"
        },
        {
          "id": "MDRW2100034557.1.6",
          "form": "자꾸 기대 앉다 보니까 허리가 자꾸 아프더라구요",
          "original_form": "자꾸 기대 앉다 보니까 허리가 자꾸 아프더라구요",
          "speaker_id": "1",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100034557.1.7",
          "form": "아ㅜㅜㅜ",
          "original_form": "아ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100034557.1.8",
          "form": "안돼는데",
          "original_form": "안돼는데",
          "speaker_id": "2",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100034557.1.9",
          "form": "허리운동하시나요",
          "original_form": "허리운동하시나요",
          "speaker_id": "2",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100034557.1.10",
          "form": "그냥 스트레칭만 좀 해요",
          "original_form": "그냥 스트레칭만 좀 해요",
          "speaker_id": "1",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100034557.1.11",
          "form": "허리운동 어떻게 해야할지 모르겠네요 ㅠ",
          "original_form": "허리운동 어떻게 해야할지 모르겠네요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100034557.1.12",
          "form": "저두 하나도 몰라서 ㅋㅋ 유투브보고 따라해요",
          "original_form": "저두 하나도 몰라서 ㅋㅋ 유투브보고 따라해요",
          "speaker_id": "2",
          "time": "20210828 15:18"
        },
        {
          "id": "MDRW2100034557.1.13",
          "form": "저도 좀 찾아봐야겠어요",
          "original_form": "저도 좀 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100034557.1.14",
          "form": "어떤 채널 보세요?",
          "original_form": "어떤 채널 보세요?",
          "speaker_id": "1",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100034557.1.15",
          "form": "머 많더라구요 빡빡이 아저씨도 봐요",
          "original_form": "머 많더라구요 빡빡이 아저씨도 봐요",
          "speaker_id": "2",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100034557.1.16",
          "form": "땅크부부랑",
          "original_form": "땅크부부랑",
          "speaker_id": "2",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100034557.1.17",
          "form": "들어봤어요 유명하더라구요",
          "original_form": "들어봤어요 유명하더라구요",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100034557.1.18",
          "form": "많이 찾아보셨군요 이미",
          "original_form": "많이 찾아보셨군요 이미",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034557.1.19",
          "form": "재생은 안 해봤어요 ㅋㅋ",
          "original_form": "재생은 안 해봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034557.1.20",
          "form": "ㅋㅋㅋㅋ바쁘시다보니 ㄲ",
          "original_form": "ㅋㅋㅋㅋ바쁘시다보니 ㄲ",
          "speaker_id": "2",
