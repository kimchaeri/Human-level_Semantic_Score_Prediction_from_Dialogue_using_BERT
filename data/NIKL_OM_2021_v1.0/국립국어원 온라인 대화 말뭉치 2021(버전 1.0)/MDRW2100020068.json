      "utterance": [
        {
          "id": "MDRW2100020068.1.1",
          "form": "선택지가 없었네요 지금 돌이켜보닠ㅋㅋㅋㅋ",
          "original_form": "선택지가 없었네요 지금 돌이켜보닠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020068.1.2",
          "form": "네 진짜로요 진심",
          "original_form": "네 진짜로요 진심",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020068.1.3",
          "form": "어떻게든 연인행세를 하려고했으나..",
          "original_form": "어떻게든 연인행세를 하려고했으나..",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020068.1.4",
          "form": "우짜노 저걸..",
          "original_form": "우짜노 저걸..",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020068.1.5",
          "form": "제가 좋아하는 마음이 없는걸 들켜버렸읍죠",
          "original_form": "제가 좋아하는 마음이 없는걸 들켜버렸읍죠",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020068.1.6",
          "form": "그리고 혹시 그거 아시나요",
          "original_form": "그리고 혹시 그거 아시나요",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020068.1.7",
          "form": "좋아하려고 노력할수록 싫어지는거",
          "original_form": "좋아하려고 노력할수록 싫어지는거",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020068.1.8",
          "form": "억지로 하다보니까 그렇게되더라구요",
          "original_form": "억지로 하다보니까 그렇게되더라구요",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020068.1.9",
          "form": "아 진짜요?????",
          "original_form": "아 진짜요?????",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020068.1.10",
          "form": "헐 그럼 더 어려우 문제 아닌가요",
          "original_form": "헐 그럼 더 어려우 문제 아닌가요",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020068.1.11",
          "form": "운",
          "original_form": "운",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020068.1.12",
          "form": "ㅜㅜ 안맞고 안좋아하는사람을 좋아하는 척하려다보니 더 괴롭더라고요",
          "original_form": "ㅜㅜ 안맞고 안좋아하는사람을 좋아하는 척하려다보니 더 괴롭더라고요",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020068.1.13",
          "form": "저는 제가 노력하면 좋아하는 마음이 생길줄알았는데 더 괴로워졌었답니다",
          "original_form": "저는 제가 노력하면 좋아하는 마음이 생길줄알았는데 더 괴로워졌었답니다",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020068.1.14",
          "form": "name1이 나쁘기보다는",
          "original_form": "&name1&이 나쁘기보다는",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020068.1.15",
          "form": "그 친구분 맞춰주려고 하시다 보니 그리된 것 같아요",
          "original_form": "그 친구분 맞춰주려고 하시다 보니 그리된 것 같아요",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020068.1.16",
          "form": "그래서 더 아이러니..",
          "original_form": "그래서 더 아이러니..",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020068.1.17",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020068.1.18",
          "form": "희대의 비극입니다",
          "original_form": "희대의 비극입니다",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020068.1.19",
          "form": "강한 마음먹고 내쳤어야 그친구도 상처를 덜 받았을텐데",
          "original_form": "강한 마음먹고 내쳤어야 그친구도 상처를 덜 받았을텐데",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020068.1.20",
          "form": "통찰력이 굉장하시네요",
          "original_form": "통찰력이 굉장하시네요",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020068.1.21",
          "form": "ㅠㅠㅠㅠㅠㅜㅠㅠㅠ바오밥님 잘못 아니여요",
          "original_form": "ㅠㅠㅠㅠㅠㅜㅠㅠㅠ바오밥님 잘못 아니여요",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020068.1.22",
          "form": "그 누구의 잘못도 아니어라",
          "original_form": "그 누구의 잘못도 아니어라",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020068.1.23",
          "form": "이런 위로를 받다니..",
          "original_form": "이런 위로를 받다니..",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020068.1.24",
          "form": "인생사 타이밍이 꼬여서",
          "original_form": "인생사 타이밍이 꼬여서",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020068.1.25",
          "form": "그런 것 같아요",
          "original_form": "그런 것 같아요",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020068.1.26",
          "form": "감동적...",
          "original_form": "감동적...",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020068.1.27",
          "form": "호오옥시 자책하시는 마음이 있으시다면",
          "original_form": "호오옥시 자책하시는 마음이 있으시다면",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020068.1.28",
          "form": "털어버리세요",
          "original_form": "털어버리세요",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020068.1.29",
          "form": "저 되게 제 잘못이라고 생각했었거든요",
          "original_form": "저 되게 제 잘못이라고 생각했었거든요",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020068.1.30",
          "form": "오앜ㅋㅋㅋㅋ 감사해요",
          "original_form": "오앜ㅋㅋㅋㅋ 감사해요",
