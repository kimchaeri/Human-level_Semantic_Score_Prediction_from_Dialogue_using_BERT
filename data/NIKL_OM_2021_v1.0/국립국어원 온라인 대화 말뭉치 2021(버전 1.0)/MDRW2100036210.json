      "utterance": [
        {
          "id": "MDRW2100036210.1.1",
          "form": "백신은 언제 맞아요",
          "original_form": "백신은 언제 맞아요",
          "speaker_id": "1",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100036210.1.2",
          "form": "아 이렇게하는거였어요 대화가후딱후딱",
          "original_form": "아 이렇게하는거였어요 대화가후딱후딱",
          "speaker_id": "2",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100036210.1.3",
          "form": "그럼그럼",
          "original_form": "그럼그럼",
          "speaker_id": "1",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100036210.1.4",
          "form": "백신9월10일요",
          "original_form": "백신9월10일요",
          "speaker_id": "2",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100036210.1.5",
          "form": "2차는언제",
          "original_form": "2차는언제",
          "speaker_id": "2",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100036210.1.6",
          "form": "예요",
          "original_form": "예요",
          "speaker_id": "2",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100036210.1.7",
          "form": "오 아직 좀 남앗둔요",
          "original_form": "오 아직 좀 남앗둔요",
          "speaker_id": "1",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100036210.1.8",
          "form": "이번주 토욜이에용",
          "original_form": "이번주 토욜이에용",
          "speaker_id": "1",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100036210.1.9",
          "form": "헙",
          "original_form": "헙",
          "speaker_id": "2",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100036210.1.10",
          "form": "곧이네요",
          "original_form": "곧이네요",
          "speaker_id": "2",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100036210.1.11",
          "form": "백신떨려요",
          "original_form": "백신떨려요",
          "speaker_id": "2",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100036210.1.12",
          "form": "점점 화이자맞고 부작용 나타난 사람들 기사가 뜨니깐",
          "original_form": "점점 화이자맞고 부작용 나타난 사람들 기사가 뜨니깐",
          "speaker_id": "1",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100036210.1.13",
          "form": "동생도골골했어요",
          "original_form": "동생도골골했어요",
          "speaker_id": "2",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100036210.1.14",
          "form": "2차 맞으러가기 시러용",
          "original_form": "2차 맞으러가기 시러용",
          "speaker_id": "1",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100036210.1.15",
          "form": "무섭더라구요",
          "original_form": "무섭더라구요",
          "speaker_id": "2",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100036210.1.16",
          "form": "청원글 봐써용??",
          "original_form": "청원글 봐써용??",
          "speaker_id": "1",
          "time": "20210830 19:04"
        },
        {
          "id": "MDRW2100036210.1.17",
          "form": "아니요 혹시부인죽은거그건가요..?",
          "original_form": "아니요 혹시부인죽은거그건가요..?",
          "speaker_id": "2",
          "time": "20210830 19:04"
        },
        {
          "id": "MDRW2100036210.1.18",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210830 19:04"
        },
        {
          "id": "MDRW2100036210.1.19",
          "form": "백신 맞고 급성 백혈병온 체육교사",
          "original_form": "백신 맞고 급성 백혈병온 체육교사",
          "speaker_id": "1",
          "time": "20210830 19:04"
        },
        {
          "id": "MDRW2100036210.1.20",
          "form": "백신 맞고 내장 다 썩은 초등 교사",
          "original_form": "백신 맞고 내장 다 썩은 초등 교사",
          "speaker_id": "1",
          "time": "20210830 19:04"
        },
        {
          "id": "MDRW2100036210.1.21",
          "form": "둘다 남자이긴한데 무섭네영....",
          "original_form": "둘다 남자이긴한데 무섭네영....",
          "speaker_id": "1",
          "time": "20210830 19:04"
        },
        {
          "id": "MDRW2100036210.1.22",
          "form": "..헐",
          "original_form": "..헐",
          "speaker_id": "2",
          "time": "20210830 19:05"
        },
        {
          "id": "MDRW2100036210.1.23",
          "form": "내장이다썩었다구..?",
          "original_form": "내장이다썩었다구..?",
          "speaker_id": "2",
          "time": "20210830 19:05"
        },
        {
          "id": "MDRW2100036210.1.24",
          "form": "백혈병도헐이네..",
          "original_form": "백혈병도헐이네..",
          "speaker_id": "2",
          "time": "20210830 19:05"
        },
        {
          "id": "MDRW2100036210.1.25",
          "form": "그니깐 무섭",
          "original_form": "그니깐 무섭",
          "speaker_id": "1",
          "time": "20210830 19:05"
        },
        {
          "id": "MDRW2100036210.1.26",
          "form": "그래서 남자들은 좀 위험하겟다 하고 넘기다가",
          "original_form": "그래서 남자들은 좀 위험하겟다 하고 넘기다가",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036210.1.27",
          "form": "뉴질랜드 화이자 첫 사망자가 나왓는데 30대 여성이엇대서",
          "original_form": "뉴질랜드 화이자 첫 사망자가 나왓는데 30대 여성이엇대서",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036210.1.28",
          "form": "후달달햇어요",
          "original_form": "후달달햇어요",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036210.1.29",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "2",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036210.1.30",
          "form": "무서운데",
          "original_form": "무서운데",
          "speaker_id": "2",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036210.1.31",
          "form": "혹시 제가 어떻게 되더라도 저와의 대화 잘 기억해 주세요\n..",
          "original_form": "혹시 제가 어떻게 되더라도 저와의 대화 잘 기억해 주세요\n..",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036210.1.32",
          "form": "백신맞고죽는건좀슬픈데",
          "original_form": "백신맞고죽는건좀슬픈데",
          "speaker_id": "2",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036210.1.33",
          "form": "아니유언을여기서",
          "original_form": "아니유언을여기서",
          "speaker_id": "2",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036210.1.34",
          "form": "그니까여...",
          "original_form": "그니까여...",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036210.1.35",
          "form": "ㅇㅈㄹ",
          "original_form": "ㅇㅈㄹ",
          "speaker_id": "2",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036210.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:06"
        }
      ]
