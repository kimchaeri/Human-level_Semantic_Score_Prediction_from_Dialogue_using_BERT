      "utterance": [
        {
          "id": "MDRW2100006651.1.1",
          "form": "name1님은 해외 중엔 어디가 좋으셨나요?",
          "original_form": "&name1&님은 해외 중엔 어디가 좋으셨나요?",
          "speaker_id": "2",
          "time": "20210908 05:16"
        },
        {
          "id": "MDRW2100006651.1.2",
          "form": "전 오사카 갔을 때가 제일 좋았아요",
          "original_form": "전 오사카 갔을 때가 제일 좋았아요",
          "speaker_id": "1",
          "time": "20210908 05:19"
        },
        {
          "id": "MDRW2100006651.1.3",
          "form": "오 오사카도 가보고싶었는데!",
          "original_form": "오 오사카도 가보고싶었는데!",
          "speaker_id": "2",
          "time": "20210908 05:20"
        },
        {
          "id": "MDRW2100006651.1.4",
          "form": "오사카 가볼만해요",
          "original_form": "오사카 가볼만해요",
          "speaker_id": "1",
          "time": "20210908 05:21"
        },
        {
          "id": "MDRW2100006651.1.5",
          "form": "다만 한국인이 워낙 많아서 여기가 한국인가 일본인가 헷갈리긴하지만요 ㅋㅋㅋ",
          "original_form": "다만 한국인이 워낙 많아서 여기가 한국인가 일본인가 헷갈리긴하지만요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:22"
        },
        {
          "id": "MDRW2100006651.1.6",
          "form": "아 진짜 그럴 것 같아요ㅋㅋㅋㅋ",
          "original_form": "아 진짜 그럴 것 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:22"
        },
        {
          "id": "MDRW2100006651.1.7",
          "form": "일본엔 확실이 한중일 사람들이 많더라고요",
          "original_form": "일본엔 확실이 한중일 사람들이 많더라고요",
          "speaker_id": "2",
          "time": "20210908 05:22"
        },
        {
          "id": "MDRW2100006651.1.8",
          "form": "그니까요 걷다가 길 묻고 싶을 때 옆에 있는 분 붙잡고 물어보면 한국분이시더라고요 ㅋㅋㅋㅋ",
          "original_form": "그니까요 걷다가 길 묻고 싶을 때 옆에 있는 분 붙잡고 물어보면 한국분이시더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:24"
        },
        {
          "id": "MDRW2100006651.1.9",
          "form": "한국말로 길 물어보고 다녔어요",
          "original_form": "한국말로 길 물어보고 다녔어요",
          "speaker_id": "1",
          "time": "20210908 05:24"
        },
        {
          "id": "MDRW2100006651.1.10",
          "form": "와 진짜 한국사람 많은가봐요ㅋㅋㅋ",
          "original_form": "와 진짜 한국사람 많은가봐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:25"
        },
        {
          "id": "MDRW2100006651.1.11",
          "form": "홋카이도는 그 만큼은 아니었는데",
          "original_form": "홋카이도는 그 만큼은 아니었는데",
          "speaker_id": "2",
          "time": "20210908 05:25"
        },
        {
          "id": "MDRW2100006651.1.12",
          "form": "그래서 의사소통이 조금 어렵긴하더라구요",
          "original_form": "그래서 의사소통이 조금 어렵긴하더라구요",
          "speaker_id": "2",
          "time": "20210908 05:25"
        },
        {
          "id": "MDRW2100006651.1.13",
          "form": "확실히 일본 중에서도 오사카가 확실한 관광지 느낌이긴 한 것 같아요",
          "original_form": "확실히 일본 중에서도 오사카가 확실한 관광지 느낌이긴 한 것 같아요",
          "speaker_id": "1",
          "time": "20210908 05:27"
        },
        {
          "id": "MDRW2100006651.1.14",
          "form": "한글 메뉴판 있는 곳도 상당히 많고 그렇더라고요",
          "original_form": "한글 메뉴판 있는 곳도 상당히 많고 그렇더라고요",
          "speaker_id": "1",
          "time": "20210908 05:27"
        },
        {
          "id": "MDRW2100006651.1.15",
          "form": "확실히 편할 것 같아요",
          "original_form": "확실히 편할 것 같아요",
          "speaker_id": "2",
          "time": "20210908 05:28"
        },
        {
          "id": "MDRW2100006651.1.16",
          "form": "역시 의사소통이 되어야 관광하기 좋죠",
          "original_form": "역시 의사소통이 되어야 관광하기 좋죠",
          "speaker_id": "2",
          "time": "20210908 05:28"
        },
        {
          "id": "MDRW2100006651.1.17",
          "form": "맞아요. 나중에 꼭 가보세요 옆에 있는 교토도 같이 가면 좋아요",
          "original_form": "맞아요. 나중에 꼭 가보세요 옆에 있는 교토도 같이 가면 좋아요",
          "speaker_id": "1",
          "time": "20210908 05:29"
        },
        {
          "id": "MDRW2100006651.1.18",
          "form": "오오 다음에 꼭 묶어서 가볼게요!",
          "original_form": "오오 다음에 꼭 묶어서 가볼게요!",
          "speaker_id": "2",
          "time": "20210908 05:29"
        },
        {
          "id": "MDRW2100006651.1.19",
          "form": "네네 오사카가 메인이었는데 교토 풍경이 워낙 예뻐서 교토 사진을 더 많이 찍었더라고요 ㅋㅋㅋ",
          "original_form": "네네 오사카가 메인이었는데 교토 풍경이 워낙 예뻐서 교토 사진을 더 많이 찍었더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:31"
        },
        {
          "id": "MDRW2100006651.1.20",
          "form": "오오 풍경이 예쁘다니 더 기대되네요",
          "original_form": "오오 풍경이 예쁘다니 더 기대되네요",
          "speaker_id": "2",
          "time": "20210908 05:31"
        },
        {
          "id": "MDRW2100006651.1.21",
          "form": "삿포로도 여름에 갔는데도 너무 예쁘더라고요",
          "original_form": "삿포로도 여름에 갔는데도 너무 예쁘더라고요",
          "speaker_id": "2",
          "time": "20210908 05:31"
        }
      ]
    }
