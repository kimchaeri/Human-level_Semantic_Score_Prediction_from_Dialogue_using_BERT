      "utterance": [
        {
          "id": "MDRW2100027033.1.1",
          "form": "아니요 ㅎㅎ name1은요?",
          "original_form": "아니요 ㅎㅎ &name1&은요?",
          "speaker_id": "2",
          "time": "20210902 09:15"
        },
        {
          "id": "MDRW2100027033.1.2",
          "form": "저도 키우지는 않아요",
          "original_form": "저도 키우지는 않아요",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027033.1.3",
          "form": "ㅋㅋ키우고 싶으신거는요",
          "original_form": "ㅋㅋ키우고 싶으신거는요",
          "speaker_id": "2",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027033.1.4",
          "form": "만약 키운다면 쪼끔한 치와와 강아지요",
          "original_form": "만약 키운다면 쪼끔한 치와와 강아지요",
          "speaker_id": "1",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100027033.1.5",
          "form": "혹키우시고 싶은 동물 있나요",
          "original_form": "혹키우시고 싶은 동물 있나요",
          "speaker_id": "1",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100027033.1.6",
          "form": "전 손 많이 안가는게 좋은데",
          "original_form": "전 손 많이 안가는게 좋은데",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100027033.1.7",
          "form": "그럼 식물은 어떤가요 ?",
          "original_form": "그럼 식물은 어떤가요 ?",
          "speaker_id": "1",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100027033.1.8",
          "form": "키우고 있어요 방토!",
          "original_form": "키우고 있어요 방토!",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100027033.1.9",
          "form": "방토가 잘 열리나요?",
          "original_form": "방토가 잘 열리나요?",
          "speaker_id": "1",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100027033.1.10",
          "form": "키우긴 수월한가요 ?",
          "original_form": "키우긴 수월한가요 ?",
          "speaker_id": "1",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100027033.1.11",
          "form": "방토요? 완전 쉽죠",
          "original_form": "방토요? 완전 쉽죠",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100027033.1.12",
          "form": "전 열매 달리는 채소는 키워 본적이 없어요\r\n아파트에서도 가능한가요 ?",
          "original_form": "전 열매 달리는 채소는 키워 본적이 없어요\r\n아파트에서도 가능한가요 ?",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100027033.1.13",
          "form": "네 아파트 베란다에서 자라고 있습니다",
          "original_form": "네 아파트 베란다에서 자라고 있습니다",
          "speaker_id": "2",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100027033.1.14",
          "form": "베란다 화분에서도 잘크나봐요",
          "original_form": "베란다 화분에서도 잘크나봐요",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
