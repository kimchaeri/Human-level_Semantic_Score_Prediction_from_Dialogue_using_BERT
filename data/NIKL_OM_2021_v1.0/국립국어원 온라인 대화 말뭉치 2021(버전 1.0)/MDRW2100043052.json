      "utterance": [
        {
          "id": "MDRW2100043052.1.1",
          "form": "맞아요 중국에서 먹는게 찐이에요",
          "original_form": "맞아요 중국에서 먹는게 찐이에요",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043052.1.2",
          "form": "유튭 자주 보세요?",
          "original_form": "유튭 자주 보세요?",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043052.1.3",
          "form": "유튭 하루에 한번은 보는것 같아요 ㅎㅎ",
          "original_form": "유튭 하루에 한번은 보는것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043052.1.4",
          "form": "우와....별로 안보시네요...",
          "original_form": "우와....별로 안보시네요...",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043052.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043052.1.6",
          "form": "저는 거의 유튜브에서 삽니다..",
          "original_form": "저는 거의 유튜브에서 삽니다..",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043052.1.7",
          "form": "출퇴근때 무조건 유튜브",
          "original_form": "출퇴근때 무조건 유튜브",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043052.1.8",
          "form": "밥먹을때 유튜브",
          "original_form": "밥먹을때 유튜브",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043052.1.9",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043052.1.10",
          "form": "와 유튜브가 재밌긴하죠 ㅋㅋ",
          "original_form": "와 유튜브가 재밌긴하죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043052.1.11",
          "form": "뭐 보시나요?",
          "original_form": "뭐 보시나요?",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043052.1.12",
          "form": "젘ㅋㅋㅋ주로 브이로그봐요",
          "original_form": "젘ㅋㅋㅋ주로 브이로그봐요",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100043052.1.13",
          "form": "소소한 일상보는게 저는 재밌더라구요?",
          "original_form": "소소한 일상보는게 저는 재밌더라구요?",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100043052.1.14",
          "form": "아 전 카페브이로그 보는데 재밌더라구요",
          "original_form": "아 전 카페브이로그 보는데 재밌더라구요",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043052.1.15",
          "form": "맞아요 그런것도 힐링ㅋㅋㅋㅋ",
          "original_form": "맞아요 그런것도 힐링ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043052.1.16",
          "form": "음료만드는거 보는거 ㅈㅐ밌어요",
          "original_form": "음료만드는거 보는거 ㅈㅐ밌어요",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043052.1.17",
          "form": "음료만드는거 신기하고 잼써요 ㅋㅋ",
          "original_form": "음료만드는거 신기하고 잼써요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043052.1.18",
          "form": "그죠그죠 ㅋㅋㅋㅋ",
          "original_form": "그죠그죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043052.1.19",
          "form": "네네 맞아요 ㅋㅋ",
          "original_form": "네네 맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043052.1.20",
          "form": "썸머썸머라고 뉴욕에서 사시는분 브이로그도재밌더라구요",
          "original_form": "썸머썸머라고 뉴욕에서 사시는분 브이로그도재밌더라구요",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043052.1.21",
          "form": "배경이 외국이라 뭔가 다 신기하고 잼써요",
          "original_form": "배경이 외국이라 뭔가 다 신기하고 잼써요",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043052.1.22",
          "form": "아!! 저 알아요 ㅋㅋㅋㅋㅋ",
          "original_form": "아!! 저 알아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:51"
        },
        {
          "id": "MDRW2100043052.1.23",
          "form": "오래전부터 봤는데 결혼도하시고",
          "original_form": "오래전부터 봤는데 결혼도하시고",
          "speaker_id": "2",
          "time": "20210907 13:51"
        },
        {
          "id": "MDRW2100043052.1.24",
          "form": "잘 정착해서 보시는게 재밌어욬ㅋㅋㅋㅋ",
          "original_form": "잘 정착해서 보시는게 재밌어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:51"
        },
        {
          "id": "MDRW2100043052.1.25",
          "form": "엥 보시는게 아니라 사시는게 ㅋㅋㅋㅋ",
          "original_form": "엥 보시는게 아니라 사시는게 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:51"
        },
        {
          "id": "MDRW2100043052.1.26",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:54"
        },
        {
          "id": "MDRW2100043052.1.27",
          "form": "네 그분 말씀도 잘하시고 재밌더라구요",
          "original_form": "네 그분 말씀도 잘하시고 재밌더라구요",
          "speaker_id": "1",
          "time": "20210907 13:54"
        },
        {
          "id": "MDRW2100043052.1.28",
          "form": "뉴욕일상도 재밌고 썰 푸시는것도 잼써요",
          "original_form": "뉴욕일상도 재밌고 썰 푸시는것도 잼써요",
