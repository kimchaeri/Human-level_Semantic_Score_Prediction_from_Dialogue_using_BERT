      "utterance": [
        {
          "id": "MDRW2100026706.1.1",
          "form": "그래서 언니 화나가지고",
          "original_form": "그래서 언니 화나가지고",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026706.1.2",
          "form": "그만둘것 같아",
          "original_form": "그만둘것 같아",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026706.1.3",
          "form": "당연하다 이 ㅈ..ㄹ하고잇네.....",
          "original_form": "당연하다 이 ㅈ..ㄹ하고잇네.....",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026706.1.4",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026706.1.5",
          "form": "근데 진짜 저번에 보내준 상표 여러개 있고",
          "original_form": "근데 진짜 저번에 보내준 상표 여러개 있고",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026706.1.6",
          "form": "그래가지고 배달이 진짜 많거든",
          "original_form": "그래가지고 배달이 진짜 많거든",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026706.1.7",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026706.1.8",
          "form": "근데도",
          "original_form": "근데도",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026706.1.9",
          "form": "뭐가 힘든거냐",
          "original_form": "뭐가 힘든거냐",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026706.1.10",
          "form": "어떤 일을 더 하는것 같냐고 계속 그런대",
          "original_form": "어떤 일을 더 하는것 같냐고 계속 그런대",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026706.1.11",
          "form": "지 가겐데도",
          "original_form": "지 가겐데도",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026706.1.12",
          "form": "아는게없나봐...",
          "original_form": "아는게없나봐...",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026706.1.13",
          "form": "그 워크맨인가 거기다가 일하는거 등록한 알바생이",
          "original_form": "그 워크맨인가 거기다가 일하는거 등록한 알바생이",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026706.1.14",
          "form": "일정 인원 이상되면",
          "original_form": "일정 인원 이상되면",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026706.1.15",
          "form": "정부에서 사장한테 지원금을 준대",
          "original_form": "정부에서 사장한테 지원금을 준대",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026706.1.16",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026706.1.17",
          "form": "근데 그거 글써서 올리라고 하고",
          "original_form": "근데 그거 글써서 올리라고 하고",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026706.1.18",
          "form": "가입할때 설정한 아이디 비밀번호 다 알려 달랬다는 거임",
          "original_form": "가입할때 설정한 아이디 비밀번호 다 알려 달랬다는 거임",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026706.1.19",
          "form": "그래서 언니가 그건 개인정보지 않냐고 하니까",
          "original_form": "그래서 언니가 그건 개인정보지 않냐고 하니까",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026706.1.20",
          "form": "안알려주려고 한 애 너뿐이다 막 이런식으로 했나보더라",
          "original_form": "안알려주려고 한 애 너뿐이다 막 이런식으로 했나보더라",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026706.1.21",
          "form": "머...머고",
          "original_form": "머...머고",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026706.1.22",
          "form": "진짜 갑질...",
          "original_form": "진짜 갑질...",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026706.1.23",
          "form": "아오진짜...",
          "original_form": "아오진짜...",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026706.1.24",
          "form": "공감능력 개없네",
          "original_form": "공감능력 개없네",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026706.1.25",
          "form": "사장부인이 그랬다는거야?",
          "original_form": "사장부인이 그랬다는거야?",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026706.1.26",
          "form": "근데 사장 카톡에 올라온",
          "original_form": "근데 사장 카톡에 올라온",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026706.1.27",
          "form": "ㅇㅇ 맞아",
          "original_form": "ㅇㅇ 맞아",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026706.1.28",
          "form": "사진 보면",
          "original_form": "사진 보면",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026706.1.29",
          "form": "ㄷㄷㄷ",
          "original_form": "ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026706.1.30",
          "form": "그냥 딱 그렇게 생김ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 딱 그렇게 생김ㅋㅋㅋㅋㅋ",
