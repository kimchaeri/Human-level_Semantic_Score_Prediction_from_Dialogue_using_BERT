      "utterance": [
        {
          "id": "MDRW2100043727.1.1",
          "form": "마라탕 좋아하세요?",
          "original_form": "마라탕 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043727.1.2",
          "form": "네! 완전완전요",
          "original_form": "네! 완전완전요",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043727.1.3",
          "form": "요즘에는 줄였지만요 ㅋㅋ 좋아하시나요?",
          "original_form": "요즘에는 줄였지만요 ㅋㅋ 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043727.1.4",
          "form": "네 저도 좋아해요",
          "original_form": "네 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043727.1.5",
          "form": "마라탕에 라면 넣은 거 먹고싶네요",
          "original_form": "마라탕에 라면 넣은 거 먹고싶네요",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043727.1.6",
          "form": "맛있을 것 같아요 라면사리는 넣어본적없는데..!",
          "original_form": "맛있을 것 같아요 라면사리는 넣어본적없는데..!",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043727.1.7",
          "form": "한 번 시도해보세요 추천추천",
          "original_form": "한 번 시도해보세요 추천추천",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043727.1.8",
          "form": "양념이 잘배서 맛날 것 같아요 ㅋㅋ",
          "original_form": "양념이 잘배서 맛날 것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043727.1.9",
          "form": "마라탕에 양고기 소고기 어떤거 넣으시나요?",
          "original_form": "마라탕에 양고기 소고기 어떤거 넣으시나요?",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043727.1.10",
          "form": "소고기 넣어요!",
          "original_form": "소고기 넣어요!",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043727.1.11",
          "form": "저는 같이 가는 사람에 따라 다른것 같아요",
          "original_form": "저는 같이 가는 사람에 따라 다른것 같아요",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043727.1.12",
          "form": "양고기가 호불호가 많이 갈리죠",
          "original_form": "양고기가 호불호가 많이 갈리죠",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043727.1.13",
          "form": "양고기 못 먹는 건 아닌데 퀄리티가 천차만별이라 좀 안전빵으로 골라요",
          "original_form": "양고기 못 먹는 건 아닌데 퀄리티가 천차만별이라 좀 안전빵으로 골라요",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043727.1.14",
          "form": "아 그렇기도 하겠네요",
          "original_form": "아 그렇기도 하겠네요",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043727.1.15",
          "form": "맵기는 어떻게 드세요?",
          "original_form": "맵기는 어떻게 드세요?",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043727.1.16",
          "form": "보통보다 조금 더 매운맛? 그런 단계?로 먹어요",
          "original_form": "보통보다 조금 더 매운맛? 그런 단계?로 먹어요",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043727.1.17",
          "form": "그러시군요 저는 마라는 너무 싸한건 싫어서 보통이나 2단계정도로 먹어요 ㅠㅠ",
          "original_form": "그러시군요 저는 마라는 너무 싸한건 싫어서 보통이나 2단계정도로 먹어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043727.1.18",
          "form": "샹궈도 드신적 있으세요?",
          "original_form": "샹궈도 드신적 있으세요?",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043727.1.19",
          "form": "네 샹궈도 좋아해요ㅎㅎ!",
          "original_form": "네 샹궈도 좋아해요ㅎㅎ!",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043727.1.20",
          "form": "마라탕과는 또다른 매력이에요 ㅍㅎ",
          "original_form": "마라탕과는 또다른 매력이에요 ㅍㅎ",
