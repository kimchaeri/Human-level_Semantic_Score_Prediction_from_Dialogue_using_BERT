      "utterance": [
        {
          "id": "MDRW2100005809.1.1",
          "form": "비타민.오메가3.칼슘요",
          "original_form": "비타민.오메가3.칼슘요",
          "speaker_id": "2",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100005809.1.2",
          "form": "영양제를 저도 최근에 챙겨먹기 시작했는데 꾸준히 먹어야 한다더군요",
          "original_form": "영양제를 저도 최근에 챙겨먹기 시작했는데 꾸준히 먹어야 한다더군요",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100005809.1.3",
          "form": "저는 비타민 씨를 먹고 있습니다",
          "original_form": "저는 비타민 씨를 먹고 있습니다",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100005809.1.4",
          "form": "딸이 그게 감기예방에 좋다고 해서요",
          "original_form": "딸이 그게 감기예방에 좋다고 해서요",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100005809.1.5",
          "form": "챙겨먹을 나이라..저도",
          "original_form": "챙겨먹을 나이라..저도",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100005809.1.6",
          "form": "홍삼은 꾸준히 먹고 있습니다",
          "original_form": "홍삼은 꾸준히 먹고 있습니다",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100005809.1.7",
          "form": "기력회복에 좋다고 하더군요",
          "original_form": "기력회복에 좋다고 하더군요",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100005809.1.8",
          "form": "홍삼도 좋죠",
          "original_form": "홍삼도 좋죠",
          "speaker_id": "2",
          "time": "20210907 15:37"
        },
        {
          "id": "MDRW2100005809.1.9",
          "form": "네 이제 잘 챙겨먹어야겠더군요",
          "original_form": "네 이제 잘 챙겨먹어야겠더군요",
          "speaker_id": "1",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100005809.1.10",
          "form": "갑자기 면역력이 떨어지기도 해서요",
          "original_form": "갑자기 면역력이 떨어지기도 해서요",
          "speaker_id": "1",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100005809.1.11",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100005809.1.12",
          "form": "잘챙겨먹으려고 노력중이예요",
          "original_form": "잘챙겨먹으려고 노력중이예요",
          "speaker_id": "2",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100005809.1.13",
          "form": "제가 면역력 악화로 고생해봐서",
          "original_form": "제가 면역력 악화로 고생해봐서",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005809.1.14",
          "form": "이젠 꼬박꼬박 챙기고 있어요",
          "original_form": "이젠 꼬박꼬박 챙기고 있어요",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005809.1.15",
          "form": "건강은 안아플때 챙겨야하더군요",
          "original_form": "건강은 안아플때 챙겨야하더군요",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005809.1.16",
          "form": "아.그렇군요",
          "original_form": "아.그렇군요",
          "speaker_id": "2",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005809.1.17",
          "form": "저는 젊은나이도아닌데 자꾸 안챙겨먹네요",
          "original_form": "저는 젊은나이도아닌데 자꾸 안챙겨먹네요",
          "speaker_id": "2",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005809.1.18",
          "form": "운동은 하시나요?",
          "original_form": "운동은 하시나요?",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100005809.1.19",
          "form": "런닝가끔요",
          "original_form": "런닝가끔요",
          "speaker_id": "2",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100005809.1.20",
          "form": "저는 산책겸 걷기운동 합니다",
          "original_form": "저는 산책겸 걷기운동 합니다",
          "speaker_id": "1",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100005809.1.21",
          "form": "그거라도 안하면 안될듯 해서요",
          "original_form": "그거라도 안하면 안될듯 해서요",
          "speaker_id": "1",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100005809.1.22",
          "form": "운동을 해야되는데 귀찮아서요",
          "original_form": "운동을 해야되는데 귀찮아서요",
          "speaker_id": "2",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100005809.1.23",
          "form": "열심히해야되는데 잘안되네요",
          "original_form": "열심히해야되는데 잘안되네요",
          "speaker_id": "2",
          "time": "20210907 16:06"
        }
