      "utterance": [
        {
          "id": "MDRW2100037711.1.1",
          "form": "쇼핑 자주하세요?",
          "original_form": "쇼핑 자주하세요?",
          "speaker_id": "1",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100037711.1.2",
          "form": "아니요 장보는것 말고는 자주 안해요",
          "original_form": "아니요 장보는것 말고는 자주 안해요",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037711.1.3",
          "form": "요즘엔 저도 인터넷으러 장보는거같아용!",
          "original_form": "요즘엔 저도 인터넷으러 장보는거같아용!",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100037711.1.4",
          "form": "인터넷으로 보시는구나 ssg사용하시나요?",
          "original_form": "인터넷으로 보시는구나 ssg사용하시나요?",
          "speaker_id": "2",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100037711.1.5",
          "form": "전 11번가 주로 쓰는데 거기도 이마트장보기있어서 그걸루용!",
          "original_form": "전 11번가 주로 쓰는데 거기도 이마트장보기있어서 그걸루용!",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100037711.1.6",
          "form": "아하 그걸로 하시는구나",
          "original_form": "아하 그걸로 하시는구나",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100037711.1.7",
          "form": "저는 집에  정수기가 없어서. 생수를 사먹는데",
          "original_form": "저는 집에  정수기가 없어서. 생수를 사먹는데",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100037711.1.8",
          "form": "매번 ssg으로 시켜먹어요",
          "original_form": "매번 ssg으로 시켜먹어요",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100037711.1.9",
          "form": "쓱배송 짱편리 ..굿",
          "original_form": "쓱배송 짱편리 ..굿",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037711.1.10",
          "form": "노브랜드 물품도 너무 좋아요",
          "original_form": "노브랜드 물품도 너무 좋아요",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037711.1.11",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037711.1.12",
          "form": "노브랜드 추천해주실거 있어요?",
          "original_form": "노브랜드 추천해주실거 있어요?",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037711.1.13",
          "form": "노브랜드 감자튀김, 칠리새우, 닭꼬치 좋아요",
          "original_form": "노브랜드 감자튀김, 칠리새우, 닭꼬치 좋아요",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100037711.1.14",
          "form": "에프있으시면 돌려먹기 좋으실거예요",
          "original_form": "에프있으시면 돌려먹기 좋으실거예요",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100037711.1.15",
          "form": "오호 닭꼬치도 맛나용?",
          "original_form": "오호 닭꼬치도 맛나용?",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037711.1.16",
          "form": "네! 데리야끼가 전 제일 좋아요",
          "original_form": "네! 데리야끼가 전 제일 좋아요",
          "speaker_id": "2",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100037711.1.17",
          "form": "말만 들어도 맛나겠오용 ㅠㅠ 저두 사먹어볼게요",
          "original_form": "말만 들어도 맛나겠오용 ㅠㅠ 저두 사먹어볼게요",
