        {
          "id": "MDRW2100012106.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100012106.1.2",
          "form": "게임 문외한이네요 ㅠ",
          "original_form": "게임 문외한이네요 ㅠ",
          "speaker_id": "2",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100012106.1.3",
          "form": "게임 뭐하시나요",
          "original_form": "게임 뭐하시나요",
          "speaker_id": "2",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100012106.1.4",
          "form": "요즘은 잘안해요",
          "original_form": "요즘은 잘안해요",
          "speaker_id": "1",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100012106.1.5",
          "form": "예전에 coc랑 데바데 했었어요",
          "original_form": "예전에 coc랑 데바데 했었어요",
          "speaker_id": "1",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100012106.1.6",
          "form": "오 정말 처음들어요..ㅋ 피파..배그..이정도만 들어봤는데 ㅋ",
          "original_form": "오 정말 처음들어요..ㅋ 피파..배그..이정도만 들어봤는데 ㅋ",
          "speaker_id": "2",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100012106.1.7",
          "form": "유명한게임이 아니예요ㅋㅋ",
          "original_form": "유명한게임이 아니예요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100012106.1.8",
          "form": "게임 한번도 해보신적 없으신가요?",
          "original_form": "게임 한번도 해보신적 없으신가요?",
