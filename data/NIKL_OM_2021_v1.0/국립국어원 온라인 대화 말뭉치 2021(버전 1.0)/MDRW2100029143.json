      "utterance": [
        {
          "id": "MDRW2100029143.1.1",
          "form": "단순해서",
          "original_form": "단순해서",
          "speaker_id": "2",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100029143.1.2",
          "form": "땅칠하눈고라 막해도 욕할사람 없어요 ㅋㅋ",
          "original_form": "땅칠하눈고라 막해도 욕할사람 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100029143.1.3",
          "form": "와리오도 기대돼요",
          "original_form": "와리오도 기대돼요",
          "speaker_id": "2",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100029143.1.4",
          "form": "와리오 체험판해봤는데 재밌더라구요!",
          "original_form": "와리오 체험판해봤는데 재밌더라구요!",
          "speaker_id": "1",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100029143.1.5",
          "form": "오 저 아직 귀찮아서 못해봤어요 ㅋㅋㅋ",
          "original_form": "오 저 아직 귀찮아서 못해봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100029143.1.6",
          "form": "ㅋㅋㅋ 재밌어요 약간 리듬세상 생각도 나고ㅋㅋ",
          "original_form": "ㅋㅋㅋ 재밌어요 약간 리듬세상 생각도 나고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029143.1.7",
          "form": "그거 진짜 잼썼는데 그건 언제나올까요 ㅠㅠ 갓겜",
          "original_form": "그거 진짜 잼썼는데 그건 언제나올까요 ㅠㅠ 갓겜",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029143.1.8",
          "form": "진짜 갓겜이죠ㅠㅠ스위치로 나왔음 좋겠는데ㅠㅠ",
          "original_form": "진짜 갓겜이죠ㅠㅠ스위치로 나왔음 좋겠는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029143.1.9",
          "form": "만들고 있을거라 믿어요",
          "original_form": "만들고 있을거라 믿어요",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029143.1.10",
          "form": "젤다도 재밌어요",
          "original_form": "젤다도 재밌어요",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029143.1.11",
          "form": "젤다 야숨 해봤는데 저한텐 넘 어렵더라구요ㅠㅠ",
          "original_form": "젤다 야숨 해봤는데 저한텐 넘 어렵더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029143.1.12",
          "form": "그쵸 저도 퍼즐빡쳐서",
          "original_form": "그쵸 저도 퍼즐빡쳐서",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029143.1.13",
          "form": "인터넷보고 푼...",
          "original_form": "인터넷보고 푼...",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029143.1.14",
          "form": "근데 돌아댕기는게 잼더라구요",
          "original_form": "근데 돌아댕기는게 잼더라구요",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029143.1.15",
          "form": "맞아요 근데 몬스터랑 안싸우려고 피해다니다가 이상한길로 가서 죽어버리고..ㅋㅋㅋ 몬스터 넘 무서워요ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 근데 몬스터랑 안싸우려고 피해다니다가 이상한길로 가서 죽어버리고..ㅋㅋㅋ 몬스터 넘 무서워요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029143.1.16",
          "form": "맞아요 점점쌔지더라구요 ㅋㅋㅋ 무기도 자꾸 부숴져요",
          "original_form": "맞아요 점점쌔지더라구요 ㅋㅋㅋ 무기도 자꾸 부숴져요",
          "speaker_id": "2",
          "time": "20210903 08:00"
        },
        {
          "id": "MDRW2100029143.1.17",
          "form": "맞아요 무기 내구도 진짜 넘 약해요ㅠㅠㅋㅋ",
          "original_form": "맞아요 무기 내구도 진짜 넘 약해요ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:09"
        },
        {
          "id": "MDRW2100029143.1.18",
          "form": "저는 렙업하는 시스템이 좋은데 그건 아니더라구요 ㅠㅠ",
          "original_form": "저는 렙업하는 시스템이 좋은데 그건 아니더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 08:12"
        },
        {
          "id": "MDRW2100029143.1.19",
          "form": "맞아요 하트가 늘어가긴하는데 그거랑 렙업은 또 다른..ㅋㅋㅋ",
          "original_form": "맞아요 하트가 늘어가긴하는데 그거랑 렙업은 또 다른..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:16"
        },
        {
          "id": "MDRW2100029143.1.20",
          "form": "2탄도 나온다더라구요",
          "original_form": "2탄도 나온다더라구요",
          "speaker_id": "2",
          "time": "20210903 08:18"
        },
        {
          "id": "MDRW2100029143.1.21",
          "form": "야숨이요? 오오..",
          "original_form": "야숨이요? 오오..",
          "speaker_id": "1",
          "time": "20210903 08:19"
        },
        {
          "id": "MDRW2100029143.1.22",
          "form": "스카이소드는 해보셨어요?",
          "original_form": "스카이소드는 해보셨어요?",
