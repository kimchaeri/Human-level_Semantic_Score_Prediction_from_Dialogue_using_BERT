      "utterance": [
        {
          "id": "MDRW2100007499.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007499.1.2",
          "form": "요즘 코로나 땜에 운동을 전 잘 못하는 것같은데",
          "original_form": "요즘 코로나 땜에 운동을 전 잘 못하는 것같은데",
          "speaker_id": "1",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007499.1.3",
          "form": "장칼국수님은 어떠신가요?",
          "original_form": "장칼국수님은 어떠신가요?",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007499.1.4",
          "form": "저는 보통 조깅이나 등산하는데",
          "original_form": "저는 보통 조깅이나 등산하는데",
          "speaker_id": "2",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007499.1.5",
          "form": "아무래도 전보다 못 가는 것 같긴해요",
          "original_form": "아무래도 전보다 못 가는 것 같긴해요",
          "speaker_id": "2",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007499.1.6",
          "form": "실내운동은 아니지만 마스크 쓰고 운동하는 것도 힘들고 해서..",
          "original_form": "실내운동은 아니지만 마스크 쓰고 운동하는 것도 힘들고 해서..",
          "speaker_id": "2",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007499.1.7",
          "form": "맞아요 마스크 끼고 하니까 더 숨차고 힘든 느낌이더라구요",
          "original_form": "맞아요 마스크 끼고 하니까 더 숨차고 힘든 느낌이더라구요",
          "speaker_id": "1",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007499.1.8",
          "form": "이러다 근육 0%되게 생겼어요",
          "original_form": "이러다 근육 0%되게 생겼어요",
          "speaker_id": "1",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007499.1.9",
          "form": "저두요..",
          "original_form": "저두요..",
          "speaker_id": "2",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007499.1.10",
          "form": "지방은 늘고",
          "original_form": "지방은 늘고",
          "speaker_id": "2",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007499.1.11",
          "form": "근육은 줄고",
          "original_form": "근육은 줄고",
          "speaker_id": "2",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007499.1.12",
          "form": "그래서 저는 헬스장 PT를 다닐까..생각 중이에요",
          "original_form": "그래서 저는 헬스장 PT를 다닐까..생각 중이에요",
          "speaker_id": "1",
          "time": "20210822 02:42"
        },
        {
          "id": "MDRW2100007499.1.13",
          "form": "피티 받아보고 싶은데 너무 힘들까봐 쫄려요..ㅋㅋ",
          "original_form": "피티 받아보고 싶은데 너무 힘들까봐 쫄려요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007499.1.14",
          "form": "맞아요ㅋㅋㅋ 먹을 때마다 PT 선생님께 사진 보내는 것도...두렵지만....",
          "original_form": "맞아요ㅋㅋㅋ 먹을 때마다 PT 선생님께 사진 보내는 것도...두렵지만....",
          "speaker_id": "1",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007499.1.15",
          "form": "안하면 미래에 골골댈 것같아요",
          "original_form": "안하면 미래에 골골댈 것같아요",
          "speaker_id": "1",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007499.1.16",
          "form": "맞아요 미래를 위해서 근육을 채워야죠",
          "original_form": "맞아요 미래를 위해서 근육을 채워야죠",
          "speaker_id": "2",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007499.1.17",
          "form": "백세시대 무병장수하려면 힘들어도 운동 꼭 해야죠",
          "original_form": "백세시대 무병장수하려면 힘들어도 운동 꼭 해야죠",
          "speaker_id": "2",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007499.1.18",
          "form": "미래에도 살아야하니까요....",
          "original_form": "미래에도 살아야하니까요....",
          "speaker_id": "1",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007499.1.19",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007499.1.20",
          "form": "다이어트는 필요없어요 건강이 중요한 것같아요",
          "original_form": "다이어트는 필요없어요 건강이 중요한 것같아요",
          "speaker_id": "1",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007499.1.21",
          "form": "건강 잡으면 저절로 다이어트가 되지 않을까 생각",
          "original_form": "건강 잡으면 저절로 다이어트가 되지 않을까 생각",
          "speaker_id": "1",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007499.1.22",
          "form": "저는 먹는 걸 워낙 좋아해서",
          "original_form": "저는 먹는 걸 워낙 좋아해서",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007499.1.23",
          "form": "살빼는 건 생각도 못하고 그냥 건강한 돼지가 되기위해 운동합니다",
          "original_form": "살빼는 건 생각도 못하고 그냥 건강한 돼지가 되기위해 운동합니다",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007499.1.24",
          "form": "아주 좋은 마인드인 것같습니다",
          "original_form": "아주 좋은 마인드인 것같습니다",
          "speaker_id": "1",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007499.1.25",
          "form": "전 마라탕 못잃거든요",
          "original_form": "전 마라탕 못잃거든요",
          "speaker_id": "1",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007499.1.26",
          "form": "마라탕 좋죠",
          "original_form": "마라탕 좋죠",
          "speaker_id": "2",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007499.1.27",
          "form": "건두부 청경채 꼭 넣기",
          "original_form": "건두부 청경채 꼭 넣기",
          "speaker_id": "2",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007499.1.28",
          "form": "필수죠 그리고 전 고구마떡이랑 분모자 안넣으면 안돼요",
          "original_form": "필수죠 그리고 전 고구마떡이랑 분모자 안넣으면 안돼요",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007499.1.29",
          "form": "많이 먹으면 운동해야하나 죄책감 느끼긴한데...ㅋㅋㅋㅋ",
          "original_form": "많이 먹으면 운동해야하나 죄책감 느끼긴한데...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:52"
        },
        {
          "id": "MDRW2100007499.1.30",
          "form": "운동하고 나면 했으니까 또 먹어야죠",
          "original_form": "운동하고 나면 했으니까 또 먹어야죠",
          "speaker_id": "2",
          "time": "20210822 02:52"
        },
        {
          "id": "MDRW2100007499.1.31",
          "form": "오 천재신 것같아요",
          "original_form": "오 천재신 것같아요",
          "speaker_id": "1",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007499.1.32",
          "form": "마라탕 먹을땐 배를 감추는 옷을 입고 가야",
          "original_form": "마라탕 먹을땐 배를 감추는 옷을 입고 가야",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007499.1.33",
          "form": "워낙 먹는 걸 좋아하긴 하는데 소화기관이 썩 건강하지는 못해서",
          "original_form": "워낙 먹는 걸 좋아하긴 하는데 소화기관이 썩 건강하지는 못해서",
          "speaker_id": "2",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007499.1.34",
          "form": "편하게 먹을 수 있어요",
          "original_form": "편하게 먹을 수 있어요",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007499.1.35",
          "form": "나이먹을수록 자제중이에요",
          "original_form": "나이먹을수록 자제중이에요",
          "speaker_id": "2",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007499.1.36",
          "form": "아ㅏ아 저희 집 혈육도 마찬가진데",
          "original_form": "아ㅏ아 저희 집 혈육도 마찬가진데",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007499.1.37",
          "form": "그래서 잘먹으려고 운동합니다ㅋㅋ",
          "original_form": "그래서 잘먹으려고 운동합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007499.1.38",
          "form": "저도...밥 두그릇 먹기 위해 운동해야겠어요",
          "original_form": "저도...밥 두그릇 먹기 위해 운동해야겠어요",
          "speaker_id": "1",
          "time": "20210822 02:55"
        }
      ]
