      "utterance": [
        {
          "id": "MDRW2100021222.1.1",
          "form": "쇼핑은 별로요 바빠서",
          "original_form": "쇼핑은 별로요 바빠서",
          "speaker_id": "2",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100021222.1.2",
          "form": "중고거래도 잘 안하시나요",
          "original_form": "중고거래도 잘 안하시나요",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021222.1.3",
          "form": "그건 더 부지런해야 가능해서 ㅜ",
          "original_form": "그건 더 부지런해야 가능해서 ㅜ",
          "speaker_id": "2",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021222.1.4",
          "form": "마자요ㅜㅠㅜ",
          "original_form": "마자요ㅜㅠㅜ",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021222.1.5",
          "form": "자주 하세요?",
          "original_form": "자주 하세요?",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021222.1.6",
          "form": "네 자주하는편이에요",
          "original_form": "네 자주하는편이에요",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021222.1.7",
          "form": "부지런하시네요 주로 어떤 아이템?",
          "original_form": "부지런하시네요 주로 어떤 아이템?",
          "speaker_id": "2",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100021222.1.8",
          "form": "옷 신발 종류요ㅎㅎ",
          "original_form": "옷 신발 종류요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021222.1.9",
          "form": "와 성공적인 가요?",
          "original_form": "와 성공적인 가요?",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021222.1.10",
          "form": "일년째 안팔리는것도 있네요ㅜ",
          "original_form": "일년째 안팔리는것도 있네요ㅜ",
          "speaker_id": "1",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100021222.1.11",
          "form": "가격이 높을까요",
          "original_form": "가격이 높을까요",
          "speaker_id": "2",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100021222.1.12",
          "form": "가격은 어떻게 정하세요?",
          "original_form": "가격은 어떻게 정하세요?",
          "speaker_id": "2",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021222.1.13",
          "form": "안높은데도 잘안팔리네여ㅜ",
          "original_form": "안높은데도 잘안팔리네여ㅜ",
          "speaker_id": "1",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021222.1.14",
          "form": "더 낮춰 다시 올리셔야겠네요 ㅠ",
          "original_form": "더 낮춰 다시 올리셔야겠네요 ㅠ",
          "speaker_id": "2",
