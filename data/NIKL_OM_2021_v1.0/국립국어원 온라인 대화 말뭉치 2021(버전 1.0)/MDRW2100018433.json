      "utterance": [
        {
          "id": "MDRW2100018433.1.1",
          "form": "군대 다녀오셨나요",
          "original_form": "군대 다녀오셨나요",
          "speaker_id": "1",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018433.1.2",
          "form": "아뇨 여자라 ㅋㅋ 나라에서 안 부르더라구요",
          "original_form": "아뇨 여자라 ㅋㅋ 나라에서 안 부르더라구요",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018433.1.3",
          "form": "군대 다녀오셨나요",
          "original_form": "군대 다녀오셨나요",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018433.1.4",
          "form": "ㅋㅋ안불러도 갈 수 있자나요",
          "original_form": "ㅋㅋ안불러도 갈 수 있자나요",
          "speaker_id": "1",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018433.1.5",
          "form": "네 덕분에 얀센 맞았습니다ㅋㄱㅋ",
          "original_form": "네 덕분에 얀센 맞았습니다ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018433.1.6",
          "form": "그러셨군요! 얀센 괜찮으셨나요?",
          "original_form": "그러셨군요! 얀센 괜찮으셨나요?",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018433.1.7",
          "form": "네ㅋㅋ근데 괜히 맞아서",
          "original_form": "네ㅋㅋ근데 괜히 맞아서",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018433.1.8",
          "form": "1달 지나니까 갑자기 화이자 접종대상",
          "original_form": "1달 지나니까 갑자기 화이자 접종대상",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018433.1.9",
          "form": "음? 왜 또 화이자 접종대상이 되셨나요????",
          "original_form": "음? 왜 또 화이자 접종대상이 되셨나요????",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018433.1.10",
          "form": "ㅋㅋ회사가 대중교통이라서",
          "original_form": "ㅋㅋ회사가 대중교통이라서",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018433.1.11",
          "form": "아 근데 일단 군대부터 ㅋㅋㅋ",
          "original_form": "아 근데 일단 군대부터 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018433.1.12",
          "form": "무튼 군대 재밌긴했어요",
          "original_form": "무튼 군대 재밌긴했어요",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018433.1.13",
          "form": "다시 가라면 못갈거 같지만",
          "original_form": "다시 가라면 못갈거 같지만",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018433.1.14",
          "form": "군대 고생 많으셨겠어요 라고 하면 10중 8할은 재밌었다고 하셔서 의외에요...ㅋㅋ",
          "original_form": "군대 고생 많으셨겠어요 라고 하면 10중 8할은 재밌었다고 하셔서 의외에요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018433.1.15",
          "form": "뭐 거기서 배운것도 많고ㅎㅎ",
          "original_form": "뭐 거기서 배운것도 많고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018433.1.16",
          "form": "나름 알차게 보내서요 ㅎㅎ",
          "original_form": "나름 알차게 보내서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018433.1.17",
          "form": "그러셨군요 . 동기분들하고 아직도 교류하고 지내시나요?",
          "original_form": "그러셨군요 . 동기분들하고 아직도 교류하고 지내시나요?",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018433.1.18",
          "form": "네 한명 한달에 한번은 봅니다 ㅎㅎ",
          "original_form": "네 한명 한달에 한번은 봅니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018433.1.19",
          "form": "좋은 친구 사귀셨나봐요 그것도 인연이죠!",
          "original_form": "좋은 친구 사귀셨나봐요 그것도 인연이죠!",
