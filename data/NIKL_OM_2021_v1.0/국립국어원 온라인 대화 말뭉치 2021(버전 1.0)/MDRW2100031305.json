      "utterance": [
        {
          "id": "MDRW2100031305.1.1",
          "form": "오 저는 집에서 하는걸 좋아해용",
          "original_form": "오 저는 집에서 하는걸 좋아해용",
          "speaker_id": "2",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100031305.1.2",
          "form": "뭔가 편하게 게임하거나 영화보면서",
          "original_form": "뭔가 편하게 게임하거나 영화보면서",
          "speaker_id": "2",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100031305.1.3",
          "form": "그런게 좋네요",
          "original_form": "그런게 좋네요",
          "speaker_id": "2",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100031305.1.4",
          "form": "저도 작년에 데이트할때 그랬는데",
          "original_form": "저도 작년에 데이트할때 그랬는데",
          "speaker_id": "1",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031305.1.5",
          "form": "코로나 때문에 그냥 집에서 같이 음식만들어 먹고",
          "original_form": "코로나 때문에 그냥 집에서 같이 음식만들어 먹고",
          "speaker_id": "1",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031305.1.6",
          "form": "넷플릭스 보고",
          "original_form": "넷플릭스 보고",
          "speaker_id": "1",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031305.1.7",
          "form": "왓챠보고",
          "original_form": "왓챠보고",
          "speaker_id": "1",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031305.1.8",
          "form": "ㅋㅋㅋ좋죠",
          "original_form": "ㅋㅋㅋ좋죠",
          "speaker_id": "2",
          "time": "20210904 20:35"
        },
        {
          "id": "MDRW2100031305.1.9",
          "form": "뭔가그런게최고인거같네요",
          "original_form": "뭔가그런게최고인거같네요",
          "speaker_id": "2",
          "time": "20210904 20:35"
        },
        {
          "id": "MDRW2100031305.1.10",
          "form": "뭔가 편하고",
          "original_form": "뭔가 편하고",
          "speaker_id": "1",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100031305.1.11",
          "form": "가성비도 좋고ㅎㅎ",
          "original_form": "가성비도 좋고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100031305.1.12",
          "form": "와이프가 카페를 많이 다녀서",
          "original_form": "와이프가 카페를 많이 다녀서",
          "speaker_id": "1",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100031305.1.13",
          "form": "밖에 나간다하면 주로",
          "original_form": "밖에 나간다하면 주로",
          "speaker_id": "1",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100031305.1.14",
          "form": "커피마시러 돌아다니는 데이트였거든요",
          "original_form": "커피마시러 돌아다니는 데이트였거든요",
          "speaker_id": "1",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100031305.1.15",
          "form": "요즘은 커피맛도 거의 평준화되고",
          "original_form": "요즘은 커피맛도 거의 평준화되고",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031305.1.16",
          "form": "카페 인테리어도 평준화되서",
          "original_form": "카페 인테리어도 평준화되서",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031305.1.17",
          "form": "다 비슷비슷한 느낌이라 별로에요",
          "original_form": "다 비슷비슷한 느낌이라 별로에요",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031305.1.18",
          "form": "옹 저도 카페 많이다니는데 항상 가던곳만가는기분...",
          "original_form": "옹 저도 카페 많이다니는데 항상 가던곳만가는기분...",
          "speaker_id": "2",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031305.1.19",
          "form": "저는 오늘 경주 황리단길가서 와이프랑 데이트하고 왔거든요",
          "original_form": "저는 오늘 경주 황리단길가서 와이프랑 데이트하고 왔거든요",
          "speaker_id": "1",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100031305.1.20",
          "form": "오스테리아밀즈라는 레스토랑 가서 리조토랑 파스타랑 먹었는데",
          "original_form": "오스테리아밀즈라는 레스토랑 가서 리조토랑 파스타랑 먹었는데",
          "speaker_id": "1",
          "time": "20210905 00:22"
        },
        {
          "id": "MDRW2100031305.1.21",
          "form": "너무 맛있더라고요",
          "original_form": "너무 맛있더라고요",
          "speaker_id": "1",
          "time": "20210905 00:22"
        },
        {
          "id": "MDRW2100031305.1.22",
          "form": "name2님 사는곳 주변에는 이런 데이트장소 어디있나요?",
          "original_form": "&name2&님 사는곳 주변에는 이런 데이트장소 어디있나요?",
          "speaker_id": "1",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100031305.1.23",
          "form": "대박 경주 황리단길!",
          "original_form": "대박 경주 황리단길!",
          "speaker_id": "2",
          "time": "20210905 02:10"
        },
        {
          "id": "MDRW2100031305.1.24",
          "form": "경주가 요즘 좋다면서요ㅠㅠ",
          "original_form": "경주가 요즘 좋다면서요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 02:11"
        },
        {
          "id": "MDRW2100031305.1.25",
          "form": ".",
          "original_form": ".",
          "speaker_id": "2",
          "time": "20210905 02:11"
        },
        {
          "id": "MDRW2100031305.1.26",
          "form": "인사동이라거나...서울숲이라거나...",
          "original_form": "인사동이라거나...서울숲이라거나...",
          "speaker_id": "2",
          "time": "20210905 02:11"
        },
        {
          "id": "MDRW2100031305.1.27",
          "form": "최근에서야갔는데 너무좋더라구요",
          "original_form": "최근에서야갔는데 너무좋더라구요",
          "speaker_id": "2",
          "time": "20210905 02:11"
        },
        {
          "id": "MDRW2100031305.1.28",
          "form": "요즘날씨도좋아서 어딜가든좋을거같긴하지만!",
          "original_form": "요즘날씨도좋아서 어딜가든좋을거같긴하지만!",
          "speaker_id": "2",
          "time": "20210905 02:11"
        },
        {
          "id": "MDRW2100031305.1.29",
          "form": "서울 가고 싶은데 너무 멀어서 쉽지가 않네요",
          "original_form": "서울 가고 싶은데 너무 멀어서 쉽지가 않네요",
          "speaker_id": "1",
          "time": "20210905 07:33"
        },
        {
          "id": "MDRW2100031305.1.30",
          "form": "좋으시겠다 생활환경 너무 좋아서",
          "original_form": "좋으시겠다 생활환경 너무 좋아서",
          "speaker_id": "1",
          "time": "20210905 07:33"
        },
        {
          "id": "MDRW2100031305.1.31",
          "form": "경기도가 좋죵 근데 굳이 서울아니여도 다른곳에도 대도시는많으니깐용",
          "original_form": "경기도가 좋죵 근데 굳이 서울아니여도 다른곳에도 대도시는많으니깐용",
          "speaker_id": "2",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100031305.1.32",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "1",
          "time": "20210905 14:55"
        },
        {
          "id": "MDRW2100031305.1.33",
          "form": "그런데 지방살면 문화생활 차이는 확실히 나거든요",
          "original_form": "그런데 지방살면 문화생활 차이는 확실히 나거든요",
          "speaker_id": "1",
          "time": "20210905 14:55"
        },
        {
          "id": "MDRW2100031305.1.34",
          "form": "공연도 훨씬적고 그래서ㅎㅎ",
          "original_form": "공연도 훨씬적고 그래서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:55"
        },
        {
          "id": "MDRW2100031305.1.35",
          "form": "ㅎㅎ그쵸그쵸",
          "original_form": "ㅎㅎ그쵸그쵸",
          "speaker_id": "2",
          "time": "20210905 14:56"
        },
        {
          "id": "MDRW2100031305.1.36",
          "form": "오오 공연좋아하시나봐요",
          "original_form": "오오 공연좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210905 14:56"
        },
        {
          "id": "MDRW2100031305.1.37",
          "form": "좋아하는데 많이 볼기회가 적어서 항상 아숴워요ㅎㅎ",
          "original_form": "좋아하는데 많이 볼기회가 적어서 항상 아숴워요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:57"
        },
        {
          "id": "MDRW2100031305.1.38",
          "form": "아쉽네요ㅠㅠ",
          "original_form": "아쉽네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 14:58"
        }
      ]
    }
