        },
        {
          "id": "MDRW2100047278.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 02:53"
        },
        {
          "id": "MDRW2100047278.1.3",
          "form": "지금 아르바이트나 직장생활 하고계신가요??",
          "original_form": "지금 아르바이트나 직장생활 하고계신가요??",
          "speaker_id": "1",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100047278.1.4",
          "form": "직장인이요^^",
          "original_form": "직장인이요^^",
          "speaker_id": "2",
          "time": "20210826 02:57"
        },
        {
          "id": "MDRW2100047278.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 02:57"
        },
        {
          "id": "MDRW2100047278.1.6",
          "form": "오 그러시구나~",
          "original_form": "오 그러시구나~",
          "speaker_id": "1",
          "time": "20210826 02:58"
        },
        {
          "id": "MDRW2100047278.1.7",
          "form": "전 알바해요 ㅎㅎ",
          "original_form": "전 알바해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 02:58"
        },
        {
          "id": "MDRW2100047278.1.8",
          "form": "아 구직중이신거여요?",
          "original_form": "아 구직중이신거여요?",
          "speaker_id": "2",
          "time": "20210826 02:58"
        },
        {
          "id": "MDRW2100047278.1.9",
          "form": "요즘 코로나땜에  알바구하기도 힘들다던데",
          "original_form": "요즘 코로나땜에  알바구하기도 힘들다던데",
          "speaker_id": "2",
          "time": "20210826 02:59"
        },
        {
          "id": "MDRW2100047278.1.10",
          "form": "아뇨 아직 대학생이라 구직까진아니구 그냥 소소하게 주말에 알바만하고있어요 ㅎㅎ",
          "original_form": "아뇨 아직 대학생이라 구직까진아니구 그냥 소소하게 주말에 알바만하고있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 03:00"
        },
        {
          "id": "MDRW2100047278.1.11",
          "form": "맞아요 ㅜ알바자리가없다죠",
          "original_form": "맞아요 ㅜ알바자리가없다죠",
          "speaker_id": "1",
          "time": "20210826 03:00"
        },
        {
          "id": "MDRW2100047278.1.12",
          "form": "아 대학생이시군요",
          "original_form": "아 대학생이시군요",
          "speaker_id": "2",
          "time": "20210826 03:00"
        },
        {
          "id": "MDRW2100047278.1.13",
          "form": "곧 개강 아니예요?",
          "original_form": "곧 개강 아니예요?",
          "speaker_id": "2",
          "time": "20210826 03:00"
        },
        {
          "id": "MDRW2100047278.1.14",
          "form": "맞아요! 곧개강이에요 ㅠ",
          "original_form": "맞아요! 곧개강이에요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 03:04"
        },
        {
          "id": "MDRW2100047278.1.15",
          "form": "좋은건지 우울한건지 몰겠지만ㅎㅎ",
          "original_form": "좋은건지 우울한건지 몰겠지만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 03:05"
        },
        {
          "id": "MDRW2100047278.1.16",
          "form": "알바는 무슨알바하시나요",
          "original_form": "알바는 무슨알바하시나요",
          "speaker_id": "2",
          "time": "20210826 03:05"
        },
        {
          "id": "MDRW2100047278.1.17",
          "form": "저는 주말에 서포터즈알바 하고있어요~",
          "original_form": "저는 주말에 서포터즈알바 하고있어요~",
          "speaker_id": "1",
          "time": "20210826 03:07"
        },
        {
          "id": "MDRW2100047278.1.18",
          "form": "온라인강의하시는 강사님들 도와주는역할이에요 ㅎㅎ",
          "original_form": "온라인강의하시는 강사님들 도와주는역할이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 03:07"
        },
        {
          "id": "MDRW2100047278.1.19",
          "form": "오ㅎ 할만 하겠어요",
          "original_form": "오ㅎ 할만 하겠어요",
          "speaker_id": "2",
          "time": "20210826 03:08"
        },
        {
          "id": "MDRW2100047278.1.20",
          "form": "알바는 음식점이 젤힘든듯요",
          "original_form": "알바는 음식점이 젤힘든듯요",
          "speaker_id": "2",
          "time": "20210826 03:08"
        },
        {
          "id": "MDRW2100047278.1.21",
          "form": "맞아요! 되게 수월한편이에요 ㅎㅎ",
          "original_form": "맞아요! 되게 수월한편이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 03:09"
        },
        {
          "id": "MDRW2100047278.1.22",
          "form": "음식점알바 안해봐서 궁금하긴해요",
          "original_form": "음식점알바 안해봐서 궁금하긴해요",
          "speaker_id": "1",
          "time": "20210826 03:09"
        },
        {
          "id": "MDRW2100047278.1.23",
          "form": "저도 딱한번해봐서 ㅋ 찜닭알바",
          "original_form": "저도 딱한번해봐서 ㅋ 찜닭알바",
          "speaker_id": "2",
          "time": "20210826 03:09"
        },
        {
          "id": "MDRW2100047278.1.24",
          "form": "오홍! 찜닭알바하면 끼니때 찜닭 공짜로 먹을수있는건가요?",
          "original_form": "오홍! 찜닭알바하면 끼니때 찜닭 공짜로 먹을수있는건가요?",
          "speaker_id": "1",
          "time": "20210826 03:10"
        },
        {
          "id": "MDRW2100047278.1.25",
          "form": "음식점서빙이 계속서있어야되고",
          "original_form": "음식점서빙이 계속서있어야되고",
          "speaker_id": "2",
          "time": "20210826 03:10"
        },
        {
          "id": "MDRW2100047278.1.26",
          "form": "설거지 산더미에ㅎ",
          "original_form": "설거지 산더미에ㅎ",
          "speaker_id": "2",
          "time": "20210826 03:10"
        },
        {
          "id": "MDRW2100047278.1.27",
          "form": "사람도 상대해야되니 힘들어요",
          "original_form": "사람도 상대해야되니 힘들어요",
          "speaker_id": "2",
          "time": "20210826 03:11"
        }
      ]
    }
