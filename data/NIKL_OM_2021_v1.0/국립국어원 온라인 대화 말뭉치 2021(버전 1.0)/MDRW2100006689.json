        {
          "id": "MDRW2100006689.1.1",
          "form": "종교가 있으신가요",
          "original_form": "종교가 있으신가요",
          "speaker_id": "1",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100006689.1.2",
          "form": "아니요~ 종교있으세요?",
          "original_form": "아니요~ 종교있으세요?",
          "speaker_id": "2",
          "time": "20210908 06:39"
        },
        {
          "id": "MDRW2100006689.1.3",
          "form": "저는 기독교요~",
          "original_form": "저는 기독교요~",
          "speaker_id": "1",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100006689.1.4",
          "form": "기독교시군요",
          "original_form": "기독교시군요",
          "speaker_id": "2",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100006689.1.5",
          "form": "제 주변에도 기독교 친구들이 많아요",
          "original_form": "제 주변에도 기독교 친구들이 많아요",
          "speaker_id": "2",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100006689.1.6",
          "form": "아 가치관이 다르지 않나요?",
          "original_form": "아 가치관이 다르지 않나요?",
          "speaker_id": "1",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100006689.1.7",
          "form": "음 종교는 다르지만",
          "original_form": "음 종교는 다르지만",
          "speaker_id": "2",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100006689.1.8",
          "form": "다른부분들에선 잘 맞는 친구들이 많아요 ㅎㅎ",
          "original_form": "다른부분들에선 잘 맞는 친구들이 많아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100006689.1.9",
          "form": "ㅎㅎ 맞아요 진짜친하면 종교상관 없지요",
          "original_form": "ㅎㅎ 맞아요 진짜친하면 종교상관 없지요",
          "speaker_id": "1",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100006689.1.10",
          "form": "목사님 자제분인 친구들도 꽤 있어요 ㅋㅋㅋ",
          "original_form": "목사님 자제분인 친구들도 꽤 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100006689.1.11",
          "form": "ㅋㅋㅋ 오 드문경우네요",
          "original_form": "ㅋㅋㅋ 오 드문경우네요",
          "speaker_id": "1",
          "time": "20210908 07:48"
        },
        {
          "id": "MDRW2100006689.1.12",
          "form": "모태신앙이신가요?",
          "original_form": "모태신앙이신가요?",
          "speaker_id": "2",
          "time": "20210908 07:48"
        },
        {
          "id": "MDRW2100006689.1.13",
          "form": "아니요ㅎㅎ",
          "original_form": "아니요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100006689.1.14",
          "form": "앗 그럼 어떻게 기독교가 되신거에요?",
          "original_form": "앗 그럼 어떻게 기독교가 되신거에요?",
          "speaker_id": "2",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100006689.1.15",
          "form": "ㅎㅎ 결혼하면서 종교가 생겼어요",
          "original_form": "ㅎㅎ 결혼하면서 종교가 생겼어요",
          "speaker_id": "1",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100006689.1.16",
          "form": "앗! 배우자분이 기독교셨군ㅇㅎ",
          "original_form": "앗! 배우자분이 기독교셨군ㅇㅎ",
          "speaker_id": "2",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100006689.1.17",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100006689.1.18",
          "form": "네 맞아요ㅎㅎ",
          "original_form": "네 맞아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100006689.1.19",
          "form": "전 제일 가까운 사람이 기독교였네요ㅋ",
          "original_form": "전 제일 가까운 사람이 기독교였네요ㅋ",
          "speaker_id": "1",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100006689.1.20",
          "form": "ㅋㅋㅋㅋㅋ저희 둘다 예상치못하게 기독교랑 연이 깊었네요!",
