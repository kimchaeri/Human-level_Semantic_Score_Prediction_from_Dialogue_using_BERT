      "utterance": [
        {
          "id": "MDRW2100022877.1.1",
          "form": "안녕하세요! name2님은 드라마 좋아하시나요?",
          "original_form": "안녕하세요! &name2&님은 드라마 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022877.1.2",
          "form": "네 좋아하는건 계속봐요ㅎㅎ",
          "original_form": "네 좋아하는건 계속봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022877.1.3",
          "form": "엇 어떤 드라마 좋아하세요!",
          "original_form": "엇 어떤 드라마 좋아하세요!",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022877.1.4",
          "form": "요즘은 펜트하우스요",
          "original_form": "요즘은 펜트하우스요",
          "speaker_id": "2",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022877.1.5",
          "form": "끝나가니 아쉬워요ㅠㅠ",
          "original_form": "끝나가니 아쉬워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022877.1.6",
          "form": "맞아요...!!!!! 너무 충격적으로 끝나가서 매번 놀라는 중이에요",
          "original_form": "맞아요...!!!!! 너무 충격적으로 끝나가서 매번 놀라는 중이에요",
          "speaker_id": "1",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022877.1.7",
          "form": "이번주 거 보셨어요*!",
          "original_form": "이번주 거 보셨어요*!",
          "speaker_id": "1",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022877.1.8",
          "form": "?!",
          "original_form": "?!",
          "speaker_id": "1",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022877.1.9",
          "form": "네네 저는 무조건 본방봅니다!!",
          "original_form": "네네 저는 무조건 본방봅니다!!",
          "speaker_id": "2",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022877.1.10",
          "form": "진짜 충격이었어요ㅠㅠ어떻게 마무리할지",
          "original_form": "진짜 충격이었어요ㅠㅠ어떻게 마무리할지",
          "speaker_id": "2",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022877.1.11",
          "form": "앗 그니까요!! 조비서 죽을 때 저",
          "original_form": "앗 그니까요!! 조비서 죽을 때 저",
          "speaker_id": "1",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022877.1.12",
          "form": "비명 질렀어요",
          "original_form": "비명 질렀어요",
          "speaker_id": "1",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022877.1.13",
          "form": "그거부터 낚시라는 뇌피셜들 있어서 한주 기다리기 넘 힘들어요ㅠㅠ",
          "original_form": "그거부터 낚시라는 뇌피셜들 있어서 한주 기다리기 넘 힘들어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022877.1.14",
          "form": "조비서 맞기만하다가 가다니",
          "original_form": "조비서 맞기만하다가 가다니",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022877.1.15",
          "form": "헐 낚시러니 그럼 진짜 엄청 혼란스러울 것 같아요 ^.ㅠ",
          "original_form": "헐 낚시러니 그럼 진짜 엄청 혼란스러울 것 같아요 ^.ㅠ",
          "speaker_id": "1",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022877.1.16",
          "form": "마지막도 엄청 잔인허게 떨어지는 거 보고...",
          "original_form": "마지막도 엄청 잔인허게 떨어지는 거 보고...",
          "speaker_id": "1",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022877.1.17",
          "form": "좀 많이 불쌍했어요",
          "original_form": "좀 많이 불쌍했어요",
          "speaker_id": "1",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022877.1.18",
          "form": "네 폭탄도 안터졌다는말도있고 천서진도 치매연기라그러고",
          "original_form": "네 폭탄도 안터졌다는말도있고 천서진도 치매연기라그러고",
          "speaker_id": "2",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022877.1.19",
          "form": "말이 너무 많아서 헷갈려요 펜하는ㅠㅋㅋ",
          "original_form": "말이 너무 많아서 헷갈려요 펜하는ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022877.1.20",
          "form": "아 치매연기면 진짜... 진짜.... 이젠 반전의 반전을 어떻게 반응해여할지도 모르겠다니까요....",
          "original_form": "아 치매연기면 진짜... 진짜.... 이젠 반전의 반전을 어떻게 반응해여할지도 모르겠다니까요....",
          "speaker_id": "1",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022877.1.21",
          "form": "하긴... 저번주까지만 해도 천서진 죽은 줄 알았거든요..",
          "original_form": "하긴... 저번주까지만 해도 천서진 죽은 줄 알았거든요..",
          "speaker_id": "1",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022877.1.22",
          "form": "그러게요 계단에서 구른 하윤철보다 빨리 회복할줄이야..",
          "original_form": "그러게요 계단에서 구른 하윤철보다 빨리 회복할줄이야..",
          "speaker_id": "2",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022877.1.23",
          "form": "아 그르니까요 -.- 그렇게 잔인하게 쓰러졌는데...",
          "original_form": "아 그르니까요 -.- 그렇게 잔인하게 쓰러졌는데...",
          "speaker_id": "1",
          "time": "20210829 19:09"
        }
      ]
    }
