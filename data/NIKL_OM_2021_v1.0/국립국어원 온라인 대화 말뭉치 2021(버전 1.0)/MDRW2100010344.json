        },
        {
          "id": "MDRW2100010344.1.2",
          "form": "좋죠ㅋㅋㅋ",
          "original_form": "좋죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:14"
        },
        {
          "id": "MDRW2100010344.1.3",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210823 17:14"
        },
        {
          "id": "MDRW2100010344.1.4",
          "form": "네 좋아하는데 못가서 요즘 너무 아쉽네요",
          "original_form": "네 좋아하는데 못가서 요즘 너무 아쉽네요",
          "speaker_id": "1",
          "time": "20210823 17:15"
        },
        {
          "id": "MDRW2100010344.1.5",
          "form": "name2님이 제일 가보고싶은 여행지는 어디인가요?",
          "original_form": "&name2&님이 제일 가보고싶은 여행지는 어디인가요?",
          "speaker_id": "1",
          "time": "20210823 17:15"
        },
        {
          "id": "MDRW2100010344.1.6",
          "form": "저 싱가폴이요ㅋㅋㅋ",
          "original_form": "저 싱가폴이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:25"
        },
        {
          "id": "MDRW2100010344.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210823 17:25"
        },
        {
          "id": "MDRW2100010344.1.8",
          "form": "저는 스페인이요 !",
          "original_form": "저는 스페인이요 !",
          "speaker_id": "1",
          "time": "20210823 17:31"
        },
        {
          "id": "MDRW2100010344.1.9",
          "form": "싱가폴 좋다는 얘기를 많이 들어서 저도 싱가폴도 가보고싶네요",
          "original_form": "싱가폴 좋다는 얘기를 많이 들어서 저도 싱가폴도 가보고싶네요",
          "speaker_id": "1",
          "time": "20210823 17:31"
        },
        {
          "id": "MDRW2100010344.1.10",
          "form": "스페인좋죠ㅋㅋㅋㅋ",
          "original_form": "스페인좋죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010344.1.11",
          "form": "저는 싱가폴 그 유명호텔가고싶어요",
          "original_form": "저는 싱가폴 그 유명호텔가고싶어요",
          "speaker_id": "2",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010344.1.12",
          "form": "그게 아이유가 사진찍은 호텔 말하는 건가요? 그 약간 핫도그 같은..!",
          "original_form": "그게 아이유가 사진찍은 호텔 말하는 건가요? 그 약간 핫도그 같은..!",
          "speaker_id": "1",
          "time": "20210823 18:05"
        },
        {
          "id": "MDRW2100010344.1.13",
          "form": "ㅋㅋㅋ 아이유는모르겠고",
          "original_form": "ㅋㅋㅋ 아이유는모르겠고",
          "speaker_id": "2",
          "time": "20210824 10:41"
        },
        {
          "id": "MDRW2100010344.1.14",
          "form": "핫도그는 맞는거같네요ㅋㅋㅋㅋ",
          "original_form": "핫도그는 맞는거같네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:41"
        },
        {
          "id": "MDRW2100010344.1.15",
          "form": "ㅋㅋㅋ저도 그 건물 되게 신기하게 생겨서 꼭 한번 가보고싶어요",
          "original_form": "ㅋㅋㅋ저도 그 건물 되게 신기하게 생겨서 꼭 한번 가보고싶어요",
          "speaker_id": "1",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100010344.1.16",
          "form": "제 버킷리스트랍니다",
          "original_form": "제 버킷리스트랍니다",
          "speaker_id": "2",
          "time": "20210824 12:46"
        },
        {
          "id": "MDRW2100010344.1.17",
          "form": "코로나끝나면 꼭 한번 가보세요ㅎㅎ!",
          "original_form": "코로나끝나면 꼭 한번 가보세요ㅎㅎ!",
          "speaker_id": "1",
          "time": "20210824 12:58"
        },
        {
          "id": "MDRW2100010344.1.18",
          "form": "네네 그러려구요",
          "original_form": "네네 그러려구요",
          "speaker_id": "2",
          "time": "20210824 13:01"
        },
        {
          "id": "MDRW2100010344.1.19",
          "form": "제주도요즘",
          "original_form": "제주도요즘",
          "speaker_id": "2",
          "time": "20210824 13:01"
        },
        {
          "id": "MDRW2100010344.1.20",
          "form": "너무가고싶어요ㅠㅠ",
          "original_form": "너무가고싶어요ㅠㅠ",
