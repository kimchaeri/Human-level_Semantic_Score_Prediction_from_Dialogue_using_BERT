      "utterance": [
        {
          "id": "MDRW2100024541.1.1",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 14:37"
        },
        {
          "id": "MDRW2100024541.1.2",
          "form": "내 클래식 공연 좋아해요 ㅎㅎㅎ",
          "original_form": "내 클래식 공연 좋아해요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100024541.1.3",
          "form": "와 쉽지 않을 것 같은데",
          "original_form": "와 쉽지 않을 것 같은데",
          "speaker_id": "1",
          "time": "20210831 14:43"
        },
        {
          "id": "MDRW2100024541.1.4",
          "form": "음악전공이신가요?",
          "original_form": "음악전공이신가요?",
          "speaker_id": "1",
          "time": "20210831 14:43"
        },
        {
          "id": "MDRW2100024541.1.5",
          "form": "아뇨 전공자만 클래식 보나요",
          "original_form": "아뇨 전공자만 클래식 보나요",
          "speaker_id": "2",
          "time": "20210831 14:56"
        },
        {
          "id": "MDRW2100024541.1.6",
          "form": "하긴 그렇죠",
          "original_form": "하긴 그렇죠",
          "speaker_id": "1",
          "time": "20210831 14:57"
        },
        {
          "id": "MDRW2100024541.1.7",
          "form": "그럼 연주회같은거 보러 다니세요?",
          "original_form": "그럼 연주회같은거 보러 다니세요?",
          "speaker_id": "1",
          "time": "20210831 14:57"
        },
        {
          "id": "MDRW2100024541.1.8",
          "form": "네 요즘은 잘 안가는데 예전엔요",
          "original_form": "네 요즘은 잘 안가는데 예전엔요",
          "speaker_id": "2",
          "time": "20210831 14:59"
        },
        {
          "id": "MDRW2100024541.1.9",
          "form": "그렇구나 다른 공연들도 좋아하시나요?",
          "original_form": "그렇구나 다른 공연들도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 15:04"
        },
        {
          "id": "MDRW2100024541.1.10",
          "form": "뮤지컬 연극같은",
          "original_form": "뮤지컬 연극같은",
          "speaker_id": "1",
          "time": "20210831 15:04"
        },
        {
          "id": "MDRW2100024541.1.11",
          "form": "아뇨 주로 클래식만 봐요",
          "original_form": "아뇨 주로 클래식만 봐요",
          "speaker_id": "2",
          "time": "20210831 15:08"
        },
        {
          "id": "MDRW2100024541.1.12",
          "form": "그러면 좋아하는 뮤지션은 누가 있어요?",
          "original_form": "그러면 좋아하는 뮤지션은 누가 있어요?",
          "speaker_id": "1",
          "time": "20210831 15:13"
        },
        {
          "id": "MDRW2100024541.1.13",
          "form": "요즘엔 문지영이요",
          "original_form": "요즘엔 문지영이요",
          "speaker_id": "2",
          "time": "20210831 15:14"
        },
        {
          "id": "MDRW2100024541.1.14",
          "form": "누군지 몰라서 찾아보고 왔네요ㅎㅎ",
          "original_form": "누군지 몰라서 찾아보고 왔네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 15:14"
        },
        {
          "id": "MDRW2100024541.1.15",
          "form": "저 사는 곳에는 박규희 연주회한다고 요새 홍보하던데",
          "original_form": "저 사는 곳에는 박규희 연주회한다고 요새 홍보하던데",
          "speaker_id": "1",
          "time": "20210831 15:15"
        },
        {
          "id": "MDRW2100024541.1.16",
          "form": "클래식 기타도 들ㅇㄷ시나요?",
          "original_form": "클래식 기타도 들ㅇㄷ시나요?",
          "speaker_id": "1",
          "time": "20210831 15:15"
        },
        {
          "id": "MDRW2100024541.1.17",
          "form": "아뇨 기타는 안듣는데요",
          "original_form": "아뇨 기타는 안듣는데요",
          "speaker_id": "2",
          "time": "20210831 15:17"
        }
      ]
