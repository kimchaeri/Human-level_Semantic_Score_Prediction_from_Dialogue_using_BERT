      "utterance": [
        {
          "id": "MDRW2100003234.1.1",
          "form": "요새 운동 하시나요?",
          "original_form": "요새 운동 하시나요?",
          "speaker_id": "1",
          "time": "20210820 20:48"
        },
        {
          "id": "MDRW2100003234.1.2",
          "form": "운동은ㅠㅠ",
          "original_form": "운동은ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 20:52"
        },
        {
          "id": "MDRW2100003234.1.3",
          "form": "유튜브 가끔 틀고 따라해요ㅋ",
          "original_form": "유튜브 가끔 틀고 따라해요ㅋ",
          "speaker_id": "2",
          "time": "20210820 20:52"
        },
        {
          "id": "MDRW2100003234.1.4",
          "form": "어 저도 주말에 홈트를 한답니다 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어 저도 주말에 홈트를 한답니다 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:53"
        },
        {
          "id": "MDRW2100003234.1.5",
          "form": "누구꺼 보세요?",
          "original_form": "누구꺼 보세요?",
          "speaker_id": "1",
          "time": "20210820 20:53"
        },
        {
          "id": "MDRW2100003234.1.6",
          "form": "타바타 암거나 들어요ㅋㅋㅋ",
          "original_form": "타바타 암거나 들어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:54"
        },
        {
          "id": "MDRW2100003234.1.7",
          "form": "추천해주세요ㅋㅋ",
          "original_form": "추천해주세요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:54"
        },
        {
          "id": "MDRW2100003234.1.8",
          "form": "전 요즘 땅끄부부 보는데 힘들어 죽습니다",
          "original_form": "전 요즘 땅끄부부 보는데 힘들어 죽습니다",
          "speaker_id": "1",
          "time": "20210820 20:55"
        },
        {
          "id": "MDRW2100003234.1.9",
          "form": "허벅지가 후들후들",
          "original_form": "허벅지가 후들후들",
          "speaker_id": "1",
          "time": "20210820 20:55"
        },
        {
          "id": "MDRW2100003234.1.10",
          "form": "ㅋㅋㅋㅋ 그거 본거 같아요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 그거 본거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003234.1.11",
          "form": "갑자기 하고싶네요ㅋㅋ",
          "original_form": "갑자기 하고싶네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003234.1.12",
          "form": "전 이따가 해보려구요",
          "original_form": "전 이따가 해보려구요",
          "speaker_id": "1",
          "time": "20210820 21:04"
        },
        {
          "id": "MDRW2100003234.1.13",
          "form": "좀 전에 먹는 밥을 불태워야해요",
          "original_form": "좀 전에 먹는 밥을 불태워야해요",
          "speaker_id": "1",
          "time": "20210820 21:04"
        },
        {
          "id": "MDRW2100003234.1.14",
          "form": "와 매일 운동하세요?",
          "original_form": "와 매일 운동하세요?",
          "speaker_id": "2",
          "time": "20210820 21:04"
        },
        {
          "id": "MDRW2100003234.1.15",
          "form": "아니요... 주말만요... 억지로해요...",
          "original_form": "아니요... 주말만요... 억지로해요...",
          "speaker_id": "1",
          "time": "20210820 21:10"
        },
        {
          "id": "MDRW2100003234.1.16",
          "form": "요즘 살이 많이쪄서요 ㅠㅠ",
          "original_form": "요즘 살이 많이쪄서요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 21:10"
        },
        {
          "id": "MDRW2100003234.1.17",
          "form": "코로나라 집에서 술도 음식도 늘었어요ㅠ",
          "original_form": "코로나라 집에서 술도 음식도 늘었어요ㅠ",
          "speaker_id": "2",
          "time": "20210820 21:57"
        },
        {
          "id": "MDRW2100003234.1.18",
          "form": "아 맞아요 ㅠㅠㅠㅠㅠ 집에 있으니까 흑",
          "original_form": "아 맞아요 ㅠㅠㅠㅠㅠ 집에 있으니까 흑",
          "speaker_id": "1",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003234.1.19",
          "form": "술 하세요?",
          "original_form": "술 하세요?",
          "speaker_id": "2",
          "time": "20210820 22:12"
        },
        {
          "id": "MDRW2100003234.1.20",
          "form": "술을 그닥 좋아하진 않아요",
          "original_form": "술을 그닥 좋아하진 않아요",
          "speaker_id": "1",
          "time": "20210820 22:12"
        }
      ]
