      "utterance": [
        {
          "id": "MDRW2100032079.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032079.1.2",
          "form": "좋아하는 메뉴가 뭔가요?",
          "original_form": "좋아하는 메뉴가 뭔가요?",
          "speaker_id": "2",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100032079.1.3",
          "form": "먹는거 딱히 가리거나 하진 않아요",
          "original_form": "먹는거 딱히 가리거나 하진 않아요",
          "speaker_id": "1",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032079.1.4",
          "form": "지금은 닭갈비 먹고싶네요...",
          "original_form": "지금은 닭갈비 먹고싶네요...",
          "speaker_id": "1",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032079.1.5",
          "form": "제 동생이 닭갈비진짜 좋아해요~",
          "original_form": "제 동생이 닭갈비진짜 좋아해요~",
          "speaker_id": "2",
          "time": "20210905 12:20"
        },
        {
          "id": "MDRW2100032079.1.6",
          "form": "요리는 잘하시나요?",
          "original_form": "요리는 잘하시나요?",
          "speaker_id": "2",
          "time": "20210905 12:20"
        },
        {
          "id": "MDRW2100032079.1.7",
          "form": "아직 제대로 해본 적 없어요...",
          "original_form": "아직 제대로 해본 적 없어요...",
          "speaker_id": "1",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100032079.1.8",
          "form": "엇ㅋㅋㅋ저도 잘못해요ㅋㅋㅋ",
          "original_form": "엇ㅋㅋㅋ저도 잘못해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100032079.1.9",
          "form": "어렵죠 아무래도",
          "original_form": "어렵죠 아무래도",
          "speaker_id": "2",
          "time": "20210905 12:22"
        },
        {
          "id": "MDRW2100032079.1.10",
          "form": "레시피따라하기도 쉽지않구",
          "original_form": "레시피따라하기도 쉽지않구",
          "speaker_id": "2",
          "time": "20210905 12:22"
        },
        {
          "id": "MDRW2100032079.1.11",
          "form": "따라해도 맛이 좀 다르죠ㅠ",
          "original_form": "따라해도 맛이 좀 다르죠ㅠ",
          "speaker_id": "2",
          "time": "20210905 12:22"
        },
        {
          "id": "MDRW2100032079.1.12",
          "form": "name2은 어떤 음식 좋아하시나요??",
          "original_form": "&name2&은 어떤 음식 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210905 12:22"
        },
        {
          "id": "MDRW2100032079.1.13",
          "form": "곱창 막창 이런거 죠아해용",
          "original_form": "곱창 막창 이런거 죠아해용",
          "speaker_id": "2",
          "time": "20210905 12:23"
        },
        {
          "id": "MDRW2100032079.1.14",
          "form": "헐 저 먹어본 적 없는데...",
          "original_form": "헐 저 먹어본 적 없는데...",
          "speaker_id": "1",
          "time": "20210905 12:23"
        },
        {
          "id": "MDRW2100032079.1.15",
          "form": "아진짜요",
          "original_form": "아진짜요",
          "speaker_id": "2",
          "time": "20210905 12:40"
        },
        {
          "id": "MDRW2100032079.1.16",
          "form": "기회 생기면 도전해봐요ㅋㅋㅋ",
          "original_form": "기회 생기면 도전해봐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:40"
        },
        {
          "id": "MDRW2100032079.1.17",
          "form": "저는 제취향이에요",
          "original_form": "저는 제취향이에요",
          "speaker_id": "2",
          "time": "20210905 12:40"
        },
        {
          "id": "MDRW2100032079.1.18",
          "form": "나중에 함 먹어봐야겠네요",
          "original_form": "나중에 함 먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210905 12:44"
        },
        {
          "id": "MDRW2100032079.1.19",
          "form": "점심은 드셨나요??",
          "original_form": "점심은 드셨나요??",
          "speaker_id": "1",
          "time": "20210905 12:45"
        },
        {
          "id": "MDRW2100032079.1.20",
          "form": "아니용",
          "original_form": "아니용",
          "speaker_id": "2",
          "time": "20210905 12:52"
        },
        {
          "id": "MDRW2100032079.1.21",
          "form": "드셨나요~?",
          "original_form": "드셨나요~?",
          "speaker_id": "2",
          "time": "20210905 12:52"
        },
        {
          "id": "MDRW2100032079.1.22",
          "form": "방금 먹었어요!",
          "original_form": "방금 먹었어요!",
          "speaker_id": "1",
          "time": "20210905 13:03"
        },
        {
          "id": "MDRW2100032079.1.23",
          "form": "불고기요",
          "original_form": "불고기요",
          "speaker_id": "1",
          "time": "20210905 13:03"
        },
        {
          "id": "MDRW2100032079.1.24",
          "form": "맛있겠네요~",
          "original_form": "맛있겠네요~",
