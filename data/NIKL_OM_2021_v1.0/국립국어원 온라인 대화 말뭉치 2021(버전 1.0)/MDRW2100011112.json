      "utterance": [
        {
          "id": "MDRW2100011112.1.1",
          "form": "나는 유산균 챙겨먹어",
          "original_form": "나는 유산균 챙겨먹어",
          "speaker_id": "2",
          "time": "20210824 01:10"
        },
        {
          "id": "MDRW2100011112.1.2",
          "form": "너는 먹는거 있어?",
          "original_form": "너는 먹는거 있어?",
          "speaker_id": "2",
          "time": "20210824 01:10"
        },
        {
          "id": "MDRW2100011112.1.3",
          "form": "나는 비타민 디랑 사과식초 챙겨 먹어!!",
          "original_form": "나는 비타민 디랑 사과식초 챙겨 먹어!!",
          "speaker_id": "1",
          "time": "20210824 01:10"
        },
        {
          "id": "MDRW2100011112.1.4",
          "form": "유산균 어디꺼 먹어 나 사야하는데",
          "original_form": "유산균 어디꺼 먹어 나 사야하는데",
          "speaker_id": "1",
          "time": "20210824 01:10"
        },
        {
          "id": "MDRW2100011112.1.5",
          "form": "우와 사과식초 신기해",
          "original_form": "우와 사과식초 신기해",
          "speaker_id": "2",
          "time": "20210824 01:11"
        },
        {
          "id": "MDRW2100011112.1.6",
          "form": "식초랑 물을 같이 마시는거야?",
          "original_form": "식초랑 물을 같이 마시는거야?",
          "speaker_id": "2",
          "time": "20210824 01:11"
        },
        {
          "id": "MDRW2100011112.1.7",
          "form": "알약으로 나오는데 그냥 삼키면 돼 ㅋㅋㅋ",
          "original_form": "알약으로 나오는데 그냥 삼키면 돼 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 01:11"
        },
        {
          "id": "MDRW2100011112.1.8",
          "form": "난도 쿠팡에서 보고 샀어 다이어트에 좋대!",
          "original_form": "난도 쿠팡에서 보고 샀어 다이어트에 좋대!",
          "speaker_id": "1",
          "time": "20210824 01:11"
        },
        {
          "id": "MDRW2100011112.1.9",
          "form": "우와 신기하다",
          "original_form": "우와 신기하다",
          "speaker_id": "2",
          "time": "20210824 01:13"
        },
        {
          "id": "MDRW2100011112.1.10",
          "form": "그치 유산균은 효과있어?",
          "original_form": "그치 유산균은 효과있어?",
          "speaker_id": "1",
          "time": "20210824 01:13"
        },
        {
          "id": "MDRW2100011112.1.11",
          "form": "응 꾸준히 먹으면 효과있어",
          "original_form": "응 꾸준히 먹으면 효과있어",
          "speaker_id": "2",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100011112.1.12",
          "form": "사과식초는 어때?",
          "original_form": "사과식초는 어때?",
          "speaker_id": "2",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011112.1.13",
          "form": "이거는 먹은지 2주 넘어서 잘 모르겠어",
          "original_form": "이거는 먹은지 2주 넘어서 잘 모르겠어",
          "speaker_id": "1",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011112.1.14",
          "form": "솔직히 배송비 맞추느라 산거여서 잘 모르겠네?",
          "original_form": "솔직히 배송비 맞추느라 산거여서 잘 모르겠네?",
          "speaker_id": "1",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011112.1.15",
          "form": "그정도면 잘 모르지",
          "original_form": "그정도면 잘 모르지",
          "speaker_id": "2",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011112.1.16",
          "form": "맞아맞아 더 먹어보려고",
          "original_form": "맞아맞아 더 먹어보려고",
          "speaker_id": "1",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011112.1.17",
          "form": "유산균은 한 한달 먹으면 안먹을때랑 차이 많이 나",
          "original_form": "유산균은 한 한달 먹으면 안먹을때랑 차이 많이 나",
          "speaker_id": "2",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011112.1.18",
          "form": "나도 유산균 잘 먹어야겟다",
          "original_form": "나도 유산균 잘 먹어야겟다",
          "speaker_id": "1",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011112.1.19",
          "form": "사과식초는 얼마야?",
          "original_form": "사과식초는 얼마야?",
          "speaker_id": "2",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011112.1.20",
          "form": "오 그래?? 사과식초는 한달 7천원 정도?",
          "original_form": "오 그래?? 사과식초는 한달 7천원 정도?",
          "speaker_id": "1",
          "time": "20210824 01:16"
