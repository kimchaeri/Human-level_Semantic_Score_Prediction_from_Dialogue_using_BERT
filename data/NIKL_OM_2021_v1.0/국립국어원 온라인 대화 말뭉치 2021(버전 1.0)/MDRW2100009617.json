      "utterance": [
        {
          "id": "MDRW2100009617.1.1",
          "form": "저녁 먹었어?",
          "original_form": "저녁 먹었어?",
          "speaker_id": "1",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009617.1.2",
          "form": "먹었어요~ 아까 8시쯤?",
          "original_form": "먹었어요~ 아까 8시쯤?",
          "speaker_id": "2",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009617.1.3",
          "form": "뭐 드셨나요? ㅎㅎ",
          "original_form": "뭐 드셨나요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 01:49"
        },
        {
          "id": "MDRW2100009617.1.4",
          "form": "저는 서브웨이  샌드위치 먹었어요~",
          "original_form": "저는 서브웨이  샌드위치 먹었어요~",
          "speaker_id": "1",
          "time": "20210823 01:49"
        },
        {
          "id": "MDRW2100009617.1.5",
          "form": "저 수육에 상추 싸서 먹었어요",
          "original_form": "저 수육에 상추 싸서 먹었어요",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009617.1.6",
          "form": "서브웨이... 주문 어렵던데 어떤 거 좋아하시나요?!",
          "original_form": "서브웨이... 주문 어렵던데 어떤 거 좋아하시나요?!",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009617.1.7",
          "form": "헐 완전 맛있는거 드셨네요 부러워요",
          "original_form": "헐 완전 맛있는거 드셨네요 부러워요",
          "speaker_id": "1",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009617.1.8",
          "form": "저는 오늘 케이 바베큐 먹었는데",
          "original_form": "저는 오늘 케이 바베큐 먹었는데",
          "speaker_id": "1",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009617.1.9",
          "form": "원래는 로티세리 좋아해요 name2님은요?",
          "original_form": "원래는 로티세리 좋아해요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009617.1.10",
          "form": "저 사실 서브웨이를 두번밖에 안 가봐서",
          "original_form": "저 사실 서브웨이를 두번밖에 안 가봐서",
          "speaker_id": "2",
          "time": "20210823 01:53"
        },
        {
          "id": "MDRW2100009617.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:53"
        },
        {
          "id": "MDRW2100009617.1.12",
          "form": "플랫브래드 주문한 것만 기억나요",
          "original_form": "플랫브래드 주문한 것만 기억나요",
          "speaker_id": "2",
          "time": "20210823 01:53"
        },
        {
          "id": "MDRW2100009617.1.13",
          "form": "앜ㅋㅋㅋㅋ 그렇구나 플랫브래드 많이 먹죠",
          "original_form": "앜ㅋㅋㅋㅋ 그렇구나 플랫브래드 많이 먹죠",
          "speaker_id": "1",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009617.1.14",
          "form": "쿠키는 드셔보셨나요?",
          "original_form": "쿠키는 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009617.1.15",
          "form": "쿠키는 먹어봤어요!",
          "original_form": "쿠키는 먹어봤어요!",
          "speaker_id": "2",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009617.1.16",
          "form": "친구들이 맛있다고 그렇게 먹어보래서",
          "original_form": "친구들이 맛있다고 그렇게 먹어보래서",
          "speaker_id": "2",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009617.1.17",
          "form": "먹었었는데 맛있더라구요",
          "original_form": "먹었었는데 맛있더라구요",
          "speaker_id": "2",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009617.1.18",
          "form": "그쵸 저는 완전 좋아해요 ㅎㅎㅎ",
          "original_form": "그쵸 저는 완전 좋아해요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 01:55"
        },
        {
          "id": "MDRW2100009617.1.19",
          "form": "라즈베리 쿠키도 있어요",
          "original_form": "라즈베리 쿠키도 있어요",
          "speaker_id": "1",
          "time": "20210823 01:55"
        },
        {
          "id": "MDRW2100009617.1.20",
          "form": "우와 라즈베리",
          "original_form": "우와 라즈베리",
          "speaker_id": "2",
          "time": "20210823 01:56"
        },
        {
          "id": "MDRW2100009617.1.21",
          "form": "상큼한가요?",
          "original_form": "상큼한가요?",
          "speaker_id": "2",
          "time": "20210823 01:56"
        },
        {
          "id": "MDRW2100009617.1.22",
          "form": "상큼하진 않고 과일 맛이 난다고 말하면 될 것 같아요 ㅎㅎ",
          "original_form": "상큼하진 않고 과일 맛이 난다고 말하면 될 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 01:57"
        },
        {
          "id": "MDRW2100009617.1.23",
          "form": "가격도 저렴해서 좋아요!",
          "original_form": "가격도 저렴해서 좋아요!",
          "speaker_id": "1",
          "time": "20210823 01:57"
        },
        {
          "id": "MDRW2100009617.1.24",
          "form": "사실 저희 지역에 서브웨이가",
          "original_form": "사실 저희 지역에 서브웨이가",
          "speaker_id": "2",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009617.1.25",
          "form": "번화가 쪽에만 있어서",
          "original_form": "번화가 쪽에만 있어서",
          "speaker_id": "2",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009617.1.26",
          "form": "아 그렇구나 번화가면 시내 인가요?",
          "original_form": "아 그렇구나 번화가면 시내 인가요?",
          "speaker_id": "1",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009617.1.27",
          "form": "다음에 가면 먹어봐야겠어요",
          "original_form": "다음에 가면 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009617.1.28",
          "form": "네 맞아요 시내!",
          "original_form": "네 맞아요 시내!",
          "speaker_id": "2",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009617.1.29",
          "form": "아하 그렇군요 ㅎㅎ 제 본가에는 아예 없다가",
          "original_form": "아하 그렇군요 ㅎㅎ 제 본가에는 아예 없다가",
          "speaker_id": "1",
          "time": "20210823 02:02"
        },
        {
          "id": "MDRW2100009617.1.30",
          "form": "1년 전에 생겼네요 ㅋㅋㅋㅋㅋ",
          "original_form": "1년 전에 생겼네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
