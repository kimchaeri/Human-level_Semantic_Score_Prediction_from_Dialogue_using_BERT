          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005658.1.4",
          "form": "여행좋아하시나요",
          "original_form": "여행좋아하시나요",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005658.1.5",
          "form": "그럼요~",
          "original_form": "그럼요~",
          "speaker_id": "2",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005658.1.6",
          "form": "여행 가고싶으신가요",
          "original_form": "여행 가고싶으신가요",
          "speaker_id": "2",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005658.1.7",
          "form": "어디 가고싶으세요",
          "original_form": "어디 가고싶으세요",
          "speaker_id": "2",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005658.1.8",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005658.1.9",
          "form": "해외여행가고싶어요",
          "original_form": "해외여행가고싶어요",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005658.1.10",
          "form": "국내랑 해외중에",
          "original_form": "국내랑 해외중에",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005658.1.11",
          "form": "어디가좋으세요",
          "original_form": "어디가좋으세요",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005658.1.12",
          "form": "해외요",
          "original_form": "해외요",
          "speaker_id": "2",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005658.1.13",
          "form": "바다에서 수영하고싶네여",
          "original_form": "바다에서 수영하고싶네여",
          "speaker_id": "2",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005658.1.14",
          "form": "해외어디가보셨나요",
          "original_form": "해외어디가보셨나요",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005658.1.15",
          "form": "일본 갔었는데 그러고보니 일본 바다는 본적이 없네요",
          "original_form": "일본 갔었는데 그러고보니 일본 바다는 본적이 없네요",
          "speaker_id": "2",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005658.1.16",
          "form": "섬나란데",
          "original_form": "섬나란데",
          "speaker_id": "2",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005658.1.17",
          "form": "저도 일본 가봤는데",
          "original_form": "저도 일본 가봤는데",
          "speaker_id": "1",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005658.1.18",
          "form": "바다본기억이없는것같네요",
          "original_form": "바다본기억이없는것같네요",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005658.1.19",
          "form": "동남아가고싶어요 휴양지 바다ㅠ",
          "original_form": "동남아가고싶어요 휴양지 바다ㅠ",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005658.1.20",
          "form": "저도요~~",
          "original_form": "저도요~~",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005658.1.21",
          "form": "야나두~~~",
          "original_form": "야나두~~~",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005658.1.22",
          "form": "코로나끝나면가고싶은곳있나ㅓ어",
          "original_form": "코로나끝나면가고싶은곳있나ㅓ어",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005658.1.23",
          "form": "코로나만 끝나면",
          "original_form": "코로나만 끝나면",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005658.1.24",
          "form": "글쎄요",
          "original_form": "글쎄요",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005658.1.25",
          "form": "유럽??",
          "original_form": "유럽??",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005658.1.26",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005658.1.27",
          "form": "유럽좋죠",
          "original_form": "유럽좋죠",
          "speaker_id": "1",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005658.1.28",
          "form": "가보셨나요^-^?",
          "original_form": "가보셨나요^-^?",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005658.1.29",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005658.1.30",
          "form": "코로나끝나면 안가본 나라들 가보고싶내요",
          "original_form": "코로나끝나면 안가본 나라들 가보고싶내요",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005658.1.31",
          "form": "스페인 스위스가 그렇게 좋대요",
          "original_form": "스페인 스위스가 그렇게 좋대요",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005658.1.32",
          "form": "유럽어디가고싶으세요",
          "original_form": "유럽어디가고싶으세요",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005658.1.33",
          "form": "저도 스위스 가보고싶고요",
          "original_form": "저도 스위스 가보고싶고요",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005658.1.34",
          "form": "이탈리아 음식 궁금해서 가보고싶내요",
          "original_form": "이탈리아 음식 궁금해서 가보고싶내요",
          "speaker_id": "2",
          "time": "20210821 23:49"
        }
      ]
