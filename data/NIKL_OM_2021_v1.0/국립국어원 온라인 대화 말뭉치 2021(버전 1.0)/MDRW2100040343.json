      "utterance": [
        {
          "id": "MDRW2100040343.1.1",
          "form": "여행가고싶네요",
          "original_form": "여행가고싶네요",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040343.1.2",
          "form": "남친은 어느나라에있나요?",
          "original_form": "남친은 어느나라에있나요?",
          "speaker_id": "1",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100040343.1.3",
          "form": "코로나가없으면 가보면좋은데",
          "original_form": "코로나가없으면 가보면좋은데",
          "speaker_id": "1",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100040343.1.4",
          "form": "남친은 유럽에 있어요",
          "original_form": "남친은 유럽에 있어요",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100040343.1.5",
          "form": "이젠 좀 지치네요",
          "original_form": "이젠 좀 지치네요",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100040343.1.6",
          "form": "볼수도없고 사이버연애 같아요",
          "original_form": "볼수도없고 사이버연애 같아요",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100040343.1.7",
          "form": "어느나라요?",
          "original_form": "어느나라요?",
          "speaker_id": "1",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100040343.1.8",
          "form": "유럽좋네요",
          "original_form": "유럽좋네요",
          "speaker_id": "1",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100040343.1.9",
          "form": "ㅡ네네 북유롭이에요",
          "original_form": "ㅡ네네 북유롭이에요",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100040343.1.10",
          "form": "좋아보이더라고요",
          "original_form": "좋아보이더라고요",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100040343.1.11",
          "form": "북유럽이멋있더라구요",
          "original_form": "북유럽이멋있더라구요",
          "speaker_id": "1",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100040343.1.12",
          "form": "구쵸 꼭 가보고싶은데",
          "original_form": "구쵸 꼭 가보고싶은데",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100040343.1.13",
          "form": "그전에 지쳐서 헤어질것같아요",
          "original_form": "그전에 지쳐서 헤어질것같아요",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100040343.1.14",
          "form": "유학중이라했죠",
          "original_form": "유학중이라했죠",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100040343.1.15",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100040343.1.16",
          "form": "유학갔다오면 좋은직장 얻겠죠",
          "original_form": "유학갔다오면 좋은직장 얻겠죠",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100040343.1.17",
          "form": "그때되묜 제 사람이 아닐까봐 걱정되어요",
          "original_form": "그때되묜 제 사람이 아닐까봐 걱정되어요",
          "speaker_id": "2",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100040343.1.18",
          "form": "기다리기만 실컷 힘들게 기다리고",
          "original_form": "기다리기만 실컷 힘들게 기다리고",
          "speaker_id": "2",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100040343.1.19",
          "form": "남줄까봐 두려워요",
          "original_form": "남줄까봐 두려워요",
          "speaker_id": "2",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100040343.1.20",
          "form": "안돼요..그건",
          "original_form": "안돼요..그건",
          "speaker_id": "1",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100040343.1.21",
          "form": "꼭 붙잡으세요",
          "original_form": "꼭 붙잡으세요",
          "speaker_id": "1",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100040343.1.22",
          "form": "그니까요 .. 하지만 아직도 1년 4개월이나 남았고",
          "original_form": "그니까요 .. 하지만 아직도 1년 4개월이나 남았고",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100040343.1.23",
          "form": "견문이 넓은사람이 좋아요",
          "original_form": "견문이 넓은사람이 좋아요",
          "speaker_id": "1",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100040343.1.24",
          "form": "못보니 점점 멀어지네요 여행을 한번 가야하는데",
          "original_form": "못보니 점점 멀어지네요 여행을 한번 가야하는데",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100040343.1.25",
          "form": "넘 머네요",
          "original_form": "넘 머네요",
          "speaker_id": "1",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100040343.1.26",
          "form": "멀기는",
          "original_form": "멀기는",
          "speaker_id": "1",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100040343.1.27",
          "form": "15시간이상 비행기탄것같아요",
          "original_form": "15시간이상 비행기탄것같아요",
          "speaker_id": "1",
          "time": "20210904 16:50"
        }
      ]
