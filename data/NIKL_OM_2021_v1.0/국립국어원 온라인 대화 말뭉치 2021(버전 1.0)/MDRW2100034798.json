      "utterance": [
        {
          "id": "MDRW2100034798.1.1",
          "form": "넼ㅋㅋㅋ 별 소득없이 끝난 소개팅이라 ㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋ 별 소득없이 끝난 소개팅이라 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100034798.1.2",
          "form": "그냥 빨리 차만 마시고 갔던",
          "original_form": "그냥 빨리 차만 마시고 갔던",
          "speaker_id": "2",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100034798.1.3",
          "form": "앜ㅋㅋㅋㅋ 그럴거였으면",
          "original_form": "앜ㅋㅋㅋㅋ 그럴거였으면",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034798.1.4",
          "form": "드실걸 그랬어요!!",
          "original_form": "드실걸 그랬어요!!",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034798.1.5",
          "form": "크림브륄레.. 오늘에서야 제 마음에 쏙 들어오는 단어네요 ㅇ",
          "original_form": "크림브륄레.. 오늘에서야 제 마음에 쏙 들어오는 단어네요 ㅇ",
          "speaker_id": "2",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034798.1.6",
          "form": "앜ㅋㅋㅋ 마음에 안착되셨나요 ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ 마음에 안착되셨나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100034798.1.7",
          "form": "네 왠지 운명적인 단어",
          "original_form": "네 왠지 운명적인 단어",
          "speaker_id": "2",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100034798.1.8",
          "form": "이름부터 뭔가 맛날거같지 않나요?ㅋㅋㅋ",
          "original_form": "이름부터 뭔가 맛날거같지 않나요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034798.1.9",
          "form": "네 그냥 고급지네요",
          "original_form": "네 그냥 고급지네요",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034798.1.10",
          "form": "뭔가 있어보이죠",
          "original_form": "뭔가 있어보이죠",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034798.1.11",
          "form": "우리동네 마카롱집엔 왜 저 맛을 안 팔아서..!!",
          "original_form": "우리동네 마카롱집엔 왜 저 맛을 안 팔아서..!!",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034798.1.12",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034798.1.13",
          "form": "흔하지 않더라구여!! 아이디어스 추천합니다",
          "original_form": "흔하지 않더라구여!! 아이디어스 추천합니다",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034798.1.14",
          "form": "아 아이디어스에도 마카롱 팔아요?",
          "original_form": "아 아이디어스에도 마카롱 팔아요?",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100034798.1.15",
          "form": "네 저 아이디어스 안써봤는데",
          "original_form": "네 저 아이디어스 안써봤는데",
          "speaker_id": "1",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100034798.1.16",
          "form": "할인이벤트하길래 샀거든요 근데 최고에요 ㅋㅋㅋ",
          "original_form": "할인이벤트하길래 샀거든요 근데 최고에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100034798.1.17",
          "form": "오오 찾아봐야겠네요",
          "original_form": "오오 찾아봐야겠네요",
