      "utterance": [
        {
          "id": "MDRW2100011515.1.1",
          "form": "집안일 자주 하시나요?",
          "original_form": "집안일 자주 하시나요?",
          "speaker_id": "1",
          "time": "20210826 09:50"
        },
        {
          "id": "MDRW2100011515.1.2",
          "form": "억지로 하려고 하죠",
          "original_form": "억지로 하려고 하죠",
          "speaker_id": "2",
          "time": "20210826 09:50"
        },
        {
          "id": "MDRW2100011515.1.3",
          "form": "밤새님은여",
          "original_form": "밤새님은여",
          "speaker_id": "2",
          "time": "20210826 09:50"
        },
        {
          "id": "MDRW2100011515.1.4",
          "form": "저두요. 너무 할 게 많아요.",
          "original_form": "저두요. 너무 할 게 많아요.",
          "speaker_id": "1",
          "time": "20210826 09:53"
        },
        {
          "id": "MDRW2100011515.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 09:56"
        },
        {
          "id": "MDRW2100011515.1.6",
          "form": "지겨워요 ㅠ",
          "original_form": "지겨워요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 09:56"
        },
        {
          "id": "MDRW2100011515.1.7",
          "form": "설거지는 바로 해야되서 귀찮아요",
          "original_form": "설거지는 바로 해야되서 귀찮아요",
          "speaker_id": "1",
          "time": "20210826 09:59"
        },
        {
          "id": "MDRW2100011515.1.8",
          "form": "ㅋㅋ 맞아요 흑",
          "original_form": "ㅋㅋ 맞아요 흑",
          "speaker_id": "2",
          "time": "20210826 10:27"
        },
        {
          "id": "MDRW2100011515.1.9",
          "form": "하기 싫어요 청소",
          "original_form": "하기 싫어요 청소",
          "speaker_id": "2",
          "time": "20210826 10:27"
        },
        {
          "id": "MDRW2100011515.1.10",
          "form": "맞아요. 청소도 해야될게 많아서ㅋㅋ",
          "original_form": "맞아요. 청소도 해야될게 많아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100011515.1.11",
          "form": "정말.. 하루종일해도",
          "original_form": "정말.. 하루종일해도",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100011515.1.12",
          "form": "안으돼여...",
          "original_form": "안으돼여...",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100011515.1.13",
          "form": "치워도 먼지 계속 쌓이구요ㅠ",
          "original_form": "치워도 먼지 계속 쌓이구요ㅠ",
          "speaker_id": "1",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100011515.1.14",
          "form": "치우느라 하루가 다 가요..",
          "original_form": "치우느라 하루가 다 가요..",
          "speaker_id": "2",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100011515.1.15",
          "form": "매일 안 치우면 또 찝찝하고요...",
          "original_form": "매일 안 치우면 또 찝찝하고요...",
          "speaker_id": "1",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100011515.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ공감합니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ공감합니다",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100011515.1.17",
          "form": "집안일은 누가 대신 해줬으면 좋겠어요",
          "original_form": "집안일은 누가 대신 해줬으면 좋겠어요",
