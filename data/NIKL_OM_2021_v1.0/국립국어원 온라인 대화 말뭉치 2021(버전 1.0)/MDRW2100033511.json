      "utterance": [
        {
          "id": "MDRW2100033511.1.1",
          "form": "각자 환경이 다르자노",
          "original_form": "각자 환경이 다르자노",
          "speaker_id": "1",
          "time": "20210824 10:43"
        },
        {
          "id": "MDRW2100033511.1.2",
          "form": "신일 선풍기 샀다고 이야기했던가요",
          "original_form": "신일 선풍기 샀다고 이야기했던가요",
          "speaker_id": "2",
          "time": "20210824 10:46"
        },
        {
          "id": "MDRW2100033511.1.3",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210824 10:53"
        },
        {
          "id": "MDRW2100033511.1.4",
          "form": "나는 버티컬마우스 당근했음",
          "original_form": "나는 버티컬마우스 당근했음",
          "speaker_id": "1",
          "time": "20210824 10:53"
        },
        {
          "id": "MDRW2100033511.1.5",
          "form": "오 얼마였어요",
          "original_form": "오 얼마였어요",
          "speaker_id": "2",
          "time": "20210824 10:56"
        },
        {
          "id": "MDRW2100033511.1.6",
          "form": "오만원 넘던데",
          "original_form": "오만원 넘던데",
          "speaker_id": "2",
          "time": "20210824 10:56"
        },
        {
          "id": "MDRW2100033511.1.7",
          "form": "새거는",
          "original_form": "새거는",
          "speaker_id": "2",
          "time": "20210824 10:56"
        },
        {
          "id": "MDRW2100033511.1.8",
          "form": "삼처넌",
          "original_form": "삼처넌",
          "speaker_id": "1",
          "time": "20210824 10:56"
        },
        {
          "id": "MDRW2100033511.1.9",
          "form": "꿀입니다",
          "original_form": "꿀입니다",
          "speaker_id": "1",
          "time": "20210824 10:56"
        },
        {
          "id": "MDRW2100033511.1.10",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210824 10:56"
        },
        {
          "id": "MDRW2100033511.1.11",
          "form": "이래서 당근당근하는구나",
          "original_form": "이래서 당근당근하는구나",
          "speaker_id": "2",
          "time": "20210824 10:57"
        },
        {
          "id": "MDRW2100033511.1.12",
          "form": "손목 안좋았는디 잘쓰고있당",
          "original_form": "손목 안좋았는디 잘쓰고있당",
          "speaker_id": "1",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100033511.1.13",
          "form": "나는 음 홈쇼핑 장어 가성비 좋음",
          "original_form": "나는 음 홈쇼핑 장어 가성비 좋음",
          "speaker_id": "2",
          "time": "20210824 11:25"
        },
        {
          "id": "MDRW2100033511.1.14",
          "form": "오오 장어 얼마유",
          "original_form": "오오 장어 얼마유",
          "speaker_id": "1",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100033511.1.15",
          "form": "10마리 44900원인가 49800원인가 ㅋㅋ",
          "original_form": "10마리 44900원인가 49800원인가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:27"
        },
        {
          "id": "MDRW2100033511.1.16",
          "form": "그랬다는",
          "original_form": "그랬다는",
          "speaker_id": "2",
          "time": "20210824 11:27"
        },
        {
          "id": "MDRW2100033511.1.17",
          "form": "오 10마리인데 사천오백원 택이네",
          "original_form": "오 10마리인데 사천오백원 택이네",
          "speaker_id": "1",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100033511.1.18",
          "form": "ㅇㅇ 무려 20마리 ㅋㅋ",
          "original_form": "ㅇㅇ 무려 20마리 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:35"
        },
        {
          "id": "MDRW2100033511.1.19",
          "form": "맛 어땠나 물어봐주세요 ㅋㅋㅋ",
          "original_form": "맛 어땠나 물어봐주세요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:35"
        },
        {
          "id": "MDRW2100033511.1.20",
