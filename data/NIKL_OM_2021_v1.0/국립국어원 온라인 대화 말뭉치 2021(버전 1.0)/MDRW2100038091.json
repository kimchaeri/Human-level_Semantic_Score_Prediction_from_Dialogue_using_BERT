      "utterance": [
        {
          "id": "MDRW2100038091.1.1",
          "form": "갑자기?",
          "original_form": "갑자기?",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038091.1.2",
          "form": "응응 내가 좋은 할인정보를 들었어",
          "original_form": "응응 내가 좋은 할인정보를 들었어",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038091.1.3",
          "form": "어디서 할인해?",
          "original_form": "어디서 할인해?",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038091.1.4",
          "form": "빨리 알려줘",
          "original_form": "빨리 알려줘",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038091.1.5",
          "form": "곧 추석이잖아",
          "original_form": "곧 추석이잖아",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038091.1.6",
          "form": "너 홈플러스 어플을 사용해?",
          "original_form": "너 홈플러스 어플을 사용해?",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038091.1.7",
          "form": "차례 장을 미리 봐둬야갰어",
          "original_form": "차례 장을 미리 봐둬야갰어",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038091.1.8",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038091.1.9",
          "form": "어플이 필요하구나",
          "original_form": "어플이 필요하구나",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038091.1.10",
          "form": "거기 오랫동안 사용안했으면 대박 큰 쿠폰을 주더라",
          "original_form": "거기 오랫동안 사용안했으면 대박 큰 쿠폰을 주더라",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038091.1.11",
          "form": "그럼 깔게!",
          "original_form": "그럼 깔게!",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038091.1.12",
          "form": "신규가입자도 줘!!",
          "original_form": "신규가입자도 줘!!",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038091.1.13",
          "form": "앱에서만 사용을 안하면 되는거야?",
          "original_form": "앱에서만 사용을 안하면 되는거야?",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038091.1.14",
          "form": "좋다!",
          "original_form": "좋다!",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038091.1.15",
          "form": "응응 나도 이번에 아주 거대하게 장을 봤어",
          "original_form": "응응 나도 이번에 아주 거대하게 장을 봤어",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038091.1.16",
          "form": "그냥 앱 둘러보기만 한 사람도 받을 슈 있어?",
          "original_form": "그냥 앱 둘러보기만 한 사람도 받을 슈 있어?",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038091.1.17",
          "form": "구매이력이 없다면",
          "original_form": "구매이력이 없다면",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038091.1.18",
          "form": "넌 뭘 샀어?",
          "original_form": "넌 뭘 샀어?",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038091.1.19",
          "form": "응 가능해 일단 받아보고 적용해봐",
          "original_form": "응 가능해 일단 받아보고 적용해봐",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038091.1.20",
          "form": "식용유는 산거야?",
          "original_form": "식용유는 산거야?",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038091.1.21",
          "form": "나는 계란을 샀어",
          "original_form": "나는 계란을 샀어",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038091.1.22",
          "form": "고마워!",
          "original_form": "고마워!",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038091.1.23",
          "form": "너 장을 거하게 봤다던데",
          "original_form": "너 장을 거하게 봤다던데",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038091.1.24",
          "form": "내 친구가 그러더라!",
          "original_form": "내 친구가 그러더라!",
          "speaker_id": "2",
          "time": "20210901 23:07"
        }
      ]
