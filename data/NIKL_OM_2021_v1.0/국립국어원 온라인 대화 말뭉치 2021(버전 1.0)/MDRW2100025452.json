      "utterance": [
        {
          "id": "MDRW2100025452.1.1",
          "form": "오늘 점심 닭튀김이래요",
          "original_form": "오늘 점심 닭튀김이래요",
          "speaker_id": "2",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025452.1.2",
          "form": "어제 밤에 치킨 먹었는데",
          "original_form": "어제 밤에 치킨 먹었는데",
          "speaker_id": "2",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025452.1.3",
          "form": "치킨도아니고 닭튀김은뭐에요 ㅋㅋㅋㅋㅋ",
          "original_form": "치킨도아니고 닭튀김은뭐에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025452.1.4",
          "form": "식당이 그렇죠",
          "original_form": "식당이 그렇죠",
          "speaker_id": "2",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025452.1.5",
          "form": "치킨 나오겠어요?",
          "original_form": "치킨 나오겠어요?",
          "speaker_id": "2",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025452.1.6",
          "form": "치킨 비스무리한 닭튀김",
          "original_form": "치킨 비스무리한 닭튀김",
          "speaker_id": "2",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025452.1.7",
          "form": "어제 교촌치킨에서 후원들어왓는데",
          "original_form": "어제 교촌치킨에서 후원들어왓는데",
          "speaker_id": "1",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025452.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025452.1.9",
          "form": "한마리남아서 그거먹을거에요 ㅠ졸맛탱이겟다",
          "original_form": "한마리남아서 그거먹을거에요 ㅠ졸맛탱이겟다",
          "speaker_id": "1",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025452.1.10",
          "form": "배고프다",
          "original_form": "배고프다",
          "speaker_id": "1",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025452.1.11",
          "form": "허니콤봅니까?",
          "original_form": "허니콤봅니까?",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025452.1.12",
          "form": "아 허니순살입니다",
          "original_form": "아 허니순살입니다",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025452.1.13",
          "form": "오 좋다",
          "original_form": "오 좋다",
          "speaker_id": "2",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100025452.1.14",
          "form": "허니순살 매콤소스 살짝 찍으면",
          "original_form": "허니순살 매콤소스 살짝 찍으면",
          "speaker_id": "2",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100025452.1.15",
          "form": "크",
          "original_form": "크",
          "speaker_id": "2",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100025452.1.16",
          "form": "근데 닭찌가 많드라구요 순살은 ㅠ",
          "original_form": "근데 닭찌가 많드라구요 순살은 ㅠ",
          "speaker_id": "1",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100025452.1.17",
          "form": "따땃할땐 그래도 먹을만한데",
          "original_form": "따땃할땐 그래도 먹을만한데",
          "speaker_id": "2",
          "time": "20210901 11:55"
        },
        {
          "id": "MDRW2100025452.1.18",
          "form": "어제꺼라 먹다 강냉이나갈수도",
          "original_form": "어제꺼라 먹다 강냉이나갈수도",
          "speaker_id": "1",
          "time": "20210901 11:55"
        },
        {
          "id": "MDRW2100025452.1.19",
          "form": "교촌의 단점..",
          "original_form": "교촌의 단점..",
          "speaker_id": "2",
          "time": "20210901 11:57"
        },
        {
          "id": "MDRW2100025452.1.20",
          "form": "다음주부터해야겟네",
          "original_form": "다음주부터해야겟네",
          "speaker_id": "1",
          "time": "20210901 11:58"
        },
        {
          "id": "MDRW2100025452.1.21",
          "form": "오늘부터 풀,만 먹으려고 했는데",
          "original_form": "오늘부터 풀,만 먹으려고 했는데",
          "speaker_id": "1",
          "time": "20210901 11:58"
        },
        {
          "id": "MDRW2100025452.1.22",
          "form": "풉ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "풉ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100025452.1.23",
          "form": "의지는 좋네요",
          "original_form": "의지는 좋네요",
          "speaker_id": "2",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100025452.1.24",
          "form": "와 풉 한단어가 날 흔드네",
          "original_form": "와 풉 한단어가 날 흔드네",
          "speaker_id": "1",
          "time": "20210901 12:53"
        },
        {
          "id": "MDRW2100025452.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:53"
        },
        {
          "id": "MDRW2100025452.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:57"
        },
        {
          "id": "MDRW2100025452.1.27",
          "form": "아 뭔 업체에서 교육한다고 왔는데",
          "original_form": "아 뭔 업체에서 교육한다고 왔는데",
          "speaker_id": "2",
          "time": "20210901 12:58"
        },
        {
          "id": "MDRW2100025452.1.28",
          "form": "장사임 이런거 다 ..",
          "original_form": "장사임 이런거 다 ..",
          "speaker_id": "2",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100025452.1.29",
          "form": "말이 교육이지",
          "original_form": "말이 교육이지",
          "speaker_id": "2",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100025452.1.30",
          "form": "후 사장님 지짜",
          "original_form": "후 사장님 지짜",
          "speaker_id": "2",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100025452.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100025452.1.32",
          "form": "아 직장내교육",
          "original_form": "아 직장내교육",
          "speaker_id": "1",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100025452.1.33",
          "form": "마지막에 보험팔고",
          "original_form": "마지막에 보험팔고",
          "speaker_id": "1",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100025452.1.34",
          "form": "다아는거~~",
          "original_form": "다아는거~~",
          "speaker_id": "1",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100025452.1.35",
          "form": "시간날리겠네 또",
          "original_form": "시간날리겠네 또",
          "speaker_id": "2",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100025452.1.36",
          "form": "ㅇㅇ.. 그런거",
          "original_form": "ㅇㅇ.. 그런거",
          "speaker_id": "2",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100025452.1.37",
          "form": "핸드폰해요 ㅋㅋㅋㅋ",
          "original_form": "핸드폰해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025452.1.38",
          "form": "그런거 다 무더기로듣지않나",
          "original_form": "그런거 다 무더기로듣지않나",
          "speaker_id": "1",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025452.1.39",
          "form": "저번에 온 사람운",
          "original_form": "저번에 온 사람운",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025452.1.40",
          "form": "지 돈 잘번다고 자랑하던데",
          "original_form": "지 돈 잘번다고 자랑하던데",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025452.1.41",
          "form": "그럼 여길 왜 옴..",
          "original_form": "그럼 여길 왜 옴..",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025452.1.42",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025452.1.43",
          "form": "산업은행에서 오셨네요",
          "original_form": "산업은행에서 오셨네요",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025452.1.44",
          "form": "우리도맨날 농협에서 전화오는뎈ㅋㅋㅋㅋ",
          "original_form": "우리도맨날 농협에서 전화오는뎈ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025452.1.45",
          "form": "뻔한 교육이긴 한데",
          "original_form": "뻔한 교육이긴 한데",
          "speaker_id": "2",
          "time": "20210901 13:06"
        },
        {
          "id": "MDRW2100025452.1.46",
          "form": "에이씨 보험이네",
          "original_form": "에이씨 보험이네",
          "speaker_id": "2",
          "time": "20210901 13:07"
        },
        {
          "id": "MDRW2100025452.1.47",
          "form": "은행보험",
          "original_form": "은행보험",
          "speaker_id": "2",
          "time": "20210901 13:07"
        },
        {
          "id": "MDRW2100025452.1.48",
          "form": "그니까요 ㅋㅋㅋㅋㅋ",
          "original_form": "그니까요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:07"
        },
        {
          "id": "MDRW2100025452.1.49",
          "form": "그런거들으면 뭐준다고햇을텐데",
          "original_form": "그런거들으면 뭐준다고햇을텐데",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025452.1.50",
          "form": "소화기이런거",
          "original_form": "소화기이런거",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025452.1.51",
          "form": "펜 주는데요?",
          "original_form": "펜 주는데요?",
          "speaker_id": "2",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025452.1.52",
          "form": "심지어 상조상품파네",
          "original_form": "심지어 상조상품파네",
          "speaker_id": "2",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025452.1.53",
          "form": "펜ㅋㅋㅋㅋㅋ",
          "original_form": "펜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:13"
        },
        {
          "id": "MDRW2100025452.1.54",
          "form": "만년필주나요?",
          "original_form": "만년필주나요?",
          "speaker_id": "1",
          "time": "20210901 13:13"
        },
        {
          "id": "MDRW2100025452.1.55",
          "form": "만년필은 무슨",
          "original_form": "만년필은 무슨",
          "speaker_id": "2",
          "time": "20210901 13:33"
        },
        {
          "id": "MDRW2100025452.1.56",
          "form": "싸구려 중국펜이죠",
          "original_form": "싸구려 중국펜이죠",
