      "utterance": [
        {
          "id": "MDRW2100042600.1.1",
          "form": "오늘 저녁 뭐 먹었어",
          "original_form": "오늘 저녁 뭐 먹었어",
          "speaker_id": "1",
          "time": "20210906 23:17"
        },
        {
          "id": "MDRW2100042600.1.2",
          "form": "너무 피곤해서 배달",
          "original_form": "너무 피곤해서 배달",
          "speaker_id": "2",
          "time": "20210906 23:18"
        },
        {
          "id": "MDRW2100042600.1.3",
          "form": "잘했네 피곤할때는 쉬어야지",
          "original_form": "잘했네 피곤할때는 쉬어야지",
          "speaker_id": "1",
          "time": "20210906 23:18"
        },
        {
          "id": "MDRW2100042600.1.4",
          "form": "엉 종종 그래야지",
          "original_form": "엉 종종 그래야지",
          "speaker_id": "2",
          "time": "20210906 23:18"
        },
        {
          "id": "MDRW2100042600.1.5",
          "form": "만드나 배달하나 그돈이 그돈이다",
          "original_form": "만드나 배달하나 그돈이 그돈이다",
          "speaker_id": "1",
          "time": "20210906 23:18"
        },
        {
          "id": "MDRW2100042600.1.6",
          "form": "안전때문에 그런거지",
          "original_form": "안전때문에 그런거지",
          "speaker_id": "2",
          "time": "20210906 23:19"
        },
        {
          "id": "MDRW2100042600.1.7",
          "form": "그건 맞다 뭘 넣어서 조리할지 모르니",
          "original_form": "그건 맞다 뭘 넣어서 조리할지 모르니",
          "speaker_id": "1",
          "time": "20210906 23:19"
        },
        {
          "id": "MDRW2100042600.1.8",
          "form": "응 거기다 씻는거부터가",
          "original_form": "응 거기다 씻는거부터가",
          "speaker_id": "2",
          "time": "20210906 23:19"
        },
        {
          "id": "MDRW2100042600.1.9",
          "form": "그래서 뭐 배달해서 먹었는데",
          "original_form": "그래서 뭐 배달해서 먹었는데",
          "speaker_id": "1",
          "time": "20210906 23:19"
        },
        {
          "id": "MDRW2100042600.1.10",
          "form": "낙곱",
          "original_form": "낙곱",
          "speaker_id": "2",
          "time": "20210906 23:19"
        },
        {
          "id": "MDRW2100042600.1.11",
          "form": "낙지랑 곱창",
          "original_form": "낙지랑 곱창",
          "speaker_id": "1",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100042600.1.12",
          "form": "엉 원래먹던데가 휴무라 새로운곳",
          "original_form": "엉 원래먹던데가 휴무라 새로운곳",
          "speaker_id": "2",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100042600.1.13",
          "form": "맛있었어",
          "original_form": "맛있었어",
          "speaker_id": "1",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100042600.1.14",
          "form": "맛은 괜찮은데 매워서 혼났다",
          "original_form": "맛은 괜찮은데 매워서 혼났다",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100042600.1.15",
          "form": "ㅋㅋ 순한맛으로 일단 먹어보고 매운맛으로 도전하지",
          "original_form": "ㅋㅋ 순한맛으로 일단 먹어보고 매운맛으로 도전하지",
          "speaker_id": "1",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100042600.1.16",
          "form": "그러게 다들 울고 난리였다",
          "original_form": "그러게 다들 울고 난리였다",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100042600.1.17",
          "form": "그정도로 매웠구나",
          "original_form": "그정도로 매웠구나",
          "speaker_id": "1",
          "time": "20210906 23:21"
        }
      ]
