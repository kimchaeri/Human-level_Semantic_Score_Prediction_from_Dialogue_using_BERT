      "utterance": [
        {
          "id": "MDRW2100044832.1.1",
          "form": "오 무슨 전 좋아해?",
          "original_form": "오 무슨 전 좋아해?",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044832.1.2",
          "form": "김치전이 먹고싶다 오늘은",
          "original_form": "김치전이 먹고싶다 오늘은",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044832.1.3",
          "form": "김치전 가생이 좀 바삭하게 구워서 먹고싶어",
          "original_form": "김치전 가생이 좀 바삭하게 구워서 먹고싶어",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044832.1.4",
          "form": "아 나도..김치전은 가생이가 맛있어",
          "original_form": "아 나도..김치전은 가생이가 맛있어",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044832.1.5",
          "form": "사실 무슨 전이든 가생이가 맛있지...",
          "original_form": "사실 무슨 전이든 가생이가 맛있지...",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044832.1.6",
          "form": "나 예전에 경주가서 먹은 유명한 파전은 질척했어..",
          "original_form": "나 예전에 경주가서 먹은 유명한 파전은 질척했어..",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044832.1.7",
          "form": "아 전이 질척하면 안되지....",
          "original_form": "아 전이 질척하면 안되지....",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044832.1.8",
          "form": "응 그니까 거의 삼만원돈이었는데..",
          "original_form": "응 그니까 거의 삼만원돈이었는데..",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044832.1.9",
          "form": "헐 비싸!",
          "original_form": "헐 비싸!",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044832.1.10",
          "form": "거기가 관광지에 좀 유명한데라 그랬나봐 ㅋㅋ 두껍고 질척했어 노맛.. 전은 역시 바삭해야",
          "original_form": "거기가 관광지에 좀 유명한데라 그랬나봐 ㅋㅋ 두껍고 질척했어 노맛.. 전은 역시 바삭해야",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044832.1.11",
          "form": "맞아맞아 전은 바삭해야돼",
          "original_form": "맞아맞아 전은 바삭해야돼",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044832.1.12",
          "form": "맞아 근데 집에서 구우면 그 바삭함이 잘 안됨",
          "original_form": "맞아 근데 집에서 구우면 그 바삭함이 잘 안됨",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044832.1.13",
          "form": "약간 도넛모양으로 만들어서 구우면 바삭한 가생이가 두배~!",
          "original_form": "약간 도넛모양으로 만들어서 구우면 바삭한 가생이가 두배~!",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044832.1.14",
          "form": "그거도 그렇고 백종원 레시피도 따라해봤는데 왜 안 바삭하지..",
          "original_form": "그거도 그렇고 백종원 레시피도 따라해봤는데 왜 안 바삭하지..",
          "speaker_id": "1",
          "time": "20210907 23:59"
        }
      ]
