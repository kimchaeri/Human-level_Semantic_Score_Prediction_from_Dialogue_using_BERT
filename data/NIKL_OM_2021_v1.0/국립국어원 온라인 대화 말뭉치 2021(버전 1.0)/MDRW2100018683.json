      "utterance": [
        {
          "id": "MDRW2100018683.1.1",
          "form": "손더게가 무당 그런거 맞죠?",
          "original_form": "손더게가 무당 그런거 맞죠?",
          "speaker_id": "2",
          "time": "20210827 10:38"
        },
        {
          "id": "MDRW2100018683.1.2",
          "form": "근데 잔인하긴해요 장면자체가 좀 충격적인것들이..",
          "original_form": "근데 잔인하긴해요 장면자체가 좀 충격적인것들이..",
          "speaker_id": "1",
          "time": "20210827 10:38"
        },
        {
          "id": "MDRW2100018683.1.3",
          "form": "귀신 무당 뭐 다나와요 ㅋㅌㅋ 악귀 뭐이런느낌ㅋㅋㅋㅋ",
          "original_form": "귀신 무당 뭐 다나와요 ㅋㅌㅋ 악귀 뭐이런느낌ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100018683.1.4",
          "form": "오오.... 참고하겠습니다ㅋㅋㅋ찜해놔야겠어요",
          "original_form": "오오.... 참고하겠습니다ㅋㅋㅋ찜해놔야겠어요",
          "speaker_id": "2",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100018683.1.5",
          "form": "영화화 추진되다가 엎어졌잖아요 ㅜㅜ 진짜 이거 이런 장면들만 극복할수있으면 재밌어요 진짜",
          "original_form": "영화화 추진되다가 엎어졌잖아요 ㅜㅜ 진짜 이거 이런 장면들만 극복할수있으면 재밌어요 진짜",
          "speaker_id": "1",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100018683.1.6",
          "form": "이건안봤겠지 하는 드라마 있으세요? ㅋㅋㅋ",
          "original_form": "이건안봤겠지 하는 드라마 있으세요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100018683.1.7",
          "form": "정말요?? 방법인가 그거는 영화도 나오던데 이번에!",
          "original_form": "정말요?? 방법인가 그거는 영화도 나오던데 이번에!",
          "speaker_id": "2",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100018683.1.8",
          "form": "안보긴했는데",
          "original_form": "안보긴했는데",
          "speaker_id": "2",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100018683.1.9",
          "form": "방법 영화나온데요? 헐 ㅋㅋ 그거 보다가 나중에 몰아봐야지 하고 아직 안본 드라마에요 ㅋㅋ",
          "original_form": "방법 영화나온데요? 헐 ㅋㅋ 그거 보다가 나중에 몰아봐야지 하고 아직 안본 드라마에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100018683.1.10",
          "form": "나왔어요! 드라마 스핀오프같은거래요ㅋㅋㅋ저도 몰아보려고 남겨둔 드라마",
          "original_form": "나왔어요! 드라마 스핀오프같은거래요ㅋㅋㅋ저도 몰아보려고 남겨둔 드라마",
          "speaker_id": "2",
          "time": "20210827 10:41"
        },
        {
          "id": "MDRW2100018683.1.11",
          "form": "아...나만본 드라마가 뭐있을까...",
          "original_form": "아...나만본 드라마가 뭐있을까...",
          "speaker_id": "2",
          "time": "20210827 10:41"
        },
        {
          "id": "MDRW2100018683.1.12",
          "form": "퍼퓸보셨어요?",
          "original_form": "퍼퓸보셨어요?",
          "speaker_id": "1",
          "time": "20210827 10:41"
        },
        {
          "id": "MDRW2100018683.1.13",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ당연하죠....ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ당연하죠....ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:41"
        },
        {
          "id": "MDRW2100018683.1.14",
          "form": "신성록나오는거..ㅋㅋㅋㅋ",
          "original_form": "신성록나오는거..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:42"
        },
        {
          "id": "MDRW2100018683.1.15",
          "form": "헐 ㅋㅋㅋㅋ 많이보시는구나!",
          "original_form": "헐 ㅋㅋㅋㅋ 많이보시는구나!",
          "speaker_id": "1",
          "time": "20210827 10:42"
        },
        {
          "id": "MDRW2100018683.1.16",
          "form": "뭔가 갑자기부끄럽네옄ㅋㅋㅋㅋㅋ",
          "original_form": "뭔가 갑자기부끄럽네옄ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:42"
        },
        {
          "id": "MDRW2100018683.1.17",
          "form": "신성록좋아요 ㅋㅋㅋ",
          "original_form": "신성록좋아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:42"
        },
        {
          "id": "MDRW2100018683.1.18",
          "form": "황품은 당연히 보셨겠죠?",
          "original_form": "황품은 당연히 보셨겠죠?",
          "speaker_id": "1",
          "time": "20210827 10:42"
        },
        {
          "id": "MDRW2100018683.1.19",
          "form": "방송국 pd보다 많이볼듯",
          "original_form": "방송국 pd보다 많이볼듯",
          "speaker_id": "2",
          "time": "20210827 10:42"
        },
        {
          "id": "MDRW2100018683.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅌ 아 너무 웃겨요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅌ 아 너무 웃겨요",
          "speaker_id": "1",
          "time": "20210827 10:42"
        },
        {
          "id": "MDRW2100018683.1.21",
          "form": "저도진짜많이봐요 ㅋㅋㅋ",
          "original_form": "저도진짜많이봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018683.1.22",
          "form": "당연하져! 황품의 연장선이",
          "original_form": "당연하져! 황품의 연장선이",
          "speaker_id": "2",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018683.1.23",
          "form": "펜트하우스 잖아요ㅋㅋㅋㅋㅋ",
          "original_form": "펜트하우스 잖아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018683.1.24",
          "form": "욕하면서 보는ㅜ",
          "original_form": "욕하면서 보는ㅜ",
