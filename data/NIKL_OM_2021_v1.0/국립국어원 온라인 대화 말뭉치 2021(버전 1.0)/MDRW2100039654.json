      "utterance": [
        {
          "id": "MDRW2100039654.1.1",
          "form": "내일은 날씨 어떨까요ㅎ",
          "original_form": "내일은 날씨 어떨까요ㅎ",
          "speaker_id": "2",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100039654.1.2",
          "form": "아 오늘이군요ㅜ",
          "original_form": "아 오늘이군요ㅜ",
          "speaker_id": "2",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100039654.1.3",
          "form": "오늘 날씨좋아보여요!",
          "original_form": "오늘 날씨좋아보여요!",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100039654.1.4",
          "form": "ㅎㅎㅎ완전 좋고 오히려 덥네요ㅋ",
          "original_form": "ㅎㅎㅎ완전 좋고 오히려 덥네요ㅋ",
          "speaker_id": "2",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100039654.1.5",
          "form": "아이고 아직도 가을보단 여름인가요?!",
          "original_form": "아이고 아직도 가을보단 여름인가요?!",
          "speaker_id": "1",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100039654.1.6",
          "form": "ㅎㅎㅎ아직 여름안지나간것 같아요ㅜ",
          "original_form": "ㅎㅎㅎ아직 여름안지나간것 같아요ㅜ",
          "speaker_id": "2",
          "time": "20210904 14:39"
        },
        {
          "id": "MDRW2100039654.1.7",
          "form": "아직 반팔의계절이군요ㅋㅋ",
          "original_form": "아직 반팔의계절이군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:41"
        },
        {
          "id": "MDRW2100039654.1.8",
          "form": "ㅎㅎㅎ반팔안입으셨나요?",
          "original_form": "ㅎㅎㅎ반팔안입으셨나요?",
          "speaker_id": "2",
          "time": "20210904 14:43"
        },
        {
          "id": "MDRW2100039654.1.9",
          "form": "저는 지금일어나서",
          "original_form": "저는 지금일어나서",
          "speaker_id": "1",
          "time": "20210904 14:48"
        },
        {
          "id": "MDRW2100039654.1.10",
          "form": "침대에누워있어요",
          "original_form": "침대에누워있어요",
          "speaker_id": "1",
          "time": "20210904 14:48"
        },
        {
          "id": "MDRW2100039654.1.11",
          "form": "오 뒹굴뒹굴 좋죠ㅋ",
          "original_form": "오 뒹굴뒹굴 좋죠ㅋ",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100039654.1.12",
          "form": "이런날씨엔 안나는게ㅠ",
          "original_form": "이런날씨엔 안나는게ㅠ",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100039654.1.13",
          "form": "ㅋㅋㄱㅋㄱㄱㅋㅋㄱ 나가볼까 고민중이에요,,",
          "original_form": "ㅋㅋㄱㅋㄱㄱㅋㅋㄱ 나가볼까 고민중이에요,,",
          "speaker_id": "1",
          "time": "20210904 16:16"
        },
        {
          "id": "MDRW2100039654.1.14",
          "form": "이제 좀 나갈만하겠어요ㅋ 더위는 좀 가셔서요ㅋ",
          "original_form": "이제 좀 나갈만하겠어요ㅋ 더위는 좀 가셔서요ㅋ",
          "speaker_id": "2",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100039654.1.15",
          "form": "밖에 너무시원하고좋네요!!!",
          "original_form": "밖에 너무시원하고좋네요!!!",
          "speaker_id": "1",
          "time": "20210904 18:58"
        },
        {
          "id": "MDRW2100039654.1.16",
          "form": "다행이네요ㅋ 피부도 타지않고ㅋ",
          "original_form": "다행이네요ㅋ 피부도 타지않고ㅋ",
          "speaker_id": "2",
          "time": "20210904 19:03"
        },
        {
          "id": "MDRW2100039654.1.17",
          "form": "바람 너무시원하네요",
          "original_form": "바람 너무시원하네요",
          "speaker_id": "1",
