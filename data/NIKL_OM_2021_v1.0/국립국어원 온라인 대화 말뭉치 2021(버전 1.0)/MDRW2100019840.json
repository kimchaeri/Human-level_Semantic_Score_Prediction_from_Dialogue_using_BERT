      "utterance": [
        {
          "id": "MDRW2100019840.1.1",
          "form": "킹덤봤어?",
          "original_form": "킹덤봤어?",
          "speaker_id": "1",
          "time": "20210827 22:23"
        },
        {
          "id": "MDRW2100019840.1.2",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019840.1.3",
          "form": "보다 말았음",
          "original_form": "보다 말았음",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019840.1.4",
          "form": "전지현나오는 영화말이야",
          "original_form": "전지현나오는 영화말이야",
          "speaker_id": "1",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019840.1.5",
          "form": "영화가?드라마아니가?넷플에서 하는거",
          "original_form": "영화가?드라마아니가?넷플에서 하는거",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100019840.1.6",
          "form": "드라마도있고... 영화 이번개봉한거 넷플에",
          "original_form": "드라마도있고... 영화 이번개봉한거 넷플에",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100019840.1.7",
          "form": "아 몰랐어",
          "original_form": "아 몰랐어",
          "speaker_id": "2",
          "time": "20210828 05:08"
        },
        {
          "id": "MDRW2100019840.1.8",
          "form": "좀 별로더라 영화킹덤은",
          "original_form": "좀 별로더라 영화킹덤은",
          "speaker_id": "1",
          "time": "20210828 08:49"
        },
        {
          "id": "MDRW2100019840.1.9",
          "form": "난 넷플로 킹덤 10분봤는데 몰입이 안 되더라",
          "original_form": "난 넷플로 킹덤 10분봤는데 몰입이 안 되더라",
          "speaker_id": "2",
          "time": "20210828 09:29"
        },
        {
          "id": "MDRW2100019840.1.10",
          "form": "아...보다봄괜찮아",
          "original_form": "아...보다봄괜찮아",
          "speaker_id": "1",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100019840.1.11",
          "form": "좀비라서 내 스타일 아니라서ㅜ",
          "original_form": "좀비라서 내 스타일 아니라서ㅜ",
          "speaker_id": "2",
          "time": "20210828 15:30"
        },
        {
          "id": "MDRW2100019840.1.12",
          "form": "맞다 내용이좀 그렇더라",
          "original_form": "맞다 내용이좀 그렇더라",
          "speaker_id": "1",
          "time": "20210828 18:14"
        },
        {
          "id": "MDRW2100019840.1.13",
          "form": "요즘 노래 뭐가좋아??",
          "original_form": "요즘 노래 뭐가좋아??",
          "speaker_id": "1",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100019840.1.14",
          "form": "난 요즘 적재 이재정 원슈타인 좋더라",
          "original_form": "난 요즘 적재 이재정 원슈타인 좋더라",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100019840.1.15",
          "form": "이재정이 누구야?? 멜론으로 듣나?",
          "original_form": "이재정이 누구야?? 멜론으로 듣나?",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100019840.1.16",
          "form": "msg워너비중 한명",
          "original_form": "msg워너비중 한명",
          "speaker_id": "2",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100019840.1.17",
          "form": "난 다운받아 들어",
          "original_form": "난 다운받아 들어",
          "speaker_id": "2",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100019840.1.18",
          "form": "박재정이잖아 ㅋㅋㅋ 노래좋나",
          "original_form": "박재정이잖아 ㅋㅋㅋ 노래좋나",
          "speaker_id": "1",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100019840.1.19",
          "form": "박재정은 목소리매력없지않나?",
          "original_form": "박재정은 목소리매력없지않나?",
          "speaker_id": "1",
          "time": "20210829 00:25"
        },
        {
          "id": "MDRW2100019840.1.20",
          "form": "아 박재정~ㅋㅋㅋ아니 김동률같다",
          "original_form": "아 박재정~ㅋㅋㅋ아니 김동률같다",
          "speaker_id": "2",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100019840.1.21",
          "form": "맞나 ㅋㅋ걘..못난건아닌데...너무 아우라가없다",
          "original_form": "맞나 ㅋㅋ걘..못난건아닌데...너무 아우라가없다",
          "speaker_id": "1",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100019840.1.22",
          "form": "근데 노래는 잘하더라 나도 그랬는데 노래는 진심이 있다",
          "original_form": "근데 노래는 잘하더라 나도 그랬는데 노래는 진심이 있다",
          "speaker_id": "2",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100019840.1.23",
          "form": "난 색깔이 없다 생각했네 나도 다운받아 취미들려보까\r\n내가 취미가 없네",
