        {
          "id": "MDRW2100033329.1.2",
          "form": "흥?",
          "original_form": "흥?",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100033329.1.3",
          "form": "하 저 완전 집순쓰~^^",
          "original_form": "하 저 완전 집순쓰~^^",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100033329.1.4",
          "form": "집에서 뭐하고 놀아요?",
          "original_form": "집에서 뭐하고 놀아요?",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033329.1.5",
          "form": "전 게임 합니자",
          "original_form": "전 게임 합니자",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033329.1.6",
          "form": "저두요 ㅎㅎ 스위치? 막이래",
          "original_form": "저두요 ㅎㅎ 스위치? 막이래",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033329.1.7",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033329.1.8",
          "form": "아이나나",
          "original_form": "아이나나",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033329.1.9",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033329.1.10",
          "form": "헐~~ 저 알아요",
          "original_form": "헐~~ 저 알아요",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100033329.1.11",
          "form": "집놀이족 용품으로. 딱이죠",
          "original_form": "집놀이족 용품으로. 딱이죠{emoji:♡}",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100033329.1.12",
          "form": "요즘 유행하길래 하나 사봤는데",
          "original_form": "요즘 유행하길래 하나 사봤는데",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100033329.1.13",
          "form": "보라색이 이쁘더라구요",
          "original_form": "보라색이 이쁘더라구요",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100033329.1.14",
          "form": "저는~~ 핫핑꾸 취향이에요",
          "original_form": "저는~~ 핫핑꾸 취향이에요",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100033329.1.15",
          "form": "보기에두 좋고^^",
          "original_form": "보기에두 좋고^^",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100033329.1.16",
          "form": "그러고 보니",
          "original_form": "그러고 보니",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100033329.1.17",
          "form": "집에서도 헬스하던데",
          "original_form": "집에서도 헬스하던데",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100033329.1.18",
          "form": "아령 추천해주세요",
          "original_form": "아령 추천해주세요",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100033329.1.19",
          "form": "흠...",
          "original_form": "흠...",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100033329.1.20",
          "form": "제가 헬스 오지게 해서",
          "original_form": "제가 헬스 오지게 해서",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100033329.1.21",
          "form": "우유통에 모래 넣어서 쓰세요",
          "original_form": "우유통에 모래 넣어서 쓰세요",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100033329.1.22",
          "form": "모래는 따로 사나요?",
          "original_form": "모래는 따로 사나요?",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100033329.1.23",
          "form": "모래는 대충 운동장에서^^",
          "original_form": "모래는 대충 운동장에서^^",
