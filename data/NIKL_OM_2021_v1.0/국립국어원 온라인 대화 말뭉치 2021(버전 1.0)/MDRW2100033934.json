      "utterance": [
        {
          "id": "MDRW2100033934.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100033934.1.2",
          "form": "청소랑 설거지 가끔해요",
          "original_form": "청소랑 설거지 가끔해요",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100033934.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100033934.1.4",
          "form": "일주일에 몇 번 정도 해요?",
          "original_form": "일주일에 몇 번 정도 해요?",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100033934.1.5",
          "form": "화장실 일주일에 한번",
          "original_form": "화장실 일주일에 한번",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100033934.1.6",
          "form": "청소기는 몇 번 돌려요?",
          "original_form": "청소기는 몇 번 돌려요?",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100033934.1.7",
          "form": "매일",
          "original_form": "매일",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100033934.1.8",
          "form": "그냥 편하게 말하자",
          "original_form": "그냥 편하게 말하자",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100033934.1.9",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100033934.1.10",
          "form": "너는 청소기 언제언제 돌려?",
          "original_form": "너는 청소기 언제언제 돌려?",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100033934.1.11",
          "form": "이틀에 한번!",
          "original_form": "이틀에 한번!",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100033934.1.12",
          "form": "집안일 좋아하는 편이야?",
          "original_form": "집안일 좋아하는 편이야?",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100033934.1.13",
          "form": "그냥 하면 뿌듯해",
          "original_form": "그냥 하면 뿌듯해",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100033934.1.14",
          "form": "근대 가끔 귀찮아",
          "original_form": "근대 가끔 귀찮아",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100033934.1.15",
          "form": "그건 맞지",
          "original_form": "그건 맞지",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100033934.1.16",
          "form": "피곤할 때는 그냥 던져 둬",
          "original_form": "피곤할 때는 그냥 던져 둬",
          "speaker_id": "2",
          "time": "20210825 16:21"
