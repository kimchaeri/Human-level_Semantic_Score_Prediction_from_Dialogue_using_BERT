      "utterance": [
        {
          "id": "MDRW2100031075.1.1",
          "form": "부산오면 내가 사줄게",
          "original_form": "부산오면 내가 사줄게",
          "speaker_id": "2",
          "time": "20210904 16:17"
        },
        {
          "id": "MDRW2100031075.1.2",
          "form": "아이참 고마워",
          "original_form": "아이참 고마워",
          "speaker_id": "1",
          "time": "20210904 16:17"
        },
        {
          "id": "MDRW2100031075.1.3",
          "form": "니 사랑 배부르다",
          "original_form": "니 사랑 배부르다",
          "speaker_id": "1",
          "time": "20210904 16:17"
        },
        {
          "id": "MDRW2100031075.1.4",
          "form": "허하하 맛있는거 먹고 건강한게 최고야",
          "original_form": "허하하 맛있는거 먹고 건강한게 최고야",
          "speaker_id": "2",
          "time": "20210904 16:17"
        },
        {
          "id": "MDRW2100031075.1.5",
          "form": "밀치회는 언제 처음 먹어봤어?",
          "original_form": "밀치회는 언제 처음 먹어봤어?",
          "speaker_id": "1",
          "time": "20210904 16:18"
        },
        {
          "id": "MDRW2100031075.1.6",
          "form": "부산내려와서 1년전에",
          "original_form": "부산내려와서 1년전에",
          "speaker_id": "2",
          "time": "20210904 16:18"
        },
        {
          "id": "MDRW2100031075.1.7",
          "form": "그때 이 빨간회는 뭐지 하고 계속 찾아봤었어",
          "original_form": "그때 이 빨간회는 뭐지 하고 계속 찾아봤었어",
          "speaker_id": "2",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100031075.1.8",
          "form": "헉 먹어본 지 얼마 안 됐네?",
          "original_form": "헉 먹어본 지 얼마 안 됐네?",
          "speaker_id": "1",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100031075.1.9",
          "form": "처음 먹고 반했니",
          "original_form": "처음 먹고 반했니",
          "speaker_id": "1",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100031075.1.10",
          "form": "응 완전 평소에도 초밥좋아하는데",
          "original_form": "응 완전 평소에도 초밥좋아하는데",
          "speaker_id": "2",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100031075.1.11",
          "form": "회로 좋아한건 이게 처음이야",
          "original_form": "회로 좋아한건 이게 처음이야",
          "speaker_id": "2",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100031075.1.12",
          "form": "헉 정말 맛있나보구나",
          "original_form": "헉 정말 맛있나보구나",
          "speaker_id": "1",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100031075.1.13",
          "form": "넌 회에 뭐 찍어먹어?",
          "original_form": "넌 회에 뭐 찍어먹어?",
          "speaker_id": "1",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100031075.1.14",
          "form": "연어 초밥이 너무 맛있었는데",
          "original_form": "연어 초밥이 너무 맛있었는데",
          "speaker_id": "2",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100031075.1.15",
          "form": "난 회에 와사비랑 간장찍어",
          "original_form": "난 회에 와사비랑 간장찍어",
          "speaker_id": "2",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100031075.1.16",
          "form": "그렇구나 약간 일식같은 느낌일 것 같아",
          "original_form": "그렇구나 약간 일식같은 느낌일 것 같아",
          "speaker_id": "1",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100031075.1.17",
          "form": "응 초밥 맛있었는데?",
          "original_form": "응 초밥 맛있었는데?",
          "speaker_id": "1",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100031075.1.18",
          "form": "응 맛있어 옛날에는 초장에 찍었는데",
          "original_form": "응 맛있어 옛날에는 초장에 찍었는데",
          "speaker_id": "2",
          "time": "20210904 16:22"
        },
        {
          "id": "MDRW2100031075.1.19",
          "form": "초밥이 너무좋아 비싸지만 않으면 자주먹겠는디",
          "original_form": "초밥이 너무좋아 비싸지만 않으면 자주먹겠는디",
          "speaker_id": "2",
          "time": "20210904 16:22"
        },
        {
          "id": "MDRW2100031075.1.20",
          "form": "난 지금 초장 찍어먹는데 다음에는 간장도 찍어먹어봐야겠어",
          "original_form": "난 지금 초장 찍어먹는데 다음에는 간장도 찍어먹어봐야겠어",
          "speaker_id": "1",
          "time": "20210904 16:22"
        },
        {
          "id": "MDRW2100031075.1.21",
          "form": "맞아 초밥 비싸 흑흑",
          "original_form": "맞아 초밥 비싸 흑흑",
          "speaker_id": "1",
          "time": "20210904 16:22"
        },
        {
          "id": "MDRW2100031075.1.22",
          "form": "초밥집 맘놓고 먹으면 기본 5만원이여",
          "original_form": "초밥집 맘놓고 먹으면 기본 5만원이여",
          "speaker_id": "2",
          "time": "20210904 16:23"
        }
