      "utterance": [
        {
          "id": "MDRW2100015659.1.1",
          "form": "어때좀 도움이됐음?",
          "original_form": "어때좀 도움이됐음?",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015659.1.2",
          "form": "내용은 유익했음",
          "original_form": "내용은 유익했음",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015659.1.3",
          "form": "근데 크나큰 단점이 있어요ㅜ",
          "original_form": "근데 크나큰 단점이 있어요ㅜ",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015659.1.4",
          "form": "뭇데",
          "original_form": "뭇데",
          "speaker_id": "1",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015659.1.5",
          "form": "구슬이 서말이라도 꿰어야 보배다 란 우리 조상님들의 현명한 명언을 알고 계십니까",
          "original_form": "구슬이 서말이라도 꿰어야 보배다 란 우리 조상님들의 현명한 명언을 알고 계십니까",
          "speaker_id": "2",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015659.1.6",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015659.1.7",
          "form": "ㅋㅋㅋ 알긴아는디ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 알긴아는디ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015659.1.8",
          "form": "정말 좋은 내용이 많았는데",
          "original_form": "정말 좋은 내용이 많았는데",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015659.1.9",
          "form": "그걸 꿰게 해주셔야죠 책님!!",
          "original_form": "그걸 꿰게 해주셔야죠 책님!!",
          "speaker_id": "1",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015659.1.10",
          "form": "실천을 아니함",
          "original_form": "실천을 아니함",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015659.1.11",
          "form": "이미 첫 단계부터 실천하지 못하고있음ㅜ",
          "original_form": "이미 첫 단계부터 실천하지 못하고있음ㅜ",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015659.1.12",
          "form": "ㅋ머얏ㅋㅋㅋㅋ",
          "original_form": "ㅋ머얏ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015659.1.13",
          "form": "소용이 없자넛ㅋㅋㅋ",
          "original_form": "소용이 없자넛ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015659.1.14",
          "form": "name1님 그 첫 단계가 뭐일거 같음요?",
          "original_form": "&name1&님 그 첫 단계가 뭐일거 같음요?",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015659.1.15",
          "form": "머지",
          "original_form": "머지",
          "speaker_id": "1",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015659.1.16",
          "form": "궁금함",
          "original_form": "궁금함",
          "speaker_id": "1",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015659.1.17",
          "form": "핸드폰 보지않기 이불속에서",
          "original_form": "핸드폰 보지않기 이불속에서",
          "speaker_id": "2",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015659.1.18",
          "form": "여하간 침대나 이불에서는 딱 잠만 잘것",
          "original_form": "여하간 침대나 이불에서는 딱 잠만 잘것",
          "speaker_id": "2",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015659.1.19",
          "form": "폰보는거 금지 ㅋㅋ잠들기 전까지",
          "original_form": "폰보는거 금지 ㅋㅋ잠들기 전까지",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015659.1.20",
          "form": "노우..",
          "original_form": "노우..",
          "speaker_id": "1",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015659.1.21",
          "form": "ㅜㅜㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015659.1.22",
          "form": "이걸 못해서 ㅋㅋ",
          "original_form": "이걸 못해서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015659.1.23",
          "form": "결국 암것도 안됨 ㅋㅋㅋ",
          "original_form": "결국 암것도 안됨 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015659.1.24",
          "form": "저것부터 해야하는데",
          "original_form": "저것부터 해야하는데",
          "speaker_id": "2",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015659.1.25",
          "form": "그 그럼 두번째는?ㅜ",
          "original_form": "그 그럼 두번째는?ㅜ",
          "speaker_id": "1",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015659.1.26",
          "form": "저게 너무 힘들어!",
          "original_form": "저게 너무 힘들어!",
          "speaker_id": "2",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015659.1.27",
          "form": "나 어제 도서관에서 책빌림",
          "original_form": "나 어제 도서관에서 책빌림",
