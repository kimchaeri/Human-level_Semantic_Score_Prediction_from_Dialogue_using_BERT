      "utterance": [
        {
          "id": "MDRW2100032200.1.1",
          "form": "순대내장 너무 좋아",
          "original_form": "순대내장 너무 좋아",
          "speaker_id": "2",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032200.1.2",
          "form": "저번에보다 천원치 더 샀는데\r\n왜 저번보다 먹어보니 작은 느낌ㅎ",
          "original_form": "저번에보다 천원치 더 샀는데\r\n왜 저번보다 먹어보니 작은 느낌ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032200.1.3",
          "form": "ㅋㅋ 난 순대만 좋아 ㅋㅋ",
          "original_form": "ㅋㅋ 난 순대만 좋아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032200.1.4",
          "form": "지금 다시 먹을려고 데우는중",
          "original_form": "지금 다시 먹을려고 데우는중",
          "speaker_id": "2",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032200.1.5",
          "form": "양 줄인거 아녀?",
          "original_form": "양 줄인거 아녀?",
          "speaker_id": "1",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100032200.1.6",
          "form": "난 순대없이 내장만도 좋아ㅋ",
          "original_form": "난 순대없이 내장만도 좋아ㅋ",
          "speaker_id": "2",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100032200.1.7",
          "form": "잘팔리는집 갈수록 양 줄이는집있더라",
          "original_form": "잘팔리는집 갈수록 양 줄이는집있더라",
          "speaker_id": "1",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100032200.1.8",
          "form": "어떻게해도 잘 팔려서 그런건지",
          "original_form": "어떻게해도 잘 팔려서 그런건지",
          "speaker_id": "1",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100032200.1.9",
          "form": "맘스터치 싸이버거만 먹고싶은데 배달시키면 많이시켜야돼ㅜㅜ",
          "original_form": "맘스터치 싸이버거만 먹고싶은데 배달시키면 많이시켜야돼ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 15:50"
        },
        {
          "id": "MDRW2100032200.1.10",
          "form": "ㅜㅜ부모님꺼랑 주문은?\r\n그럼 많으니까ㅎ",
          "original_form": "ㅜㅜ부모님꺼랑 주문은?\r\n그럼 많으니까ㅎ",
          "speaker_id": "2",
          "time": "20210905 18:23"
        },
        {
          "id": "MDRW2100032200.1.11",
          "form": "부모님 없어 ㅋㅋ",
          "original_form": "부모님 없어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:24"
        },
        {
          "id": "MDRW2100032200.1.12",
          "form": "나 혼자 시켯엉 나머지 냉장고 넣어두려고",
          "original_form": "나 혼자 시켯엉 나머지 냉장고 넣어두려고",
          "speaker_id": "1",
          "time": "20210905 18:25"
        },
        {
          "id": "MDRW2100032200.1.13",
          "form": "좋은데ㅋ",
          "original_form": "좋은데ㅋ",
          "speaker_id": "2",
          "time": "20210905 18:40"
        },
        {
          "id": "MDRW2100032200.1.14",
          "form": "난 어께랑 목이 계속아프서 자다깨다자다하는중\r\n핑계일지돜ㅋ",
          "original_form": "난 어께랑 목이 계속아프서 자다깨다자다하는중\r\n핑계일지돜ㅋ",
          "speaker_id": "2",
          "time": "20210905 18:41"
        },
        {
          "id": "MDRW2100032200.1.15",
          "form": "글서 오늘 밥은 떡볶이 순대였어ㅋ",
          "original_form": "글서 오늘 밥은 떡볶이 순대였어ㅋ",
          "speaker_id": "2",
          "time": "20210905 18:41"
        },
        {
          "id": "MDRW2100032200.1.16",
          "form": "ㅋㅋ 떡볶이 어제 사다놓길 잘했네",
          "original_form": "ㅋㅋ 떡볶이 어제 사다놓길 잘했네",
          "speaker_id": "1",
          "time": "20210905 18:59"
        },
        {
          "id": "MDRW2100032200.1.17",
          "form": "응ㅎ없음 식당 갔을텐데ㅋㅋ",
          "original_form": "응ㅎ없음 식당 갔을텐데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032200.1.18",
          "form": "ㅇㅇ 떡볶이 하나 사서 세끼해결 ㅋㅋ",
          "original_form": "ㅇㅇ 떡볶이 하나 사서 세끼해결 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032200.1.19",
          "form": "맞어ㅋㅋ빵이랑 요거트랑 복숭이랑 잘먹었어ㅋ",
          "original_form": "맞어ㅋㅋ빵이랑 요거트랑 복숭이랑 잘먹었어ㅋ",
          "speaker_id": "2",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100032200.1.20",
          "form": "난 점심으로 장각삼계탕 먹었는데\r\n반찬으로 봄동무침이 새콤하니 맛있더라",
          "original_form": "난 점심으로 장각삼계탕 먹었는데\r\n반찬으로 봄동무침이 새콤하니 맛있더라",
          "speaker_id": "2",
          "time": "20210906 12:49"
        }
      ]
