      "utterance": [
        {
          "id": "MDRW2100024976.1.1",
          "form": "전 완전 맵찔이라",
          "original_form": "전 완전 맵찔이라",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024976.1.2",
          "form": "근데 매운거 안먹는데 몸에는 백만배좋은것같아요ㅋㅋ",
          "original_form": "근데 매운거 안먹는데 몸에는 백만배좋은것같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024976.1.3",
          "form": "사실 매운 거 먹을 순 있는데 땀이 많이 나요 ",
          "original_form": "사실 매운 거 먹을 순 있는데 땀이 많이 나요 ",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024976.1.4",
          "form": "음 그렇구나 매운거 못먹는데 왜먹는진  알겠어요 진짜",
          "original_form": "음 그렇구나 매운거 못먹는데 왜먹는진  알겠어요 진짜",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100024976.1.5",
          "form": "먹으면 아무 생각 안나고 스트레스 풀리더라구요",
          "original_form": "먹으면 아무 생각 안나고 스트레스 풀리더라구요",
          "speaker_id": "2",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100024976.1.6",
          "form": "진짜요ㅋㅋ",
          "original_form": "진짜요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100024976.1.7",
          "form": "몸에 안좋은거 알면서도 찾더라고요",
          "original_form": "몸에 안좋은거 알면서도 찾더라고요",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100024976.1.8",
          "form": "그죠 전지금 닭발에 주먹밥 먹고 싶네요",
          "original_form": "그죠 전지금 닭발에 주먹밥 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100024976.1.9",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100024976.1.10",
          "form": "제껀요?",
          "original_form": "제껀요?",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100024976.1.11",
          "form": "저도 못먹고 있는데여ㅋㅋㅋㅋ",
          "original_form": "저도 못먹고 있는데여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100024976.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100024976.1.13",
          "form": "먹고계시다는줄 알았어요",
          "original_form": "먹고계시다는줄 알았어요",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100024976.1.14",
          "form": "ㅋㅋㅋㅋㅋ 정말 제가 보고싶은대로 봤네요",
          "original_form": "ㅋㅋㅋㅋㅋ 정말 제가 보고싶은대로 봤네요",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100024976.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ서울 왕십리에 엽기꼼닭발이라고 있는데 꼭 먹어보세요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ서울 왕십리에 엽기꼼닭발이라고 있는데 꼭 먹어보세요",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100024976.1.16",
          "form": "ㅈㅁㅌ 진짜",
          "original_form": "ㅈㅁㅌ 진짜",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100024976.1.17",
          "form": "ㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100024976.1.18",
          "form": "진심이 느껴져요",
          "original_form": "진심이 느껴져요",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100024976.1.19",
          "form": "치즈추가하면 극락갑니다",
          "original_form": "치즈추가하면 극락갑니다",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100024976.1.20",
          "form": "배고파요",
          "original_form": "배고파요",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100024976.1.21",
          "form": "내일 먹을 계획을 세워봐요 우리",
          "original_form": "내일 먹을 계획을 세워봐요 우리",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100024976.1.22",
          "form": "저는 내일 콩국수 먹을거에요 올여름 마지막 콩국수",
          "original_form": "저는 내일 콩국수 먹을거에요 올여름 마지막 콩국수",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100024976.1.23",
          "form": "콩국수 지난번에도 드시지않았나요???ㅋㅋㅋㅋ",
          "original_form": "콩국수 지난번에도 드시지않았나요???ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100024976.1.24",
          "form": "아그렇네요 그치만 또먹을겁니다",
          "original_form": "아그렇네요 그치만 또먹을겁니다",
