      "utterance": [
        {
          "id": "MDRW2100038160.1.1",
          "form": "젤 좋아하는 음식 있으십니까 ㅎㅎ",
          "original_form": "젤 좋아하는 음식 있으십니까 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100038160.1.2",
          "form": "하... 젤 좋아하는 음식이라...",
          "original_form": "하... 젤 좋아하는 음식이라...",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100038160.1.3",
          "form": "그때그때 바뀌긴 하는데요, 음...",
          "original_form": "그때그때 바뀌긴 하는데요, 음...",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100038160.1.4",
          "form": "지금은 닭도리탕이 생각나네요 ㅎㅎ",
          "original_form": "지금은 닭도리탕이 생각나네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100038160.1.5",
          "form": "으앗...저도 참 좋아합니다 ㅜㅜ",
          "original_form": "으앗...저도 참 좋아합니다 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100038160.1.6",
          "form": "감자으깨서 국물에 밥 비벼서 먹고싶군요..",
          "original_form": "감자으깨서 국물에 밥 비벼서 먹고싶군요..",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100038160.1.7",
          "form": "캬~ 감자 존맛이죠",
          "original_form": "캬~ 감자 존맛이죠",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100038160.1.8",
          "form": "전 닭중에선 다리날개파인데",
          "original_form": "전 닭중에선 다리날개파인데",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100038160.1.9",
          "form": "님은 어떠세요?",
          "original_form": "님은 어떠세요?",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100038160.1.10",
          "form": "헐 저는 가 슴 살 이요 !",
          "original_form": "헐 저는 가 슴 살 이요 !",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100038160.1.11",
          "form": "저랑 나눠드시죠 ㅎㅎㅎ",
          "original_form": "저랑 나눠드시죠 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100038160.1.12",
          "form": "앗! 소울메이트! ㅋㅋㅋㅋ",
          "original_form": "앗! 소울메이트! ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100038160.1.13",
          "form": "우리집은 죄다 다리파라 맨날 싸워요 ㅠ",
          "original_form": "우리집은 죄다 다리파라 맨날 싸워요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100038160.1.14",
          "form": "어렸을땐 부모님이 양보해주셨는데",
          "original_form": "어렸을땐 부모님이 양보해주셨는데",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100038160.1.15",
          "form": "이젠 제가 효도함 ㅎ",
          "original_form": "이젠 제가 효도함 ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100038160.1.16",
          "form": "헐 저희집은 엄마만 다리드셔서",
          "original_form": "헐 저희집은 엄마만 다리드셔서",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100038160.1.17",
          "form": "엄마가 항상 다 드셔요 ㅋㅋㅋ",
          "original_form": "엄마가 항상 다 드셔요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100038160.1.18",
          "form": "저희집은 가 슴 살 싸움이랍니다... 흑흑",
          "original_form": "저희집은 가 슴 살 싸움이랍니다... 흑흑",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100038160.1.19",
          "form": "저런...",
          "original_form": "저런...",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100038160.1.20",
          "form": "닭가슴살 팩을 따로 사서 추가해보는건 어떨가요?",
          "original_form": "닭가슴살 팩을 따로 사서 추가해보는건 어떨가요?",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100038160.1.21",
          "form": "저희집은 다리추가 해서 먹어본적 있는데",
          "original_form": "저희집은 다리추가 해서 먹어본적 있는데",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100038160.1.22",
          "form": "오 한번 건의해봐야겠어요",
          "original_form": "오 한번 건의해봐야겠어요",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100038160.1.23",
          "form": "엄마가 선택하시기때믠에 ㅋㅋㅋ",
          "original_form": "엄마가 선택하시기때믠에 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100038160.1.24",
          "form": "해보세요! 많이 추가하면 맛의 밸런스가 무너질순 있는데",
          "original_form": "해보세요! 많이 추가하면 맛의 밸런스가 무너질순 있는데",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100038160.1.25",
          "form": "서너개정돈 괜찮을걸요?",
          "original_form": "서너개정돈 괜찮을걸요?",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100038160.1.26",
          "form": "완전 꿀팁입니다 !",
          "original_form": "완전 꿀팁입니다 !",
          "speaker_id": "1",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100038160.1.27",
          "form": "왜 다리추가는 알면서 그생각을 못했을까욬ㅋㅋ",
          "original_form": "왜 다리추가는 알면서 그생각을 못했을까욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100038160.1.28",
          "form": "ㅋㅋㅋㅋ 생각의 전환? ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 생각의 전환? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100038160.1.29",
          "form": "평화로운 식탁 되세요  ㅋㅋㅋㅋ",
          "original_form": "평화로운 식탁 되세요  ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100038160.1.30",
          "form": "평화롭게 해주셔서 감사합니다 ㅋㅋㅋ !!!",
          "original_form": "평화롭게 해주셔서 감사합니다 ㅋㅋㅋ !!!",
          "speaker_id": "1",
          "time": "20210902 00:26"
        }
      ]
    }
