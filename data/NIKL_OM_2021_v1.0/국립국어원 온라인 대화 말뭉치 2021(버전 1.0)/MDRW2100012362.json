      "utterance": [
        {
          "id": "MDRW2100012362.1.1",
          "form": "뮤지컬이나 연극 가끔 보세요?",
          "original_form": "뮤지컬이나 연극 가끔 보세요?",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012362.1.2",
          "form": "뮤지컬 보고싶어요!",
          "original_form": "뮤지컬 보고싶어요!",
          "speaker_id": "1",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100012362.1.3",
          "form": "재밌게 보셨던 작품 있으신가요?",
          "original_form": "재밌게 보셨던 작품 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 21:38"
        },
        {
          "id": "MDRW2100012362.1.4",
          "form": "한번도 못봤어요ㅠ",
          "original_form": "한번도 못봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012362.1.5",
          "form": "그럼 보고 싶은 공연 있으신가요?",
          "original_form": "그럼 보고 싶은 공연 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012362.1.6",
          "form": "추천뮤지컬 있을까요",
          "original_form": "추천뮤지컬 있을까요",
          "speaker_id": "1",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100012362.1.7",
          "form": "처음이시라면 화려하고 신나는 뮤지컬이 좋을 것 같아요",
          "original_form": "처음이시라면 화려하고 신나는 뮤지컬이 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100012362.1.8",
          "form": "제가 봤던 것 중에는 킨키부츠 재밌었어요.",
          "original_form": "제가 봤던 것 중에는 킨키부츠 재밌었어요.",
          "speaker_id": "2",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100012362.1.9",
          "form": "오 처음들어보는데 유명한가요",
          "original_form": "오 처음들어보는데 유명한가요",
          "speaker_id": "1",
          "time": "20210824 22:18"
        },
        {
          "id": "MDRW2100012362.1.10",
          "form": "나름 유명한 편일거에요ㅋㅋ",
          "original_form": "나름 유명한 편일거에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:18"
        },
        {
          "id": "MDRW2100012362.1.11",
          "form": "한번 검색해봐야겠어요",
          "original_form": "한번 검색해봐야겠어요",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012362.1.12",
          "form": "정확히는 킹키부츠네요 ㅋㅋ",
          "original_form": "정확히는 킹키부츠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012362.1.13",
          "form": "뮤지컬 그날들도 재밌었어요",
          "original_form": "뮤지컬 그날들도 재밌었어요",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012362.1.14",
          "form": "오 그날들 많이 들어봤어요",
          "original_form": "오 그날들 많이 들어봤어요",
