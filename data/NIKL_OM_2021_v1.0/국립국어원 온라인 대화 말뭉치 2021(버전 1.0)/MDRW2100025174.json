      "utterance": [
        {
          "id": "MDRW2100025174.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 00:09"
        },
        {
          "id": "MDRW2100025174.1.2",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100025174.1.3",
          "form": "저 게임 좋아해요!",
          "original_form": "저 게임 좋아해요!",
          "speaker_id": "2",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100025174.1.4",
          "form": "헉 어떤게임 하세요?",
          "original_form": "헉 어떤게임 하세요?",
          "speaker_id": "1",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100025174.1.5",
          "form": "컴퓨터? 폰?",
          "original_form": "컴퓨터? 폰?",
          "speaker_id": "1",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100025174.1.6",
          "form": "저는 컴퓨터 게임 좋아해요",
          "original_form": "저는 컴퓨터 게임 좋아해요",
          "speaker_id": "2",
          "time": "20210901 00:14"
        },
        {
          "id": "MDRW2100025174.1.7",
          "form": "어떤거해보셨나요?",
          "original_form": "어떤거해보셨나요?",
          "speaker_id": "1",
          "time": "20210901 00:15"
        },
        {
          "id": "MDRW2100025174.1.8",
          "form": "저도 좋아해요!",
          "original_form": "저도 좋아해요!",
          "speaker_id": "1",
          "time": "20210901 00:15"
        },
        {
          "id": "MDRW2100025174.1.9",
          "form": "저는 오버워치랑 혹시 스팀게임 데바데라고 아세요?",
          "original_form": "저는 오버워치랑 혹시 스팀게임 데바데라고 아세요?",
          "speaker_id": "2",
          "time": "20210901 00:15"
        },
        {
          "id": "MDRW2100025174.1.10",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210901 00:16"
        },
        {
          "id": "MDRW2100025174.1.11",
          "form": "저 데바데해요!!!!ㅠㅠ!!",
          "original_form": "저 데바데해요!!!!ㅠㅠ!!",
          "speaker_id": "1",
          "time": "20210901 00:16"
        },
        {
          "id": "MDRW2100025174.1.12",
          "form": "갑자기 동질감 느끼기",
          "original_form": "갑자기 동질감 느끼기",
          "speaker_id": "1",
          "time": "20210901 00:16"
        },
        {
          "id": "MDRW2100025174.1.13",
          "form": "옵치는 하다가 친구들이 다접어서 저도 같이 접었어욯ㅋㅋㄱㅠ",
          "original_form": "옵치는 하다가 친구들이 다접어서 저도 같이 접었어욯ㅋㅋㄱㅠ",
          "speaker_id": "1",
          "time": "20210901 00:16"
        },
        {
          "id": "MDRW2100025174.1.14",
          "form": "헐...데바데 하는 사람이 있다니!",
          "original_form": "헐...데바데 하는 사람이 있다니!",
          "speaker_id": "2",
          "time": "20210901 00:17"
        },
        {
          "id": "MDRW2100025174.1.15",
          "form": "저도 옵치하다가 접고",
          "original_form": "저도 옵치하다가 접고",
          "speaker_id": "2",
          "time": "20210901 00:17"
        },
        {
          "id": "MDRW2100025174.1.16",
          "form": "데바데에 입문했는데..신기하넹ㆍ",
          "original_form": "데바데에 입문했는데..신기하넹ㆍ",
          "speaker_id": "2",
          "time": "20210901 00:17"
        },
        {
          "id": "MDRW2100025174.1.17",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "2",
          "time": "20210901 00:17"
        },
        {
          "id": "MDRW2100025174.1.18",
          "form": "ㅋㅋㅋㄱㅋㅋ그러게요",
          "original_form": "ㅋㅋㅋㄱㅋㅋ그러게요",
          "speaker_id": "1",
          "time": "20210901 00:17"
        },
        {
          "id": "MDRW2100025174.1.19",
          "form": "근데 잘하는건 아니라 걍 맨날탈출못하고",
          "original_form": "근데 잘하는건 아니라 걍 맨날탈출못하고",
          "speaker_id": "1",
          "time": "20210901 00:18"
        },
        {
          "id": "MDRW2100025174.1.20",
          "form": "싹싹 빌다가 걸렷어요",
          "original_form": "싹싹 빌다가 걸렷어요",
          "speaker_id": "1",
          "time": "20210901 00:18"
        },
        {
          "id": "MDRW2100025174.1.21",
          "form": "쫄본데 굳이 무서운게임 찾아간거라ㅋㅋㅠ",
          "original_form": "쫄본데 굳이 무서운게임 찾아간거라ㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210901 00:18"
        },
        {
          "id": "MDRW2100025174.1.22",
          "form": "ㅋㅋㅋㅋㅋ 저도 맨날 살인마한테 애교 부려서 탈출하고",
          "original_form": "ㅋㅋㅋㅋㅋ 저도 맨날 살인마한테 애교 부려서 탈출하고",
          "speaker_id": "2",
          "time": "20210901 00:18"
        },
        {
          "id": "MDRW2100025174.1.23",
          "form": "주로 생존자 하시나봐요?",
          "original_form": "주로 생존자 하시나봐요?",
          "speaker_id": "2",
          "time": "20210901 00:18"
        },
        {
          "id": "MDRW2100025174.1.24",
          "form": "아무래도 살인마해봣는데",
          "original_form": "아무래도 살인마해봣는데",
          "speaker_id": "1",
          "time": "20210901 00:19"
        },
        {
          "id": "MDRW2100025174.1.25",
          "form": "저 평화주의자더라구요",
          "original_form": "저 평화주의자더라구요",
          "speaker_id": "1",
          "time": "20210901 00:19"
        },
        {
          "id": "MDRW2100025174.1.26",
          "form": "ㅋㅋ 저는 살인마 했는데 너무 못해서 생존자들이 미안해 하더라구요",
          "original_form": "ㅋㅋ 저는 살인마 했는데 너무 못해서 생존자들이 미안해 하더라구요",
          "speaker_id": "2",
          "time": "20210901 00:21"
        },
        {
          "id": "MDRW2100025174.1.27",
          "form": "ㅋㅋㅋㅋㄱㅋㅋㄱㅋㅋㄱㅋ너무 어려워요..",
          "original_form": "ㅋㅋㅋㅋㄱㅋㅋㄱㅋㅋㄱㅋ너무 어려워요..",
          "speaker_id": "1",
          "time": "20210901 00:22"
        },
        {
          "id": "MDRW2100025174.1.28",
          "form": "나분명 칼휘둘렀는데 왜 다들 무사해?",
          "original_form": "나분명 칼휘둘렀는데 왜 다들 무사해?",
          "speaker_id": "1",
          "time": "20210901 00:22"
        },
        {
          "id": "MDRW2100025174.1.29",
          "form": "몇번 고인물들한테 농락당하고",
          "original_form": "몇번 고인물들한테 농락당하고",
          "speaker_id": "1",
          "time": "20210901 00:22"
        },
        {
          "id": "MDRW2100025174.1.30",
          "form": "그냥 찔려죽는게 낫다는걸 깨달았어요",
          "original_form": "그냥 찔려죽는게 낫다는걸 깨달았어요",
          "speaker_id": "1",
          "time": "20210901 00:22"
        }
      ]
