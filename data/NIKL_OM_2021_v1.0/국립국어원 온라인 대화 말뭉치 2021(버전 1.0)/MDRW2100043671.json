      "utterance": [
        {
          "id": "MDRW2100043671.1.1",
          "form": "반려동물키우시나요",
          "original_form": "반려동물키우시나요",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043671.1.2",
          "form": "아뇨 name2님은요?",
          "original_form": "아뇨 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043671.1.3",
          "form": "전 키워요 고양이1마리여 ㅎㅎ",
          "original_form": "전 키워요 고양이1마리여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043671.1.4",
          "form": "오 냥집사시네요 ㅎㅎ",
          "original_form": "오 냥집사시네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043671.1.5",
          "form": "고양이 이름 물어봐도 될까요?",
          "original_form": "고양이 이름 물어봐도 될까요?",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043671.1.6",
          "form": "넹 name3에요 용처럼용맹하고건강하라고 지었어요 ㅎㅎ",
          "original_form": "넹 &name3&에요 용처럼용맹하고건강하라고 지었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043671.1.7",
          "form": "순우리말 이름이네요 ㅎㅎ",
          "original_form": "순우리말 이름이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043671.1.8",
          "form": "네 맞아요 어릴때 아팠어서 그치만 지금은 이름처럼 아주 용맹무쌍합니다 ㅎㅎㅎ",
          "original_form": "네 맞아요 어릴때 아팠어서 그치만 지금은 이름처럼 아주 용맹무쌍합니다 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043671.1.9",
          "form": "아팠군요 지금 건강해서 다행이네요",
          "original_form": "아팠군요 지금 건강해서 다행이네요",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043671.1.10",
          "form": "이름을 잘 지어 주셨네요",
          "original_form": "이름을 잘 지어 주셨네요",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043671.1.11",
          "form": "네 완전 지가 최고대빵이에여",
          "original_form": "네 완전 지가 최고대빵이에여",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043671.1.12",
          "form": "넘 오냐오냐키웠어요",
          "original_form": "넘 오냐오냐키웠어요",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043671.1.13",
          "form": "애정을 많이 주셨나봐요 ㅎㅎ",
          "original_form": "애정을 많이 주셨나봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043671.1.14",
          "form": "ㅋㅋㅋ네 넘 오냐오냐키웠어요",
          "original_form": "ㅋㅋㅋ네 넘 오냐오냐키웠어요",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043671.1.15",
          "form": "name1님은 만약 동물키우게되신다면 어떤동물키울거같아요?",
          "original_form": "&name1&님은 만약 동물키우게되신다면 어떤동물키울거같아요?",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043671.1.16",
