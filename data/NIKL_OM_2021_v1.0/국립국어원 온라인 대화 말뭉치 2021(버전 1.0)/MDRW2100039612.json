      "utterance": [
        {
          "id": "MDRW2100039612.1.1",
          "form": "저도요!!?",
          "original_form": "저도요!!?",
          "speaker_id": "2",
          "time": "20210903 19:42"
        },
        {
          "id": "MDRW2100039612.1.2",
          "form": "헤그리드에여",
          "original_form": "헤그리드에여",
          "speaker_id": "1",
          "time": "20210903 19:42"
        },
        {
          "id": "MDRW2100039612.1.3",
          "form": "미용사분들이 한숨...",
          "original_form": "미용사분들이 한숨...",
          "speaker_id": "2",
          "time": "20210903 19:42"
        },
        {
          "id": "MDRW2100039612.1.4",
          "form": "지금도ㅜㅜㅜ",
          "original_form": "지금도ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 19:42"
        },
        {
          "id": "MDRW2100039612.1.5",
          "form": "그래서 늘 단발펌하거든요",
          "original_form": "그래서 늘 단발펌하거든요",
          "speaker_id": "2",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100039612.1.6",
          "form": "머리말릴때마다",
          "original_form": "머리말릴때마다",
          "speaker_id": "1",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100039612.1.7",
          "form": "히피펌 고고고!!!!!",
          "original_form": "히피펌 고고고!!!!!",
          "speaker_id": "1",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100039612.1.8",
          "form": "길러서 갑시다!!!",
          "original_form": "길러서 갑시다!!!",
          "speaker_id": "1",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100039612.1.9",
          "form": "회사언니가 히피펌한거보니 이뻐요",
          "original_form": "회사언니가 히피펌한거보니 이뻐요",
          "speaker_id": "2",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100039612.1.10",
          "form": "그분도 머리숱 짱 많거든요",
          "original_form": "그분도 머리숱 짱 많거든요",
          "speaker_id": "2",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100039612.1.11",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210903 19:44"
        },
        {
          "id": "MDRW2100039612.1.12",
          "form": "나이드니 머리숱 많은거 큰 장점이죠!",
          "original_form": "나이드니 머리숱 많은거 큰 장점이죠!",
          "speaker_id": "2",
          "time": "20210903 19:44"
        },
        {
          "id": "MDRW2100039612.1.13",
          "form": "저는 나이들엇는데",
          "original_form": "저는 나이들엇는데",
          "speaker_id": "1",
          "time": "20210903 19:44"
        },
        {
          "id": "MDRW2100039612.1.14",
          "form": "아직모르것어요",
          "original_form": "아직모르것어요",
          "speaker_id": "1",
          "time": "20210903 19:44"
        },
        {
          "id": "MDRW2100039612.1.15",
          "form": "말리기귀찮아서",
          "original_form": "말리기귀찮아서",
          "speaker_id": "1",
          "time": "20210903 19:44"
        },
        {
          "id": "MDRW2100039612.1.16",
          "form": "대충말리고자요ㅜㅜㅜㅜㅜㅜㅜ",
          "original_form": "대충말리고자요ㅜㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 19:44"
        },
        {
          "id": "MDRW2100039612.1.17",
          "form": "철사...",
          "original_form": "철사...",
          "speaker_id": "2",
          "time": "20210903 19:44"
        },
        {
          "id": "MDRW2100039612.1.18",
