      "utterance": [
        {
          "id": "MDRW2100040053.1.1",
          "form": "강아지 잘 잇나요",
          "original_form": "강아지 잘 잇나요",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100040053.1.2",
          "form": "오늘 이사한 집이 아직 낯선가봐요",
          "original_form": "오늘 이사한 집이 아직 낯선가봐요",
          "speaker_id": "1",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100040053.1.3",
          "form": "자꾸 안아달래요",
          "original_form": "자꾸 안아달래요",
          "speaker_id": "1",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100040053.1.4",
          "form": "아이구애기 무섭나보내",
          "original_form": "아이구애기 무섭나보내",
          "speaker_id": "2",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100040053.1.5",
          "form": "겁쟁인가바요",
          "original_form": "겁쟁인가바요",
          "speaker_id": "2",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100040053.1.6",
          "form": "네 원래 겁쟁이긴해여... 근데 그게 또 귀엽...",
          "original_form": "네 원래 겁쟁이긴해여... 근데 그게 또 귀엽...",
          "speaker_id": "1",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100040053.1.7",
          "form": "울강아지에게 콩깍지!!",
          "original_form": "울강아지에게 콩깍지!!",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100040053.1.8",
          "form": "그죠그죠 겁쟁이 옆에 두고싶다",
          "original_form": "그죠그죠 겁쟁이 옆에 두고싶다",
          "speaker_id": "2",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100040053.1.9",
          "form": "내품에안겨ㅇ~~~~",
          "original_form": "내품에안겨ㅇ~~~~",
          "speaker_id": "2",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100040053.1.10",
          "form": "글고 약간 기분이 업됐는지 저는 추운데 자꾸 헥헥대요",
          "original_form": "글고 약간 기분이 업됐는지 저는 추운데 자꾸 헥헥대요",
          "speaker_id": "1",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100040053.1.11",
          "form": "ㅋㅋㅋ무섭지만 설레는건가요",
          "original_form": "ㅋㅋㅋ무섭지만 설레는건가요",
          "speaker_id": "2",
          "time": "20210904 01:56"
        },
        {
          "id": "MDRW2100040053.1.12",
          "form": "그르게요 ㅋㅋㅋㅋ 귀여워라 ㅠㅠ",
          "original_form": "그르게요 ㅋㅋㅋㅋ 귀여워라 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 01:56"
        },
        {
          "id": "MDRW2100040053.1.13",
          "form": "요즘 털이자라서 눈이 잘안보였거든요",
          "original_form": "요즘 털이자라서 눈이 잘안보였거든요",
          "speaker_id": "1",
          "time": "20210904 01:56"
        },
        {
          "id": "MDRW2100040053.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 귀엽당",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 귀엽당",
          "speaker_id": "2",
          "time": "20210904 01:57"
        },
        {
          "id": "MDRW2100040053.1.15",
          "form": "그래서 엄마가 방금 눈 쪽 털정리해줬는데요",
          "original_form": "그래서 엄마가 방금 눈 쪽 털정리해줬는데요",
          "speaker_id": "1",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100040053.1.16",
          "form": "더 귀여워져버렸어요...",
          "original_form": "더 귀여워져버렸어요...",
          "speaker_id": "1",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100040053.1.17",
          "form": "사진찌거용",
          "original_form": "사진찌거용",
          "speaker_id": "2",
          "time": "20210904 02:07"
        },
        {
          "id": "MDRW2100040053.1.18",
          "form": "정말 보여드리고 싶다..!!",
          "original_form": "정말 보여드리고 싶다..!!",
          "speaker_id": "1",
          "time": "20210904 02:08"
        },
        {
          "id": "MDRW2100040053.1.19",
          "form": "봣어요 상상력 좋아서",
