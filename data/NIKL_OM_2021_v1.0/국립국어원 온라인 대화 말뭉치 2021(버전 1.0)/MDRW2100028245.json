      "utterance": [
        {
          "id": "MDRW2100028245.1.1",
          "form": "컨디션이 영 별로예요",
          "original_form": "컨디션이 영 별로예요",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028245.1.2",
          "form": "왜 입맛이 없어요",
          "original_form": "왜 입맛이 없어요",
          "speaker_id": "1",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028245.1.3",
          "form": "오늘 약간 상태가 안 좋아서",
          "original_form": "오늘 약간 상태가 안 좋아서",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028245.1.4",
          "form": "그럴 땐 억지로라도 영양가 있게 먹어야 기력이 돌아요",
          "original_form": "그럴 땐 억지로라도 영양가 있게 먹어야 기력이 돌아요",
          "speaker_id": "1",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028245.1.5",
          "form": "내일도 이러면 병원에 한번 가 보려고요",
          "original_form": "내일도 이러면 병원에 한번 가 보려고요",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028245.1.6",
          "form": "그래요 가서 링거 주사라도 맞아 보는게 좋을 듯해요",
          "original_form": "그래요 가서 링거 주사라도 맞아 보는게 좋을 듯해요",
          "speaker_id": "1",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100028245.1.7",
          "form": "근데 요즘 코로나라",
          "original_form": "근데 요즘 코로나라",
          "speaker_id": "2",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100028245.1.8",
          "form": "병원 가는 것도 큰 맘 먹고 가야 해서",
          "original_form": "병원 가는 것도 큰 맘 먹고 가야 해서",
          "speaker_id": "2",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100028245.1.9",
          "form": "좀 걱정스러워요",
          "original_form": "좀 걱정스러워요",
          "speaker_id": "2",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100028245.1.10",
          "form": "그렇겠네요",
          "original_form": "그렇겠네요",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028245.1.11",
          "form": "건강할 때 건강을 잘 챙겨야 하는데",
          "original_form": "건강할 때 건강을 잘 챙겨야 하는데",
          "speaker_id": "2",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028245.1.12",
          "form": "후회돼요",
          "original_form": "후회돼요",
          "speaker_id": "2",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028245.1.13",
          "form": "아직 젊어서 지금도 늦지 않았어요",
          "original_form": "아직 젊어서 지금도 늦지 않았어요",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028245.1.14",
          "form": "운동이라도 꾸준히 해야겠어요",
          "original_form": "운동이라도 꾸준히 해야겠어요",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028245.1.15",
          "form": "좋은 생각이네요",
          "original_form": "좋은 생각이네요",
          "speaker_id": "1",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028245.1.16",
          "form": "맨날 말만 하고 귀찮아서 미루어뒀거든요",
          "original_form": "맨날 말만 하고 귀찮아서 미루어뒀거든요",
