      "utterance": [
        {
          "id": "MDRW2100038919.1.1",
          "form": "오늘도 집안일 하셨나요?",
          "original_form": "오늘도 집안일 하셨나요?",
          "speaker_id": "1",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100038919.1.2",
          "form": "집안일 이라기 보다는 오늘은 옷장정리 했어요",
          "original_form": "집안일 이라기 보다는 오늘은 옷장정리 했어요",
          "speaker_id": "2",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100038919.1.3",
          "form": "오 부지런하시네요 ㅎ",
          "original_form": "오 부지런하시네요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100038919.1.4",
          "form": "요 며칠 아침 저녁으로 찬바람이 불어서 긴팔이랑 가디건 꺼내놨어요",
          "original_form": "요 며칠 아침 저녁으로 찬바람이 불어서 긴팔이랑 가디건 꺼내놨어요",
          "speaker_id": "2",
          "time": "20210904 20:14"
        },
        {
          "id": "MDRW2100038919.1.5",
          "form": "갑자기 서늘해지긴 했죠",
          "original_form": "갑자기 서늘해지긴 했죠",
          "speaker_id": "1",
          "time": "20210904 20:16"
        },
        {
          "id": "MDRW2100038919.1.6",
          "form": "간만에 옷장 정리 했더니 피곤하더라구요~ㅋ",
          "original_form": "간만에 옷장 정리 했더니 피곤하더라구요~ㅋ",
          "speaker_id": "2",
          "time": "20210904 20:20"
        },
        {
          "id": "MDRW2100038919.1.7",
          "form": "큰일 하셨네요 ㅜ 후련하실 듯",
          "original_form": "큰일 하셨네요 ㅜ 후련하실 듯",
          "speaker_id": "1",
          "time": "20210904 20:21"
        },
        {
          "id": "MDRW2100038919.1.8",
          "form": "큰일은 아닌데 그것도 일이라고 요즘 근력이 떨어졌는지 조금만 움직여도 피곤하네요",
          "original_form": "큰일은 아닌데 그것도 일이라고 요즘 근력이 떨어졌는지 조금만 움직여도 피곤하네요",
          "speaker_id": "2",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100038919.1.9",
          "form": "큰일이죠 ㅎ 에너지 소모도 크고 ㅎ",
          "original_form": "큰일이죠 ㅎ 에너지 소모도 크고 ㅎ",
          "speaker_id": "1",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100038919.1.10",
          "form": "말이 옷장정리지 긴팔티랑 가디건 꺼냈을 뿐인데~ㅋ\r\n옷장정리 하기 전에 청소를 해서 그런가? ㅋ",
          "original_form": "말이 옷장정리지 긴팔티랑 가디건 꺼냈을 뿐인데~ㅋ\r\n옷장정리 하기 전에 청소를 해서 그런가? ㅋ",
          "speaker_id": "2",
          "time": "20210904 20:26"
        },
        {
          "id": "MDRW2100038919.1.11",
          "form": "ㅋㅋㅋㅋ 청소 좀 누가 해주면 좋겠어요",
          "original_form": "ㅋㅋㅋㅋ 청소 좀 누가 해주면 좋겠어요",
          "speaker_id": "1",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100038919.1.12",
          "form": "공기 환기 되라고 창문을 열어 두는 편이라 먼지가 계속 날아드네요",
          "original_form": "공기 환기 되라고 창문을 열어 두는 편이라 먼지가 계속 날아드네요",
          "speaker_id": "2",
          "time": "20210904 20:30"
        },
        {
          "id": "MDRW2100038919.1.13",
          "form": "ㅇㅏ 오늘 공기는 괜찮나요",
          "original_form": "ㅇㅏ 오늘 공기는 괜찮나요",
          "speaker_id": "1",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100038919.1.14",
          "form": "먼지가 들어와서 청소는 열심히 해야 되지만 자연바람이 최고죠~",
          "original_form": "먼지가 들어와서 청소는 열심히 해야 되지만 자연바람이 최고죠~",
          "speaker_id": "2",
          "time": "20210904 20:37"
        }
      ]
