          "id": "MDRW2100025286.1.1",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210901 09:17"
        },
        {
          "id": "MDRW2100025286.1.2",
          "form": "저도 아직은",
          "original_form": "저도 아직은",
          "speaker_id": "1",
          "time": "20210901 09:18"
        },
        {
          "id": "MDRW2100025286.1.3",
          "form": "강아지키우고싶은데",
          "original_form": "강아지키우고싶은데",
          "speaker_id": "1",
          "time": "20210901 12:10"
        },
        {
          "id": "MDRW2100025286.1.4",
          "form": "어떤 종류요?",
          "original_form": "어떤 종류요?",
          "speaker_id": "2",
          "time": "20210901 12:12"
        },
        {
          "id": "MDRW2100025286.1.5",
          "form": "종은 잘 몰라서",
          "original_form": "종은 잘 몰라서",
          "speaker_id": "1",
          "time": "20210901 12:14"
        },
        {
          "id": "MDRW2100025286.1.6",
          "form": "작은거요",
          "original_form": "작은거요",
          "speaker_id": "1",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100025286.1.7",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100025286.1.8",
          "form": "관리가 쉬울거 같은데.",
          "original_form": "관리가 쉬울거 같은데.",
          "speaker_id": "1",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100025286.1.9",
          "form": "아닌가요?",
          "original_form": "아닌가요?",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100025286.1.10",
          "form": "힘드러여",
          "original_form": "힘드러여",
          "speaker_id": "1",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100025286.1.11",
          "form": "맞아요 동물과 함께 산다는건 힘들죠",
          "original_form": "맞아요 동물과 함께 산다는건 힘들죠",
          "speaker_id": "2",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100025286.1.12",
          "form": "케어가힘들어요",
          "original_form": "케어가힘들어요",
          "speaker_id": "1",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100025286.1.13",
          "form": "하얀개가 좋으신가요 아님 갈색이나 까만개가 좋으신가요?",
          "original_form": "하얀개가 좋으신가요 아님 갈색이나 까만개가 좋으신가요?",
          "speaker_id": "2",
          "time": "20210903 14:12"
        },
        {
          "id": "MDRW2100025286.1.14",
          "form": "하얀니요",
          "original_form": "하얀니요",
          "speaker_id": "1",
          "time": "20210903 14:27"
        },
        {
          "id": "MDRW2100025286.1.15",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100025286.1.16",
          "form": "이쁘잖아요",
          "original_form": "이쁘잖아요",
          "speaker_id": "1",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100025286.1.17",
