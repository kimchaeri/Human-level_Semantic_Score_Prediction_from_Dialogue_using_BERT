      "utterance": [
        {
          "id": "MDRW2100032039.1.1",
          "form": "오 넷플릭스에서 나온 것 중엔 뭐가 젤 재밋으셨어요",
          "original_form": "오 넷플릭스에서 나온 것 중엔 뭐가 젤 재밋으셨어요",
          "speaker_id": "2",
          "time": "20210905 11:20"
        },
        {
          "id": "MDRW2100032039.1.2",
          "form": "킹덤이죠 역시",
          "original_form": "킹덤이죠 역시",
          "speaker_id": "1",
          "time": "20210905 11:21"
        },
        {
          "id": "MDRW2100032039.1.3",
          "form": "두둥",
          "original_form": "두둥",
          "speaker_id": "1",
          "time": "20210905 11:21"
        },
        {
          "id": "MDRW2100032039.1.4",
          "form": "역시 킹덤이 최고죠ㅋㅋㅋ",
          "original_form": "역시 킹덤이 최고죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:22"
        },
        {
          "id": "MDRW2100032039.1.5",
          "form": "너무 좋아요",
          "original_form": "너무 좋아요",
          "speaker_id": "2",
          "time": "20210905 11:22"
        },
        {
          "id": "MDRW2100032039.1.6",
          "form": "주지훈",
          "original_form": "주지훈",
          "speaker_id": "1",
          "time": "20210905 11:23"
        },
        {
          "id": "MDRW2100032039.1.7",
          "form": "마약했어도 캐스팅되는 이유가..",
          "original_form": "마약했어도 캐스팅되는 이유가..",
          "speaker_id": "1",
          "time": "20210905 11:23"
        },
        {
          "id": "MDRW2100032039.1.8",
          "form": "확실히 그 분위기는 주지훈밖에 못낼거같아요",
          "original_form": "확실히 그 분위기는 주지훈밖에 못낼거같아요",
          "speaker_id": "2",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100032039.1.9",
          "form": "진짜 세자 최고...",
          "original_form": "진짜 세자 최고...",
          "speaker_id": "1",
          "time": "20210905 11:38"
        },
        {
          "id": "MDRW2100032039.1.10",
          "form": "이번에 아신전도 보셨겠네요",
          "original_form": "이번에 아신전도 보셨겠네요",
          "speaker_id": "2",
          "time": "20210905 11:48"
        },
        {
          "id": "MDRW2100032039.1.11",
          "form": "네 아신전은 불편했어요",
          "original_form": "네 아신전은 불편했어요",
          "speaker_id": "1",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100032039.1.12",
          "form": "조선 나쁜것들ㅠ",
          "original_form": "조선 나쁜것들ㅠ",
          "speaker_id": "1",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100032039.1.13",
          "form": "ㅋㅋㅋ그니까요 전지현 역할 너무 이해되고",
          "original_form": "ㅋㅋㅋ그니까요 전지현 역할 너무 이해되고",
          "speaker_id": "2",
          "time": "20210905 11:50"
        },
        {
          "id": "MDRW2100032039.1.14",
          "form": "저같아도 죽였습니다ㅋㅋ",
          "original_form": "저같아도 죽였습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100032039.1.15",
          "form": "아직 시즌3 촬영하는지 안하는지 모르겠네요",
          "original_form": "아직 시즌3 촬영하는지 안하는지 모르겠네요",
          "speaker_id": "2",
          "time": "20210905 11:57"
        },
        {
          "id": "MDRW2100032039.1.16",
          "form": "아직일거에요 ㅎㅎ",
          "original_form": "아직일거에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 12:23"
        },
        {
          "id": "MDRW2100032039.1.17",
          "form": "아ㅠㅠ 더 기다려야되는구나",
          "original_form": "아ㅠㅠ 더 기다려야되는구나",
          "speaker_id": "2",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100032039.1.18",
          "form": "무서운 거 잘 보시나봐요",
          "original_form": "무서운 거 잘 보시나봐요",
          "speaker_id": "2",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100032039.1.19",
          "form": "욕하면서",
          "original_form": "욕하면서",
          "speaker_id": "1",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032039.1.20",
          "form": "봅니다 ㅋㅋ",
          "original_form": "봅니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032039.1.21",
          "form": "ㅋㅋㅋㅋ눈 가리면서?",
          "original_form": "ㅋㅋㅋㅋ눈 가리면서?",
          "speaker_id": "2",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032039.1.22",
          "form": "가리지는 않아요",
          "original_form": "가리지는 않아요",
          "speaker_id": "1",
          "time": "20210905 14:21"
        },
        {
          "id": "MDRW2100032039.1.23",
          "form": "화들짝은 합니다 ㅋㅋㅋ",
          "original_form": "화들짝은 합니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:21"
        },
        {
          "id": "MDRW2100032039.1.24",
          "form": "예전에 곡성보다가 영화관에서 소리 질렀어요",
          "original_form": "예전에 곡성보다가 영화관에서 소리 질렀어요",
          "speaker_id": "1",
          "time": "20210905 14:22"
        },
        {
          "id": "MDRW2100032039.1.25",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100032039.1.26",
          "form": "곡성은 막 되게 무서운 장면은 없지않나요",
          "original_form": "곡성은 막 되게 무서운 장면은 없지않나요",
          "speaker_id": "2",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100032039.1.27",
          "form": "그 곽도원이 차에서 스킨십하다가",
          "original_form": "그 곽도원이 차에서 스킨십하다가",
          "speaker_id": "1",
          "time": "20210905 20:07"
        },
        {
          "id": "MDRW2100032039.1.28",
          "form": "딸한테 걸릴때 ㅋㅋ",
          "original_form": "딸한테 걸릴때 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:07"
        },
        {
          "id": "MDRW2100032039.1.29",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "2",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100032039.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100032039.1.31",
          "form": "곡성에서 것도 나름 공포포인트네요 ㅋㅋㅋㅋㅋ",
          "original_form": "곡성에서 것도 나름 공포포인트네요 ㅋㅋㅋㅋㅋ",
