      "utterance": [
        {
          "id": "MDRW2100037223.1.1",
          "form": "날씨 어떤가오?",
          "original_form": "날씨 어떤가오?",
          "speaker_id": "1",
          "time": "20210831 22:03"
        },
        {
          "id": "MDRW2100037223.1.2",
          "form": "밤새 비가 많이 쏟아질거라는",
          "original_form": "밤새 비가 많이 쏟아질거라는",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100037223.1.3",
          "form": "재난문자가 왔어요",
          "original_form": "재난문자가 왔어요",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100037223.1.4",
          "form": "아직 안오는데 습도가 높아요",
          "original_form": "아직 안오는데 습도가 높아요",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100037223.1.5",
          "form": "저희도요!",
          "original_form": "저희도요!",
          "speaker_id": "1",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100037223.1.6",
          "form": "여기도요!",
          "original_form": "여기도요!",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037223.1.7",
          "form": "내일 아침에 운동해야해서 비안왔음 했는데 속상해요ㅜ",
          "original_form": "내일 아침에 운동해야해서 비안왔음 했는데 속상해요ㅜ",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037223.1.8",
          "form": "오 아침운동을 하시다닛?!",
          "original_form": "오 아침운동을 하시다닛?!",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037223.1.9",
          "form": "헿 건강을 위해..!",
          "original_form": "헿 건강을 위해..!",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037223.1.10",
          "form": "근데 비가오면 너무 슬퍼요ㅜㅠㅜ",
          "original_form": "근데 비가오면 너무 슬퍼요ㅜㅠㅜ",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037223.1.11",
          "form": "으와 러닝하나요",
          "original_form": "으와 러닝하나요",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037223.1.12",
          "form": "조금 오면 우산쓰고 할 수 있지만 많이오믄 답이 없어오오",
          "original_form": "조금 오면 우산쓰고 할 수 있지만 많이오믄 답이 없어오오",
          "speaker_id": "1",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100037223.1.13",
          "form": "네!ㅋㅋ",
          "original_form": "네!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100037223.1.14",
          "form": "그쳐 조금하면 그래두 맞을만한데 ㅋㅋㅋ",
          "original_form": "그쳐 조금하면 그래두 맞을만한데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100037223.1.15",
          "form": "와 혼자 뛰시나요? 러닝메이트가 있어요?",
          "original_form": "와 혼자 뛰시나요? 러닝메이트가 있어요?",
          "speaker_id": "2",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100037223.1.16",
          "form": "엄마가 시간있으시면 같이하고 아님 혼자해오!",
          "original_form": "엄마가 시간있으시면 같이하고 아님 혼자해오!",
          "speaker_id": "1",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037223.1.17",
          "form": "작은 동산이 있어서 날좋으면 바람도 불고 좋더라구요",
          "original_form": "작은 동산이 있어서 날좋으면 바람도 불고 좋더라구요",
          "speaker_id": "1",
          "time": "20210831 22:20"
        },
        {
          "id": "MDRW2100037223.1.18",
          "form": "우와 엄마랑 러닝도 좋네요",
          "original_form": "우와 엄마랑 러닝도 좋네요",
          "speaker_id": "2",
          "time": "20210901 06:40"
        },
        {
          "id": "MDRW2100037223.1.19",
          "form": "운동가시나요 ㅋㅋㅋㅋ",
          "original_form": "운동가시나요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 06:40"
        },
        {
          "id": "MDRW2100037223.1.20",
          "form": "비가 너무 많이오네요ㅜㅜㅜ",
          "original_form": "비가 너무 많이오네요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 07:29"
        },
        {
          "id": "MDRW2100037223.1.21",
          "form": "아ㅠ 아쉬워라ㅜ 여기두 빗줄기가 굵어지기 시작했어요",
          "original_form": "아ㅠ 아쉬워라ㅜ 여기두 빗줄기가 굵어지기 시작했어요",
          "speaker_id": "2",
          "time": "20210901 08:33"
        },
        {
          "id": "MDRW2100037223.1.22",
          "form": "ㅜㅜ새벽부터 두드리는 소리나길래 깨서 바라보다 다시 잤어욬ㅋㅋ",
          "original_form": "ㅜㅜ새벽부터 두드리는 소리나길래 깨서 바라보다 다시 잤어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 08:35"
        },
        {
          "id": "MDRW2100037223.1.23",
          "form": "조심하셔요ㅜ",
          "original_form": "조심하셔요ㅜ",
          "speaker_id": "1",
          "time": "20210901 08:35"
        }
      ]
    }
