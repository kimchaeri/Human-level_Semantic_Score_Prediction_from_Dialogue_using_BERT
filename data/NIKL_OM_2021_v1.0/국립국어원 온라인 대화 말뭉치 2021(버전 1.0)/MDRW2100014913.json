      "utterance": [
        {
          "id": "MDRW2100014913.1.1",
          "form": "스포츠 좋아하시는 거 있으신가요?",
          "original_form": "스포츠 좋아하시는 거 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014913.1.2",
          "form": "축구좋아하긴 하는데",
          "original_form": "축구좋아하긴 하는데",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014913.1.3",
          "form": "스포츠 하면 아무래도 이번 도쿄가",
          "original_form": "스포츠 하면 아무래도 이번 도쿄가",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014913.1.4",
          "form": "인상적이었죠",
          "original_form": "인상적이었죠",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014913.1.5",
          "form": "도쿄보셨나요",
          "original_form": "도쿄보셨나요",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014913.1.6",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014913.1.7",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014913.1.8",
          "form": "저는 우상혁 선수가 기억에 많이 남더라고요",
          "original_form": "저는 우상혁 선수가 기억에 많이 남더라고요",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014913.1.9",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014913.1.10",
          "form": "통했다",
          "original_form": "통했다",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014913.1.11",
          "form": "우상혁선수 진짜 인상깊었죠",
          "original_form": "우상혁선수 진짜 인상깊었죠",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014913.1.12",
          "form": "그쵸!!!",
          "original_form": "그쵸!!!",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014913.1.13",
          "form": "상혁아!!!",
          "original_form": "상혁아!!!",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014913.1.14",
          "form": "잘했어!!!!",
          "original_form": "잘했어!!!!",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014913.1.15",
          "form": "눈빛이 진짜 초롱초롱 하시더라고요",
          "original_form": "눈빛이 진짜 초롱초롱 하시더라고요",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014913.1.16",
          "form": "글고 막 혼자 다독이면서 하는데",
          "original_form": "글고 막 혼자 다독이면서 하는데",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014913.1.17",
          "form": "와 진짜",
          "original_form": "와 진짜",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014913.1.18",
          "form": "나중에 인터뷰 보니까 기억 안나셨대요",
          "original_form": "나중에 인터뷰 보니까 기억 안나셨대요",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014913.1.19",
          "form": "저것이 올림픽정신",
          "original_form": "저것이 올림픽정신",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014913.1.20",
          "form": "경례한 것만 기억나신다 그러시더라고요",
          "original_form": "경례한 것만 기억나신다 그러시더라고요",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014913.1.21",
          "form": "헉 기억이 안났다니",
          "original_form": "헉 기억이 안났다니",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014913.1.22",
          "form": "영상보고 놀랬겠다",
          "original_form": "영상보고 놀랬겠다",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014913.1.23",
          "form": "멋지쥬?",
          "original_form": "멋지쥬?",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014913.1.24",
          "form": "대박 멋지죠",
          "original_form": "대박 멋지죠",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014913.1.25",
          "form": "저도 그런사람 되고싶었어요",
          "original_form": "저도 그런사람 되고싶었어요",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014913.1.26",
          "form": "또 뭐 재밌게 봤어요",
          "original_form": "또 뭐 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210825 21:14"
        }
      ]
