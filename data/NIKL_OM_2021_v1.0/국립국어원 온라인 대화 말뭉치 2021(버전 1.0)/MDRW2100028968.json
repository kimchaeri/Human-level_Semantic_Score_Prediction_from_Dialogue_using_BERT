      "utterance": [
        {
          "id": "MDRW2100028968.1.1",
          "form": "시간이 많이 늦긴했지만",
          "original_form": "시간이 많이 늦긴했지만",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028968.1.2",
          "form": "야식 땡기네요ㅋㅋ 큰일났어요",
          "original_form": "야식 땡기네요ㅋㅋ 큰일났어요",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028968.1.3",
          "form": "참으세요ㅋㅋ살찝니다.",
          "original_form": "참으세요ㅋㅋ살찝니다.",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028968.1.4",
          "form": "저는 저녁외식해서",
          "original_form": "저는 저녁외식해서",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028968.1.5",
          "form": "배꺼지면자려고요",
          "original_form": "배꺼지면자려고요",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028968.1.6",
          "form": "저는 가만히 있다가 자야겠네요",
          "original_form": "저는 가만히 있다가 자야겠네요",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028968.1.7",
          "form": "네넹 저는 책이나 읽다가 자려고요\n낼맛있는거 먹어야죠 불금인데",
          "original_form": "네넹 저는 책이나 읽다가 자려고요\n낼맛있는거 먹어야죠 불금인데",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028968.1.8",
          "form": "아 맞아요 벌써 금요일이 코앞이군요",
          "original_form": "아 맞아요 벌써 금요일이 코앞이군요",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100028968.1.9",
          "form": "네네 낼 점심 뭐드세요",
          "original_form": "네네 낼 점심 뭐드세요",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100028968.1.10",
          "form": "그냥 밥 볶아 먹을거 같아요",
          "original_form": "그냥 밥 볶아 먹을거 같아요",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100028968.1.11",
          "form": "아침은 챙겨드세요?",
          "original_form": "아침은 챙겨드세요?",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100028968.1.12",
          "form": "무조건 먹어요",
          "original_form": "무조건 먹어요",
          "speaker_id": "2",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100028968.1.13",
          "form": "부모님이 억지로라도 먹이십니다",
          "original_form": "부모님이 억지로라도 먹이십니다",
          "speaker_id": "2",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100028968.1.14",
          "form": "아~ 저도 어릴적에 그랬어요",
          "original_form": "아~ 저도 어릴적에 그랬어요",
          "speaker_id": "1",
          "time": "20210903 05:14"
        },
        {
          "id": "MDRW2100028968.1.15",
          "form": "부모님들도 식습관이 변화하시더라고요",
          "original_form": "부모님들도 식습관이 변화하시더라고요",
          "speaker_id": "1",
          "time": "20210903 05:14"
        },
        {
          "id": "MDRW2100028968.1.16",
          "form": "어떤 면에서 변하셨나요",
          "original_form": "어떤 면에서 변하셨나요",
          "speaker_id": "2",
          "time": "20210903 08:19"
        },
        {
          "id": "MDRW2100028968.1.17",
          "form": "이제 아침밥 안드세요",
          "original_form": "이제 아침밥 안드세요",
          "speaker_id": "1",
          "time": "20210903 08:26"
        },
        {
          "id": "MDRW2100028968.1.18",
          "form": "예전에는 꼬박꼬박 같이 먹었던기억이ㅎㅎ",
          "original_form": "예전에는 꼬박꼬박 같이 먹었던기억이ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 08:26"
        },
        {
          "id": "MDRW2100028968.1.19",
          "form": "안 드시면 안좋을텐데ㅠ",
          "original_form": "안 드시면 안좋을텐데ㅠ",
          "speaker_id": "2",
          "time": "20210903 08:29"
        },
        {
          "id": "MDRW2100028968.1.20",
          "form": "회사에 아침먹고오는 사람들이 확실히 전보다적어지긴한것같아요",
          "original_form": "회사에 아침먹고오는 사람들이 확실히 전보다적어지긴한것같아요",
          "speaker_id": "1",
          "time": "20210903 09:26"
        }
      ]
