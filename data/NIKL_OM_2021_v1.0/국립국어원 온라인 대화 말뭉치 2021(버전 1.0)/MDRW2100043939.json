      "utterance": [
        {
          "id": "MDRW2100043939.1.1",
          "form": "그 스듀 비슷한",
          "original_form": "그 스듀 비슷한",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043939.1.2",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043939.1.3",
          "form": "귀여운 겜 나왔다던데",
          "original_form": "귀여운 겜 나왔다던데",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043939.1.4",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043939.1.5",
          "form": "나온다던데",
          "original_form": "나온다던데",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043939.1.6",
          "form": "스팀 게임이야?",
          "original_form": "스팀 게임이야?",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043939.1.7",
          "form": "이름 기억안남",
          "original_form": "이름 기억안남",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043939.1.8",
          "form": "자빱님이 한번 언급하신적 있으셔",
          "original_form": "자빱님이 한번 언급하신적 있으셔",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043939.1.9",
          "form": "어...몰러",
          "original_form": "어...몰러",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043939.1.10",
          "form": "아 진짜? 잘 모르겠네",
          "original_form": "아 진짜? 잘 모르겠네",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043939.1.11",
          "form": "나도 요즘 바빠서 거의 편집 영상만 봐서 토크나 게임 생방 많이 못 봤어",
          "original_form": "나도 요즘 바빠서 거의 편집 영상만 봐서 토크나 게임 생방 많이 못 봤어",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043939.1.12",
          "form": "완전 귀욤뽀짝하다고",
          "original_form": "완전 귀욤뽀짝하다고",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043939.1.13",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043939.1.14",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043939.1.15",
          "form": "나 스듀 가끔씩 재방으로",
          "original_form": "나 스듀 가끔씩 재방으로",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043939.1.16",
          "form": "혼자 돌려보",
          "original_form": "혼자 돌려보",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043939.1.17",
          "form": "다시보기",
          "original_form": "다시보기",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043939.1.18",
          "form": "우정리밸리 너무 재밌어",
          "original_form": "우정리밸리 너무 재밌어",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043939.1.19",
          "form": "예전에 하신 스듀 방송도 재밌고",
          "original_form": "예전에 하신 스듀 방송도 재밌고",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043939.1.20",
          "form": "ㅇㅈ",
          "original_form": "ㅇㅈ",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043939.1.21",
          "form": "게임하고 싶다",
          "original_form": "게임하고 싶다",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043939.1.22",
          "form": "심즈도 진짜 웃겼는데",
          "original_form": "심즈도 진짜 웃겼는데",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043939.1.23",
          "form": "나두...",
          "original_form": "나두...",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043939.1.24",
          "form": "마자 ㅋㅋㅋㅋㅋ",
          "original_form": "마자 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043939.1.25",
          "form": "아니 그",
          "original_form": "아니 그",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043939.1.26",
          "form": "우정밸리에서",
          "original_form": "우정밸리에서",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043939.1.27",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043939.1.28",
          "form": "곡길댁 우스댁 기린댁 나와서",
          "original_form": "곡길댁 우스댁 기린댁 나와서",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043939.1.29",
          "form": "빱님 치킨 받으러 갔을때",
          "original_form": "빱님 치킨 받으러 갔을때",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043939.1.30",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043939.1.31",
          "form": "너무 좋댜아ㅏ",
          "original_form": "너무 좋댜아ㅏ",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043939.1.32",
          "form": "이거",
          "original_form": "이거",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043939.1.33",
          "form": "너무 웃김",
          "original_form": "너무 웃김",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043939.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043939.1.35",
          "form": "그니까ㅋㅋㅋㅋ",
          "original_form": "그니까ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043939.1.36",
          "form": "다음에 스듀 같이 하자",
          "original_form": "다음에 스듀 같이 하자",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043939.1.37",
          "form": "좋아좋아!",
          "original_form": "좋아좋아!",
          "speaker_id": "2",
          "time": "20210907 20:58"
        }
      ]
    }
