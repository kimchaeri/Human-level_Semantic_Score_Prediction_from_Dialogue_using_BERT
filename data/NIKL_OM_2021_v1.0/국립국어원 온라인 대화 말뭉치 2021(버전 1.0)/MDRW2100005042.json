      "utterance": [
        {
          "id": "MDRW2100005042.1.1",
          "form": "name2 무슨음식 좋아해?",
          "original_form": "&name2& 무슨음식 좋아해?",
          "speaker_id": "1",
          "time": "20210821 20:08"
        },
        {
          "id": "MDRW2100005042.1.2",
          "form": "초밥",
          "original_form": "초밥",
          "speaker_id": "2",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100005042.1.3",
          "form": "수제비",
          "original_form": "수제비",
          "speaker_id": "2",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100005042.1.4",
          "form": "마라샹궈",
          "original_form": "마라샹궈",
          "speaker_id": "2",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100005042.1.5",
          "form": "최애 음식은 초밥이야?",
          "original_form": "최애 음식은 초밥이야?",
          "speaker_id": "1",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100005042.1.6",
          "form": "웅 거의 그래",
          "original_form": "웅 거의 그래",
          "speaker_id": "2",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100005042.1.7",
          "form": "회 좋아해",
          "original_form": "회 좋아해",
          "speaker_id": "2",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100005042.1.8",
          "form": "name1은?",
          "original_form": "&name1&은?",
          "speaker_id": "2",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100005042.1.9",
          "form": "우와 나도 회좋아해! 연어도",
          "original_form": "우와 나도 회좋아해! 연어도",
          "speaker_id": "1",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100005042.1.10",
          "form": "요즘엔 회 배달도 좋더라",
          "original_form": "요즘엔 회 배달도 좋더라",
          "speaker_id": "1",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100005042.1.11",
          "form": "연어회 맛있지",
          "original_form": "연어회 맛있지",
          "speaker_id": "2",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100005042.1.12",
          "form": "맞아 배달도 싱싱하게 잘 와",
          "original_form": "맞아 배달도 싱싱하게 잘 와",
          "speaker_id": "2",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100005042.1.13",
          "form": "커피 좋아해?",
          "original_form": "커피 좋아해?",
          "speaker_id": "2",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100005042.1.14",
          "form": "웅 나는 아아메!",
          "original_form": "웅 나는 아아메!",
          "speaker_id": "1",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100005042.1.15",
          "form": "name2는?",
          "original_form": "&name2&는?",
          "speaker_id": "1",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100005042.1.16",
          "form": "나는 주로 아이스라떼 많이 마셔",
          "original_form": "나는 주로 아이스라떼 많이 마셔",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005042.1.17",
          "form": "커피는 어느브랜드 좋아해?",
          "original_form": "커피는 어느브랜드 좋아해?",
          "speaker_id": "1",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005042.1.18",
          "form": "딱히 좋아한다기보다는 스타벅스 자주가 왜냐면 맛이 평타 쳐... 다른 체인점은 체인점 마다 맛 차이가 심해서",
          "original_form": "딱히 좋아한다기보다는 스타벅스 자주가 왜냐면 맛이 평타 쳐... 다른 체인점은 체인점 마다 맛 차이가 심해서",
          "speaker_id": "2",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005042.1.19",
          "form": "아 스타벅스는 맛이 똑같구나!",
          "original_form": "아 스타벅스는 맛이 똑같구나!",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005042.1.20",
          "form": "맞아 그리고 스벅은 기프티콘도 많이 받아",
          "original_form": "맞아 그리고 스벅은 기프티콘도 많이 받아",
          "speaker_id": "1",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100005042.1.21",
          "form": "맞아 그거 사용하러 가는 재미도 있어",
          "original_form": "맞아 그거 사용하러 가는 재미도 있어",
          "speaker_id": "2",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100005042.1.22",
          "form": "시즌음료도 맛있고?",
          "original_form": "시즌음료도 맛있고?",
          "speaker_id": "1",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100005042.1.23",
          "form": "이번에 지인이 사줘서 루비 레드 칠링 아이스티 먹어봤는데 맛있었어",
          "original_form": "이번에 지인이 사줘서 루비 레드 칠링 아이스티 먹어봤는데 맛있었어",
          "speaker_id": "2",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100005042.1.24",
          "form": "루프탑 그레이 라떼 먹어보고싶은데 주변 스타벅스는 다 품절이라 아직 못 먹어봤어",
          "original_form": "루프탑 그레이 라떼 먹어보고싶은데 주변 스타벅스는 다 품절이라 아직 못 먹어봤어",
          "speaker_id": "2",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100005042.1.25",
          "form": "이름이 신기하다, 루비 레드 칠링은 무슨맛이야?",
          "original_form": "이름이 신기하다, 루비 레드 칠링은 무슨맛이야?",
          "speaker_id": "1",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100005042.1.26",
          "form": "포도, 라임, 민트 합쳐진 맛",
          "original_form": "포도, 라임, 민트 합쳐진 맛",
          "speaker_id": "2",
          "time": "20210821 20:17"
        }
      ]
    }
