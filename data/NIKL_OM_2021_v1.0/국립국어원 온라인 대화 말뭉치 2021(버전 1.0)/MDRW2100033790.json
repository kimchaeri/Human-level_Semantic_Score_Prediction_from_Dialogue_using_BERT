        {
          "id": "MDRW2100033790.1.1",
          "form": "중식 좋아하세요?",
          "original_form": "중식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 01:54"
        },
        {
          "id": "MDRW2100033790.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 11:25"
        },
        {
          "id": "MDRW2100033790.1.3",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100033790.1.4",
          "form": "중식좋아하죠ㅋㅋ",
          "original_form": "중식좋아하죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100033790.1.5",
          "form": "자주 시켜먹어요ㅋ",
          "original_form": "자주 시켜먹어요ㅋ",
          "speaker_id": "2",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100033790.1.6",
          "form": "짬뽕vs짜장?",
          "original_form": "짬뽕vs짜장?",
          "speaker_id": "1",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100033790.1.7",
          "form": "짜장이요ㅋㅋ",
          "original_form": "짜장이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100033790.1.8",
          "form": "ㅋㅋㅋ 그럼",
          "original_form": "ㅋㅋㅋ 그럼",
          "speaker_id": "1",
          "time": "20210825 11:48"
        },
        {
          "id": "MDRW2100033790.1.9",
          "form": "찍먹vs부먹은요?",
          "original_form": "찍먹vs부먹은요?",
          "speaker_id": "1",
          "time": "20210825 11:48"
        },
        {
          "id": "MDRW2100033790.1.10",
          "form": "찍먹이요",
          "original_form": "찍먹이요",
          "speaker_id": "2",
          "time": "20210825 11:48"
        },
        {
          "id": "MDRW2100033790.1.11",
          "form": "부먹은 싫어요ㅋㅋ",
          "original_form": "부먹은 싫어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:48"
        },
        {
          "id": "MDRW2100033790.1.12",
          "form": "짜장이 좋으세요? 짬뽕?",
          "original_form": "짜장이 좋으세요? 짬뽕?",
          "speaker_id": "2",
          "time": "20210825 11:49"
        },
        {
          "id": "MDRW2100033790.1.13",
          "form": "ㅋㅋㅋ전 짬뽕이요",
          "original_form": "ㅋㅋㅋ전 짬뽕이요",
          "speaker_id": "1",
          "time": "20210825 11:53"
        },
        {
          "id": "MDRW2100033790.1.14",
          "form": "아주 얼큰하면 좋겠네요",
          "original_form": "아주 얼큰하면 좋겠네요",
          "speaker_id": "1",
          "time": "20210825 11:53"
        },
        {
          "id": "MDRW2100033790.1.15",
          "form": "아 ㅋㅋ얼큰한거 좋아하시는구나ㅋㅋ",
          "original_form": "아 ㅋㅋ얼큰한거 좋아하시는구나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100033790.1.16",
          "form": "저도 한번씩 땡길때 있더라구요.",
          "original_form": "저도 한번씩 땡길때 있더라구요.",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100033790.1.17",
          "form": "넵ㅋㅋㅋ",
          "original_form": "넵ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100033790.1.18",
          "form": "떡볶이는 좋아하세요?",
          "original_form": "떡볶이는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100033790.1.19",
          "form": "네 요즘에는 로제떡볶이 자주 먹어요. 맛있어요.",
          "original_form": "네 요즘에는 로제떡볶이 자주 먹어요. 맛있어요.",
          "speaker_id": "2",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100033790.1.20",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "1",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100033790.1.21",
          "form": "저랑같네요",
          "original_form": "저랑같네요",
          "speaker_id": "1",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100033790.1.22",
          "form": "네네 요즘 자주먹어요ㅋㅋ",
          "original_form": "네네 요즘 자주먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100033790.1.23",
          "form": "지금은 김치볶음밥먹으려구요.ㅋ",
          "original_form": "지금은 김치볶음밥먹으려구요.ㅋ",
          "speaker_id": "2",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100033790.1.24",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100033790.1.25",
          "form": "점심은먹었답니다!",
          "original_form": "점심은먹었답니다!",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100033790.1.26",
          "form": "보쌈vs족발은 어떠세요?",
          "original_form": "보쌈vs족발은 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100033790.1.27",
          "form": "아 어렵네요.ㅋㅋ",
          "original_form": "아 어렵네요.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100033790.1.28",
          "form": "둘다좋아하는데ㅋㅋ",
          "original_form": "둘다좋아하는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100033790.1.29",
          "form": "보쌈이요ㅋ",
