      "utterance": [
        {
          "id": "MDRW2100020624.1.1",
          "form": "전 학사 졸업했어요~",
          "original_form": "전 학사 졸업했어요~",
          "speaker_id": "2",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100020624.1.2",
          "form": "코로나 아닐 적에 대학교 다녀서 어찌나 다행이던지..",
          "original_form": "코로나 아닐 적에 대학교 다녀서 어찌나 다행이던지..",
          "speaker_id": "2",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100020624.1.3",
          "form": "그러게요ㅠㅠ 작년 입학한 신입생들부터는 대학생활의 추억이 없더라구요",
          "original_form": "그러게요ㅠㅠ 작년 입학한 신입생들부터는 대학생활의 추억이 없더라구요",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020624.1.4",
          "form": "그들도 모르는데 신입생들이 질문하면...ㅋㅋㅋ",
          "original_form": "그들도 모르는데 신입생들이 질문하면...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020624.1.5",
          "form": "선배님! 도서관은 어디있어요? / 어 그게....어..",
          "original_form": "선배님! 도서관은 어디있어요? / 어 그게....어..",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020624.1.6",
          "form": "선배님! 우리학교 행사 재밌어요? / 어.. 그게..ㅠ",
          "original_form": "선배님! 우리학교 행사 재밌어요? / 어.. 그게..ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020624.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅌ 그럴것같아요 물어보니 아직도 학교 온라인 수업으로 진행한대요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅌ 그럴것같아요 물어보니 아직도 학교 온라인 수업으로 진행한대요",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020624.1.8",
          "form": "50일 넘게 확진자수가 네자리 수니..",
          "original_form": "50일 넘게 확진자수가 네자리 수니..",
          "speaker_id": "2",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020624.1.9",
          "form": "2학기도 원격수업 하나보네요 ㅠㅠ",
          "original_form": "2학기도 원격수업 하나보네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020624.1.10",
          "form": "네네 9월에 개강은 진행대로 하고",
          "original_form": "네네 9월에 개강은 진행대로 하고",
          "speaker_id": "1",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020624.1.11",
          "form": "여전히 등교는 안한대요ㅠㅠ",
          "original_form": "여전히 등교는 안한대요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020624.1.12",
          "form": "너무 아쉽겠어요...",
          "original_form": "너무 아쉽겠어요...",
          "speaker_id": "1",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020624.1.13",
          "form": "학교 며칠 가보지도 못하구",
          "original_form": "학교 며칠 가보지도 못하구",
          "speaker_id": "1",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020624.1.14",
          "form": "캠퍼스라이프를 즐겨야되는데...",
          "original_form": "캠퍼스라이프를 즐겨야되는데...",
          "speaker_id": "2",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020624.1.15",
          "form": "CC도 하곸ㅋㅋㅋㅋㅋ",
          "original_form": "CC도 하곸ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020624.1.16",
          "form": "그럼 개총도 안해봤겠네요...",
          "original_form": "그럼 개총도 안해봤겠네요...",
          "speaker_id": "2",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020624.1.17",
          "form": "그럴거예요ㅠㅠ 이대로 졸업하면 진짜 허무할것같아요ㅠㅠ",
          "original_form": "그럴거예요ㅠㅠ 이대로 졸업하면 진짜 허무할것같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020624.1.18",
          "form": "대학생활은 뭐니뭐니해도",
          "original_form": "대학생활은 뭐니뭐니해도",
          "speaker_id": "1",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020624.1.19",
          "form": "동아리활동인데",
          "original_form": "동아리활동인데",
          "speaker_id": "1",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020624.1.20",
          "form": "어우 너무 불쌍해요 진짜",
          "original_form": "어우 너무 불쌍해요 진짜",
          "speaker_id": "2",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020624.1.21",
          "form": "name1님은 학교 다닐 적에 동아리활동 해보셨어요?",
          "original_form": "&name1&님은 학교 다닐 적에 동아리활동 해보셨어요?",
          "speaker_id": "2",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020624.1.22",
          "form": "저는 독서동아리였어요!",
          "original_form": "저는 독서동아리였어요!",
          "speaker_id": "1",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020624.1.23",
          "form": "너무 정적인 동아리라 막",
          "original_form": "너무 정적인 동아리라 막",
          "speaker_id": "1",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020624.1.24",
          "form": "활동적인 추억은 없긴한데ㅠㅠㅋㅋ",
          "original_form": "활동적인 추억은 없긴한데ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020624.1.25",
          "form": "독서 동아리 신기하다",
          "original_form": "독서 동아리 신기하다",
          "speaker_id": "2",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020624.1.26",
          "form": "각자 책 읽고 모여서 토론토의 하는 거에요?",
          "original_form": "각자 책 읽고 모여서 토론토의 하는 거에요?",
          "speaker_id": "2",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020624.1.27",
          "form": "넹넹! 그리고 서로 읽었던 책중에 좋았던 부분 말하고",
          "original_form": "넹넹! 그리고 서로 읽었던 책중에 좋았던 부분 말하고",
          "speaker_id": "1",
          "time": "20210828 11:21"
        },
        {
          "id": "MDRW2100020624.1.28",
          "form": "추천도 해줘용",
          "original_form": "추천도 해줘용",
          "speaker_id": "1",
          "time": "20210828 11:21"
        },
        {
          "id": "MDRW2100020624.1.29",
          "form": "원작있는 영화 개봉하면 같이 영화도 보러가용ㅋㅋㅋㅋ",
          "original_form": "원작있는 영화 개봉하면 같이 영화도 보러가용ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:21"
        },
        {
          "id": "MDRW2100020624.1.30",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 11:21"
        },
        {
          "id": "MDRW2100020624.1.31",
          "form": "저 운동동아리 했었어요~",
          "original_form": "저 운동동아리 했었어요~",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020624.1.32",
          "form": "오 무슨 운동 하셨어요?",
          "original_form": "오 무슨 운동 하셨어요?",
          "speaker_id": "1",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020624.1.33",
          "form": "티볼이랑 배드민턴 해봤는데.. 둘 다 안맞더라구요ㅠㅠ",
          "original_form": "티볼이랑 배드민턴 해봤는데.. 둘 다 안맞더라구요ㅠㅠ",
