          "speaker_id": "2",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100023637.1.8",
          "form": "거기서1등하면",
          "original_form": "거기서1등하면",
          "speaker_id": "1",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023637.1.9",
          "form": "정식연재가능한거임?",
          "original_form": "정식연재가능한거임?",
          "speaker_id": "1",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023637.1.10",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023637.1.11",
          "form": "근데 약간 올해는 내 스터일이 없네",
          "original_form": "근데 약간 올해는 내 스터일이 없네",
          "speaker_id": "2",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023637.1.12",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023637.1.13",
          "form": "올해는 패스다",
          "original_form": "올해는 패스다",
          "speaker_id": "2",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023637.1.14",
          "form": "매년하는거임?",
          "original_form": "매년하는거임?",
          "speaker_id": "1",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023637.1.15",
          "form": "그론가보ㅓ",
          "original_form": "그론가보ㅓ",
          "speaker_id": "2",
          "time": "20210830 13:33"
        },
