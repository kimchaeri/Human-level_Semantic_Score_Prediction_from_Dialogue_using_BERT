      "utterance": [
        {
          "id": "MDRW2100040165.1.1",
          "form": "점심 뭐 드세요?",
          "original_form": "점심 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210904 12:27"
        },
        {
          "id": "MDRW2100040165.1.2",
          "form": "저 샐러드사왔어요 뭐드세요",
          "original_form": "저 샐러드사왔어요 뭐드세요",
          "speaker_id": "2",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100040165.1.3",
          "form": "전 냉장고안에 뭐가 있는지 찾아봐야겠어요",
          "original_form": "전 냉장고안에 뭐가 있는지 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100040165.1.4",
          "form": "무슨 샐러드인가요?",
          "original_form": "무슨 샐러드인가요?",
          "speaker_id": "1",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100040165.1.5",
          "form": "닭 가 슴 살 이에요",
          "original_form": "닭 가 슴 살 이에요",
          "speaker_id": "2",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100040165.1.6",
          "form": "다이어트 식단인데요??",
          "original_form": "다이어트 식단인데요??",
          "speaker_id": "1",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100040165.1.7",
          "form": "그러게요 ㅋㅋ",
          "original_form": "그러게요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100040165.1.8",
          "form": "마땅치않아서 ㅋㅋ",
          "original_form": "마땅치않아서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100040165.1.9",
          "form": "늘 그렇게 드시는건 아니시죠?",
          "original_form": "늘 그렇게 드시는건 아니시죠?",
          "speaker_id": "1",
          "time": "20210904 13:15"
        },
        {
          "id": "MDRW2100040165.1.10",
          "form": "네네 오늘만요",
          "original_form": "네네 오늘만요",
          "speaker_id": "2",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100040165.1.11",
          "form": "전 냉동 떡갈비 데워서 밥 먹어요",
          "original_form": "전 냉동 떡갈비 데워서 밥 먹어요",
          "speaker_id": "1",
          "time": "20210904 13:18"
        },
        {
          "id": "MDRW2100040165.1.12",
          "form": "오 떡갈비 어디거가 맛나나요",
          "original_form": "오 떡갈비 어디거가 맛나나요",
          "speaker_id": "2",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100040165.1.13",
          "form": "비비고꺼 먹어요^^",
          "original_form": "비비고꺼 먹어요^^",
          "speaker_id": "1",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100040165.1.14",
          "form": "오 비비고꺼 맛있어요? 사봐야겠다",
          "original_form": "오 비비고꺼 맛있어요? 사봐야겠다",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100040165.1.15",
          "form": "비비고는 다 맛있어요 ㅎ",
          "original_form": "비비고는 다 맛있어요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100040165.1.16",
          "form": "비비고 만두도 맛나요",
          "original_form": "비비고 만두도 맛나요",
