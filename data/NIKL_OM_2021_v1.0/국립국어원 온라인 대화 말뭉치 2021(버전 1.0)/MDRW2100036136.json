      "utterance": [
        {
          "id": "MDRW2100036136.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210830 17:12"
        },
        {
          "id": "MDRW2100036136.1.2",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 17:13"
        },
        {
          "id": "MDRW2100036136.1.3",
          "form": "네 먹었어요",
          "original_form": "네 먹었어요",
          "speaker_id": "2",
          "time": "20210830 17:13"
        },
        {
          "id": "MDRW2100036136.1.4",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210830 17:13"
        },
        {
          "id": "MDRW2100036136.1.5",
          "form": "비빔면과 삶은계란+비엔나 구워서 먹었어요",
          "original_form": "비빔면과 삶은계란+비엔나 구워서 먹었어요",
          "speaker_id": "2",
          "time": "20210830 17:15"
        },
        {
          "id": "MDRW2100036136.1.6",
          "form": "와ㅠㅠ 맛있겠내요 비엔나와의 조합은 생각치 못했었는데 잘 어울리나요",
          "original_form": "와ㅠㅠ 맛있겠내요 비엔나와의 조합은 생각치 못했었는데 잘 어울리나요",
          "speaker_id": "1",
          "time": "20210830 17:16"
        },
        {
          "id": "MDRW2100036136.1.7",
          "form": "저도 비엔나와 함께는 처음이었는데 나쁘지 않았어요",
          "original_form": "저도 비엔나와 함께는 처음이었는데 나쁘지 않았어요",
          "speaker_id": "2",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100036136.1.8",
          "form": "글쿤요 전 어제 비빔면을 참치 통조림이랑 먹는거 보고 솔깃했어요",
          "original_form": "글쿤요 전 어제 비빔면을 참치 통조림이랑 먹는거 보고 솔깃했어요",
          "speaker_id": "1",
          "time": "20210830 17:29"
        },
        {
          "id": "MDRW2100036136.1.9",
          "form": "비빔면+참치 조합으로도 많이 먹더라구요\r\n고기와 함께 먹는분들도 많구요",
          "original_form": "비빔면+참치 조합으로도 많이 먹더라구요\r\n고기와 함께 먹는분들도 많구요",
          "speaker_id": "2",
          "time": "20210830 17:50"
        },
        {
          "id": "MDRW2100036136.1.10",
          "form": "참치통조림은 잘 드시나요?",
          "original_form": "참치통조림은 잘 드시나요?",
          "speaker_id": "1",
          "time": "20210830 17:53"
        },
        {
          "id": "MDRW2100036136.1.11",
          "form": "자주는 아니고 가끔이요  \r\n그냥 먹기 보다는 보통 요리에 넣게 되는거같아요",
          "original_form": "자주는 아니고 가끔이요  \r\n그냥 먹기 보다는 보통 요리에 넣게 되는거같아요",
          "speaker_id": "2",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100036136.1.12",
          "form": "저도요 전 참치 통조림은 샌드위치로 먹는걸 가장 좋아해요",
          "original_form": "저도요 전 참치 통조림은 샌드위치로 먹는걸 가장 좋아해요",
          "speaker_id": "1",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100036136.1.13",
          "form": "샌드위치에 참치 안 넣어봤는데,  궁금하네요",
          "original_form": "샌드위치에 참치 안 넣어봤는데,  궁금하네요",
          "speaker_id": "2",
          "time": "20210830 17:58"
        }
      ]
