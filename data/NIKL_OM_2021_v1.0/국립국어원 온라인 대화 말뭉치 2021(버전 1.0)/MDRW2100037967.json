      "utterance": [
        {
          "id": "MDRW2100037967.1.1",
          "form": "신랑 지금 오는중이래",
          "original_form": "신랑 지금 오는중이래",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100037967.1.2",
          "form": "신랑 오늘 왜 이렇게 늦었노",
          "original_form": "신랑 오늘 왜 이렇게 늦었노",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100037967.1.3",
          "form": "일이 많구만 요즘",
          "original_form": "일이 많구만 요즘",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100037967.1.4",
          "form": "관장님이",
          "original_form": "관장님이",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100037967.1.5",
          "form": "저녁같이먹자고",
          "original_form": "저녁같이먹자고",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100037967.1.6",
          "form": "하셔가",
          "original_form": "하셔가",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100037967.1.7",
          "form": "일이많단다",
          "original_form": "일이많단다",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100037967.1.8",
          "form": "아 웃겨ㅋㅋ",
          "original_form": "아 웃겨ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100037967.1.9",
          "form": "내가 너무 내 맘대로 생각했네ㅋㅋ",
          "original_form": "내가 너무 내 맘대로 생각했네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100037967.1.10",
          "form": "밥 먹고 오는 거라고 생각 못 했는데ㅋㅋㅋㅋ",
          "original_form": "밥 먹고 오는 거라고 생각 못 했는데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037967.1.11",
          "form": "ㅋㅋㅋㄱㅋㅋㅋ웃겨ㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋ웃겨ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037967.1.12",
          "form": "그러이ㅋㅋ",
          "original_form": "그러이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037967.1.13",
          "form": "밥먹자고ㅋㅋ",
          "original_form": "밥먹자고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037967.1.14",
          "form": "우린 김밥 먹었는데ㅋㅋ",
          "original_form": "우린 김밥 먹었는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037967.1.15",
          "form": "김밥 먹고 싶어ㅋㅋ",
          "original_form": "김밥 먹고 싶어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037967.1.16",
          "form": "오오 ㅋㅋㅋ 뭔소리래? 김밥먹었는데",
          "original_form": "오오 ㅋㅋㅋ 뭔소리래? 김밥먹었는데",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037967.1.17",
          "form": "김밥먹고싶다고?",
          "original_form": "김밥먹고싶다고?",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037967.1.18",
          "form": "너흰 김밥 먹었다고ㅋㅋ",
          "original_form": "너흰 김밥 먹었다고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100037967.1.19",
          "form": "나는 지금 김밥이 먹고 싶다고ㅋㅋㅋ",
          "original_form": "나는 지금 김밥이 먹고 싶다고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100037967.1.20",
          "form": "음성으로 하니까 무슨 말인지 잘 모르겠다ㅋㅋ",
          "original_form": "음성으로 하니까 무슨 말인지 잘 모르겠다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037967.1.21",
          "form": "ㅋㅋㅋㅋ아 웃겨",
          "original_form": "ㅋㅋㅋㅋ아 웃겨",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100037967.1.22",
          "form": "그러니깐ㅋㅋ",
          "original_form": "그러니깐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100037967.1.23",
          "form": "진미 언니랑 얘기하는데도 계속 오타나서",
          "original_form": "진미 언니랑 얘기하는데도 계속 오타나서",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100037967.1.24",
          "form": "계속 흐름 깨짐ㅋㅋ",
          "original_form": "계속 흐름 깨짐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100037967.1.25",
          "form": "둘째는 옆에서 놀고 있나?",
          "original_form": "둘째는 옆에서 놀고 있나?",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100037967.1.26",
          "form": "아 ㅋㅋㅋㅋ 웃겨 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ 웃겨 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100037967.1.27",
          "form": "첫째는 합기도 가고 잘자 제",
          "original_form": "첫째는 합기도 가고 잘자 제",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037967.1.28",
          "form": "응 잘간다 ㅋㅋ",
          "original_form": "응 잘간다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037967.1.29",
          "form": "재미잇다고 하더라 ㅋㅋ",
          "original_form": "재미잇다고 하더라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037967.1.30",
          "form": "근데 다닌지 얼마 안됐으니 ㅋㅋㅋ 잼있다고 해야지 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 다닌지 얼마 안됐으니 ㅋㅋㅋ 잼있다고 해야지 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:04"
        }
      ]
    }
