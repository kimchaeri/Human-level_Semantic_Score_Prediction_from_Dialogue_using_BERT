      "utterance": [
        {
          "id": "MDRW2100038902.1.1",
          "form": "여행 마지막으로 간게 언제유",
          "original_form": "여행 마지막으로 간게 언제유",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038902.1.2",
          "form": "어디 다른 지역으로 간건 작년 여름이 마지막....흑흑",
          "original_form": "어디 다른 지역으로 간건 작년 여름이 마지막....흑흑",
          "speaker_id": "1",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038902.1.3",
          "form": "사실 부모님이 여행가자고 얘기했는데 귀찮아서 안감ㅎ...",
          "original_form": "사실 부모님이 여행가자고 얘기했는데 귀찮아서 안감ㅎ...",
          "speaker_id": "1",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038902.1.4",
          "form": "세상에 혹시 부산,,,,?",
          "original_form": "세상에 혹시 부산,,,,?",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100038902.1.5",
          "form": "나도 작년이 마지막이야ㅎ",
          "original_form": "나도 작년이 마지막이야ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100038902.1.6",
          "form": "알바+코시국으로 어디 못가는...",
          "original_form": "알바+코시국으로 어디 못가는...",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100038902.1.7",
          "form": "ㅋㅋㅋㅋ귀차니즘이 이겼구나",
          "original_form": "ㅋㅋㅋㅋ귀차니즘이 이겼구나",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100038902.1.8",
          "form": "요새 경주 많이들 가서 경주 가고싶더라!",
          "original_form": "요새 경주 많이들 가서 경주 가고싶더라!",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100038902.1.9",
          "form": "아니 서울ㅋㅋㅋ맞아 경주 가깝기도 하고 경리단길도 있고",
          "original_form": "아니 서울ㅋㅋㅋ맞아 경주 가깝기도 하고 경리단길도 있고",
          "speaker_id": "1",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100038902.1.10",
          "form": "코로나가 좀 괜찮아지면 국내여행이라도 가고싶어유ㅠㅠ",
          "original_form": "코로나가 좀 괜찮아지면 국내여행이라도 가고싶어유ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100038902.1.11",
          "form": "앗 우리 같이 갔던 부산인가했지(코쓱",
          "original_form": "앗 우리 같이 갔던 부산인가했지(코쓱",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100038902.1.12",
          "form": "담에 경주 고고링?",
          "original_form": "담에 경주 고고링?",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100038902.1.13",
          "form": "맛집이랑 카페 많더라ㅠㅠ",
          "original_form": "맛집이랑 카페 많더라ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100038902.1.14",
          "form": "국내여행 좋쥐ㅠㅠ",
          "original_form": "국내여행 좋쥐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100038902.1.15",
          "form": "부산 뒤에 서울 갔었거든...ㅎ...그게 마지막일줄이야...",
          "original_form": "부산 뒤에 서울 갔었거든...ㅎ...그게 마지막일줄이야...",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100038902.1.16",
          "form": "담에 경주도 함 갑시다!!",
          "original_form": "담에 경주도 함 갑시다!!",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100038902.1.17",
          "form": "맞아 국내에도 좋은데 많더라규 여수도 통영도 전주도 대전도 여기저기 다 가보고 싶어ㅋㅋ",
          "original_form": "맞아 국내에도 좋은데 많더라규 여수도 통영도 전주도 대전도 여기저기 다 가보고 싶어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100038902.1.18",
          "form": "아앗...ㅠㅠ 훌쩍",
          "original_form": "아앗...ㅠㅠ 훌쩍",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100038902.1.19",
          "form": "좋아요~~!! 경주하니깐 우리 백금회",
          "original_form": "좋아요~~!! 경주하니깐 우리 백금회",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100038902.1.20",
          "form": "졸업하고 갔던거 생각난다",
          "original_form": "졸업하고 갔던거 생각난다",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100038902.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그것도 잘 기억안나지만",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그것도 잘 기억안나지만",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100038902.1.22",
          "form": "헐 나두 전주에 한복입고싶다",
          "original_form": "헐 나두 전주에 한복입고싶다",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100038902.1.23",
          "form": "아 맞아 그때 펜션잡고 놀았던거 재밌었지ㅠㅠ",
          "original_form": "아 맞아 그때 펜션잡고 놀았던거 재밌었지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100038902.1.24",
          "form": "맞아 한복입고 돌아다니기ㅋㅋ전주에 먹거리도 엄청 많고 한옥에도 한번 묵어보고 싶당",
          "original_form": "맞아 한복입고 돌아다니기ㅋㅋ전주에 먹거리도 엄청 많고 한옥에도 한번 묵어보고 싶당",
          "speaker_id": "1",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100038902.1.25",
          "form": "그때 정확히 뭘 했는지도 기억이 가물쓰",
          "original_form": "그때 정확히 뭘 했는지도 기억이 가물쓰",
          "speaker_id": "2",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100038902.1.26",
          "form": "다같이 전멸한 기억",
          "original_form": "다같이 전멸한 기억",
          "speaker_id": "2",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100038902.1.27",
          "form": "맞아ㅠㅠㅠㅠ한복입고 돌아다니기 우리 꼭 하자!!",
          "original_form": "맞아ㅠㅠㅠㅠ한복입고 돌아다니기 우리 꼭 하자!!",
          "speaker_id": "2",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100038902.1.28",
          "form": "1박2일 전주여행",
          "original_form": "1박2일 전주여행",
          "speaker_id": "2",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100038902.1.29",
          "form": "맞아ㅋㅋㅋ새벽까지 버티다가 하나둘 잠들었었지ㅋㅋㅋㅋ",
          "original_form": "맞아ㅋㅋㅋ새벽까지 버티다가 하나둘 잠들었었지ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100038902.1.30",
