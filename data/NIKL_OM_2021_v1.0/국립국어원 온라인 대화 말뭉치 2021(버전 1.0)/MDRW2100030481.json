      "utterance": [
        {
          "id": "MDRW2100030481.1.1",
          "form": "책 주제로 대화하는거 안할라다가 ㅋㅋㅋㅋ",
          "original_form": "책 주제로 대화하는거 안할라다가 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100030481.1.2",
          "form": "지옥의 영업타임이 될것 같지만",
          "original_form": "지옥의 영업타임이 될것 같지만",
          "speaker_id": "1",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100030481.1.3",
          "form": "카카오페이지에 다한아 무료분 다 봤는데",
          "original_form": "카카오페이지에 다한아 무료분 다 봤는데",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030481.1.4",
          "form": "이제 20편 남겨진거 쓰고있거든?",
          "original_form": "이제 20편 남겨진거 쓰고있거든?",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030481.1.5",
          "form": "대여권이다..",
          "original_form": "대여권이다..",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030481.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100030481.1.7",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100030481.1.8",
          "form": "그래서 감상은?",
          "original_form": "그래서 감상은?",
          "speaker_id": "2",
          "time": "20210904 01:56"
        },
        {
          "id": "MDRW2100030481.1.9",
          "form": "대여권일건 예상햇지만..",
          "original_form": "대여권일건 예상햇지만..",
          "speaker_id": "2",
          "time": "20210904 01:56"
        },
        {
          "id": "MDRW2100030481.1.10",
          "form": "감상이 필요했어?",
          "original_form": "감상이 필요했어?",
          "speaker_id": "1",
          "time": "20210904 01:57"
        },
        {
          "id": "MDRW2100030481.1.11",
          "form": "일단 데못죽이랑 비교를 하자면",
          "original_form": "일단 데못죽이랑 비교를 하자면",
          "speaker_id": "1",
          "time": "20210904 01:57"
        },
        {
          "id": "MDRW2100030481.1.12",
          "form": "문대보다 설명이 친절해",
          "original_form": "문대보다 설명이 친절해",
          "speaker_id": "1",
          "time": "20210904 01:57"
        },
        {
          "id": "MDRW2100030481.1.13",
          "form": "서술 트릭은 없음",
          "original_form": "서술 트릭은 없음",
          "speaker_id": "1",
          "time": "20210904 01:57"
        },
        {
          "id": "MDRW2100030481.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030481.1.15",
          "form": "좀 편하게 볼 수 잇겟다",
          "original_form": "좀 편하게 볼 수 잇겟다",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030481.1.16",
          "form": "대신에 음악적인 부분은",
          "original_form": "대신에 음악적인 부분은",
          "speaker_id": "1",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030481.1.17",
          "form": "백덕수 작가님이 잘 쓰셨어",
          "original_form": "백덕수 작가님이 잘 쓰셨어",
          "speaker_id": "1",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030481.1.18",
          "form": "가사라던가 무대 설명은 오히려 데못죽 쪽이 훌륭함",
          "original_form": "가사라던가 무대 설명은 오히려 데못죽 쪽이 훌륭함",
          "speaker_id": "1",
          "time": "20210904 01:59"
        },
        {
          "id": "MDRW2100030481.1.19",
          "form": "역시 백덕수씨..",
          "original_form": "역시 백덕수씨..",
          "speaker_id": "2",
          "time": "20210904 01:59"
        },
        {
          "id": "MDRW2100030481.1.20",
          "form": "돌판에 몸 담궈본게 틀림없다",
          "original_form": "돌판에 몸 담궈본게 틀림없다",
          "speaker_id": "2",
          "time": "20210904 01:59"
        },
        {
          "id": "MDRW2100030481.1.21",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210904 02:00"
        },
        {
          "id": "MDRW2100030481.1.22",
          "form": "그렇지 않고선 나올 수 없는 필력",
          "original_form": "그렇지 않고선 나올 수 없는 필력",
          "speaker_id": "1",
          "time": "20210904 02:00"
        },
        {
          "id": "MDRW2100030481.1.23",
          "form": "섬세하다가 틀어버린 그 묘사",
          "original_form": "섬세하다가 틀어버린 그 묘사",
          "speaker_id": "1",
          "time": "20210904 02:00"
        },
        {
          "id": "MDRW2100030481.1.24",
          "form": "그런면에선 작가님 훌륭하다고봄",
          "original_form": "그런면에선 작가님 훌륭하다고봄",
          "speaker_id": "1",
          "time": "20210904 02:00"
        },
        {
          "id": "MDRW2100030481.1.25",
          "form": "ㅋㅋㅋㅋ 단기간에 독자 그렇게 끌어모이기도 쉽지않은데",
          "original_form": "ㅋㅋㅋㅋ 단기간에 독자 그렇게 끌어모이기도 쉽지않은데",
          "speaker_id": "2",
          "time": "20210904 02:01"
        },
        {
          "id": "MDRW2100030481.1.26",
          "form": "이전에 다른 필명으러 활동하셧데도 믿겟는데",
          "original_form": "이전에 다른 필명으러 활동하셧데도 믿겟는데",
          "speaker_id": "2",
          "time": "20210904 02:01"
        },
        {
          "id": "MDRW2100030481.1.27",
          "form": "좋은 책은 좋은 책이라는거지 ㅋㅋㅋㅋ",
          "original_form": "좋은 책은 좋은 책이라는거지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:02"
        },
        {
          "id": "MDRW2100030481.1.28",
          "form": "그런의미에서 50편 열려있을때 보는거 추천해",
          "original_form": "그런의미에서 50편 열려있을때 보는거 추천해",
          "speaker_id": "1",
          "time": "20210904 02:02"
        },
        {
          "id": "MDRW2100030481.1.29",
          "form": "다른말로하면 데못죽보다 약간 루즈하게 느껴질 수도 있거든",
          "original_form": "다른말로하면 데못죽보다 약간 루즈하게 느껴질 수도 있거든",
          "speaker_id": "1",
          "time": "20210904 02:02"
        },
        {
          "id": "MDRW2100030481.1.30",
          "form": "아유 그런건 괜찮아여",
          "original_form": "아유 그런건 괜찮아여",
          "speaker_id": "2",
          "time": "20210904 02:03"
        },
        {
          "id": "MDRW2100030481.1.31",
          "form": "전개가 순서가 완전 반대야",
          "original_form": "전개가 순서가 완전 반대야",
          "speaker_id": "1",
          "time": "20210904 02:03"
        },
        {
          "id": "MDRW2100030481.1.32",
          "form": "오히려 데못죽은 진도가 스피디한 느낌이라",
          "original_form": "오히려 데못죽은 진도가 스피디한 느낌이라",
          "speaker_id": "2",
          "time": "20210904 02:03"
        },
        {
          "id": "MDRW2100030481.1.33",
          "form": "문대는 서바이벌 출신인데",
          "original_form": "문대는 서바이벌 출신인데",
          "speaker_id": "1",
          "time": "20210904 02:04"
        },
        {
          "id": "MDRW2100030481.1.34",
          "form": "이친구는 연습생에서 서바이벌로 건너간 케이스거든",
          "original_form": "이친구는 연습생에서 서바이벌로 건너간 케이스거든",
          "speaker_id": "1",
          "time": "20210904 02:04"
        },
        {
          "id": "MDRW2100030481.1.35",
          "form": "계속 책 이야기나 하면 될 것 같아",
          "original_form": "계속 책 이야기나 하면 될 것 같아",
