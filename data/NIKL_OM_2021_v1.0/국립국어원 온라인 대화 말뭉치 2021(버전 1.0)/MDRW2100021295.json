      "utterance": [
        {
          "id": "MDRW2100021295.1.1",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "1",
          "time": "20210901 09:52"
        },
        {
          "id": "MDRW2100021295.1.2",
          "form": "아뇨 없어요 ~",
          "original_form": "아뇨 없어요 ~",
          "speaker_id": "2",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100021295.1.3",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "2",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100021295.1.4",
          "form": "아뇨ㅋㅋ",
          "original_form": "아뇨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100021295.1.5",
          "form": "부모님이 불교 믿으셔서 절을 가끔 가긴 했어요",
          "original_form": "부모님이 불교 믿으셔서 절을 가끔 가긴 했어요",
          "speaker_id": "1",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100021295.1.6",
          "form": "절가서",
          "original_form": "절가서",
          "speaker_id": "2",
          "time": "20210901 10:02"
        },
        {
          "id": "MDRW2100021295.1.7",
          "form": "뭐하세요?",
          "original_form": "뭐하세요?",
          "speaker_id": "2",
          "time": "20210901 10:02"
        },
        {
          "id": "MDRW2100021295.1.8",
          "form": "절올리고 그러는데 전 구경하고 그랬죠",
          "original_form": "절올리고 그러는데 전 구경하고 그랬죠",
          "speaker_id": "1",
          "time": "20210901 10:08"
        },
        {
          "id": "MDRW2100021295.1.9",
          "form": "템플스테이는 재밌었어요",
          "original_form": "템플스테이는 재밌었어요",
          "speaker_id": "1",
          "time": "20210901 10:08"
        },
        {
          "id": "MDRW2100021295.1.10",
          "form": "해보신적 있나요?",
          "original_form": "해보신적 있나요?",
          "speaker_id": "1",
          "time": "20210901 10:09"
        },
        {
          "id": "MDRW2100021295.1.11",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210901 10:11"
        },
        {
          "id": "MDRW2100021295.1.12",
          "form": "저 해보고싶어요!!",
          "original_form": "저 해보고싶어요!!",
          "speaker_id": "2",
          "time": "20210901 10:11"
        },
        {
          "id": "MDRW2100021295.1.13",
          "form": "마음수양으로 진지하게 하는 것도 있는데",
          "original_form": "마음수양으로 진지하게 하는 것도 있는데",
          "speaker_id": "1",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100021295.1.14",
          "form": "전 그냥 노는것처럼 하는 게 좋아서ㅋㅋ",
          "original_form": "전 그냥 노는것처럼 하는 게 좋아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100021295.1.15",
          "form": "아하ㅋㅋㅋ 혹시",
          "original_form": "아하ㅋㅋㅋ 혹시",
          "speaker_id": "2",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100021295.1.16",
          "form": "밥은.어떤가요",
          "original_form": "밥은.어떤가요",
          "speaker_id": "2",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100021295.1.17",
          "form": "절밥이 맛있다고해서요",
          "original_form": "절밥이 맛있다고해서요",
          "speaker_id": "2",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100021295.1.18",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "1",
          "time": "20210901 11:24"
        },
        {
          "id": "MDRW2100021295.1.19",
          "form": "자극적인 맛은 아닌데 다 맛있더라구요",
          "original_form": "자극적인 맛은 아닌데 다 맛있더라구요",
          "speaker_id": "1",
          "time": "20210901 11:24"
        },
        {
          "id": "MDRW2100021295.1.20",
          "form": "으아아아아아아",
          "original_form": "으아아아아아아",
          "speaker_id": "2",
          "time": "20210901 11:25"
        },
        {
          "id": "MDRW2100021295.1.21",
          "form": "먹고싶네요...",
          "original_form": "먹고싶네요...",
          "speaker_id": "2",
          "time": "20210901 11:25"
        },
        {
          "id": "MDRW2100021295.1.22",
          "form": "한번쯤 가보셔야겠네요ㅋㅋ 요샌 비슷한 음식으로 식당하는 집도 있더라구요",
          "original_form": "한번쯤 가보셔야겠네요ㅋㅋ 요샌 비슷한 음식으로 식당하는 집도 있더라구요",
          "speaker_id": "1",
          "time": "20210901 11:26"
        },
        {
          "id": "MDRW2100021295.1.23",
          "form": "아 진짜요??!??!??",
          "original_form": "아 진짜요??!??!??",
          "speaker_id": "2",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100021295.1.24",
          "form": "그렁 식당이 있어요???!",
          "original_form": "그렁 식당이 있어요???!",
          "speaker_id": "2",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100021295.1.25",
          "form": "네 사찰음식으로 찾아보시면 있을거에요",
          "original_form": "네 사찰음식으로 찾아보시면 있을거에요",
          "speaker_id": "1",
          "time": "20210901 11:30"
        },
        {
          "id": "MDRW2100021295.1.26",
          "form": "웰빙이나 비건으로 찾는사람도 있더라구요",
          "original_form": "웰빙이나 비건으로 찾는사람도 있더라구요",
          "speaker_id": "1",
          "time": "20210901 11:30"
        },
        {
          "id": "MDRW2100021295.1.27",
          "form": "ㅎ 한번 찾아봐야겠네요",
          "original_form": "ㅎ 한번 찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210901 12:05"
        },
        {
          "id": "MDRW2100021295.1.28",
          "form": "떨리네요",
          "original_form": "떨리네요",
          "speaker_id": "2",
