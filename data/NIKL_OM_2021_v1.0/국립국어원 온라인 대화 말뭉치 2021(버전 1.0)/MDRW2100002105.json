      "utterance": [
        {
          "id": "MDRW2100002105.1.1",
          "form": "안녕하세요. 육퇴하고보니 심심해서요!",
          "original_form": "안녕하세요. 육퇴하고보니 심심해서요!",
          "speaker_id": "1",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002105.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210820 10:05"
        },
        {
          "id": "MDRW2100002105.1.3",
          "form": "육퇴가 육아퇴직 맞나요?",
          "original_form": "육퇴가 육아퇴직 맞나요?",
          "speaker_id": "2",
          "time": "20210820 10:05"
        },
        {
          "id": "MDRW2100002105.1.4",
          "form": "아 육아퇴근이에요ㅎㅎ",
          "original_form": "아 육아퇴근이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 10:18"
        },
        {
          "id": "MDRW2100002105.1.5",
          "form": "밤늦게 퇴근했다는 ㅎㅎ",
          "original_form": "밤늦게 퇴근했다는 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 10:18"
        },
        {
          "id": "MDRW2100002105.1.6",
          "form": "아앗.. 퇴직자로 만들어버렸네요 제가",
          "original_form": "아앗.. 퇴직자로 만들어버렸네요 제가",
          "speaker_id": "2",
          "time": "20210820 10:18"
        },
        {
          "id": "MDRW2100002105.1.7",
          "form": "아기는 몇 개월이에요??!",
          "original_form": "아기는 몇 개월이에요??!",
          "speaker_id": "2",
          "time": "20210820 10:18"
        },
        {
          "id": "MDRW2100002105.1.8",
          "form": "휴직중입니다ㅎ",
          "original_form": "휴직중입니다ㅎ",
          "speaker_id": "1",
          "time": "20210820 10:18"
        },
        {
          "id": "MDRW2100002105.1.9",
          "form": "19개월이요",
          "original_form": "19개월이요",
          "speaker_id": "1",
          "time": "20210820 10:19"
        },
        {
          "id": "MDRW2100002105.1.10",
          "form": "육아 힘드시겠어요,,,",
          "original_form": "육아 힘드시겠어요,,,",
          "speaker_id": "2",
          "time": "20210820 10:20"
        },
        {
          "id": "MDRW2100002105.1.11",
          "form": "힘들죠ㅜㅜ 아직 애기는 없으세요?",
          "original_form": "힘들죠ㅜㅜ 아직 애기는 없으세요?",
          "speaker_id": "1",
          "time": "20210820 10:20"
        },
        {
          "id": "MDRW2100002105.1.12",
          "form": "저는 친척중에서도 동생이 없어서 아이를 돌본 적이 없는데",
          "original_form": "저는 친척중에서도 동생이 없어서 아이를 돌본 적이 없는데",
          "speaker_id": "2",
          "time": "20210820 10:20"
        },
        {
          "id": "MDRW2100002105.1.13",
          "form": "실례지만 결혼하신지부터 여쭤봣어야했네요ㅎㅎ",
          "original_form": "실례지만 결혼하신지부터 여쭤봣어야했네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 10:21"
        },
        {
          "id": "MDRW2100002105.1.14",
          "form": "저는 아직 결혼도 안했어요 ㅎㅎㅎ",
          "original_form": "저는 아직 결혼도 안했어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 10:21"
        },
        {
          "id": "MDRW2100002105.1.15",
          "form": "아이는 한명이에요?",
          "original_form": "아이는 한명이에요?",
          "speaker_id": "2",
          "time": "20210820 10:21"
        },
        {
          "id": "MDRW2100002105.1.16",
          "form": "한명인데 다음달 출산예정이에요ㅎ",
          "original_form": "한명인데 다음달 출산예정이에요ㅎ",
          "speaker_id": "1",
          "time": "20210820 10:22"
        },
        {
          "id": "MDRW2100002105.1.17",
          "form": "허얼 축하드려요!!",
          "original_form": "허얼 축하드려요!!",
          "speaker_id": "2",
          "time": "20210820 10:23"
        },
        {
          "id": "MDRW2100002105.1.18",
          "form": "져는 주위에 애가 있는 친구나 선배들도 없어서 조금 신기하고 뭔가 정말 어른같으세요... ㅋㅋㅋㅋㅋ",
          "original_form": "져는 주위에 애가 있는 친구나 선배들도 없어서 조금 신기하고 뭔가 정말 어른같으세요... ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 10:23"
        },
        {
          "id": "MDRW2100002105.1.19",
          "form": "별말씀을요ㅋ많이 놀다가 결혼하세요",
          "original_form": "별말씀을요ㅋ많이 놀다가 결혼하세요",
          "speaker_id": "1",
          "time": "20210820 10:58"
        },
        {
          "id": "MDRW2100002105.1.20",
          "form": "충분히 논거같은데도 아쉬워요ㅎ",
          "original_form": "충분히 논거같은데도 아쉬워요ㅎ",
          "speaker_id": "1",
          "time": "20210820 10:58"
        },
        {
          "id": "MDRW2100002105.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ충고 잘 들을게요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ충고 잘 들을게요!",
          "speaker_id": "2",
          "time": "20210820 11:13"
        },
        {
          "id": "MDRW2100002105.1.22",
          "form": "그럼 지금은 가족 구성이 어떻게 되세요??",
          "original_form": "그럼 지금은 가족 구성이 어떻게 되세요??",
          "speaker_id": "2",
          "time": "20210820 11:13"
        },
        {
          "id": "MDRW2100002105.1.23",
          "form": "누구랑 같이 사시나욥?",
          "original_form": "누구랑 같이 사시나욥?",
          "speaker_id": "2",
          "time": "20210820 11:13"
        },
        {
          "id": "MDRW2100002105.1.24",
          "form": "저는 신랑이랑 아기랑요",
          "original_form": "저는 신랑이랑 아기랑요",
          "speaker_id": "1",
          "time": "20210820 11:13"
        },
        {
          "id": "MDRW2100002105.1.25",
          "form": "아아 저는 자취하다가 다시 부모님이랑 같이 살아요",
          "original_form": "아아 저는 자취하다가 다시 부모님이랑 같이 살아요",
          "speaker_id": "2",
          "time": "20210820 11:17"
        },
        {
          "id": "MDRW2100002105.1.26",
          "form": "가사 진짜 어렵더라고요ㅠㅋㅋㅋㅋㅋ 청소하면 또 청소할 게 생겨요..",
          "original_form": "가사 진짜 어렵더라고요ㅠㅋㅋㅋㅋㅋ 청소하면 또 청소할 게 생겨요..",
          "speaker_id": "2",
          "time": "20210820 11:17"
        },
        {
          "id": "MDRW2100002105.1.27",
          "form": "저도 부모님이랑 살다 나와사니",
          "original_form": "저도 부모님이랑 살다 나와사니",
          "speaker_id": "1",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100002105.1.28",
          "form": "하루종일 청소만해ㅇᆞ",
          "original_form": "하루종일 청소만해ㅇᆞ",
          "speaker_id": "1",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100002105.1.29",
          "form": "어우 진짜 힘드시겠어요",
          "original_form": "어우 진짜 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100002105.1.30",
          "form": "저도 오늘 청소 하려는데 또 엄두가 안나네요..",
          "original_form": "저도 오늘 청소 하려는데 또 엄두가 안나네요..",
          "speaker_id": "2",
          "time": "20210820 11:41"
        }
      ]
