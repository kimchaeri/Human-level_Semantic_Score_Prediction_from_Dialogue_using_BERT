      "utterance": [
        {
          "id": "MDRW2100011139.1.1",
          "form": "아르바이트 해본 적 있어?",
          "original_form": "아르바이트 해본 적 있어?",
          "speaker_id": "1",
          "time": "20210824 01:35"
        },
        {
          "id": "MDRW2100011139.1.2",
          "form": "과외만 해봤어!",
          "original_form": "과외만 해봤어!",
          "speaker_id": "2",
          "time": "20210824 01:35"
        },
        {
          "id": "MDRW2100011139.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210824 01:35"
        },
        {
          "id": "MDRW2100011139.1.4",
          "form": "나는 지금 과외 중이야!!",
          "original_form": "나는 지금 과외 중이야!!",
          "speaker_id": "1",
          "time": "20210824 01:37"
        },
        {
          "id": "MDRW2100011139.1.5",
          "form": "초6 과외 하는데 3월 부터 했다 ㅎㅎ",
          "original_form": "초6 과외 하는데 3월 부터 했다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:37"
        },
        {
          "id": "MDRW2100011139.1.6",
          "form": "초6 과외 좋은거 같아!! 중고등에 덜 부담스럽고 ㅎㅎ",
          "original_form": "초6 과외 좋은거 같아!! 중고등에 덜 부담스럽고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 01:38"
        },
        {
          "id": "MDRW2100011139.1.7",
          "form": "맞아 나는 고등들어오면 거절 해 ㅎㅎ 부담 스러워서ㅠㅠ",
          "original_form": "맞아 나는 고등들어오면 거절 해 ㅎㅎ 부담 스러워서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 01:38"
        },
        {
          "id": "MDRW2100011139.1.8",
          "form": "일단 시험을 안치니까!",
          "original_form": "일단 시험을 안치니까!",
          "speaker_id": "1",
          "time": "20210824 01:38"
        },
        {
          "id": "MDRW2100011139.1.9",
          "form": "어느 과목 가르쳐?",
          "original_form": "어느 과목 가르쳐?",
          "speaker_id": "2",
          "time": "20210824 01:38"
        },
        {
          "id": "MDRW2100011139.1.10",
          "form": "나는 영어 수학 두개 가르쳐  주 3회~",
          "original_form": "나는 영어 수학 두개 가르쳐  주 3회~",
          "speaker_id": "1",
          "time": "20210824 01:39"
        },
        {
          "id": "MDRW2100011139.1.11",
          "form": "너는 뭐 가르쳤어?",
          "original_form": "너는 뭐 가르쳤어?",
          "speaker_id": "1",
          "time": "20210824 01:39"
        },
        {
          "id": "MDRW2100011139.1.12",
          "form": "나는 영어 가르쳤어!!",
          "original_form": "나는 영어 가르쳤어!!",
          "speaker_id": "2",
          "time": "20210824 01:39"
        },
        {
          "id": "MDRW2100011139.1.13",
          "form": "초등학생이 시험을 안쳐?? 왜???",
          "original_form": "초등학생이 시험을 안쳐?? 왜???",
          "speaker_id": "2",
          "time": "20210824 01:40"
        },
        {
          "id": "MDRW2100011139.1.14",
          "form": "평가라고 할게 없다고 하는게 맞는 것 같아 ㅎㅎ",
          "original_form": "평가라고 할게 없다고 하는게 맞는 것 같아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:40"
        },
        {
          "id": "MDRW2100011139.1.15",
          "form": "20문제 풀고 말더라고~",
          "original_form": "20문제 풀고 말더라고~",
          "speaker_id": "1",
          "time": "20210824 01:41"
        },
        {
          "id": "MDRW2100011139.1.16",
          "form": "초등은 저학년만 가르쳐봐서 잘 모르겠네 ㅎㅎㅎ",
          "original_form": "초등은 저학년만 가르쳐봐서 잘 모르겠네 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 01:41"
        },
        {
          "id": "MDRW2100011139.1.17",
          "form": "영어만 가르치는 거야?",
          "original_form": "영어만 가르치는 거야?",
          "speaker_id": "1",
          "time": "20210824 01:41"
        },
        {
          "id": "MDRW2100011139.1.18",
          "form": "역사도 특강으로 가르쳐봤는데 준비 시간이 오래걸려서 그냥 영어만 하고 있어!",
          "original_form": "역사도 특강으로 가르쳐봤는데 준비 시간이 오래걸려서 그냥 영어만 하고 있어!",
          "speaker_id": "2",
          "time": "20210824 01:42"
        },
        {
          "id": "MDRW2100011139.1.19",
          "form": "오 영어 잘하는 구나 멋져!",
          "original_form": "오 영어 잘하는 구나 멋져!",
          "speaker_id": "1",
          "time": "20210824 01:44"
        }
      ]
