      "utterance": [
        {
          "id": "MDRW2100043881.1.1",
          "form": "괜찮아용 저녁은 드셨나요?",
          "original_form": "괜찮아용 저녁은 드셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043881.1.2",
          "form": "네! 저녁먹었습니당",
          "original_form": "네! 저녁먹었습니당",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043881.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043881.1.4",
          "form": "저두용",
          "original_form": "저두용",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043881.1.5",
          "form": "어떤거 드셨어요?",
          "original_form": "어떤거 드셨어요?",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043881.1.6",
          "form": "저는 콩나물국에 고추잡채먹었어요ㅎ",
          "original_form": "저는 콩나물국에 고추잡채먹었어요ㅎ",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043881.1.7",
          "form": "name1님은 뭐드셨어요?",
          "original_form": "&name1&님은 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043881.1.8",
          "form": "와 고추잡채 저 엄청 좋아해요ㅋㅋ",
          "original_form": "와 고추잡채 저 엄청 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043881.1.9",
          "form": "저는 불닭크림리조또 먹었어요 ㅎㅎ",
          "original_form": "저는 불닭크림리조또 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043881.1.10",
          "form": "요즘 불닭관련요리 드시는 분들이 많네요!",
          "original_form": "요즘 불닭관련요리 드시는 분들이 많네요!",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043881.1.11",
          "form": "저는 매운걸 못먹어서 불닭 한 번도 못먹어봤어요ㅜ",
          "original_form": "저는 매운걸 못먹어서 불닭 한 번도 못먹어봤어요ㅜ",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043881.1.12",
          "form": "불닭소스 쫌만 넣어도 맛있더라구요 ㅎㅎ!",
          "original_form": "불닭소스 쫌만 넣어도 맛있더라구요 ㅎㅎ!",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043881.1.13",
          "form": "약간 불닭소스는 만능인 것 같아요..",
          "original_form": "약간 불닭소스는 만능인 것 같아요..",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043881.1.14",
          "form": "저도 눈 딱 한 번 감고 불닭소스 넣어서 요리해먹어볼까나...",
          "original_form": "저도 눈 딱 한 번 감고 불닭소스 넣어서 요리해먹어볼까나...",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043881.1.15",
          "form": "어느정도 맵기인지 모르겠어서 애매하네요ㅜ",
          "original_form": "어느정도 맵기인지 모르겠어서 애매하네요ㅜ",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043881.1.16",
          "form": "매운거 못드시면 반숟가락 정도만 넣으셔도 될 것 같아요!",
          "original_form": "매운거 못드시면 반숟가락 정도만 넣으셔도 될 것 같아요!",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043881.1.17",
          "form": "오오 꿀팁 감사해요!",
          "original_form": "오오 꿀팁 감사해요!",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043881.1.18",
          "form": "네네 담에 기회되심 해먹어 보셔요 ㅎㅎ",
          "original_form": "네네 담에 기회되심 해먹어 보셔요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043881.1.19",
          "form": "네!! 그래야겠어요ㅋㅋㅋ",
          "original_form": "네!! 그래야겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043881.1.20",
          "form": "매운거 도전   .....!!",
          "original_form": "매운거 도전   .....!!",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043881.1.21",
          "form": "네네 좋아용 ㅋㅋ 도전 ..!",
