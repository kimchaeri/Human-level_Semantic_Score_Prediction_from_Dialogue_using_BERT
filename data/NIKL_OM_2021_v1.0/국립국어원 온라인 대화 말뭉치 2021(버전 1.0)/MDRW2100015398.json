      "utterance": [
        {
          "id": "MDRW2100015398.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015398.1.2",
          "form": "네 영화 좋아해요!",
          "original_form": "네 영화 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015398.1.3",
          "form": "좋아하는 영화장르 있으신가요?",
          "original_form": "좋아하는 영화장르 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015398.1.4",
          "form": "로코요",
          "original_form": "로코요",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015398.1.5",
          "form": "저도 로코 좋아요!",
          "original_form": "저도 로코 좋아요!",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015398.1.6",
          "form": "로코 좋아하시면 어바웃타임도 좋아하시겠어요~",
          "original_form": "로코 좋아하시면 어바웃타임도 좋아하시겠어요~",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015398.1.7",
          "form": "본 거 같은데",
          "original_form": "본 거 같은데",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015398.1.8",
          "form": "극장가서 보신 영화 중에 기억에 남는 영화 있으신가요?",
          "original_form": "극장가서 보신 영화 중에 기억에 남는 영화 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015398.1.9",
          "form": "아마 보셨을꺼에요 빨간 드레스입고 비맞는? 그런 포스터에요",
          "original_form": "아마 보셨을꺼에요 빨간 드레스입고 비맞는? 그런 포스터에요",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015398.1.10",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015398.1.11",
          "form": "기억 나시나요?",
          "original_form": "기억 나시나요?",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015398.1.12",
          "form": "전 이영화 영화관에서 봤음 하는 아쉬움이 있어요 ㅠㅠ",
          "original_form": "전 이영화 영화관에서 봤음 하는 아쉬움이 있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015398.1.13",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210826 06:34"
        },
        {
          "id": "MDRW2100015398.1.14",
          "form": "넷플해요?",
          "original_form": "넷플해요?",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100015398.1.15",
          "form": "네 드라마 보려고 구독중이에요",
          "original_form": "네 드라마 보려고 구독중이에요",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100015398.1.16",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100015398.1.17",
          "form": "이전에는 라켓소년단 봤구요 요즘은 슬기로운 의사생활 간간히 보고있어요",
          "original_form": "이전에는 라켓소년단 봤구요 요즘은 슬기로운 의사생활 간간히 보고있어요",
          "speaker_id": "2",
          "time": "20210901 07:47"
        },
        {
          "id": "MDRW2100015398.1.18",
          "form": "넷플릭스에서 볼만한거 추천해주실 수 있나요??",
          "original_form": "넷플릭스에서 볼만한거 추천해주실 수 있나요??",
          "speaker_id": "2",
          "time": "20210901 07:47"
        },
        {
          "id": "MDRW2100015398.1.19",
          "form": "모가디슈볼려구요",
          "original_form": "모가디슈볼려구요",
          "speaker_id": "1",
          "time": "20210901 07:47"
        }
      ]
