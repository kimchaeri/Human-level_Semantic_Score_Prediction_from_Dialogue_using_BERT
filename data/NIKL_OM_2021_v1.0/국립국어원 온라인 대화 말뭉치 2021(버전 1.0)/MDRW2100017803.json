      "utterance": [
        {
          "id": "MDRW2100017803.1.1",
          "form": "붕어빵 좋아하시나요?",
          "original_form": "붕어빵 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100017803.1.2",
          "form": "헉 네 너무좋아해요 !!",
          "original_form": "헉 네 너무좋아해요 !!",
          "speaker_id": "2",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100017803.1.3",
          "form": "name1은 어떠셔요",
          "original_form": "&name1&은 어떠셔요",
          "speaker_id": "2",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100017803.1.4",
          "form": "저도 좋아해용 겨울을 기다리고 있어요 !",
          "original_form": "저도 좋아해용 겨울을 기다리고 있어요 !",
          "speaker_id": "1",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100017803.1.5",
          "form": "팥붕파이신가요 슈붕파이신가요",
          "original_form": "팥붕파이신가요 슈붕파이신가요",
          "speaker_id": "1",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100017803.1.6",
          "form": "크으 전 팥붕입니다",
          "original_form": "크으 전 팥붕입니다",
          "speaker_id": "2",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100017803.1.7",
          "form": "제마음속의 척화비",
          "original_form": "제마음속의 척화비",
          "speaker_id": "2",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100017803.1.8",
          "form": "저는 .. 둘다 좋지만 굳이 따지자면 슈붕파입니다..",
          "original_form": "저는 .. 둘다 좋지만 굳이 따지자면 슈붕파입니다..",
          "speaker_id": "1",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100017803.1.9",
          "form": "앗..... 안맞았군요...",
          "original_form": "앗..... 안맞았군요...",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100017803.1.10",
          "form": "그렇다면",
          "original_form": "그렇다면",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100017803.1.11",
          "form": "딱복 vs 물복!",
          "original_form": "딱복 vs 물복!",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100017803.1.12",
          "form": "물복이지만 사실 딱복도 먹긴해요..!",
          "original_form": "물복이지만 사실 딱복도 먹긴해요..!",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100017803.1.13",
          "form": "저는 그냥 다먹일까요..?",
          "original_form": "저는 그냥 다먹일까요..?",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100017803.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 다먹 ㅋㅋㅋㅋㅋㅋ 넘 귀여워요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 다먹 ㅋㅋㅋㅋㅋㅋ 넘 귀여워요",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100017803.1.15",
          "form": "저고 물복이에요 후후",
          "original_form": "저고 물복이에요 후후",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100017803.1.16",
          "form": "물복에 요거트랑 섞어먹으면 크으으",
          "original_form": "물복에 요거트랑 섞어먹으면 크으으",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100017803.1.17",
          "form": "맛있죠... 그래도 물복인 것 같아요 단맛없는 딱복 잘못걸리면 최악인거죠 ..",
          "original_form": "맛있죠... 그래도 물복인 것 같아요 단맛없는 딱복 잘못걸리면 최악인거죠 ..",
          "speaker_id": "1",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100017803.1.18",
          "form": "아악 상상해버렸어요",
          "original_form": "아악 상상해버렸어요",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100017803.1.19",
          "form": "진짜 이빨다나갈줄아러 텊쿠키..",
          "original_form": "진짜 이빨다나갈줄아러 텊쿠키..",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100017803.1.20",
          "form": "정말 조심해야해요..",
          "original_form": "정말 조심해야해요..",
          "speaker_id": "1",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100017803.1.21",
          "form": "혹시 밤고구마 vs 군고구마 무슨 파이신가요",
          "original_form": "혹시 밤고구마 vs 군고구마 무슨 파이신가요",
          "speaker_id": "1",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100017803.1.22",
          "form": "헐 전 둘다 겁나 사랑하는데",
          "original_form": "헐 전 둘다 겁나 사랑하는데",
          "speaker_id": "2",
          "time": "20210827 16:21"
        },
        {
          "id": "MDRW2100017803.1.23",
          "form": "역대급의 문제네요....",
          "original_form": "역대급의 문제네요....",
          "speaker_id": "2",
          "time": "20210827 16:21"
        },
        {
          "id": "MDRW2100017803.1.24",
          "form": "하지만 역시 전 흥선대원군이라 군고구마....",
          "original_form": "하지만 역시 전 흥선대원군이라 군고구마....",
          "speaker_id": "2",
          "time": "20210827 16:21"
        },
        {
          "id": "MDRW2100017803.1.25",
          "form": "하지만 밤고구마도 달고맛나죠....",
          "original_form": "하지만 밤고구마도 달고맛나죠....",
          "speaker_id": "2",
          "time": "20210827 16:21"
        },
        {
          "id": "MDRW2100017803.1.26",
          "form": "군고구마일때는 호박고구마.. 아니면 저는 밤고구마요..",
          "original_form": "군고구마일때는 호박고구마.. 아니면 저는 밤고구마요..",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100017803.1.27",
          "form": "근데 둘다 진짜 맛있죠..",
          "original_form": "근데 둘다 진짜 맛있죠..",
