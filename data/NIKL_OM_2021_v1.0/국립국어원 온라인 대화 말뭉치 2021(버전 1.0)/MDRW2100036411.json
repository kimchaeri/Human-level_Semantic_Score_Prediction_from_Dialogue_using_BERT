      "utterance": [
        {
          "id": "MDRW2100036411.1.1",
          "form": "야식드세요??",
          "original_form": "야식드세요??",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036411.1.2",
          "form": "헉 !",
          "original_form": "헉 !",
          "speaker_id": "2",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100036411.1.3",
          "form": "지금??혹시??",
          "original_form": "지금??혹시??",
          "speaker_id": "1",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100036411.1.4",
          "form": "어케 아셨지..  멸치와 나쵸먹구있었는데",
          "original_form": "어케 아셨지..  멸치와 나쵸먹구있었는데",
          "speaker_id": "2",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100036411.1.5",
          "form": "ㅋㅋㅋㅋㅋ 지금 딱 야식 땡길시간이져",
          "original_form": "ㅋㅋㅋㅋㅋ 지금 딱 야식 땡길시간이져",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036411.1.6",
          "form": "한의원선생님께 비밀로해주세요ㅋㅋㅋ 저녁에 많이먹지말라구했어요",
          "original_form": "한의원선생님께 비밀로해주세요ㅋㅋㅋ 저녁에 많이먹지말라구했어요",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036411.1.7",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036411.1.8",
          "form": "어딥니까 거기 전화 한통 해야겠네여 ㅎ",
          "original_form": "어딥니까 거기 전화 한통 해야겠네여 ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036411.1.9",
          "form": "저는 콜라한잔...",
          "original_form": "저는 콜라한잔...",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036411.1.10",
          "form": "ㅋㅋ 콜라라니액상과당이 얼마나 무서운데요 name1님네 한의원은어딥니까 전화넣어야겠어요",
          "original_form": "ㅋㅋ 콜라라니액상과당이 얼마나 무서운데요 &name1&님네 한의원은어딥니까 전화넣어야겠어요",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036411.1.11",
          "form": "전 한의원 안다녀요 한의원 왜 다니시나요 ㅎㅎ",
          "original_form": "전 한의원 안다녀요 한의원 왜 다니시나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036411.1.12",
          "form": "설마 다이나마이트??",
          "original_form": "설마 다이나마이트??",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036411.1.13",
          "form": "ㅋㅋ오설마요",
          "original_form": "ㅋㅋ오설마요",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036411.1.14",
          "form": "배가아파서 진단받았죠 몸이 허해서 한약받았어요",
          "original_form": "배가아파서 진단받았죠 몸이 허해서 한약받았어요",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036411.1.15",
          "form": "아 허하신 분이시구나..",
          "original_form": "아 허하신 분이시구나..",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036411.1.16",
          "form": "이제 약먹고 다나앗습니다",
          "original_form": "이제 약먹고 다나앗습니다",
          "speaker_id": "2",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100036411.1.17",
          "form": "전 너무 건강해서 ㅋㅋ 쫌 뺴야는데 안되네여 세상에 너무 맛있는것들이 많아서ㅠㅠ",
          "original_form": "전 너무 건강해서 ㅋㅋ 쫌 뺴야는데 안되네여 세상에 너무 맛있는것들이 많아서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100036411.1.18",
          "form": "기능이 멀쩡한데 머가문제에요 딱좋죠ㅋ",
          "original_form": "기능이 멀쩡한데 머가문제에요 딱좋죠ㅋ",
          "speaker_id": "2",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100036411.1.19",
          "form": "음 내실이 어떨지 모르겠어요 ㅎㅎ",
          "original_form": "음 내실이 어떨지 모르겠어요 ㅎㅎ",
