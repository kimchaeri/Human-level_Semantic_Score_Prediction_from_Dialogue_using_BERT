        },
        {
          "id": "MDRW2100022426.1.3",
          "form": "오호 ㅎㅎ",
          "original_form": "오호 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100022426.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 촌사람 진짜 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 촌사람 진짜 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100022426.1.5",
          "form": "김치 좀 달라하지",
          "original_form": "김치 좀 달라하지",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022426.1.6",
          "form": "김치도 밥도 없데요 ㅋㅋㅋㅋ",
          "original_form": "김치도 밥도 없데요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100022426.1.7",
          "form": "물어봤죠 이미 ㅋㅋㅋ",
          "original_form": "물어봤죠 이미 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100022426.1.8",
          "form": "웁스....ㅋㅋㅋㅋㅋㅋㅋㅋ",
