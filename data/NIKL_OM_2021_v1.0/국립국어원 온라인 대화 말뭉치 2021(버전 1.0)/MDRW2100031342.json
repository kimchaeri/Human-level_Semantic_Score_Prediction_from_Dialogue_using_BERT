      "utterance": [
        {
          "id": "MDRW2100031342.1.1",
          "form": "머였지?",
          "original_form": "머였지?",
          "speaker_id": "2",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100031342.1.2",
          "form": "아 만두?",
          "original_form": "아 만두?",
          "speaker_id": "2",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100031342.1.3",
          "form": "ㅇㅇ만두랑 떡",
          "original_form": "ㅇㅇ만두랑 떡",
          "speaker_id": "1",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100031342.1.4",
          "form": "대박 진짜 빨리왔다",
          "original_form": "대박 진짜 빨리왔다",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031342.1.5",
          "form": "냉동실에 다행히 다 들어가더라",
          "original_form": "냉동실에 다행히 다 들어가더라",
          "speaker_id": "1",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031342.1.6",
          "form": "내가 냉동실 비워서그래",
          "original_form": "내가 냉동실 비워서그래",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031342.1.7",
          "form": "소시지떡이랑 멘보샤 다 돌려서 아까",
          "original_form": "소시지떡이랑 멘보샤 다 돌려서 아까",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031342.1.8",
          "form": "잘했다",
          "original_form": "잘했다",
          "speaker_id": "1",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031342.1.9",
          "form": "그거 다 먹느라 혼났다ㅋㅋ",
          "original_form": "그거 다 먹느라 혼났다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100031342.1.10",
          "form": "왜 얼마 안 남아서 어차피 먹어야하는 거였어",
          "original_form": "왜 얼마 안 남아서 어차피 먹어야하는 거였어",
          "speaker_id": "2",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100031342.1.11",
          "form": "다른 잉간들이 양심없게 안 먹어서 그랬지",
          "original_form": "다른 잉간들이 양심없게 안 먹어서 그랬지",
          "speaker_id": "2",
          "time": "20210904 20:43"
        },
        {
          "id": "MDRW2100031342.1.12",
          "form": "그러게 안먹드라 웃기는 사람들이",
          "original_form": "그러게 안먹드라 웃기는 사람들이",
          "speaker_id": "1",
          "time": "20210904 20:43"
        },
        {
          "id": "MDRW2100031342.1.13",
          "form": "진짜 어이없었음",
          "original_form": "진짜 어이없었음",
          "speaker_id": "2",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100031342.1.14",
          "form": "엄마랑 나만 먹었다ㅠㅜ",
          "original_form": "엄마랑 나만 먹었다ㅠㅜ",
          "speaker_id": "2",
          "time": "20210904 20:45"
        },
        {
          "id": "MDRW2100031342.1.15",
          "form": "내일 만두랑 떡 돌려먹자",
          "original_form": "내일 만두랑 떡 돌려먹자",
          "speaker_id": "1",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100031342.1.16",
          "form": "오 궁금하다 맛있대?",
          "original_form": "오 궁금하다 맛있대?",
          "speaker_id": "2",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100031342.1.17",
          "form": "맛있다던데",
          "original_form": "맛있다던데",
          "speaker_id": "1",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100031342.1.18",
          "form": "쫄깃쫄깃 할거같애",
          "original_form": "쫄깃쫄깃 할거같애",
          "speaker_id": "1",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100031342.1.19",
          "form": "점심에 꽃게찜이랑 같이 해서 먹자",
          "original_form": "점심에 꽃게찜이랑 같이 해서 먹자",
