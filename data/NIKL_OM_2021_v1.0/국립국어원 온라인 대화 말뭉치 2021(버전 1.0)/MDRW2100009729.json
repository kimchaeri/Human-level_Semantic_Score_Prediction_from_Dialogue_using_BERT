      "utterance": [
        {
          "id": "MDRW2100009729.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210823 08:38"
        },
        {
          "id": "MDRW2100009729.1.2",
          "form": "아니요 저는 안키웁니다",
          "original_form": "아니요 저는 안키웁니다",
          "speaker_id": "2",
          "time": "20210823 08:38"
        },
        {
          "id": "MDRW2100009729.1.3",
          "form": "name1님은 키우세요?",
          "original_form": "&name1&님은 키우세요?",
          "speaker_id": "2",
          "time": "20210823 08:39"
        },
        {
          "id": "MDRW2100009729.1.4",
          "form": "저도 아직",
          "original_form": "저도 아직",
          "speaker_id": "1",
          "time": "20210823 08:40"
        },
        {
          "id": "MDRW2100009729.1.5",
          "form": "강아지 키우고싶은데",
          "original_form": "강아지 키우고싶은데",
          "speaker_id": "1",
          "time": "20210823 08:40"
        },
        {
          "id": "MDRW2100009729.1.6",
          "form": "저도 강아지 너무 사랑스러워서 키우고 싶은데",
          "original_form": "저도 강아지 너무 사랑스러워서 키우고 싶은데",
          "speaker_id": "2",
          "time": "20210823 08:50"
        },
        {
          "id": "MDRW2100009729.1.7",
          "form": "기회가 안되네요 ㅠㅠ",
          "original_form": "기회가 안되네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 08:50"
        },
        {
          "id": "MDRW2100009729.1.8",
          "form": "근데 강아지 키우면 슬픈일이 더 많을거 같아요",
          "original_form": "근데 강아지 키우면 슬픈일이 더 많을거 같아요",
          "speaker_id": "2",
          "time": "20210823 08:50"
        },
        {
          "id": "MDRW2100009729.1.9",
          "form": "강아지는 수명이 너무 짧으니까 정주면 죽으니까 ㅠㅠ",
          "original_form": "강아지는 수명이 너무 짧으니까 정주면 죽으니까 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 08:51"
        },
        {
          "id": "MDRW2100009729.1.10",
          "form": "고양이는 털때문에",
          "original_form": "고양이는 털때문에",
          "speaker_id": "1",
          "time": "20210823 08:51"
        },
        {
          "id": "MDRW2100009729.1.11",
          "form": "고양이가 엄청 조용하죠 좋아하는 사람들 많더라구요",
          "original_form": "고양이가 엄청 조용하죠 좋아하는 사람들 많더라구요",
          "speaker_id": "2",
          "time": "20210823 09:04"
        },
        {
          "id": "MDRW2100009729.1.12",
          "form": "조용조용한 사람들이 좋아하는거 같아요",
          "original_form": "조용조용한 사람들이 좋아하는거 같아요",
          "speaker_id": "2",
          "time": "20210823 09:04"
        },
        {
          "id": "MDRW2100009729.1.13",
          "form": "돌돌이 필수죠",
          "original_form": "돌돌이 필수죠",
          "speaker_id": "1",
          "time": "20210823 09:38"
        },
        {
          "id": "MDRW2100009729.1.14",
          "form": "돌돌이 필수죠 필수 ㅎㅎ",
          "original_form": "돌돌이 필수죠 필수 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100009729.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210823 11:23"
        },
        {
          "id": "MDRW2100009729.1.16",
          "form": "대화 제출해야하는데 더 많이 대화해야할거 같은데 ㅎㅎ",
          "original_form": "대화 제출해야하는데 더 많이 대화해야할거 같은데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100009729.1.17",
          "form": "검은옷에 털 붙으면 장난 아님",
          "original_form": "검은옷에 털 붙으면 장난 아님",
          "speaker_id": "1",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100009729.1.18",
          "form": "미치죠 그러면 ㅋㅋ",
          "original_form": "미치죠 그러면 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 07:48"
        },
        {
          "id": "MDRW2100009729.1.19",
          "form": "쉬운일 아니에요 진짜 그냥 아가 한명 키운다고 생각해야지",
          "original_form": "쉬운일 아니에요 진짜 그냥 아가 한명 키운다고 생각해야지",
          "speaker_id": "2",
          "time": "20210824 07:48"
        },
        {
          "id": "MDRW2100009729.1.20",
          "form": "네 맞아요",
