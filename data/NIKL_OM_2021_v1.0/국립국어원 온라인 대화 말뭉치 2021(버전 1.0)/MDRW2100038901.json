      "utterance": [
        {
          "id": "MDRW2100038901.1.1",
          "form": "목살이랑 소고기 구워봤어요",
          "original_form": "목살이랑 소고기 구워봤어요",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100038901.1.2",
          "form": "아 전 에프로는 레토르트 인스턴트만 다뤘는데",
          "original_form": "아 전 에프로는 레토르트 인스턴트만 다뤘는데",
          "speaker_id": "1",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100038901.1.3",
          "form": "고기는 무조건 자이글ㅎ",
          "original_form": "고기는 무조건 자이글ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100038901.1.4",
          "form": "고기는 또 노릇하게 잘 익나봐요",
          "original_form": "고기는 또 노릇하게 잘 익나봐요",
          "speaker_id": "1",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100038901.1.5",
          "form": "팬에 굽는 것보다 고기가 부드럽게 되더라고요",
          "original_form": "팬에 굽는 것보다 고기가 부드럽게 되더라고요",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038901.1.6",
          "form": "근데 최고의 단점은 시간이 오래걸린다는거ㅠ",
          "original_form": "근데 최고의 단점은 시간이 오래걸린다는거ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038901.1.7",
          "form": "아ㅜㅜ 전 그냥 자이글로ㅜ",
          "original_form": "아ㅜㅜ 전 그냥 자이글로ㅜ",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038901.1.8",
          "form": "못 기다려요",
          "original_form": "못 기다려요",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038901.1.9",
          "form": "맞아요 사실 굽다가 팬으로 옮길뻔 했어요ㅋㅋ",
          "original_form": "맞아요 사실 굽다가 팬으로 옮길뻔 했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038901.1.10",
          "form": "에프로 고기 하면 시간이 얼마나 걸리나요?",
          "original_form": "에프로 고기 하면 시간이 얼마나 걸리나요?",
          "speaker_id": "1",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100038901.1.11",
          "form": "제가 작은걸 사서 그런지 20분이나 그 이상으로 걸리더라고요",
          "original_form": "제가 작은걸 사서 그런지 20분이나 그 이상으로 걸리더라고요",
          "speaker_id": "2",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100038901.1.12",
          "form": "아이고ㅜ 우리집 에프도 작아서;",
          "original_form": "아이고ㅜ 우리집 에프도 작아서;",
          "speaker_id": "1",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100038901.1.13",
          "form": "오늘 점심 삼겹살 먹을 예정이라 에프 고민했는데",
          "original_form": "오늘 점심 삼겹살 먹을 예정이라 에프 고민했는데",
          "speaker_id": "1",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100038901.1.14",
          "form": "그냥 자이글로 가야겠어요",
          "original_form": "그냥 자이글로 가야겠어요",
          "speaker_id": "1",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100038901.1.15",
          "form": "에프가 작고 바스켓이 깊으면 오래걸리더라고요",
          "original_form": "에프가 작고 바스켓이 깊으면 오래걸리더라고요",
          "speaker_id": "2",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100038901.1.16",
          "form": "ㅜㅜ딱 우리집 거네요",
          "original_form": "ㅜㅜ딱 우리집 거네요",
          "speaker_id": "1",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100038901.1.17",
          "form": "님은 점심  뭐 드시나요",
          "original_form": "님은 점심  뭐 드시나요",
          "speaker_id": "1",
          "time": "20210904 12:06"
        },
        {
          "id": "MDRW2100038901.1.18",
          "form": "저는 지금 고민중이에요ㅋㅋ",
          "original_form": "저는 지금 고민중이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:07"
        },
        {
          "id": "MDRW2100038901.1.19",
          "form": "아니 이제 점심이 코앞인데 배 안고프세요?",
          "original_form": "아니 이제 점심이 코앞인데 배 안고프세요?",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100038901.1.20",
          "form": "그냥 지금 식빵 뜯어먹고있네요ㅋㅋㅋ",
          "original_form": "그냥 지금 식빵 뜯어먹고있네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100038901.1.21",
          "form": "그래서 지금 그냥 식빵 뜯어먹고 있어요ㅋㅋㅋ",
          "original_form": "그래서 지금 그냥 식빵 뜯어먹고 있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:23"
        }
      ]
