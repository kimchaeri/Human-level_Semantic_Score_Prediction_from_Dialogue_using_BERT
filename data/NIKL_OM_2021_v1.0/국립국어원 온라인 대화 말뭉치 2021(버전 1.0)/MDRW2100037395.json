      "utterance": [
        {
          "id": "MDRW2100037395.1.1",
          "form": "기억에 남는 기념품 이ㅛ낭",
          "original_form": "기억에 남는 기념품 이ㅛ낭",
          "speaker_id": "1",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100037395.1.2",
          "form": "있나요",
          "original_form": "있나요",
          "speaker_id": "1",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100037395.1.3",
          "form": "음.. 전 일본에서 사온 인형이요 !",
          "original_form": "음.. 전 일본에서 사온 인형이요 !",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037395.1.4",
          "form": "어떤 인형일까요?",
          "original_form": "어떤 인형일까요?",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037395.1.5",
          "form": "그 포켓몬스터 아시나요!",
          "original_form": "그 포켓몬스터 아시나요!",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037395.1.6",
          "form": "네 알죠! 일본 캐릭터인가봐요?",
          "original_form": "네 알죠! 일본 캐릭터인가봐요?",
          "speaker_id": "1",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037395.1.7",
          "form": "네네 ! 일본꺼에요 !",
          "original_form": "네네 ! 일본꺼에요 !",
          "speaker_id": "2",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037395.1.8",
          "form": "국내 여행지에서는요~?^^",
          "original_form": "국내 여행지에서는요~?^^",
          "speaker_id": "1",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037395.1.9",
          "form": "음... 전 제주도에서 사온 파우치요 !",
          "original_form": "음... 전 제주도에서 사온 파우치요 !",
          "speaker_id": "2",
          "time": "20210831 23:25"
        },
        {
          "id": "MDRW2100037395.1.10",
          "form": "제주도 어디요? 공방?",
          "original_form": "제주도 어디요? 공방?",
          "speaker_id": "1",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100037395.1.11",
          "form": "아니요 아마 소품샵이였는데",
          "original_form": "아니요 아마 소품샵이였는데",
          "speaker_id": "2",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037395.1.12",
          "form": "한라봉 파우치에요 ㅎㅎ",
          "original_form": "한라봉 파우치에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037395.1.13",
          "form": "끄약 깜찍한 기념품이네요^^",
          "original_form": "끄약 깜찍한 기념품이네요^^",
          "speaker_id": "1",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100037395.1.14",
          "form": "맞죠 !",
          "original_form": "맞죠 !",
          "speaker_id": "2",
          "time": "20210831 23:32"
        },
        {
          "id": "MDRW2100037395.1.15",
          "form": "가격은 비쌌지만.. 잘쓴답니다 ㅎㅎ",
          "original_form": "가격은 비쌌지만.. 잘쓴답니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 23:32"
        },
        {
          "id": "MDRW2100037395.1.16",
          "form": "저는 주로 먹을것이 기억에 남아요..",
          "original_form": "저는 주로 먹을것이 기억에 남아요..",
