      "utterance": [
        {
          "id": "MDRW2100028720.1.1",
          "form": "인스타셀럽들도 딸랑이들 엄청나다던데",
          "original_form": "인스타셀럽들도 딸랑이들 엄청나다던데",
          "speaker_id": "1",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100028720.1.2",
          "form": "라방때 이름 불러달라고 ㅇㅇ 그거하나 하려고 막 마켓열면",
          "original_form": "라방때 이름 불러달라고 ㅇㅇ 그거하나 하려고 막 마켓열면",
          "speaker_id": "1",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100028720.1.3",
          "form": "선물사가고 커피 과자 음료 이런거 사서 조공하고",
          "original_form": "선물사가고 커피 과자 음료 이런거 사서 조공하고",
          "speaker_id": "1",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100028720.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ헐...진짜 이해불가다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ헐...진짜 이해불가다",
          "speaker_id": "2",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028720.1.5",
          "form": "그처...",
          "original_form": "그처...",
          "speaker_id": "1",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028720.1.6",
          "form": "차라리 돌덕질을 하겠어....",
          "original_form": "차라리 돌덕질을 하겠어....",
          "speaker_id": "2",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028720.1.7",
          "form": "제일 이해안가는 부류",
          "original_form": "제일 이해안가는 부류",
          "speaker_id": "1",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028720.1.8",
          "form": "ㄹㅇ 진짜 연예인한테 이름 불리면",
          "original_form": "ㄹㅇ 진짜 연예인한테 이름 불리면",
          "speaker_id": "1",
          "time": "20210902 21:47"
        },
        {
          "id": "MDRW2100028720.1.9",
          "form": "몰라도",
          "original_form": "몰라도",
          "speaker_id": "1",
          "time": "20210902 21:47"
        },
        {
          "id": "MDRW2100028720.1.10",
          "form": "그냥인스타셀럽 ㅎㅎㅎㅋㅋㅋㅋㅋ",
          "original_form": "그냥인스타셀럽 ㅎㅎㅎㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:47"
        },
        {
          "id": "MDRW2100028720.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ인스타셀럽은 막 공구하면서 세금도 제대로 안내잖아...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ인스타셀럽은 막 공구하면서 세금도 제대로 안내잖아...",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028720.1.12",
          "form": "왜 추앙하는지 이해불가",
          "original_form": "왜 추앙하는지 이해불가",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028720.1.13",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028720.1.14",
          "form": "그리고 환볼 안되고",
          "original_form": "그리고 환볼 안되고",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028720.1.15",
          "form": "교환도안되고",
          "original_form": "교환도안되고",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028720.1.16",
          "form": "가격도 공개안하고",
          "original_form": "가격도 공개안하고",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028720.1.17",
          "form": "맞어ㅋㅋㅋㅋㅋㅋㅋㅋ 그리고 물건 팔았으면서",
          "original_form": "맞어ㅋㅋㅋㅋㅋㅋㅋㅋ 그리고 물건 팔았으면서",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028720.1.18",
          "form": "문의글남기면",
          "original_form": "문의글남기면",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028720.1.19",
          "form": "공손하게면박줌",
          "original_form": "공손하게면박줌",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028720.1.20",
          "form": "아공손한척하는 손",
          "original_form": "아공손한척하는 손",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028720.1.21",
          "form": "합장하는거 ㅋㅋㅋㅋㄹㅇ",
          "original_form": "합장하는거 ㅋㅋㅋㅋㄹㅇ",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028720.1.22",
          "form": "환불이 어려우세요",
          "original_form": "환불이 어려우세요",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028720.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028720.1.24",
          "form": "제가 정말 열심히 발품팔아 찾은 아이랍니다",
          "original_form": "제가 정말 열심히 발품팔아 찾은 아이랍니다",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028720.1.25",
          "form": "교환이어려우세요",
          "original_form": "교환이어려우세요",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028720.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028720.1.27",
          "form": "어쩜ㅈ 하나같이 말투가 똑같지",
          "original_form": "어쩜ㅈ 하나같이 말투가 똑같지",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028720.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맨날 어렵대....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맨날 어렵대....",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028720.1.29",
          "form": "진짜 이해불가야",
          "original_form": "진짜 이해불가야",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028720.1.30",
          "form": "소비자보호법 어디갔어ㅜㅜ",
          "original_form": "소비자보호법 어디갔어ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028720.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ진짜 다 신고해야해",
          "original_form": "ㅋㅋㅋㅋㅋㅋ진짜 다 신고해야해",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028720.1.32",
          "form": "안그러니까 저렇게 날뛰지",
          "original_form": "안그러니까 저렇게 날뛰지",
          "speaker_id": "1",
          "time": "20210902 21:52"
        }
      ]
