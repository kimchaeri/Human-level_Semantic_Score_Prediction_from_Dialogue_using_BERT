      "utterance": [
        {
          "id": "MDRW2100042362.1.1",
          "form": "응. 장대비가 쏟아진다",
          "original_form": "응. 장대비가 쏟아진다",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042362.1.2",
          "form": "내일이랑 수요일에도 비온대",
          "original_form": "내일이랑 수요일에도 비온대",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042362.1.3",
          "form": "습도도 또 올라가겠네",
          "original_form": "습도도 또 올라가겠네",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042362.1.4",
          "form": "그치ㅠ 좀 가을 날씨 되나 했더니..",
          "original_form": "그치ㅠ 좀 가을 날씨 되나 했더니..",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042362.1.5",
          "form": "그러게 도로 여름 날씨네",
          "original_form": "그러게 도로 여름 날씨네",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042362.1.6",
          "form": "그나마 좀 시원하긴 한데 그래도 찝찝해",
          "original_form": "그나마 좀 시원하긴 한데 그래도 찝찝해",
          "speaker_id": "1",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042362.1.7",
          "form": "비 와서 새벽에는 추우니까 문단속 잘해",
          "original_form": "비 와서 새벽에는 추우니까 문단속 잘해",
          "speaker_id": "2",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042362.1.8",
          "form": "어 일단 창문은 다 닫아놧어",
          "original_form": "어 일단 창문은 다 닫아놧어",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042362.1.9",
          "form": "바람도 많이 분다",
          "original_form": "바람도 많이 분다",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042362.1.10",
          "form": "비바람 불면 감기 걸리기 쉬우니까 옷 단단히 입고 다녀",
          "original_form": "비바람 불면 감기 걸리기 쉬우니까 옷 단단히 입고 다녀",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042362.1.11",
          "form": "응 엄마두 외투 하나는 가지고 다녀~~",
          "original_form": "응 엄마두 외투 하나는 가지고 다녀~~",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042362.1.12",
          "form": "응 이제 가디건 걸치고 다녀",
          "original_form": "응 이제 가디건 걸치고 다녀",
          "speaker_id": "2",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042362.1.13",
          "form": "요새 일교차 너무 심하더라구",
          "original_form": "요새 일교차 너무 심하더라구",
          "speaker_id": "1",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042362.1.14",
          "form": "이런 날씨엔 감기도 잘 들어. 건강 조심하고 일기예보 잘 보고 다녀",
          "original_form": "이런 날씨엔 감기도 잘 들어. 건강 조심하고 일기예보 잘 보고 다녀",
          "speaker_id": "2",
          "time": "20210906 22:10"
        }
      ]
