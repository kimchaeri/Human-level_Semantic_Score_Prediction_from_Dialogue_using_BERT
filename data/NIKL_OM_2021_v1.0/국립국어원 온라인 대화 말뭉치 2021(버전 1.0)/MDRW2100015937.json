      "utterance": [
        {
          "id": "MDRW2100015937.1.1",
          "form": "name2님은 지금 직장인이셔요?",
          "original_form": "&name2&님은 지금 직장인이셔요?",
          "speaker_id": "1",
          "time": "20210826 03:12"
        },
        {
          "id": "MDRW2100015937.1.2",
          "form": "알바하실때",
          "original_form": "알바하실때",
          "speaker_id": "2",
          "time": "20210826 03:12"
        },
        {
          "id": "MDRW2100015937.1.3",
          "form": "어떤부분이 제일 힘드셨어용?",
          "original_form": "어떤부분이 제일 힘드셨어용?",
          "speaker_id": "2",
          "time": "20210826 03:12"
        },
        {
          "id": "MDRW2100015937.1.4",
          "form": "저는 퇴사했어용...!ㅠㅠ",
          "original_form": "저는 퇴사했어용...!ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 03:13"
        },
        {
          "id": "MDRW2100015937.1.5",
          "form": "백수랍니닷",
          "original_form": "백수랍니닷",
          "speaker_id": "2",
          "time": "20210826 03:13"
        },
        {
          "id": "MDRW2100015937.1.6",
          "form": "저는 아무래도 같이 일하는 사람들 텃세??ㅋㅋㅋㅋ",
          "original_form": "저는 아무래도 같이 일하는 사람들 텃세??ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 03:13"
        },
        {
          "id": "MDRW2100015937.1.7",
          "form": "첨엔 그런게 좀 잇엇어요 ㅎ",
          "original_form": "첨엔 그런게 좀 잇엇어요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 03:13"
        },
        {
          "id": "MDRW2100015937.1.8",
          "form": "나중되선 갠찬아지더라구요!",
          "original_form": "나중되선 갠찬아지더라구요!",
          "speaker_id": "1",
          "time": "20210826 03:13"
        },
        {
          "id": "MDRW2100015937.1.9",
          "form": "아이쿠... 사람 잘 만나는게 중요하더라구요ㅠㅠ",
          "original_form": "아이쿠... 사람 잘 만나는게 중요하더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 03:13"
        },
        {
          "id": "MDRW2100015937.1.10",
          "form": "왜그럴까용",
          "original_form": "왜그럴까용",
          "speaker_id": "2",
          "time": "20210826 03:13"
        },
        {
          "id": "MDRW2100015937.1.11",
          "form": "같이 열심히 일하면 좋은데ㅠㅠ",
          "original_form": "같이 열심히 일하면 좋은데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 03:13"
        },
        {
          "id": "MDRW2100015937.1.12",
          "form": "앗 그렇군요ㅠㅠ 저는 (아가리) 공시생이요 ㅋㅋㅋㅋ",
          "original_form": "앗 그렇군요ㅠㅠ 저는 (아가리) 공시생이요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 03:13"
        },
        {
          "id": "MDRW2100015937.1.13",
          "form": "ㅋㅋㅋ 아무리 그래도 공시 준비하는게",
          "original_form": "ㅋㅋㅋ 아무리 그래도 공시 준비하는게",
          "speaker_id": "2",
          "time": "20210826 03:14"
        },
        {
          "id": "MDRW2100015937.1.14",
          "form": "그니까요ㅎㅎㅎ 누가 첨 들어오면 처음엔 좀 텃세부리고 자기들끼리도 서로 막 욕하고 글트라구요",
          "original_form": "그니까요ㅎㅎㅎ 누가 첨 들어오면 처음엔 좀 텃세부리고 자기들끼리도 서로 막 욕하고 글트라구요",
          "speaker_id": "1",
          "time": "20210826 03:14"
        },
        {
          "id": "MDRW2100015937.1.15",
          "form": "만만치 않져ㅠ",
          "original_form": "만만치 않져ㅠ",
          "speaker_id": "2",
          "time": "20210826 03:14"
        },
        {
          "id": "MDRW2100015937.1.16",
          "form": "여럿이서 일하다보니 그런가봐용",
          "original_form": "여럿이서 일하다보니 그런가봐용",
