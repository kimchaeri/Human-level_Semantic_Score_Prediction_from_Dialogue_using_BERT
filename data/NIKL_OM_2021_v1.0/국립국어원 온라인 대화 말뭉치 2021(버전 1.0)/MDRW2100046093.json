      "utterance": [
        {
          "id": "MDRW2100046093.1.1",
          "form": "샐러드 좋아하세요?",
          "original_form": "샐러드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046093.1.2",
          "form": "네! 풀때기 좋아해요ㅎㅎㅎ",
          "original_form": "네! 풀때기 좋아해요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046093.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046093.1.4",
          "form": "원래 샐러드먹으면 지나가는 직원들이 어머 다이어트해? 이랬는데",
          "original_form": "원래 샐러드먹으면 지나가는 직원들이 어머 다이어트해? 이랬는데",
          "speaker_id": "1",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046093.1.5",
          "form": "최근엔 샐러드가 식사메뉴로 많이 자리 잡은 것 같아요",
          "original_form": "최근엔 샐러드가 식사메뉴로 많이 자리 잡은 것 같아요",
          "speaker_id": "1",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046093.1.6",
          "form": "아 맞아요 아예 끼니문화가 된 것 같더라구요",
          "original_form": "아 맞아요 아예 끼니문화가 된 것 같더라구요",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046093.1.7",
          "form": "다들 건강 챙겨서 그런가",
          "original_form": "다들 건강 챙겨서 그런가",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046093.1.8",
          "form": "샐러드 배달 가게들도 생겼던데요!",
          "original_form": "샐러드 배달 가게들도 생겼던데요!",
          "speaker_id": "2",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046093.1.9",
          "form": "맞아요 그런 것 같아요",
          "original_form": "맞아요 그런 것 같아요",
          "speaker_id": "1",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046093.1.10",
          "form": "그리고 전 진짜 많이 먹는 사람이었거든요..??",
          "original_form": "그리고 전 진짜 많이 먹는 사람이었거든요..??",
          "speaker_id": "1",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046093.1.11",
          "form": "최근에 양이 갑자기 줄어서...",
          "original_form": "최근에 양이 갑자기 줄어서...",
          "speaker_id": "1",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046093.1.12",
          "form": "부담스럽게 먹기 싫을때 자즈가요",
          "original_form": "부담스럽게 먹기 싫을때 자즈가요",
          "speaker_id": "1",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046093.1.13",
          "form": "안 부담스러운 게 제일 좋아요",
          "original_form": "안 부담스러운 게 제일 좋아요",
          "speaker_id": "2",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046093.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046093.1.15",
          "form": "전 아직 배달시켜먹은 적은 없는데",
          "original_form": "전 아직 배달시켜먹은 적은 없는데",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046093.1.16",
          "form": "투고샐러드라고 아세요?",
          "original_form": "투고샐러드라고 아세요?",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046093.1.17",
          "form": "아니요! 그게 뭐예요?",
          "original_form": "아니요! 그게 뭐예요?",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046093.1.18",
          "form": "샐러드 프랮차이즈 중 하나인데",
          "original_form": "샐러드 프랮차이즈 중 하나인데",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046093.1.19",
          "form": "가격은 저렴한데 꽤나 맛있어요!",
          "original_form": "가격은 저렴한데 꽤나 맛있어요!",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046093.1.20",
          "form": "가성비 짱",
          "original_form": "가성비 짱",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046093.1.21",
          "form": "헉 적어둘래요",
          "original_form": "헉 적어둘래요",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046093.1.22",
          "form": "저도 아침 아직 안 먹었는데",
          "original_form": "저도 아침 아직 안 먹었는데",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046093.1.23",
          "form": "풀때기 와구와구 먹고싶어지네요 ㅌㅋㅌㅌㅌ",
          "original_form": "풀때기 와구와구 먹고싶어지네요 ㅌㅋㅌㅌㅌ",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046093.1.24",
          "form": "새우자몽샐러드를 시켰는데",
          "original_form": "새우자몽샐러드를 시켰는데",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046093.1.25",
          "form": "6,900원에 새우가 7개나 올라간거 있죠",
          "original_form": "6,900원에 새우가 7개나 올라간거 있죠",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046093.1.26",
          "form": "칵테일 새우 아님!",
          "original_form": "칵테일 새우 아님!",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046093.1.27",
          "form": "오동통한 새우가 7개나 올라가요",
          "original_form": "오동통한 새우가 7개나 올라가요",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046093.1.28",
          "form": "자몽은 조금 쓰긴 했는데",
          "original_form": "자몽은 조금 쓰긴 했는데",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046093.1.29",
          "form": "(원래 쓴 과일이긴하지만여...ㅋㅋ)",
          "original_form": "(원래 쓴 과일이긴하지만여...ㅋㅋ)",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046093.1.30",
          "form": "칵테일새우도 아니고 꽤 적당한 크기의 새우가 7개나 올라가는데도",
          "original_form": "칵테일새우도 아니고 꽤 적당한 크기의 새우가 7개나 올라가는데도",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046093.1.31",
          "form": "6,900원인게 넘나... 굿... 가성비 짱입니다",
          "original_form": "6,900원인게 넘나... 굿... 가성비 짱입니다",
          "speaker_id": "1",
          "time": "20210908 10:25"
        }
      ]
    }
