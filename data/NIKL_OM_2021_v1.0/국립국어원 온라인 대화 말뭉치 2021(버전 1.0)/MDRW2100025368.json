        {
          "id": "MDRW2100025368.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025368.1.2",
          "form": "가끔 가요",
          "original_form": "가끔 가요",
          "speaker_id": "2",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100025368.1.3",
          "form": "저도 가끔",
          "original_form": "저도 가끔",
          "speaker_id": "1",
          "time": "20210901 10:18"
        },
        {
          "id": "MDRW2100025368.1.4",
          "form": "요즘은 너무안가서 가고싶긴해요",
          "original_form": "요즘은 너무안가서 가고싶긴해요",
          "speaker_id": "2",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025368.1.5",
          "form": "코로나때문에",
          "original_form": "코로나때문에",
          "speaker_id": "1",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025368.1.6",
          "form": "맞아요 답답해서",
          "original_form": "맞아요 답답해서",
          "speaker_id": "2",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025368.1.7",
          "form": "유럽가고 시 ㅍ어요",
          "original_form": "유럽가고 시 ㅍ어요",
          "speaker_id": "1",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025368.1.8",
          "form": "거긴 한번도 안가봤네요",
          "original_form": "거긴 한번도 안가봤네요",
          "speaker_id": "2",
          "time": "20210901 10:24"
        },
        {
          "id": "MDRW2100025368.1.9",
          "form": "스페인 좋다고 하더라구요",
          "original_form": "스페인 좋다고 하더라구요",
          "speaker_id": "1",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025368.1.10",
          "form": "티비에서 보면 좋을거같아요",
          "original_form": "티비에서 보면 좋을거같아요",
          "speaker_id": "2",
          "time": "20210901 10:27"
        },
        {
          "id": "MDRW2100025368.1.11",
          "form": "요즘은 가질 못하니 티비에 여행프로만 보게되네요",
          "original_form": "요즘은 가질 못하니 티비에 여행프로만 보게되네요",
          "speaker_id": "2",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025368.1.12",
          "form": "대리만족해요",
          "original_form": "대리만족해요",
          "speaker_id": "1",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025368.1.13",
          "form": "얼른 마스크없이 여행가고 싶어요",
          "original_form": "얼른 마스크없이 여행가고 싶어요",
          "speaker_id": "2",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025368.1.14",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025368.1.15",
