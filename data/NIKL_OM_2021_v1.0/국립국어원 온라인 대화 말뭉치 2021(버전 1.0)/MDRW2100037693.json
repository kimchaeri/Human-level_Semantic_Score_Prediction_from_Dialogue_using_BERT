        },
        {
          "id": "MDRW2100037693.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 16:16"
        },
        {
          "id": "MDRW2100037693.1.3",
          "form": "점심 맛있게 드셨나요?",
          "original_form": "점심 맛있게 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100037693.1.4",
          "form": "네ㅎㅎ 점심 드셨어요?",
          "original_form": "네ㅎㅎ 점심 드셨어요?",
          "speaker_id": "2",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100037693.1.5",
          "form": "쫄면이랑 만두 먹었습니다 ㅎㅎ",
          "original_form": "쫄면이랑 만두 먹었습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100037693.1.6",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 16:24"
        },
        {
          "id": "MDRW2100037693.1.7",
          "form": "저는 비빔밥이요\r\n쫄면이랑 만두 조합 맛있죠",
          "original_form": "저는 비빔밥이요\r\n쫄면이랑 만두 조합 맛있죠",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037693.1.8",
          "form": "비빔밥도 맛있죠!",
          "original_form": "비빔밥도 맛있죠!",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100037693.1.9",
          "form": "나물비빔밥 드셨나요?",
          "original_form": "나물비빔밥 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100037693.1.10",
          "form": "아니요 남아있던 반찬들 몇 개 넣어서 먹었어요ㅎㅎ",
          "original_form": "아니요 남아있던 반찬들 몇 개 넣어서 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100037693.1.11",
          "form": "저는 반찬들로 비빔밥 만들 때 간 맞추기가 어렵던데 잘 맞추시나요??",
          "original_form": "저는 반찬들로 비빔밥 만들 때 간 맞추기가 어렵던데 잘 맞추시나요??",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100037693.1.12",
          "form": "반찬들이 간이 되어 있으니까",
          "original_form": "반찬들이 간이 되어 있으니까",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037693.1.13",
          "form": "따로 고추장같은 양념을 사용하지 않았어요",
          "original_form": "따로 고추장같은 양념을 사용하지 않았어요",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037693.1.14",
          "form": "재료들만 넣고 비벼서 먹으면 괜찮더라구요",
          "original_form": "재료들만 넣고 비벼서 먹으면 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037693.1.15",
          "form": "그래서 계속 밥이 많아졌나봐요 ㅎㅎ",
          "original_form": "그래서 계속 밥이 많아졌나봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100037693.1.16",
          "form": "고추장에 계속 집착하다보니... ㅎㅎ",
          "original_form": "고추장에 계속 집착하다보니... ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100037693.1.17",
          "form": "비빔밥 만들때 고추장이 뭔가 필수조건처럼 생각되어지기는 하죠ㅎㅎ",
          "original_form": "비빔밥 만들때 고추장이 뭔가 필수조건처럼 생각되어지기는 하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100037693.1.18",
          "form": "고추장이랑 참기름 조합을 너무 좋아해서요 ㅎㅎ",
          "original_form": "고추장이랑 참기름 조합을 너무 좋아해서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 17:39"
        }
      ]
