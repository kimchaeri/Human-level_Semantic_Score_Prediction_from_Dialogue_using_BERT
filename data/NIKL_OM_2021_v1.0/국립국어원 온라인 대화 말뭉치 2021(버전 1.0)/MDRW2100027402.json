      "utterance": [
        {
          "id": "MDRW2100027402.1.1",
          "form": "점심식사 하셨어요?",
          "original_form": "점심식사 하셨어요?",
          "speaker_id": "2",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100027402.1.2",
          "form": "아직이요:)\n이제 하려고 준비해요",
          "original_form": "아직이요:)\n이제 하려고 준비해요",
          "speaker_id": "1",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027402.1.3",
          "form": "식사 하셨나요?",
          "original_form": "식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027402.1.4",
          "form": "네 구내식당에서 먹었어요 ㅎ",
          "original_form": "네 구내식당에서 먹었어요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100027402.1.5",
          "form": "오 구내식당이 있군요:)",
          "original_form": "오 구내식당이 있군요:)",
          "speaker_id": "1",
          "time": "20210902 12:45"
        },
        {
          "id": "MDRW2100027402.1.6",
          "form": "맛은 어때요?",
          "original_form": "맛은 어때요?",
          "speaker_id": "1",
          "time": "20210902 12:45"
        },
        {
          "id": "MDRW2100027402.1.7",
          "form": "쏘쏘?그래도 가격이 저렴하니까 좋아요 ㅎㅎ",
          "original_form": "쏘쏘?그래도 가격이 저렴하니까 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 12:51"
        },
        {
          "id": "MDRW2100027402.1.8",
          "form": "구내식당 얼마에요?",
          "original_form": "구내식당 얼마에요?",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027402.1.9",
          "form": "저희 회사는 3500원\n3장 구입하면 만원이였는뎅ㅋ",
          "original_form": "저희 회사는 3500원\n3장 구입하면 만원이였는뎅ㅋ",
          "speaker_id": "1",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100027402.1.10",
          "form": "저희는 한달에 4만원....ㅎㅎ",
          "original_form": "저희는 한달에 4만원....ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027402.1.11",
          "form": "엄청싸죠 진짜로",
          "original_form": "엄청싸죠 진짜로",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027402.1.12",
          "form": "우와 진짜 싸네요 !!!!",
          "original_form": "우와 진짜 싸네요 !!!!",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027402.1.13",
          "form": "네 그래서 별 불만없이 나오는데로 먹고있습니다 ㅎ",
          "original_form": "네 그래서 별 불만없이 나오는데로 먹고있습니다 ㅎ",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027402.1.14",
          "form": "ㅋㅋ그 정도면\n정말 감사합니다 하고\n투정부리지 말고 먹어야겠어영ㅋㅋ",
          "original_form": "ㅋㅋ그 정도면\n정말 감사합니다 하고\n투정부리지 말고 먹어야겠어영ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027402.1.15",
          "form": "네 ㅋㅋ  ㅎ그러고있어요 ㅎㅎㅎ",
          "original_form": "네 ㅋㅋ  ㅎ그러고있어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100027402.1.16",
          "form": "음식도 맛없는 건 아니라 잘 먹고있어요 항상 ㅎ",
          "original_form": "음식도 맛없는 건 아니라 잘 먹고있어요 항상 ㅎ",
          "speaker_id": "2",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100027402.1.17",
          "form": "와 완전 훌륭한  구내식당이네요:)",
          "original_form": "와 완전 훌륭한  구내식당이네요:)",
          "speaker_id": "1",
          "time": "20210902 14:55"
        },
        {
          "id": "MDRW2100027402.1.18",
          "form": "저도 입사하고 싶네요ㅋㅋ",
          "original_form": "저도 입사하고 싶네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:55"
        },
        {
          "id": "MDRW2100027402.1.19",
          "form": "ㅎㅎ 근처에 식당이 별로없어서 구내식당만 이용해야되기도 해서 ㅎㅎ",
          "original_form": "ㅎㅎ 근처에 식당이 별로없어서 구내식당만 이용해야되기도 해서 ㅎㅎ",
