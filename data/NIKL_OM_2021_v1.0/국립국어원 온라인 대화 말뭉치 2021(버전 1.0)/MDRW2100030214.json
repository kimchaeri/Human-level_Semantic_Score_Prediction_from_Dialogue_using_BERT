      "utterance": [
        {
          "id": "MDRW2100030214.1.1",
          "form": "나 아몬드우유 산 거 있잖아",
          "original_form": "나 아몬드우유 산 거 있잖아",
          "speaker_id": "2",
          "time": "20210904 20:30"
        },
        {
          "id": "MDRW2100030214.1.2",
          "form": "아 코스트코에서 집은거?",
          "original_form": "아 코스트코에서 집은거?",
          "speaker_id": "1",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100030214.1.3",
          "form": "ㅇㅇ그거",
          "original_form": "ㅇㅇ그거",
          "speaker_id": "2",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100030214.1.4",
          "form": "삼육에서 새로 나온 거래",
          "original_form": "삼육에서 새로 나온 거래",
          "speaker_id": "2",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100030214.1.5",
          "form": "별걸 다 만든다",
          "original_form": "별걸 다 만든다",
          "speaker_id": "1",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100030214.1.6",
          "form": "당이 없고 우유가 아니라",
          "original_form": "당이 없고 우유가 아니라",
          "speaker_id": "2",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100030214.1.7",
          "form": "맛은 별로 없을 줄 알았거든",
          "original_form": "맛은 별로 없을 줄 알았거든",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100030214.1.8",
          "form": "맛 없을 줄 알면서 왜사",
          "original_form": "맛 없을 줄 알면서 왜사",
          "speaker_id": "1",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100030214.1.9",
          "form": "아니 우유먹는 게 여드름에 안 좋다 그래서",
          "original_form": "아니 우유먹는 게 여드름에 안 좋다 그래서",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100030214.1.10",
          "form": "우유가 안 좋대?",
          "original_form": "우유가 안 좋대?",
          "speaker_id": "1",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100030214.1.11",
          "form": "한국인이 유제품이 잘 안 맞는 사람이 많대",
          "original_form": "한국인이 유제품이 잘 안 맞는 사람이 많대",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100030214.1.12",
          "form": "그래서 샀냐?",
          "original_form": "그래서 샀냐?",
          "speaker_id": "1",
          "time": "20210904 20:35"
        },
        {
          "id": "MDRW2100030214.1.13",
          "form": "ㅇㅇ그런데",
          "original_form": "ㅇㅇ그런데",
          "speaker_id": "2",
          "time": "20210904 20:35"
        },
        {
          "id": "MDRW2100030214.1.14",
          "form": "생각보다는 괜찮은데",
          "original_form": "생각보다는 괜찮은데",
          "speaker_id": "2",
          "time": "20210904 20:35"
        },
        {
          "id": "MDRW2100030214.1.15",
          "form": "그럼 다행이지",
          "original_form": "그럼 다행이지",
          "speaker_id": "1",
          "time": "20210904 20:35"
        },
        {
          "id": "MDRW2100030214.1.16",
          "form": "근데 커피에 라떼로 마시려 그랬는데",
          "original_form": "근데 커피에 라떼로 마시려 그랬는데",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100030214.1.17",
          "form": "그냥 먹을 땐 괜찮고?",
          "original_form": "그냥 먹을 땐 괜찮고?",
          "speaker_id": "1",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100030214.1.18",
          "form": "ㅇㅇ진짜 어이없음..",
          "original_form": "ㅇㅇ진짜 어이없음..",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100030214.1.19",
          "form": "그냥 먹어 그럼",
          "original_form": "그냥 먹어 그럼",
          "speaker_id": "1",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100030214.1.20",
          "form": "저거 다 먹음",
          "original_form": "저거 다 먹음",
          "speaker_id": "2",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100030214.1.21",
          "form": "아몬드브리즈나 오틀리",
          "original_form": "아몬드브리즈나 오틀리",
          "speaker_id": "2",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100030214.1.22",
          "form": "귀리우유 사 볼라고ㅠㅜ",
          "original_form": "귀리우유 사 볼라고ㅠㅜ",
          "speaker_id": "2",
          "time": "20210904 20:37"
        }
