      "utterance": [
        {
          "id": "MDRW2100041360.1.1",
          "form": "먹는걸 좋아하시나요?",
          "original_form": "먹는걸 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100041360.1.2",
          "form": "좋아하죠",
          "original_form": "좋아하죠",
          "speaker_id": "2",
          "time": "20210906 06:50"
        },
        {
          "id": "MDRW2100041360.1.3",
          "form": "그래서 요리도 좋아해요",
          "original_form": "그래서 요리도 좋아해요",
          "speaker_id": "2",
          "time": "20210906 06:50"
        },
        {
          "id": "MDRW2100041360.1.4",
          "form": "그렇군요!!",
          "original_form": "그렇군요!!",
          "speaker_id": "1",
          "time": "20210906 11:23"
        },
        {
          "id": "MDRW2100041360.1.5",
          "form": "ㅎㅎ 먹는거 이야기하면 너무 기쁜것같아요",
          "original_form": "ㅎㅎ 먹는거 이야기하면 너무 기쁜것같아요",
          "speaker_id": "1",
          "time": "20210906 11:23"
        },
        {
          "id": "MDRW2100041360.1.6",
          "form": "즐거워요",
          "original_form": "즐거워요",
          "speaker_id": "1",
          "time": "20210906 11:23"
        },
        {
          "id": "MDRW2100041360.1.7",
          "form": "전 미숫가루 마셨는데 좋아하시나요??",
          "original_form": "전 미숫가루 마셨는데 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210906 11:23"
        },
        {
          "id": "MDRW2100041360.1.8",
          "form": "미숫가루 맛있죠",
          "original_form": "미숫가루 맛있죠",
          "speaker_id": "2",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041360.1.9",
          "form": "하루 한끼 미숫가루 타마시나요?",
          "original_form": "하루 한끼 미숫가루 타마시나요?",
          "speaker_id": "2",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041360.1.10",
          "form": "네 이제부터 그러려구요",
          "original_form": "네 이제부터 그러려구요",
          "speaker_id": "1",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041360.1.11",
          "form": "오늘이 첫날입니당ㅎㅎㅎ",
          "original_form": "오늘이 첫날입니당ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041360.1.12",
          "form": "아침은 미숫가루만 마시려구요",
          "original_form": "아침은 미숫가루만 마시려구요",
          "speaker_id": "1",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041360.1.13",
          "form": "아 아침메뉴를 미숫가루로 정한거군요",
          "original_form": "아 아침메뉴를 미숫가루로 정한거군요",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100041360.1.14",
          "form": "건강식이네요 포만감도 있고",
          "original_form": "건강식이네요 포만감도 있고",
          "speaker_id": "2",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100041360.1.15",
          "form": "미숫가루 달달하게 타서 얼음 동동 띄우면 맛있는데",
          "original_form": "미숫가루 달달하게 타서 얼음 동동 띄우면 맛있는데",
          "speaker_id": "2",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100041360.1.16",
          "form": "그럼 건강식 느낌은 아니겠죠 ㅎ",
          "original_form": "그럼 건강식 느낌은 아니겠죠 ㅎ",
          "speaker_id": "2",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100041360.1.17",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100041360.1.18",
          "form": "아휴 그렇게 먹으면 너무 맛있죠",
          "original_form": "아휴 그렇게 먹으면 너무 맛있죠",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100041360.1.19",
          "form": "여름에 그렇게 먹으면 한끼뚝딱이잖아요",
          "original_form": "여름에 그렇게 먹으면 한끼뚝딱이잖아요",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100041360.1.20",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210907 08:31"
        },
        {
          "id": "MDRW2100041360.1.21",
          "form": "저희 가족은 오늘 아침 토스트를 해먹었어요",
          "original_form": "저희 가족은 오늘 아침 토스트를 해먹었어요",
          "speaker_id": "2",
          "time": "20210907 08:32"
        },
        {
          "id": "MDRW2100041360.1.22",
          "form": "계란과 치즈를 넣어서 우유랑 먹었죠",
          "original_form": "계란과 치즈를 넣어서 우유랑 먹었죠",
          "speaker_id": "2",
          "time": "20210907 08:32"
        },
        {
          "id": "MDRW2100041360.1.23",
          "form": "와 맛있겠어요",
          "original_form": "와 맛있겠어요",
          "speaker_id": "1",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100041360.1.24",
          "form": "계란과 치즈라니 기본에 충실하지만 너무 맛이잖아요 ㅎㅎ",
          "original_form": "계란과 치즈라니 기본에 충실하지만 너무 맛이잖아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100041360.1.25",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "1",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100041360.1.26",
          "form": "딱 기본 토스트죠",
          "original_form": "딱 기본 토스트죠",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100041360.1.27",
          "form": "점심은 뭐 먹었나요?",
          "original_form": "점심은 뭐 먹었나요?",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100041360.1.28",
          "form": "계란프라이 해먹었어요",
          "original_form": "계란프라이 해먹었어요",
          "speaker_id": "1",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100041360.1.29",
          "form": "맛있더라구요 ㅎㅎ",
          "original_form": "맛있더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100041360.1.30",
          "form": "맛있죠",
          "original_form": "맛있죠",
          "speaker_id": "2",
          "time": "20210907 13:58"
        },
        {
          "id": "MDRW2100041360.1.31",
          "form": "전 감자채볶음이랑 가지볶음 해서 먹었네요",
          "original_form": "전 감자채볶음이랑 가지볶음 해서 먹었네요",
          "speaker_id": "2",
          "time": "20210907 13:58"
        },
        {
          "id": "MDRW2100041360.1.32",
          "form": "맛있겠어요!",
          "original_form": "맛있겠어요!",
          "speaker_id": "1",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100041360.1.33",
          "form": "저는 저녁으로 보쌈먹으려구요",
          "original_form": "저는 저녁으로 보쌈먹으려구요",
          "speaker_id": "1",
          "time": "20210907 15:39"
        }
      ]
    }
