      "utterance": [
        {
          "id": "MDRW2100008984.1.1",
          "form": "요즘은 핫딜 없는가?.",
          "original_form": "요즘은 핫딜 없는가?.",
          "speaker_id": "2",
          "time": "20210822 20:11"
        },
        {
          "id": "MDRW2100008984.1.2",
          "form": "위메프 포인트행사도 아숩고이~",
          "original_form": "위메프 포인트행사도 아숩고이~",
          "speaker_id": "2",
          "time": "20210822 20:11"
        },
        {
          "id": "MDRW2100008984.1.3",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "1",
          "time": "20210822 20:11"
        },
        {
          "id": "MDRW2100008984.1.4",
          "form": "그건 이제안하려나봐ㅜㅠ",
          "original_form": "그건 이제안하려나봐ㅜㅠ",
          "speaker_id": "1",
          "time": "20210822 20:11"
        },
        {
          "id": "MDRW2100008984.1.5",
          "form": "요즘은 핫딜 잘 안뜨더라ㅋㅋ",
          "original_form": "요즘은 핫딜 잘 안뜨더라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:11"
        },
        {
          "id": "MDRW2100008984.1.6",
          "form": "내 잠시 애들 재우고 올게ㅋㅋ",
          "original_form": "내 잠시 애들 재우고 올게ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:12"
        },
        {
          "id": "MDRW2100008984.1.7",
          "form": "그니깐~위메프는 손해일끼라~",
          "original_form": "그니깐~위메프는 손해일끼라~",
          "speaker_id": "2",
          "time": "20210822 20:12"
        },
        {
          "id": "MDRW2100008984.1.8",
          "form": "다녀오시소^^",
          "original_form": "다녀오시소^^",
          "speaker_id": "2",
          "time": "20210822 20:13"
        },
        {
          "id": "MDRW2100008984.1.9",
          "form": "둘째는 자고 첫째는 조금이따가 잔대",
          "original_form": "둘째는 자고 첫째는 조금이따가 잔대",
          "speaker_id": "1",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100008984.1.10",
          "form": "자나?ㅋㅋ",
          "original_form": "자나?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100008984.1.11",
          "form": "잠들었다가ㅠ 둘찌 낮잠이불 빤다고 깼다ㅠ",
          "original_form": "잠들었다가ㅠ 둘찌 낮잠이불 빤다고 깼다ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100008984.1.12",
          "form": "와따 낮잠이불 한세트 더사야되나 ㅋㅋㅋㅋㅋ",
          "original_form": "와따 낮잠이불 한세트 더사야되나 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100008984.1.13",
          "form": "세탁기에서 걍 빨고 말리고 다되는거 사곱다이~",
          "original_form": "세탁기에서 걍 빨고 말리고 다되는거 사곱다이~",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100008984.1.14",
          "form": "낮잠이불ㅋㅋ내년이면 안녕하잖아ㅋ",
          "original_form": "낮잠이불ㅋㅋ내년이면 안녕하잖아ㅋ",
          "speaker_id": "1",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100008984.1.15",
          "form": "구래서 안사고있지 ㅋㅋㅋㅋㅋ",
          "original_form": "구래서 안사고있지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100008984.1.16",
          "form": "그러니깐ㅋㅋ",
          "original_form": "그러니깐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100008984.1.17",
          "form": "나도 둘째 낮잠이불은 바로 세탁한다ㅋㅋ",
          "original_form": "나도 둘째 낮잠이불은 바로 세탁한다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100008984.1.18",
          "form": "이불 안보내주면",
          "original_form": "이불 안보내주면",
          "speaker_id": "2",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100008984.1.19",
          "form": "먼가 안쓰럽다 ㅋㅋㅋㅋ",
          "original_form": "먼가 안쓰럽다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100008984.1.20",
          "form": "그러니깐ㅋㅋ",
          "original_form": "그러니깐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100008984.1.21",
          "form": "오늘 가을 양말을 쿠팡으로 시켰는데 꽃무늬 늠 예쁨 ㅋㅋㅋㅋ",
          "original_form": "오늘 가을 양말을 쿠팡으로 시켰는데 꽃무늬 늠 예쁨 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100008984.1.22",
          "form": "오오 양말 주문했나?",
          "original_form": "오오 양말 주문했나?",
          "speaker_id": "1",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100008984.1.23",
          "form": "이쁘겠다",
          "original_form": "이쁘겠다",
          "speaker_id": "1",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100008984.1.24",
          "form": "애들꺼?",
          "original_form": "애들꺼?",
          "speaker_id": "1",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100008984.1.25",
          "form": "나는 요즘 핫딜방 조용해서 재미가 없네ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나는 요즘 핫딜방 조용해서 재미가 없네ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100008984.1.26",
          "form": "웅~양말나라에서 걍 몇개씩 샀는데",
          "original_form": "웅~양말나라에서 걍 몇개씩 샀는데",
          "speaker_id": "2",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100008984.1.27",
          "form": "그래~",
          "original_form": "그래~",
          "speaker_id": "2",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100008984.1.28",
          "form": "요즘 핫딜 늠없네 ㅋㅋ",
          "original_form": "요즘 핫딜 늠없네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100008984.1.29",
          "form": "ㅋㅋㅋ 나도 양말은 한번살때 왕창산다ㅋ",
          "original_form": "ㅋㅋㅋ 나도 양말은 한번살때 왕창산다ㅋ",
          "speaker_id": "1",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100008984.1.30",
          "form": "쌀 물티슈도 핫딜은 잘 안보인다ㅋㅋ",
          "original_form": "쌀 물티슈도 핫딜은 잘 안보인다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100008984.1.31",
          "form": "물티도 난 요즘 걍 쿠팡으로 시킨다~",
          "original_form": "물티도 난 요즘 걍 쿠팡으로 시킨다~",
          "speaker_id": "2",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100008984.1.32",
          "form": "ㅋㅋ 쿠팡은 사랑이지ㅋㅋ",
          "original_form": "ㅋㅋ 쿠팡은 사랑이지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100008984.1.33",
          "form": "걍 필요할때 10개씩 ㅋㅋㅋㅋ",
          "original_form": "걍 필요할때 10개씩 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100008984.1.34",
          "form": "쿠팡 없음 불안함 ㅋㅋㅋㅋ",
          "original_form": "쿠팡 없음 불안함 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100008984.1.35",
          "form": "나도 둘째 바지사야되는데ㅋ",
