      "utterance": [
        {
          "id": "MDRW2100033140.1.1",
          "form": "안녕하세요 반갑습니다",
          "original_form": "안녕하세요 반갑습니다",
          "speaker_id": "1",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100033140.1.2",
          "form": "날씨가 선선해지면서 가을이 성큼 다가왔습니다",
          "original_form": "날씨가 선선해지면서 가을이 성큼 다가왔습니다",
          "speaker_id": "1",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100033140.1.3",
          "form": "가을이오면 어느 것이 가장 기대되시나요?",
          "original_form": "가을이오면 어느 것이 가장 기대되시나요?",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100033140.1.4",
          "form": "일단 옷차림이 길어져서 좋습니다.",
          "original_form": "일단 옷차림이 길어져서 좋습니다.",
          "speaker_id": "2",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100033140.1.5",
          "form": "긴옷을 좋아하는 군요",
          "original_form": "긴옷을 좋아하는 군요",
          "speaker_id": "1",
          "time": "20210908 06:49"
        },
        {
          "id": "MDRW2100033140.1.6",
          "form": "네ㅎㅎ 반팔 반바지는 별로 안 좋아해서요. 어느것이 기대되세요?",
          "original_form": "네ㅎㅎ 반팔 반바지는 별로 안 좋아해서요. 어느것이 기대되세요?",
          "speaker_id": "2",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100033140.1.7",
          "form": "저는 가을 먹거리가 기대됩니다",
          "original_form": "저는 가을 먹거리가 기대됩니다",
          "speaker_id": "1",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100033140.1.8",
          "form": "특히 감을 좋아합니다",
          "original_form": "특히 감을 좋아합니다",
          "speaker_id": "1",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100033140.1.9",
          "form": "와 연시요 아님 단감이요",
          "original_form": "와 연시요 아님 단감이요",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100033140.1.10",
          "form": "단감 홍시 곶감 다 좋아합니다",
          "original_form": "단감 홍시 곶감 다 좋아합니다",
          "speaker_id": "1",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100033140.1.11",
          "form": "와 감을 진짜 좋아하시네요",
          "original_form": "와 감을 진짜 좋아하시네요",
          "speaker_id": "2",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100033140.1.12",
          "form": "감말랭이도?ㅋㅋ",
          "original_form": "감말랭이도?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100033140.1.13",
          "form": "그것도 맛있지요",
          "original_form": "그것도 맛있지요",
          "speaker_id": "1",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100033140.1.14",
          "form": "아이스홍시도 좋아합니다",
          "original_form": "아이스홍시도 좋아합니다",
          "speaker_id": "1",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100033140.1.15",
          "form": "감이 풍년이면 좋겠네요",
          "original_form": "감이 풍년이면 좋겠네요",
          "speaker_id": "2",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100033140.1.16",
          "form": "감은 2년에 한번씩 풍년이라서요",
          "original_form": "감은 2년에 한번씩 풍년이라서요",
          "speaker_id": "1",
          "time": "20210908 07:38"
        },
        {
          "id": "MDRW2100033140.1.17",
          "form": "작년엔 어땠는지 기억이 안나네요",
          "original_form": "작년엔 어땠는지 기억이 안나네요",
          "speaker_id": "1",
          "time": "20210908 07:38"
        },
        {
          "id": "MDRW2100033140.1.18",
          "form": "와 그렇군요",
          "original_form": "와 그렇군요",
          "speaker_id": "2",
          "time": "20210908 07:38"
        },
        {
          "id": "MDRW2100033140.1.19",
          "form": "역시 가을은 수확의 계절이에요",
          "original_form": "역시 가을은 수확의 계절이에요",
          "speaker_id": "2",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100033140.1.20",
          "form": "그래서 좋습니다",
          "original_form": "그래서 좋습니다",
          "speaker_id": "1",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100033140.1.21",
          "form": "마음까지 풍성해지지요",
          "original_form": "마음까지 풍성해지지요",
          "speaker_id": "1",
          "time": "20210908 07:42"
        }
