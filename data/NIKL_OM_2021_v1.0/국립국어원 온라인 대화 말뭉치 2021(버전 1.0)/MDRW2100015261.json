      "utterance": [
        {
          "id": "MDRW2100015261.1.1",
          "form": "기네스죠ㅎㅎ",
          "original_form": "기네스죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015261.1.2",
          "form": "흑맥주는ㅎㅎㅎ",
          "original_form": "흑맥주는ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015261.1.3",
          "form": "뭐드세요?",
          "original_form": "뭐드세요?",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015261.1.4",
          "form": "저는 사실 흑맥주는 그닥ㅋㅋㅋㅋ",
          "original_form": "저는 사실 흑맥주는 그닥ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015261.1.5",
          "form": "근데 그 술.집가면파는",
          "original_form": "근데 그 술.집가면파는",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015261.1.6",
          "form": "흑설탕뿌려주는? 그거 비쥬얼이 혹하더라고요",
          "original_form": "흑설탕뿌려주는? 그거 비쥬얼이 혹하더라고요",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015261.1.7",
          "form": "ㅎㅎ 좋죠ㅎㅎ",
          "original_form": "ㅎㅎ 좋죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015261.1.8",
          "form": "아는사란만아는 맛ㅎㅎㅎ",
          "original_form": "아는사란만아는 맛ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015261.1.9",
          "form": "맥주에 어울리는 안주는?",
          "original_form": "맥주에 어울리는 안주는?",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015261.1.10",
          "form": "ㅋㅋㅋ 사실 맥주를 즐기는편은아니랍니다",
          "original_form": "ㅋㅋㅋ 사실 맥주를 즐기는편은아니랍니다",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015261.1.11",
          "form": "그나마 제일좋아하는술은",
          "original_form": "그나마 제일좋아하는술은",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015261.1.12",
          "form": "하이볼이죠",
          "original_form": "하이볼이죠",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015261.1.13",
          "form": "ㅎㅎㅎ 그럼 배달음식은 자주시켜드세요m",
          "original_form": "ㅎㅎㅎ 그럼 배달음식은 자주시켜드세요m",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015261.1.14",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015261.1.15",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015261.1.16",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015261.1.17",
          "form": "저는 주말이면,거의 대부분 시켜먹어요ㅎㅎ",
          "original_form": "저는 주말이면,거의 대부분 시켜먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015261.1.18",
          "form": "메뉴는 우선 제가 고르는게 아니라ㅎㅎ",
          "original_form": "메뉴는 우선 제가 고르는게 아니라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015261.1.19",
          "form": "ㅋㅋㅋ 그럼",
          "original_form": "ㅋㅋㅋ 그럼",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015261.1.20",
          "form": "누가고르나요?",
          "original_form": "누가고르나요?",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015261.1.21",
          "form": "아내가 좋아하는거 골라요",
          "original_form": "아내가 좋아하는거 골라요",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015261.1.22",
          "form": "그래서 좀 싫네요ㄹㄹㄹㄹ",
          "original_form": "그래서 좀 싫네요ㄹㄹㄹㄹ",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015261.1.23",
          "form": "맨날떡볶이... ㅠ",
          "original_form": "맨날떡볶이... ㅠ",
