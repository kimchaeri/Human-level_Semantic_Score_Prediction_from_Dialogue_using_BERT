      "utterance": [
        {
          "id": "MDRW2100004539.1.1",
          "form": "요즘 아침은 뭘로 드시나요?",
          "original_form": "요즘 아침은 뭘로 드시나요?",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004539.1.2",
          "form": "그 닭칼국수 김치먹어보셧나요?",
          "original_form": "그 닭칼국수 김치먹어보셧나요?",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004539.1.3",
          "form": "그 김치가 진짜 짜파게티랑먹으면 장난아니에요",
          "original_form": "그 김치가 진짜 짜파게티랑먹으면 장난아니에요",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004539.1.4",
          "form": "무슨소리예요 닭칼을 사면 주는 김치를 짜파게티랑 먹는다는 건가요?",
          "original_form": "무슨소리예요 닭칼을 사면 주는 김치를 짜파게티랑 먹는다는 건가요?",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004539.1.5",
          "form": "닭칼에 대한 배신 아닌가요?",
          "original_form": "닭칼에 대한 배신 아닌가요?",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004539.1.6",
          "form": "참내 닭칼도 헤치워주고먹는거죠",
          "original_form": "참내 닭칼도 헤치워주고먹는거죠",
          "speaker_id": "1",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004539.1.7",
          "form": "존경스러워요",
          "original_form": "존경스러워요",
          "speaker_id": "2",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004539.1.8",
          "form": "저는 아직 아침 전인데",
          "original_form": "저는 아직 아침 전인데",
          "speaker_id": "2",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004539.1.9",
          "form": "추천해줄 수 있나요?",
          "original_form": "추천해줄 수 있나요?",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004539.1.10",
          "form": "안타까워요",
          "original_form": "안타까워요",
          "speaker_id": "1",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004539.1.11",
          "form": "물 소금 설탕",
          "original_form": "물 소금 설탕",
          "speaker_id": "1",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004539.1.12",
          "form": "주식 말고 특별식 먹고싶어요",
          "original_form": "주식 말고 특별식 먹고싶어요",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004539.1.13",
          "form": "특별식 짬뽕어떠세요",
          "original_form": "특별식 짬뽕어떠세요",
          "speaker_id": "1",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004539.1.14",
          "form": "송장",
          "original_form": "송장",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004539.1.15",
          "form": "안그래도 배달시킬까 했는데 폭우가 와서 고민중이에요",
          "original_form": "안그래도 배달시킬까 했는데 폭우가 와서 고민중이에요",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004539.1.16",
          "form": "아 저런... 그럼 name2님이 사러가요",
          "original_form": "아 저런... 그럼 &name2&님이 사러가요",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004539.1.17",
          "form": "비를맞고..",
          "original_form": "비를맞고..",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004539.1.18",
          "form": "눈물젖은짬뽕을",
          "original_form": "눈물젖은짬뽕을",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004539.1.19",
          "form": "쟁취해요",
          "original_form": "쟁취해요",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004539.1.20",
          "form": "선넘네요 배달뽀이보단 제가 소중해요",
          "original_form": "선넘네요 배달뽀이보단 제가 소중해요",
          "speaker_id": "2",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004539.1.21",
          "form": "답변이 도움됐어요 시킵니다",
          "original_form": "답변이 도움됐어요 시킵니다",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004539.1.22",
          "form": "역시 전 현명해요",
          "original_form": "역시 전 현명해요",
          "speaker_id": "1",
          "time": "20210821 17:15"
