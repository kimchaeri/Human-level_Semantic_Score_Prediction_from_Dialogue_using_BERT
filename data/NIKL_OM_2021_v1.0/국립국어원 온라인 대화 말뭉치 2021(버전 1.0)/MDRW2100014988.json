      "utterance": [
        {
          "id": "MDRW2100014988.1.1",
          "form": "닌자컨셉..",
          "original_form": "닌자컨셉..",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014988.1.2",
          "form": "뮤비 spp 수리검던지기",
          "original_form": "뮤비 spp 수리검던지기",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014988.1.3",
          "form": "칼도 쓰고 우산도 돌리는데 수리검이 안될 리가",
          "original_form": "칼도 쓰고 우산도 돌리는데 수리검이 안될 리가",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014988.1.4",
          "form": "ㅠㅠ 앙스타 제발.. 잘하자",
          "original_form": "ㅠㅠ 앙스타 제발.. 잘하자",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014988.1.5",
          "form": "..",
          "original_form": "..",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014988.1.6",
          "form": "나 에투제가 싫은 건 아닌데",
          "original_form": "나 에투제가 싫은 건 아닌데",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014988.1.7",
          "form": "에투제 정말 좋은데",
          "original_form": "에투제 정말 좋은데",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014988.1.8",
          "form": "치아키가 차라리 다른 컨셉으로 왔으면",
          "original_form": "치아키가 차라리 다른 컨셉으로 왔으면",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014988.1.9",
          "form": "어땠을까 싶다",
          "original_form": "어땠을까 싶다",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014988.1.10",
          "form": ".....",
          "original_form": ".....",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014988.1.11",
          "form": "초록정장도 한몫했고..",
          "original_form": "초록정장도 한몫했고..",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014988.1.12",
          "form": "ㅠㅠ 근데 스토리도 좋았고",
          "original_form": "ㅠㅠ 근데 스토리도 좋았고",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014988.1.13",
          "form": "카오루랑 같이 나와서.. 포기할 수 없음",
          "original_form": "카오루랑 같이 나와서.. 포기할 수 없음",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014988.1.14",
          "form": "...그건 맞다",
          "original_form": "...그건 맞다",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014988.1.15",
          "form": "즈!!로 업뎃되고 나서 제일 잘한 이벤트라고 생각",
          "original_form": "즈!!로 업뎃되고 나서 제일 잘한 이벤트라고 생각",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014988.1.16",
          "form": "하필 같이 나온 상대가.. 카오루..",
          "original_form": "하필 같이 나온 상대가.. 카오루..",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014988.1.17",
          "form": "초록정장에 속아 소중함을 잊지 말자",
          "original_form": "초록정장에 속아 소중함을 잊지 말자",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014988.1.18",
          "form": "초록정장만 생각하면",
          "original_form": "초록정장만 생각하면",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014988.1.19",
          "form": "누워있다가도 벌떡 일어날수잇음",
          "original_form": "누워있다가도 벌떡 일어날수잇음",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014988.1.20",
          "form": "ㅠㅠㅠ 근데 넘 귀여워",
          "original_form": "ㅠㅠㅠ 근데 넘 귀여워",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014988.1.21",
          "form": "모리사와군...",
          "original_form": "모리사와군...",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014988.1.22",
          "form": "ㅜㅜㅜㅜㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014988.1.23",
          "form": "귀여운 애들 둘이서.. 5성 먹고..",
          "original_form": "귀여운 애들 둘이서.. 5성 먹고..",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014988.1.24",
          "form": "나와 데이트하장!",
          "original_form": "나와 데이트하장!",
          "speaker_id": "2",
          "time": "20210825 21:32"
