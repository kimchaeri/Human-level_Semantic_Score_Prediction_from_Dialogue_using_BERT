      "utterance": [
        {
          "id": "MDRW2100025017.1.1",
          "form": "명랑핫도그도 좋아하세요?",
          "original_form": "명랑핫도그도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100025017.1.2",
          "form": "핫도그 너무좋아하는데",
          "original_form": "핫도그 너무좋아하는데",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100025017.1.3",
          "form": "명랑은 못먹어본 사람...",
          "original_form": "명랑은 못먹어본 사람...",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100025017.1.4",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100025017.1.5",
          "form": "어째서죠?!?!",
          "original_form": "어째서죠?!?!",
          "speaker_id": "1",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100025017.1.6",
          "form": "ㅋㅋㄱㅋㅋㅋ 냉동핫도그만먹어서...",
          "original_form": "ㅋㅋㄱㅋㅋㅋ 냉동핫도그만먹어서...",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100025017.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025017.1.8",
          "form": "사실 시켜먹어볼일이없어서",
          "original_form": "사실 시켜먹어볼일이없어서",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025017.1.9",
          "form": "근데 명랑은",
          "original_form": "근데 명랑은",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025017.1.10",
          "form": "기본이 넘 작아요",
          "original_form": "기본이 넘 작아요",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025017.1.11",
          "form": "먹어보고싶어요ㅋㄱㅋㄱ",
          "original_form": "먹어보고싶어요ㅋㄱㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025017.1.12",
          "form": "근데명랑은 무조건",
          "original_form": "근데명랑은 무조건",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025017.1.13",
          "form": "냉동핫도그 튼실한 거 드셨으면",
          "original_form": "냉동핫도그 튼실한 거 드셨으면",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025017.1.14",
          "form": "바깥에설탕토핑되어있나요?",
          "original_form": "바깥에설탕토핑되어있나요?",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025017.1.15",
          "form": "기본 보시면 이게핫더그..?ㅋ",
          "original_form": "기본 보시면 이게핫더그..?ㅋ",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025017.1.16",
          "form": "이러실거예요",
          "original_form": "이러실거예요",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025017.1.17",
          "form": "아하...",
          "original_form": "아하...",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025017.1.18",
          "form": "그 나오는 건 그냥 아무것도 안 바른 자연의 핫도그상태로 나오고",
          "original_form": "그 나오는 건 그냥 아무것도 안 바른 자연의 핫도그상태로 나오고",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025017.1.19",
          "form": "설탕이랑 소스는",
          "original_form": "설탕이랑 소스는",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025017.1.20",
          "form": "셀프바에서 직접 뿌려 먹는 거예요!",
          "original_form": "셀프바에서 직접 뿌려 먹는 거예요!",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025017.1.21",
          "form": "전 기본형을 좋아해서",
          "original_form": "전 기본형을 좋아해서",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025017.1.22",
          "form": "설탕안뿌리는데 명랑 사진보면",
          "original_form": "설탕안뿌리는데 명랑 사진보면",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100025017.1.23",
          "form": "다 설탕이막뿌려져있더라구요",
          "original_form": "다 설탕이막뿌려져있더라구요",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100025017.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100025017.1.25",
          "form": "사람들이 다 설탕좋아하나봐요",
          "original_form": "사람들이 다 설탕좋아하나봐요",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100025017.1.26",
          "form": "근데진짜 명랑을 어떻게 지금까지 안먹어봤지",
          "original_form": "근데진짜 명랑을 어떻게 지금까지 안먹어봤지",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100025017.1.27",
          "form": "설탕뿌리지않는 걸로 옵션 있을거예요",
          "original_form": "설탕뿌리지않는 걸로 옵션 있을거예요",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100025017.1.28",
          "form": "카디비도 명랑먹어봤는데",
          "original_form": "카디비도 명랑먹어봤는데",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100025017.1.29",
          "form": "나만못먹어봤네...",
          "original_form": "나만못먹어봤네...",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100025017.1.30",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100025017.1.31",
          "form": "카디비보다 한국의 문화를 못 누린 사람!!!",
          "original_form": "카디비보다 한국의 문화를 못 누린 사람!!!",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100025017.1.32",
          "form": "내일먹어봐야겠어요",
          "original_form": "내일먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210831 22:16"
        }
      ]
    }
