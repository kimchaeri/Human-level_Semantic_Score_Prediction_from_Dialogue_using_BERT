      "utterance": [
        {
          "id": "MDRW2100041600.1.1",
          "form": "일하세요?",
          "original_form": "일하세요?",
          "speaker_id": "1",
          "time": "20210906 12:08"
        },
        {
          "id": "MDRW2100041600.1.2",
          "form": "네네, 재택 프리랜서예요",
          "original_form": "네네, 재택 프리랜서예요",
          "speaker_id": "2",
          "time": "20210906 12:08"
        },
        {
          "id": "MDRW2100041600.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210906 12:09"
        },
        {
          "id": "MDRW2100041600.1.4",
          "form": "와 좋겠어요ㅋㅋ",
          "original_form": "와 좋겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:09"
        },
        {
          "id": "MDRW2100041600.1.5",
          "form": "저는 복지관에서 미술보조강사로 알바다녀요",
          "original_form": "저는 복지관에서 미술보조강사로 알바다녀요",
          "speaker_id": "1",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100041600.1.6",
          "form": "오, 미술 전공이신 거예요??",
          "original_form": "오, 미술 전공이신 거예요??",
          "speaker_id": "2",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100041600.1.7",
          "form": "재택 프리랜서면 시간조절가능한거예요?ㅋ",
          "original_form": "재택 프리랜서면 시간조절가능한거예요?ㅋ",
          "speaker_id": "1",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100041600.1.8",
          "form": "미술전공은 아닌데ㅋㅋ",
          "original_form": "미술전공은 아닌데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100041600.1.9",
          "form": "소개로 원서한번 넣어봤거든요.ㅋㅋ",
          "original_form": "소개로 원서한번 넣어봤거든요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100041600.1.10",
          "form": "네네, 마감기한만 지키면 되고 출퇴근 개념이 없어요",
          "original_form": "네네, 마감기한만 지키면 되고 출퇴근 개념이 없어요",
          "speaker_id": "2",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100041600.1.11",
          "form": "오, 복지관이면 아동인가요? 어르신?",
          "original_form": "오, 복지관이면 아동인가요? 어르신?",
          "speaker_id": "2",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100041600.1.12",
          "form": "너무좋으시겠어요.ㅋ",
          "original_form": "너무좋으시겠어요.ㅋ",
          "speaker_id": "1",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100041600.1.13",
          "form": "맞아요ㅋㅋㅋ 시간장소의 제약이 없죠",
          "original_form": "맞아요ㅋㅋㅋ 시간장소의 제약이 없죠",
          "speaker_id": "2",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100041600.1.14",
          "form": "그러니깐요ㅋㅋ",
          "original_form": "그러니깐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100041600.1.15",
          "form": "제가 원하는ㅋㅋ직업인데요ㅋㅋ",
          "original_form": "제가 원하는ㅋㅋ직업인데요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100041600.1.16",
          "form": "저는 몸이 불편하신분들 옆에서 도와드리고있어요ㅋㅋ",
          "original_form": "저는 몸이 불편하신분들 옆에서 도와드리고있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100041600.1.17",
          "form": "재택 일을 6년째 하고 있는데 주부분들도 많이 하세요ㅋㅋㅋ",
          "original_form": "재택 일을 6년째 하고 있는데 주부분들도 많이 하세요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100041600.1.18",
          "form": "아하, 장애인분들이시군요",
          "original_form": "아하, 장애인분들이시군요",
          "speaker_id": "2",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100041600.1.19",
          "form": "네네ㅋㅋ",
          "original_form": "네네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100041600.1.20",
          "form": "어떤일이신거예요?",
          "original_form": "어떤일이신거예요?",
          "speaker_id": "1",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100041600.1.21",
          "form": "지금 하는 건 교정교열이랑 한자>한글번역이예요",
          "original_form": "지금 하는 건 교정교열이랑 한자>한글번역이예요",
          "speaker_id": "2",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100041600.1.22",
          "form": "와 한자 한글번역ㅋㅋ멋있어요. 한자 잘 아시나봐요",
          "original_form": "와 한자 한글번역ㅋㅋ멋있어요. 한자 잘 아시나봐요",
          "speaker_id": "1",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100041600.1.23",
          "form": "어릴 때부터 배워서요ㅎㅎ",
          "original_form": "어릴 때부터 배워서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100041600.1.24",
          "form": "와 대단하세요.ㅋㅋ",
          "original_form": "와 대단하세요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:23"
        }
      ]
    }
