      "utterance": [
        {
          "id": "MDRW2100007701.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210822 09:47"
        },
        {
          "id": "MDRW2100007701.1.2",
          "form": "좋아합니다! 최근엔 못갔지만요ㅜ",
          "original_form": "좋아합니다! 최근엔 못갔지만요ㅜ",
          "speaker_id": "2",
          "time": "20210822 09:49"
        },
        {
          "id": "MDRW2100007701.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210822 09:49"
        },
        {
          "id": "MDRW2100007701.1.4",
          "form": "코로나 끝나면 가고 싶은 곳 있으세요?",
          "original_form": "코로나 끝나면 가고 싶은 곳 있으세요?",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007701.1.5",
          "form": "국외로요 ㅋㅋ",
          "original_form": "국외로요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007701.1.6",
          "form": "스페인 가보고 싶아뇨",
          "original_form": "스페인 가보고 싶아뇨",
          "speaker_id": "1",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007701.1.7",
          "form": "오 저는 가까운 나라 일본이라도 가보싶어요 ㅋㅋ",
          "original_form": "오 저는 가까운 나라 일본이라도 가보싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007701.1.8",
          "form": "국내는그래도 다닐수있긴한데 애매하죠ㅜ",
          "original_form": "국내는그래도 다닐수있긴한데 애매하죠ㅜ",
          "speaker_id": "2",
          "time": "20210822 09:53"
        },
        {
          "id": "MDRW2100007701.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007701.1.10",
          "form": "저는 서울 친구 못보러가겠어요",
          "original_form": "저는 서울 친구 못보러가겠어요",
          "speaker_id": "2",
          "time": "20210822 10:09"
        },
        {
          "id": "MDRW2100007701.1.11",
          "form": "늘 놀러오라고는하는데",
          "original_form": "늘 놀러오라고는하는데",
          "speaker_id": "2",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100007701.1.12",
          "form": "서울 너무 많이 나와서",
          "original_form": "서울 너무 많이 나와서",
          "speaker_id": "1",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007701.1.13",
          "form": "사실 결혼식도 못갔어요ㅜ",
          "original_form": "사실 결혼식도 못갔어요ㅜ",
          "speaker_id": "2",
          "time": "20210822 10:25"
        },
        {
          "id": "MDRW2100007701.1.14",
          "form": "그냥 몸만오랬는데 ㅋㅋ",
          "original_form": "그냥 몸만오랬는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:25"
        },
        {
          "id": "MDRW2100007701.1.15",
          "form": "최근에 여행다녀오신곳은 있으신가요",
          "original_form": "최근에 여행다녀오신곳은 있으신가요",
          "speaker_id": "2",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100007701.1.16",
          "form": "호주에",
          "original_form": "호주에",
          "speaker_id": "1",
          "time": "20210822 11:37"
        },
        {
          "id": "MDRW2100007701.1.17",
          "form": "얼마동안 다녀오셨나요",
          "original_form": "얼마동안 다녀오셨나요",
          "speaker_id": "2",
