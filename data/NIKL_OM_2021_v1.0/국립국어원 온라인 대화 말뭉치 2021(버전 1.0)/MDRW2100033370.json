        },
        {
          "id": "MDRW2100033370.1.2",
          "form": "나 친구도 디피 봤다더라",
          "original_form": "나 친구도 디피 봤다더라",
          "speaker_id": "1",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100033370.1.3",
          "form": "지금 인기인 군대 드라마 말하는 거지",
          "original_form": "지금 인기인 군대 드라마 말하는 거지",
          "speaker_id": "2",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100033370.1.4",
          "form": "군대를 다녀온 친구라 그런지 정말 재밌게 봤다더라",
          "original_form": "군대를 다녀온 친구라 그런지 정말 재밌게 봤다더라",
          "speaker_id": "1",
          "time": "20210906 23:23"
        },
        {
          "id": "MDRW2100033370.1.5",
          "form": "그랬겠다",
          "original_form": "그랬겠다",
          "speaker_id": "2",
          "time": "20210906 23:23"
        },
        {
          "id": "MDRW2100033370.1.6",
          "form": "심지어 자기 부대에 건물도 드라마에 나왔다더라",
          "original_form": "심지어 자기 부대에 건물도 드라마에 나왔다더라",
          "speaker_id": "1",
          "time": "20210906 23:24"
        },
        {
          "id": "MDRW2100033370.1.7",
          "form": "오랜만에 좋았겠네",
          "original_form": "오랜만에 좋았겠네",
          "speaker_id": "2",
          "time": "20210906 23:25"
        },
        {
          "id": "MDRW2100033370.1.8",
          "form": "자기 군대 동기들도 이 드라마 얘기만 한다더라",
          "original_form": "자기 군대 동기들도 이 드라마 얘기만 한다더라",
          "speaker_id": "1",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100033370.1.9",
          "form": "추억에 잠겼겠네",
          "original_form": "추억에 잠겼겠네",
          "speaker_id": "2",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100033370.1.10",
          "form": "그래도 자기 된 가혹행위는 없었다던데",
          "original_form": "그래도 자기 된 가혹행위는 없었다던데",
          "speaker_id": "1",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100033370.1.11",
          "form": "2014년 이후의 군대가 바뀌었나봐",
          "original_form": "2014년 이후의 군대가 바뀌었나봐",
          "speaker_id": "2",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100033370.1.12",
          "form": "다행인 일이지",
