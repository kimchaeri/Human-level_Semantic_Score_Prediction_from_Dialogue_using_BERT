      "utterance": [
        {
          "id": "MDRW2100017338.1.1",
          "form": "꿔바로우 먹고싶다",
          "original_form": "꿔바로우 먹고싶다",
          "speaker_id": "1",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017338.1.2",
          "form": "오 공연좋지",
          "original_form": "오 공연좋지",
          "speaker_id": "2",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100017338.1.3",
          "form": "뮤지컬 보고싶다 ",
          "original_form": "뮤지컬 보고싶다 ",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017338.1.4",
          "form": "오늘은 비와서",
          "original_form": "오늘은 비와서",
          "speaker_id": "2",
          "time": "20210831 09:27"
        },
        {
          "id": "MDRW2100017338.1.5",
          "form": "우취각인가",
          "original_form": "우취각인가",
          "speaker_id": "2",
          "time": "20210831 09:27"
        },
        {
          "id": "MDRW2100017338.1.6",
          "form": "비 올지 안올지 모르는고 아냐?",
          "original_form": "비 올지 안올지 모르는고 아냐?",
          "speaker_id": "1",
          "time": "20210831 09:29"
        },
        {
          "id": "MDRW2100017338.1.7",
          "form": "비가오면 무슨작품을",
          "original_form": "비가오면 무슨작품을",
          "speaker_id": "2",
          "time": "20210831 10:11"
        },
        {
          "id": "MDRW2100017338.1.8",
          "form": "보는게 좋을까",
          "original_form": "보는게 좋을까",
          "speaker_id": "2",
          "time": "20210831 10:11"
        },
        {
          "id": "MDRW2100017338.1.9",
          "form": "오늘 전국비랬는데",
          "original_form": "오늘 전국비랬는데",
          "speaker_id": "2",
          "time": "20210831 10:11"
        },
        {
          "id": "MDRW2100017338.1.10",
          "form": "비오면...센치한거?",
          "original_form": "비오면...센치한거?",
          "speaker_id": "1",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100017338.1.11",
          "form": "추천해주실?",
          "original_form": "추천해주실?",
          "speaker_id": "2",
          "time": "20210831 12:02"
        },
        {
          "id": "MDRW2100017338.1.12",
          "form": "영화?",
          "original_form": "영화?",
          "speaker_id": "1",
          "time": "20210831 12:14"
        },
        {
          "id": "MDRW2100017338.1.13",
          "form": "좋아 영화",
          "original_form": "좋아 영화",
          "speaker_id": "2",
          "time": "20210831 12:32"
        },
        {
          "id": "MDRW2100017338.1.14",
          "form": "외화하나 국영하나",
          "original_form": "외화하나 국영하나",
          "speaker_id": "2",
          "time": "20210831 12:32"
        },
        {
          "id": "MDRW2100017338.1.15",
          "form": "이렇게 추천 ㄱㄱ",
          "original_form": "이렇게 추천 ㄱㄱ",
          "speaker_id": "2",
          "time": "20210831 12:32"
        },
        {
          "id": "MDRW2100017338.1.16",
          "form": "혹시 좀 스릴러공포도 괜찮니?",
          "original_form": "혹시 좀 스릴러공포도 괜찮니?",
          "speaker_id": "1",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100017338.1.17",
          "form": "앗 일단 해줘봐",
          "original_form": "앗 일단 해줘봐",
          "speaker_id": "2",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100017338.1.18",
          "form": "최근에 본건데 미스트(영화) 진짜 괜찮게 봤어",
          "original_form": "최근에 본건데 미스트(영화) 진짜 괜찮게 봤어",
          "speaker_id": "1",
          "time": "20210831 16:12"
        },
        {
          "id": "MDRW2100017338.1.19",
          "form": "근데 많이 잔인해서 고려하고 봐야해",
          "original_form": "근데 많이 잔인해서 고려하고 봐야해",
          "speaker_id": "1",
          "time": "20210831 16:13"
        },
        {
          "id": "MDRW2100017338.1.20",
          "form": "오ㅏ 나는그거",
          "original_form": "오ㅏ 나는그거",
          "speaker_id": "2",
          "time": "20210831 16:17"
        },
        {
          "id": "MDRW2100017338.1.21",
          "form": "겜으로..했어",
          "original_form": "겜으로..했어",
          "speaker_id": "2",
          "time": "20210831 16:17"
        },
        {
          "id": "MDRW2100017338.1.22",
          "form": "영상미가좋은거야?",
          "original_form": "영상미가좋은거야?",
          "speaker_id": "2",
          "time": "20210831 16:17"
        },
        {
          "id": "MDRW2100017338.1.23",
          "form": "오잉 게임이 있던가?",
          "original_form": "오잉 게임이 있던가?",
          "speaker_id": "1",
          "time": "20210831 18:34"
        },
        {
          "id": "MDRW2100017338.1.24",
          "form": "아니 엔딩이 되게 인상깊었어",
          "original_form": "아니 엔딩이 되게 인상깊었어",
          "speaker_id": "1",
          "time": "20210831 18:34"
        },
        {
          "id": "MDRW2100017338.1.25",
          "form": "그래 겜도ㅋㅋㅋ엔딩이",
          "original_form": "그래 겜도ㅋㅋㅋ엔딩이",
          "speaker_id": "2",
          "time": "20210831 18:35"
        },
        {
          "id": "MDRW2100017338.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:35"
        },
        {
          "id": "MDRW2100017338.1.27",
          "form": "한번 찾아보고 영화 봐볼게",
          "original_form": "한번 찾아보고 영화 봐볼게",
          "speaker_id": "2",
          "time": "20210831 18:35"
        },
        {
          "id": "MDRW2100017338.1.28",
          "form": "국산영화는",
          "original_form": "국산영화는",
          "speaker_id": "2",
          "time": "20210831 18:35"
        },
        {
          "id": "MDRW2100017338.1.29",
          "form": "추천해주고싶은거 있어?",
          "original_form": "추천해주고싶은거 있어?",
          "speaker_id": "2",
          "time": "20210831 18:35"
        },
        {
          "id": "MDRW2100017338.1.30",
          "form": "게임이 있는줄은 몰랐네 ㅋㅋㅋㅋ",
          "original_form": "게임이 있는줄은 몰랐네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:35"
        },
        {
          "id": "MDRW2100017338.1.31",
          "form": "국산은...최근에 뭐 딱히 본게 없어서 흠",
          "original_form": "국산은...최근에 뭐 딱히 본게 없어서 흠",
          "speaker_id": "1",
          "time": "20210831 18:36"
        },
        {
          "id": "MDRW2100017338.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ진짜",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ진짜",
          "speaker_id": "2",
          "time": "20210831 18:36"
        },
        {
          "id": "MDRW2100017338.1.33",
          "form": "한국문화와 거리가 더 멀다",
          "original_form": "한국문화와 거리가 더 멀다",
          "speaker_id": "2",
          "time": "20210831 18:36"
        },
        {
          "id": "MDRW2100017338.1.34",
          "form": "(˘̩̩̩ε˘̩ƪ)",
          "original_form": "(˘̩̩̩ε˘̩ƪ)",
          "speaker_id": "2",
          "time": "20210831 18:36"
        },
        {
          "id": "MDRW2100017338.1.35",
          "form": "아 사바하 재밌었어",
          "original_form": "아 사바하 재밌었어",
          "speaker_id": "1",
          "time": "20210831 18:36"
        },
        {
          "id": "MDRW2100017338.1.36",
          "form": "아아아 니가",
          "original_form": "아아아 니가",
          "speaker_id": "2",
          "time": "20210831 18:36"
        },
        {
          "id": "MDRW2100017338.1.37",
          "form": "추천해줬던거같다",
          "original_form": "추천해줬던거같다",
          "speaker_id": "2",
          "time": "20210831 18:36"
        },
        {
          "id": "MDRW2100017338.1.38",
          "form": "오키 위시리스트에 추가",
          "original_form": "오키 위시리스트에 추가",
          "speaker_id": "2",
          "time": "20210831 18:36"
        },
        {
          "id": "MDRW2100017338.1.39",
          "form": "검은사제들이랑 같은 감독 ㅇㅇ",
          "original_form": "검은사제들이랑 같은 감독 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210831 18:36"
        }
      ]
    }
