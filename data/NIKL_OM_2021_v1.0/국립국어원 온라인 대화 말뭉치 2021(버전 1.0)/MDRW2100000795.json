      "utterance": [
        {
          "id": "MDRW2100000795.1.1",
          "form": "백신 예약했어?",
          "original_form": "백신 예약했어?",
          "speaker_id": "1",
          "time": "20210819 19:29"
        },
        {
          "id": "MDRW2100000795.1.2",
          "form": "응 했는데 짜증남",
          "original_form": "응 했는데 짜증남",
          "speaker_id": "2",
          "time": "20210819 19:30"
        },
        {
          "id": "MDRW2100000795.1.3",
          "form": "나는 수월하게 한듯",
          "original_form": "나는 수월하게 한듯",
          "speaker_id": "1",
          "time": "20210819 19:30"
        },
        {
          "id": "MDRW2100000795.1.4",
          "form": "난 튕겨서 원하는 날짜못함",
          "original_form": "난 튕겨서 원하는 날짜못함",
          "speaker_id": "2",
          "time": "20210819 19:30"
        },
        {
          "id": "MDRW2100000795.1.5",
          "form": "많이 밀렸어?",
          "original_form": "많이 밀렸어?",
          "speaker_id": "1",
          "time": "20210819 19:30"
        },
        {
          "id": "MDRW2100000795.1.6",
          "form": "구월초하려했는디 중반",
          "original_form": "구월초하려했는디 중반",
          "speaker_id": "2",
          "time": "20210819 19:31"
        },
        {
          "id": "MDRW2100000795.1.7",
          "form": "나는 팔월 삼십일",
          "original_form": "나는 팔월 삼십일",
          "speaker_id": "1",
          "time": "20210819 19:31"
        },
        {
          "id": "MDRW2100000795.1.8",
          "form": "난 구월 십사일",
          "original_form": "난 구월 십사일",
          "speaker_id": "2",
          "time": "20210819 19:31"
        },
        {
          "id": "MDRW2100000795.1.9",
          "form": "모더나랑 화이자중에 뭘까",
          "original_form": "모더나랑 화이자중에 뭘까",
          "speaker_id": "1",
          "time": "20210819 19:31"
        },
        {
          "id": "MDRW2100000795.1.10",
          "form": "왠지 모더나일듯한",
          "original_form": "왠지 모더나일듯한",
          "speaker_id": "2",
          "time": "20210819 19:32"
        },
        {
          "id": "MDRW2100000795.1.11",
          "form": "난 화이자일것같긴한데",
          "original_form": "난 화이자일것같긴한데",
          "speaker_id": "1",
          "time": "20210819 19:32"
        },
        {
          "id": "MDRW2100000795.1.12",
          "form": "둘다 복불복이다",
          "original_form": "둘다 복불복이다",
          "speaker_id": "2",
          "time": "20210819 19:32"
        },
        {
          "id": "MDRW2100000795.1.13",
          "form": "ㅇㅇ 뭐가 더 좋은지 그런건 없으니까",
          "original_form": "ㅇㅇ 뭐가 더 좋은지 그런건 없으니까",
          "speaker_id": "1",
          "time": "20210819 19:32"
        },
        {
          "id": "MDRW2100000795.1.14",
