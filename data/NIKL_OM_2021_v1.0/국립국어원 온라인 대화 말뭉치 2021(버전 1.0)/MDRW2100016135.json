      "utterance": [
        {
          "id": "MDRW2100016135.1.1",
          "form": "저는 타이쿤게임 좋아했어요",
          "original_form": "저는 타이쿤게임 좋아했어요",
          "speaker_id": "2",
          "time": "20210826 10:09"
        },
        {
          "id": "MDRW2100016135.1.2",
          "form": "전 브롤해요ㅎ",
          "original_form": "전 브롤해요ㅎ",
          "speaker_id": "1",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100016135.1.3",
          "form": "저는 게임을 잘 하지는 못하는데 플스는 왜샀는지 ㅋㅋ",
          "original_form": "저는 게임을 잘 하지는 못하는데 플스는 왜샀는지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016135.1.4",
          "form": "플스어때요? 살까하는데ㅎ",
          "original_form": "플스어때요? 살까하는데ㅎ",
          "speaker_id": "1",
          "time": "20210826 11:49"
        },
        {
          "id": "MDRW2100016135.1.5",
          "form": "저는 워낙 겜을 잘 못해서 닌텐도 살걸 후회되어요",
          "original_form": "저는 워낙 겜을 잘 못해서 닌텐도 살걸 후회되어요",
          "speaker_id": "2",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100016135.1.6",
          "form": "아기자기한 겜은 닌텐도가많더라고요",
          "original_form": "아기자기한 겜은 닌텐도가많더라고요",
          "speaker_id": "2",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100016135.1.7",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016135.1.8",
          "form": "닌텐도가 더 좋은가바요",
          "original_form": "닌텐도가 더 좋은가바요",
          "speaker_id": "1",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016135.1.9",
          "form": "링피트 등 할게 더 많은 것 같아요",
          "original_form": "링피트 등 할게 더 많은 것 같아요",
          "speaker_id": "2",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100016135.1.10",
          "form": "아 그렇군요 한번 살펴볼께요ㅎ",
          "original_form": "아 그렇군요 한번 살펴볼께요ㅎ",
          "speaker_id": "1",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100016135.1.11",
          "form": "네 플스는 기술이 필요힌 게임이 많은거같아요",
          "original_form": "네 플스는 기술이 필요힌 게임이 많은거같아요",
          "speaker_id": "2",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100016135.1.12",
          "form": "아 그렇구나 몰랐어요",
          "original_form": "아 그렇구나 몰랐어요",
          "speaker_id": "1",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100016135.1.13",
          "form": "좀비게임은화면보심 서워서 ㅋㅋ인트로 보다가 포기했어요",
          "original_form": "좀비게임은화면보심 서워서 ㅋㅋ인트로 보다가 포기했어요",
          "speaker_id": "2",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100016135.1.14",
          "form": "좀비게임ㅠ",
          "original_form": "좀비게임ㅠ",
          "speaker_id": "1",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100016135.1.15",
          "form": "프로게이머 좋아하세요?",
          "original_form": "프로게이머 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100016135.1.16",
          "form": "아니요 잘 몰라요 ㅎㅎ",
          "original_form": "아니요 잘 몰라요 ㅎㅎ",
          "speaker_id": "2",
