      "utterance": [
        {
          "id": "MDRW2100042477.1.1",
          "form": "요새 보시고 있는 프로그램 있으세요???",
          "original_form": "요새 보시고 있는 프로그램 있으세요???",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042477.1.2",
          "form": "드라마 슬의 보고 있고",
          "original_form": "드라마 슬의 보고 있고",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042477.1.3",
          "form": "지금은 슈퍼밴드 보는 중이에요",
          "original_form": "지금은 슈퍼밴드 보는 중이에요",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042477.1.4",
          "form": "오 슬의 저번주 결방이라 슬프던데ㅠ",
          "original_form": "오 슬의 저번주 결방이라 슬프던데ㅠ",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042477.1.5",
          "form": "슈퍼밴드는 뭔가요???",
          "original_form": "슈퍼밴드는 뭔가요???",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042477.1.6",
          "form": "지금 jtbc에서 하는 밴드 경연 프로그램이에요",
          "original_form": "지금 jtbc에서 하는 밴드 경연 프로그램이에요",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042477.1.7",
          "form": "슬의를 보시는 군요!",
          "original_form": "슬의를 보시는 군요!",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042477.1.8",
          "form": "맞아요 너무 아쉬웠어요 ㅠ",
          "original_form": "맞아요 너무 아쉬웠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042477.1.9",
          "form": "넵 재밋더라구요 원래 시즌1도 안봤었는데 가족이 재방보는거 따라봤는데 재밋어서 시즌2는 챙겨보네요",
          "original_form": "넵 재밋더라구요 원래 시즌1도 안봤었는데 가족이 재방보는거 따라봤는데 재밋어서 시즌2는 챙겨보네요",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042477.1.10",
          "form": "그쵸 ㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042477.1.11",
          "form": "음악도 다 너무 좋은 것 같아요",
          "original_form": "음악도 다 너무 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042477.1.12",
          "form": "네 맞어요 추억의노래들ㅋㅋ 힐링되구요",
          "original_form": "네 맞어요 추억의노래들ㅋㅋ 힐링되구요",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042477.1.13",
          "form": "어떤 노래가 제일 기억에 남으시나요?",
          "original_form": "어떤 노래가 제일 기억에 남으시나요?",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042477.1.14",
          "form": "슈퍼스타나 너에게난 이요!",
          "original_form": "슈퍼스타나 너에게난 이요!",
          "speaker_id": "1",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042477.1.15",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042477.1.16",
          "form": "슈퍼스타 노래가 원래도 좋지만",
          "original_form": "슈퍼스타 노래가 원래도 좋지만",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042477.1.17",
          "form": "이번에 들으니 더 좋은 것 같아요!!",
          "original_form": "이번에 들으니 더 좋은 것 같아요!!",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042477.1.18",
          "form": "가사도좋은데 스토리도 같이있으니 더좋은거같아요!!!",
          "original_form": "가사도좋은데 스토리도 같이있으니 더좋은거같아요!!!",
          "speaker_id": "1",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042477.1.19",
          "form": "역시 뭘 좀 아시는 군요!",
          "original_form": "역시 뭘 좀 아시는 군요!",
          "speaker_id": "2",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042477.1.20",
          "form": "name1님께선 요새 챙겨보시는 프로그램이 있으신가요?",
