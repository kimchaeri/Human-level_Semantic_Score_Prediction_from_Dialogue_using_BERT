      "utterance": [
        {
          "id": "MDRW2100022506.1.1",
          "form": "이거 매년 어떻게 맞나요",
          "original_form": "이거 매년 어떻게 맞나요",
          "speaker_id": "2",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100022506.1.2",
          "form": "법으로 백신휴가 정해주던가",
          "original_form": "법으로 백신휴가 정해주던가",
          "speaker_id": "2",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100022506.1.3",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022506.1.4",
          "form": "건강하려고 맞다가",
          "original_form": "건강하려고 맞다가",
          "speaker_id": "1",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022506.1.5",
          "form": "백신맞고 일하고 건강 해칠듯",
          "original_form": "백신맞고 일하고 건강 해칠듯",
          "speaker_id": "1",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022506.1.6",
          "form": "2차는 맞고 무조건 하루이상 쉬어야한다던데",
          "original_form": "2차는 맞고 무조건 하루이상 쉬어야한다던데",
          "speaker_id": "2",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100022506.1.7",
          "form": "ㅠㅠ중간에 출근해야 하니.. 휴...",
          "original_form": "ㅠㅠ중간에 출근해야 하니.. 휴...",
          "speaker_id": "1",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100022506.1.8",
          "form": "출근자체가 싫네요 ㅋㅋㅋ",
          "original_form": "출근자체가 싫네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022506.1.9",
          "form": "네네ㅜㅜ 건강 해칠 것 같아요",
          "original_form": "네네ㅜㅜ 건강 해칠 것 같아요",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022506.1.10",
          "form": "코로나땜에 너무 힘들어진거 같아요",
          "original_form": "코로나땜에 너무 힘들어진거 같아요",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022506.1.11",
          "form": "네네 코로나 땜에 일상이 무너졌죠",
          "original_form": "네네 코로나 땜에 일상이 무너졌죠",
          "speaker_id": "1",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022506.1.12",
          "form": "진짜 너무 싫다 코로나",
          "original_form": "진짜 너무 싫다 코로나",
          "speaker_id": "2",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022506.1.13",
          "form": "네 코로나 정말 지긋지긋",
          "original_form": "네 코로나 정말 지긋지긋",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022506.1.14",
          "form": "지쳤어요 이제",
          "original_form": "지쳤어요 이제",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022506.1.15",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022506.1.16",
          "form": "코로나 지치네요 이제",
          "original_form": "코로나 지치네요 이제",
          "speaker_id": "1",
          "time": "20210829 19:53"
        },
        {
