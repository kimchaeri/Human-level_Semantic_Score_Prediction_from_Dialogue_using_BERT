      "utterance": [
        {
          "id": "MDRW2100034395.1.1",
          "form": "와 유용해요 완전",
          "original_form": "와 유용해요 완전",
          "speaker_id": "2",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100034395.1.2",
          "form": "뭐니뭐니해도 생필품이 중요하죠",
          "original_form": "뭐니뭐니해도 생필품이 중요하죠",
          "speaker_id": "2",
          "time": "20210828 23:04"
        },
        {
          "id": "MDRW2100034395.1.3",
          "form": "맞아용ㅎㅎ",
          "original_form": "맞아용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100034395.1.4",
          "form": "저는 쇼핑몰에서 그래뇰라사서 간편하게 먹으니까 넘좋았어요",
          "original_form": "저는 쇼핑몰에서 그래뇰라사서 간편하게 먹으니까 넘좋았어요",
          "speaker_id": "2",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100034395.1.5",
          "form": "그래놀라 어디꺼 드세요?",
          "original_form": "그래놀라 어디꺼 드세요?",
          "speaker_id": "1",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100034395.1.6",
          "form": "저 그래놀라 라즈베리인가 들어간거 되게 잘먹는데ㅋㅋㅋ",
          "original_form": "저 그래놀라 라즈베리인가 들어간거 되게 잘먹는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100034395.1.7",
          "form": "아 크랜베리!ㅋㅋㅋㅋㅋ",
          "original_form": "아 크랜베리!ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100034395.1.8",
          "form": "name1님은 이상형있으세요",
          "original_form": "&name1&님은 이상형있으세요",
          "speaker_id": "2",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100034395.1.9",
          "form": "저는 일단 잘생기고ㅋㅋㅋㅋ",
          "original_form": "저는 일단 잘생기고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100034395.1.10",
          "form": "약간 똑똑한 사람 좋아해요",
          "original_form": "약간 똑똑한 사람 좋아해요",
          "speaker_id": "1",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100034395.1.11",
          "form": "오우",
          "original_form": "오우",
          "speaker_id": "2",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100034395.1.12",
          "form": "잘생기고 스마트함 중요하죠",
          "original_form": "잘생기고 스마트함 중요하죠",
          "speaker_id": "2",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100034395.1.13",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100034395.1.14",
          "form": "그래서 약간 너드미라고 아세요?",
          "original_form": "그래서 약간 너드미라고 아세요?",
          "speaker_id": "1",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100034395.1.15",
          "form": "안경쓰고 좀 똑똑해보이는",
          "original_form": "안경쓰고 좀 똑똑해보이는",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100034395.1.16",
          "form": "연예인으로 치면 이장원님같은",
          "original_form": "연예인으로 치면 이장원님같은",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100034395.1.17",
          "form": "ㅋㅋ 오",
          "original_form": "ㅋㅋ 오",
          "speaker_id": "2",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100034395.1.18",
          "form": "너드미알죠 하지만 잘생겨야한다는 그 너드",
          "original_form": "너드미알죠 하지만 잘생겨야한다는 그 너드",
          "speaker_id": "2",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100034395.1.19",
          "form": "근데 사실 이상형 있어도",
          "original_form": "근데 사실 이상형 있어도",
          "speaker_id": "1",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100034395.1.20",
          "form": "이상형 만나도 연애만 하고싶지",
          "original_form": "이상형 만나도 연애만 하고싶지",
          "speaker_id": "1",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100034395.1.21",
          "form": "결혼은 안하고싶어요ㅠㅠ",
          "original_form": "결혼은 안하고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100034395.1.22",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100034395.1.23",
          "form": "결혼은 제약이 많죠ㅜ",
          "original_form": "결혼은 제약이 많죠ㅜ",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100034395.1.24",
          "form": "네네 이 사람 부모님을",
          "original_form": "네네 이 사람 부모님을",
          "speaker_id": "1",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100034395.1.25",
          "form": "감당할 자신이 없달까요....",
          "original_form": "감당할 자신이 없달까요....",
          "speaker_id": "1",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100034395.1.26",
          "form": "양가부모님 일정이 생겨요",
          "original_form": "양가부모님 일정이 생겨요",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100034395.1.27",
          "form": "가족행사가 늘어남",
          "original_form": "가족행사가 늘어남",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100034395.1.28",
          "form": "저는 제 부모님이랑도 투닥거리는데 남의 부모님은....ㅋㅋㅋㅋ",
          "original_form": "저는 제 부모님이랑도 투닥거리는데 남의 부모님은....ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100034395.1.29",
          "form": "맞아요ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100034395.1.30",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100034395.1.31",
