      "utterance": [
        {
          "id": "MDRW2100045464.1.1",
          "form": "가장 좋아하시는 음식 있으세요?",
          "original_form": "가장 좋아하시는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210908 02:43"
        },
        {
          "id": "MDRW2100045464.1.2",
          "form": "곱창 좋아합니당",
          "original_form": "곱창 좋아합니당",
          "speaker_id": "2",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045464.1.3",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045464.1.4",
          "form": "매운 곱창이요?",
          "original_form": "매운 곱창이요?",
          "speaker_id": "1",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045464.1.5",
          "form": "음 하얀거 선호하고 곱창전골도 좋아해요",
          "original_form": "음 하얀거 선호하고 곱창전골도 좋아해요",
          "speaker_id": "2",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045464.1.6",
          "form": "크아아 침 나와요",
          "original_form": "크아아 침 나와요",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045464.1.7",
          "form": "새벽에 배고파요ㅋㅋㅌ",
          "original_form": "새벽에 배고파요ㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045464.1.8",
          "form": "술안주 종류 음식을 좋아하시는 건가요?",
          "original_form": "술안주 종류 음식을 좋아하시는 건가요?",
          "speaker_id": "1",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045464.1.9",
          "form": "엇 ㅋㅋㅋ그냥 막창이런게 맛있더라구여",
          "original_form": "엇 ㅋㅋㅋ그냥 막창이런게 맛있더라구여",
          "speaker_id": "2",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045464.1.10",
          "form": "막 닭발 돼지껍데기도 좋아하시나요!",
          "original_form": "막 닭발 돼지껍데기도 좋아하시나요!",
          "speaker_id": "1",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045464.1.11",
          "form": "음 좋아하는편이긴 해요",
          "original_form": "음 좋아하는편이긴 해요",
          "speaker_id": "2",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045464.1.12",
          "form": "좋아하시나여?",
          "original_form": "좋아하시나여?",
          "speaker_id": "2",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045464.1.13",
          "form": "네! 곱창도 좋아하고 닭발 돼껍도 먹어요",
          "original_form": "네! 곱창도 좋아하고 닭발 돼껍도 먹어요",
          "speaker_id": "1",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045464.1.14",
          "form": "근데 구운 곱창은 아직 안 먹어봤어요 전골만 먹어봤어뇨",
          "original_form": "근데 구운 곱창은 아직 안 먹어봤어요 전골만 먹어봤어뇨",
          "speaker_id": "1",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045464.1.15",
          "form": "name2님은 구워서도 드셔보셨나요?",
          "original_form": "&name2&님은 구워서도 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045464.1.16",
          "form": "넵 당연하져",
          "original_form": "넵 당연하져",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045464.1.17",
          "form": "식당도 많은데 한번가보세여!",
          "original_form": "식당도 많은데 한번가보세여!",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045464.1.18",
          "form": "친구 꼬셔서 한번 가 봐야겠어요 ㅋㅋㅋㅋ",
          "original_form": "친구 꼬셔서 한번 가 봐야겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045464.1.19",
          "form": "저 막창 대창은 안 먹어봤어요",
          "original_form": "저 막창 대창은 안 먹어봤어요",
          "speaker_id": "1",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045464.1.20",
          "form": "막창이랑 대창 둘이 다른 거예요?",
          "original_form": "막창이랑 대창 둘이 다른 거예요?",
          "speaker_id": "1",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045464.1.21",
          "form": "넵 달라요ㅋㅋㅋ",
          "original_form": "넵 달라요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045464.1.22",
          "form": "근데 저 구분잘못해여ㅋㅋㅋ",
          "original_form": "근데 저 구분잘못해여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045464.1.23",
          "form": "ㅋㅋㅋㅋ저도 일단 굵은 부위인가보다 하고 구분 못해요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ저도 일단 굵은 부위인가보다 하고 구분 못해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045464.1.24",
          "form": "야채는 잘 드시나요",
          "original_form": "야채는 잘 드시나요",
          "speaker_id": "1",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100045464.1.25",
          "form": "넵 함께 먹으면 맛있쥬",
          "original_form": "넵 함께 먹으면 맛있쥬",
          "speaker_id": "2",
          "time": "20210908 02:57"
        }
      ]
