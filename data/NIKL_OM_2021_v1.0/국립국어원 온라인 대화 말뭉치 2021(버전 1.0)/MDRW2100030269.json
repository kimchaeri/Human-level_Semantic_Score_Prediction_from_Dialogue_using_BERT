      "utterance": [
        {
          "id": "MDRW2100030269.1.1",
          "form": "이삭토스트로 대신해서",
          "original_form": "이삭토스트로 대신해서",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030269.1.2",
          "form": "막는중이에요",
          "original_form": "막는중이에요",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030269.1.3",
          "form": "대체제가 있군요",
          "original_form": "대체제가 있군요",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030269.1.4",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030269.1.5",
          "form": "고모가 좋아하세요",
          "original_form": "고모가 좋아하세요",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030269.1.6",
          "form": "일주일에 한번은 먹어요",
          "original_form": "일주일에 한번은 먹어요",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030269.1.7",
          "form": "(",
          "original_form": "(",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030269.1.8",
          "form": "맛밌머요",
          "original_form": "맛밌머요",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030269.1.9",
          "form": "치즈들어간거좋아합니다",
          "original_form": "치즈들어간거좋아합니다",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030269.1.10",
          "form": "어떤맛좋아하세요?",
          "original_form": "어떤맛좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030269.1.11",
          "form": "치즈는 어디든 어울려요",
          "original_form": "치즈는 어디든 어울려요",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030269.1.12",
          "form": "존맛이죠",
          "original_form": "존맛이죠",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030269.1.13",
          "form": "토스트에 베이컨이나  계란후라이요",
          "original_form": "토스트에 베이컨이나  계란후라이요",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030269.1.14",
          "form": "저두좋아요",
          "original_form": "저두좋아요",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030269.1.15",
          "form": "돈카스 들어가도 좋아요",
          "original_form": "돈카스 들어가도 좋아요",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030269.1.16",
          "form": "빵이 베이글인것도맛나요",
          "original_form": "빵이 베이글인것도맛나요",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030269.1.17",
          "form": "토마토나 양배추요",
          "original_form": "토마토나 양배추요",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030269.1.18",
          "form": "피자맛도 좋아요",
          "original_form": "피자맛도 좋아요",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030269.1.19",
          "form": "베이글이 더 낫네요",
          "original_form": "베이글이 더 낫네요",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030269.1.20",
          "form": "최애는더블포테이토치즈",
          "original_form": "최애는더블포테이토치즈",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030269.1.21",
          "form": "피자맛 궁금해요",
          "original_form": "피자맛 궁금해요",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030269.1.22",
          "form": "감자는진리요",
          "original_form": "감자는진리요",
          "speaker_id": "2",
          "time": "20210903 23:36"
        }
      ]
