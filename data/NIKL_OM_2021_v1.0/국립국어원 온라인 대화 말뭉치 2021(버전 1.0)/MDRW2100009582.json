      "utterance": [
        {
          "id": "MDRW2100009582.1.1",
          "form": "최근에 마우스 샀어",
          "original_form": "최근에 마우스 샀어",
          "speaker_id": "2",
          "time": "20210823 01:03"
        },
        {
          "id": "MDRW2100009582.1.2",
          "form": "어떤 마우스?",
          "original_form": "어떤 마우스?",
          "speaker_id": "1",
          "time": "20210823 01:04"
        },
        {
          "id": "MDRW2100009582.1.3",
          "form": "마우스를 샀구나",
          "original_form": "마우스를 샀구나",
          "speaker_id": "1",
          "time": "20210823 01:04"
        },
        {
          "id": "MDRW2100009582.1.4",
          "form": "로지텍 마우스 샀어",
          "original_form": "로지텍 마우스 샀어",
          "speaker_id": "2",
          "time": "20210823 01:05"
        },
        {
          "id": "MDRW2100009582.1.5",
          "form": "한번에 두 기기를 사용할 수 있어",
          "original_form": "한번에 두 기기를 사용할 수 있어",
          "speaker_id": "2",
          "time": "20210823 01:05"
        },
        {
          "id": "MDRW2100009582.1.6",
          "form": "오 나도 그거 아는데 그 기능 좋지?",
          "original_form": "오 나도 그거 아는데 그 기능 좋지?",
          "speaker_id": "1",
          "time": "20210823 01:13"
        },
        {
          "id": "MDRW2100009582.1.7",
          "form": "응 정말 편해",
          "original_form": "응 정말 편해",
          "speaker_id": "2",
          "time": "20210823 01:16"
        },
        {
          "id": "MDRW2100009582.1.8",
          "form": "너도 로지텍 써?",
          "original_form": "너도 로지텍 써?",
          "speaker_id": "2",
          "time": "20210823 01:16"
        },
        {
          "id": "MDRW2100009582.1.9",
          "form": "응응 나도 친구가 추천해줘서 써",
          "original_form": "응응 나도 친구가 추천해줘서 써",
          "speaker_id": "1",
          "time": "20210823 01:16"
        },
        {
          "id": "MDRW2100009582.1.10",
          "form": "로지텍 유명한 이유가 있더라",
          "original_form": "로지텍 유명한 이유가 있더라",
          "speaker_id": "1",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009582.1.11",
          "form": "응 로지텍 진짜 마우스 잘 만들어",
          "original_form": "응 로지텍 진짜 마우스 잘 만들어",
          "speaker_id": "2",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009582.1.12",
          "form": "키보드도 유명하지 않아?",
          "original_form": "키보드도 유명하지 않아?",
          "speaker_id": "1",
          "time": "20210823 01:18"
        },
        {
          "id": "MDRW2100009582.1.13",
          "form": "키보드로 처음 알았어 나는",
          "original_form": "키보드로 처음 알았어 나는",
          "speaker_id": "1",
          "time": "20210823 01:18"
        },
        {
          "id": "MDRW2100009582.1.14",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "2",
          "time": "20210823 01:18"
        },
        {
          "id": "MDRW2100009582.1.15",
          "form": "마우스도 엄청 좋아",
          "original_form": "마우스도 엄청 좋아",
          "speaker_id": "2",
          "time": "20210823 01:31"
        },
        {
          "id": "MDRW2100009582.1.16",
          "form": "구매 추천할게",
          "original_form": "구매 추천할게",
          "speaker_id": "2",
          "time": "20210823 01:31"
        },
        {
          "id": "MDRW2100009582.1.17",
          "form": "이 참에 맞춰야겠당 고마워",
          "original_form": "이 참에 맞춰야겠당 고마워",
          "speaker_id": "1",
          "time": "20210823 01:34"
        },
        {
          "id": "MDRW2100009582.1.18",
          "form": "마우스도 있다니 신기한걸",
          "original_form": "마우스도 있다니 신기한걸",
          "speaker_id": "1",
          "time": "20210823 01:34"
        },
        {
          "id": "MDRW2100009582.1.19",
          "form": "마우스 기능이 다양해",
          "original_form": "마우스 기능이 다양해",
          "speaker_id": "2",
          "time": "20210823 01:36"
        },
        {
          "id": "MDRW2100009582.1.20",
          "form": "어떤 기능이 있어?",
          "original_form": "어떤 기능이 있어?",
          "speaker_id": "1",
          "time": "20210823 01:36"
        },
        {
          "id": "MDRW2100009582.1.21",
          "form": "다양하구나",
          "original_form": "다양하구나",
          "speaker_id": "1",
          "time": "20210823 01:36"
        },
        {
          "id": "MDRW2100009582.1.22",
          "form": "특정한 키를 저장할 수 있어",
          "original_form": "특정한 키를 저장할 수 있어",
          "speaker_id": "2",
          "time": "20210823 01:37"
        }
      ]
