      "utterance": [
        {
          "id": "MDRW2100028677.1.1",
          "form": "이제 바꾸셔요",
          "original_form": "이제 바꾸셔요",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028677.1.2",
          "form": "새폰으로",
          "original_form": "새폰으로",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028677.1.3",
          "form": "그럼 님은 최근에 사신게 뭐에요?",
          "original_form": "그럼 님은 최근에 사신게 뭐에요?",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028677.1.4",
          "form": "음... 최근에 산거 결혼식때\n입고갈 스커트?",
          "original_form": "음... 최근에 산거 결혼식때\n입고갈 스커트?",
          "speaker_id": "1",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028677.1.5",
          "form": "다녀오셨어요?",
          "original_form": "다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028677.1.6",
          "form": "지난 주말에요 ㅋㅋ",
          "original_form": "지난 주말에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028677.1.7",
          "form": "그럼 그 뒤로 산게 없나요?",
          "original_form": "그럼 그 뒤로 산게 없나요?",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028677.1.8",
          "form": "장본거 말고는 없는데영 ㅋㅋ",
          "original_form": "장본거 말고는 없는데영 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100028677.1.9",
          "form": "그건 쇼핑이라고 하기엔...ㅋㅋ",
          "original_form": "그건 쇼핑이라고 하기엔...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100028677.1.10",
          "form": "오로지 본인을 위한거",
          "original_form": "오로지 본인을 위한거",
          "speaker_id": "2",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100028677.1.11",
          "form": "아ㅋㅋ 자동차 키링",
          "original_form": "아ㅋㅋ 자동차 키링",
          "speaker_id": "1",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100028677.1.12",
          "form": "어제 키링 주문했어요",
          "original_form": "어제 키링 주문했어요",
          "speaker_id": "1",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100028677.1.13",
          "form": "드라이버를 위한 준비?",
          "original_form": "드라이버를 위한 준비?",
          "speaker_id": "2",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028677.1.14",
          "form": "ㅋㅋ 휠도 바꾸고 싶네요",
          "original_form": "ㅋㅋ 휠도 바꾸고 싶네요",
          "speaker_id": "1",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028677.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100028677.1.16",
          "form": "이렇게 거덜내는건가요?",
          "original_form": "이렇게 거덜내는건가요?",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100028677.1.17",
          "form": "남편이 눈치껏 알아서\n바꿔줌 좋겠네염ㅋㅋㅋㅋㅋ",
          "original_form": "남편이 눈치껏 알아서\n바꿔줌 좋겠네염ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028677.1.18",
          "form": "그럼 눈치좀 주세요",
          "original_form": "그럼 눈치좀 주세요",
          "speaker_id": "2",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028677.1.19",
          "form": "어떻게 시그널을 보내야쥬?ㅋㅋ",
          "original_form": "어떻게 시그널을 보내야쥬?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028677.1.20",
          "form": "타이어 휠 사진을 카톡프사로 해두세요",
          "original_form": "타이어 휠 사진을 카톡프사로 해두세요",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028677.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028677.1.22",
          "form": "오? ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오? ㅋㅋㅋㅋㅋㅋㅋㅋ",
