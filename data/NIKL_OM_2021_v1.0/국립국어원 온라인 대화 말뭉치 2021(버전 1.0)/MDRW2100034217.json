      "utterance": [
        {
          "id": "MDRW2100034217.1.1",
          "form": "나의 빵이 잘 구워지고 있다",
          "original_form": "나의 빵이 잘 구워지고 있다",
          "speaker_id": "2",
          "time": "20210828 12:11"
        },
        {
          "id": "MDRW2100034217.1.2",
          "form": "오 거의 끝나가? 맛있겠다",
          "original_form": "오 거의 끝나가? 맛있겠다",
          "speaker_id": "1",
          "time": "20210828 12:11"
        },
        {
          "id": "MDRW2100034217.1.3",
          "form": "레몬파운드 4개 초코파운드 4개",
          "original_form": "레몬파운드 4개 초코파운드 4개",
          "speaker_id": "2",
          "time": "20210828 12:11"
        },
        {
          "id": "MDRW2100034217.1.4",
          "form": "큐브파운드",
          "original_form": "큐브파운드",
          "speaker_id": "1",
          "time": "20210828 12:11"
        },
        {
          "id": "MDRW2100034217.1.5",
          "form": "오 양이 꽤 되네",
          "original_form": "오 양이 꽤 되네",
          "speaker_id": "1",
          "time": "20210828 12:11"
        },
        {
          "id": "MDRW2100034217.1.6",
          "form": "근데 식히고 아이싱도 해야해",
          "original_form": "근데 식히고 아이싱도 해야해",
          "speaker_id": "2",
          "time": "20210828 12:11"
        },
        {
          "id": "MDRW2100034217.1.7",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:11"
        },
        {
          "id": "MDRW2100034217.1.8",
          "form": "아직 제조과정이 좀 남았구나",
          "original_form": "아직 제조과정이 좀 남았구나",
          "speaker_id": "1",
          "time": "20210828 12:12"
        },
        {
          "id": "MDRW2100034217.1.9",
          "form": "ㅌㅋ",
          "original_form": "ㅌㅋ",
          "speaker_id": "1",
          "time": "20210828 12:12"
        },
        {
          "id": "MDRW2100034217.1.10",
          "form": "아직 파운드 굽는 시간도 5분 남았고",
          "original_form": "아직 파운드 굽는 시간도 5분 남았고",
          "speaker_id": "2",
          "time": "20210828 12:12"
        },
        {
          "id": "MDRW2100034217.1.11",
          "form": "식힌 다음에 아이싱도 하고 그러면 한 시 넘겠구나",
          "original_form": "식힌 다음에 아이싱도 하고 그러면 한 시 넘겠구나",
          "speaker_id": "1",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100034217.1.12",
          "form": "응 조리시간이 원래 1시까지였어",
          "original_form": "응 조리시간이 원래 1시까지였어",
          "speaker_id": "2",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100034217.1.13",
          "form": "점심식사 삼치 먼저 먹어도됨",
          "original_form": "점심식사 삼치 먼저 먹어도됨",
          "speaker_id": "2",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100034217.1.14",
          "form": "엄마아빠 주무셔",
          "original_form": "엄마아빠 주무셔",
          "speaker_id": "1",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100034217.1.15",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100034217.1.16",
          "form": "아 주무시면 식사 천천히 하겠네",
          "original_form": "아 주무시면 식사 천천히 하겠네",
          "speaker_id": "2",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100034217.1.17",
          "form": "아직 배도 안고프고.. 김치찌개 많이 먹어서!",
          "original_form": "아직 배도 안고프고.. 김치찌개 많이 먹어서!",
          "speaker_id": "1",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100034217.1.18",
          "form": "응 너 온담에 파운드케이크 먹고 저녁에 삼치먹을듯..?",
          "original_form": "응 너 온담에 파운드케이크 먹고 저녁에 삼치먹을듯..?",
          "speaker_id": "1",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100034217.1.19",
          "form": "점저로 삼치와 김치찌개",
          "original_form": "점저로 삼치와 김치찌개",
          "speaker_id": "2",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100034217.1.20",
          "form": "엉ㅋㅋ",
          "original_form": "엉ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100034217.1.21",
          "form": "삼치삼치~",
          "original_form": "삼치삼치~",
          "speaker_id": "1",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100034217.1.22",
          "form": "김치찌개쓰~",
          "original_form": "김치찌개쓰~",
          "speaker_id": "2",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100034217.1.23",
          "form": "기다리는 동안 심심한가보구만",
          "original_form": "기다리는 동안 심심한가보구만",
          "speaker_id": "1",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100034217.1.24",
          "form": "아이싱도 다 만들어져있어?",
          "original_form": "아이싱도 다 만들어져있어?",
          "speaker_id": "1",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100034217.1.25",
          "form": "아이싱 만들어뒀어",
          "original_form": "아이싱 만들어뒀어",
          "speaker_id": "2",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100034217.1.26",
          "form": "오 좋네 너무 많이 바르진 말아주십쇼 ㅋㅋ",
          "original_form": "오 좋네 너무 많이 바르진 말아주십쇼 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100034217.1.27",
          "form": "아이싱 양 적당하죠?",
          "original_form": "아이싱 양 적당하죠?",
          "speaker_id": "2",
          "time": "20210828 13:35"
        },
        {
          "id": "MDRW2100034217.1.28",
          "form": "아빠랑 치킨도 사고 간김에 던킨도넛두 샀어",
          "original_form": "아빠랑 치킨도 사고 간김에 던킨도넛두 샀어",
          "speaker_id": "1",
