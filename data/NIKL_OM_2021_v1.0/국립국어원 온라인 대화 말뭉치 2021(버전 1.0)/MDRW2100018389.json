      "utterance": [
        {
          "id": "MDRW2100018389.1.1",
          "form": "넷플릭스 보시나요?",
          "original_form": "넷플릭스 보시나요?",
          "speaker_id": "1",
          "time": "20210827 00:57"
        },
        {
          "id": "MDRW2100018389.1.2",
          "form": "아뇨 저는 유튜브 자주봐요",
          "original_form": "아뇨 저는 유튜브 자주봐요",
          "speaker_id": "2",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100018389.1.3",
          "form": "유튜브 어떤거 주로 보세요?",
          "original_form": "유튜브 어떤거 주로 보세요?",
          "speaker_id": "1",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100018389.1.4",
          "form": "최근에 피식대학 재밌게보고있어요",
          "original_form": "최근에 피식대학 재밌게보고있어요",
          "speaker_id": "2",
          "time": "20210827 14:23"
        },
        {
          "id": "MDRW2100018389.1.5",
          "form": "아 그거 피치스 재밌게 봤어요 ㅋㅋ",
          "original_form": "아 그거 피치스 재밌게 봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100018389.1.6",
          "form": "ㅋㅋㅋ 저는 피치스 전에 입문했어서",
          "original_form": "ㅋㅋㅋ 저는 피치스 전에 입문했어서",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100018389.1.7",
          "form": "한사랑산악회 재밌게 보고있어요",
          "original_form": "한사랑산악회 재밌게 보고있어요",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100018389.1.8",
          "form": "재밌는 것 같아요ㅋㅋ 먹방은 안보시나요?",
          "original_form": "재밌는 것 같아요ㅋㅋ 먹방은 안보시나요?",
          "speaker_id": "1",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100018389.1.9",
          "form": "저는 먹방은 잘안보는데",
          "original_form": "저는 먹방은 잘안보는데",
          "speaker_id": "2",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100018389.1.10",
          "form": "추천해주실만한 유튜버 있나요",
          "original_form": "추천해주실만한 유튜버 있나요",
          "speaker_id": "2",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100018389.1.11",
          "form": "저는 입짧은햇님님이랑 나도님꺼 자주 봐요!",
          "original_form": "저는 입짧은햇님님이랑 나도님꺼 자주 봐요!",
          "speaker_id": "1",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100018389.1.12",
          "form": "보신적 있으신가요?",
          "original_form": "보신적 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100018389.1.13",
          "form": "입짧은햇님 그분은 예능에서 봤어요",
          "original_form": "입짧은햇님 그분은 예능에서 봤어요",
          "speaker_id": "2",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100018389.1.14",
          "form": "놀라운토요일!",
          "original_form": "놀라운토요일!",
          "speaker_id": "2",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100018389.1.15",
          "form": "오 맞아요 그분ㅋㅋ 맛있게 드셔서 좋아해요!",
          "original_form": "오 맞아요 그분ㅋㅋ 맛있게 드셔서 좋아해요!",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100018389.1.16",
          "form": "나도님은 처음 들어보는데",
          "original_form": "나도님은 처음 들어보는데",
          "speaker_id": "2",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100018389.1.17",
          "form": "한번 찾어봐야겠어요",
          "original_form": "한번 찾어봐야겠어요",
          "speaker_id": "2",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100018389.1.18",
          "form": "햇님님이랑 친하셔서 그런가 알고리즘에 올라와서 봤는데 맛있게 드시더라구요 ㅋㅋ",
          "original_form": "햇님님이랑 친하셔서 그런가 알고리즘에 올라와서 봤는데 맛있게 드시더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100018389.1.19",
          "form": "나중에 한번 봐보셔요!",
          "original_form": "나중에 한번 봐보셔요!",
          "speaker_id": "1",
          "time": "20210827 15:16"
        }
      ]
