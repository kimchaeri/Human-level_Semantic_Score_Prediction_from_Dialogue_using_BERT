      "utterance": [
        {
          "id": "MDRW2100038761.1.1",
          "form": "저녁드셖나요",
          "original_form": "저녁드셖나요",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100038761.1.2",
          "form": "넵 먹었습니다 ㅎㅎ",
          "original_form": "넵 먹었습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100038761.1.3",
          "form": "드셨나요",
          "original_form": "드셨나요",
          "speaker_id": "2",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100038761.1.4",
          "form": "네네 신라면볶음면이요",
          "original_form": "네네 신라면볶음면이요",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100038761.1.5",
          "form": "오 맛있나요",
          "original_form": "오 맛있나요",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100038761.1.6",
          "form": "아 저는 좀 비추에여",
          "original_form": "아 저는 좀 비추에여",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100038761.1.7",
          "form": "불닭이더맛나요",
          "original_form": "불닭이더맛나요",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100038761.1.8",
          "form": "앗 그렇군여 좀 짜다하더라구여 ㅋㅋㅋ",
          "original_form": "앗 그렇군여 좀 짜다하더라구여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100038761.1.9",
          "form": "저는 닭발먹었습니다 ㅎㅎ",
          "original_form": "저는 닭발먹었습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100038761.1.10",
          "form": "와 닭발",
          "original_form": "와 닭발",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100038761.1.11",
          "form": "저는 닭발 못먹어요ㅠㅠ",
          "original_form": "저는 닭발 못먹어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100038761.1.12",
          "form": "헐 왜요?매워서?아니면 흉측하게생겨서인가여..ㅠㅠㅋ",
          "original_form": "헐 왜요?매워서?아니면 흉측하게생겨서인가여..ㅠㅠㅋ",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100038761.1.13",
          "form": "아 닭발이 무서워요 ㅋㅋㅋ",
          "original_form": "아 닭발이 무서워요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:45"
        },
        {
          "id": "MDRW2100038761.1.14",
          "form": "양념은 맛있을것같아요",
          "original_form": "양념은 맛있을것같아요",
          "speaker_id": "1",
          "time": "20210903 08:45"
        },
        {
          "id": "MDRW2100038761.1.15",
          "form": "ㅋㅋㅋ아아 그럴수있죠 다른 맛있는것들도 많으니 괜찮아여 ㅋㅋ",
          "original_form": "ㅋㅋㅋ아아 그럴수있죠 다른 맛있는것들도 많으니 괜찮아여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:47"
        },
        {
          "id": "MDRW2100038761.1.16",
          "form": "맞아여 ㅋㅋㅋ",
          "original_form": "맞아여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:48"
        },
        {
          "id": "MDRW2100038761.1.17",
          "form": "닭발 궁금해요",
          "original_form": "닭발 궁금해요",
          "speaker_id": "1",
          "time": "20210903 08:48"
        },
        {
          "id": "MDRW2100038761.1.18",
          "form": "잘라진건 먹울수있어요",
          "original_form": "잘라진건 먹울수있어요",
          "speaker_id": "1",
          "time": "20210903 08:48"
        },
        {
          "id": "MDRW2100038761.1.19",
          "form": "그러면 무뼈에 도전해보세요 ㅎㅎ 쌈에 싸먹으면 맛있답니다ㅋㅋㅋ",
          "original_form": "그러면 무뼈에 도전해보세요 ㅎㅎ 쌈에 싸먹으면 맛있답니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:09"
        },
        {
          "id": "MDRW2100038761.1.20",
          "form": "와 무뼈도 있군요",
          "original_form": "와 무뼈도 있군요",
          "speaker_id": "1",
          "time": "20210903 09:11"
        },
        {
          "id": "MDRW2100038761.1.21",
          "form": "주먹밥이 맛나보이던데",
          "original_form": "주먹밥이 맛나보이던데",
          "speaker_id": "1",
