      "utterance": [
        {
          "id": "MDRW2100028731.1.1",
          "form": "동물키워보신 적 있나요?",
          "original_form": "동물키워보신 적 있나요?",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028731.1.2",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028731.1.3",
          "form": "저는 어렸을때 믹스견을 키웠었어요!",
          "original_form": "저는 어렸을때 믹스견을 키웠었어요!",
          "speaker_id": "1",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028731.1.4",
          "form": "이름이 뭔가요?",
          "original_form": "이름이 뭔가요?",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028731.1.5",
          "form": "name3였어요!",
          "original_form": "&name3&였어요!",
          "speaker_id": "1",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028731.1.6",
          "form": "ㅋㅋㅋ귀엽네요",
          "original_form": "ㅋㅋㅋ귀엽네요",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028731.1.7",
          "form": "그쵸?",
          "original_form": "그쵸?",
          "speaker_id": "1",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028731.1.8",
          "form": "name3가 이름인 이유가 있나요?",
          "original_form": "&name3&가 이름인 이유가 있나요?",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028731.1.9",
          "form": "원래 이름이 다른거였는데",
          "original_form": "원래 이름이 다른거였는데",
          "speaker_id": "1",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100028731.1.10",
          "form": "방울달린 목줄을 사왔는데",
          "original_form": "방울달린 목줄을 사왔는데",
          "speaker_id": "1",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100028731.1.11",
          "form": "그 방울소리를 되게 좋아하더라구요ㅋㅋㅋㅋ",
          "original_form": "그 방울소리를 되게 좋아하더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100028731.1.12",
          "form": "그 이후로 name3가 되었어요",
          "original_form": "그 이후로 &name3&가 되었어요",
          "speaker_id": "1",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100028731.1.13",
          "form": "ㅋㅋㅋㅋ귀엽네요",
          "original_form": "ㅋㅋㅋㅋ귀엽네요",
          "speaker_id": "2",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100028731.1.14",
          "form": "저는 키워보고는 싶은데",
          "original_form": "저는 키워보고는 싶은데",
          "speaker_id": "2",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100028731.1.15",
          "form": "무섭더라구요",
          "original_form": "무섭더라구요",
          "speaker_id": "2",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100028731.1.16",
          "form": "저도 name3가 무지개다리 건너고부터는",
          "original_form": "저도 &name3&가 무지개다리 건너고부터는",
          "speaker_id": "1",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028731.1.17",
          "form": "그냥 눈으로만 키우고 있어요",
          "original_form": "그냥 눈으로만 키우고 있어요",
          "speaker_id": "1",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028731.1.18",
          "form": "ㅋㅋㅋㅋㅋ저도 유튜브나 인스타에서 보고 만족해요",
          "original_form": "ㅋㅋㅋㅋㅋ저도 유튜브나 인스타에서 보고 만족해요",
          "speaker_id": "2",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028731.1.19",
          "form": "반려동물 키우는 게 정말 힘든 일이라서",
          "original_form": "반려동물 키우는 게 정말 힘든 일이라서",
