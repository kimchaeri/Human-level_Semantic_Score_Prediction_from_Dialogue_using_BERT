      "utterance": [
        {
          "id": "MDRW2100039766.1.1",
          "form": "언니 영양제 드시는거 있으세요?",
          "original_form": "언니 영양제 드시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100039766.1.2",
          "form": "종합비타민이랑 프로폴리스요",
          "original_form": "종합비타민이랑 프로폴리스요",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100039766.1.3",
          "form": "유산균도 먹을 생각이에요. 지금 배송중 ㅋㅋ",
          "original_form": "유산균도 먹을 생각이에요. 지금 배송중 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100039766.1.4",
          "form": "저도 유산균 가져왔는데",
          "original_form": "저도 유산균 가져왔는데",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100039766.1.5",
          "form": "매일 까먹고 안먹어요",
          "original_form": "매일 까먹고 안먹어요",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100039766.1.6",
          "form": "그러다 홍삼도 유통기한이 지났어요ㅋㅋㅋ",
          "original_form": "그러다 홍삼도 유통기한이 지났어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100039766.1.7",
          "form": "아이고... 그런 경우가 많죠",
          "original_form": "아이고... 그런 경우가 많죠",
          "speaker_id": "1",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100039766.1.8",
          "form": "저도 좀 그런편이라 뭐 생유산균 그런건 안사요",
          "original_form": "저도 좀 그런편이라 뭐 생유산균 그런건 안사요",
          "speaker_id": "1",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100039766.1.9",
          "form": "유통기한 긴거 사지",
          "original_form": "유통기한 긴거 사지",
          "speaker_id": "1",
          "time": "20210904 15:29"
        },
        {
          "id": "MDRW2100039766.1.10",
          "form": "ㅋㅋㅋ유통기한 한달남아 싸게 파는 데일리 홍삼스틱을 사주셨는데",
          "original_form": "ㅋㅋㅋ유통기한 한달남아 싸게 파는 데일리 홍삼스틱을 사주셨는데",
          "speaker_id": "2",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100039766.1.11",
          "form": "잘까먹는 제가 방치해뒀답니닼ㅋㅋㅋㅋㅋ",
          "original_form": "잘까먹는 제가 방치해뒀답니닼ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100039766.1.12",
          "form": "저런... ㅋㅋㅋ 맛이 단거면 뜨거운물에 타먹으면 그럭저럭 괜찮지 않을까요?",
          "original_form": "저런... ㅋㅋㅋ 맛이 단거면 뜨거운물에 타먹으면 그럭저럭 괜찮지 않을까요?",
          "speaker_id": "1",
          "time": "20210904 18:21"
        },
        {
          "id": "MDRW2100039766.1.13",
          "form": "상하지만않았다면 괜찮아요ㅋㅋ",
          "original_form": "상하지만않았다면 괜찮아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100039766.1.14",
          "form": "거의 일년이 지났을거에요(?)",
          "original_form": "거의 일년이 지났을거에요(?)",
          "speaker_id": "2",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100039766.1.15",
          "form": "그래서 지금 드시고 계세요?",
          "original_form": "그래서 지금 드시고 계세요?",
          "speaker_id": "1",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100039766.1.16",
          "form": "아니요ㅋㅋ",
          "original_form": "아니요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:20"
        },
        {
          "id": "MDRW2100039766.1.17",
          "form": "저 좀 먹으라고 혼내주세요(?)",
          "original_form": "저 좀 먹으라고 혼내주세요(?)",
          "speaker_id": "2",
          "time": "20210906 11:23"
        },
        {
          "id": "MDRW2100039766.1.18",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 11:24"
        },
        {
          "id": "MDRW2100039766.1.19",
          "form": "얼른 먹어요!!",
          "original_form": "얼른 먹어요!!",
          "speaker_id": "1",
          "time": "20210906 11:24"
        },
        {
          "id": "MDRW2100039766.1.20",
          "form": "언니 저 지금 백신맞았어오ㆍ",
          "original_form": "언니 저 지금 백신맞았어오ㆍ",
          "speaker_id": "2",
          "time": "20210906 11:25"
        },
        {
          "id": "MDRW2100039766.1.21",
          "form": "15분 대기중이에요!!",
          "original_form": "15분 대기중이에요!!",
          "speaker_id": "2",
          "time": "20210906 11:25"
        },
        {
          "id": "MDRW2100039766.1.22",
          "form": "앗! 화이자에요?",
          "original_form": "앗! 화이자에요?",
          "speaker_id": "1",
          "time": "20210906 11:25"
        },
        {
          "id": "MDRW2100039766.1.23",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210906 11:26"
        },
        {
          "id": "MDRW2100039766.1.24",
          "form": "근데 주사부위만 뻐근하고 별 이상 없어요ㅋㅋ",
          "original_form": "근데 주사부위만 뻐근하고 별 이상 없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:26"
        },
        {
          "id": "MDRW2100039766.1.25",
          "form": "1차는 보통 괜찮다더라구요",
          "original_form": "1차는 보통 괜찮다더라구요",
          "speaker_id": "1",
          "time": "20210906 11:27"
        },
        {
          "id": "MDRW2100039766.1.26",
          "form": "냉찜질만 좀 잘해주세요",
          "original_form": "냉찜질만 좀 잘해주세요",
          "speaker_id": "1",
