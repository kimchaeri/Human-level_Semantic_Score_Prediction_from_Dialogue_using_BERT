      "utterance": [
        {
          "id": "MDRW2100028830.1.1",
          "form": "힝...",
          "original_form": "힝...",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028830.1.2",
          "form": "전 무교예요...",
          "original_form": "전 무교예요...",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028830.1.3",
          "form": "사실 세례를 받았지만 냉담자라 무교나 마찬가지죠",
          "original_form": "사실 세례를 받았지만 냉담자라 무교나 마찬가지죠",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028830.1.4",
          "form": "저도 무교에얍",
          "original_form": "저도 무교에얍",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028830.1.5",
          "form": "사이비한테 잡혀서",
          "original_form": "사이비한테 잡혀서",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028830.1.6",
          "form": "2시간 동안 얘기한 적 있어요",
          "original_form": "2시간 동안 얘기한 적 있어요",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028830.1.7",
          "form": "그 이야기 들었어요 불쌍한 사이비",
          "original_form": "그 이야기 들었어요 불쌍한 사이비",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028830.1.8",
          "form": "그들은 잘못된 타겟을 골랐어요",
          "original_form": "그들은 잘못된 타겟을 골랐어요",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028830.1.9",
          "form": "내가 불쌍해야죠",
          "original_form": "내가 불쌍해야죠",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028830.1.10",
          "form": "에? 전혀 아니던데???",
          "original_form": "에? 전혀 아니던데???",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028830.1.11",
          "form": "에? 내 시간은?",
          "original_form": "에? 내 시간은?",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028830.1.12",
          "form": "나 그때 마감중이었어",
          "original_form": "나 그때 마감중이었어",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028830.1.13",
          "form": "그들은 처음부터 끝까지 당신의 흥미에 희생당한 거예요",
          "original_form": "그들은 처음부터 끝까지 당신의 흥미에 희생당한 거예요",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028830.1.14",
          "form": "3주동안 120p를 쳐야하는 상황이었지",
          "original_form": "3주동안 120p를 쳐야하는 상황이었지",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028830.1.15",
          "form": "그들은 진심이었지만 당신은 진심이 아니었어요",
          "original_form": "그들은 진심이었지만 당신은 진심이 아니었어요",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028830.1.16",
          "form": "이럴 땐 진심인 사람이 지는 거예요",
          "original_form": "이럴 땐 진심인 사람이 지는 거예요",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028830.1.17",
          "form": "그들은 나의 마감시간을 방해한 몹쓸 종교였어",
          "original_form": "그들은 나의 마감시간을 방해한 몹쓸 종교였어",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028830.1.18",
          "form": "그건 인정해",
          "original_form": "그건 인정해",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028830.1.19",
          "form": "근데 참 저런 거에 넘어가나? 싶긴했음",
          "original_form": "근데 참 저런 거에 넘어가나? 싶긴했음",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028830.1.20",
          "form": "넘어가긴 하겠지...",
          "original_form": "넘어가긴 하겠지...",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028830.1.21",
          "form": "돈 주면 넘어갈 자신 있는데",
          "original_form": "돈 주면 넘어갈 자신 있는데",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028830.1.22",
          "form": "다들 불교로 정신수양이나 하자",
          "original_form": "다들 불교로 정신수양이나 하자",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028830.1.23",
          "form": "돈 안 줘서 안 넘어갈래",
          "original_form": "돈 안 줘서 안 넘어갈래",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028830.1.24",
          "form": "아니 좀 듣다보면 좀 돈줘서ㅜ넘어가기에도",
          "original_form": "아니 좀 듣다보면 좀 돈줘서ㅜ넘어가기에도",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028830.1.25",
          "form": "쫀심 상해요",
          "original_form": "쫀심 상해요",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028830.1.26",
          "form": "백억 주고 외부은하의 아우터 갓을 믿으면서 소환진 그리라고 하면 그리지",
          "original_form": "백억 주고 외부은하의 아우터 갓을 믿으면서 소환진 그리라고 하면 그리지",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028830.1.27",
          "form": "금액에 따라 달라요",
          "original_form": "금액에 따라 달라요",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028830.1.28",
          "form": "일당 10만원이면 난 종교집회까진 참여 가능해",
          "original_form": "일당 10만원이면 난 종교집회까진 참여 가능해",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028830.1.29",
          "form": "대신 그 종교집회에서 아우터갓의 존재를 증명해줘야 해...",
          "original_form": "대신 그 종교집회에서 아우터갓의 존재를 증명해줘야 해...",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028830.1.30",
          "form": "재밌겠다...",
          "original_form": "재밌겠다...",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028830.1.31",
          "form": "증명은 당신의 존재가 증명이 되겠죠",
          "original_form": "증명은 당신의 존재가 증명이 되겠죠",
          "speaker_id": "1",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100028830.1.32",
          "form": "종교...",
          "original_form": "종교...",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028830.1.33",
          "form": "불교공부가 해보고 싶어요",
          "original_form": "불교공부가 해보고 싶어요",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028830.1.34",
          "form": "성경공부하자고 하면 의심해야하지만",
          "original_form": "성경공부하자고 하면 의심해야하지만",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028830.1.35",
          "form": "불교는 공부가 어울리는 종교예요",
          "original_form": "불교는 공부가 어울리는 종교예요",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028830.1.36",
          "form": "불교는 믿음 공부가 아니라 자기수양이라",
          "original_form": "불교는 믿음 공부가 아니라 자기수양이라",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028830.1.37",
          "form": "사실 종교라기보단...수양의 느낌이랄까",
          "original_form": "사실 종교라기보단...수양의 느낌이랄까",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028830.1.38",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
