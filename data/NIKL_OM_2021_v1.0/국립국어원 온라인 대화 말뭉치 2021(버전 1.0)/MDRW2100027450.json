      "utterance": [
        {
          "id": "MDRW2100027450.1.1",
          "form": "날씨가 많이 쌀쌀해졌네요",
          "original_form": "날씨가 많이 쌀쌀해졌네요",
          "speaker_id": "1",
          "time": "20210902 12:45"
        },
        {
          "id": "MDRW2100027450.1.2",
          "form": "아침에는 많이 춥더라고요.",
          "original_form": "아침에는 많이 춥더라고요.",
          "speaker_id": "2",
          "time": "20210902 12:47"
        },
        {
          "id": "MDRW2100027450.1.3",
          "form": "반팔입고 얇은 자켓걸치고다녀요",
          "original_form": "반팔입고 얇은 자켓걸치고다녀요",
          "speaker_id": "1",
          "time": "20210902 12:47"
        },
        {
          "id": "MDRW2100027450.1.4",
          "form": "그러네요.ㅎ",
          "original_form": "그러네요.ㅎ",
          "speaker_id": "2",
          "time": "20210902 12:48"
        },
        {
          "id": "MDRW2100027450.1.5",
          "form": "요즘시대어 감기조심하세요",
          "original_form": "요즘시대어 감기조심하세요",
          "speaker_id": "1",
          "time": "20210902 12:48"
        },
        {
          "id": "MDRW2100027450.1.6",
          "form": "name1도 조심하세요",
          "original_form": "&name1&도 조심하세요",
          "speaker_id": "2",
          "time": "20210902 12:50"
        },
        {
          "id": "MDRW2100027450.1.7",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "1",
          "time": "20210902 12:50"
        },
        {
          "id": "MDRW2100027450.1.8",
          "form": "찬바람에 재채기가 나오더라구요",
          "original_form": "찬바람에 재채기가 나오더라구요",
          "speaker_id": "2",
          "time": "20210902 12:50"
        },
        {
          "id": "MDRW2100027450.1.9",
          "form": "저두 가끔요",
          "original_form": "저두 가끔요",
          "speaker_id": "1",
          "time": "20210902 12:51"
        },
        {
          "id": "MDRW2100027450.1.10",
          "form": "어느계절좋아하세요?",
          "original_form": "어느계절좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 12:51"
        },
        {
          "id": "MDRW2100027450.1.11",
          "form": "저는 가을요",
          "original_form": "저는 가을요",
          "speaker_id": "1",
          "time": "20210902 12:51"
        },
        {
          "id": "MDRW2100027450.1.12",
          "form": "저는 겨울요",
          "original_form": "저는 겨울요",
          "speaker_id": "2",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027450.1.13",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027450.1.14",
          "form": "겨울스포츠를 좋아하거든요..눈오는것도 너무좋구요",
          "original_form": "겨울스포츠를 좋아하거든요..눈오는것도 너무좋구요",
          "speaker_id": "2",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027450.1.15",
          "form": "문슨운동좋아하시나요? 전 잘못해서ㅜㅜ",
          "original_form": "문슨운동좋아하시나요? 전 잘못해서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027450.1.16",
