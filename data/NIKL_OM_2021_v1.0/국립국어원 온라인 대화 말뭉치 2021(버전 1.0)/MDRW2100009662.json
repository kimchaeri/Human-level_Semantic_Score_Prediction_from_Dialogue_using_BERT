      "utterance": [
        {
          "id": "MDRW2100009662.1.1",
          "form": "긴머리하고싶어요.",
          "original_form": "긴머리하고싶어요.",
          "speaker_id": "2",
          "time": "20210823 02:49"
        },
        {
          "id": "MDRW2100009662.1.2",
          "form": "님 제발..",
          "original_form": "님 제발..",
          "speaker_id": "2",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100009662.1.3",
          "form": "오초만 기다려달라고요",
          "original_form": "오초만 기다려달라고요",
          "speaker_id": "2",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100009662.1.4",
          "form": "오늘 비 엄청오네요 비오는 날 좋아하시나요?",
          "original_form": "오늘 비 엄청오네요 비오는 날 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210823 13:15"
        },
        {
          "id": "MDRW2100009662.1.5",
          "form": "오케이..오초..",
          "original_form": "오케이..오초..",
          "speaker_id": "1",
          "time": "20210823 13:16"
        },
        {
          "id": "MDRW2100009662.1.6",
          "form": "거기 비 많이 오나요",
          "original_form": "거기 비 많이 오나요",
          "speaker_id": "1",
          "time": "20210823 13:16"
        },
        {
          "id": "MDRW2100009662.1.7",
          "form": "아니 사실 생선구이 시킬라했는데 비와가지고 라이더분께 좀 죄송하네요",
          "original_form": "아니 사실 생선구이 시킬라했는데 비와가지고 라이더분께 좀 죄송하네요",
          "speaker_id": "1",
          "time": "20210823 13:16"
        },
        {
          "id": "MDRW2100009662.1.8",
          "form": "네 지금... 진짜 많이와요 아",
          "original_form": "네 지금... 진짜 많이와요 아",
          "speaker_id": "2",
          "time": "20210823 13:16"
        },
        {
          "id": "MDRW2100009662.1.9",
          "form": "그래서 문열고 생선굽는 중",
          "original_form": "그래서 문열고 생선굽는 중",
          "speaker_id": "1",
          "time": "20210823 13:16"
        },
        {
          "id": "MDRW2100009662.1.10",
          "form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:16"
        },
        {
          "id": "MDRW2100009662.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ비오는 날 구우면 냄새.. 더 나는 느낌 들지 않나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ비오는 날 구우면 냄새.. 더 나는 느낌 들지 않나요?",
          "speaker_id": "2",
          "time": "20210823 13:16"
        },
        {
          "id": "MDRW2100009662.1.12",
          "form": "헐 맞아요",
          "original_form": "헐 맞아요",
          "speaker_id": "1",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009662.1.13",
          "form": "그게 과학적으로 뭔가 있을거같은데",
          "original_form": "그게 과학적으로 뭔가 있을거같은데",
          "speaker_id": "1",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009662.1.14",
          "form": "저는 핑프니까",
          "original_form": "저는 핑프니까",
          "speaker_id": "1",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009662.1.15",
          "form": "그쵸 약간 비가 냄새 더 나게 해주는 느낌;",
          "original_form": "그쵸 약간 비가 냄새 더 나게 해주는 느낌;",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009662.1.16",
          "form": "이 합리적 의심이 들어요",
          "original_form": "이 합리적 의심이 들어요",
          "speaker_id": "1",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009662.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009662.1.18",
          "form": "대충 맞다고 해요",
          "original_form": "대충 맞다고 해요",
          "speaker_id": "1",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009662.1.19",
          "form": "궁금하긴한데. 저는 궁금한채로 살겠습니다",
          "original_form": "궁금하긴한데. 저는 궁금한채로 살겠습니다",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009662.1.20",
          "form": "중력파가 문제가 아닐지(?)",
          "original_form": "중력파가 문제가 아닐지(?)",
          "speaker_id": "1",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009662.1.21",
          "form": "원래 그런거잖아요",
          "original_form": "원래 그런거잖아요",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009662.1.22",
          "form": "맞아요 궁금한거 팟으면",
          "original_form": "맞아요 궁금한거 팟으면",
          "speaker_id": "1",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009662.1.23",
          "form": "박사됐죠",
          "original_form": "박사됐죠",
          "speaker_id": "1",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009662.1.24",
          "form": "그쵸그쵸 근데 아니니까 저는 궁금한채로 살겠습니다",
          "original_form": "그쵸그쵸 근데 아니니까 저는 궁금한채로 살겠습니다",
          "speaker_id": "2",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100009662.1.25",
          "form": "궁금한채로 남으니 척척고졸 또는 척척학사인겁니다",
          "original_form": "궁금한채로 남으니 척척고졸 또는 척척학사인겁니다",
          "speaker_id": "1",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100009662.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100009662.1.27",
          "form": "아 맞다 태풍온다면서요",
          "original_form": "아 맞다 태풍온다면서요",
          "speaker_id": "2",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100009662.1.28",
          "form": "헐 이게 태풍 + 장마였나요",
          "original_form": "헐 이게 태풍 + 장마였나요",
          "speaker_id": "1",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100009662.1.29",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100009662.1.30",
          "form": "리디 비오는 날 포인트 주는데 많이받기만을 바랄 뿐입니다",
          "original_form": "리디 비오는 날 포인트 주는데 많이받기만을 바랄 뿐입니다",
          "speaker_id": "1",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100009662.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100009662.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100009662.1.33",
          "form": "왜..서울은 비가 안오는지?",
          "original_form": "왜..서울은 비가 안오는지?",
          "speaker_id": "1",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100009662.1.34",
          "form": "저 비오는날만 기다렸어요",
          "original_form": "저 비오는날만 기다렸어요",
          "speaker_id": "2",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100009662.1.35",
          "form": "포인트 굿굿~~^^!",
          "original_form": "포인트 굿굿~~^^!",
          "speaker_id": "2",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100009662.1.36",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "2",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100009662.1.37",
          "form": "서울안오면 안준다고요",
          "original_form": "서울안오면 안준다고요",
          "speaker_id": "2",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100009662.1.38",
          "form": "그게 웃긴게 선릉에",
          "original_form": "그게 웃긴게 선릉에",
          "speaker_id": "1",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100009662.1.39",
          "form": "긍까요",
          "original_form": "긍까요",
          "speaker_id": "1",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100009662.1.40",
          "form": "리디서울에있어서ㅠㅠㅠ!",
          "original_form": "리디서울에있어서ㅠㅠㅠ!",
          "speaker_id": "2",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100009662.1.41",
          "form": "심지어 4시까지 와야해",
          "original_form": "심지어 4시까지 와야해",
          "speaker_id": "1",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100009662.1.42",
          "form": "아 치사하네",
          "original_form": "아 치사하네",
          "speaker_id": "2",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100009662.1.43",
          "form": "이게 말이되나요",
          "original_form": "이게 말이되나요",
          "speaker_id": "1",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100009662.1.44",
          "form": "천지신명님 가성비제사 갑니다",
          "original_form": "천지신명님 가성비제사 갑니다",
          "speaker_id": "1",
          "time": "20210823 13:20"
        },
        {
          "id": "MDRW2100009662.1.45",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 씁... 날씨 조종하는 사람 리디에 입사해라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 씁... 날씨 조종하는 사람 리디에 입사해라",
          "speaker_id": "2",
          "time": "20210823 13:20"
        },
        {
          "id": "MDRW2100009662.1.46",
          "form": "저 위험한 편의점 사야하는데 내려주소서",
          "original_form": "저 위험한 편의점 사야하는데 내려주소서",
          "speaker_id": "1",
