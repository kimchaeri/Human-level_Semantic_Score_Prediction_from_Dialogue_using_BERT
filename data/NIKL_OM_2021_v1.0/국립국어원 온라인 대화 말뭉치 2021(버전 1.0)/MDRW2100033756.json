      "utterance": [
        {
          "id": "MDRW2100033756.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100033756.1.2",
          "form": "안녕하세요 그곳도 비가오는 중인가요?",
          "original_form": "안녕하세요 그곳도 비가오는 중인가요?",
          "speaker_id": "2",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100033756.1.3",
          "form": "비그쳤어요!",
          "original_form": "비그쳤어요!",
          "speaker_id": "1",
          "time": "20210825 00:41"
        },
        {
          "id": "MDRW2100033756.1.4",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210825 00:41"
        },
        {
          "id": "MDRW2100033756.1.5",
          "form": "그래도 여긴 좀 꿉꿉해서",
          "original_form": "그래도 여긴 좀 꿉꿉해서",
          "speaker_id": "2",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100033756.1.6",
          "form": "방금 첨으로 제습기를 틀었어요ㅠㅠ",
          "original_form": "방금 첨으로 제습기를 틀었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100033756.1.7",
          "form": "여기도 엄청 습하네요~",
          "original_form": "여기도 엄청 습하네요~",
          "speaker_id": "1",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100033756.1.8",
          "form": "젤 좋아하는 계절이 어떤거세요?",
          "original_form": "젤 좋아하는 계절이 어떤거세요?",
          "speaker_id": "2",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100033756.1.9",
          "form": "여름이요~",
          "original_form": "여름이요~",
          "speaker_id": "1",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100033756.1.10",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100033756.1.11",
          "form": "저는 가을이요!",
          "original_form": "저는 가을이요!",
          "speaker_id": "2",
          "time": "20210825 00:45"
        },
        {
          "id": "MDRW2100033756.1.12",
          "form": "앗 여름 어떤 점이 가장 좋으세여?!!",
          "original_form": "앗 여름 어떤 점이 가장 좋으세여?!!",
          "speaker_id": "2",
          "time": "20210825 00:45"
        },
        {
          "id": "MDRW2100033756.1.13",
          "form": "더위 안타시는 편인가요?",
          "original_form": "더위 안타시는 편인가요?",
          "speaker_id": "2",
          "time": "20210825 00:45"
        },
        {
          "id": "MDRW2100033756.1.14",
          "form": "안추워서요..ㅋㅋㅋ",
          "original_form": "안추워서요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100033756.1.15",
          "form": "에어컨 끼고살아요",
          "original_form": "에어컨 끼고살아요",
          "speaker_id": "1",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100033756.1.16",
          "form": "아 추위를 더",
          "original_form": "아 추위를 더",
          "speaker_id": "2",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100033756.1.17",
          "form": "타시는군욬ㅋㅋㅋㅋ",
          "original_form": "타시는군욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100033756.1.18",
          "form": "여름 스포츠는 안즐기시나요?!",
          "original_form": "여름 스포츠는 안즐기시나요?!",
          "speaker_id": "2",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100033756.1.19",
          "form": "수상 스키 타본적있어요!",
          "original_form": "수상 스키 타본적있어요!",
          "speaker_id": "1",
          "time": "20210825 00:48"
        },
        {
          "id": "MDRW2100033756.1.20",
          "form": "대애박 잼나던가요",
          "original_form": "대애박 잼나던가요",
          "speaker_id": "2",
          "time": "20210825 00:49"
        },
        {
          "id": "MDRW2100033756.1.21",
          "form": "아니 잼날것같아요 엄청",
          "original_form": "아니 잼날것같아요 엄청",
          "speaker_id": "2",
          "time": "20210825 00:49"
        },
        {
          "id": "MDRW2100033756.1.22",
          "form": "재밌어용",
          "original_form": "재밌어용",
          "speaker_id": "1",
