      "utterance": [
        {
          "id": "MDRW2100034687.1.1",
          "form": "우와 타투!!!!",
          "original_form": "우와 타투!!!!",
          "speaker_id": "2",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100034687.1.2",
          "form": "세번쯤 참고 악세사리에 했거든요",
          "original_form": "세번쯤 참고 악세사리에 했거든요",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100034687.1.3",
          "form": "전 아픈거 무서워하는 쫄보라 헤나 정도 도전했습니다",
          "original_form": "전 아픈거 무서워하는 쫄보라 헤나 정도 도전했습니다",
          "speaker_id": "2",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100034687.1.4",
          "form": "크",
          "original_form": "크",
          "speaker_id": "2",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100034687.1.5",
          "form": "멋져요",
          "original_form": "멋져요",
          "speaker_id": "2",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100034687.1.6",
          "form": "악세사리는.. 내 피부의 연장선이쥬",
          "original_form": "악세사리는.. 내 피부의 연장선이쥬",
          "speaker_id": "2",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100034687.1.7",
          "form": "저도 없어요ㅠ몸에 하려다 악세사리에 한거에요ㅜ",
          "original_form": "저도 없어요ㅠ몸에 하려다 악세사리에 한거에요ㅜ",
          "speaker_id": "1",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100034687.1.8",
          "form": "타투 하고싶어요!",
          "original_form": "타투 하고싶어요!",
          "speaker_id": "1",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100034687.1.9",
          "form": "오 어디 하고싶으세요?",
          "original_form": "오 어디 하고싶으세요?",
          "speaker_id": "2",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100034687.1.10",
          "form": "저는 손이나 허리쪽이요",
          "original_form": "저는 손이나 허리쪽이요",
          "speaker_id": "1",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100034687.1.11",
          "form": "직장에 팔뚝뒤에 얼굴 겁나크게하신 분이 계신데 멋지더라구요",
          "original_form": "직장에 팔뚝뒤에 얼굴 겁나크게하신 분이 계신데 멋지더라구요",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100034687.1.12",
          "form": "오오...",
          "original_form": "오오...",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100034687.1.13",
          "form": "손가락처럼 자주 씻는곳은 빨리 지워진대요",
          "original_form": "손가락처럼 자주 씻는곳은 빨리 지워진대요",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100034687.1.14",
          "form": "네 그래서 허리는 어떨까 고민해보았습니다ㅋㅋ",
          "original_form": "네 그래서 허리는 어떨까 고민해보았습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:59"
        },
        {
          "id": "MDRW2100034687.1.15",
          "form": "헤나는 휴가가시면서 하신거에요?",
          "original_form": "헤나는 휴가가시면서 하신거에요?",
          "speaker_id": "1",
          "time": "20210828 17:59"
        },
        {
          "id": "MDRW2100034687.1.16",
          "form": "네 그것도 십년 전이에요 ㅋㅋ 요새는.. 그냥 가죽입니다",
          "original_form": "네 그것도 십년 전이에요 ㅋㅋ 요새는.. 그냥 가죽입니다",
          "speaker_id": "2",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100034687.1.17",
          "form": "해외여행가면서 친구랑 맞춰서 손부터 팔 아래쪽까지 문양을 넣었어요 싱기방기",
          "original_form": "해외여행가면서 친구랑 맞춰서 손부터 팔 아래쪽까지 문양을 넣었어요 싱기방기",
          "speaker_id": "2",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100034687.1.18",
          "form": "오 멋져부러",
          "original_form": "오 멋져부러",
          "speaker_id": "1",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100034687.1.19",
          "form": "그런게 추억이죠",
          "original_form": "그런게 추억이죠",
          "speaker_id": "1",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100034687.1.20",
          "form": "타투는 새기면 지울때 넘 아픈 추억인거구요ㅋㅋ",
          "original_form": "타투는 새기면 지울때 넘 아픈 추억인거구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100034687.1.21",
          "form": "ㅋㅋ새기면 못지우는 미용 액션이죠",
          "original_form": "ㅋㅋ새기면 못지우는 미용 액션이죠",
          "speaker_id": "2",
          "time": "20210828 18:04"
        },
        {
          "id": "MDRW2100034687.1.22",
          "form": "레이저로 지워두 흔적이 남는다며요",
          "original_form": "레이저로 지워두 흔적이 남는다며요",
          "speaker_id": "2",
          "time": "20210828 18:04"
        },
        {
          "id": "MDRW2100034687.1.23",
          "form": "네 그리고 으엄청 아프대요",
          "original_form": "네 그리고 으엄청 아프대요",
          "speaker_id": "1",
          "time": "20210828 18:04"
        },
        {
          "id": "MDRW2100034687.1.24",
          "form": "등짝 가득 하신분은 진짜 대단하고 돈도 많으신..",
          "original_form": "등짝 가득 하신분은 진짜 대단하고 돈도 많으신..",
          "speaker_id": "1",
          "time": "20210828 18:05"
        },
        {
          "id": "MDRW2100034687.1.25",
          "form": "피도 많이 나나 보더라고요",
          "original_form": "피도 많이 나나 보더라고요",
          "speaker_id": "2",
          "time": "20210828 18:06"
        },
        {
          "id": "MDRW2100034687.1.26",
          "form": "한동안 그 타투 디자인하시는 분들 사진이 인스타에 떠서 봤는데.. 아파보였어요..........",
          "original_form": "한동안 그 타투 디자인하시는 분들 사진이 인스타에 떠서 봤는데.. 아파보였어요..........",
          "speaker_id": "2",
          "time": "20210828 18:06"
        },
        {
          "id": "MDRW2100034687.1.27",
          "form": "그렇겠죠 바늘로 계속 찌르는 거니까.......",
          "original_form": "그렇겠죠 바늘로 계속 찌르는 거니까.......",
          "speaker_id": "2",
          "time": "20210828 18:06"
        },
        {
          "id": "MDRW2100034687.1.28",
          "form": "네 사진은 이쁜데 주변에 피부가 시뻘겧게 돼서..",
          "original_form": "네 사진은 이쁜데 주변에 피부가 시뻘겧게 돼서..",
          "speaker_id": "1",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100034687.1.29",
          "form": "지금 거리두기가 3단계로 내려갔는데요",
          "original_form": "지금 거리두기가 3단계로 내려갔는데요",
          "speaker_id": "1",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100034687.1.30",
          "form": "헬스장이 너모 가고싶어요..",
          "original_form": "헬스장이 너모 가고싶어요..",
          "speaker_id": "1",
