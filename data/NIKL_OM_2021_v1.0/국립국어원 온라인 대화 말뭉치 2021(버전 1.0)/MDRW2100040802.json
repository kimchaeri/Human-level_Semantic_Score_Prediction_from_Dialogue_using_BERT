      "utterance": [
        {
          "id": "MDRW2100040802.1.1",
          "form": "진짜 괴로워요ㅠㅠ",
          "original_form": "진짜 괴로워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100040802.1.2",
          "form": "고치는방법은 없나요?",
          "original_form": "고치는방법은 없나요?",
          "speaker_id": "1",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100040802.1.3",
          "form": "눈도 빨개요..어쩔땐..",
          "original_form": "눈도 빨개요..어쩔땐..",
          "speaker_id": "1",
          "time": "20210905 07:50"
        },
        {
          "id": "MDRW2100040802.1.4",
          "form": "전 코안쪽도 휘고",
          "original_form": "전 코안쪽도 휘고",
          "speaker_id": "2",
          "time": "20210905 07:50"
        },
        {
          "id": "MDRW2100040802.1.5",
          "form": "점막이 공기가 닿는부위가.어쩌고 저쩌고 수술도",
          "original_form": "점막이 공기가 닿는부위가.어쩌고 저쩌고 수술도",
          "speaker_id": "2",
          "time": "20210905 07:50"
        },
        {
          "id": "MDRW2100040802.1.6",
          "form": "안 권하더라구요",
          "original_form": "안 권하더라구요",
          "speaker_id": "2",
          "time": "20210905 07:50"
        },
        {
          "id": "MDRW2100040802.1.7",
          "form": "서울무슨 대학병원이 잘본다고하는데 비염땜에 서울까지가야하나해서요",
          "original_form": "서울무슨 대학병원이 잘본다고하는데 비염땜에 서울까지가야하나해서요",
          "speaker_id": "1",
          "time": "20210905 07:50"
        },
        {
          "id": "MDRW2100040802.1.8",
          "form": "또 이게 체질이랑 위부요인이기때문에",
          "original_form": "또 이게 체질이랑 위부요인이기때문에",
          "speaker_id": "2",
          "time": "20210905 07:50"
        },
        {
          "id": "MDRW2100040802.1.9",
          "form": "완치보단 그냥 어찌 잘 넘어가냐의",
          "original_form": "완치보단 그냥 어찌 잘 넘어가냐의",
          "speaker_id": "2",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100040802.1.10",
          "form": "차이가아닌가 싶어요ㅠ",
          "original_form": "차이가아닌가 싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100040802.1.11",
          "form": "아~~그렇군요",
          "original_form": "아~~그렇군요",
          "speaker_id": "1",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100040802.1.12",
          "form": "저는 가을이너무좋은데 아들비염보면 빨리지나가길바래요",
          "original_form": "저는 가을이너무좋은데 아들비염보면 빨리지나가길바래요",
          "speaker_id": "1",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100040802.1.13",
          "form": "아드님이 잘 견디길 바래요ㅠㅠ~",
          "original_form": "아드님이 잘 견디길 바래요ㅠㅠ~",
          "speaker_id": "2",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100040802.1.14",
          "form": "가을이 제일심한것같아요",
          "original_form": "가을이 제일심한것같아요",
          "speaker_id": "1",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100040802.1.15",
          "form": "맞아여 또 병원에 따라 약을 너무 쌔게 처방해주면",
          "original_form": "맞아여 또 병원에 따라 약을 너무 쌔게 처방해주면",
          "speaker_id": "2",
          "time": "20210905 07:52"
        },
        {
          "id": "MDRW2100040802.1.16",
          "form": "코가 너무 건조해지더라구여ㅠ",
          "original_form": "코가 너무 건조해지더라구여ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:52"
        },
        {
          "id": "MDRW2100040802.1.17",
          "form": "아..그래요?",
          "original_form": "아..그래요?",
          "speaker_id": "1",
          "time": "20210905 07:52"
        },
        {
          "id": "MDRW2100040802.1.18",
          "form": "이게 진짜 외부 요인인게 저도 코로 올때도 있고 눈으로 올때도 잇고",
          "original_form": "이게 진짜 외부 요인인게 저도 코로 올때도 있고 눈으로 올때도 잇고",
          "speaker_id": "2",
          "time": "20210905 07:52"
        },
        {
          "id": "MDRW2100040802.1.19",
          "form": "비염되다충농증된다고하고",
          "original_form": "비염되다충농증된다고하고",
          "speaker_id": "1",
          "time": "20210905 07:52"
        },
        {
          "id": "MDRW2100040802.1.20",
          "form": "염증이 뱐응하는곳이 다른거같아요 ㅠ",
          "original_form": "염증이 뱐응하는곳이 다른거같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:52"
        }
      ]
