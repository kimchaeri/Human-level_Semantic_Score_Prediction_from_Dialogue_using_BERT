      "utterance": [
        {
          "id": "MDRW2100021499.1.1",
          "form": "요즘에 날씨가 너무 왔다갔다해요",
          "original_form": "요즘에 날씨가 너무 왔다갔다해요",
          "speaker_id": "2",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100021499.1.2",
          "form": "우산을 항상 들고다녀야겠더라구요ㅜㅜ",
          "original_form": "우산을 항상 들고다녀야겠더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100021499.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100021499.1.4",
          "form": "가을장마래요!  처음 들어봤어요",
          "original_form": "가을장마래요!  처음 들어봤어요",
          "speaker_id": "1",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100021499.1.5",
          "form": "뭔가 날씨에 따라 오는게 반박자씩 늦춰진거 같아요",
          "original_form": "뭔가 날씨에 따라 오는게 반박자씩 늦춰진거 같아요",
          "speaker_id": "2",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100021499.1.6",
          "form": "헐 그런가요?",
          "original_form": "헐 그런가요?",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100021499.1.7",
          "form": "뉴스도 안맞더라구요",
          "original_form": "뉴스도 안맞더라구요",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100021499.1.8",
          "form": "아 최근에 기사에서 폭염에도 이름붙여야한다는 논의가 있다는걸 봤어요",
          "original_form": "아 최근에 기사에서 폭염에도 이름붙여야한다는 논의가 있다는걸 봤어요",
          "speaker_id": "2",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100021499.1.9",
          "form": "아그래요?? 신기하네요",
          "original_form": "아그래요?? 신기하네요",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100021499.1.10",
          "form": "폭염에 이름이라니..!",
          "original_form": "폭염에 이름이라니..!",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100021499.1.11",
          "form": "슈퍼컴퓨터도 예측하기 힘든 날씬가봐요",
          "original_form": "슈퍼컴퓨터도 예측하기 힘든 날씬가봐요",
          "speaker_id": "2",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100021499.1.12",
          "form": "태풍에 이름 붙이잖아요",
          "original_form": "태풍에 이름 붙이잖아요",
          "speaker_id": "2",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100021499.1.13",
          "form": "아~ 그런거처럼 붙이려는건가요",
          "original_form": "아~ 그런거처럼 붙이려는건가요",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100021499.1.14",
          "form": "이름까지 붙이니 사람들이 심각하게 생각하고 주의하니까요",
          "original_form": "이름까지 붙이니 사람들이 심각하게 생각하고 주의하니까요",
          "speaker_id": "2",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100021499.1.15",
          "form": "네 폭염도 조심해야한다는 취지로 발의한 것 같아요",
          "original_form": "네 폭염도 조심해야한다는 취지로 발의한 것 같아요",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100021499.1.16",
          "form": "그것도 그러네요 폭염이 너무 심하더라구요",
          "original_form": "그것도 그러네요 폭염이 너무 심하더라구요",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100021499.1.17",
          "form": "폭염에 사망자도 많이 나오니까요ㅜㅜ",
          "original_form": "폭염에 사망자도 많이 나오니까요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100021499.1.18",
          "form": "갈수록 더워져서 큰일이에요",
          "original_form": "갈수록 더워져서 큰일이에요",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100021499.1.19",
          "form": "맞아요 더워서 사망자가 나오다니..",
          "original_form": "맞아요 더워서 사망자가 나오다니..",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100021499.1.20",
          "form": "더위에 사람이 죽으리라고는 생각도 못한 일인데요 예전엔..",
          "original_form": "더위에 사람이 죽으리라고는 생각도 못한 일인데요 예전엔..",
          "speaker_id": "1",
          "time": "20210828 19:46"
        }
      ]
