      "utterance": [
        {
          "id": "MDRW2100020841.1.1",
          "form": "요즘 날씨 꽤 추워졌던데",
          "original_form": "요즘 날씨 꽤 추워졌던데",
          "speaker_id": "1",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020841.1.2",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100020841.1.3",
          "form": "근데 낮에는 너무 더워",
          "original_form": "근데 낮에는 너무 더워",
          "speaker_id": "2",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100020841.1.4",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "1",
          "time": "20210828 13:21"
        },
        {
          "id": "MDRW2100020841.1.5",
          "form": "그늘밖은 덥더라",
          "original_form": "그늘밖은 덥더라",
          "speaker_id": "1",
          "time": "20210828 13:26"
        },
        {
          "id": "MDRW2100020841.1.6",
          "form": "너는 어느 계절이 가장 좋아?",
          "original_form": "너는 어느 계절이 가장 좋아?",
          "speaker_id": "2",
          "time": "20210828 13:27"
        },
        {
          "id": "MDRW2100020841.1.7",
          "form": "나는 가을",
          "original_form": "나는 가을",
          "speaker_id": "1",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100020841.1.8",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "1",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100020841.1.9",
          "form": "겨울??",
          "original_form": "겨울??",
          "speaker_id": "1",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100020841.1.10",
          "form": "나도 가을",
          "original_form": "나도 가을",
          "speaker_id": "2",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020841.1.11",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020841.1.12",
          "form": "나는 산책하기 좋은 날씨라서 딱 적당하기도하고",
          "original_form": "나는 산책하기 좋은 날씨라서 딱 적당하기도하고",
          "speaker_id": "2",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020841.1.13",
          "form": "나도 선선한거 좋아서",
          "original_form": "나도 선선한거 좋아서",
          "speaker_id": "1",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020841.1.14",
          "form": "그리고 하늘이 이쁘잖아",
          "original_form": "그리고 하늘이 이쁘잖아",
          "speaker_id": "1",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020841.1.15",
          "form": "미먼도 적고",
          "original_form": "미먼도 적고",
          "speaker_id": "1",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020841.1.16",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "1",
          "time": "20210828 13:33"
        },
        {
          "id": "MDRW2100020841.1.17",
          "form": "맞아.. 완전 좋지",
          "original_form": "맞아.. 완전 좋지",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020841.1.18",
          "form": "가을에 주로 입는 옷이 뭐야?",
          "original_form": "가을에 주로 입는 옷이 뭐야?",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020841.1.19",
          "form": "가디건",
          "original_form": "가디건",
          "speaker_id": "1",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100020841.1.20",
          "form": "많이 입어",
          "original_form": "많이 입어",
          "speaker_id": "1",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100020841.1.21",
          "form": "옷이 별로 없어서",
          "original_form": "옷이 별로 없어서",
          "speaker_id": "1",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100020841.1.22",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "1",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100020841.1.23",
          "form": "아 나 가디건 좋아하는데",
          "original_form": "아 나 가디건 좋아하는데",
          "speaker_id": "2",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100020841.1.24",
          "form": "없어가지고 못 입어 ㅋㅋㅋㅋㅋ",
          "original_form": "없어가지고 못 입어 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100020841.1.25",
          "form": "아 그래??",
          "original_form": "아 그래??",
          "speaker_id": "1",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100020841.1.26",
          "form": "나도 걍 동생꺼 입는거야",
          "original_form": "나도 걍 동생꺼 입는거야",
          "speaker_id": "1",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100020841.1.27",
          "form": "봄에는 뭐 입어?",
          "original_form": "봄에는 뭐 입어?",
          "speaker_id": "1",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100020841.1.28",
          "form": "봄에는 맨투맨이나 후드티 많이 입는 것 같아",
          "original_form": "봄에는 맨투맨이나 후드티 많이 입는 것 같아",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020841.1.29",
          "form": "놀러갈때는 얇은 코트나 후드집업",
          "original_form": "놀러갈때는 얇은 코트나 후드집업",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020841.1.30",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020841.1.31",
          "form": "나도 후드티나 맨투맨 많이 입어",
          "original_form": "나도 후드티나 맨투맨 많이 입어",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020841.1.32",
          "form": "몸에 뭐 많이 걸치는걸 싫어해서",
          "original_form": "몸에 뭐 많이 걸치는걸 싫어해서",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020841.1.33",
          "form": "겉옷은 잘 안 입는편",
          "original_form": "겉옷은 잘 안 입는편",
          "speaker_id": "1",
          "time": "20210828 13:43"
        }
      ]
    }
  ]
