      "utterance": [
        {
          "id": "MDRW2100024516.1.1",
          "form": "안녕하세요 운동하시는거 있으세요?",
          "original_form": "안녕하세요 운동하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100024516.1.2",
          "form": "전 주로 걷기운동해요",
          "original_form": "전 주로 걷기운동해요",
          "speaker_id": "2",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100024516.1.3",
          "form": "오 걷기 좋네요 다이어트에 도움되나요?",
          "original_form": "오 걷기 좋네요 다이어트에 도움되나요?",
          "speaker_id": "1",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100024516.1.4",
          "form": "아뇨ㅋㅋㅋㅋ",
          "original_form": "아뇨ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:42"
        },
        {
          "id": "MDRW2100024516.1.5",
          "form": "잘먹기까지 해서 건강한 돼지가 되고 있어요ㅋㅋ",
          "original_form": "잘먹기까지 해서 건강한 돼지가 되고 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:42"
        },
        {
          "id": "MDRW2100024516.1.6",
          "form": "스포츠경기 보는거는 있으세요?",
          "original_form": "스포츠경기 보는거는 있으세요?",
          "speaker_id": "1",
          "time": "20210831 12:44"
        },
        {
          "id": "MDRW2100024516.1.7",
          "form": "아뇨ㅜㅜ",
          "original_form": "아뇨ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100024516.1.8",
          "form": "보시는거 있으세요?",
          "original_form": "보시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100024516.1.9",
          "form": "예전엔 봤는데 요즘은 거의 안챙겨봐요",
          "original_form": "예전엔 봤는데 요즘은 거의 안챙겨봐요",
          "speaker_id": "1",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100024516.1.10",
          "form": "그쵸 저도 예전에는 야구장도 가고 그랬는데 안간지 꽤됐네요",
          "original_form": "그쵸 저도 예전에는 야구장도 가고 그랬는데 안간지 꽤됐네요",
          "speaker_id": "2",
          "time": "20210831 12:47"
        },
        {
          "id": "MDRW2100024516.1.11",
          "form": "이번에 올림픽하는거는 보셨어요?",
          "original_form": "이번에 올림픽하는거는 보셨어요?",
          "speaker_id": "1",
          "time": "20210831 12:48"
        },
        {
          "id": "MDRW2100024516.1.12",
          "form": "아뇨 뉴스로만 접했어요ㅜㅜ",
          "original_form": "아뇨 뉴스로만 접했어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100024516.1.13",
          "form": "그래도 양궁은 다봤네요ㅎ",
          "original_form": "그래도 양궁은 다봤네요ㅎ",
          "speaker_id": "2",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100024516.1.14",
          "form": "아아 양궁 보셨군요 저는 그거도 안봤어요",
          "original_form": "아아 양궁 보셨군요 저는 그거도 안봤어요",
          "speaker_id": "1",
          "time": "20210831 12:59"
        },
        {
          "id": "MDRW2100024516.1.15",
          "form": "이상하게 별로 관심이 안가요",
          "original_form": "이상하게 별로 관심이 안가요",
          "speaker_id": "1",
          "time": "20210831 12:59"
        },
        {
          "id": "MDRW2100024516.1.16",
          "form": "ㅎㅎㅎㅎ그럴 수 있죠ㅋㅋ",
          "original_form": "ㅎㅎㅎㅎ그럴 수 있죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:03"
        },
        {
          "id": "MDRW2100024516.1.17",
          "form": "운동하시는거 있으세요?",
          "original_form": "운동하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210831 13:04"
        },
        {
          "id": "MDRW2100024516.1.18",
          "form": "수영예전에 했는데요 요즘은 안해요",
