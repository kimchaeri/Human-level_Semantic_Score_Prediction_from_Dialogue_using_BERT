      "utterance": [
        {
          "id": "MDRW2100015316.1.1",
          "form": "오캐이",
          "original_form": "오캐이",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015316.1.2",
          "form": "가고싶은 여행지 있어요",
          "original_form": "가고싶은 여행지 있어요",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015316.1.3",
          "form": "여행 다녀와보신 곳 중에 젤 좋으셨던 곳",
          "original_form": "여행 다녀와보신 곳 중에 젤 좋으셨던 곳",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015316.1.4",
          "form": "전 전주",
          "original_form": "전 전주",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015316.1.5",
          "form": "한옥마을",
          "original_form": "한옥마을",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015316.1.6",
          "form": "저는 파리올림픽 가고 말겁니다",
          "original_form": "저는 파리올림픽 가고 말겁니다",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015316.1.7",
          "form": "아 전주",
          "original_form": "아 전주",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015316.1.8",
          "form": "거기서 한복입고 인생샷 찍으시게요?!",
          "original_form": "거기서 한복입고 인생샷 찍으시게요?!",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015316.1.9",
          "form": "파리",
          "original_form": "파리",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015316.1.10",
          "form": "아니요 먹으러요",
          "original_form": "아니요 먹으러요",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015316.1.11",
          "form": "여행은 먹는게 남는거니까요 (진지)",
          "original_form": "여행은 먹는게 남는거니까요 (진지)",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015316.1.12",
          "form": "파리 이번에 영상보니까 개오지던데",
          "original_form": "파리 이번에 영상보니까 개오지던데",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015316.1.13",
          "form": "오케이 박사니므",
          "original_form": "오케이 박사니므",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015316.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015316.1.15",
          "form": "올림픽 경기장이 뭔 영화냐구요",
          "original_form": "올림픽 경기장이 뭔 영화냐구요",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015316.1.16",
          "form": "그거 보러가면 인생 엄청재밌을듯",
          "original_form": "그거 보러가면 인생 엄청재밌을듯",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015316.1.17",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015316.1.18",
          "form": "가시죠",
          "original_form": "가시죠",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015316.1.19",
          "form": "갈까요",
          "original_form": "갈까요",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015316.1.20",
          "form": "그때 퇴사해서라도 간드아",
          "original_form": "그때 퇴사해서라도 간드아",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015316.1.21",
          "form": "3년후에 파리 에펠탑앞에서 만날까요",
          "original_form": "3년후에 파리 에펠탑앞에서 만날까요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015316.1.22",
          "form": "어머나",
          "original_form": "어머나",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015316.1.23",
          "form": "근데 알아볼수나 있나요",
          "original_form": "근데 알아볼수나 있나요",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015316.1.24",
          "form": "우린 푸드메이트니까",
          "original_form": "우린 푸드메이트니까",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015316.1.25",
          "form": "박사님 팜플렛 들고있으면",
          "original_form": "박사님 팜플렛 들고있으면",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015316.1.26",
          "form": "오세요",
          "original_form": "오세요",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015316.1.27",
          "form": "알겠습니다",
          "original_form": "알겠습니다",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015316.1.28",
          "form": "꼭 기억할게요",
          "original_form": "꼭 기억할게요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015316.1.29",
          "form": "저는 해외를 가본적이 없어요.",
          "original_form": "저는 해외를 가본적이 없어요.",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015316.1.30",
          "form": "가려는데 코로나가..",
          "original_form": "가려는데 코로나가..",
          "speaker_id": "1",
          "time": "20210825 22:41"
        }
      ]
    }
  ]
