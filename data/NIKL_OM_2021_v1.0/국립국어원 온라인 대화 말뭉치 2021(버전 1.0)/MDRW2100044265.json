        },
        {
          "id": "MDRW2100044265.1.2",
          "form": "환승연애 체인지데이즈 돌싱글즈 다보고있어용!",
          "original_form": "환승연애 체인지데이즈 돌싱글즈 다보고있어용!",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044265.1.3",
          "form": "ㅋㅋㅋ 저 환승빠고다봄",
          "original_form": "ㅋㅋㅋ 저 환승빠고다봄",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044265.1.4",
          "form": "환승음 플랫폼 없어서ㅠ",
          "original_form": "환승음 플랫폼 없어서ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044265.1.5",
          "form": "오늘보ㅎ심 ?",
          "original_form": "오늘보ㅎ심 ?",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044265.1.6",
          "form": "네네!!! 환승연애가 티빙이죠 ㅋㅋㅋ",
          "original_form": "네네!!! 환승연애가 티빙이죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044265.1.7",
          "form": "결과 다보심?",
          "original_form": "결과 다보심?",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044265.1.8",
          "form": "체인지 데이즈요???,",
          "original_form": "체인지 데이즈요???,",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044265.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044265.1.10",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044265.1.11",
          "form": "그 봣는데... 스포 일까여?, ㅋㅋㅋ",
          "original_form": "그 봣는데... 스포 일까여?, ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044265.1.12",
          "form": "아뇨 저 거의다봄",
          "original_form": "아뇨 저 거의다봄",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044265.1.13",
          "form": "근데갑자기 대본같음 ㅋㅋㅋ",
          "original_form": "근데갑자기 대본같음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044265.1.14",
          "form": "결과.. 너무.. 허무.... ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "결과.. 너무.. 허무.... ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044265.1.15",
          "form": "저 그냥 스킵하고 마지막 장면만 봣어여",
          "original_form": "저 그냥 스킵하고 마지막 장면만 봣어여",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044265.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 근데 너무 길게끌엇죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 근데 너무 길게끌엇죠",
          "speaker_id": "2",
