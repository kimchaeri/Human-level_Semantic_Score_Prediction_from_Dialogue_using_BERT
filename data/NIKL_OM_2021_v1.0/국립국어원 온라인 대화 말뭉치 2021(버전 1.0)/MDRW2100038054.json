        },
        {
          "id": "MDRW2100038054.1.2",
          "form": "발목 아대샀어요!",
          "original_form": "발목 아대샀어요!",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100038054.1.3",
          "form": "발목을 접질러서..!",
          "original_form": "발목을 접질러서..!",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038054.1.4",
          "form": "헉 어쩌다가요",
          "original_form": "헉 어쩌다가요",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038054.1.5",
          "form": "계단에서 내려오다가 홀랑,,,",
          "original_form": "계단에서 내려오다가 홀랑,,,",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038054.1.6",
          "form": "아이고 아대는 인터넷서 사셧나요",
          "original_form": "아이고 아대는 인터넷서 사셧나요",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038054.1.7",
          "form": "효과잇나요",
          "original_form": "효과잇나요",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038054.1.8",
          "form": "약국가서샀는데",
          "original_form": "약국가서샀는데",
