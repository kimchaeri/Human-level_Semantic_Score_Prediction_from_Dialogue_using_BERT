      "utterance": [
        {
          "id": "MDRW2100016072.1.1",
          "form": "너는 주위에 결혼한 친구들 있나?",
          "original_form": "너는 주위에 결혼한 친구들 있나?",
          "speaker_id": "1",
          "time": "20210826 09:12"
        },
        {
          "id": "MDRW2100016072.1.2",
          "form": "웅웅 있어ㅋㅋㅋ",
          "original_form": "웅웅 있어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100016072.1.3",
          "form": "동기는 작년에했고",
          "original_form": "동기는 작년에했고",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100016072.1.4",
          "form": "우연찮게 카톡목록보다가 초딩동창 2명도 한거봤어ㅋㅋㅋ",
          "original_form": "우연찮게 카톡목록보다가 초딩동창 2명도 한거봤어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100016072.1.5",
          "form": "새삼 다들 결혼하는 나이가 다가오는거같아",
          "original_form": "새삼 다들 결혼하는 나이가 다가오는거같아",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016072.1.6",
          "form": "작년이면 25살이잖아 대박적",
          "original_form": "작년이면 25살이잖아 대박적",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016072.1.7",
          "form": "초딩동창이 둘이나?",
          "original_form": "초딩동창이 둘이나?",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016072.1.8",
          "form": "웅웅ㅋㅋㅋ",
          "original_form": "웅웅ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016072.1.9",
          "form": "와 진짜신기하다",
          "original_form": "와 진짜신기하다",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016072.1.10",
          "form": "웅 둘다 남자애들인데 별 친분은 없어가지고ㅋㅋㅋㅋ",
          "original_form": "웅 둘다 남자애들인데 별 친분은 없어가지고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016072.1.11",
          "form": "아직 애기가진친구는 없지?",
          "original_form": "아직 애기가진친구는 없지?",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016072.1.12",
          "form": "그냥 프사 결혼사진보고 알았음ㅋㅋㅋ",
          "original_form": "그냥 프사 결혼사진보고 알았음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016072.1.13",
          "form": "와 또 의외다",
          "original_form": "와 또 의외다",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016072.1.14",
          "form": "여자일줄",
          "original_form": "여자일줄",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016072.1.15",
          "form": "웅 아직 애기있는친구는 없어ㅋㅋㅋ",
          "original_form": "웅 아직 애기있는친구는 없어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016072.1.16",
          "form": "남자들은 좀 늦게가잖아",
          "original_form": "남자들은 좀 늦게가잖아",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016072.1.17",
          "form": "그니까ㅋㅋㅋㅋ",
          "original_form": "그니까ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016072.1.18",
          "form": "여자들은 연상많이들 만나니까",
          "original_form": "여자들은 연상많이들 만나니까",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016072.1.19",
          "form": "너는 결혼하고싶어??",
          "original_form": "너는 결혼하고싶어??",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016072.1.20",
          "form": "그 남자애들 부인 나이 궁굼하다",
          "original_form": "그 남자애들 부인 나이 궁굼하다",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016072.1.21",
          "form": "혼.자.살.거.에.요",
          "original_form": "혼.자.살.거.에.요",
          "speaker_id": "1",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016072.1.22",
          "form": "혼자가 최고아니냐고",
          "original_form": "혼자가 최고아니냐고",
          "speaker_id": "1",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016072.1.23",
          "form": "외로운거 모르겠음",
          "original_form": "외로운거 모르겠음",
          "speaker_id": "1",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016072.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100016072.1.25",
          "form": "근데 사진보니까 또래같던데ㅋㅋㅋㅋ",
          "original_form": "근데 사진보니까 또래같던데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100016072.1.26",
          "form": "나는.....하고는 싶은데 모르겠다^^",
          "original_form": "나는.....하고는 싶은데 모르겠다^^",
          "speaker_id": "2",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100016072.1.27",
          "form": "일단 내가 집순이라 뭐 남자를 만날일두 없구 마스크쓰니까 마기꾼들 많아서 뭐..^^",
          "original_form": "일단 내가 집순이라 뭐 남자를 만날일두 없구 마스크쓰니까 마기꾼들 많아서 뭐..^^",
          "speaker_id": "2",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100016072.1.28",
          "form": "ㅋㅋㅋ마기꾼 인정",
          "original_form": "ㅋㅋㅋ마기꾼 인정",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100016072.1.29",
          "form": "아니 전철타면 이쁜여자들 수두룩인데",
          "original_form": "아니 전철타면 이쁜여자들 수두룩인데",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100016072.1.30",
          "form": "잘생긴남자 진짜 못봐...",
          "original_form": "잘생긴남자 진짜 못봐...",
          "speaker_id": "1",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100016072.1.31",
          "form": "나는 눈높아서 연애도 못할듯",
          "original_form": "나는 눈높아서 연애도 못할듯",
          "speaker_id": "1",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100016072.1.32",
          "form": "못생기면 보기도싫어",
          "original_form": "못생기면 보기도싫어",
          "speaker_id": "1",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100016072.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ개웃겨ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ개웃겨ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100016072.1.34",
          "form": "진짜 마기꾼 넘많드라",
          "original_form": "진짜 마기꾼 넘많드라",
          "speaker_id": "2",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100016072.1.35",
          "form": "소개팅받고싶어도 마스크 벗을때 민망할듯...ㅎㅎ",
          "original_form": "소개팅받고싶어도 마스크 벗을때 민망할듯...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100016072.1.36",
          "form": "그냥 지금처럼 친구들이랑 노는게 좋다",
          "original_form": "그냥 지금처럼 친구들이랑 노는게 좋다",
