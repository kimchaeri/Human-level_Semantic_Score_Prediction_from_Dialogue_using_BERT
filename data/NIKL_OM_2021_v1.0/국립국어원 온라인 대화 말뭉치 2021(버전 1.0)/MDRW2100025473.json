      "utterance": [
        {
          "id": "MDRW2100025473.1.1",
          "form": "책 많이 읽으시나요",
          "original_form": "책 많이 읽으시나요",
          "speaker_id": "2",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100025473.1.2",
          "form": "일주일에 한권은  읽으려하고 있어요",
          "original_form": "일주일에 한권은  읽으려하고 있어요",
          "speaker_id": "1",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025473.1.3",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025473.1.4",
          "form": "전 한달에 두권 정도요",
          "original_form": "전 한달에 두권 정도요",
          "speaker_id": "2",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100025473.1.5",
          "form": "고전문학 한권 정도와",
          "original_form": "고전문학 한권 정도와",
          "speaker_id": "2",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100025473.1.6",
          "form": "이슈관련책 1권정도 읽으려고 생각중이에요",
          "original_form": "이슈관련책 1권정도 읽으려고 생각중이에요",
          "speaker_id": "2",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100025473.1.7",
          "form": "고전도 읽으시군요",
          "original_form": "고전도 읽으시군요",
          "speaker_id": "1",
          "time": "20210901 12:18"
        },
        {
          "id": "MDRW2100025473.1.8",
          "form": "요즘은 멋진 신세계 읽는중이에요",
          "original_form": "요즘은 멋진 신세계 읽는중이에요",
          "speaker_id": "2",
          "time": "20210901 12:19"
        },
        {
          "id": "MDRW2100025473.1.9",
          "form": "뭐읽고 계세요?",
          "original_form": "뭐읽고 계세요?",
          "speaker_id": "2",
          "time": "20210901 12:19"
        },
        {
          "id": "MDRW2100025473.1.10",
          "form": "전 살아남은 아이요",
          "original_form": "전 살아남은 아이요",
          "speaker_id": "1",
          "time": "20210901 12:19"
        },
        {
          "id": "MDRW2100025473.1.11",
          "form": "잼나나요?",
          "original_form": "잼나나요?",
          "speaker_id": "2",
          "time": "20210901 12:20"
        },
        {
          "id": "MDRW2100025473.1.12",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100025473.1.13",
          "form": "슬프고 화나요",
          "original_form": "슬프고 화나요",
          "speaker_id": "1",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100025473.1.14",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100025473.1.15",
          "form": "현실 고발 책인가요",
          "original_form": "현실 고발 책인가요",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025473.1.16",
          "form": "내얘기좀들어주세요 라고 끝나는데 너무 먹먹하네요",
          "original_form": "내얘기좀들어주세요 라고 끝나는데 너무 먹먹하네요",
          "speaker_id": "1",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100025473.1.17",
          "form": "아이고 안타깝네요",
          "original_form": "아이고 안타깝네요",
          "speaker_id": "2",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100025473.1.18",
          "form": "이번엔 힐링물로 읽어보시는건 어떨까요",
          "original_form": "이번엔 힐링물로 읽어보시는건 어떨까요",
          "speaker_id": "2",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100025473.1.19",
          "form": "아직 다 못읽어서ㅜㅜ",
          "original_form": "아직 다 못읽어서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100025473.1.20",
          "form": "책이 두꺼워요",
          "original_form": "책이 두꺼워요",
          "speaker_id": "1",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100025473.1.21",
          "form": "한번에 두권 읽어도 괜찮더라구요",
          "original_form": "한번에 두권 읽어도 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100025473.1.22",
          "form": "번갈아가며 읽으세요",
          "original_form": "번갈아가며 읽으세요",
          "speaker_id": "2",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100025473.1.23",
          "form": "먹먹하실때 기분좋아지는 책으로 맘달래세요",
          "original_form": "먹먹하실때 기분좋아지는 책으로 맘달래세요",
          "speaker_id": "2",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100025473.1.24",
          "form": "멀티가 ㅜㅜ 힘들어서..ㅜㅜ",
          "original_form": "멀티가 ㅜㅜ 힘들어서..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100025473.1.25",
          "form": "그럼 빨리 완독하세요!",
          "original_form": "그럼 빨리 완독하세요!",
          "speaker_id": "2",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100025473.1.26",
          "form": "속독으로 ㄱㄱ",
          "original_form": "속독으로 ㄱㄱ",
          "speaker_id": "2",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100025473.1.27",
          "form": "2부는 내용이 어렵더라구요",
          "original_form": "2부는 내용이 어렵더라구요",
          "speaker_id": "1",
          "time": "20210901 12:33"
        }
      ]
    }
  ]
