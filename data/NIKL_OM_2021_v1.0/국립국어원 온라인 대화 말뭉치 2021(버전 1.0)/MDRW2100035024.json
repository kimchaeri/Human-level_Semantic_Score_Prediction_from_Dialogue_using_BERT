      "utterance": [
        {
          "id": "MDRW2100035024.1.1",
          "form": "게임 무식자예요 ㅋㅋㅋㅋㅋ",
          "original_form": "게임 무식자예요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100035024.1.2",
          "form": "게임 잘하는 사람을 보면서 신기해하는 타입...ㅋㅋㅋㅋㅋ",
          "original_form": "게임 잘하는 사람을 보면서 신기해하는 타입...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100035024.1.3",
          "form": "악ㅋㅋㅋ",
          "original_form": "악ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100035024.1.4",
          "form": "저는테트리스좋아했는데 없어져서 이젠못해요흑흑",
          "original_form": "저는테트리스좋아했는데 없어져서 이젠못해요흑흑",
          "speaker_id": "1",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100035024.1.5",
          "form": "테트리스 좋아하셧구나",
          "original_form": "테트리스 좋아하셧구나",
          "speaker_id": "2",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100035024.1.6",
          "form": "전 어설프게 쌓다가 작대기 이상한데 꽂고 폭망하곤 햇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 어설프게 쌓다가 작대기 이상한데 꽂고 폭망하곤 햇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100035024.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 한방노리다가 작대기 못 꽂으면 게임오버...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 한방노리다가 작대기 못 꽂으면 게임오버...",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100035024.1.8",
          "form": "ㅋㅋㄱㄲㄱㅋㅜㅜ",
          "original_form": "ㅋㅋㄱㄲㄱㅋㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100035024.1.9",
          "form": "레알팜이라구들어보셨어요혹시?",
          "original_form": "레알팜이라구들어보셨어요혹시?",
          "speaker_id": "1",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100035024.1.10",
          "form": "농사짓는거 아니에요?",
          "original_form": "농사짓는거 아니에요?",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100035024.1.11",
          "form": "오 맞아요 직관적인 이름이네요\r\nㅋㅋㅋㅋㅋㅋㅋㅋ\r\n이거  뭐 농사잘지으면 실제 농작물을보내준다",
          "original_form": "오 맞아요 직관적인 이름이네요\r\nㅋㅋㅋㅋㅋㅋㅋㅋ\r\n이거  뭐 농사잘지으면 실제 농작물을보내준다",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100035024.1.12",
          "form": "이렇게홍보하는게임이라시작했는데",
          "original_form": "이렇게홍보하는게임이라시작했는데",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100035024.1.13",
          "form": "맞아요 광고본적잇어요",
          "original_form": "맞아요 광고본적잇어요",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100035024.1.14",
          "form": "진짜 받기힘들더라구요..",
          "original_form": "진짜 받기힘들더라구요..",
          "speaker_id": "1",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100035024.1.15",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100035024.1.16",
          "form": "광고만 보면 많이 주는 것 같앗는데 ㅋㅋㅋㅋ",
          "original_form": "광고만 보면 많이 주는 것 같앗는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100035024.1.17",
          "form": "그쵸 대부분 그 광고에 낚여서 오시는거같아요 \r\nㅋㅋㅋㄱㅋㅋㅋ저도마찬가지 ㅋㅋㄱㅋㅋ",
          "original_form": "그쵸 대부분 그 광고에 낚여서 오시는거같아요 \r\nㅋㅋㅋㄱㅋㅋㅋ저도마찬가지 ㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100035024.1.18",
          "form": "게임 자체는 재밋어요?",
          "original_form": "게임 자체는 재밋어요?",
          "speaker_id": "2",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100035024.1.19",
          "form": "진짜농사꾼마냥농사만지어요\r\nㅋㅋㅋㅋㅋㅋㅋㄱ\r\n상품타본적도 있긴 있는데\r\n티켓팅에버금가는 경쟁률..ㅋㅋㅋㅋ",
          "original_form": "진짜농사꾼마냥농사만지어요\r\nㅋㅋㅋㅋㅋㅋㅋㄱ\r\n상품타본적도 있긴 있는데\r\n티켓팅에버금가는 경쟁률..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100035024.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ치열하군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ치열하군요",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100035024.1.21",
