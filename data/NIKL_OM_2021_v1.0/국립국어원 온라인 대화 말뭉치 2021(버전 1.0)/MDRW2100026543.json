      "utterance": [
        {
          "id": "MDRW2100026543.1.1",
          "form": "티니님 안녕하세용~",
          "original_form": "티니님 안녕하세용~",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026543.1.2",
          "form": "사람은 왜 일을 해야하는가... 같은 생각",
          "original_form": "사람은 왜 일을 해야하는가... 같은 생각",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026543.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 인정...ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 인정...ㅠ",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026543.1.4",
          "form": "저는 정말로...구직을 하고 싶지만요",
          "original_form": "저는 정말로...구직을 하고 싶지만요",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026543.1.5",
          "form": "일하고 싶단 것도...일하고 싶지 않단 것도...",
          "original_form": "일하고 싶단 것도...일하고 싶지 않단 것도...",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026543.1.6",
          "form": "전부 숨김없는 진심이야",
          "original_form": "전부 숨김없는 진심이야",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026543.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅜㅜㅜㅜ마자마자",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅜㅜㅜㅜ마자마자",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026543.1.8",
          "form": "일을 하고 싶지만 일을 하고 싶지 않아..",
          "original_form": "일을 하고 싶지만 일을 하고 싶지 않아..",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026543.1.9",
          "form": "돈이 필요한 거지 일을 하고 싶은 건 아닙니다",
          "original_form": "돈이 필요한 거지 일을 하고 싶은 건 아닙니다",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026543.1.10",
          "form": "돈이 뭐라고 사람을...",
          "original_form": "돈이 뭐라고 사람을...",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026543.1.11",
          "form": "하지만 역시... 주5일을 일하고 이틀을 쉬는건? 불합리하달지?",
          "original_form": "하지만 역시... 주5일을 일하고 이틀을 쉬는건? 불합리하달지?",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026543.1.12",
          "form": "이틀을 쉬려고 5일을 일하는 것....",
          "original_form": "이틀을 쉬려고 5일을 일하는 것....",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026543.1.13",
          "form": "ㅠㅠㅠㅠㅠㅠㅠ인정합니다...",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠ인정합니다...",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026543.1.14",
          "form": "아니 근데 진짜 전에 알바할때....",
          "original_form": "아니 근데 진짜 전에 알바할때....",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026543.1.15",
          "form": "그 주말이 너무 소중한 거예요",
          "original_form": "그 주말이 너무 소중한 거예요",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026543.1.16",
          "form": "근데 일요일만 되면 하...다시 일을 나가야 하는 건가",
          "original_form": "근데 일요일만 되면 하...다시 일을 나가야 하는 건가",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026543.1.17",
          "form": "사람들 평생 이러고 사는 건가",
          "original_form": "사람들 평생 이러고 사는 건가",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026543.1.18",
          "form": "라는 생각에 취직하기 싫어졌던...",
          "original_form": "라는 생각에 취직하기 싫어졌던...",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026543.1.19",
          "form": "그래서 아직도 백순가바여...",
          "original_form": "그래서 아직도 백순가바여...",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026543.1.20",
          "form": "진짜 일요일 저녁의 괴로움은",
          "original_form": "진짜 일요일 저녁의 괴로움은",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026543.1.21",
          "form": "그게 아마 엔트로피일 거야",
          "original_form": "그게 아마 엔트로피일 거야",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026543.1.22",
          "form": "우주를 유지시키는...",
          "original_form": "우주를 유지시키는...",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026543.1.23",
          "form": "ㅋㅋ큐ㅠ큐큐ㅠㅠㅠㅠㅠㅠ아 이런 우주 차라리 멸망해버리라구요~!~!~",
          "original_form": "ㅋㅋ큐ㅠ큐큐ㅠㅠㅠㅠㅠㅠ아 이런 우주 차라리 멸망해버리라구요~!~!~",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026543.1.24",
          "form": "너무 싫어",
          "original_form": "너무 싫어",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026543.1.25",
          "form": "인간이 일을 해야만 유지되는 사회같은 거...",
          "original_form": "인간이 일을 해야만 유지되는 사회같은 거...",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026543.1.26",
          "form": "저 지금 완전 중2병 대사 치고 있는데...",
          "original_form": "저 지금 완전 중2병 대사 치고 있는데...",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026543.1.27",
          "form": "그런 콘텐츠가 아직 나오고 있는 건 어른들이 그걸 원하기 때문이 아닐지",
          "original_form": "그런 콘텐츠가 아직 나오고 있는 건 어른들이 그걸 원하기 때문이 아닐지",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026543.1.28",
          "form": "합리적인 생각",
          "original_form": "합리적인 생각",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026543.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ아 그러네....",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 그러네....",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026543.1.30",
          "form": "청소년이 지구 멸망시켜주길 바랐네.... 어른들이...",
          "original_form": "청소년이 지구 멸망시켜주길 바랐네.... 어른들이...",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026543.1.31",
          "form": "멸망조차 스스로 해내지못하는...",
          "original_form": "멸망조차 스스로 해내지못하는...",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026543.1.32",
          "form": "낡은 어른들...",
          "original_form": "낡은 어른들...",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026543.1.33",
          "form": "ㅋㅋ큐ㅠㅠ큐ㅜ쿠쿠....멸망할 용기로 솔직히 퇴사했다...",
          "original_form": "ㅋㅋ큐ㅠㅠ큐ㅜ쿠쿠....멸망할 용기로 솔직히 퇴사했다...",
          "speaker_id": "1",
