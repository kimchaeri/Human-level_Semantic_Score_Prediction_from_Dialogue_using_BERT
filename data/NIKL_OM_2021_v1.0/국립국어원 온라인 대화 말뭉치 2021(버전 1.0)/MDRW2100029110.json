      "utterance": [
        {
          "id": "MDRW2100029110.1.1",
          "form": "그런 것도 하는구만",
          "original_form": "그런 것도 하는구만",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029110.1.2",
          "form": "학교 행사 참여 잘 안 해서",
          "original_form": "학교 행사 참여 잘 안 해서",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029110.1.3",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029110.1.4",
          "form": "있는 줄도 잘 모르는",
          "original_form": "있는 줄도 잘 모르는",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029110.1.5",
          "form": "우리는 참여 안하면 참여도 깎아서...",
          "original_form": "우리는 참여 안하면 참여도 깎아서...",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029110.1.6",
          "form": "장학금에 영향 있어",
          "original_form": "장학금에 영향 있어",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029110.1.7",
          "form": "저런...",
          "original_form": "저런...",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029110.1.8",
          "form": "우리도 그런 거 있나",
          "original_form": "우리도 그런 거 있나",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029110.1.9",
          "form": "모르냐고",
          "original_form": "모르냐고",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029110.1.10",
          "form": "모르겠어 개총 걍 새내기 때나 가본 것 같은데",
          "original_form": "모르겠어 개총 걍 새내기 때나 가본 것 같은데",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029110.1.11",
          "form": "가면.... 뭐하나",
          "original_form": "가면.... 뭐하나",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029110.1.12",
          "form": "학과에 필요한 여러 안건에 대해 인준을 진행하고",
          "original_form": "학과에 필요한 여러 안건에 대해 인준을 진행하고",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029110.1.13",
          "form": "공지를 합니다",
          "original_form": "공지를 합니다",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029110.1.14",
          "form": "진짜 낯선데",
          "original_form": "진짜 낯선데",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029110.1.15",
          "form": "안 하는 거 아냐 우리",
          "original_form": "안 하는 거 아냐 우리",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029110.1.16",
          "form": "많은참여부탁드려요",
          "original_form": "많은참여부탁드려요",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029110.1.17",
          "form": "우리학교도아닌데",
          "original_form": "우리학교도아닌데",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029110.1.18",
          "form": "걍 그런 거 참여해달라는 얘기를",
          "original_form": "걍 그런 거 참여해달라는 얘기를",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029110.1.19",
          "form": "학생 수 50퍼 이상 참여해야 인준이 가능해요",
          "original_form": "학생 수 50퍼 이상 참여해야 인준이 가능해요",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029110.1.20",
          "form": "들은 적이",
          "original_form": "들은 적이",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029110.1.21",
          "form": "없는 것 같은데",
          "original_form": "없는 것 같은데",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029110.1.22",
          "form": "진짜냐",
          "original_form": "진짜냐",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029110.1.23",
          "form": "학생회일안하네",
          "original_form": "학생회일안하네",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029110.1.24",
          "form": "웃겨",
          "original_form": "웃겨",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029110.1.25",
          "form": "어차피 참여 안 하니까",
          "original_form": "어차피 참여 안 하니까",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029110.1.26",
          "form": "윈윈",
          "original_form": "윈윈",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029110.1.27",
          "form": "우리 과가 근데 좀 FM이기도 함",
          "original_form": "우리 과가 근데 좀 FM이기도 함",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029110.1.28",
          "form": "내가 궁금해서 에타에 쳐봤거든",
          "original_form": "내가 궁금해서 에타에 쳐봤거든",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029110.1.29",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029110.1.30",
          "form": "우리과... 게시판에",
          "original_form": "우리과... 게시판에",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029110.1.31",
          "form": "암것도 안 나와",
          "original_form": "암것도 안 나와",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029110.1.32",
          "form": "과 게시판도 있냐",
          "original_form": "과 게시판도 있냐",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029110.1.33",
          "form": "메이저네",
          "original_form": "메이저네",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029110.1.34",
          "form": "아까 장미칼 얘기 들었을 때 기분이 이러했을까",
          "original_form": "아까 장미칼 얘기 들었을 때 기분이 이러했을까",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029110.1.35",
          "form": "진짜 충격적이다,",
          "original_form": "진짜 충격적이다,",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029110.1.36",
          "form": "아오",
          "original_form": "아오",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029110.1.37",
          "form": "보통 공지는 카톡으로 하는데 카톡방에도 없나",
          "original_form": "보통 공지는 카톡으로 하는데 카톡방에도 없나",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029110.1.38",
          "form": "원래 과 게시판 정도는 다 있는 거 아니냐고",
          "original_form": "원래 과 게시판 정도는 다 있는 거 아니냐고",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029110.1.39",
          "form": "일부러그러는거냐고",
          "original_form": "일부러그러는거냐고",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029110.1.40",
          "form": "톡방에도 전혀 안 나와",
          "original_form": "톡방에도 전혀 안 나와",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029110.1.41",
          "form": "그냥 우리과는 없나본데",
          "original_form": "그냥 우리과는 없나본데",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029110.1.42",
          "form": "과 게시판 다 있는 거 아님????",
          "original_form": "과 게시판 다 있는 거 아님????",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029110.1.43",
          "form": "게시판은 진짜없음... 찐 소수과라",
          "original_form": "게시판은 진짜없음... 찐 소수과라",
          "speaker_id": "1",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029110.1.44",
          "form": "우리 완전 소수과나 연계전공도 다 게시판 있는데",
          "original_form": "우리 완전 소수과나 연계전공도 다 게시판 있는데",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029110.1.45",
          "form": "우린... 전혀 없어",
          "original_form": "우린... 전혀 없어",
          "speaker_id": "1",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029110.1.46",
          "form": "어차피 공지카톡방 다 있으니까",
          "original_form": "어차피 공지카톡방 다 있으니까",
          "speaker_id": "1",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029110.1.47",
          "form": "시디나 산디도 따로 없고 통틀어서 미대게시판은 있다",
          "original_form": "시디나 산디도 따로 없고 통틀어서 미대게시판은 있다",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029110.1.48",
          "form": "게시판 있어야... 잡담 하잖아",
          "original_form": "게시판 있어야... 잡담 하잖아",
          "speaker_id": "2",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029110.1.49",
          "form": "전공 얘기도 하고",
          "original_form": "전공 얘기도 하고",
          "speaker_id": "2",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029110.1.50",
          "form": "이럴수가",
          "original_form": "이럴수가",
          "speaker_id": "2",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029110.1.51",
          "form": "그런 건 그냥... 친한 사람들끼리 알아서 하지",
          "original_form": "그런 건 그냥... 친한 사람들끼리 알아서 하지",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029110.1.52",
          "form": "우리 진짜 별 게시판 다 있는데",
          "original_form": "우리 진짜 별 게시판 다 있는데",
          "speaker_id": "2",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029110.1.53",
          "form": "신기하네",
          "original_form": "신기하네",
          "speaker_id": "2",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029110.1.54",
          "form": "우리도 별 게시판이 다 있긴 하지만",
          "original_form": "우리도 별 게시판이 다 있긴 하지만",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029110.1.55",
          "form": "아무튼 과별로 있진 않음 대부분 미술대학~ 공과대학~ 이정도",
          "original_form": "아무튼 과별로 있진 않음 대부분 미술대학~ 공과대학~ 이정도",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029110.1.56",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "2",
