      "utterance": [
        {
          "id": "MDRW2100021803.1.1",
          "form": "여행조아하시나여",
          "original_form": "여행조아하시나여",
          "speaker_id": "1",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100021803.1.2",
          "form": "막상가면 재밌긴한데 이동이 싫어요 ㅋㅋㅋ",
          "original_form": "막상가면 재밌긴한데 이동이 싫어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021803.1.3",
          "form": "어디가보셨나요",
          "original_form": "어디가보셨나요",
          "speaker_id": "1",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100021803.1.4",
          "form": "해외는 사실 아시아랑 북미지역 밖에 못가봤구 국내는 거진 다 가본거 같아요 ㅋㅋ 아울릉도는 못가봤어요",
          "original_form": "해외는 사실 아시아랑 북미지역 밖에 못가봤구 국내는 거진 다 가본거 같아요 ㅋㅋ 아울릉도는 못가봤어요",
          "speaker_id": "2",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100021803.1.5",
          "form": "오저도 독도 가보고싶어요",
          "original_form": "오저도 독도 가보고싶어요",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100021803.1.6",
          "form": "울릉도 가보신분이 너무 좋았다고 하시더라구요 근데 2번실패했다가 세번째에나 가셨어요 ㅋㅋ 날씨가 도와줘야하는곳이니 ㅜ",
          "original_form": "울릉도 가보신분이 너무 좋았다고 하시더라구요 근데 2번실패했다가 세번째에나 가셨어요 ㅋㅋ 날씨가 도와줘야하는곳이니 ㅜ",
          "speaker_id": "2",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100021803.1.7",
          "form": "진짜 꼭 한번은 가봐야하는데",
          "original_form": "진짜 꼭 한번은 가봐야하는데",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100021803.1.8",
          "form": "국내 여행지 중 어디 좋으셨어요?",
          "original_form": "국내 여행지 중 어디 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100021803.1.9",
          "form": "역시 제주도죠ㅎㅎ",
          "original_form": "역시 제주도죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100021803.1.10",
          "form": "아무래도 딱 휴양지! 이런느낌인거 같아요 ㅋㅋ",
          "original_form": "아무래도 딱 휴양지! 이런느낌인거 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100021803.1.11",
          "form": "어느계절에 가보셨어요?",
          "original_form": "어느계절에 가보셨어요?",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100021803.1.12",
          "form": "여름이요ㅎㅎ 섭지코지넘좋아요",
          "original_form": "여름이요ㅎㅎ 섭지코지넘좋아요",
          "speaker_id": "1",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100021803.1.13",
          "form": "뭐드셨어요! 여행은 먹거리죠! ㅜㅜ",
          "original_form": "뭐드셨어요! 여행은 먹거리죠! ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100021803.1.14",
          "form": "역시 흑돼지죠!",
          "original_form": "역시 흑돼지죠!",
          "speaker_id": "1",
          "time": "20210829 14:29"
        }
      ]
