      "utterance": [
        {
          "id": "MDRW2100019935.1.1",
          "form": "아니요///!!",
          "original_form": "아니요///!!",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019935.1.2",
          "form": "고양이키우고싶어요!",
          "original_form": "고양이키우고싶어요!",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019935.1.3",
          "form": "고양이 너무 귀엽죠ㅠㅠ 제 친구가 고양이 키우는데 완전 개냥이에요!!",
          "original_form": "고양이 너무 귀엽죠ㅠㅠ 제 친구가 고양이 키우는데 완전 개냥이에요!!",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019935.1.4",
          "form": "저한테 부비부비하고 제 무릎에 누워서 자요ㅠㅠㅠ",
          "original_form": "저한테 부비부비하고 제 무릎에 누워서 자요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019935.1.5",
          "form": "헐헝 졸귀겠다 ㅠㅠㅠ",
          "original_form": "헐헝 졸귀겠다 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019935.1.6",
          "form": "미쳤다 ㅠㅠ",
          "original_form": "미쳤다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019935.1.7",
          "form": "대박이져ㅠㅠㅠ",
          "original_form": "대박이져ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100019935.1.8",
          "form": "너무 사랑스러유ㅓ요ㅠㅠㅠㅠ",
          "original_form": "너무 사랑스러유ㅓ요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019935.1.9",
          "form": "그리고 이름부르면 자기길 가다가도 저한테 와요ㅠㅠ 완전 천재고양이",
          "original_form": "그리고 이름부르면 자기길 가다가도 저한테 와요ㅠㅠ 완전 천재고양이",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019935.1.10",
          "form": "와 그정도면진짜 ㅠㅠㅠ",
          "original_form": "와 그정도면진짜 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019935.1.11",
          "form": "강아지에여 ㅠㅠ",
          "original_form": "강아지에여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019935.1.12",
          "form": "웃긴건 친구집에 강아지도 있는데 강아지는 불러도 안와요..",
          "original_form": "웃긴건 친구집에 강아지도 있는데 강아지는 불러도 안와요..",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019935.1.13",
          "form": "간식 들고 있을때만 아는척하고ㅋㅋㅋ",
          "original_form": "간식 들고 있을때만 아는척하고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019935.1.14",
          "form": "아 똑똑하다",
          "original_form": "아 똑똑하다",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019935.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019935.1.16",
          "form": "그치만 관종이라서",
          "original_form": "그치만 관종이라서",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019935.1.17",
          "form": "만지다가 나쁜 말인가요ㅠㅠ",
          "original_form": "만지다가 나쁜 말인가요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019935.1.18",
          "form": "고양이 만zi면 자기 만zi라고 들이대요ㅋㅋㅋ",
          "original_form": "고양이 만zi면 자기 만zi라고 들이대요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019935.1.19",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019935.1.20",
          "form": "미렸다진짜 ㅠㅠㅠ",
          "original_form": "미렸다진짜 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019935.1.21",
          "form": "그쵸ㅠㅠ 그래서 그 친구집 가면 완전 힐링도ㅑ요ㅠㅠ",
          "original_form": "그쵸ㅠㅠ 그래서 그 친구집 가면 완전 힐링도ㅑ요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100019935.1.22",
          "form": "진짜로 너무 부러워요 ㅠㅠ",
          "original_form": "진짜로 너무 부러워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100019935.1.23",
          "form": "근데 또 키우는거랑 보는거랑 달라서 막상 제가 키우는건 어러울거같아요ㅠㅠ",
          "original_form": "근데 또 키우는거랑 보는거랑 달라서 막상 제가 키우는건 어러울거같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100019935.1.24",
          "form": "하긴 막상키우는거랑은또",
          "original_form": "하긴 막상키우는거랑은또",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100019935.1.25",
          "form": "다르다보니까 ㅠㅠㅠ",
          "original_form": "다르다보니까 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100019935.1.26",
          "form": "돈도 많이들고 관리도 잘해줘야하고ㅠㅠ 그래서 친구집 가서 보는게 저랑은 맞는거 같아요ㅋㅋ",
          "original_form": "돈도 많이들고 관리도 잘해줘야하고ㅠㅠ 그래서 친구집 가서 보는게 저랑은 맞는거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100019935.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100019935.1.28",
          "form": "저두그래요",
          "original_form": "저두그래요",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100019935.1.29",
          "form": "길냥이들보고 ㅠㅠ",
          "original_form": "길냥이들보고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100019935.1.30",
          "form": "요즘은 길냥이들도 사람손타서 애교 많은 애들도 많더라구요ㅋㅋㅋㅋ",
          "original_form": "요즘은 길냥이들도 사람손타서 애교 많은 애들도 많더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100019935.1.31",
          "form": "집근처에 문구사가 있는데 문구사 주인분께서 길냥이들 밥주고 그러니까 애들이 사람보면 엄청 애교를 부려요ㅋㅋㅋ",
          "original_form": "집근처에 문구사가 있는데 문구사 주인분께서 길냥이들 밥주고 그러니까 애들이 사람보면 엄청 애교를 부려요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:08"
        }
      ]
