      "utterance": [
        {
          "id": "MDRW2100014056.1.1",
          "form": "그럼요 안그럼 맨날 요리 몇개 해야해요",
          "original_form": "그럼요 안그럼 맨날 요리 몇개 해야해요",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014056.1.2",
          "form": "여행가서도 누구 하나 틀리면 음식점 찾기 어려워요",
          "original_form": "여행가서도 누구 하나 틀리면 음식점 찾기 어려워요",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014056.1.3",
          "form": "입맛은 다 맞으세요?",
          "original_form": "입맛은 다 맞으세요?",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014056.1.4",
          "form": "네 저희 다행히 같아요",
          "original_form": "네 저희 다행히 같아요",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014056.1.5",
          "form": "애들이 다잘먹어요 ㅎㅎ",
          "original_form": "애들이 다잘먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014056.1.6",
          "form": "마라탕도 먹고",
          "original_form": "마라탕도 먹고",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014056.1.7",
          "form": "입맛이 세계적입니다 ㅋㅋ",
          "original_form": "입맛이 세계적입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014056.1.8",
          "form": "어떠세요?",
          "original_form": "어떠세요?",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014056.1.9",
          "form": "와 해외여행도 걱정 없으시겠어요",
          "original_form": "와 해외여행도 걱정 없으시겠어요",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014056.1.10",
          "form": "저흰 안 맞는 편이에요ㅠ",
          "original_form": "저흰 안 맞는 편이에요ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014056.1.11",
          "form": "대신 여러군데를 가죠ㅋㅋ",
          "original_form": "대신 여러군데를 가죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014056.1.12",
          "form": "네 ㅎㅎ가서 다 도전해서 먹었어요",
          "original_form": "네 ㅎㅎ가서 다 도전해서 먹었어요",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014056.1.13",
          "form": "어머 그럼 맘껏 못즐기는데",
          "original_form": "어머 그럼 맘껏 못즐기는데",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014056.1.14",
          "form": "해외에 가서 고수도 잘 드시려나ㅋㅋ",
          "original_form": "해외에 가서 고수도 잘 드시려나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014056.1.15",
          "form": "전 고수는 절대적으로 저만 먹을 줄 알아서",
          "original_form": "전 고수는 절대적으로 저만 먹을 줄 알아서",
          "speaker_id": "1",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014056.1.16",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014056.1.17",
          "form": "저희는 다 먹어요",
          "original_form": "저희는 다 먹어요",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014056.1.18",
          "form": "저랑딸만 조금먹고",
          "original_form": "저랑딸만 조금먹고",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014056.1.19",
          "form": "다 팍팍요 ㅋㅋ",
          "original_form": "다 팍팍요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014056.1.20",
          "form": "ㅋㅋ 잘 드시는 것도 복이죠ㅋㅋ",
          "original_form": "ㅋㅋ 잘 드시는 것도 복이죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014056.1.21",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014056.1.22",
          "form": "맛집이 많으시면 좋죠ㅋㅋ",
          "original_form": "맛집이 많으시면 좋죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014056.1.23",
          "form": "저녁 순개세요?",
          "original_form": "저녁 순개세요?",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014056.1.24",
          "form": "순대?",
          "original_form": "순대?",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014056.1.25",
          "form": "근데 반대로 뭐 먹으면 불평부터 하는 사람",
          "original_form": "근데 반대로 뭐 먹으면 불평부터 하는 사람",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014056.1.26",
          "form": "싫어요ㅋㅋ",
          "original_form": "싫어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014056.1.27",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014056.1.28",
          "form": "그거 곤란해요",
          "original_form": "그거 곤란해요",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014056.1.29",
          "form": "좀 더 생각중이요ㅋㅋ",
          "original_form": "좀 더 생각중이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014056.1.30",
          "form": "진짜 입맛이 뚝이요",
          "original_form": "진짜 입맛이 뚝이요",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014056.1.31",
          "form": "입맛이 같이 달아남ㅋ",
          "original_form": "입맛이 같이 달아남ㅋ",
          "speaker_id": "1",
