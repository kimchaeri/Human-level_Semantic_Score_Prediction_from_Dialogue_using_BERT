      "utterance": [
        {
          "id": "MDRW2100018521.1.1",
          "form": "아침 챙겨드시나요?",
          "original_form": "아침 챙겨드시나요?",
          "speaker_id": "2",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100018521.1.2",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100018521.1.3",
          "form": "아침 드셨어요?",
          "original_form": "아침 드셨어요?",
          "speaker_id": "1",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100018521.1.4",
          "form": "네! 요즘은 쥬스 갈아 마시고 있어요",
          "original_form": "네! 요즘은 쥬스 갈아 마시고 있어요",
          "speaker_id": "2",
          "time": "20210827 07:41"
        },
        {
          "id": "MDRW2100018521.1.5",
          "form": "왠지 건강한 느낌인데요",
          "original_form": "왠지 건강한 느낌인데요",
          "speaker_id": "1",
          "time": "20210827 07:42"
        },
        {
          "id": "MDRW2100018521.1.6",
          "form": "다이어트 한다고 먹는데 부담이 없어 좋더라구요!",
          "original_form": "다이어트 한다고 먹는데 부담이 없어 좋더라구요!",
          "speaker_id": "2",
          "time": "20210827 07:42"
        },
        {
          "id": "MDRW2100018521.1.7",
          "form": "전 쥬서기 없어서 생각만해요 ㅋㅋ",
          "original_form": "전 쥬서기 없어서 생각만해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:42"
        },
        {
          "id": "MDRW2100018521.1.8",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210827 07:42"
        },
        {
          "id": "MDRW2100018521.1.9",
          "form": "다이어트면 점심은 어찌 드세요",
          "original_form": "다이어트면 점심은 어찌 드세요",
          "speaker_id": "1",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100018521.1.10",
          "form": "아침은 주로 어떤거 드세요?",
          "original_form": "아침은 주로 어떤거 드세요?",
          "speaker_id": "2",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100018521.1.11",
          "form": "점심은 자유식하고 있어요! 조금 먹으면 배가 너무 고파서 힘들더라구요ㅜㅠ",
          "original_form": "점심은 자유식하고 있어요! 조금 먹으면 배가 너무 고파서 힘들더라구요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100018521.1.12",
          "form": "전 한식으로요 아침은",
          "original_form": "전 한식으로요 아침은",
          "speaker_id": "1",
          "time": "20210827 07:45"
        },
        {
          "id": "MDRW2100018521.1.13",
          "form": "그래야죠",
          "original_form": "그래야죠",
          "speaker_id": "1",
          "time": "20210827 07:45"
        },
        {
          "id": "MDRW2100018521.1.14",
          "form": "안그럼 넘 배고파 안돼요",
          "original_form": "안그럼 넘 배고파 안돼요",
          "speaker_id": "1",
          "time": "20210827 07:45"
        },
        {
          "id": "MDRW2100018521.1.15",
          "form": "안먹음 금방찌고",
          "original_form": "안먹음 금방찌고",
          "speaker_id": "1",
          "time": "20210827 07:45"
        },
        {
          "id": "MDRW2100018521.1.16",
          "form": "점심은 꼭 드세요 맛난거로",
          "original_form": "점심은 꼭 드세요 맛난거로",
          "speaker_id": "1",
          "time": "20210827 07:45"
        },
        {
          "id": "MDRW2100018521.1.17",
          "form": "아침이 젤고민요 뭘할지 오늘 주먹밥요",
          "original_form": "아침이 젤고민요 뭘할지 오늘 주먹밥요",
          "speaker_id": "1",
          "time": "20210827 07:46"
        },
        {
          "id": "MDRW2100018521.1.18",
          "form": "맞아요 아침은 챙겨먹는게 쉽지않아요ㅠㅜ",
          "original_form": "맞아요 아침은 챙겨먹는게 쉽지않아요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 07:46"
        },
        {
          "id": "MDRW2100018521.1.19",
          "form": "그쵸 안먹음 찌고..ㅋㅋ 저녁을 적게 먹으려하는데 쉽지않네요",
          "original_form": "그쵸 안먹음 찌고..ㅋㅋ 저녁을 적게 먹으려하는데 쉽지않네요",
          "speaker_id": "2",
          "time": "20210827 07:47"
        },
        {
          "id": "MDRW2100018521.1.20",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:48"
        },
        {
          "id": "MDRW2100018521.1.21",
          "form": "저녁에 맛난게 젤많아요",
          "original_form": "저녁에 맛난게 젤많아요",
          "speaker_id": "1",
          "time": "20210827 07:48"
        },
        {
          "id": "MDRW2100018521.1.22",
          "form": "그러니 자재 힘들죠",
          "original_form": "그러니 자재 힘들죠",
          "speaker_id": "1",
          "time": "20210827 07:49"
        },
        {
          "id": "MDRW2100018521.1.23",
          "form": "맞아요ㅋㅋ 배달도 잘되서 더 그런거같아요",
          "original_form": "맞아요ㅋㅋ 배달도 잘되서 더 그런거같아요",
          "speaker_id": "2",
          "time": "20210827 07:49"
        },
        {
          "id": "MDRW2100018521.1.24",
          "form": "맞아요 배달이 잘되죠 낮에 안팔던게 밤에 팔죠 ㅎㅎ",
          "original_form": "맞아요 배달이 잘되죠 낮에 안팔던게 밤에 팔죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 07:51"
        },
        {
          "id": "MDRW2100018521.1.25",
          "form": "네ㅎㅎ 요즘 닭발이 그리 맛있네요",
          "original_form": "네ㅎㅎ 요즘 닭발이 그리 맛있네요",
          "speaker_id": "2",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018521.1.26",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018521.1.27",
          "form": "근데 넘 매워서 좀 덜 매움 좋겠어요",
          "original_form": "근데 넘 매워서 좀 덜 매움 좋겠어요",
          "speaker_id": "1",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018521.1.28",
          "form": "닭발 좋아하시나봐요!!",
          "original_form": "닭발 좋아하시나봐요!!",
          "speaker_id": "2",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018521.1.29",
          "form": "맞아요ㅠ 그래도 순한맛인곳도 꽤 생기더라구요",
          "original_form": "맞아요ㅠ 그래도 순한맛인곳도 꽤 생기더라구요",
          "speaker_id": "2",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018521.1.30",
          "form": "그래야 해요",
          "original_form": "그래야 해요",
          "speaker_id": "1",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018521.1.31",
          "form": "먹지도 못하게 맵고 ㅜㅜ좋아하는데",
          "original_form": "먹지도 못하게 맵고 ㅜㅜ좋아하는데",
          "speaker_id": "1",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018521.1.32",
          "form": "울며 먹어요 ㅋㅋ",
          "original_form": "울며 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018521.1.33",
          "form": "저도요ㅋㅋ 안먹을수는 없어요ㅋㅋ",
          "original_form": "저도요ㅋㅋ 안먹을수는 없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:56"
        },
        {
          "id": "MDRW2100018521.1.34",
          "form": "그럼요 맛난걸 먹어야죠",
          "original_form": "그럼요 맛난걸 먹어야죠",
          "speaker_id": "1",
          "time": "20210827 07:57"
        }
      ]
    }
