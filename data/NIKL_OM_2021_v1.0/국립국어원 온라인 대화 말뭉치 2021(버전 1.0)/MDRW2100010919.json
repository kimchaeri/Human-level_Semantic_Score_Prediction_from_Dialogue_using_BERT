      "utterance": [
        {
          "id": "MDRW2100010919.1.1",
          "form": "떡볶이 맛집 알아?",
          "original_form": "떡볶이 맛집 알아?",
          "speaker_id": "2",
          "time": "20210823 22:58"
        },
        {
          "id": "MDRW2100010919.1.2",
          "form": "나 모르는데",
          "original_form": "나 모르는데",
          "speaker_id": "1",
          "time": "20210823 22:58"
        },
        {
          "id": "MDRW2100010919.1.3",
          "form": "배떡 로제 먹고싶어",
          "original_form": "배떡 로제 먹고싶어",
          "speaker_id": "1",
          "time": "20210823 22:58"
        },
        {
          "id": "MDRW2100010919.1.4",
          "form": "나도 먹고 싶어",
          "original_form": "나도 먹고 싶어",
          "speaker_id": "2",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100010919.1.5",
          "form": "배떡 로제 너무 맛있어",
          "original_form": "배떡 로제 너무 맛있어",
          "speaker_id": "2",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100010919.1.6",
          "form": "먹어봤어?",
          "original_form": "먹어봤어?",
          "speaker_id": "1",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100010919.1.7",
          "form": "무슨 맛이야?",
          "original_form": "무슨 맛이야?",
          "speaker_id": "1",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100010919.1.8",
          "form": "분모자가 진짜 맛있는 맛?",
          "original_form": "분모자가 진짜 맛있는 맛?",
          "speaker_id": "2",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100010919.1.9",
          "form": "중당 넣어 먹어도 너무 맛있는 맛",
          "original_form": "중당 넣어 먹어도 너무 맛있는 맛",
          "speaker_id": "2",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100010919.1.10",
          "form": "오 분모자 맛있겠다",
          "original_form": "오 분모자 맛있겠다",
          "speaker_id": "1",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100010919.1.11",
          "form": "로제 분모자 좋다",
          "original_form": "로제 분모자 좋다",
          "speaker_id": "1",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100010919.1.12",
          "form": "분모자는 비싸?",
          "original_form": "분모자는 비싸?",
          "speaker_id": "1",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100010919.1.13",
          "form": "분모자 추가는 조금 비싸",
          "original_form": "분모자 추가는 조금 비싸",
          "speaker_id": "2",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100010919.1.14",
          "form": "3000원",
          "original_form": "3000원",
          "speaker_id": "2",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100010919.1.15",
          "form": "근데 짱맛있어",
          "original_form": "근데 짱맛있어",
          "speaker_id": "2",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100010919.1.16",
          "form": "아하 그러면",
          "original_form": "아하 그러면",
          "speaker_id": "1",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100010919.1.17",
          "form": "분모자를 직접 사서 넣는거는?",
          "original_form": "분모자를 직접 사서 넣는거는?",
          "speaker_id": "1",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100010919.1.18",
          "form": "엥 이게 뭐야",
          "original_form": "엥 이게 뭐야",
          "speaker_id": "2",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100010919.1.19",
          "form": "분모자는 근데",
          "original_form": "분모자는 근데",
          "speaker_id": "2",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100010919.1.20",
          "form": "비싸",
          "original_form": "비싸",
          "speaker_id": "2",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100010919.1.21",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "1",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100010919.1.22",
          "form": "나는 요즘",
          "original_form": "나는 요즘",
          "speaker_id": "1",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100010919.1.23",
          "form": "버블티먹고싶어",
          "original_form": "버블티먹고싶어",
          "speaker_id": "1",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100010919.1.24",
          "form": "우리 학교 앞에 버블티 집 있어",
          "original_form": "우리 학교 앞에 버블티 집 있어",
          "speaker_id": "2",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100010919.1.25",
          "form": "버블티 쿠폰 다 썼어?",
          "original_form": "버블티 쿠폰 다 썼어?",
          "speaker_id": "1",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100010919.1.26",
          "form": "웅 다썼어",
          "original_form": "웅 다썼어",
          "speaker_id": "2",
          "time": "20210823 23:10"
        },
        {
