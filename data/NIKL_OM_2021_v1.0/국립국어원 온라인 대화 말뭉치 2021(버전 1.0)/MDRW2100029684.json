      "utterance": [
        {
          "id": "MDRW2100029684.1.1",
          "form": "코로나라 애들이 학교 못가서",
          "original_form": "코로나라 애들이 학교 못가서",
          "speaker_id": "1",
          "time": "20210903 11:39"
        },
        {
          "id": "MDRW2100029684.1.2",
          "form": "엄청 정신없으시겠어요ㅠ",
          "original_form": "엄청 정신없으시겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029684.1.3",
          "form": "네 대구는 매일가는데\n어제오늘은 교사2차접종이라서 집에서수업해요",
          "original_form": "네 대구는 매일가는데\n어제오늘은 교사2차접종이라서 집에서수업해요",
          "speaker_id": "2",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029684.1.4",
          "form": "먹이는게 젤힘든거같아요",
          "original_form": "먹이는게 젤힘든거같아요",
          "speaker_id": "2",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029684.1.5",
          "form": "아하ㅜㅠ 아이들이 입이 짧은가요?",
          "original_form": "아하ㅜㅠ 아이들이 입이 짧은가요?",
          "speaker_id": "1",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029684.1.6",
          "form": "육아만큼 힘든게 없는 것 같아요ㅠ",
          "original_form": "육아만큼 힘든게 없는 것 같아요ㅠ",
          "speaker_id": "1",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029684.1.7",
          "form": "잘먹긴하는데..음식하는게 힘들어서요 ㅠ",
          "original_form": "잘먹긴하는데..음식하는게 힘들어서요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100029684.1.8",
          "form": "메뉴 정하는것부터 쉽지 않은것같아요,,,,",
          "original_form": "메뉴 정하는것부터 쉽지 않은것같아요,,,,",
          "speaker_id": "1",
          "time": "20210903 11:49"
        },
        {
          "id": "MDRW2100029684.1.9",
          "form": "자취할 때 맨날 오늘 점심 뭐먹지 저녁뭐먹지",
          "original_form": "자취할 때 맨날 오늘 점심 뭐먹지 저녁뭐먹지",
          "speaker_id": "1",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100029684.1.10",
          "form": "이런생각만 했어요ㅋㅋㅋㅋㅋ...ㅠㅠㅠㅜ",
          "original_form": "이런생각만 했어요ㅋㅋㅋㅋㅋ...ㅠㅠㅠㅜ",
          "speaker_id": "1",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100029684.1.11",
          "form": "맞아요 전날밤부터 낼은뭐먹지 그생각만요 ㅠ",
          "original_form": "맞아요 전날밤부터 낼은뭐먹지 그생각만요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100029684.1.12",
          "form": "먹고사는게 힘든거같아요ㅠㅋ",
          "original_form": "먹고사는게 힘든거같아요ㅠㅋ",
          "speaker_id": "2",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100029684.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100029684.1.14",
          "form": "다른 집안일도 힘드시죠",
          "original_form": "다른 집안일도 힘드시죠",
          "speaker_id": "1",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100029684.1.15",
          "form": "아이있으면",
          "original_form": "아이있으면",
          "speaker_id": "1",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100029684.1.16",
          "form": "네 딱히 하는거없이 정신없는듯해요",
          "original_form": "네 딱히 하는거없이 정신없는듯해요",
          "speaker_id": "2",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100029684.1.17",
          "form": "결혼하셨어요?",
          "original_form": "결혼하셨어요?",
          "speaker_id": "2",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100029684.1.18",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "1",
          "time": "20210903 11:56"
        },
        {
          "id": "MDRW2100029684.1.19",
          "form": "결혼은 안했습니닷",
          "original_form": "결혼은 안했습니닷",
          "speaker_id": "1",
          "time": "20210903 11:56"
        },
        {
          "id": "MDRW2100029684.1.20",
          "form": "아 지금이 가장좋으실땝니다 즐기세요^^",
          "original_form": "아 지금이 가장좋으실땝니다 즐기세요^^",
          "speaker_id": "2",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100029684.1.21",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:59"
        },
        {
          "id": "MDRW2100029684.1.22",
          "form": "어린 동생이 있는데,",
          "original_form": "어린 동생이 있는데,",
          "speaker_id": "1",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100029684.1.23",
          "form": "어리나요?",
          "original_form": "어리나요?",
          "speaker_id": "2",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100029684.1.24",
          "form": "동생을 보면 뭔가 결혼하고 애낳는게 너무 무서워요ㅋㅋ",
          "original_form": "동생을 보면 뭔가 결혼하고 애낳는게 너무 무서워요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100029684.1.25",
          "form": "네 지금은 중1입니닷",
          "original_form": "네 지금은 중1입니닷",
          "speaker_id": "1",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100029684.1.26",
          "form": "너무 말을 안들어서.....ㅎ",
          "original_form": "너무 말을 안들어서.....ㅎ",
          "speaker_id": "1",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100029684.1.27",
          "form": "막둥입니다ㅎㅎ",
          "original_form": "막둥입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 12:01"
        },
        {
          "id": "MDRW2100029684.1.28",
          "form": "아 ㅎㅎ 동생이있어서 그런생각이들만하네요",
          "original_form": "아 ㅎㅎ 동생이있어서 그런생각이들만하네요",
          "speaker_id": "2",
          "time": "20210903 12:01"
        },
        {
          "id": "MDRW2100029684.1.29",
          "form": "한참 사춘기라서 더그렇겠어요 ㅋ",
          "original_form": "한참 사춘기라서 더그렇겠어요 ㅋ",
          "speaker_id": "2",
