      "utterance": [
        {
          "id": "MDRW2100012968.1.1",
          "form": "전 어제 산 고등어오면 저녁에 고등어김치찜 해먹으려고요",
          "original_form": "전 어제 산 고등어오면 저녁에 고등어김치찜 해먹으려고요",
          "speaker_id": "1",
          "time": "20210825 09:39"
        },
        {
          "id": "MDRW2100012968.1.2",
          "form": "근래 뭐 사셨어요?",
          "original_form": "근래 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210825 09:39"
        },
        {
          "id": "MDRW2100012968.1.3",
          "form": "와우와우",
          "original_form": "와우와우{emoji:♡}",
          "speaker_id": "2",
          "time": "20210825 09:39"
        },
        {
          "id": "MDRW2100012968.1.4",
          "form": "저는 클렌징 오일요ㅋㅋ",
          "original_form": "저는 클렌징 오일요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:39"
        },
        {
          "id": "MDRW2100012968.1.5",
          "form": "중고거래 해보신적 있나요?",
          "original_form": "중고거래 해보신적 있나요?",
          "speaker_id": "2",
          "time": "20210825 09:40"
        },
        {
          "id": "MDRW2100012968.1.6",
          "form": "당근?ㅋㅋㅋ",
          "original_form": "당근?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:40"
        },
        {
          "id": "MDRW2100012968.1.7",
          "form": "당근마켓해보셨어요?",
          "original_form": "당근마켓해보셨어요?",
          "speaker_id": "1",
          "time": "20210825 09:42"
        },
        {
          "id": "MDRW2100012968.1.8",
          "form": "네ㅋㅋ저도 당근 이용해봤죠",
          "original_form": "네ㅋㅋ저도 당근 이용해봤죠",
          "speaker_id": "2",
          "time": "20210825 09:42"
        },
        {
          "id": "MDRW2100012968.1.9",
          "form": "구매랑 판매 다 해보셨나요?",
          "original_form": "구매랑 판매 다 해보셨나요?",
          "speaker_id": "2",
          "time": "20210825 09:43"
        },
        {
          "id": "MDRW2100012968.1.10",
          "form": "네 ㅋㅋㅋ당근 진짜 신세계 ㅎㅎ",
          "original_form": "네 ㅋㅋㅋ당근 진짜 신세계 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 09:44"
        },
        {
          "id": "MDRW2100012968.1.11",
          "form": "둘다 해보셨어요?",
          "original_form": "둘다 해보셨어요?",
          "speaker_id": "1",
          "time": "20210825 09:44"
        },
        {
          "id": "MDRW2100012968.1.12",
          "form": "넹ㅋㅋ 둘다 해봤어요ㅋㅋ",
          "original_form": "넹ㅋㅋ 둘다 해봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:44"
        },
        {
          "id": "MDRW2100012968.1.13",
          "form": "기억에 남는 분 계신가요?ㅋㅋ",
          "original_form": "기억에 남는 분 계신가요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:45"
        },
        {
          "id": "MDRW2100012968.1.14",
          "form": "최근 기프티콘 팔았는데 거래속도가...대화 인사하자마자 총알입금인분있었어요",
          "original_form": "최근 기프티콘 팔았는데 거래속도가...대화 인사하자마자 총알입금인분있었어요",
          "speaker_id": "1",
          "time": "20210825 09:46"
        },
        {
          "id": "MDRW2100012968.1.15",
          "form": "name2님은 어떤분 기억에 남으셨어요?",
          "original_form": "&name2&님은 어떤분 기억에 남으셨어요?",
          "speaker_id": "1",
          "time": "20210825 09:46"
        },
        {
          "id": "MDRW2100012968.1.16",
          "form": "와 그런분 너무 좋죠",
          "original_form": "와 그런분 너무 좋죠{emoji:♡}",
          "speaker_id": "2",
          "time": "20210825 09:47"
        },
        {
          "id": "MDRW2100012968.1.17",
          "form": "제꺼 다 사가셨음 좋겠음ㅋㅋㅋ",
          "original_form": "제꺼 다 사가셨음 좋겠음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:47"
        },
        {
          "id": "MDRW2100012968.1.18",
          "form": "진짜 정신없이 거래했어요",
          "original_form": "진짜 정신없이 거래했어요",
          "speaker_id": "1",
          "time": "20210825 09:48"
        },
        {
          "id": "MDRW2100012968.1.19",
          "form": "저는 새신발을 판적이 있었는데요",
          "original_form": "저는 새신발을 판적이 있었는데요",
          "speaker_id": "2",
          "time": "20210825 09:48"
        },
        {
          "id": "MDRW2100012968.1.20",
          "form": "새신발이다보니 박스까지 \r\n들고 나갔었거든요ㅋㅋ",
          "original_form": "새신발이다보니 박스까지 \r\n들고 나갔었거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100012968.1.21",
          "form": "땅바닥에 아무것도 깔지 않고\r\n막 신어보시던 아주머니",
          "original_form": "땅바닥에 아무것도 깔지 않고\r\n막 신어보시던 아주머니",
          "speaker_id": "2",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100012968.1.22",
          "form": "오오 박스까지면 진짜 새거인게....",
          "original_form": "오오 박스까지면 진짜 새거인게....",
          "speaker_id": "1",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100012968.1.23",
          "form": "아직도 잊혀지지가 않네요ㅋㅋ",
          "original_form": "아직도 잊혀지지가 않네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100012968.1.24",
          "form": "와 충격이네요",
          "original_form": "와 충격이네요",
          "speaker_id": "1",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100012968.1.25",
          "form": "뭐 말리거나 말씀 드릴 틈도 없이",
          "original_form": "뭐 말리거나 말씀 드릴 틈도 없이",
          "speaker_id": "2",
          "time": "20210825 09:50"
        },
        {
          "id": "MDRW2100012968.1.26",
          "form": "사고 신었다고 해주세요",
          "original_form": "사고 신었다고 해주세요",
          "speaker_id": "1",
          "time": "20210825 09:50"
        },
        {
          "id": "MDRW2100012968.1.27",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "1",
          "time": "20210825 09:50"
        },
        {
          "id": "MDRW2100012968.1.28",
          "form": "꺼내 신어서 당황했는데",
          "original_form": "꺼내 신어서 당황했는데",
          "speaker_id": "2",
          "time": "20210825 09:50"
        },
        {
          "id": "MDRW2100012968.1.29",
          "form": "다행히도 사가셨어요 ㅜㅜㅋㅋ",
