      "utterance": [
        {
          "id": "MDRW2100027324.1.1",
          "form": "백신맞으셨나요?",
          "original_form": "백신맞으셨나요?",
          "speaker_id": "1",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027324.1.2",
          "form": "아니요:)",
          "original_form": "아니요:)",
          "speaker_id": "2",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027324.1.3",
          "form": "접종 하셨나요?",
          "original_form": "접종 하셨나요?",
          "speaker_id": "2",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027324.1.4",
          "form": "네 약 보름전에 백신맞았어요",
          "original_form": "네 약 보름전에 백신맞았어요",
          "speaker_id": "1",
          "time": "20210902 11:47"
        },
        {
          "id": "MDRW2100027324.1.5",
          "form": "언제 맞으시나요? 예정이 있나요?",
          "original_form": "언제 맞으시나요? 예정이 있나요?",
          "speaker_id": "1",
          "time": "20210902 11:47"
        },
        {
          "id": "MDRW2100027324.1.6",
          "form": "1차 접종하신건가요?",
          "original_form": "1차 접종하신건가요?",
          "speaker_id": "2",
          "time": "20210902 11:48"
        },
        {
          "id": "MDRW2100027324.1.7",
          "form": "저는 아직 신청을 안해서요ㅠㅠ\n예정은 없네요ㅠ",
          "original_form": "저는 아직 신청을 안해서요ㅠㅠ\n예정은 없네요ㅠ",
          "speaker_id": "2",
          "time": "20210902 11:49"
        },
        {
          "id": "MDRW2100027324.1.8",
          "form": "네 모더나로 잔여백신으로 맞았어요^^",
          "original_form": "네 모더나로 잔여백신으로 맞았어요^^",
          "speaker_id": "1",
          "time": "20210902 11:49"
        },
        {
          "id": "MDRW2100027324.1.9",
          "form": "잔여백신의 운이 따르길바래요!",
          "original_form": "잔여백신의 운이 따르길바래요!",
          "speaker_id": "1",
          "time": "20210902 11:49"
        },
        {
          "id": "MDRW2100027324.1.10",
          "form": "백신 부작용 이야기도 많지요",
          "original_form": "백신 부작용 이야기도 많지요",
          "speaker_id": "1",
          "time": "20210902 11:49"
        },
        {
          "id": "MDRW2100027324.1.11",
          "form": "모더나로 맞으셨군용",
          "original_form": "모더나로 맞으셨군용",
          "speaker_id": "2",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100027324.1.12",
          "form": "아이들이 있어서 \n남편 회사 스케쥴하고도\n맞춰야되고 하다보니\n아직이네요",
          "original_form": "아이들이 있어서 \n남편 회사 스케쥴하고도\n맞춰야되고 하다보니\n아직이네요",
          "speaker_id": "2",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100027324.1.13",
          "form": "잔여백신은 광클이라 되는거 아무거나 맞았어요",
          "original_form": "잔여백신은 광클이라 되는거 아무거나 맞았어요",
          "speaker_id": "1",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100027324.1.14",
          "form": "부작용때문에 여유두시는 것이지요?",
          "original_form": "부작용때문에 여유두시는 것이지요?",
          "speaker_id": "1",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100027324.1.15",
          "form": "아프지는 않으셨어요?",
          "original_form": "아프지는 않으셨어요?",
          "speaker_id": "2",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100027324.1.16",
          "form": "이틀 팔이 붓고요",
          "original_form": "이틀 팔이 붓고요",
          "speaker_id": "1",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100027324.1.17",
          "form": "일주일후 겨드랑이 림프절이 부었어요",
          "original_form": "일주일후 겨드랑이 림프절이 부었어요",
          "speaker_id": "1",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100027324.1.18",
          "form": "아이들이 어려서\n제가 봐야하는데\n아프면은 당장 아이들을\n돌보기가 힘들어서요ㅠ",
          "original_form": "아이들이 어려서\n제가 봐야하는데\n아프면은 당장 아이들을\n돌보기가 힘들어서요ㅠ",
          "speaker_id": "2",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100027324.1.19",
          "form": "일주일후 증상은 흔하대요",
          "original_form": "일주일후 증상은 흔하대요",
          "speaker_id": "1",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100027324.1.20",
          "form": "맞아요 여유시간이 필요해요",
          "original_form": "맞아요 여유시간이 필요해요",
          "speaker_id": "1",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100027324.1.21",
          "form": "여자는 림프절 붓는거 흔하대요",
          "original_form": "여자는 림프절 붓는거 흔하대요",
          "speaker_id": "1",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100027324.1.22",
          "form": "그래도 두번맞아야 하니까",
          "original_form": "그래도 두번맞아야 하니까",
          "speaker_id": "1",
          "time": "20210902 11:59"
        },
        {
          "id": "MDRW2100027324.1.23",
          "form": "1차는 무난하대요",
          "original_form": "1차는 무난하대요",
          "speaker_id": "1",
          "time": "20210902 11:59"
        },
        {
          "id": "MDRW2100027324.1.24",
          "form": "이제 조금 있으면은\n또 2차도 맞으시겠네요?",
          "original_form": "이제 조금 있으면은\n또 2차도 맞으시겠네요?",
          "speaker_id": "2",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100027324.1.25",
          "form": "네 다음달초에요",
          "original_form": "네 다음달초에요",
          "speaker_id": "1",
          "time": "20210902 12:27"
        }
      ]
    }
  ]
