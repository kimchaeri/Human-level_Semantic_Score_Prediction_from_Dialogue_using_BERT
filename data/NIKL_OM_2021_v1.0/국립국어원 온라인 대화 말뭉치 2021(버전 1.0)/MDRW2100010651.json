      "utterance": [
        {
          "id": "MDRW2100010651.1.1",
          "form": "name2 비 지금도 새나여",
          "original_form": "&name2& 비 지금도 새나여",
          "speaker_id": "1",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010651.1.2",
          "form": "아니용 집에 비안오니까 안새요",
          "original_form": "아니용 집에 비안오니까 안새요",
          "speaker_id": "2",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010651.1.3",
          "form": "성당에서 아름다운가게여는데 가봤낭? 복음자리 잼도 파는데",
          "original_form": "성당에서 아름다운가게여는데 가봤낭? 복음자리 잼도 파는데",
          "speaker_id": "2",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010651.1.4",
          "form": "울 본당에도 하나있음",
          "original_form": "울 본당에도 하나있음",
          "speaker_id": "1",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010651.1.5",
          "form": "근데 성당 안가본지 진짜오래됨",
          "original_form": "근데 성당 안가본지 진짜오래됨",
          "speaker_id": "1",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010651.1.6",
          "form": "성당에 어릴땐 많이 갔었는데 유일한 놀이터가 있었거등",
          "original_form": "성당에 어릴땐 많이 갔었는데 유일한 놀이터가 있었거등",
          "speaker_id": "2",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010651.1.7",
          "form": "코로나라사 폐쇄되었을수도",
          "original_form": "코로나라사 폐쇄되었을수도",
          "speaker_id": "1",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010651.1.8",
          "form": "웅 아무래도 거기도 폐쇄 된듯",
          "original_form": "웅 아무래도 거기도 폐쇄 된듯",
          "speaker_id": "2",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100010651.1.9",
          "form": "요새 종교 모임이나 행사가 있을수있나요",
          "original_form": "요새 종교 모임이나 행사가 있을수있나요",
          "speaker_id": "1",
          "time": "20210823 22:40"
        },
        {
          "id": "MDRW2100010651.1.10",
          "form": "음 하문안되는데 암암리에 할수도있궁",
          "original_form": "음 하문안되는데 암암리에 할수도있궁",
          "speaker_id": "2",
          "time": "20210823 22:43"
        },
        {
          "id": "MDRW2100010651.1.11",
          "form": "난 사실 스스로 무늬만 카톨릭 같아서",
          "original_form": "난 사실 스스로 무늬만 카톨릭 같아서",
          "speaker_id": "1",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100010651.1.12",
          "form": "좀 반성되기도하고",
          "original_form": "좀 반성되기도하고",
          "speaker_id": "1",
          "time": "20210824 07:40"
        },
        {
          "id": "MDRW2100010651.1.13",
          "form": "종교가 있다는 사실이 싫지는않지만 뭔가 나필요할때만 찾는것같기도함",
          "original_form": "종교가 있다는 사실이 싫지는않지만 뭔가 나필요할때만 찾는것같기도함",
          "speaker_id": "1",
          "time": "20210824 07:40"
        },
        {
          "id": "MDRW2100010651.1.14",
          "form": "인간이 급할때 찾는거지 먼가 신이 있다면 째째하게 안구실듯",
          "original_form": "인간이 급할때 찾는거지 먼가 신이 있다면 째째하게 안구실듯",
          "speaker_id": "2",
          "time": "20210824 09:42"
        },
        {
          "id": "MDRW2100010651.1.15",
          "form": "그러신다면 다행이고 난드니님이 나한테 그 이야기 해줬던가요",
          "original_form": "그러신다면 다행이고 난드니님이 나한테 그 이야기 해줬던가요",
          "speaker_id": "1",
          "time": "20210824 09:44"
        },
        {
          "id": "MDRW2100010651.1.16",
          "form": "머용?",
          "original_form": "머용?",
          "speaker_id": "2",
          "time": "20210824 09:45"
        },
        {
          "id": "MDRW2100010651.1.17",
          "form": "사람이 죽으면 하느님은 분리수거를 하시고 부처님은 재활용을 하신다",
          "original_form": "사람이 죽으면 하느님은 분리수거를 하시고 부처님은 재활용을 하신다",
          "speaker_id": "1",
          "time": "20210824 09:47"
        },
        {
          "id": "MDRW2100010651.1.18",
          "form": "아항 그런얘기는 첨듣는디 그런 차이가 있구나",
          "original_form": "아항 그런얘기는 첨듣는디 그런 차이가 있구나",
          "speaker_id": "2",
          "time": "20210824 09:51"
        },
