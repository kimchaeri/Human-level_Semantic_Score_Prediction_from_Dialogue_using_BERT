      "utterance": [
        {
          "id": "MDRW2100016065.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 09:07"
        },
        {
          "id": "MDRW2100016065.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100016065.1.3",
          "form": "오늘 날씨어떤가요",
          "original_form": "오늘 날씨어떤가요",
          "speaker_id": "1",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100016065.1.4",
          "form": "여긴 비가왔다가 해가 뜨는거같아요",
          "original_form": "여긴 비가왔다가 해가 뜨는거같아요",
          "speaker_id": "1",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100016065.1.5",
          "form": "여기도 오늘은 가을 하늘이네요",
          "original_form": "여기도 오늘은 가을 하늘이네요",
          "speaker_id": "2",
          "time": "20210826 09:56"
        },
        {
          "id": "MDRW2100016065.1.6",
          "form": "날이 맑은가봐요",
          "original_form": "날이 맑은가봐요",
          "speaker_id": "1",
          "time": "20210826 09:56"
        },
        {
          "id": "MDRW2100016065.1.7",
          "form": "날도 맑고 미세먼지도 없어서 좋으네요",
          "original_form": "날도 맑고 미세먼지도 없어서 좋으네요",
          "speaker_id": "2",
          "time": "20210826 09:56"
        },
        {
          "id": "MDRW2100016065.1.8",
          "form": "그게 가을의 매력인거같아요",
          "original_form": "그게 가을의 매력인거같아요",
          "speaker_id": "1",
          "time": "20210826 09:57"
        },
        {
          "id": "MDRW2100016065.1.9",
          "form": "좋아하는 계절이 있으신가요",
          "original_form": "좋아하는 계절이 있으신가요",
          "speaker_id": "1",
          "time": "20210826 09:57"
        },
        {
          "id": "MDRW2100016065.1.10",
          "form": "글쎄요 그냥 생각해보니 초여름 좋아하는 것 같아요",
          "original_form": "글쎄요 그냥 생각해보니 초여름 좋아하는 것 같아요",
          "speaker_id": "2",
          "time": "20210826 09:57"
        },
        {
          "id": "MDRW2100016065.1.11",
          "form": "더운 날 좋아하는 분 많지 않던데",
          "original_form": "더운 날 좋아하는 분 많지 않던데",
          "speaker_id": "1",
          "time": "20210826 09:57"
        },
        {
          "id": "MDRW2100016065.1.12",
          "form": "이유가있나요?",
          "original_form": "이유가있나요?",
          "speaker_id": "1",
          "time": "20210826 09:57"
        },
        {
          "id": "MDRW2100016065.1.13",
          "form": "봄 지나고 바로 오는 그여름이 좋더라고요",
          "original_form": "봄 지나고 바로 오는 그여름이 좋더라고요",
          "speaker_id": "2",
          "time": "20210826 10:10"
        },
        {
          "id": "MDRW2100016065.1.14",
          "form": "좋아하는계절있어요?",
          "original_form": "좋아하는계절있어요?",
          "speaker_id": "2",
          "time": "20210826 10:10"
        },
        {
          "id": "MDRW2100016065.1.15",
          "form": "음 여름과 다른 파릇파릇한걸 좋아하시나봐요",
          "original_form": "음 여름과 다른 파릇파릇한걸 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210826 10:10"
        },
        {
          "id": "MDRW2100016065.1.16",
          "form": "전 겨울 좋아해요",
          "original_form": "전 겨울 좋아해요",
          "speaker_id": "1",
          "time": "20210826 10:10"
        },
        {
          "id": "MDRW2100016065.1.17",
          "form": "아무래도 더운걸 안좋아하는지라ㅎ",
          "original_form": "아무래도 더운걸 안좋아하는지라ㅎ",
          "speaker_id": "1",
          "time": "20210826 10:10"
        },
        {
          "id": "MDRW2100016065.1.18",
          "form": "네ㅎ 겨울도좋죠ㅎㅎ사실 계절은 뭐 다좋으네요ㅎㅎ",
          "original_form": "네ㅎ 겨울도좋죠ㅎㅎ사실 계절은 뭐 다좋으네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100016065.1.19",
          "form": "오늘날씨도좋고",
          "original_form": "오늘날씨도좋고",
          "speaker_id": "2",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100016065.1.20",
          "form": "맞아요 너무 좋은 낧씨에요",
          "original_form": "맞아요 너무 좋은 낧씨에요",
          "speaker_id": "1",
          "time": "20210826 10:12"
        }
      ]
