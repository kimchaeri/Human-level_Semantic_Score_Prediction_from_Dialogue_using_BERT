      "utterance": [
        {
          "id": "MDRW2100033080.1.1",
          "form": "집에 있던데? 화장대 위에",
          "original_form": "집에 있던데? 화장대 위에",
          "speaker_id": "2",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100033080.1.2",
          "form": "그거 너무 오래됐어",
          "original_form": "그거 너무 오래됐어",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100033080.1.3",
          "form": "언제 산건데?",
          "original_form": "언제 산건데?",
          "speaker_id": "2",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100033080.1.4",
          "form": "한 2년 된 것 같아 ㅋㅋ",
          "original_form": "한 2년 된 것 같아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100033080.1.5",
          "form": "훌 다시 사야겠네",
          "original_form": "훌 다시 사야겠네",
          "speaker_id": "2",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100033080.1.6",
          "form": "약국에 가면 니베아 무향 팔던데 그거 사야겠다",
          "original_form": "약국에 가면 니베아 무향 팔던데 그거 사야겠다",
          "speaker_id": "1",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100033080.1.7",
          "form": "ㅇㅇ 무향으로 사~ 다 같이 바르게",
          "original_form": "ㅇㅇ 무향으로 사~ 다 같이 바르게",
          "speaker_id": "2",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100033080.1.8",
          "form": "입술 보호제는 따로 쓰는 게 좋은데",
          "original_form": "입술 보호제는 따로 쓰는 게 좋은데",
          "speaker_id": "1",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100033080.1.9",
          "form": "가족인데 뭐 어때 ㅎㅎ",
          "original_form": "가족인데 뭐 어때 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100033080.1.10",
          "form": "그래도 세균이랑 감염되기 쉬우니까",
          "original_form": "그래도 세균이랑 감염되기 쉬우니까",
          "speaker_id": "1",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100033080.1.11",
          "form": "그럼 한 2~3개 사 나도 바르게",
          "original_form": "그럼 한 2~3개 사 나도 바르게",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100033080.1.12",
          "form": "그래야겠다.. 애들도 슬슬 입술 트려고 하던데",
          "original_form": "그래야겠다.. 애들도 슬슬 입술 트려고 하던데",
          "speaker_id": "1",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100033080.1.13",
          "form": "요즘 너무 건조해서 그래",
          "original_form": "요즘 너무 건조해서 그래",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100033080.1.14",
          "form": "입술 트기전에 미리미리 발라야 효과 좋아",
          "original_form": "입술 트기전에 미리미리 발라야 효과 좋아",
