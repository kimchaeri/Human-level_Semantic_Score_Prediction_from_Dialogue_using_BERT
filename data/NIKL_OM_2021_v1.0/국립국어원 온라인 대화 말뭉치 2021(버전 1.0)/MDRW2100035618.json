      "utterance": [
        {
          "id": "MDRW2100035618.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100035618.1.2",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100035618.1.3",
          "form": "저녁먹었어요",
          "original_form": "저녁먹었어요",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035618.1.4",
          "form": "어떤 메뉴였나요?",
          "original_form": "어떤 메뉴였나요?",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100035618.1.5",
          "form": "계란말이랑 된장국에 밥먹었어요",
          "original_form": "계란말이랑 된장국에 밥먹었어요",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035618.1.6",
          "form": "한식으로 드셨군요",
          "original_form": "한식으로 드셨군요",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035618.1.7",
          "form": "네 요즘 계란이 비싸서 잘 안먹다가 오랜만에 먹었어요",
          "original_form": "네 요즘 계란이 비싸서 잘 안먹다가 오랜만에 먹었어요",
          "speaker_id": "2",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100035618.1.8",
          "form": "계란값이 요즘 많이 비싸졌죠ㅜㅜ",
          "original_form": "계란값이 요즘 많이 비싸졌죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100035618.1.9",
          "form": "네ㅠㅠ 뭔 담합이라는데 정말 괘씸해요ㅠ",
          "original_form": "네ㅠㅠ 뭔 담합이라는데 정말 괘씸해요ㅠ",
          "speaker_id": "2",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100035618.1.10",
          "form": "계란 비싼게 담합해서였나요?",
          "original_form": "계란 비싼게 담합해서였나요?",
          "speaker_id": "1",
          "time": "20210829 23:25"
        },
        {
          "id": "MDRW2100035618.1.11",
          "form": "첨엔 ai땜에라는데 요즘은 담합이라네요ㅋㅋㅋ",
          "original_form": "첨엔 ai땜에라는데 요즘은 담합이라네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:00"
        },
        {
          "id": "MDRW2100035618.1.12",
          "form": "그렇군요\n하긴 문제되었던 상황이 끝나도 왜 계속 비싼건가 싶었어요",
          "original_form": "그렇군요\n하긴 문제되었던 상황이 끝나도 왜 계속 비싼건가 싶었어요",
          "speaker_id": "1",
          "time": "20210830 00:02"
        },
        {
          "id": "MDRW2100035618.1.13",
          "form": "계란 너무 비싸서 어쩔땐 직접 닭이라도 키우고 싶더라고요ㅋㅋ",
          "original_form": "계란 너무 비싸서 어쩔땐 직접 닭이라도 키우고 싶더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:07"
        }
      ]
