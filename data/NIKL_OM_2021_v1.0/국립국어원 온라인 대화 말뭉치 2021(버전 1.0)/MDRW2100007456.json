        },
        {
          "id": "MDRW2100007456.1.3",
          "form": "25일 개봉인데",
          "original_form": "25일 개봉인데",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007456.1.4",
          "form": "수요일이라 못본다ㅠㅜㅠ",
          "original_form": "수요일이라 못본다ㅠㅜㅠ",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007456.1.5",
          "form": "나는 칠월과안생",
          "original_form": "나는 칠월과안생",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007456.1.6",
          "form": "보고싶어",
          "original_form": "보고싶어",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007456.1.7",
          "form": "근대 슬프대",
          "original_form": "근대 슬프대",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007456.1.8",
          "form": "이것저것 이벤트 많이할텐데",
          "original_form": "이것저것 이벤트 많이할텐데",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007456.1.9",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007456.1.10",
          "form": "그거 영화관에서 보면 오열각이여ㅠ",
          "original_form": "그거 영화관에서 보면 오열각이여ㅠ",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007456.1.11",
          "form": "쁘앵",
          "original_form": "쁘앵",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007456.1.12",
          "form": ";>n<;",
          "original_form": ";>n<;",
          "speaker_id": "2",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007456.1.13",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "2",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007456.1.14",
          "form": "학교에서 보는데 눈물날뻔 했어",
          "original_form": "학교에서 보는데 눈물날뻔 했어",
          "speaker_id": "1",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007456.1.15",
          "form": "봤었어?",
          "original_form": "봤었어?",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007456.1.16",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007456.1.17",
          "form": "중국어시간에",
          "original_form": "중국어시간에",
          "speaker_id": "1",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007456.1.18",
          "form": "오오옹",
          "original_form": "오오옹",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007456.1.19",
          "form": "한국리메이크판은 언제나오나?",
          "original_form": "한국리메이크판은 언제나오나?",
          "speaker_id": "1",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007456.1.20",
          "form": "별거 다 보여주네",
          "original_form": "별거 다 보여주네",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007456.1.21",
          "form": "몰라몰라",
          "original_form": "몰라몰라",
          "speaker_id": "2",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007456.1.22",
          "form": "2학년 끝날따쯤 할꺼 없어서",
          "original_form": "2학년 끝날따쯤 할꺼 없어서",
          "speaker_id": "1",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007456.1.23",
          "form": "''",
          "original_form": "'{emoji:♡}'",
          "speaker_id": "2",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007456.1.24",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007456.1.25",
          "form": "아니 아니 그 뭐지",
          "original_form": "아니 아니 그 뭐지",
          "speaker_id": "2",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007456.1.26",
          "form": "근데 아무리 잘만들어도 주동우는 못살릴듯",
          "original_form": "근데 아무리 잘만들어도 주동우는 못살릴듯",
          "speaker_id": "1",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007456.1.27",
          "form": "뭐?",
          "original_form": "뭐?",
          "speaker_id": "1",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007456.1.28",
          "form": "주동우 역할이 뭐였지?",
          "original_form": "주동우 역할이 뭐였지?",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007456.1.29",
          "form": "안생일껄",
          "original_form": "안생일껄",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007456.1.30",
          "form": "마사순이 칠월",
          "original_form": "마사순이 칠월",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007456.1.31",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007456.1.32",
          "form": "근데 그거 드라마도 있잖나",
          "original_form": "근데 그거 드라마도 있잖나",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007456.1.33",
          "form": "안생이 비행청소년인가?",
          "original_form": "안생이 비행청소년인가?",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007456.1.34",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007456.1.35",
          "form": "아니 아마 칠월이가 그럴껄",
          "original_form": "아니 아마 칠월이가 그럴껄",
          "speaker_id": "2",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007456.1.36",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007456.1.37",
          "form": "누가봐도 심월이 칠월",
          "original_form": "누가봐도 심월이 칠월",
