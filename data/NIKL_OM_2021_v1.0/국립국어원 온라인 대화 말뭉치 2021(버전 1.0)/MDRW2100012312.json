      "utterance": [
        {
          "id": "MDRW2100012312.1.1",
          "form": "음 직장인이신가요",
          "original_form": "음 직장인이신가요",
          "speaker_id": "2",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100012312.1.2",
          "form": "네 워킹맘 입니다",
          "original_form": "네 워킹맘 입니다",
          "speaker_id": "1",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100012312.1.3",
          "form": "저는 요즘 취업준비중이에요",
          "original_form": "저는 요즘 취업준비중이에요",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100012312.1.4",
          "form": "어떤 일을 하시나요",
          "original_form": "어떤 일을 하시나요",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100012312.1.5",
          "form": "의료기 생산업체 근무해요",
          "original_form": "의료기 생산업체 근무해요",
          "speaker_id": "1",
          "time": "20210902 18:42"
        },
        {
          "id": "MDRW2100012312.1.6",
          "form": "오 혹시 세라젬이나 미건? 아시나요 ㅎㅎ",
          "original_form": "오 혹시 세라젬이나 미건? 아시나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100012312.1.7",
          "form": "알기 하지만 자세히는 몰라요",
          "original_form": "알기 하지만 자세히는 몰라요",
          "speaker_id": "1",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100012312.1.8",
          "form": "그쪽 계통 아시나요",
          "original_form": "그쪽 계통 아시나요",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100012312.1.9",
          "form": "아 최근 미건 초음파기랑 척추기사서요",
          "original_form": "아 최근 미건 초음파기랑 척추기사서요",
          "speaker_id": "2",
          "time": "20210903 07:09"
        },
        {
          "id": "MDRW2100012312.1.10",
          "form": "혹시 그쪽인가했어요 음 그럼 뭘 만드는 회사인가요?",
          "original_form": "혹시 그쪽인가했어요 음 그럼 뭘 만드는 회사인가요?",
          "speaker_id": "2",
          "time": "20210903 07:10"
        },
        {
          "id": "MDRW2100012312.1.11",
          "form": "병원 기기 입니다",
          "original_form": "병원 기기 입니다",
          "speaker_id": "1",
          "time": "20210903 07:26"
        },
        {
          "id": "MDRW2100012312.1.12",
          "form": "약품 쪽이요",
          "original_form": "약품 쪽이요",
          "speaker_id": "1",
          "time": "20210903 07:26"
        },
        {
          "id": "MDRW2100012312.1.13",
          "form": "그렇구나 저는 계약직으로 일하다가 요즘 아예 새로운 분야로 취업을 알아보고있어요",
          "original_form": "그렇구나 저는 계약직으로 일하다가 요즘 아예 새로운 분야로 취업을 알아보고있어요",
          "speaker_id": "2",
          "time": "20210903 07:44"
        },
        {
          "id": "MDRW2100012312.1.14",
          "form": "코로나로 취업 힘들죠",
          "original_form": "코로나로 취업 힘들죠",
          "speaker_id": "1",
          "time": "20210903 07:51"
        },
        {
          "id": "MDRW2100012312.1.15",
          "form": "그래서 취업지원제도 지원받는데",
          "original_form": "그래서 취업지원제도 지원받는데",
          "speaker_id": "2",
          "time": "20210903 07:52"
        },
        {
          "id": "MDRW2100012312.1.16",
          "form": "솔직히 이거 안받아도 좋으니 취업하고싶어요",
          "original_form": "솔직히 이거 안받아도 좋으니 취업하고싶어요",
          "speaker_id": "2",
          "time": "20210903 07:52"
        },
        {
          "id": "MDRW2100012312.1.17",
          "form": "취업이 모두에 마음 일거예요",
          "original_form": "취업이 모두에 마음 일거예요",
          "speaker_id": "1",
          "time": "20210903 07:53"
        },
        {
          "id": "MDRW2100012312.1.18",
          "form": "지원제도 받으면 도음이 되나요",
          "original_form": "지원제도 받으면 도음이 되나요",
          "speaker_id": "1",
          "time": "20210903 07:53"
        },
        {
          "id": "MDRW2100012312.1.19",
          "form": "전에 누가 실업급여랑 취업지원제도 혜택금액이늘어서 젊은사람들이 취업안한다 이런기사를 읽었거든요",
          "original_form": "전에 누가 실업급여랑 취업지원제도 혜택금액이늘어서 젊은사람들이 취업안한다 이런기사를 읽었거든요",
          "speaker_id": "2",
          "time": "20210903 07:54"
        },
        {
          "id": "MDRW2100012312.1.20",
          "form": "무슨 헛소리인가 싶었던 기억이 나서 ㅎㅎ",
          "original_form": "무슨 헛소리인가 싶었던 기억이 나서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 07:54"
        }
      ]
