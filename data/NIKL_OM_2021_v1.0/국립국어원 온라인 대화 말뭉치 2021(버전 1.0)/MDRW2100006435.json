      "utterance": [
        {
          "id": "MDRW2100006435.1.1",
          "form": "아빠 과자좀 먹을래용",
          "original_form": "아빠 과자좀 먹을래용",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100006435.1.2",
          "form": "지금 시간에 무슨 과자",
          "original_form": "지금 시간에 무슨 과자",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100006435.1.3",
          "form": "좀 그른가 ㅋㅋ",
          "original_form": "좀 그른가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100006435.1.4",
          "form": "그래 살찐다",
          "original_form": "그래 살찐다",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100006435.1.5",
          "form": "먹던거 다 못먹겠어서",
          "original_form": "먹던거 다 못먹겠어서",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100006435.1.6",
          "form": "그럼 좀 주던지",
