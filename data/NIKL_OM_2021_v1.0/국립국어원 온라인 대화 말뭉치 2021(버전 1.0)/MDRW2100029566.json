        {
          "id": "MDRW2100029566.1.1",
          "form": "여긴 해가 쨍쨍하네요",
          "original_form": "여긴 해가 쨍쨍하네요",
          "speaker_id": "2",
          "time": "20210903 10:25"
        },
        {
          "id": "MDRW2100029566.1.2",
          "form": "여긴 구름이 많아요",
          "original_form": "여긴 구름이 많아요",
          "speaker_id": "1",
          "time": "20210903 10:27"
        },
        {
          "id": "MDRW2100029566.1.3",
          "form": "이제 긴팔 꺼내입으세요?",
          "original_form": "이제 긴팔 꺼내입으세요?",
          "speaker_id": "2",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029566.1.4",
          "form": "그래야될 것 같아요",
          "original_form": "그래야될 것 같아요",
          "speaker_id": "1",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029566.1.5",
          "form": "환절기가 옷 입기 제일 애매해요",
          "original_form": "환절기가 옷 입기 제일 애매해요",
          "speaker_id": "2",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029566.1.6",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029566.1.7",
          "form": "더위많이 타세요?",
          "original_form": "더위많이 타세요?",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100029566.1.8",
          "form": "네.많이여",
          "original_form": "네.많이여",
          "speaker_id": "1",
          "time": "20210903 11:22"
        },
        {
          "id": "MDRW2100029566.1.9",
          "form": "그럼 늦가을까지 계속 반팔",
          "original_form": "그럼 늦가을까지 계속 반팔",
          "speaker_id": "2",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029566.1.10",
          "form": "입는 편이시겠어요",
          "original_form": "입는 편이시겠어요",
          "speaker_id": "2",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029566.1.11",
          "form": "네 땀이",
          "original_form": "네 땀이",
          "speaker_id": "1",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029566.1.12",
          "form": "많군요",
          "original_form": "많군요",
          "speaker_id": "2",
          "time": "20210903 11:32"
        },
        {
