      "utterance": [
        {
          "id": "MDRW2100004049.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100004049.1.2",
          "form": "반려동물 있으신가요?",
          "original_form": "반려동물 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100004049.1.3",
          "form": "네 저는 반려동물을 키워요",
          "original_form": "네 저는 반려동물을 키워요",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004049.1.4",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004049.1.5",
          "form": "어떤 동물인가요??",
          "original_form": "어떤 동물인가요??",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004049.1.6",
          "form": "저는 강아지 키워요!",
          "original_form": "저는 강아지 키워요!",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004049.1.7",
          "form": "검은색 푸들이에요",
          "original_form": "검은색 푸들이에요",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004049.1.8",
          "form": "너무 귀여울 것 같아요ㅜㅜ",
          "original_form": "너무 귀여울 것 같아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004049.1.9",
          "form": "저는 키워본 적이 없네요ㅜㅜ",
          "original_form": "저는 키워본 적이 없네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004049.1.10",
          "form": "아 정말요? 반려동물을요?",
          "original_form": "아 정말요? 반려동물을요?",
          "speaker_id": "1",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004049.1.11",
          "form": "네네ㅜㅜ어릴 때는 부모님이 싫어하셔서 못 키웠고",
          "original_form": "네네ㅜㅜ어릴 때는 부모님이 싫어하셔서 못 키웠고",
          "speaker_id": "2",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004049.1.12",
          "form": "지금은 살고있는 곳이 반려동물 X여서ㅜㅜ",
          "original_form": "지금은 살고있는 곳이 반려동물 X여서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004049.1.13",
          "form": "키우시는 분들 대단한 것 같아요",
          "original_form": "키우시는 분들 대단한 것 같아요",
          "speaker_id": "2",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004049.1.14",
          "form": "헉... ㅠㅠ 하긴 반려동물을 못 키우는 곳도 있죠 ㅠㅠ",
          "original_form": "헉... ㅠㅠ 하긴 반려동물을 못 키우는 곳도 있죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004049.1.15",
          "form": "맞아요 책임이 많이 따르니까",
          "original_form": "맞아요 책임이 많이 따르니까",
          "speaker_id": "1",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004049.1.16",
          "form": "키우시는 분들은 다 대단한 것 같네요...",
          "original_form": "키우시는 분들은 다 대단한 것 같네요...",
          "speaker_id": "1",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004049.1.17",
          "form": "맞아요 책임",
          "original_form": "맞아요 책임",
          "speaker_id": "2",
          "time": "20210821 14:47"
        },
        {
          "id": "MDRW2100004049.1.18",
          "form": "나중에 제 집도 사고 하면 강아지를 키워보고 싶은데",
          "original_form": "나중에 제 집도 사고 하면 강아지를 키워보고 싶은데",
          "speaker_id": "2",
          "time": "20210821 14:48"
        },
        {
          "id": "MDRW2100004049.1.19",
          "form": "제가 그 아이를 완전히 책임질 수 있을까란 생각이 계속 들어서",
          "original_form": "제가 그 아이를 완전히 책임질 수 있을까란 생각이 계속 들어서",
          "speaker_id": "2",
          "time": "20210821 14:48"
        },
        {
          "id": "MDRW2100004049.1.20",
          "form": "결국 기르지는 못할 것 같네요...ㅋㅋㅋ",
          "original_form": "결국 기르지는 못할 것 같네요...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:48"
        },
        {
          "id": "MDRW2100004049.1.21",
          "form": "아이고... 그렇죠 ㅠㅠ 직장이라든지 다니면 강아지가 집에 혼자 남아있어야 하니까요...",
          "original_form": "아이고... 그렇죠 ㅠㅠ 직장이라든지 다니면 강아지가 집에 혼자 남아있어야 하니까요...",
          "speaker_id": "1",
          "time": "20210821 14:49"
        },
        {
          "id": "MDRW2100004049.1.22",
          "form": "그래도 다른 문제가 없다고 가정한다면",
          "original_form": "그래도 다른 문제가 없다고 가정한다면",
          "speaker_id": "1",
          "time": "20210821 14:49"
        },
        {
          "id": "MDRW2100004049.1.23",
          "form": "기르고 싶은 반려동물 종이 있으신가요?",
          "original_form": "기르고 싶은 반려동물 종이 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 14:49"
        },
        {
          "id": "MDRW2100004049.1.24",
          "form": "음 저는 골든 리트리버..?",
          "original_form": "음 저는 골든 리트리버..?",
          "speaker_id": "2",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004049.1.25",
          "form": "그 뭔가 착하고 순하다고 해서",
          "original_form": "그 뭔가 착하고 순하다고 해서",
          "speaker_id": "2",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004049.1.26",
          "form": "그리고 크면 저만큼 클 것 같아서",
          "original_form": "그리고 크면 저만큼 클 것 같아서",
          "speaker_id": "2",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004049.1.27",
          "form": "뭔가 껴안고 있으면 행복할 것 같아요",
          "original_form": "뭔가 껴안고 있으면 행복할 것 같아요",
          "speaker_id": "2",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004049.1.28",
          "form": "아 그렇죠!! 저도 골든 리트리버를 키워본 적이 있어요!",
          "original_form": "아 그렇죠!! 저도 골든 리트리버를 키워본 적이 있어요!",
          "speaker_id": "1",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004049.1.29",
          "form": "대형견의 매력은 역시 사람만큼 크다는 점에 있는 것 같아요",
          "original_form": "대형견의 매력은 역시 사람만큼 크다는 점에 있는 것 같아요",
          "speaker_id": "1",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004049.1.30",
          "form": "우와!",
          "original_form": "우와!",
          "speaker_id": "2",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004049.1.31",
          "form": "너무 부러워요ㅜㅜㅜㅜㅜ",
          "original_form": "너무 부러워요ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004049.1.32",
          "form": "헤헤...",
          "original_form": "헤헤...",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004049.1.33",
          "form": "근데 강아지들은 외로움을 많이 타나요??",
          "original_form": "근데 강아지들은 외로움을 많이 타나요??",
          "speaker_id": "2",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004049.1.34",
          "form": "아 네...!! 지금 제가 키우고 있는 강아지는",
          "original_form": "아 네...!! 지금 제가 키우고 있는 강아지는",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004049.1.35",
          "form": "가족 중 한명이라도 나가면 현관 앞에서 계속 울더라고요 ㅠㅠ",
          "original_form": "가족 중 한명이라도 나가면 현관 앞에서 계속 울더라고요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004049.1.36",
          "form": "사랑을 많이 줄수록? 외로움을 많이 타는 것 같아요",
          "original_form": "사랑을 많이 줄수록? 외로움을 많이 타는 것 같아요",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004049.1.37",
          "form": "ㅜㅜㅜㅜㅜㅜㅜㅜㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜㅜㅜㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004049.1.38",
          "form": "귀여운데 슬프넹요ㅜㅜ",
          "original_form": "귀여운데 슬프넹요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004049.1.39",
