      "utterance": [
        {
          "id": "MDRW2100010745.1.1",
          "form": "안싸우면 다행이야 프로 보고",
          "original_form": "안싸우면 다행이야 프로 보고",
          "speaker_id": "2",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100010745.1.2",
          "form": "전복 생거는 무슨 맛일까?",
          "original_form": "전복 생거는 무슨 맛일까?",
          "speaker_id": "1",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010745.1.3",
          "form": "현주엽이 완전 하마랑 닮았다 여기서 별명 히포",
          "original_form": "현주엽이 완전 하마랑 닮았다 여기서 별명 히포",
          "speaker_id": "2",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010745.1.4",
          "form": "입도 크고 하마 닮았다",
          "original_form": "입도 크고 하마 닮았다",
          "speaker_id": "1",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010745.1.5",
          "form": "그냥 바다에 손만 넣으면 전복이네",
          "original_form": "그냥 바다에 손만 넣으면 전복이네",
          "speaker_id": "2",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010745.1.6",
          "form": "우와 장난 아닌 곳이네",
          "original_form": "우와 장난 아닌 곳이네",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010745.1.7",
          "form": "먹을때 보면 그냥 음식이 빨려들어가는거 같다",
          "original_form": "먹을때 보면 그냥 음식이 빨려들어가는거 같다",
          "speaker_id": "2",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010745.1.8",
          "form": "현주엽은 고기 먹을때도 장난 아님",
          "original_form": "현주엽은 고기 먹을때도 장난 아님",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010745.1.9",
          "form": "응 여기서도 먹는거 장난이 아니다",
          "original_form": "응 여기서도 먹는거 장난이 아니다",
          "speaker_id": "2",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010745.1.10",
          "form": "맛있는거 먹고 돈도 벌고 좋은 직업이다",
          "original_form": "맛있는거 먹고 돈도 벌고 좋은 직업이다",
          "speaker_id": "1",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100010745.1.11",
          "form": "해상이랑 전복잡아서 물회 해먹을거란다",
          "original_form": "해상이랑 전복잡아서 물회 해먹을거란다",
          "speaker_id": "2",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100010745.1.12",
          "form": "해산물 잔치하네",
          "original_form": "해산물 잔치하네",
          "speaker_id": "1",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100010745.1.13",
          "form": "난 해삼은 생거 못먹겠던데 ㅜ.ㅜ",
          "original_form": "난 해삼은 생거 못먹겠던데 ㅜ.ㅜ",
          "speaker_id": "2",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100010745.1.14",
          "form": "난 날것은 잘 못 먹음",
          "original_form": "난 날것은 잘 못 먹음",
          "speaker_id": "1",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100010745.1.15",
          "form": "너도나도 줍 줍이란다.",
          "original_form": "너도나도 줍 줍이란다.",
          "speaker_id": "2",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100010745.1.16",
          "form": "ㅋㅋ  우리도 거기 가면 줍줍이네",
          "original_form": "ㅋㅋ  우리도 거기 가면 줍줍이네",
          "speaker_id": "1",
