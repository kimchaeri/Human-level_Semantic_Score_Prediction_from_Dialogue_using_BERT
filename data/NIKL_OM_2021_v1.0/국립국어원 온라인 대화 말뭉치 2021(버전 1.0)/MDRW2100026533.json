      "utterance": [
        {
          "id": "MDRW2100026533.1.1",
          "form": "네 운동 좋아해요ㅎㅎ",
          "original_form": "네 운동 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026533.1.2",
          "form": "전 홈트",
          "original_form": "전 홈트",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026533.1.3",
          "form": "그러시군요 ㅣㅎㅎ",
          "original_form": "그러시군요 ㅣㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026533.1.4",
          "form": "운동 자주 하세요?",
          "original_form": "운동 자주 하세요?",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026533.1.5",
          "form": "매일오",
          "original_form": "매일오",
          "speaker_id": "1",
          "time": "20210902 07:17"
        },
        {
          "id": "MDRW2100026533.1.6",
          "form": "아하 매일하시는군여 저도 헬스장에서 매일 운동해요!ㅎㅎ",
          "original_form": "아하 매일하시는군여 저도 헬스장에서 매일 운동해요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:08"
        },
        {
          "id": "MDRW2100026533.1.7",
          "form": "부지런하시네여",
          "original_form": "부지런하시네여",
          "speaker_id": "1",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100026533.1.8",
          "form": "감사합니당ㅎㅎ",
          "original_form": "감사합니당ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100026533.1.9",
          "form": "하기싫은날도 일단 가면 열심히 하게되더라구요!ㅋㅋㅋ",
          "original_form": "하기싫은날도 일단 가면 열심히 하게되더라구요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100026533.1.10",
          "form": "가면 하죠 ㅋㅋ",
          "original_form": "가면 하죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:11"
        },
        {
          "id": "MDRW2100026533.1.11",
          "form": "맞아요ㅋㅋㅋ 어쨌든 가면 땀도 좀나고 하다보면 열심히 하더라구용",
          "original_form": "맞아요ㅋㅋㅋ 어쨌든 가면 땀도 좀나고 하다보면 열심히 하더라구용",
          "speaker_id": "2",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100026533.1.12",
          "form": "동기부여되네여",
          "original_form": "동기부여되네여",
          "speaker_id": "1",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100026533.1.13",
          "form": "구기종목같은 운동하시는건 안좋아하시나요?",
          "original_form": "구기종목같은 운동하시는건 안좋아하시나요?",
          "speaker_id": "2",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100026533.1.14",
          "form": "맨손운동이죠",
          "original_form": "맨손운동이죠",
          "speaker_id": "1",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100026533.1.15",
          "form": "구기종목운 별로 안좋아하시나보네용ㅎㅎ 저는 농구좋아해요!",
          "original_form": "구기종목운 별로 안좋아하시나보네용ㅎㅎ 저는 농구좋아해요!",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100026533.1.16",
          "form": "잘 못해요",
          "original_form": "잘 못해요",
          "speaker_id": "1",
          "time": "20210902 22:48"
        }
      ]
