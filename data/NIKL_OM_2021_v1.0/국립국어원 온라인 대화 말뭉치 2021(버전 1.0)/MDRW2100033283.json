          "speaker_id": "2",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100033283.1.22",
          "form": "오?",
          "original_form": "오?",
          "speaker_id": "2",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100033283.1.23",
          "form": "저번주엔 백신맞고 쉬었지만",
