      "utterance": [
        {
          "id": "MDRW2100039097.1.1",
          "form": "취업하구싶네요",
          "original_form": "취업하구싶네요",
          "speaker_id": "2",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100039097.1.2",
          "form": "어떤 회사에 가고싶어요?",
          "original_form": "어떤 회사에 가고싶어요?",
          "speaker_id": "1",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100039097.1.3",
          "form": "돈 vs 워라벨",
          "original_form": "돈 vs 워라벨",
          "speaker_id": "1",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100039097.1.4",
          "form": "둘다요",
          "original_form": "둘다요",
          "speaker_id": "2",
          "time": "20210903 10:01"
        },
        {
          "id": "MDRW2100039097.1.5",
          "form": "하나만 골라야해요",
          "original_form": "하나만 골라야해요",
          "speaker_id": "1",
          "time": "20210903 10:01"
        },
        {
          "id": "MDRW2100039097.1.6",
          "form": "ㅎ 극단적인거에요?",
          "original_form": "ㅎ 극단적인거에요?",
          "speaker_id": "2",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100039097.1.7",
          "form": "세상엔 둘다 안주는 회사가 태반이에요....",
          "original_form": "세상엔 둘다 안주는 회사가 태반이에요....",
          "speaker_id": "1",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100039097.1.8",
          "form": "슬프지만요...",
          "original_form": "슬프지만요...",
          "speaker_id": "1",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100039097.1.9",
          "form": "그래서 상상에서도 하나만 선택할수잇어요 ㅠ",
          "original_form": "그래서 상상에서도 하나만 선택할수잇어요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100039097.1.10",
          "form": "하참내",
          "original_form": "하참내",
          "speaker_id": "2",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100039097.1.11",
          "form": "워라ㅡ밸 !",
          "original_form": "워라ㅡ밸 !",
          "speaker_id": "2",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100039097.1.12",
          "form": "오.. 퇴근하고의 삶이 중요하시군여",
          "original_form": "오.. 퇴근하고의 삶이 중요하시군여",
          "speaker_id": "1",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100039097.1.13",
          "form": "퇴근하고 어떤거 하고싶으세요?",
          "original_form": "퇴근하고 어떤거 하고싶으세요?",
          "speaker_id": "1",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100039097.1.14",
          "form": "검도요",
          "original_form": "검도요",
          "speaker_id": "2",
          "time": "20210903 10:24"
        },
        {
          "id": "MDRW2100039097.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:24"
        },
        {
          "id": "MDRW2100039097.1.16",
          "form": "ㅋㅋㅋㅋ검도쪽으로 취직할 방안은 없나요?",
          "original_form": "ㅋㅋㅋㅋ검도쪽으로 취직할 방안은 없나요?",
          "speaker_id": "1",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100039097.1.17",
          "form": "그정도 능력은 안대네요",
          "original_form": "그정도 능력은 안대네요",
          "speaker_id": "2",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100039097.1.18",
          "form": "저희 팀 동료 부인이 그냥 공대 나오셔서 회사다니고있었는데",
          "original_form": "저희 팀 동료 부인이 그냥 공대 나오셔서 회사다니고있었는데",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100039097.1.19",
          "form": "필라테스가 너무 재밌어서 자격증 따서서",
          "original_form": "필라테스가 너무 재밌어서 자격증 따서서",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100039097.1.20",
          "form": "투잡하신대용",
          "original_form": "투잡하신대용",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100039097.1.21",
          "form": "우와아...대박 가능란가요투잡",
