      "utterance": [
        {
          "id": "MDRW2100028755.1.1",
          "form": "특히 그 누구지 부산행나온",
          "original_form": "특히 그 누구지 부산행나온",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028755.1.2",
          "form": "여자애 나오는 부분이나..",
          "original_form": "여자애 나오는 부분이나..",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028755.1.3",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028755.1.4",
          "form": "그 어린 대왕",
          "original_form": "그 어린 대왕",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028755.1.5",
          "form": "김해숙 나오지않안ㅆ나",
          "original_form": "김해숙 나오지않안ㅆ나",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028755.1.6",
          "form": "혀뽑는...",
          "original_form": "혀뽑는...",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028755.1.7",
          "form": "ㅇㅇ 김해숙이 나태지옥",
          "original_form": "ㅇㅇ 김해숙이 나태지옥",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028755.1.8",
          "form": "진짜 너무 이상했어",
          "original_form": "진짜 너무 이상했어",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028755.1.9",
          "form": "그럴수록 뻔뻔해야하는건 알겠지만",
          "original_form": "그럴수록 뻔뻔해야하는건 알겠지만",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028755.1.10",
          "form": "그것도 적당히지....",
          "original_form": "그것도 적당히지....",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028755.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ3에서도 지옥 배경이겟지..?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ3에서도 지옥 배경이겟지..?",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028755.1.12",
          "form": "이동욱 진기한으로 쓰려면",
          "original_form": "이동욱 진기한으로 쓰려면",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028755.1.13",
          "form": "ㅇㅇㅇㅇㅇㅇ.......",
          "original_form": "ㅇㅇㅇㅇㅇㅇ.......",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028755.1.14",
          "form": "근데 그래도 저는 안보러갈듯...",
          "original_form": "근데 그래도 저는 안보러갈듯...",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028755.1.15",
          "form": "ㅋㅋㅋㅋ덱스터 스튜디오 이번에 모가디슈에선 잘하던데",
          "original_form": "ㅋㅋㅋㅋ덱스터 스튜디오 이번에 모가디슈에선 잘하던데",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028755.1.16",
          "form": "신함 씨지는 별로였는데....",
          "original_form": "신함 씨지는 별로였는데....",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028755.1.17",
          "form": "아 모가디슈도 덱스터였구나",
          "original_form": "아 모가디슈도 덱스터였구나",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028755.1.18",
          "form": "신함은 너무...",
          "original_form": "신함은 너무...",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028755.1.19",
          "form": "중국영화같았어",
          "original_form": "중국영화같았어",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028755.1.20",
          "form": "안좋은 의미로...",
          "original_form": "안좋은 의미로...",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028755.1.21",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028755.1.22",
          "form": "모가디슈는 진짜 씨지 티안나는데",
          "original_form": "모가디슈는 진짜 씨지 티안나는데",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028755.1.23",
          "form": "진짜 중국영화 ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 중국영화 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028755.1.24",
          "form": "그리고 분장도별로고....",
          "original_form": "그리고 분장도별로고....",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028755.1.25",
          "form": "이정재...",
          "original_form": "이정재...",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028755.1.26",
          "form": "하긴 신함도 나온지 꽤 됐으니까",
          "original_form": "하긴 신함도 나온지 꽤 됐으니까",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028755.1.27",
          "form": "그사이 기술이 발전했을수도..",
          "original_form": "그사이 기술이 발전했을수도..",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028755.1.28",
          "form": "ㅇㅇㅇㅇㅇ그것도있을거고",
          "original_form": "ㅇㅇㅇㅇㅇ그것도있을거고",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028755.1.29",
          "form": "모로코에서찍어서",
          "original_form": "모로코에서찍어서",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028755.1.30",
          "form": "외부전경이나 글씨이런거",
          "original_form": "외부전경이나 글씨이런거",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028755.1.31",
          "form": "다 씨지작업했다던데",
          "original_form": "다 씨지작업했다던데",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028755.1.32",
          "form": "아진짜??",
          "original_form": "아진짜??",
          "speaker_id": "2",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100028755.1.33",
          "form": "모로코랑 소말리아랑 언어가 아예 다른가?",
          "original_form": "모로코랑 소말리아랑 언어가 아예 다른가?",
          "speaker_id": "2",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100028755.1.34",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028755.1.35",
          "form": "느낌도다르고",
          "original_form": "느낌도다르고",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028755.1.36",
          "form": "인종도 살짝 다르고 그렇대",
          "original_form": "인종도 살짝 다르고 그렇대",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028755.1.37",
          "form": "아프리카인도 실제 그 동아프리카 사람을 써야한느데ㅔ 서아프리카사람? 을 썼다고",
          "original_form": "아프리카인도 실제 그 동아프리카 사람을 써야한느데ㅔ 서아프리카사람? 을 썼다고",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028755.1.38",
          "form": "아쉽지만 사실 한국에서만드는 영화가 그걸 어케 다 고증하곘냐는 말을 하지...",
          "original_form": "아쉽지만 사실 한국에서만드는 영화가 그걸 어케 다 고증하곘냐는 말을 하지...",
          "speaker_id": "1",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100028755.1.39",
          "form": "말을하지가아니고 고증하겠냐고 다들 이해하는 분위기더라",
          "original_form": "말을하지가아니고 고증하겠냐고 다들 이해하는 분위기더라",
          "speaker_id": "1",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100028755.1.40",
          "form": "ㅇㅇㅇ영화 찍겠다고 소말리아를 들어갈 수는 없잖아요",
          "original_form": "ㅇㅇㅇ영화 찍겠다고 소말리아를 들어갈 수는 없잖아요",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100028755.1.41",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028755.1.42",
          "form": "근데고증진짜잘했5ㅐ",
          "original_form": "근데고증진짜잘했5ㅐ",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028755.1.43",
          "form": "당시계시던분도감탄했다고",
          "original_form": "당시계시던분도감탄했다고",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028755.1.44",
          "form": "류승완 군함도로 욕 많이 먹고",
          "original_form": "류승완 군함도로 욕 많이 먹고",
          "speaker_id": "2",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100028755.1.45",
          "form": "각성해서 찍었나봐...",
          "original_form": "각성해서 찍었나봐...",
          "speaker_id": "2",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100028755.1.46",
          "form": "아 완전",
          "original_form": "아 완전",
          "speaker_id": "1",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100028755.1.47",
