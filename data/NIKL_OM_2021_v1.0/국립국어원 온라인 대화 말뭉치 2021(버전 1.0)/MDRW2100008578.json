      "utterance": [
        {
          "id": "MDRW2100008578.1.1",
          "form": "가보고싶은 나라있으신가요?",
          "original_form": "가보고싶은 나라있으신가요?",
          "speaker_id": "2",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008578.1.2",
          "form": "저는 터키 가보고 싶어용",
          "original_form": "저는 터키 가보고 싶어용",
          "speaker_id": "1",
          "time": "20210822 16:49"
        },
        {
          "id": "MDRW2100008578.1.3",
          "form": "음식이 맛있다고 해서 ....",
          "original_form": "음식이 맛있다고 해서 ....",
          "speaker_id": "1",
          "time": "20210822 16:49"
        },
        {
          "id": "MDRW2100008578.1.4",
          "form": "오 맞아요! 저두 한번 가보고싶어요 열기구도 한번 타보고싶더라구요",
          "original_form": "오 맞아요! 저두 한번 가보고싶어요 열기구도 한번 타보고싶더라구요",
          "speaker_id": "2",
          "time": "20210822 16:49"
        },
        {
          "id": "MDRW2100008578.1.5",
          "form": "카이막이 그렇게ㅜ맛있다고 하네요",
          "original_form": "카이막이 그렇게ㅜ맛있다고 하네요",
          "speaker_id": "2",
          "time": "20210822 16:49"
        },
        {
          "id": "MDRW2100008578.1.6",
          "form": "맞아요 ㅠㅠ... 백종원 유튜브 보고 너무 먹고 싶어져서...",
          "original_form": "맞아요 ㅠㅠ... 백종원 유튜브 보고 너무 먹고 싶어져서...",
          "speaker_id": "1",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008578.1.7",
          "form": "저는 터키쉬 딜라이트도 먹어보고 싶어요",
          "original_form": "저는 터키쉬 딜라이트도 먹어보고 싶어요",
          "speaker_id": "1",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008578.1.8",
          "form": "오 맞아요 ..! 엄청 달다는데 무슨 맛일지 궁금하네요",
          "original_form": "오 맞아요 ..! 엄청 달다는데 무슨 맛일지 궁금하네요",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008578.1.9",
          "form": "저는 아이슬란드가서 오로라도 한번 보고싶어요~",
          "original_form": "저는 아이슬란드가서 오로라도 한번 보고싶어요~",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008578.1.10",
          "form": "오로라도 로망이죠 ㅠㅜ",
          "original_form": "오로라도 로망이죠 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008578.1.11",
          "form": "사막도 가보고 싶고 그래요",
          "original_form": "사막도 가보고 싶고 그래요",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008578.1.12",
          "form": "맞아요 .. 사막 그 엄청 바퀴큰차타고 달리면 너무 좋을 것 같아요",
          "original_form": "맞아요 .. 사막 그 엄청 바퀴큰차타고 달리면 너무 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008578.1.13",
          "form": "좀 힘들 것 같긴 한데 그래도요",
          "original_form": "좀 힘들 것 같긴 한데 그래도요",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008578.1.14",
          "form": "저는 대학 others을 지금 코로나로 보내고 있어서 더 가고 싶어요...",
          "original_form": "저는 대학 &others&을 지금 코로나로 보내고 있어서 더 가고 싶어요...",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008578.1.15",
          "form": "방학때는 여행가는게 최고인데 말이죠ㅠㅠ",
          "original_form": "방학때는 여행가는게 최고인데 말이죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008578.1.16",
          "form": "그러니까요 ㅠㅠ 잠잠해지면 가까운 곳으로 호캉스라도 다녀오고 싶네요...",
          "original_form": "그러니까요 ㅠㅠ 잠잠해지면 가까운 곳으로 호캉스라도 다녀오고 싶네요...",
          "speaker_id": "1",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008578.1.17",
          "form": "맞아요 .. 물놀이 하고 호캉스 하고 싶네요",
          "original_form": "맞아요 .. 물놀이 하고 호캉스 하고 싶네요",
          "speaker_id": "2",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008578.1.18",
          "form": "물놀이 안 한지 꽤 된 것 같아요",
          "original_form": "물놀이 안 한지 꽤 된 것 같아요",
          "speaker_id": "1",
          "time": "20210822 17:48"
        },
        {
          "id": "MDRW2100008578.1.19",
          "form": "바다 가고 싶네요...",
          "original_form": "바다 가고 싶네요...",
          "speaker_id": "1",
          "time": "20210822 17:48"
        },
        {
          "id": "MDRW2100008578.1.20",
          "form": "바다가는 것도 요즘 쉽지 않더라구요..",
          "original_form": "바다가는 것도 요즘 쉽지 않더라구요..",
          "speaker_id": "2",
          "time": "20210822 17:51"
        },
        {
          "id": "MDRW2100008578.1.21",
          "form": "저도 물놀이 안간지 좀 됐네요..",
          "original_form": "저도 물놀이 안간지 좀 됐네요..",
          "speaker_id": "2",
          "time": "20210822 17:51"
        }
