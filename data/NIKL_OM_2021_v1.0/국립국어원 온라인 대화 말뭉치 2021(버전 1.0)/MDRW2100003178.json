      "utterance": [
        {
          "id": "MDRW2100003178.1.1",
          "form": "여행좋아하세여",
          "original_form": "여행좋아하세여",
          "speaker_id": "2",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003178.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003178.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003178.1.4",
          "form": "여행 싫어하는사람도 있긴하던데",
          "original_form": "여행 싫어하는사람도 있긴하던데",
          "speaker_id": "1",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003178.1.5",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "2",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003178.1.6",
          "form": "코로나때문에 요즘은 못가지만ㅠㅠ",
          "original_form": "코로나때문에 요즘은 못가지만ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003178.1.7",
          "form": "국내말고 해외가고싶어요",
          "original_form": "국내말고 해외가고싶어요",
          "speaker_id": "1",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003178.1.8",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003178.1.9",
          "form": "저도 비행기가 너무타고싶네요",
          "original_form": "저도 비행기가 너무타고싶네요",
          "speaker_id": "2",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003178.1.10",
          "form": "그동안 어디 다녀오셨나요?",
          "original_form": "그동안 어디 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210820 22:20"
        },
        {
          "id": "MDRW2100003178.1.11",
          "form": "저는 일본 홍콩 마카오 영국 프랑스",
          "original_form": "저는 일본 홍콩 마카오 영국 프랑스",
          "speaker_id": "1",
          "time": "20210820 22:20"
        },
        {
          "id": "MDRW2100003178.1.12",
          "form": "저는 최근에는 태국 베트남 싱가포르 갔다왔어여",
          "original_form": "저는 최근에는 태국 베트남 싱가포르 갔다왔어여",
          "speaker_id": "2",
          "time": "20210820 22:20"
        },
        {
          "id": "MDRW2100003178.1.13",
          "form": "이정도...  해외여행에 맛들릴때쯤 코로나...",
          "original_form": "이정도...  해외여행에 맛들릴때쯤 코로나...",
          "speaker_id": "1",
          "time": "20210820 22:21"
        },
        {
          "id": "MDRW2100003178.1.14",
          "form": "싱가포르 가고싶네요",
          "original_form": "싱가포르 가고싶네요",
          "speaker_id": "1",
          "time": "20210820 22:21"
        },
        {
          "id": "MDRW2100003178.1.15",
          "form": "홍콩!!!!",
          "original_form": "홍콩!!!!",
          "speaker_id": "2",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100003178.1.16",
          "form": "안가봤어요",
          "original_form": "안가봤어요",
          "speaker_id": "2",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100003178.1.17",
          "form": "진짜좋다던데",
          "original_form": "진짜좋다던데",
          "speaker_id": "2",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100003178.1.18",
          "form": "좋아요 습한거빼면",
          "original_form": "좋아요 습한거빼면",
          "speaker_id": "1",
          "time": "20210820 22:23"
        },
        {
          "id": "MDRW2100003178.1.19",
          "form": "여름에 갔거든요 ㅋㅋ",
          "original_form": "여름에 갔거든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:23"
        },
        {
          "id": "MDRW2100003178.1.20",
          "form": "쇼핑의천국",
          "original_form": "쇼핑의천국",
          "speaker_id": "1",
          "time": "20210820 22:23"
        },
        {
          "id": "MDRW2100003178.1.21",
          "form": "프랑스 영국도 좋아요",
          "original_form": "프랑스 영국도 좋아요",
          "speaker_id": "1",
          "time": "20210820 22:27"
        },
        {
          "id": "MDRW2100003178.1.22",
          "form": "프랑스는 가봤는데",
          "original_form": "프랑스는 가봤는데",
          "speaker_id": "2",
          "time": "20210820 22:30"
        },
        {
          "id": "MDRW2100003178.1.23",
          "form": "영국은 안가봤어요ㅋㅋ",
          "original_form": "영국은 안가봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:30"
        },
        {
          "id": "MDRW2100003178.1.24",
          "form": "영국은 맛집이 없다던데",
          "original_form": "영국은 맛집이 없다던데",
          "speaker_id": "2",
          "time": "20210820 22:30"
        },
        {
          "id": "MDRW2100003178.1.25",
          "form": "영국",
          "original_form": "영국",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003178.1.26",
          "form": "진짜 먹을거 없어요",
          "original_form": "진짜 먹을거 없어요",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003178.1.27",
          "form": "피쉬앤칩스 최악",
          "original_form": "피쉬앤칩스 최악",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003178.1.28",
          "form": "ㅋㅋㅋㅋㄲ최악ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㄲ최악ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:53"
        },
        {
          "id": "MDRW2100003178.1.29",
          "form": "저는 여행지에서 무조건 맛집만 찾아서ㅋㅋㅋ",
          "original_form": "저는 여행지에서 무조건 맛집만 찾아서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:53"
        },
        {
          "id": "MDRW2100003178.1.30",
          "form": "영국 블로그 맛집 갔는데 진짜 다 노맛.....",
          "original_form": "영국 블로그 맛집 갔는데 진짜 다 노맛.....",
          "speaker_id": "1",
          "time": "20210820 22:54"
        },
        {
          "id": "MDRW2100003178.1.31",
          "form": "진짜 맥날이 제일 맛있었어요",
          "original_form": "진짜 맥날이 제일 맛있었어요",
          "speaker_id": "1",
          "time": "20210820 22:55"
        },
        {
          "id": "MDRW2100003178.1.32",
          "form": "ㅋㅋㅋㅋ영국은 안가는걸로",
          "original_form": "ㅋㅋㅋㅋ영국은 안가는걸로",
          "speaker_id": "2",
          "time": "20210820 23:22"
        },
        {
          "id": "MDRW2100003178.1.33",
          "form": "가장좋았던 여행지는",
          "original_form": "가장좋았던 여행지는",
          "speaker_id": "2",
          "time": "20210820 23:22"
        },
        {
          "id": "MDRW2100003178.1.34",
          "form": "어디세여?",
          "original_form": "어디세여?",
          "speaker_id": "2",
          "time": "20210820 23:22"
        },
        {
          "id": "MDRW2100003178.1.35",
          "form": "오키나와도 좋았어요",
          "original_form": "오키나와도 좋았어요",
          "speaker_id": "1",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003178.1.36",
          "form": "너무 더워서 그것만 빼면....",
          "original_form": "너무 더워서 그것만 빼면....",
          "speaker_id": "1",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100003178.1.37",
          "form": "오키나와!!!!!",
          "original_form": "오키나와!!!!!",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100003178.1.38",
          "form": "좋다고 들었어요",
          "original_form": "좋다고 들었어요",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100003178.1.39",
          "form": "좋긴해요 렌트해서 돌아댕겼어요 ㅋㅋㅋ",
          "original_form": "좋긴해요 렌트해서 돌아댕겼어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003178.1.40",
          "form": "전여친과....",
          "original_form": "전여친과....",
          "speaker_id": "1",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003178.1.41",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003178.1.42",
          "form": "전여친ㅋㅋㅋㅋㅋ",
          "original_form": "전여친ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 00:10"
        },
        {
          "id": "MDRW2100003178.1.43",
          "form": "왜 헤어지셨어요ㅠㅠ",
          "original_form": "왜 헤어지셨어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 00:10"
        },
        {
          "id": "MDRW2100003178.1.44",
          "form": "뭐 다들 그렇죠..  많이 싸우다가 서로불만 쌓이고",
