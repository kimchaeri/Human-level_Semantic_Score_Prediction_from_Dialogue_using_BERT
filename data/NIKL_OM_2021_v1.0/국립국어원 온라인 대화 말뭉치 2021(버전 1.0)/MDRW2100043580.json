      "utterance": [
        {
          "id": "MDRW2100043580.1.1",
          "form": "내일은 월병도 먹자",
          "original_form": "내일은 월병도 먹자",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043580.1.2",
          "form": "엉 집에 먹을게 아주 풍부하군",
          "original_form": "엉 집에 먹을게 아주 풍부하군",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043580.1.3",
          "form": "한가위네",
          "original_form": "한가위네",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043580.1.4",
          "form": "ㅋㅋㅋ한가위 아녀도 먹을 건 많지",
          "original_form": "ㅋㅋㅋ한가위 아녀도 먹을 건 많지",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043580.1.5",
          "form": "응 ㅋㅋㅋ 그래도 뭔가 추석느낌이지",
          "original_form": "응 ㅋㅋㅋ 그래도 뭔가 추석느낌이지",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043580.1.6",
          "form": "한층 모든게 풍요로워지는 느낌",
          "original_form": "한층 모든게 풍요로워지는 느낌",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043580.1.7",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043580.1.8",
          "form": "한가위의 풍족한 밥상",
          "original_form": "한가위의 풍족한 밥상",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043580.1.9",
          "form": "이번엔 무슨 음식 선물이 오려나",
          "original_form": "이번엔 무슨 음식 선물이 오려나",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043580.1.10",
          "form": "아 그러네 선물도 들어오지",
          "original_form": "아 그러네 선물도 들어오지",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043580.1.11",
          "form": "전복이나 생선쓰",
          "original_form": "전복이나 생선쓰",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043580.1.12",
          "form": "문어가 젤 좋은데",
          "original_form": "문어가 젤 좋은데",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043580.1.13",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043580.1.14",
          "form": "문어 오면 좋겠드",
          "original_form": "문어 오면 좋겠드",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043580.1.15",
          "form": "과일은 배나 포도 오면 좋겠다",
          "original_form": "과일은 배나 포도 오면 좋겠다",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043580.1.16",
          "form": "ㅋㅋㅋ그러게 배 먹은지 꽤 됐네",
          "original_form": "ㅋㅋㅋ그러게 배 먹은지 꽤 됐네",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043580.1.17",
          "form": "우리동네도 과일 썰어파는 카페 있더라",
          "original_form": "우리동네도 과일 썰어파는 카페 있더라",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043580.1.18",
          "form": "응 시원하고 물 많은 배",
          "original_form": "응 시원하고 물 많은 배",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043580.1.19",
          "form": "요즘 썰어서 파는 과일 많지",
          "original_form": "요즘 썰어서 파는 과일 많지",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043580.1.20",
          "form": "그거 편하더라고",
          "original_form": "그거 편하더라고",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043580.1.21",
          "form": "특히 인원수가 적으면",
          "original_form": "특히 인원수가 적으면",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043580.1.22",
          "form": "돈 좀 더 내고 음식 쓰 레기도 적고",
          "original_form": "돈 좀 더 내고 음식 쓰 레기도 적고",
          "speaker_id": "1",
          "time": "20210907 19:53"
        }
      ]
