      "utterance": [
        {
          "id": "MDRW2100039914.1.1",
          "form": "아침메뉴는 정하셨나요?",
          "original_form": "아침메뉴는 정하셨나요?",
          "speaker_id": "2",
          "time": "20210908 06:53"
        },
        {
          "id": "MDRW2100039914.1.2",
          "form": "아침은 굶었습니다..ㅠㅠ",
          "original_form": "아침은 굶었습니다..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100039914.1.3",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "1",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100039914.1.4",
          "form": "아고 아침 안드시는군요",
          "original_form": "아고 아침 안드시는군요",
          "speaker_id": "2",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100039914.1.5",
          "form": "네네 잠 5분 더 자는게 행복해서..",
          "original_form": "네네 잠 5분 더 자는게 행복해서..",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100039914.1.6",
          "form": "식사하셨어요",
          "original_form": "식사하셨어요",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100039914.1.7",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100039914.1.8",
          "form": "네 저는 도넛먹었어요",
          "original_form": "네 저는 도넛먹었어요",
          "speaker_id": "2",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100039914.1.9",
          "form": "오 도넛",
          "original_form": "오 도넛",
          "speaker_id": "1",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100039914.1.10",
          "form": "안먹은지 오래된 것 같아요",
          "original_form": "안먹은지 오래된 것 같아요",
          "speaker_id": "1",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100039914.1.11",
          "form": "크리스피도넛 드셨나요!",
          "original_form": "크리스피도넛 드셨나요!",
          "speaker_id": "1",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100039914.1.12",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100039914.1.13",
          "form": "던킨 ㅋ",
          "original_form": "던킨 ㅋ",
          "speaker_id": "2",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100039914.1.14",
          "form": "저도 오랫만에 도넛먹었네요",
          "original_form": "저도 오랫만에 도넛먹었네요",
          "speaker_id": "2",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100039914.1.15",
          "form": "아 저도 던킨 도넛을 더 좋아해요",
          "original_form": "아 저도 던킨 도넛을 더 좋아해요",
          "speaker_id": "1",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100039914.1.16",
          "form": "저는 쿠폰땜에갔어요ㅋ",
          "original_form": "저는 쿠폰땜에갔어요ㅋ",
          "speaker_id": "2",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100039914.1.17",
          "form": "아 던킨 할인 쿠폰 쟈주 날아오는 것 같더라거요",
          "original_form": "아 던킨 할인 쿠폰 쟈주 날아오는 것 같더라거요",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100039914.1.18",
          "form": "해피앱에서",
          "original_form": "해피앱에서",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100039914.1.19",
          "form": "미니도넛팩 천원쿠폰주길래",
          "original_form": "미니도넛팩 천원쿠폰주길래",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100039914.1.20",
          "form": "그래도 도넛팩이니 비쌀줄알았는데",
          "original_form": "그래도 도넛팩이니 비쌀줄알았는데",
          "speaker_id": "2",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100039914.1.21",
          "form": "3300원이더라구요",
          "original_form": "3300원이더라구요",
          "speaker_id": "2",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100039914.1.22",
          "form": "오오 몇개입인가요!",
          "original_form": "오오 몇개입인가요!",
          "speaker_id": "1",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100039914.1.23",
          "form": "6개들었어요",
          "original_form": "6개들었어요",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100039914.1.24",
          "form": "미니사이즈라 손바닥만해요",
          "original_form": "미니사이즈라 손바닥만해요",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100039914.1.25",
          "form": "거기에 천원쿠폰이라 2300원에 사왔거든요",
          "original_form": "거기에 천원쿠폰이라 2300원에 사왔거든요",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100039914.1.26",
          "form": "맛은 던킨도낫맛 ㅎㅎ",
          "original_form": "맛은 던킨도낫맛 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 08:29"
