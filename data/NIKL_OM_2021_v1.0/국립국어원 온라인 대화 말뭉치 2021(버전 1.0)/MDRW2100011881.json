      "utterance": [
        {
          "id": "MDRW2100011881.1.1",
          "form": "종교가 있으세요??",
          "original_form": "종교가 있으세요??",
          "speaker_id": "2",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011881.1.2",
          "form": "아니요 무교예요",
          "original_form": "아니요 무교예요",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011881.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011881.1.4",
          "form": "저도 무교에요",
          "original_form": "저도 무교에요",
          "speaker_id": "2",
          "time": "20210824 18:23"
        },
        {
          "id": "MDRW2100011881.1.5",
          "form": "교회 가본 적은 있으시죠??!",
          "original_form": "교회 가본 적은 있으시죠??!",
          "speaker_id": "2",
          "time": "20210824 18:23"
        },
        {
          "id": "MDRW2100011881.1.6",
          "form": "친구가 목사 딸이라 한번 가봤어요",
          "original_form": "친구가 목사 딸이라 한번 가봤어요",
          "speaker_id": "1",
          "time": "20210824 18:34"
        },
        {
          "id": "MDRW2100011881.1.7",
          "form": "name2님도 가보셨나요?",
          "original_form": "&name2&님도 가보셨나요?",
          "speaker_id": "1",
          "time": "20210824 18:34"
        },
        {
          "id": "MDRW2100011881.1.8",
          "form": "전 완전 어릴 때",
          "original_form": "전 완전 어릴 때",
          "speaker_id": "2",
          "time": "20210824 18:36"
        },
        {
          "id": "MDRW2100011881.1.9",
          "form": "성경학교 이런 거 간다고 가봠ㅅ어요ㅋㅋ",
          "original_form": "성경학교 이런 거 간다고 가봠ㅅ어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:36"
        },
        {
          "id": "MDRW2100011881.1.10",
          "form": "그런거 가면 뭐해요 보통?",
          "original_form": "그런거 가면 뭐해요 보통?",
          "speaker_id": "1",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100011881.1.11",
          "form": "그냥 노는 거에요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그냥 노는 거에요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100011881.1.12",
          "form": "그렇구나 주변에서 한번도 그런쪽으로 가는거 못봐서 몰랐어요",
          "original_form": "그렇구나 주변에서 한번도 그런쪽으로 가는거 못봐서 몰랐어요",
          "speaker_id": "1",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100011881.1.13",
          "form": "ㅋㅋㅋ먹을 거 줘서 간거에요!",
          "original_form": "ㅋㅋㅋ먹을 거 줘서 간거에요!",
          "speaker_id": "2",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100011881.1.14",
          "form": "앜ㅋㅋㅋ제주변도 크리스마스에 과자받으러 간분들 많아요",
          "original_form": "앜ㅋㅋㅋ제주변도 크리스마스에 과자받으러 간분들 많아요",
          "speaker_id": "1",
          "time": "20210824 20:33"
        },
        {
          "id": "MDRW2100011881.1.15",
          "form": "다 저와 같은 사람들이네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "다 저와 같은 사람들이네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:34"
        },
        {
          "id": "MDRW2100011881.1.16",
          "form": "아무래도 제 세대는 무교가 더 많아서 그런가봐요",
          "original_form": "아무래도 제 세대는 무교가 더 많아서 그런가봐요",
          "speaker_id": "1",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100011881.1.17",
          "form": "과자를 주면가고 아니면 안간다",
          "original_form": "과자를 주면가고 아니면 안간다",
          "speaker_id": "1",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100011881.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요",
