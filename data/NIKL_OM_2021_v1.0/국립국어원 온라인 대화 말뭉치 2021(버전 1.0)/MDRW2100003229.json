      "utterance": [
        {
          "id": "MDRW2100003229.1.1",
          "form": "당근에서 최근에 뭐 삼???",
          "original_form": "당근에서 최근에 뭐 삼???",
          "speaker_id": "1",
          "time": "20210820 20:45"
        },
        {
          "id": "MDRW2100003229.1.2",
          "form": "아기수영장 ㅋㅋㅋ 다 육아용품이다",
          "original_form": "아기수영장 ㅋㅋㅋ 다 육아용품이다",
          "speaker_id": "2",
          "time": "20210820 21:09"
        },
        {
          "id": "MDRW2100003229.1.3",
          "form": "넌 최근에 뭐 산거있음?",
          "original_form": "넌 최근에 뭐 산거있음?",
          "speaker_id": "2",
          "time": "20210820 21:09"
        },
        {
          "id": "MDRW2100003229.1.4",
          "form": "나는 캠핑용품 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나는 캠핑용품 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:13"
        },
        {
          "id": "MDRW2100003229.1.5",
          "form": "가격대 있는건 현금이 많지않아서 못사고",
          "original_form": "가격대 있는건 현금이 많지않아서 못사고",
          "speaker_id": "1",
          "time": "20210820 21:14"
        },
        {
          "id": "MDRW2100003229.1.6",
          "form": "몇만원대 안으로 은근 구함 ㅋㅋㅋ",
          "original_form": "몇만원대 안으로 은근 구함 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:14"
        },
        {
          "id": "MDRW2100003229.1.7",
          "form": "ㅋㅋㅋ당근이 최고인거같아",
          "original_form": "ㅋㅋㅋ당근이 최고인거같아",
          "speaker_id": "2",
          "time": "20210820 21:24"
        },
        {
          "id": "MDRW2100003229.1.8",
          "form": "나도 10마넌넘는거 3마넌밑으로구함",
          "original_form": "나도 10마넌넘는거 3마넌밑으로구함",
          "speaker_id": "2",
          "time": "20210820 21:24"
        },
        {
          "id": "MDRW2100003229.1.9",
          "form": "당근 몇번해봄??",
          "original_form": "당근 몇번해봄??",
          "speaker_id": "2",
          "time": "20210820 21:25"
        },
        {
          "id": "MDRW2100003229.1.10",
          "form": "자주 하지 ㅋㅋㅋㅋ",
          "original_form": "자주 하지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:30"
        },
        {
          "id": "MDRW2100003229.1.11",
          "form": "주로 판매를 더 많이 함",
          "original_form": "주로 판매를 더 많이 함",
          "speaker_id": "1",
          "time": "20210820 21:30"
        },
        {
          "id": "MDRW2100003229.1.12",
          "form": "나는 판매는 한번도 안해봄 ㅋㅋ",
          "original_form": "나는 판매는 한번도 안해봄 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:30"
        },
        {
          "id": "MDRW2100003229.1.13",
          "form": "은근 우리 동네 매물이 잘 없다",
          "original_form": "은근 우리 동네 매물이 잘 없다",
          "speaker_id": "1",
          "time": "20210820 21:30"
        },
        {
          "id": "MDRW2100003229.1.14",
          "form": "살거는 천지인데",
          "original_form": "살거는 천지인데",
          "speaker_id": "1",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003229.1.15",
          "form": "오늘 와이프가 버즈라이브 9마넌에팜 ㅋㅋ",
          "original_form": "오늘 와이프가 버즈라이브 9마넌에팜 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003229.1.16",
          "form": "안팔아....",
          "original_form": "안팔아....",
          "speaker_id": "1",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003229.1.17",
          "form": "오 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003229.1.18",
          "form": "잘 팔았네",
          "original_form": "잘 팔았네",
          "speaker_id": "1",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003229.1.19",
          "form": "그걸로 서랍장살라고 ㅋㅋ",
          "original_form": "그걸로 서랍장살라고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003229.1.20",
          "form": "굿이네 ㅋㅋㅋㅋㅋ",
          "original_form": "굿이네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003229.1.21",
          "form": "당근에서 판 돈 당근에서 사기",
          "original_form": "당근에서 판 돈 당근에서 사기",
          "speaker_id": "1",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003229.1.22",
          "form": "그치 돌려막기지",
          "original_form": "그치 돌려막기지",
          "speaker_id": "2",
          "time": "20210820 21:32"
        },
        {
          "id": "MDRW2100003229.1.23",
          "form": "내 동네에 내가 원하는 물건 좀 많았으면",
          "original_form": "내 동네에 내가 원하는 물건 좀 많았으면",
          "speaker_id": "1",
          "time": "20210820 21:32"
        },
        {
          "id": "MDRW2100003229.1.24",
          "form": "캠핑용품이라 그런지 은근 프리미엄을 붙인다",
          "original_form": "캠핑용품이라 그런지 은근 프리미엄을 붙인다",
          "speaker_id": "1",
          "time": "20210820 21:32"
        },
        {
          "id": "MDRW2100003229.1.25",
          "form": "정가보다 비싸게 팔면 그게 당근이냐",
          "original_form": "정가보다 비싸게 팔면 그게 당근이냐",
          "speaker_id": "1",
          "time": "20210820 21:32"
        },
        {
          "id": "MDRW2100003229.1.26",
          "form": "다른동네도 검색해봐 ㅋㅋ",
          "original_form": "다른동네도 검색해봐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:32"
        },
        {
          "id": "MDRW2100003229.1.27",
          "form": "ㅋㅋㅋㅋ그건오바네",
          "original_form": "ㅋㅋㅋㅋ그건오바네",
          "speaker_id": "2",
          "time": "20210820 21:33"
        },
        {
          "id": "MDRW2100003229.1.28",
          "form": "당근이 새제품도 정가보다 싸게내놓아서 좋은건데",
          "original_form": "당근이 새제품도 정가보다 싸게내놓아서 좋은건데",
          "speaker_id": "2",
          "time": "20210820 21:33"
        },
        {
          "id": "MDRW2100003229.1.29",
          "form": "근처 동네 백몇개까지 열어뒀다...",
          "original_form": "근처 동네 백몇개까지 열어뒀다...",
          "speaker_id": "1",
          "time": "20210820 21:33"
        },
        {
          "id": "MDRW2100003229.1.30",
          "form": "저번에 컵 사러 차 타고 상계 다녀옴 ㅋㅋㅋㅋ",
          "original_form": "저번에 컵 사러 차 타고 상계 다녀옴 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:33"
        },
        {
          "id": "MDRW2100003229.1.31",
          "form": "ㅋㅋㅋㅋ차있으면 당근하기좋지",
          "original_form": "ㅋㅋㅋㅋ차있으면 당근하기좋지",
          "speaker_id": "2",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100003229.1.32",
          "form": "난 당근으로 옷장 쇼파 가구도 많이삼 ㅋㅋ",
          "original_form": "난 당근으로 옷장 쇼파 가구도 많이삼 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:38"
        }
      ]
