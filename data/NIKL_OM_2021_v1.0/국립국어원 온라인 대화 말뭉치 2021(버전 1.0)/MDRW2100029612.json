      "utterance": [
        {
          "id": "MDRW2100029612.1.1",
          "form": "조립은 아빠가 해주셨어요 ㅎㅎ",
          "original_form": "조립은 아빠가 해주셨어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029612.1.2",
          "form": "인터넷에 보니까 조립이 은근 힘들다고 하네요...",
          "original_form": "인터넷에 보니까 조립이 은근 힘들다고 하네요...",
          "speaker_id": "2",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029612.1.3",
          "form": "그쵸 혼자사는 저는",
          "original_form": "그쵸 혼자사는 저는",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029612.1.4",
          "form": "1시간 각이에요",
          "original_form": "1시간 각이에요",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029612.1.5",
          "form": "그래서 못사는중 ㅜㅜ",
          "original_form": "그래서 못사는중 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029612.1.6",
          "form": "아이구 그럼 너무 힘드시겠네요 ㅜㅜ",
          "original_form": "아이구 그럼 너무 힘드시겠네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029612.1.7",
          "form": "스텝퍼 같은 것도 운동효과 좋다고 하던데!",
          "original_form": "스텝퍼 같은 것도 운동효과 좋다고 하던데!",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029612.1.8",
          "form": "페달에서 소리는 안나나요",
          "original_form": "페달에서 소리는 안나나요",
          "speaker_id": "1",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029612.1.9",
          "form": "? 불량도 있더라구요 ㅜㅜ 어디껀지는 모르겠지만",
          "original_form": "? 불량도 있더라구요 ㅜㅜ 어디껀지는 모르겠지만",
          "speaker_id": "1",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029612.1.10",
          "form": "소리는 조금 나요 그렇게 시끄럽진 않구요",
          "original_form": "소리는 조금 나요 그렇게 시끄럽진 않구요",
          "speaker_id": "2",
          "time": "20210903 14:15"
        },
        {
          "id": "MDRW2100029612.1.11",
          "form": "아 정말요?? 써보셨어요??",
          "original_form": "아 정말요?? 써보셨어요??",
          "speaker_id": "2",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100029612.1.12",
          "form": "후기만 봤어요",
          "original_form": "후기만 봤어요",
          "speaker_id": "1",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100029612.1.13",
          "form": "그래서 샀다가 불량 걸릴까보ㅏ 걱정즁",
          "original_form": "그래서 샀다가 불량 걸릴까보ㅏ 걱정즁",
          "speaker_id": "1",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100029612.1.14",
          "form": "헐 불량이라니 ㅠㅠ",
          "original_form": "헐 불량이라니 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 15:15"
        },
        {
          "id": "MDRW2100029612.1.15",
          "form": "저 이제 실내자전거 탈 건데 너무 귀찮네요...",
          "original_form": "저 이제 실내자전거 탈 건데 너무 귀찮네요...",
          "speaker_id": "2",
          "time": "20210903 15:15"
        },
        {
          "id": "MDRW2100029612.1.16",
          "form": "보통 얼마동안 타시나요?",
          "original_form": "보통 얼마동안 타시나요?",
          "speaker_id": "1",
          "time": "20210903 15:18"
        },
        {
          "id": "MDRW2100029612.1.17",
          "form": "1시간 정도 타요",
          "original_form": "1시간 정도 타요",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100029612.1.18",
          "form": "땀으로 샤워해요 ㅎㅎ",
          "original_form": "땀으로 샤워해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100029612.1.19",
          "form": "우와 진짜 운동이 되겠어요",
          "original_form": "우와 진짜 운동이 되겠어요",
          "speaker_id": "1",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100029612.1.20",
          "form": "당근에 매물이있긴하네요",
          "original_form": "당근에 매물이있긴하네요",
          "speaker_id": "1",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100029612.1.21",
          "form": "운동하고 샤워하고 나오니까 상쾌하네용",
          "original_form": "운동하고 샤워하고 나오니까 상쾌하네용",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100029612.1.22",
          "form": "오 저렴한가요??",
          "original_form": "오 저렴한가요??",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100029612.1.23",
          "form": "반값도 있긴한데 직접 옮기래서 포기...  ㅜㅜㅋㅋ",
          "original_form": "반값도 있긴한데 직접 옮기래서 포기...  ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029612.1.24",
          "form": "헉... 직접 옮기라는 건 너무한 거 아닌가요 ㅠㅠ",
          "original_form": "헉... 직접 옮기라는 건 너무한 거 아닌가요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100029612.1.25",
          "form": "살은 그냥 몸에 붙여놔야... ㅋㅋㅋㅋ",
          "original_form": "살은 그냥 몸에 붙여놔야... ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100029612.1.26",
          "form": "ㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100029612.1.27",
          "form": "자전거 말고 사고 싶은 거 있나요??",
          "original_form": "자전거 말고 사고 싶은 거 있나요??",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100029612.1.28",
          "form": "플립3이랑 노트북이요 님은요?",
          "original_form": "플립3이랑 노트북이요 님은요?",
          "speaker_id": "1",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100029612.1.29",
          "form": "저는 가디건이나 맨투맨 새로 사고 싶네용",
          "original_form": "저는 가디건이나 맨투맨 새로 사고 싶네용",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100029612.1.30",
          "form": "플립3 너무 예쁘더라구요 ㅜㅜ",
          "original_form": "플립3 너무 예쁘더라구요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100029612.1.31",
          "form": "추워서 가을옷 점점 지륾대가 온듯요 ㅎㅎㅎ",
          "original_form": "추워서 가을옷 점점 지륾대가 온듯요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100029612.1.32",
          "form": "저도 맨투맨 하나삿어요",
          "original_form": "저도 맨투맨 하나삿어요",
          "speaker_id": "1",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100029612.1.33",
          "form": "근데 배터리가 용량이 작더라구요",
          "original_form": "근데 배터리가 용량이 작더라구요",
          "speaker_id": "1",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100029612.1.34",
          "form": "지름신 누르는중 ㅠㅠㅠㅠ",
          "original_form": "지름신 누르는중 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100029612.1.35",
          "form": "그쵸 저도 뭘 살까 구경 중 ㅎㅎ",
          "original_form": "그쵸 저도 뭘 살까 구경 중 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 13:52"
        },
        {
          "id": "MDRW2100029612.1.36",
          "form": "아... 아쉽네여 케이스도 진짜 예쁜 거 많던데 ㅠㅠ",
          "original_form": "아... 아쉽네여 케이스도 진짜 예쁜 거 많던데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 13:52"
        },
        {
          "id": "MDRW2100029612.1.37",
          "form": "요즘 바람이 엄청 불더라구요 ㅎㅎ",
          "original_form": "요즘 바람이 엄청 불더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:33"
        },
        {
          "id": "MDRW2100029612.1.38",
          "form": "케이스 진짜 이쁜거 많더라구요 ㅎㅎ",
          "original_form": "케이스 진짜 이쁜거 많더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:33"
        },
        {
          "id": "MDRW2100029612.1.39",
          "form": "ㅎㅎ 선선해져서 너무 좋아여",
          "original_form": "ㅎㅎ 선선해져서 너무 좋아여",
          "speaker_id": "2",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100029612.1.40",
          "form": "케이스도 사고 싶네요 투명 케이스 너무 더러워져서 ㅠㅠ",
          "original_form": "케이스도 사고 싶네요 투명 케이스 너무 더러워져서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 15:50"
        }
      ]
    }
