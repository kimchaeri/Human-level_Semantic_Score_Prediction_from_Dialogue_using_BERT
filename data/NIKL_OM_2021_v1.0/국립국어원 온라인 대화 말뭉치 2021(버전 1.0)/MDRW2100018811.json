      "utterance": [
        {
          "id": "MDRW2100018811.1.1",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018811.1.2",
          "form": "지금 먹고 있는 중이에요 ㅎㅎㅎ",
          "original_form": "지금 먹고 있는 중이에요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018811.1.3",
          "form": "뭐드세요?",
          "original_form": "뭐드세요?",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100018811.1.4",
          "form": "치즈케이크랑 커피요~",
          "original_form": "치즈케이크랑 커피요~",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018811.1.5",
          "form": "님은 점심 드셨어요?",
          "original_form": "님은 점심 드셨어요?",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018811.1.6",
          "form": "네 방금 또띠아 먹었어요!",
          "original_form": "네 방금 또띠아 먹었어요!",
          "speaker_id": "1",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018811.1.7",
          "form": "케이크 드시면 배 안고프세요?",
          "original_form": "케이크 드시면 배 안고프세요?",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018811.1.8",
          "form": "또띠아 맛있겠다 ㅎㅎㅎ",
          "original_form": "또띠아 맛있겠다 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018811.1.9",
          "form": "어.. 케이크가 열량이 높아요",
          "original_form": "어.. 케이크가 열량이 높아요",
          "speaker_id": "2",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018811.1.10",
          "form": "앗.. 그렇긴해요ㅋㅋ",
          "original_form": "앗.. 그렇긴해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018811.1.11",
          "form": "저는 배가 안차더라구요ㅠ",
          "original_form": "저는 배가 안차더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018811.1.12",
          "form": "그리고 오늘 불금이라서 치킨 시켜먹어야죠~!!",
          "original_form": "그리고 오늘 불금이라서 치킨 시켜먹어야죠~!!",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018811.1.13",
          "form": "퇴근하면서 치킨 픽업하려구요 ㅎㅎ",
          "original_form": "퇴근하면서 치킨 픽업하려구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018811.1.14",
          "form": "크으! 저는 닭갈비요!!",
          "original_form": "크으! 저는 닭갈비요!!",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018811.1.15",
          "form": "오늘은 닭이죠",
          "original_form": "오늘은 닭이죠",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018811.1.16",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018811.1.17",
          "form": "닭갈비는 어디서 먹게요?",
          "original_form": "닭갈비는 어디서 먹게요?",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018811.1.18",
          "form": "춘천닭갈비요!",
          "original_form": "춘천닭갈비요!",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018811.1.19",
          "form": "오~춘천닭갈비!",
          "original_form": "오~춘천닭갈비!",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018811.1.20",
          "form": "재단장 했다는데 안먹으면 안된달까요",
          "original_form": "재단장 했다는데 안먹으면 안된달까요",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018811.1.21",
          "form": "그거 넓적다리로 된거 맞아요?",
          "original_form": "그거 넓적다리로 된거 맞아요?",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018811.1.22",
          "form": "숯불에 샤악 구워주고??",
          "original_form": "숯불에 샤악 구워주고??",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018811.1.23",
          "form": "있다더라구요",
          "original_form": "있다더라구요",
          "speaker_id": "1",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100018811.1.24",
          "form": "기대중입니닷",
          "original_form": "기대중입니닷",
          "speaker_id": "1",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100018811.1.25",
          "form": "드셔보셨나요?",
          "original_form": "드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100018811.1.26",
          "form": "아 그럼요 동네에 있오요 ㅋㅋㅋ",
          "original_form": "아 그럼요 동네에 있오요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018811.1.27",
          "form": "진짜 너무 맛있어요 야들야들하고~",
          "original_form": "진짜 너무 맛있어요 야들야들하고~",
          "speaker_id": "2",
          "time": "20210827 13:37"
        }
      ]
    }
  ]
