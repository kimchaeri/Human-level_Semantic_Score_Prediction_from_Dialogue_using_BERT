      "utterance": [
        {
          "id": "MDRW2100004308.1.1",
          "form": "주제 바꿔봤어",
          "original_form": "주제 바꿔봤어",
          "speaker_id": "1",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004308.1.2",
          "form": "오늘 비 진짜 많이 오지 않니...",
          "original_form": "오늘 비 진짜 많이 오지 않니...",
          "speaker_id": "1",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004308.1.3",
          "form": "밤에 선풍기 틀면 요새는 좀 춥더라",
          "original_form": "밤에 선풍기 틀면 요새는 좀 춥더라",
          "speaker_id": "1",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004308.1.4",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004308.1.5",
          "form": "갑자기 날이 확 바뀜..",
          "original_form": "갑자기 날이 확 바뀜..",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004308.1.6",
          "form": "그래서 조만간 가을 옷 사려는데",
          "original_form": "그래서 조만간 가을 옷 사려는데",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004308.1.7",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004308.1.8",
          "form": "가을 옷 배송 올 때 쯤에 겨울 되는 거 아니겠지?",
          "original_form": "가을 옷 배송 올 때 쯤에 겨울 되는 거 아니겠지?",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004308.1.9",
          "form": "가능성있다..",
          "original_form": "가능성있다..",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004308.1.10",
          "form": "나는 가디건 사려고",
          "original_form": "나는 가디건 사려고",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004308.1.11",
          "form": "카디건 좋지 한번 사두면 오래 입을 수 있잖아",
          "original_form": "카디건 좋지 한번 사두면 오래 입을 수 있잖아",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004308.1.12",
          "form": "오홍",
          "original_form": "오홍",
          "speaker_id": "2",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004308.1.13",
          "form": "그건 몰랐다",
          "original_form": "그건 몰랐다",
          "speaker_id": "2",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004308.1.14",
          "form": "요새 밖에 잘 안 나가서 계절감각이 떨어지는 거 같아",
          "original_form": "요새 밖에 잘 안 나가서 계절감각이 떨어지는 거 같아",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004308.1.15",
          "form": "가디건을 입어 본 적이 없어서",
          "original_form": "가디건을 입어 본 적이 없어서",
          "speaker_id": "2",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004308.1.16",
          "form": "처음 알았네",
          "original_form": "처음 알았네",
          "speaker_id": "2",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004308.1.17",
          "form": "맞아!!",
          "original_form": "맞아!!",
          "speaker_id": "2",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004308.1.18",
          "form": "이러다가 오랜만에 여름옷 입고 나갔는데 바깥 날씨는 쌀쌀한 가을 돼버릴 지도",
          "original_form": "이러다가 오랜만에 여름옷 입고 나갔는데 바깥 날씨는 쌀쌀한 가을 돼버릴 지도",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004308.1.19",
          "form": "앜",
          "original_form": "앜",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004308.1.20",
          "form": "나는 요새 밤낮이 애매하게",
          "original_form": "나는 요새 밤낮이 애매하게",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004308.1.21",
          "form": "바껴서",
          "original_form": "바껴서",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004308.1.22",
          "form": "자고 일어나서 좀 있으면 아버지가 오시거든",
          "original_form": "자고 일어나서 좀 있으면 아버지가 오시거든",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004308.1.23",
          "form": "아버지가 오늘 비 많이 오네하면",
          "original_form": "아버지가 오늘 비 많이 오네하면",
          "speaker_id": "2",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004308.1.24",
          "form": "잉?? 비가 왔어??",
          "original_form": "잉?? 비가 왔어??",
          "speaker_id": "2",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004308.1.25",
          "form": "이러고 있어ㅋㅋㅋㅋ",
          "original_form": "이러고 있어ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004308.1.26",
          "form": "오늘은 새벽부터 비 많이 오더라",
          "original_form": "오늘은 새벽부터 비 많이 오더라",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004308.1.27",
          "form": "여름에 거의 안 온 거",
          "original_form": "여름에 거의 안 온 거",
          "speaker_id": "2",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004308.1.28",
          "form": "오늘 다 내리는 기분",
          "original_form": "오늘 다 내리는 기분",
          "speaker_id": "2",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004308.1.29",
          "form": "맞아 다음주까지는 올 거 같다던데",
          "original_form": "맞아 다음주까지는 올 거 같다던데",
          "speaker_id": "1",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004308.1.30",
          "form": "자취방 가기 전에는 그쳤으면 좋겠다",
          "original_form": "자취방 가기 전에는 그쳤으면 좋겠다",
          "speaker_id": "1",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004308.1.31",
          "form": "그러면 좋겠다",
          "original_form": "그러면 좋겠다",
          "speaker_id": "2",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004308.1.32",
          "form": "보니까",
          "original_form": "보니까",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004308.1.33",
          "form": "22일~27일까지는 비 오네",
          "original_form": "22일~27일까지는 비 오네",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004308.1.34",
          "form": "길게도 온다..",
          "original_form": "길게도 온다..",
          "speaker_id": "2",
          "time": "20210821 16:04"
        }
      ]
    }
