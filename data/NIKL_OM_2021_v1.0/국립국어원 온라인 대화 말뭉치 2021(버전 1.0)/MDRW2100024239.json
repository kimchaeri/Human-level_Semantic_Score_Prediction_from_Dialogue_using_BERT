        {
          "id": "MDRW2100024239.1.1",
          "form": "싸이월드 하셨나요?",
          "original_form": "싸이월드 하셨나요?",
          "speaker_id": "1",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100024239.1.2",
          "form": "옛날에요?",
          "original_form": "옛날에요?",
          "speaker_id": "2",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100024239.1.3",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100024239.1.4",
          "form": "네~제가 거의 고등학생 유행이었어요",
          "original_form": "네~제가 거의 고등학생 유행이었어요",
          "speaker_id": "2",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100024239.1.5",
          "form": "다시 하실건가요?",
          "original_form": "다시 하실건가요?",
          "speaker_id": "1",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100024239.1.6",
          "form": "부활하면 옛날처럼 열심히는 못할것 같아요 ㅎ",
          "original_form": "부활하면 옛날처럼 열심히는 못할것 같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:38"
        },
        {
          "id": "MDRW2100024239.1.7",
          "form": "전 로그인도 못하겠어요",
          "original_form": "전 로그인도 못하겠어요",
          "speaker_id": "1",
          "time": "20210830 23:38"
        },
        {
          "id": "MDRW2100024239.1.8",
          "form": "ㅋㅋ 근데 옛날에 어떤거 써놨을지 궁금해요",
          "original_form": "ㅋㅋ 근데 옛날에 어떤거 써놨을지 궁금해요",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024239.1.9",
          "form": "으앗 넘 오글거릴거같아요",
          "original_form": "으앗 넘 오글거릴거같아요",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024239.1.10",
          "form": "네~ㅋㅋ 저두 ㅋㅋ 부활하면 일기는 싹 지워야 겠어요,ㅋ",
          "original_form": "네~ㅋㅋ 저두 ㅋㅋ 부활하면 일기는 싹 지워야 겠어요,ㅋ",
          "speaker_id": "2",
