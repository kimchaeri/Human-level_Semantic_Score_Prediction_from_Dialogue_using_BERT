      "utterance": [
        {
          "id": "MDRW2100016698.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 14:49"
        },
        {
          "id": "MDRW2100016698.1.2",
          "form": "네 좋아해요 최근에 하동다녀왓어요",
          "original_form": "네 좋아해요 최근에 하동다녀왓어요",
          "speaker_id": "1",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016698.1.3",
          "form": "하동! 안그래도 재첩국 너무 먹고싶었는데ㅠㅠ 재첩국 드셨나요",
          "original_form": "하동! 안그래도 재첩국 너무 먹고싶었는데ㅠㅠ 재첩국 드셨나요",
          "speaker_id": "2",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016698.1.4",
          "form": "네 ㅋㅋ재첩국 좋아하세요?",
          "original_form": "네 ㅋㅋ재첩국 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016698.1.5",
          "form": "네! 하동하면 저는 재첩국부터 생각나더라구요",
          "original_form": "네! 하동하면 저는 재첩국부터 생각나더라구요",
          "speaker_id": "2",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016698.1.6",
          "form": "하동에서 어디어디가셨나요?",
          "original_form": "하동에서 어디어디가셨나요?",
          "speaker_id": "2",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016698.1.7",
          "form": "매암제다원 아시나요?",
          "original_form": "매암제다원 아시나요?",
          "speaker_id": "1",
          "time": "20210826 14:54"
        },
        {
          "id": "MDRW2100016698.1.8",
          "form": "앗 처음듣는데 어떤곳인가요?",
          "original_form": "앗 처음듣는데 어떤곳인가요?",
          "speaker_id": "2",
          "time": "20210826 14:54"
        },
        {
          "id": "MDRW2100016698.1.9",
          "form": "다원인데 유명하더라구요",
          "original_form": "다원인데 유명하더라구요",
          "speaker_id": "1",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016698.1.10",
          "form": "name1님은 어떠셨어요?",
          "original_form": "&name1&님은 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016698.1.11",
          "form": "하동이 다원으로도 유명해요 저도 첨알았어요",
          "original_form": "하동이 다원으로도 유명해요 저도 첨알았어요",
          "speaker_id": "1",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016698.1.12",
          "form": "거기도 좋았고. 도심다원이라는 다원이 정말 좋았어요",
          "original_form": "거기도 좋았고. 도심다원이라는 다원이 정말 좋았어요",
          "speaker_id": "1",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016698.1.13",
          "form": "와... 다원으로도 유명하군요 다음에 하동갈때 가봐야겠어요",
          "original_form": "와... 다원으로도 유명하군요 다음에 하동갈때 가봐야겠어요",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016698.1.14",
          "form": "네 꼭 가보세요",
          "original_form": "네 꼭 가보세요",
          "speaker_id": "1",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016698.1.15",
          "form": "최근에 여행다녀오신곳 있어요?",
          "original_form": "최근에 여행다녀오신곳 있어요?",
          "speaker_id": "1",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016698.1.16",
          "form": "코로나라 못간지 꽤 되었지만 마지막은 전주갔었어요",
          "original_form": "코로나라 못간지 꽤 되었지만 마지막은 전주갔었어요",
          "speaker_id": "2",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016698.1.17",
          "form": "와 전주도 좋죠 어디가셨어요?",
          "original_form": "와 전주도 좋죠 어디가셨어요?",
          "speaker_id": "1",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016698.1.18",
          "form": "한옥거리랑 시장갔었어요 청년마켓도가구요",
          "original_form": "한옥거리랑 시장갔었어요 청년마켓도가구요",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016698.1.19",
          "form": "그때 시장에서 콩나물국밥 먹은거 생각나네요",
          "original_form": "그때 시장에서 콩나물국밥 먹은거 생각나네요",
          "speaker_id": "1",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016698.1.20",
          "form": "엄청 맛있게 먹은 기억이ㅎ",
          "original_form": "엄청 맛있게 먹은 기억이ㅎ",
          "speaker_id": "1",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016698.1.21",
          "form": "저도 시장에서 콩나물국밥 먹었는데",
          "original_form": "저도 시장에서 콩나물국밥 먹었는데",
          "speaker_id": "2",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016698.1.22",
          "form": "수란에 오징어 너무 맛밌었어요",
          "original_form": "수란에 오징어 너무 맛밌었어요",
          "speaker_id": "2",
          "time": "20210826 15:03"
        },
        {
          "id": "MDRW2100016698.1.23",
          "form": "저두 수란하고 엄청 맛있게먹었어요",
          "original_form": "저두 수란하고 엄청 맛있게먹었어요",
