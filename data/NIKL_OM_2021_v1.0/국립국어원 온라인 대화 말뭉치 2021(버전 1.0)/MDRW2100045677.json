        },
        {
          "id": "MDRW2100045677.1.2",
          "form": "저는 지금 초밥이 먹고 싶어요",
          "original_form": "저는 지금 초밥이 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210908 05:17"
        },
        {
          "id": "MDRW2100045677.1.3",
          "form": "연어 잡으러 가고싶어요 ㅋㅋㅋ",
          "original_form": "연어 잡으러 가고싶어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:17"
        },
        {
          "id": "MDRW2100045677.1.4",
          "form": "악 ㅋㅋㅋㅋ",
          "original_form": "악 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:20"
        },
        {
          "id": "MDRW2100045677.1.5",
          "form": "연어팟에 저도 끼워주세요",
          "original_form": "연어팟에 저도 끼워주세요",
          "speaker_id": "2",
          "time": "20210908 05:21"
        },
        {
          "id": "MDRW2100045677.1.6",
          "form": "저도 요즘 초밥이랑 연어랑 너무 먹고싶네요",
          "original_form": "저도 요즘 초밥이랑 연어랑 너무 먹고싶네요",
          "speaker_id": "2",
          "time": "20210908 05:21"
        },
        {
          "id": "MDRW2100045677.1.7",
          "form": "으엉 저두요ㅠㅜㅜㅜㅠㅜㅠㅠㅠ",
          "original_form": "으엉 저두요ㅠㅜㅜㅜㅠㅜㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:21"
        },
        {
          "id": "MDRW2100045677.1.8",
          "form": "훈제가 좋으세요 그냥이 좋으세요?",
          "original_form": "훈제가 좋으세요 그냥이 좋으세요?",
          "speaker_id": "1",
          "time": "20210908 05:21"
        },
        {
          "id": "MDRW2100045677.1.9",
          "form": "저는 생연어가 좋아요 ㅎㅎ",
          "original_form": "저는 생연어가 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 05:22"
        },
        {
          "id": "MDRW2100045677.1.10",
          "form": "하지만 훈제도 주면 먹습니다",
          "original_form": "하지만 훈제도 주면 먹습니다",
          "speaker_id": "2",
          "time": "20210908 05:22"
        },
        {
          "id": "MDRW2100045677.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:22"
        },
        {
          "id": "MDRW2100045677.1.12",
          "form": "저두요 누가 연어 샐러드 만들어주면 좋겠어",
          "original_form": "저두요 누가 연어 샐러드 만들어주면 좋겠어",
          "speaker_id": "1",
          "time": "20210908 05:23"
        },
        {
          "id": "MDRW2100045677.1.13",
          "form": "퍼먹고 싶에여",
          "original_form": "퍼먹고 싶에여",
          "speaker_id": "1",
          "time": "20210908 05:23"
        },
        {
          "id": "MDRW2100045677.1.14",
          "form": "으악 저두요..",
          "original_form": "으악 저두요..",
          "speaker_id": "2",
          "time": "20210908 05:24"
        },
        {
          "id": "MDRW2100045677.1.15",
          "form": "요즘 샐러드 파는거 잘나오더라구요",
          "original_form": "요즘 샐러드 파는거 잘나오더라구요",
          "speaker_id": "2",
          "time": "20210908 05:25"
        },
        {
          "id": "MDRW2100045677.1.16",
          "form": "저도 한 번 사먹어보려구요 ㅋㅋㅋ",
          "original_form": "저도 한 번 사먹어보려구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:26"
        },
        {
          "id": "MDRW2100045677.1.17",
          "form": "맞아요  맞아요ㅠ고기 그만먹고 살랴",
          "original_form": "맞아요  맞아요ㅠ고기 그만먹고 살랴",
          "speaker_id": "1",
          "time": "20210908 05:27"
        },
        {
          "id": "MDRW2100045677.1.18",
          "form": "그만먹고 살아야하는데",
          "original_form": "그만먹고 살아야하는데",
          "speaker_id": "1",
          "time": "20210908 05:27"
        },
        {
          "id": "MDRW2100045677.1.19",
          "form": "마음대로 안돼요ㅠㅠ",
          "original_form": "마음대로 안돼요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:27"
        },
        {
          "id": "MDRW2100045677.1.20",
          "form": "아하 ㅋㅋㅋㅋ 저는.. 편식을 안 해서 샐러드도 좋고 고기도 좋아요 ㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋ 저는.. 편식을 안 해서 샐러드도 좋고 고기도 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:28"
        },
        {
          "id": "MDRW2100045677.1.21",
          "form": "그냥 다 잘 먹는 타입..ㅎ",
          "original_form": "그냥 다 잘 먹는 타입..ㅎ",
          "speaker_id": "2",
          "time": "20210908 05:29"
        },
        {
          "id": "MDRW2100045677.1.22",
          "form": "저두 둘다 좋은데 그래도 고기가 더 좋아요 ㅋㅋㅋㅋ",
          "original_form": "저두 둘다 좋은데 그래도 고기가 더 좋아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:30"
        },
        {
          "id": "MDRW2100045677.1.23",
          "form": "앗 저도요 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 저도요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:33"
        },
        {
          "id": "MDRW2100045677.1.24",
          "form": "요즘 양꼬치랑 삼겹살도 넘 먹고싶어요",
          "original_form": "요즘 양꼬치랑 삼겹살도 넘 먹고싶어요",
          "speaker_id": "2",
          "time": "20210908 05:33"
        },
        {
          "id": "MDRW2100045677.1.25",
          "form": "삼걉살은 진짜 ㅜ 코로나때문에 고깃집을 별로 못간것같네요",
          "original_form": "삼걉살은 진짜 ㅜ 코로나때문에 고깃집을 별로 못간것같네요",
          "speaker_id": "2",
          "time": "20210908 05:33"
        },
        {
          "id": "MDRW2100045677.1.26",
          "form": "집에서 먹어야죠 머ㅠㅠㅠㅠ",
          "original_form": "집에서 먹어야죠 머ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045677.1.27",
          "form": "막고 열심히 바닥 닦기 ㅠㅋㅋㅋ",
          "original_form": "막고 열심히 바닥 닦기 ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045677.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ하 근데 그 고깃집에서 구워먹는 게 너무 그리워요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ하 근데 그 고깃집에서 구워먹는 게 너무 그리워요",
          "speaker_id": "2",
          "time": "20210908 05:35"
        },
        {
          "id": "MDRW2100045677.1.29",
          "form": "먹을수는 있지만 다녀와서 벌벌 떠는것보다는 나으니까 어쩔수없죠 ㅠㅠㅠㅠㅠ 저도 가서 먹고 싶네요",
          "original_form": "먹을수는 있지만 다녀와서 벌벌 떠는것보다는 나으니까 어쩔수없죠 ㅠㅠㅠㅠㅠ 저도 가서 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210908 05:37"
        },
        {
          "id": "MDRW2100045677.1.30",
          "form": "맞아요.. 집에서 구워먹긴 귀찮아서 그냥 안먹고있네요 ㅋㅋㅋㅋ",
          "original_form": "맞아요.. 집에서 구워먹긴 귀찮아서 그냥 안먹고있네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:37"
        },
        {
          "id": "MDRW2100045677.1.31",
          "form": "기름튀는 것도 그렇고",
          "original_form": "기름튀는 것도 그렇고",
          "speaker_id": "2",
          "time": "20210908 05:38"
        },
        {
          "id": "MDRW2100045677.1.32",
          "form": "냄새도 잘 안 빠져서 ㅋㅋ ㅜ",
          "original_form": "냄새도 잘 안 빠져서 ㅋㅋ ㅜ",
          "speaker_id": "2",
          "time": "20210908 05:38"
        },
        {
          "id": "MDRW2100045677.1.33",
          "form": "끕 아! 저도 그래서 배달 삼겹살 종종 시켜요",
          "original_form": "끕 아! 저도 그래서 배달 삼겹살 종종 시켜요",
          "speaker_id": "1",
          "time": "20210908 05:39"
        },
