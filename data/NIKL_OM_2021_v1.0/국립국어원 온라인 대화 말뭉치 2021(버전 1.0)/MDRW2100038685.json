      "utterance": [
        {
          "id": "MDRW2100038685.1.1",
          "form": "이야기 하니까",
          "original_form": "이야기 하니까",
          "speaker_id": "2",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100038685.1.2",
          "form": "너무 파스타가 먹고싶네요",
          "original_form": "너무 파스타가 먹고싶네요",
          "speaker_id": "2",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100038685.1.3",
          "form": "다음에 한번 도전해보세요 소스랑 파스타면 사면 뚝딱입니다",
          "original_form": "다음에 한번 도전해보세요 소스랑 파스타면 사면 뚝딱입니다",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100038685.1.4",
          "form": "좋습니다",
          "original_form": "좋습니다",
          "speaker_id": "2",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100038685.1.5",
          "form": "근데 최애 파스타가",
          "original_form": "근데 최애 파스타가",
          "speaker_id": "2",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100038685.1.6",
          "form": "뭔가요",
          "original_form": "뭔가요",
          "speaker_id": "2",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100038685.1.7",
          "form": "저는 알리오올리오요! 안질리고 계속 먹게되요ㅋㅋ",
          "original_form": "저는 알리오올리오요! 안질리고 계속 먹게되요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:17"
        },
        {
          "id": "MDRW2100038685.1.8",
          "form": "아 그게 은근 매력 있어요",
          "original_form": "아 그게 은근 매력 있어요",
          "speaker_id": "2",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100038685.1.9",
          "form": "전 로제도 좋아해요",
          "original_form": "전 로제도 좋아해요",
          "speaker_id": "2",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100038685.1.10",
          "form": "청정원 로제소스 맛있는데 드셔보셨나요?",
          "original_form": "청정원 로제소스 맛있는데 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100038685.1.11",
          "form": "아뇨 전 폰타나 크림만요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 전 폰타나 크림만요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100038685.1.12",
          "form": "집에 있어서 만들어 먹은 적 있거든요",
          "original_form": "집에 있어서 만들어 먹은 적 있거든요",
          "speaker_id": "2",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100038685.1.13",
          "form": "그러면 다음에 로제생각나시면 한번 도전해보세요 청정원 로제맛집입니다",
          "original_form": "그러면 다음에 로제생각나시면 한번 도전해보세요 청정원 로제맛집입니다",
          "speaker_id": "1",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100038685.1.14",
          "form": "그래야겠어요",
          "original_form": "그래야겠어요",
          "speaker_id": "2",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100038685.1.15",
          "form": "만들 때 새우 이런것도",
          "original_form": "만들 때 새우 이런것도",
          "speaker_id": "2",
          "time": "20210902 19:40"
        },
        {
          "id": "MDRW2100038685.1.16",
          "form": "넣으세요?",
          "original_form": "넣으세요?",
          "speaker_id": "2",
          "time": "20210902 19:40"
        },
        {
          "id": "MDRW2100038685.1.17",
          "form": "넣으면 좋겠지만 가난한 자취생에겐 사치죠",
          "original_form": "넣으면 좋겠지만 가난한 자취생에겐 사치죠",
          "speaker_id": "1",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100038685.1.18",
          "form": "그나마 양파는 넣어요ㅋㅋ",
          "original_form": "그나마 양파는 넣어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100038685.1.19",
          "form": "ㅋㅋㅋ양파도 맛있죠!",
          "original_form": "ㅋㅋㅋ양파도 맛있죠!",
          "speaker_id": "2",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100038685.1.20",
          "form": "폰타나 크림 소스는 맛있나요?",
          "original_form": "폰타나 크림 소스는 맛있나요?",
          "speaker_id": "1",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100038685.1.21",
          "form": "네 괜찮아요",
          "original_form": "네 괜찮아요",
          "speaker_id": "2",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100038685.1.22",
          "form": "근데 크림이라 그런지 먹다보니 느끼해서",
          "original_form": "근데 크림이라 그런지 먹다보니 느끼해서",
          "speaker_id": "2",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100038685.1.23",
          "form": "김치랑 같이 먹었어요ㅋㅋ",
          "original_form": "김치랑 같이 먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:17"
        }
      ]
    }
