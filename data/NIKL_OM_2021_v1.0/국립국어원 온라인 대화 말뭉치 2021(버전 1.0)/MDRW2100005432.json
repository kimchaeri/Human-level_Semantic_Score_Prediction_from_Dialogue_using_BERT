      "utterance": [
        {
          "id": "MDRW2100005432.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005432.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005432.1.3",
          "form": "제가 아는 사람 반려견 이름이",
          "original_form": "제가 아는 사람 반려견 이름이",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005432.1.4",
          "form": "name3라서 생각났어요...",
          "original_form": "&name3&라서 생각났어요...",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005432.1.5",
          "form": "마침 주제가 반려동물이더라고요",
          "original_form": "마침 주제가 반려동물이더라고요",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005432.1.6",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005432.1.7",
          "form": "아하 ㅋㅋㅋㅋ 그러셨군요",
          "original_form": "아하 ㅋㅋㅋㅋ 그러셨군요",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005432.1.8",
          "form": "name3 귀여운 이름이네요 ㅎㅎ",
          "original_form": "&name3& 귀여운 이름이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005432.1.9",
          "form": "반려동물 이름으론 처음 들어봐요 유니크하고 귀엽네요 ㅎㅎ",
          "original_form": "반려동물 이름으론 처음 들어봐요 유니크하고 귀엽네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005432.1.10",
          "form": "그쵸 name3는 반려동물 이름으로 흔하지 않죠 ㅎㅎ",
          "original_form": "그쵸 &name3&는 반려동물 이름으로 흔하지 않죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005432.1.11",
          "form": "반려동물 키워보신 적 있으세요??",
          "original_form": "반려동물 키워보신 적 있으세요??",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005432.1.12",
          "form": "어렸을때 물고기랑 햄스터는 키워봤는데 그거 말곤 없어요 ㅋㅋㅋ",
          "original_form": "어렸을때 물고기랑 햄스터는 키워봤는데 그거 말곤 없어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005432.1.13",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005432.1.14",
          "form": "오 저도 어릴 때 햄스터 키웠어요",
          "original_form": "오 저도 어릴 때 햄스터 키웠어요",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005432.1.15",
          "form": "근데 두마리 같이 두니까 ㅈ한마리가 잡아먹.....었더라고요",
          "original_form": "근데 두마리 같이 두니까 ㅈ한마리가 잡아먹.....었더라고요",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005432.1.16",
          "form": "어릴 때 충격 받았어요 ㅋㅋㅋ ㅠㅠㅠ",
          "original_form": "어릴 때 충격 받았어요 ㅋㅋㅋ ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005432.1.17",
          "form": "헉..",
          "original_form": "헉..",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005432.1.18",
          "form": "맞아요 그런다더라고요..",
          "original_form": "맞아요 그런다더라고요..",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005432.1.19",
          "form": "밥 안주면 자기 애기도 잡아먹는다고 하더라고요ㅜ",
          "original_form": "밥 안주면 자기 애기도 잡아먹는다고 하더라고요ㅜ",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005432.1.20",
          "form": "ㅠㅠ 맞아요 7살 때",
          "original_form": "ㅠㅠ 맞아요 7살 때",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005432.1.21",
          "form": "어린이집 가려다가 계단 내려가는데",
          "original_form": "어린이집 가려다가 계단 내려가는데",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005432.1.22",
          "form": "마당에 있는 햄스터 시체 보고 충격 먹어서",
          "original_form": "마당에 있는 햄스터 시체 보고 충격 먹어서",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005432.1.23",
          "form": "소리 빽 질렀던 게 아직도 기억 나요",
          "original_form": "소리 빽 질렀던 게 아직도 기억 나요",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005432.1.24",
          "form": "으악 ㅜ",
          "original_form": "으악 ㅜ",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005432.1.25",
          "form": "근데 저도 어렸을때 햄쥐 키우다가",
          "original_form": "근데 저도 어렸을때 햄쥐 키우다가",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005432.1.26",
          "form": "어린마음에 햄스터에게 자유를 주려다..",
          "original_form": "어린마음에 햄스터에게 자유를 주려다..",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005432.1.27",
          "form": "사고로 햄쥐 죽은적이 있어요 ㅜ 흑",
          "original_form": "사고로 햄쥐 죽은적이 있어요 ㅜ 흑",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005432.1.28",
          "form": "헉 ㅠㅠㅠㅠ 안 좋은 기억으로 남으셨겠네요....",
          "original_form": "헉 ㅠㅠㅠㅠ 안 좋은 기억으로 남으셨겠네요....",
          "speaker_id": "1",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005432.1.29",
          "form": "ㅠㅠ네.. 제 실수때문에 죽은거라 그땐 너무 슬펐어요 흑",
          "original_form": "ㅠㅠ네.. 제 실수때문에 죽은거라 그땐 너무 슬펐어요 흑",
          "speaker_id": "2",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005432.1.30",
          "form": "name1님은 반려동물 키울 계획같은 건 없으신가요? 뭐 당장 아니고 나중에라도 키우고 싶다거나",
          "original_form": "&name1&님은 반려동물 키울 계획같은 건 없으신가요? 뭐 당장 아니고 나중에라도 키우고 싶다거나",
          "speaker_id": "2",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005432.1.31",
          "form": "저는 초딩 때 이구아나 잘 못 키워서",
          "original_form": "저는 초딩 때 이구아나 잘 못 키워서",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005432.1.32",
          "form": "죽었던 게 기억에 남아서",
          "original_form": "죽었던 게 기억에 남아서",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005432.1.33",
          "form": "나중에 다시 잘 키우고 싶어요ㅠ",
          "original_form": "나중에 다시 잘 키우고 싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005432.1.34",
          "form": "그때 막 조명 같은 거 비싸서",
          "original_form": "그때 막 조명 같은 거 비싸서",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005432.1.35",
          "form": "부모님이 안 사주셨는데",
          "original_form": "부모님이 안 사주셨는데",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005432.1.36",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005432.1.37",
          "form": "아이구 흑흑",
          "original_form": "아이구 흑흑",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005432.1.38",
          "form": "저는 뭔가.. 동물을 좋아하긴 하지만",
          "original_form": "저는 뭔가.. 동물을 좋아하긴 하지만",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005432.1.39",
          "form": "제대로 케어해줄 자신이 없어서 그냥",
          "original_form": "제대로 케어해줄 자신이 없어서 그냥",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005432.1.40",
          "form": "인스타나 인터넷에서 귀여운 동물 짤들 보는 걸로 만족하려고요 ㅋㅋㅋ",
          "original_form": "인스타나 인터넷에서 귀여운 동물 짤들 보는 걸로 만족하려고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005432.1.41",
          "form": "고등학생때까지만 해도 고양이 엄청 키우고 싶었는데 ㅋㅋㅋ 생각이 바뀌었어요",
          "original_form": "고등학생때까지만 해도 고양이 엄청 키우고 싶었는데 ㅋㅋㅋ 생각이 바뀌었어요",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005432.1.42",
          "form": "저도요ㅠㅠ 그냥 가끔 짤로 만족하는 게 최고인 것 같아요",
          "original_form": "저도요ㅠㅠ 그냥 가끔 짤로 만족하는 게 최고인 것 같아요",
          "speaker_id": "1",
