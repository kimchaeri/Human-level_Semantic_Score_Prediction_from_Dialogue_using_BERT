      "utterance": [
        {
          "id": "MDRW2100038141.1.1",
          "form": "그래서 또 골프는 재미있어",
          "original_form": "그래서 또 골프는 재미있어",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038141.1.2",
          "form": "골프가 건강에 좋대",
          "original_form": "골프가 건강에 좋대",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038141.1.3",
          "form": "야외 스포츠라 그런지 확실히 몸이 좋아지는 기분",
          "original_form": "야외 스포츠라 그런지 확실히 몸이 좋아지는 기분",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038141.1.4",
          "form": "필드 나가면 많이 걸어서 좋대",
          "original_form": "필드 나가면 많이 걸어서 좋대",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038141.1.5",
          "form": "그러네 간식 만 조절하면은 몸이 좋겠다",
          "original_form": "그러네 간식 만 조절하면은 몸이 좋겠다",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038141.1.6",
          "form": "너는 필드 나가 실력이 되었니",
          "original_form": "너는 필드 나가 실력이 되었니",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038141.1.7",
          "form": "그럴 리가",
          "original_form": "그럴 리가",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038141.1.8",
          "form": "얼른 열심히 배워서 나도 같이 가자",
          "original_form": "얼른 열심히 배워서 나도 같이 가자",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038141.1.9",
          "form": "너는 필드 몇 번 가봤니",
          "original_form": "너는 필드 몇 번 가봤니",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038141.1.10",
          "form": "한번 밖에 안 가봤어요",
          "original_form": "한번 밖에 안 가봤어요",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038141.1.11",
          "form": "골프가 몸에 좋은 이유가 뭘까",
          "original_form": "골프가 몸에 좋은 이유가 뭘까",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038141.1.12",
          "form": "팔운동이 열심히 될 듯",
          "original_form": "팔운동이 열심히 될 듯",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100038141.1.13",
          "form": "맞어 팔 그러더라",
          "original_form": "맞어 팔 그러더라",
          "speaker_id": "2",
