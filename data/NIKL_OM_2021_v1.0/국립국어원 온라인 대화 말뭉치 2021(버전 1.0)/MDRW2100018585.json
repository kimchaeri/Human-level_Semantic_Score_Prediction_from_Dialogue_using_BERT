      "utterance": [
        {
          "id": "MDRW2100018585.1.1",
          "form": "오늘 점심 뭐 드셨나요",
          "original_form": "오늘 점심 뭐 드셨나요",
          "speaker_id": "1",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100018585.1.2",
          "form": "라면이요. 배달음식 시키려다가 때를 놓쳤어요",
          "original_form": "라면이요. 배달음식 시키려다가 때를 놓쳤어요",
          "speaker_id": "2",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100018585.1.3",
          "form": "name1는 뭐 드셨어요?",
          "original_form": "&name1&는 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100018585.1.4",
          "form": "삼겹살을 먹었어요",
          "original_form": "삼겹살을 먹었어요",
          "speaker_id": "1",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100018585.1.5",
          "form": "언니가 도드람 한돈을 샀는데 고소하더라고요",
          "original_form": "언니가 도드람 한돈을 샀는데 고소하더라고요",
          "speaker_id": "1",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100018585.1.6",
          "form": "도드람 한돈 맛있죠ㅜㅜ 비싸지만 값어치를 합니다",
          "original_form": "도드람 한돈 맛있죠ㅜㅜ 비싸지만 값어치를 합니다",
          "speaker_id": "2",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100018585.1.7",
          "form": "ㅎㅎ 드셔보셨군요.",
          "original_form": "ㅎㅎ 드셔보셨군요.",
          "speaker_id": "1",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100018585.1.8",
          "form": "일반 한돈과 다르더라고요",
          "original_form": "일반 한돈과 다르더라고요",
          "speaker_id": "1",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100018585.1.9",
          "form": "무슨 라면 드셨나요",
          "original_form": "무슨 라면 드셨나요",
          "speaker_id": "1",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100018585.1.10",
          "form": "전 가볍게 진라면에 계란이랑 파 송송 넣어서 먹었어요",
          "original_form": "전 가볍게 진라면에 계란이랑 파 송송 넣어서 먹었어요",
          "speaker_id": "2",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100018585.1.11",
          "form": "국물은 안먹고 면만 싹 건져먹었네요",
          "original_form": "국물은 안먹고 면만 싹 건져먹었네요",
          "speaker_id": "2",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100018585.1.12",
          "form": "국물은 원래 안 드세요?",
          "original_form": "국물은 원래 안 드세요?",
          "speaker_id": "1",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100018585.1.13",
          "form": "진라면 국물도 맛있는데ㅜ",
          "original_form": "진라면 국물도 맛있는데ㅜ",
          "speaker_id": "1",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100018585.1.14",
          "form": "국물까지 먹으면 배불러요ㅜㅜ",
          "original_form": "국물까지 먹으면 배불러요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100018585.1.15",
          "form": "아 끓일 때 물 조금 덜 넣고 우유 넣어도 맛있어요",
          "original_form": "아 끓일 때 물 조금 덜 넣고 우유 넣어도 맛있어요",
          "speaker_id": "2",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100018585.1.16",
          "form": "면도 꼬들함 유지됩니다",
          "original_form": "면도 꼬들함 유지됩니다",
          "speaker_id": "2",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100018585.1.17",
          "form": "우유와 물 비율이 얼마인가요?",
          "original_form": "우유와 물 비율이 얼마인가요?",
          "speaker_id": "1",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100018585.1.18",
          "form": "물 3/4, 우유 1/4이요",
          "original_form": "물 3/4, 우유 1/4이요",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100018585.1.19",
          "form": "무슨 라면 좋아하세요?",
          "original_form": "무슨 라면 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100018585.1.20",
          "form": "진매요!",
          "original_form": "진매요!",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100018585.1.21",
          "form": "진라면 매운맛은 사랑입니다",
          "original_form": "진라면 매운맛은 사랑입니다",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100018585.1.22",
          "form": "아 같으시네요 배우신분",
          "original_form": "아 같으시네요 배우신분",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100018585.1.23",
          "form": "ㅎㅎ 순한맛은 겸상 안해요",
          "original_form": "ㅎㅎ 순한맛은 겸상 안해요",
