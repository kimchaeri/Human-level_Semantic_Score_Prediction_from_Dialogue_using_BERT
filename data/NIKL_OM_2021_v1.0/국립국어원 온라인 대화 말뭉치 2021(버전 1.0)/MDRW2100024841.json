      "utterance": [
        {
          "id": "MDRW2100024841.1.1",
          "form": "안녕하세요 저녁식사하셨나요?",
          "original_form": "안녕하세요 저녁식사하셨나요?",
          "speaker_id": "1",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100024841.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024841.1.3",
          "form": "평일 9시쯤 저녁을 먹네요ㅠㅠ아직 전이에요!",
          "original_form": "평일 9시쯤 저녁을 먹네요ㅠㅠ아직 전이에요!",
          "speaker_id": "2",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024841.1.4",
          "form": "헉 늦게 드시네요",
          "original_form": "헉 늦게 드시네요",
          "speaker_id": "1",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024841.1.5",
          "form": "매일 그시간에 드세요?",
          "original_form": "매일 그시간에 드세요?",
          "speaker_id": "1",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024841.1.6",
          "form": "평일엔 보통그래여ㅠ ~ 지금집에가는길이라 씻고하다보면 !",
          "original_form": "평일엔 보통그래여ㅠ ~ 지금집에가는길이라 씻고하다보면 !",
          "speaker_id": "2",
          "time": "20210831 20:05"
        },
        {
          "id": "MDRW2100024841.1.7",
          "form": "아 퇴근이 늦으신가보네요ㅜ",
          "original_form": "아 퇴근이 늦으신가보네요ㅜ",
          "speaker_id": "1",
          "time": "20210831 20:06"
        },
        {
          "id": "MDRW2100024841.1.8",
          "form": "그럼 저녁은 뭐 드실 예정이신가요?",
          "original_form": "그럼 저녁은 뭐 드실 예정이신가요?",
          "speaker_id": "1",
          "time": "20210831 20:06"
        },
        {
          "id": "MDRW2100024841.1.9",
          "form": "전 그냥알바하는건데 저보다 더 늦게들어오는사람있어요 ㅋㅋ",
          "original_form": "전 그냥알바하는건데 저보다 더 늦게들어오는사람있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:06"
        },
        {
          "id": "MDRW2100024841.1.10",
          "form": "아까 빵을 먹었더니 막상 입맛이 없네요ㅠㅠ 뭘먹긴해야할텐데",
          "original_form": "아까 빵을 먹었더니 막상 입맛이 없네요ㅠㅠ 뭘먹긴해야할텐데",
          "speaker_id": "2",
          "time": "20210831 20:06"
        },
        {
          "id": "MDRW2100024841.1.11",
          "form": "아 ㅜㅜ 밥 잘챙겨드세요",
          "original_form": "아 ㅜㅜ 밥 잘챙겨드세요",
          "speaker_id": "1",
          "time": "20210831 20:07"
        },
        {
          "id": "MDRW2100024841.1.12",
          "form": "건강 있을때 챙겨야해요ㅜㅜㅜ",
          "original_form": "건강 있을때 챙겨야해요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 20:07"
        },
        {
          "id": "MDRW2100024841.1.13",
          "form": "맞아요ㅠㅠㅠ 근데 원래 국수먹을까?싶었는데ㅠ밀가루 넘좋아하네요ㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ 근데 원래 국수먹을까?싶었는데ㅠ밀가루 넘좋아하네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 20:09"
        },
        {
          "id": "MDRW2100024841.1.14",
          "form": "밀가루 너무 맛있죠ㅜㅜㅜ",
          "original_form": "밀가루 너무 맛있죠ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 20:09"
        },
        {
          "id": "MDRW2100024841.1.15",
          "form": "라면 못 끊어요",
          "original_form": "라면 못 끊어요",
          "speaker_id": "1",
          "time": "20210831 20:09"
        },
        {
          "id": "MDRW2100024841.1.16",
          "form": "맞아요ㅠㅠ 전 면은 다좋아하네요ㅠ",
          "original_form": "맞아요ㅠㅠ 전 면은 다좋아하네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 20:11"
        },
        {
          "id": "MDRW2100024841.1.17",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "1",
          "time": "20210831 20:11"
        },
        {
          "id": "MDRW2100024841.1.18",
          "form": "요즘 쌀국수도 먹고싶어요ㅜ",
          "original_form": "요즘 쌀국수도 먹고싶어요ㅜ",
          "speaker_id": "1",
          "time": "20210831 20:11"
        },
        {
          "id": "MDRW2100024841.1.19",
          "form": "쌀국수넘맛잇죠ㅠㅠ",
          "original_form": "쌀국수넘맛잇죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 20:11"
        },
        {
          "id": "MDRW2100024841.1.20",
          "form": "전 고수도좋아해요ㅠㅠ",
          "original_form": "전 고수도좋아해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 20:11"
        },
        {
          "id": "MDRW2100024841.1.21",
          "form": "전 고수는 못먹어요ㅜ",
          "original_form": "전 고수는 못먹어요ㅜ",
          "speaker_id": "1",
          "time": "20210831 20:12"
        }
      ]
    }
