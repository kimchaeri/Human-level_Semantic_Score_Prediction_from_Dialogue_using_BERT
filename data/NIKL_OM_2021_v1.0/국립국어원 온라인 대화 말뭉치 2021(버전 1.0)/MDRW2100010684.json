      "utterance": [
        {
          "id": "MDRW2100010684.1.1",
          "form": "요즘 날씨 이상하다",
          "original_form": "요즘 날씨 이상하다",
          "speaker_id": "1",
          "time": "20210823 20:36"
        },
        {
          "id": "MDRW2100010684.1.2",
          "form": "그러니까 날씨왜이럼",
          "original_form": "그러니까 날씨왜이럼",
          "speaker_id": "2",
          "time": "20210823 20:36"
        },
        {
          "id": "MDRW2100010684.1.3",
          "form": "태풍와서그래",
          "original_form": "태풍와서그래",
          "speaker_id": "2",
          "time": "20210823 20:36"
        },
        {
          "id": "MDRW2100010684.1.4",
          "form": "태풍 언제까지 오려나",
          "original_form": "태풍 언제까지 오려나",
          "speaker_id": "1",
          "time": "20210823 20:37"
        },
        {
          "id": "MDRW2100010684.1.5",
          "form": "오늘저녁이 고비래",
          "original_form": "오늘저녁이 고비래",
          "speaker_id": "2",
          "time": "20210823 20:37"
        },
        {
          "id": "MDRW2100010684.1.6",
          "form": "비가 이상하게 와",
          "original_form": "비가 이상하게 와",
          "speaker_id": "1",
          "time": "20210823 20:37"
        },
        {
          "id": "MDRW2100010684.1.7",
          "form": "비오니까 진짜 찝찝해",
          "original_form": "비오니까 진짜 찝찝해",
          "speaker_id": "2",
          "time": "20210823 20:37"
        },
        {
          "id": "MDRW2100010684.1.8",
          "form": "근데 또 시원해져서 좋긴해",
          "original_form": "근데 또 시원해져서 좋긴해",
          "speaker_id": "2",
          "time": "20210823 20:37"
        },
        {
          "id": "MDRW2100010684.1.9",
          "form": "그니깓",
          "original_form": "그니깓",
          "speaker_id": "1",
          "time": "20210823 21:13"
        },
        {
          "id": "MDRW2100010684.1.10",
          "form": "다리 다 젖었어 ㅋㅋㅋ",
          "original_form": "다리 다 젖었어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:13"
        },
        {
          "id": "MDRW2100010684.1.11",
          "form": "서울도착햇겟다",
          "original_form": "서울도착햇겟다",
          "speaker_id": "2",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010684.1.12",
          "form": "서울도 비와?",
          "original_form": "서울도 비와?",
          "speaker_id": "2",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010684.1.13",
          "form": "예쓰",
          "original_form": "예쓰",
          "speaker_id": "1",
          "time": "20210823 21:46"
        },
        {
          "id": "MDRW2100010684.1.14",
          "form": "오늘 오후에는 비 많이온다그래가지고",
          "original_form": "오늘 오후에는 비 많이온다그래가지고",
          "speaker_id": "1",
          "time": "20210823 21:46"
        },
        {
          "id": "MDRW2100010684.1.15",
          "form": "그래서 일찍 왔옼ㅋㅋㅋ",
          "original_form": "그래서 일찍 왔옼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:46"
        },
        {
          "id": "MDRW2100010684.1.16",
          "form": "낼은 나가는데 뭐입지..또 비와",
          "original_form": "낼은 나가는데 뭐입지..또 비와",
          "speaker_id": "1",
          "time": "20210823 21:46"
        },
        {
          "id": "MDRW2100010684.1.17",
          "form": "잘했네 ㅋㅋㅋㅋ",
          "original_form": "잘했네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:46"
        },
        {
          "id": "MDRW2100010684.1.18",
          "form": "여긴 하루죙ㅇㅇ일와",
          "original_form": "여긴 하루죙ㅇㅇ일와",
          "speaker_id": "2",
          "time": "20210823 21:46"
        },
        {
          "id": "MDRW2100010684.1.19",
          "form": "아주지겨워죽겟어",
          "original_form": "아주지겨워죽겟어",
          "speaker_id": "2",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100010684.1.20",
          "form": "비 언제까지오지?",
          "original_form": "비 언제까지오지?",
          "speaker_id": "1",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100010684.1.21",
          "form": "그래두 이제 미세먼지는 없겠다",
          "original_form": "그래두 이제 미세먼지는 없겠다",
          "speaker_id": "1",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100010684.1.22",
          "form": "여기는아마 홍수날거같아",
          "original_form": "여기는아마 홍수날거같아",
          "speaker_id": "2",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100010684.1.23",
          "form": "하늘에구멍낫어",
          "original_form": "하늘에구멍낫어",
          "speaker_id": "2",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100010684.1.24",
          "form": "아직도 비 많이내려?",
          "original_form": "아직도 비 많이내려?",
          "speaker_id": "1",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100010684.1.25",
          "form": "비엄청와 내일 만년교 넘.칠듯",
          "original_form": "비엄청와 내일 만년교 넘.칠듯",
          "speaker_id": "2",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100010684.1.26",
          "form": "만년교가 넘치다니",
          "original_form": "만년교가 넘치다니",
          "speaker_id": "1",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100010684.1.27",
          "form": "내일 나가기 두렵다...",
          "original_form": "내일 나가기 두렵다...",
          "speaker_id": "1",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100010684.1.28",
          "form": "나가긴 해야돠는데 말이죠",
          "original_form": "나가긴 해야돠는데 말이죠",
          "speaker_id": "1",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100010684.1.29",
          "form": "내일 날씬어때",
          "original_form": "내일 날씬어때",
          "speaker_id": "2",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100010684.1.30",
          "form": "야기는 내일도 비가 온대",
          "original_form": "야기는 내일도 비가 온대",
          "speaker_id": "1",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100010684.1.31",
          "form": "그만좀왓으면 좋겠다",
          "original_form": "그만좀왓으면 좋겠다",
          "speaker_id": "2",
          "time": "20210824 06:30"
        },
        {
          "id": "MDRW2100010684.1.32",
          "form": "아직도비가와",
          "original_form": "아직도비가와",
          "speaker_id": "2",
          "time": "20210824 06:30"
        },
        {
          "id": "MDRW2100010684.1.33",
          "form": "오늘 쌀쌀하다 잘챙겨입어",
          "original_form": "오늘 쌀쌀하다 잘챙겨입어",
          "speaker_id": "2",
          "time": "20210824 06:30"
        },
        {
          "id": "MDRW2100010684.1.34",
          "form": "서울 지금 비 난리냤다",
          "original_form": "서울 지금 비 난리냤다",
          "speaker_id": "1",
          "time": "20210824 09:18"
        },
        {
          "id": "MDRW2100010684.1.35",
          "form": "하늘에 구멍 뚫림",
          "original_form": "하늘에 구멍 뚫림",
          "speaker_id": "1",
          "time": "20210824 09:18"
        },
        {
          "id": "MDRW2100010684.1.36",
          "form": "대전이 한 1시간전까지만해도 하늘에구멍났었거든",
          "original_form": "대전이 한 1시간전까지만해도 하늘에구멍났었거든",
          "speaker_id": "2",
          "time": "20210824 09:19"
        },
        {
          "id": "MDRW2100010684.1.37",
          "form": "근데 한 10분내리고 그쳐써",
          "original_form": "근데 한 10분내리고 그쳐써",
          "speaker_id": "2",
          "time": "20210824 09:19"
        },
        {
          "id": "MDRW2100010684.1.38",
          "form": "여기로 넘어왔나봐",
          "original_form": "여기로 넘어왔나봐",
          "speaker_id": "1",
          "time": "20210824 09:20"
        },
        {
          "id": "MDRW2100010684.1.39",
          "form": "나갈 엄두가 안 난ㄴ다....",
          "original_form": "나갈 엄두가 안 난ㄴ다....",
          "speaker_id": "1",
          "time": "20210824 09:20"
        },
        {
          "id": "MDRW2100010684.1.40",
          "form": "어디로가?",
          "original_form": "어디로가?",
          "speaker_id": "2",
          "time": "20210824 09:20"
        }
      ]
    }
  ]
