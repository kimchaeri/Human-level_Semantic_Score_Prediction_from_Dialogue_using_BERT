          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015453.1.3",
          "form": "종교 없어요!",
          "original_form": "종교 없어요!",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100015453.1.4",
          "form": "저도 없고 부모님은 불교요",
          "original_form": "저도 없고 부모님은 불교요",
          "speaker_id": "1",
          "time": "20210827 06:56"
        },
        {
          "id": "MDRW2100015453.1.5",
          "form": "어릴때 부모님께 물어봤던거 같아요",
          "original_form": "어릴때 부모님께 물어봤던거 같아요",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100015453.1.6",
          "form": "우리집은 종교있어? 라고요",
          "original_form": "우리집은 종교있어? 라고요",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100015453.1.7",
          "form": "부모님이 없는데 굳~~~~이 따지면 불교지라고 하시더라구요",
          "original_form": "부모님이 없는데 굳~~~~이 따지면 불교지라고 하시더라구요",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100015453.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100015453.1.9",
          "form": "절은 유명한 곳이 많은 거 같아요",
          "original_form": "절은 유명한 곳이 많은 거 같아요",
          "speaker_id": "2",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100015453.1.10",
          "form": "불국사요",
          "original_form": "불국사요",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100015453.1.11",
          "form": "표충사도 있고",
          "original_form": "표충사도 있고",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100015453.1.12",
          "form": "역사적인 곳도 많네요",
          "original_form": "역사적인 곳도 많네요",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100015453.1.13",
          "form": "그르네여",
          "original_form": "그르네여",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100015453.1.14",
          "form": "부처님 오신날에 부모님따라서",
          "original_form": "부처님 오신날에 부모님따라서",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100015453.1.15",
          "form": "절에 간 기억이 나네요",
          "original_form": "절에 간 기억이 나네요",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100015453.1.16",
          "form": "저도 절밥 기억나요",
          "original_form": "저도 절밥 기억나요",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100015453.1.17",
          "form": "절밥 먹고나면 떡도 나눠주셨던거 같아요1",
