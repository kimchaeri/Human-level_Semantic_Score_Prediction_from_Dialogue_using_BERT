        },
        {
          "id": "MDRW2100004632.1.12",
          "form": "ㅋㅋ과일매력",
          "original_form": "ㅋㅋ과일매력",
          "speaker_id": "2",
          "time": "20210822 10:32"
        },
        {
          "id": "MDRW2100004632.1.13",
          "form": "세제랑",
          "original_form": "세제랑",
          "speaker_id": "1",
          "time": "20210822 10:34"
        },
        {
          "id": "MDRW2100004632.1.14",
          "form": "회도사고",
          "original_form": "회도사고",
          "speaker_id": "1",
          "time": "20210822 10:34"
        },
        {
          "id": "MDRW2100004632.1.15",
          "form": "ㅋㅋ회맛잇게따ㅜㅜ",
