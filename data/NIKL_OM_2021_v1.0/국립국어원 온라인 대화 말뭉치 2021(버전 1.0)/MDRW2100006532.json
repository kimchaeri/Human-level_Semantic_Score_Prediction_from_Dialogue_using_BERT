      "utterance": [
        {
          "id": "MDRW2100006532.1.1",
          "form": "하시는 커뮤니티 있으신가요",
          "original_form": "하시는 커뮤니티 있으신가요",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100006532.1.2",
          "form": "딱히 활동하는 데는 없어요",
          "original_form": "딱히 활동하는 데는 없어요",
          "speaker_id": "1",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100006532.1.3",
          "form": "저는 더쿠 자주 갑니다",
          "original_form": "저는 더쿠 자주 갑니다",
          "speaker_id": "2",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100006532.1.4",
          "form": "거기 가입이 힘들더라구요",
          "original_form": "거기 가입이 힘들더라구요",
          "speaker_id": "1",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100006532.1.5",
          "form": "저도 초반에 가입한지라",
          "original_form": "저도 초반에 가입한지라",
          "speaker_id": "2",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100006532.1.6",
          "form": "거기 사람 많나요",
          "original_form": "거기 사람 많나요",
          "speaker_id": "1",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100006532.1.7",
          "form": "방이 나눠져서 되게 많을 듯 요",
          "original_form": "방이 나눠져서 되게 많을 듯 요",
          "speaker_id": "2",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100006532.1.8",
          "form": "주로 어디 어디 가세요",
          "original_form": "주로 어디 어디 가세요",
          "speaker_id": "1",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100006532.1.9",
          "form": "고양이도 가고  뷰티 방도 가요",
          "original_form": "고양이도 가고  뷰티 방도 가요",
          "speaker_id": "2",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100006532.1.10",
          "form": "거기도 세세하게 나눠져 있네요",
          "original_form": "거기도 세세하게 나눠져 있네요",
          "speaker_id": "1",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100006532.1.11",
          "form": "정보도 많고 재미있어요",
          "original_form": "정보도 많고 재미있어요",
          "speaker_id": "2",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100006532.1.12",
