        {
          "id": "MDRW2100026216.1.1",
          "form": "저는 요즘 마라탕 많이 시켜먹어요",
          "original_form": "저는 요즘 마라탕 많이 시켜먹어요",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026216.1.2",
          "form": "저는 보통 디저트 많이 배달해서 먹어요",
          "original_form": "저는 보통 디저트 많이 배달해서 먹어요",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026216.1.3",
          "form": "단 걸 좋아해서 ㅋㅋㅋ",
          "original_form": "단 걸 좋아해서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026216.1.4",
          "form": "와 마라탕 좋죠",
          "original_form": "와 마라탕 좋죠",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026216.1.5",
          "form": "마라탕 좋아하세요?",
          "original_form": "마라탕 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026216.1.6",
          "form": "저 엄청 매운 건 못먹는데 적당히 매운건 잘 먹고 좋아해요~",
          "original_form": "저 엄청 매운 건 못먹는데 적당히 매운건 잘 먹고 좋아해요~",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026216.1.7",
          "form": "마라탕이 맵기 조절을 할 수 있어서 좋더라고요",
          "original_form": "마라탕이 맵기 조절을 할 수 있어서 좋더라고요",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026216.1.8",
          "form": "어떤 재료 넣는 거 좋아하세요?",
          "original_form": "어떤 재료 넣는 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026216.1.9",
          "form": "저 두부 엄청 좋아해요! 청경채도요",
          "original_form": "저 두부 엄청 좋아해요! 청경채도요",
          "speaker_id": "2",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026216.1.10",
          "form": "저도 건두부 제일 좋아해요! 야채도 안 가리고 다 먹어요ㅎㅎ",
          "original_form": "저도 건두부 제일 좋아해요! 야채도 안 가리고 다 먹어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026216.1.11",
          "form": "아 건두부 너무 좋아요~",
          "original_form": "아 건두부 너무 좋아요~",
          "speaker_id": "2",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100026216.1.12",
          "form": "고기 추가도 하시나요?",
          "original_form": "고기 추가도 하시나요?",
          "speaker_id": "1",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100026216.1.13",
          "form": "하루종일 건두부만 먹어도 돼요 전 ㅋㅋ",
          "original_form": "하루종일 건두부만 먹어도 돼요 전 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100026216.1.14",
          "form": "저 고기는 잘 안먹어요~",
          "original_form": "저 고기는 잘 안먹어요~",
          "speaker_id": "2",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100026216.1.15",
          "form": "두부가 좋아서 ㅋㅋㅋ",
          "original_form": "두부가 좋아서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100026216.1.16",
          "form": "저도 고기보다는 건두부나 푸주 추가해서 먹어요ㅎㅎ",
          "original_form": "저도 고기보다는 건두부나 푸주 추가해서 먹어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100026216.1.17",
          "form": "건두부로 야채 싸서먹으면 너무 맛있어요",
          "original_form": "건두부로 야채 싸서먹으면 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100026216.1.18",
          "form": "크 그게 최고죠!!",
          "original_form": "크 그게 최고죠!!",
          "speaker_id": "2",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100026216.1.19",
          "form": "매운거 잘 드시나봐요",
          "original_form": "매운거 잘 드시나봐요",
          "speaker_id": "2",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100026216.1.20",
          "form": "엄청 잘 먹는 건 아닌데 좋아해요",
          "original_form": "엄청 잘 먹는 건 아닌데 좋아해요",
          "speaker_id": "1",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100026216.1.21",
          "form": "속에서 너무 매운 건 안 받아주더라고요",
          "original_form": "속에서 너무 매운 건 안 받아주더라고요",
