      "utterance": [
        {
          "id": "MDRW2100008972.1.1",
          "form": "이제 언니 잘한데이 ㅋㅋㅋㅋㅋ",
          "original_form": "이제 언니 잘한데이 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:04"
        },
        {
          "id": "MDRW2100008972.1.2",
          "form": "우리의.과제는 다이어트 ㅋㅋㅋㅋㅋ",
          "original_form": "우리의.과제는 다이어트 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:04"
        },
        {
          "id": "MDRW2100008972.1.3",
          "form": "언제 끝날까여 ㅋㅋㅋㅋ",
          "original_form": "언제 끝날까여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:04"
        },
        {
          "id": "MDRW2100008972.1.4",
          "form": "언넝 살쫙빼서 예쁜옷 사곱네여~",
          "original_form": "언넝 살쫙빼서 예쁜옷 사곱네여~",
          "speaker_id": "2",
          "time": "20210822 20:05"
        },
        {
          "id": "MDRW2100008972.1.5",
          "form": "다이어트 해야된다",
          "original_form": "다이어트 해야된다",
          "speaker_id": "1",
          "time": "20210822 20:10"
        },
        {
          "id": "MDRW2100008972.1.6",
          "form": "오늘도3키로 찐듯",
          "original_form": "오늘도3키로 찐듯",
          "speaker_id": "1",
          "time": "20210822 20:10"
        },
        {
          "id": "MDRW2100008972.1.7",
          "form": "좀이따 야식 시킨다는데 괜찮겠나ㅡㅡ",
          "original_form": "좀이따 야식 시킨다는데 괜찮겠나ㅡㅡ",
          "speaker_id": "1",
          "time": "20210822 20:10"
        },
        {
          "id": "MDRW2100008972.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ야식은 애들이 좋아하는걸로다가~",
          "original_form": "ㅋㅋㅋㅋㅋㅋ야식은 애들이 좋아하는걸로다가~",
          "speaker_id": "2",
          "time": "20210822 20:11"
        },
        {
          "id": "MDRW2100008972.1.9",
          "form": "건강생각해서 낼 잡사요 ㅋㅋㅋ",
          "original_form": "건강생각해서 낼 잡사요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:12"
        },
        {
          "id": "MDRW2100008972.1.10",
          "form": "운동도안하는데 야식까지 먹으면 후회하긋재?",
          "original_form": "운동도안하는데 야식까지 먹으면 후회하긋재?",
          "speaker_id": "1",
          "time": "20210822 20:23"
        },
        {
          "id": "MDRW2100008972.1.11",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:23"
        },
        {
          "id": "MDRW2100008972.1.12",
          "form": "후회는..늘 하죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "후회는..늘 하죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100008972.1.13",
          "form": "그치만 똑같은 실수를 반복하져 ㅋㅋㅋㅋㅋ",
          "original_form": "그치만 똑같은 실수를 반복하져 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100008972.1.14",
          "form": "이게.문제임 ㅋㅋㅋㅋ",
          "original_form": "이게.문제임 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100008972.1.15",
          "form": "각종질병의 원인 야식ㅋㅋㅇ",
          "original_form": "각종질병의 원인 야식ㅋㅋㅇ",
          "speaker_id": "1",
          "time": "20210822 21:01"
        },
        {
          "id": "MDRW2100008972.1.16",
          "form": "알면서 못끊음",
          "original_form": "알면서 못끊음",
          "speaker_id": "1",
          "time": "20210822 21:01"
        },
        {
          "id": "MDRW2100008972.1.17",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:01"
        },
        {
          "id": "MDRW2100008972.1.18",
          "form": "시켰어요??",
          "original_form": "시켰어요??",
          "speaker_id": "2",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100008972.1.19",
          "form": "지금 입은 행복하고 맘은 불편허고 구럽니꺼??ㅋㅋㅋ",
          "original_form": "지금 입은 행복하고 맘은 불편허고 구럽니꺼??ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100008972.1.20",
          "form": "피자먹고 겁나후회중",
          "original_form": "피자먹고 겁나후회중",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100008972.1.21",
          "form": "난 다음생애 다이어트해야되나?",
          "original_form": "난 다음생애 다이어트해야되나?",
          "speaker_id": "1",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100008972.1.22",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "1",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100008972.1.23",
          "form": "옷도없는데",
          "original_form": "옷도없는데",
          "speaker_id": "1",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100008972.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ겨울옷은 새로 삽시다 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ겨울옷은 새로 삽시다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100008972.1.25",
          "form": "난 너무 많이먹어서 저녁먹은거 아직도 더부룩해여ㅠ",
          "original_form": "난 너무 많이먹어서 저녁먹은거 아직도 더부룩해여ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100008972.1.26",
          "form": "이제 진짜 많이 먹은날은 소화도 잘 안되고ㅠ",
          "original_form": "이제 진짜 많이 먹은날은 소화도 잘 안되고ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100008972.1.27",
          "form": "이래가 오늘굶어도 될듯ㅋ",
          "original_form": "이래가 오늘굶어도 될듯ㅋ",
          "speaker_id": "1",
          "time": "20210823 07:56"
        },
        {
          "id": "MDRW2100008972.1.28",
          "form": "9월부터 운동해야긋다ㅋ",
          "original_form": "9월부터 운동해야긋다ㅋ",
          "speaker_id": "1",
          "time": "20210823 07:56"
        },
        {
          "id": "MDRW2100008972.1.29",
          "form": "오늘은 또 오늘의.해가 떴는데요??ㅋㅋㅋ",
          "original_form": "오늘은 또 오늘의.해가 떴는데요??ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 08:44"
        },
        {
          "id": "MDRW2100008972.1.30",
          "form": "내마음처럼비가오네",
          "original_form": "내마음처럼비가오네",
