        {
          "id": "MDRW2100038178.1.2",
          "form": "대탈출봤니",
          "original_form": "대탈출봤니",
          "speaker_id": "2",
          "time": "20210906 00:10"
        },
        {
          "id": "MDRW2100038178.1.3",
          "form": "응 일욜 저녁마다 봐",
          "original_form": "응 일욜 저녁마다 봐",
          "speaker_id": "1",
          "time": "20210906 00:11"
        },
        {
          "id": "MDRW2100038178.1.4",
          "form": "오늘은 어떤내용이야",
          "original_form": "오늘은 어떤내용이야",
          "speaker_id": "2",
          "time": "20210906 00:11"
        },
        {
          "id": "MDRW2100038178.1.5",
          "form": "오늘 공장 탈출해",
          "original_form": "오늘 공장 탈출해",
          "speaker_id": "1",
          "time": "20210906 00:11"
        },
        {
          "id": "MDRW2100038178.1.6",
          "form": "좀비도나와?",
          "original_form": "좀비도나와?",
          "speaker_id": "2",
          "time": "20210906 00:11"
        },
        {
          "id": "MDRW2100038178.1.7",
          "form": "아니 폭탄을 해체해",
          "original_form": "아니 폭탄을 해체해",
          "speaker_id": "1",
          "time": "20210906 00:11"
        },
        {
          "id": "MDRW2100038178.1.8",
          "form": "누가제일활약해?",
          "original_form": "누가제일활약해?",
          "speaker_id": "2",
          "time": "20210906 00:12"
        },
        {
          "id": "MDRW2100038178.1.9",
          "form": "오늘은 유병재랑 강호동 가둬놨어 ㅋㅋ",
          "original_form": "오늘은 유병재랑 강호동 가둬놨어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:12"
        },
        {
          "id": "MDRW2100038178.1.10",
          "form": "오오 유병재가잘하는데 아쉽겠네",
          "original_form": "오오 유병재가잘하는데 아쉽겠네",
          "speaker_id": "2",
          "time": "20210906 00:12"
        },
        {
          "id": "MDRW2100038178.1.11",
          "form": "무전으로만 소통해서 어렵겠더라",
          "original_form": "무전으로만 소통해서 어렵겠더라",
          "speaker_id": "1",
          "time": "20210906 00:13"
        },
        {
          "id": "MDRW2100038178.1.12",
          "form": "헉 좀 답답하겠다",
          "original_form": "헉 좀 답답하겠다",
          "speaker_id": "2",
          "time": "20210906 00:13"
        },
        {
          "id": "MDRW2100038178.1.13",
          "form": "생각보다 재밌지는않더라 ㅋㅋ",
          "original_form": "생각보다 재밌지는않더라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:13"
