      "utterance": [
        {
          "id": "MDRW2100032068.1.1",
          "form": "미술관가 보고 싶어요",
          "original_form": "미술관가 보고 싶어요",
          "speaker_id": "2",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100032068.1.2",
          "form": "안 간지 몇 년이 됐어요",
          "original_form": "안 간지 몇 년이 됐어요",
          "speaker_id": "2",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100032068.1.3",
          "form": "서귀포의 이중섭미술관이 있는데요",
          "original_form": "서귀포의 이중섭미술관이 있는데요",
          "speaker_id": "1",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100032068.1.4",
          "form": "언제쯤 한번가 볼 수 있을까요",
          "original_form": "언제쯤 한번가 볼 수 있을까요",
          "speaker_id": "2",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100032068.1.5",
          "form": "이번에 전시회를 무료로 해요",
          "original_form": "이번에 전시회를 무료로 해요",
          "speaker_id": "1",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100032068.1.6",
          "form": "아 정말이요 이중석 관련해서 전에 본 적이 있어요 책도 선물 받았었는데",
          "original_form": "아 정말이요 이중석 관련해서 전에 본 적이 있어요 책도 선물 받았었는데",
          "speaker_id": "2",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100032068.1.7",
          "form": "이건희가 기증한 자료를 보여 준대요",
          "original_form": "이건희가 기증한 자료를 보여 준대요",
          "speaker_id": "1",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100032068.1.8",
          "form": "이중섭 하면 소지호",
          "original_form": "이중섭 하면 소지호",
          "speaker_id": "2",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100032068.1.9",
          "form": "그러나 저는 귀찮아서 가지 않습니다",
          "original_form": "그러나 저는 귀찮아서 가지 않습니다",
          "speaker_id": "1",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100032068.1.10",
          "form": "와 진짜요 완전 신기하네요",
          "original_form": "와 진짜요 완전 신기하네요",
          "speaker_id": "2",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100032068.1.11",
          "form": "귀차니즘이 무료관람을 이깁니다",
          "original_form": "귀차니즘이 무료관람을 이깁니다",
          "speaker_id": "1",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100032068.1.12",
          "form": "한번가 보시는 건 어떨까요 그림이 정말 인상깊 없거든요",
          "original_form": "한번가 보시는 건 어떨까요 그림이 정말 인상깊 없거든요",
          "speaker_id": "2",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100032068.1.13",
          "form": "저 답지요",
          "original_form": "저 답지요",
          "speaker_id": "1",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100032068.1.14",
          "form": "돈 주고도 가는데 무료 많은가 볼만 하지 않을까요",
          "original_form": "돈 주고도 가는데 무료 많은가 볼만 하지 않을까요",
          "speaker_id": "2",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100032068.1.15",
          "form": "1박 2일로 놀러가 볼까 했는데",
          "original_form": "1박 2일로 놀러가 볼까 했는데",
          "speaker_id": "1",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100032068.1.16",
          "form": "갈 때가 아닌 거 같아요",
          "original_form": "갈 때가 아닌 거 같아요",
          "speaker_id": "1",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100032068.1.17",
          "form": "안타깝네요",
          "original_form": "안타깝네요",
          "speaker_id": "2",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100032068.1.18",
          "form": "제주도에 포도 박물관이라고 있는데",
          "original_form": "제주도에 포도 박물관이라고 있는데",
          "speaker_id": "1",
          "time": "20210905 11:55"
        },
        {
          "id": "MDRW2100032068.1.19",
          "form": "특이한 전시를 많이 하니까 다음에 오시면 가보세요",
          "original_form": "특이한 전시를 많이 하니까 다음에 오시면 가보세요",
          "speaker_id": "1",
          "time": "20210905 11:55"
        },
        {
          "id": "MDRW2100032068.1.20",
          "form": "포도박물관 이요",
          "original_form": "포도박물관 이요",
          "speaker_id": "2",
          "time": "20210905 11:55"
        },
        {
          "id": "MDRW2100032068.1.21",
          "form": "이름이 세콤 달콤해 보이네요",
          "original_form": "이름이 세콤 달콤해 보이네요",
          "speaker_id": "2",
          "time": "20210905 11:55"
        },
        {
          "id": "MDRW2100032068.1.22",
          "form": "서귀포 근처에 있어요",
          "original_form": "서귀포 근처에 있어요",
          "speaker_id": "1",
          "time": "20210905 11:55"
        },
        {
          "id": "MDRW2100032068.1.23",
          "form": "내 생긴지 얼마 안 되었어요",
          "original_form": "내 생긴지 얼마 안 되었어요",
          "speaker_id": "1",
          "time": "20210905 11:55"
        },
        {
          "id": "MDRW2100032068.1.24",
          "form": "다음에 놀러 가봐야겠어요",
          "original_form": "다음에 놀러 가봐야겠어요",
          "speaker_id": "2",
          "time": "20210905 11:55"
        },
        {
          "id": "MDRW2100032068.1.25",
          "form": "올 때마다 새롭게 생겨나요",
          "original_form": "올 때마다 새롭게 생겨나요",
          "speaker_id": "1",
          "time": "20210905 11:55"
        }
      ]
