        {
          "id": "MDRW2100015517.1.1",
          "form": "오 게임 잘하세요?",
          "original_form": "오 게임 잘하세요?",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015517.1.2",
          "form": "좋아합니다ㅎㅎ",
          "original_form": "좋아합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100015517.1.3",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "2",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100015517.1.4",
          "form": "피파온라인입니다",
          "original_form": "피파온라인입니다",
          "speaker_id": "1",
          "time": "20210826 10:37"
        },
        {
          "id": "MDRW2100015517.1.5",
          "form": "아아 안해봤어요 축구도 잘 모르고 ㅎ",
          "original_form": "아아 안해봤어요 축구도 잘 모르고 ㅎ",
          "speaker_id": "2",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100015517.1.6",
          "form": "어떤게임 하시나요?",
          "original_form": "어떤게임 하시나요?",
          "speaker_id": "1",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100015517.1.7",
          "form": "게임 전혀 안하고 못해요. 어려서부터쭉 ㅠㅠ",
          "original_form": "게임 전혀 안하고 못해요. 어려서부터쭉 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100015517.1.8",
          "form": "음 애니팡도 안하셨나요ㅎ",
          "original_form": "음 애니팡도 안하셨나요ㅎ",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100015517.1.9",
          "form": "네 안했어요 ㅋㅋ 몇번 해봤는데 너무 못해서 흥미가 안생겨요",
          "original_form": "네 안했어요 ㅋㅋ 몇번 해봤는데 너무 못해서 흥미가 안생겨요",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100015517.1.10",
          "form": "오그래도 애니팡은 해보셨군요",
          "original_form": "오그래도 애니팡은 해보셨군요",
          "speaker_id": "1",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100015517.1.11",
          "form": "ㅎㅎㅎ 인기 많으면 시도는 해보는데 못해서 금방... 끝",
          "original_form": "ㅎㅎㅎ 인기 많으면 시도는 해보는데 못해서 금방... 끝",
