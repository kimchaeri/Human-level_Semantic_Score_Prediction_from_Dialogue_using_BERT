      "utterance": [
        {
          "id": "MDRW2100015627.1.1",
          "form": "이상민 보면 연예인 걱정할필요가없는듯ㅎ",
          "original_form": "이상민 보면 연예인 걱정할필요가없는듯ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015627.1.2",
          "form": "72억이었나요?",
          "original_form": "72억이었나요?",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015627.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015627.1.4",
          "form": "그래도 대단한거 같아요",
          "original_form": "그래도 대단한거 같아요",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015627.1.5",
          "form": "우리같은 서민은 그런거 못갚을꺼에요",
          "original_form": "우리같은 서민은 그런거 못갚을꺼에요",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015627.1.6",
          "form": "다 갚을려고 노력하는거 보니",
          "original_form": "다 갚을려고 노력하는거 보니",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015627.1.7",
          "form": "대단하네요 정말",
          "original_form": "대단하네요 정말",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015627.1.8",
          "form": "급여받아서는ㅊ힘든데ㅎㅎ",
          "original_form": "급여받아서는ㅊ힘든데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015627.1.9",
          "form": "연예인이니깐 가능한것같음ㅎㅎㅎ",
          "original_form": "연예인이니깐 가능한것같음ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015627.1.10",
          "form": "그 돈빌려주신분들이",
          "original_form": "그 돈빌려주신분들이",
          "speaker_id": "1",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015627.1.11",
          "form": "안아파야 돈 빨리 갚는다고",
          "original_form": "안아파야 돈 빨리 갚는다고",
          "speaker_id": "1",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015627.1.12",
          "form": "이상민한테 진짜 공?!들이더라구요",
          "original_form": "이상민한테 진짜 공?!들이더라구요",
          "speaker_id": "1",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015627.1.13",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "2",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015627.1.14",
          "form": "밥도 사주고  사시사철 보약 보내고",
          "original_form": "밥도 사주고  사시사철 보약 보내고",
          "speaker_id": "1",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015627.1.15",
          "form": "사람이 안좋으면 그렇게 안할꺼에요",
          "original_form": "사람이 안좋으면 그렇게 안할꺼에요",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015627.1.16",
          "form": "사람이 좋으니깐 그런거겟죠?ㅎ",
          "original_form": "사람이 좋으니깐 그런거겟죠?ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015627.1.17",
          "form": "맞아요ㅎ 그러기 쉽지않잖아요ㅎ",
          "original_form": "맞아요ㅎ 그러기 쉽지않잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015627.1.18",
          "form": "그쳐 갚으려는 의지가 있는데",
          "original_form": "그쳐 갚으려는 의지가 있는데",
          "speaker_id": "1",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015627.1.19",
          "form": "무리할까 더챙기는 느낌",
          "original_form": "무리할까 더챙기는 느낌",
          "speaker_id": "1",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015627.1.20",
          "form": "근데 이상민이 정말 좋은게",
          "original_form": "근데 이상민이 정말 좋은게",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015627.1.21",
          "form": "열씸히해서 더 좋아요",
          "original_form": "열씸히해서 더 좋아요",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015627.1.22",
          "form": "사람이 그정도로 가면 힘들고 지칠텐데",
          "original_form": "사람이 그정도로 가면 힘들고 지칠텐데",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015627.1.23",
          "form": "오히려 미안하니깐 하나라도 더 하려고하고",
          "original_form": "오히려 미안하니깐 하나라도 더 하려고하고",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015627.1.24",
          "form": "자기시간 아끼면서 하는거보면 대단하더라구요",
          "original_form": "자기시간 아끼면서 하는거보면 대단하더라구요",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
