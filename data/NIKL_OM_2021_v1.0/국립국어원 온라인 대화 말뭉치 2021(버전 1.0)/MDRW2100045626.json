      "utterance": [
        {
          "id": "MDRW2100045626.1.1",
          "form": "향신료가 아닙니다!",
          "original_form": "향신료가 아닙니다!",
          "speaker_id": "2",
          "time": "20210908 04:22"
        },
        {
          "id": "MDRW2100045626.1.2",
          "form": "주로 어떤 요리에 쓰이나요?",
          "original_form": "주로 어떤 요리에 쓰이나요?",
          "speaker_id": "1",
          "time": "20210908 04:23"
        },
        {
          "id": "MDRW2100045626.1.3",
          "form": "구워먹기도하고 고기에 곁들임으로 먹기도하고 볶아서도 먹고 삶아서도 먹어요!",
          "original_form": "구워먹기도하고 고기에 곁들임으로 먹기도하고 볶아서도 먹고 삶아서도 먹어요!",
          "speaker_id": "2",
          "time": "20210908 04:24"
        },
        {
          "id": "MDRW2100045626.1.4",
          "form": "너무 어려운가요??ㅠㅠ",
          "original_form": "너무 어려운가요??ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:25"
        },
        {
          "id": "MDRW2100045626.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ이럴수가 진짜 뭔지 모르겠는데요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ이럴수가 진짜 뭔지 모르겠는데요?",
          "speaker_id": "1",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045626.1.6",
          "form": "혹시 가늘은가요?",
          "original_form": "혹시 가늘은가요?",
          "speaker_id": "1",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045626.1.7",
          "form": "아닝ㅅ!!",
          "original_form": "아닝ㅅ!!",
          "speaker_id": "2",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045626.1.8",
          "form": "아니요!!",
          "original_form": "아니요!!",
          "speaker_id": "2",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045626.1.9",
          "form": "가늘지 않아요!",
          "original_form": "가늘지 않아요!",
          "speaker_id": "2",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045626.1.10",
          "form": "넓은데 고기를 안 싸먹는 거예요?",
          "original_form": "넓은데 고기를 안 싸먹는 거예요?",
          "speaker_id": "1",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045626.1.11",
          "form": "그런 야채도 있다구요....?",
          "original_form": "그런 야채도 있다구요....?",
          "speaker_id": "1",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045626.1.12",
          "form": "이단이다!",
          "original_form": "이단이다!",
          "speaker_id": "1",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045626.1.13",
          "form": "넓지 않아욬ㅋ",
          "original_form": "넓지 않아욬ㅋ",
          "speaker_id": "2",
          "time": "20210908 04:30"
        },
        {
          "id": "MDRW2100045626.1.14",
          "form": "제가 넓다고 했었나요??? ㅋㅋㅋ",
          "original_form": "제가 넓다고 했었나요??? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:30"
        },
        {
          "id": "MDRW2100045626.1.15",
          "form": "딱 고기 싸먹기는 애매한 너비인가요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "딱 고기 싸먹기는 애매한 너비인가요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:30"
        },
        {
          "id": "MDRW2100045626.1.16",
          "form": "잎채소는 맞는데 작아욬ㅋㅋㅋ",
          "original_form": "잎채소는 맞는데 작아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:30"
        },
        {
          "id": "MDRW2100045626.1.17",
          "form": "하긴 가늘지 않다가 넓다는 건 아니니께",
          "original_form": "하긴 가늘지 않다가 넓다는 건 아니니께",
          "speaker_id": "1",
          "time": "20210908 04:30"
        },
        {
          "id": "MDRW2100045626.1.18",
          "form": "설마 새싹인가요ㅋㅋㅋㅋㅋ",
          "original_form": "설마 새싹인가요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:31"
        },
        {
          "id": "MDRW2100045626.1.19",
          "form": "아니요! 새싹이 아닙니다!",
          "original_form": "아니요! 새싹이 아닙니다!",
          "speaker_id": "2",
          "time": "20210908 04:31"
        },
        {
          "id": "MDRW2100045626.1.20",
          "form": "혹시 부추..?",
          "original_form": "혹시 부추..?",
          "speaker_id": "1",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045626.1.21",
          "form": "으아아악 분하다",
          "original_form": "으아아악 분하다",
          "speaker_id": "1",
          "time": "20210908 04:33"
        },
        {
          "id": "MDRW2100045626.1.22",
          "form": "국내 재배가 되나요?",
          "original_form": "국내 재배가 되나요?",
          "speaker_id": "1",
          "time": "20210908 04:34"
        },
        {
          "id": "MDRW2100045626.1.23",
          "form": "엇 그거까진 잘 모르겠어요ㅠㅠ",
          "original_form": "엇 그거까진 잘 모르겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:35"
        },
        {
          "id": "MDRW2100045626.1.24",
          "form": "우와 진짜 어렵다",
          "original_form": "우와 진짜 어렵다",
