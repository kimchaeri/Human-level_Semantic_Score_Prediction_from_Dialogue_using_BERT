      "utterance": [
        {
          "id": "MDRW2100027290.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210902 11:08"
        },
        {
          "id": "MDRW2100027290.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100027290.1.3",
          "form": "학교다니시나용",
          "original_form": "학교다니시나용",
          "speaker_id": "2",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100027290.1.4",
          "form": "아니요ㅠ 다니고 있진 않아요",
          "original_form": "아니요ㅠ 다니고 있진 않아요",
          "speaker_id": "1",
          "time": "20210902 12:02"
        },
        {
          "id": "MDRW2100027290.1.5",
          "form": "그래도 편입을 할까 고민도했어요",
          "original_form": "그래도 편입을 할까 고민도했어요",
          "speaker_id": "1",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100027290.1.6",
          "form": "아 편입 하고픈 전공있으세요",
          "original_form": "아 편입 하고픈 전공있으세요",
          "speaker_id": "2",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100027290.1.7",
          "form": "저 하고싶어서 한다기보다",
          "original_form": "저 하고싶어서 한다기보다",
          "speaker_id": "1",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100027290.1.8",
          "form": "더 안정적인 직업 찾으려다보니",
          "original_form": "더 안정적인 직업 찾으려다보니",
          "speaker_id": "1",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100027290.1.9",
          "form": "특수교육 쪽 봤는데",
          "original_form": "특수교육 쪽 봤는데",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.10",
          "form": "오 정말요",
          "original_form": "오 정말요",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.11",
          "form": "포기했어요ㅜ",
          "original_form": "포기했어요ㅜ",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.12",
          "form": "제가 대학원이 교육쪽인데",
          "original_form": "제가 대학원이 교육쪽인데",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.13",
          "form": "직장 그만두고 갈수가 없더라고요",
          "original_form": "직장 그만두고 갈수가 없더라고요",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.14",
          "form": "대학원진학은 어떠세요",
          "original_form": "대학원진학은 어떠세요",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.15",
          "form": "오 그러시구나",
          "original_form": "오 그러시구나",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.16",
          "form": "아저도 직장인데 교육대학원은 야간이에요 보통",
          "original_form": "아저도 직장인데 교육대학원은 야간이에요 보통",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.17",
          "form": "대학원도 생각했는데",
          "original_form": "대학원도 생각했는데",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.18",
          "form": "7시수업시작",
          "original_form": "7시수업시작",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.19",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.20",
          "form": "기빨려서",
          "original_form": "기빨려서",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.21",
          "form": "일과병행하려니",
          "original_form": "일과병행하려니",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.22",
          "form": "할수있나싶어요",
          "original_form": "할수있나싶어요",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.23",
          "form": "저 지금해골이에요",
          "original_form": "저 지금해골이에요",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.24",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.25",
          "form": "ㅋㅋㅋㄱ 대단하십니다",
          "original_form": "ㅋㅋㅋㄱ 대단하십니다",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.26",
          "form": "나중에여유생기심 해용!",
          "original_form": "나중에여유생기심 해용!",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027290.1.27",
          "form": "후회중이지만 이미 발을 담가서",
          "original_form": "후회중이지만 이미 발을 담가서",
          "speaker_id": "2",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027290.1.28",
          "form": "일단 지금 자격증 1급 부터 따놓으려구요",
          "original_form": "일단 지금 자격증 1급 부터 따놓으려구요",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027290.1.29",
          "form": "교직 준비하세요?",
          "original_form": "교직 준비하세요?",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027290.1.30",
          "form": "네네 지금 공기업인데 교직으로 넘어가고싶어요",
          "original_form": "네네 지금 공기업인데 교직으로 넘어가고싶어요",
          "speaker_id": "2",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027290.1.31",
          "form": "원래꿈이어서요ㅠㅠ",
          "original_form": "원래꿈이어서요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027290.1.32",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100027290.1.33",
          "form": "공기업도 좋다던데",
          "original_form": "공기업도 좋다던데",
          "speaker_id": "1",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100027290.1.34",
          "form": "대단하세요",
          "original_form": "대단하세요",
          "speaker_id": "1",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100027290.1.35",
          "form": "임용 준비하시는거에요?",
          "original_form": "임용 준비하시는거에요?",
          "speaker_id": "1",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100027290.1.36",
          "form": "임용은 추후에 생각해보려고요",
          "original_form": "임용은 추후에 생각해보려고요",
          "speaker_id": "2",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027290.1.37",
          "form": "공기업 노잼이에요ㅠㅠ",
          "original_form": "공기업 노잼이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 12:53"
        }
      ]
    }
