      "utterance": [
        {
          "id": "MDRW2100033642.1.1",
          "form": "가족과 함께 생활하시나요?",
          "original_form": "가족과 함께 생활하시나요?",
          "speaker_id": "1",
          "time": "20210824 19:48"
        },
        {
          "id": "MDRW2100033642.1.2",
          "form": "네 ㅎ",
          "original_form": "네 ㅎ",
          "speaker_id": "2",
          "time": "20210824 19:54"
        },
        {
          "id": "MDRW2100033642.1.3",
          "form": "자취하세요?",
          "original_form": "자취하세요?",
          "speaker_id": "2",
          "time": "20210824 19:54"
        },
        {
          "id": "MDRW2100033642.1.4",
          "form": "저도 가족과 함께 생활해요, 예전에 잠깐 기숙사에서 살았어요",
          "original_form": "저도 가족과 함께 생활해요, 예전에 잠깐 기숙사에서 살았어요",
          "speaker_id": "1",
          "time": "20210824 21:06"
        },
        {
          "id": "MDRW2100033642.1.5",
          "form": "기숙사 생활 어땠오요?",
          "original_form": "기숙사 생활 어땠오요?",
          "speaker_id": "2",
          "time": "20210824 21:07"
        },
        {
          "id": "MDRW2100033642.1.6",
          "form": "룸메와 큰 충돌 없이 지내서 나쁘지 않았어요",
          "original_form": "룸메와 큰 충돌 없이 지내서 나쁘지 않았어요",
          "speaker_id": "1",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100033642.1.7",
          "form": "재밌을 거 같아요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "재밌을 거 같아요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:13"
        },
        {
          "id": "MDRW2100033642.1.8",
          "form": "기숙사에서 같이 배달시켜먹기도 하고 술도 마셔서 재밌긴 했어요ㅋㅋ",
          "original_form": "기숙사에서 같이 배달시켜먹기도 하고 술도 마셔서 재밌긴 했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100033642.1.9",
          "form": "배달 같이 시켜먹는게",
          "original_form": "배달 같이 시켜먹는게",
          "speaker_id": "2",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100033642.1.10",
          "form": "젤 좋을 거 같깅해요ㅋㅋㅋㅋㅋ",
          "original_form": "젤 좋을 거 같깅해요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100033642.1.11",
          "form": "조별과제를 할 때 같이 무임승차를 하는 조원에 대한 이야기도 하구요!",
          "original_form": "조별과제를 할 때 같이 무임승차를 하는 조원에 대한 이야기도 하구요!",
          "speaker_id": "1",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100033642.1.12",
          "form": "ㅋㅋㅋㅋ아 ㅠ 그런거라도 털어놓으면 낫죠",
          "original_form": "ㅋㅋㅋㅋ아 ㅠ 그런거라도 털어놓으면 낫죠",
          "speaker_id": "2",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100033642.1.13",
          "form": "장점도 많네요",
          "original_form": "장점도 많네요",
          "speaker_id": "2",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100033642.1.14",
          "form": "룸메와는 생활반경이 많이 겹쳐서 그러기 쉬운 것 같아요",
          "original_form": "룸메와는 생활반경이 많이 겹쳐서 그러기 쉬운 것 같아요",
          "speaker_id": "1",
          "time": "20210824 21:48"
        },
        {
          "id": "MDRW2100033642.1.15",
          "form": "잘 맞아서 다행이에요",
          "original_form": "잘 맞아서 다행이에요",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100033642.1.16",
          "form": "그게 은근 어렵잖아요",
          "original_form": "그게 은근 어렵잖아요",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100033642.1.17",
          "form": "같이 생활하는 사람이 잘 맞기 어려운데 그래도 다행이었어요ㅋㅋ",
          "original_form": "같이 생활하는 사람이 잘 맞기 어려운데 그래도 다행이었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033642.1.18",
          "form": "그러니가요 완전 좋으시겠어요!!",
          "original_form": "그러니가요 완전 좋으시겠어요!!",
          "speaker_id": "2",
          "time": "20210824 23:04"
        }
      ]
