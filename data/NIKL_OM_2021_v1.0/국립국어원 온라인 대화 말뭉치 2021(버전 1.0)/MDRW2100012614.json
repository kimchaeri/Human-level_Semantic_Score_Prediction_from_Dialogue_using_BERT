      "utterance": [
        {
          "id": "MDRW2100012614.1.1",
          "form": "연애는 몇년하셨나요?",
          "original_form": "연애는 몇년하셨나요?",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012614.1.2",
          "form": "전 4년했어요",
          "original_form": "전 4년했어요",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012614.1.3",
          "form": "전 2년 했어요^^",
          "original_form": "전 2년 했어요^^",
          "speaker_id": "2",
          "time": "20210825 09:38"
        },
        {
          "id": "MDRW2100012614.1.4",
          "form": "아 딱좋은데요",
          "original_form": "아 딱좋은데요",
          "speaker_id": "1",
          "time": "20210825 09:39"
        },
        {
          "id": "MDRW2100012614.1.5",
          "form": "넘 오래하면 안좋아요",
          "original_form": "넘 오래하면 안좋아요",
          "speaker_id": "1",
          "time": "20210825 09:39"
        },
        {
          "id": "MDRW2100012614.1.6",
          "form": "어떤 부분이 안 좋으시던가요??",
          "original_form": "어떤 부분이 안 좋으시던가요??",
          "speaker_id": "2",
          "time": "20210825 09:41"
        },
        {
          "id": "MDRW2100012614.1.7",
          "form": "결혼해보니 연애랑 결혼은 다르긴 하더라고요ㅎㅎ",
          "original_form": "결혼해보니 연애랑 결혼은 다르긴 하더라고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 09:41"
        },
        {
          "id": "MDRW2100012614.1.8",
          "form": "어래되니 잘싸우고 넘다 알게되서 별로예요",
          "original_form": "어래되니 잘싸우고 넘다 알게되서 별로예요",
          "speaker_id": "1",
          "time": "20210825 09:41"
        },
        {
          "id": "MDRW2100012614.1.9",
          "form": "맞아요 잘 싸우는 건 있는듯합니다ㅎㅎ",
          "original_form": "맞아요 잘 싸우는 건 있는듯합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 09:42"
        },
        {
          "id": "MDRW2100012614.1.10",
          "form": "네 ㅡㅡ신비감도 없고",
          "original_form": "네 ㅡㅡ신비감도 없고",
          "speaker_id": "1",
          "time": "20210825 09:43"
        },
        {
          "id": "MDRW2100012614.1.11",
          "form": "적당히 모를때하는게 좋은거 같아요 ㅎㅎ",
          "original_form": "적당히 모를때하는게 좋은거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 09:44"
        },
        {
          "id": "MDRW2100012614.1.12",
          "form": "성격은 서로 어떤가요?",
          "original_form": "성격은 서로 어떤가요?",
          "speaker_id": "1",
          "time": "20210825 09:44"
        },
        {
          "id": "MDRW2100012614.1.13",
          "form": "전 성격 정반대예요",
          "original_form": "전 성격 정반대예요",
          "speaker_id": "1",
          "time": "20210825 09:44"
        },
        {
          "id": "MDRW2100012614.1.14",
          "form": "결혼은 반대로한대요 ㅋㅋ",
          "original_form": "결혼은 반대로한대요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:44"
        },
        {
          "id": "MDRW2100012614.1.15",
          "form": "맞아요ㅋㅋㅋㅋ 반대인것 같아요",
          "original_form": "맞아요ㅋㅋㅋㅋ 반대인것 같아요",
          "speaker_id": "2",
          "time": "20210825 09:56"
        },
        {
          "id": "MDRW2100012614.1.16",
          "form": "전 미지근",
          "original_form": "전 미지근",
          "speaker_id": "1",
          "time": "20210825 09:57"
        },
        {
          "id": "MDRW2100012614.1.17",
          "form": "어제도 우린 너무 안 맞아~이랬어요ㅋㅋㅋ",
          "original_form": "어제도 우린 너무 안 맞아~이랬어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:57"
        },
        {
          "id": "MDRW2100012614.1.18",
          "form": "남편은불입니다 ㅡㅡ",
          "original_form": "남편은불입니다 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210825 09:57"
        },
        {
          "id": "MDRW2100012614.1.19",
          "form": "저도요 ㅋㅋ똑샅네요",
          "original_form": "저도요 ㅋㅋ똑샅네요",
          "speaker_id": "1",
          "time": "20210825 09:57"
        },
        {
          "id": "MDRW2100012614.1.20",
          "form": "다 비슷한것 같아요",
          "original_form": "다 비슷한것 같아요",
          "speaker_id": "1",
          "time": "20210825 09:58"
        },
        {
          "id": "MDRW2100012614.1.21",
          "form": "이상형과 결혼하거나 맞는사람 거의 없을듯요 ㅎㅎ",
          "original_form": "이상형과 결혼하거나 맞는사람 거의 없을듯요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 09:59"
        },
        {
          "id": "MDRW2100012614.1.22",
          "form": "맞아요. 맞는사람은 진짜 복 받은 사람!",
          "original_form": "맞아요. 맞는사람은 진짜 복 받은 사람!",
          "speaker_id": "2",
          "time": "20210825 09:59"
        },
        {
          "id": "MDRW2100012614.1.23",
          "form": "네 전복이 없네요",
          "original_form": "네 전복이 없네요",
          "speaker_id": "1",
          "time": "20210825 10:00"
        },
        {
          "id": "MDRW2100012614.1.24",
          "form": "전생에 나라를 말아먹었나 ㅋㅋ",
          "original_form": "전생에 나라를 말아먹었나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:00"
        },
        {
          "id": "MDRW2100012614.1.25",
          "form": "ㅋㅋㅋㅋㅋ그래도 한번씩 이런 부분은 괜찮네~있지 않나요??",
          "original_form": "ㅋㅋㅋㅋㅋ그래도 한번씩 이런 부분은 괜찮네~있지 않나요??",
          "speaker_id": "2",
          "time": "20210825 10:02"
        },
        {
          "id": "MDRW2100012614.1.26",
          "form": "네 아 제가",
          "original_form": "네 아 제가",
          "speaker_id": "1",
          "time": "20210825 10:07"
        },
        {
          "id": "MDRW2100012614.1.27",
          "form": "허술한데 잘챙겨요 ㅋㅋ",
          "original_form": "허술한데 잘챙겨요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:07"
        },
        {
          "id": "MDRW2100012614.1.28",
          "form": "논리적인 사람이죠",
          "original_form": "논리적인 사람이죠",
          "speaker_id": "1",
          "time": "20210825 10:07"
        },
        {
          "id": "MDRW2100012614.1.29",
          "form": "오오 좋네요",
          "original_form": "오오 좋네요{emoji:♡}",
          "speaker_id": "2",
          "time": "20210825 10:08"
        },
        {
          "id": "MDRW2100012614.1.30",
          "form": "어떤점 좋으세요?",
          "original_form": "어떤점 좋으세요?",
          "speaker_id": "1",
          "time": "20210825 10:08"
        },
        {
          "id": "MDRW2100012614.1.31",
          "form": "저희 신랑 경제관념 확실한거요ㅎㅎ",
          "original_form": "저희 신랑 경제관념 확실한거요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 10:09"
        },
        {
          "id": "MDRW2100012614.1.32",
          "form": "아 그게 짱이죠",
          "original_form": "아 그게 짱이죠",
          "speaker_id": "1",
          "time": "20210825 10:09"
        },
        {
          "id": "MDRW2100012614.1.33",
          "form": "금방 부자 되시겠어요",
          "original_form": "금방 부자 되시겠어요",
          "speaker_id": "1",
          "time": "20210825 10:09"
        },
        {
          "id": "MDRW2100012614.1.34",
          "form": "그거 흐리면 못살아요",
          "original_form": "그거 흐리면 못살아요",
          "speaker_id": "1",
          "time": "20210825 10:09"
        },
        {
          "id": "MDRW2100012614.1.35",
          "form": "ㅋㅋㅋㅋ아뇨 부자는 먼 얘기.....",
          "original_form": "ㅋㅋㅋㅋ아뇨 부자는 먼 얘기.....",
          "speaker_id": "2",
          "time": "20210825 10:10"
        },
        {
          "id": "MDRW2100012614.1.36",
          "form": "ㅋㅋ그래도 복입니다",
          "original_form": "ㅋㅋ그래도 복입니다",
          "speaker_id": "1",
          "time": "20210825 10:10"
        },
        {
          "id": "MDRW2100012614.1.37",
          "form": "맞아요.이 부분은 진짜 믿고 맡겨요!",
