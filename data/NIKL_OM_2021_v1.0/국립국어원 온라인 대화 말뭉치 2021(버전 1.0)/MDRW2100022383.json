      "utterance": [
        {
          "id": "MDRW2100022383.1.1",
          "form": "뮤지컬이나 연극 좋아하시나요",
          "original_form": "뮤지컬이나 연극 좋아하시나요",
          "speaker_id": "2",
          "time": "20210907 16:41"
        },
        {
          "id": "MDRW2100022383.1.2",
          "form": "저는 안타깝게도 안좋아해요",
          "original_form": "저는 안타깝게도 안좋아해요",
          "speaker_id": "1",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100022383.1.3",
          "form": "공연시설은 답답해하고",
          "original_form": "공연시설은 답답해하고",
          "speaker_id": "1",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100022383.1.4",
          "form": "음향에 귀아파해서요",
          "original_form": "음향에 귀아파해서요",
          "speaker_id": "1",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100022383.1.5",
          "form": "소리에 예민하면 그럴수있죠",
          "original_form": "소리에 예민하면 그럴수있죠",
          "speaker_id": "2",
          "time": "20210907 16:57"
        },
        {
          "id": "MDRW2100022383.1.6",
          "form": "이보단 조용한 전시회는 어떠신가요",
          "original_form": "이보단 조용한 전시회는 어떠신가요",
          "speaker_id": "2",
          "time": "20210907 16:57"
        },
        {
          "id": "MDRW2100022383.1.7",
          "form": "전시회는 괜찮은데",
          "original_form": "전시회는 괜찮은데",
          "speaker_id": "1",
          "time": "20210907 17:05"
        },
        {
          "id": "MDRW2100022383.1.8",
          "form": "저희세대나 문화를 즐기던 세다가 아니다 보니 기회를 잡지 않았던 거 같습니다",
          "original_form": "저희세대나 문화를 즐기던 세다가 아니다 보니 기회를 잡지 않았던 거 같습니다",
          "speaker_id": "1",
          "time": "20210907 17:06"
        },
        {
          "id": "MDRW2100022383.1.9",
          "form": "name2님은 뭘 좋아하시나요?",
          "original_form": "&name2&님은 뭘 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 17:06"
        },
        {
          "id": "MDRW2100022383.1.10",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100022383.1.11",
          "form": "전 뮤지컬 좋아해요ㅋㅋ",
          "original_form": "전 뮤지컬 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100022383.1.12",
          "form": "가장 최근에 본 작품이 뭔가요?",
          "original_form": "가장 최근에 본 작품이 뭔가요?",
          "speaker_id": "1",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100022383.1.13",
          "form": "코로나 때문에 최근엔 잘 못가서ㅋㅋ",
          "original_form": "코로나 때문에 최근엔 잘 못가서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100022383.1.14",
          "form": "모짜르트입니당",
          "original_form": "모짜르트입니당",
          "speaker_id": "2",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100022383.1.15",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100022383.1.16",
          "form": "제 아내도 공연 좋아하는데",
          "original_form": "제 아내도 공연 좋아하는데",
          "speaker_id": "1",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100022383.1.17",
          "form": "제가 안좋아하니까 같이 못다녀서 서운해 하더군요",
          "original_form": "제가 안좋아하니까 같이 못다녀서 서운해 하더군요",
          "speaker_id": "1",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100022383.1.18",
          "form": "보통 혼자 보러 다니십니까?",
          "original_form": "보통 혼자 보러 다니십니까?",
          "speaker_id": "1",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100022383.1.19",
          "form": "보통 여자친구랑 보러다닙니다",
          "original_form": "보통 여자친구랑 보러다닙니다",
          "speaker_id": "2",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100022383.1.20",
          "form": "여자친구분도 뮤지컬을 좋아하나요?",
          "original_form": "여자친구분도 뮤지컬을 좋아하나요?",
