      "utterance": [
        {
          "id": "MDRW2100020440.1.1",
          "form": "물론 일주일차지만...ㅋㅋ",
          "original_form": "물론 일주일차지만...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:48"
        },
        {
          "id": "MDRW2100020440.1.2",
          "form": "그리고 약간",
          "original_form": "그리고 약간",
          "speaker_id": "1",
          "time": "20210828 05:48"
        },
        {
          "id": "MDRW2100020440.1.3",
          "form": "저는 아직 결혼 안했으니까~",
          "original_form": "저는 아직 결혼 안했으니까~",
          "speaker_id": "1",
          "time": "20210828 05:48"
        },
        {
          "id": "MDRW2100020440.1.4",
          "form": "하면서 떠밀때도 있더라구요",
          "original_form": "하면서 떠밀때도 있더라구요",
          "speaker_id": "1",
          "time": "20210828 05:48"
        },
        {
          "id": "MDRW2100020440.1.5",
          "form": "인사발령 시즌에",
          "original_form": "인사발령 시즌에",
          "speaker_id": "1",
          "time": "20210828 05:48"
        },
        {
          "id": "MDRW2100020440.1.6",
          "form": "지역을 옮겼어야했는데",
          "original_form": "지역을 옮겼어야했는데",
          "speaker_id": "1",
          "time": "20210828 05:49"
        },
        {
          "id": "MDRW2100020440.1.7",
          "form": "맞아요 현재 저도 그래서 뭘 많이 맡고 있어요ㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "맞아요 현재 저도 그래서 뭘 많이 맡고 있어요ㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 05:49"
        },
        {
          "id": "MDRW2100020440.1.8",
          "form": "저보고 결혼안했으니까 니가 가~",
          "original_form": "저보고 결혼안했으니까 니가 가~",
          "speaker_id": "1",
          "time": "20210828 05:49"
        },
        {
          "id": "MDRW2100020440.1.9",
          "form": "출장도 다 보내시고ㅋㅋㅋ",
          "original_form": "출장도 다 보내시고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:49"
        },
        {
          "id": "MDRW2100020440.1.10",
          "form": "맨날 그랬어요",
          "original_form": "맨날 그랬어요",
          "speaker_id": "1",
          "time": "20210828 05:49"
        },
        {
          "id": "MDRW2100020440.1.11",
          "form": "헐 맞아요",
          "original_form": "헐 맞아요",
          "speaker_id": "1",
          "time": "20210828 05:49"
        },
        {
          "id": "MDRW2100020440.1.12",
          "form": "완전 공감",
          "original_form": "완전 공감",
          "speaker_id": "1",
          "time": "20210828 05:49"
        },
        {
          "id": "MDRW2100020440.1.13",
          "form": "토요일마다 출장 잡혀있어요ㅋㅋㅋ",
          "original_form": "토요일마다 출장 잡혀있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:49"
        },
        {
          "id": "MDRW2100020440.1.14",
          "form": "헐ㅠㅠㅠ 너무해요",
          "original_form": "헐ㅠㅠㅠ 너무해요",
          "speaker_id": "1",
          "time": "20210828 05:49"
        },
        {
          "id": "MDRW2100020440.1.15",
          "form": "미혼 직장인이란...ㅋㅋㅋ",
          "original_form": "미혼 직장인이란...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:50"
        },
        {
          "id": "MDRW2100020440.1.16",
          "form": "남편 핑계 많이 대시더라구용...",
          "original_form": "남편 핑계 많이 대시더라구용...",
          "speaker_id": "1",
          "time": "20210828 05:50"
        },
        {
          "id": "MDRW2100020440.1.17",
          "form": "남편 없어서 서러워욬ㅋㅋㅋ",
          "original_form": "남편 없어서 서러워욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:50"
        },
        {
          "id": "MDRW2100020440.1.18",
          "form": "그리고 저 교육계 종사해서",
          "original_form": "그리고 저 교육계 종사해서",
          "speaker_id": "2",
          "time": "20210828 05:50"
        },
        {
          "id": "MDRW2100020440.1.19",
          "form": "학부모님들도 좀 그래요ㅋㅋㅋㅋ",
          "original_form": "학부모님들도 좀 그래요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:50"
        },
        {
          "id": "MDRW2100020440.1.20",
          "form": "어머ㅠㅠㅠ",
          "original_form": "어머ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 05:50"
        },
        {
          "id": "MDRW2100020440.1.21",
          "form": "미혼에 대한 불신이 좀 있지않나..ㅋㅋㅋ",
          "original_form": "미혼에 대한 불신이 좀 있지않나..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:50"
        },
        {
          "id": "MDRW2100020440.1.22",
          "form": "그 힘들다는....",
          "original_form": "그 힘들다는....",
          "speaker_id": "1",
          "time": "20210828 05:50"
        },
        {
          "id": "MDRW2100020440.1.23",
          "form": "아아 넌 결혼 안해봐서 몰라 이런거군요",
