      "utterance": [
        {
          "id": "MDRW2100042077.1.1",
          "form": "비가 내리네요",
          "original_form": "비가 내리네요",
          "speaker_id": "1",
          "time": "20210906 19:28"
        },
        {
          "id": "MDRW2100042077.1.2",
          "form": "name2님 계신 곳도 비가 오나요?",
          "original_form": "&name2&님 계신 곳도 비가 오나요?",
          "speaker_id": "1",
          "time": "20210906 19:28"
        },
        {
          "id": "MDRW2100042077.1.3",
          "form": "네 몰랐는데 지금 보니 비가 조금 오네요!",
          "original_form": "네 몰랐는데 지금 보니 비가 조금 오네요!",
          "speaker_id": "2",
          "time": "20210906 19:30"
        },
        {
          "id": "MDRW2100042077.1.4",
          "form": "아으.. 그래서 제 무릎이 아팠나봐요..ㅜㅡㅜ",
          "original_form": "아으.. 그래서 제 무릎이 아팠나봐요..ㅜㅡㅜ",
          "speaker_id": "2",
          "time": "20210906 19:30"
        },
        {
          "id": "MDRW2100042077.1.5",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042077.1.6",
          "form": "무릎이 일기예보군요",
          "original_form": "무릎이 일기예보군요",
          "speaker_id": "1",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042077.1.7",
          "form": "ㅎㅎ 네 기압차 어쩌구... 해서 관절이 영향 받는다더라구요",
          "original_form": "ㅎㅎ 네 기압차 어쩌구... 해서 관절이 영향 받는다더라구요",
          "speaker_id": "2",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042077.1.8",
          "form": "날씨의 영향이 있죠",
          "original_form": "날씨의 영향이 있죠",
          "speaker_id": "1",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100042077.1.9",
          "form": "컨디션도 그렇고",
          "original_form": "컨디션도 그렇고",
          "speaker_id": "1",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100042077.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100042077.1.11",
          "form": "지지난주였나 일주일내내 비올때",
          "original_form": "지지난주였나 일주일내내 비올때",
          "speaker_id": "2",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100042077.1.12",
          "form": "아휴",
          "original_form": "아휴",
          "speaker_id": "1",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100042077.1.13",
          "form": "그때 정말 몸도 축 처지고",
          "original_form": "그때 정말 몸도 축 처지고",
          "speaker_id": "1",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100042077.1.14",
          "form": "아침에 일어나는데 몸이 너무 무거웠어요..",
          "original_form": "아침에 일어나는데 몸이 너무 무거웠어요..",
          "speaker_id": "2",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100042077.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100042077.1.16",
          "form": "눅눅하고",
          "original_form": "눅눅하고",
          "speaker_id": "1",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100042077.1.17",
          "form": "그야말로 몸이 찌뿌둥 그 자체 ",
          "original_form": "그야말로 몸이 찌뿌둥 그 자체 ",
          "speaker_id": "2",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100042077.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100042077.1.19",
          "form": "비 오고 나면 기온도 떨어지겠네요",
          "original_form": "비 오고 나면 기온도 떨어지겠네요",
          "speaker_id": "1",
          "time": "20210906 19:34"
        },
        {
          "id": "MDRW2100042077.1.20",
          "form": "네~ 지금도 베란다 문 열어놨더니 바람이 솔솔 들어와여 ㅎㅎ",
          "original_form": "네~ 지금도 베란다 문 열어놨더니 바람이 솔솔 들어와여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042077.1.21",
          "form": "저는 벌써 긴팔 긴바지를 입었습니다 ㅎㅎ",
          "original_form": "저는 벌써 긴팔 긴바지를 입었습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042077.1.22",
          "form": "앗 추위를 잘 타시나요?",
          "original_form": "앗 추위를 잘 타시나요?",
          "speaker_id": "1",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042077.1.23",
          "form": "전 아직까진 반팔 반바지",
          "original_form": "전 아직까진 반팔 반바지",
          "speaker_id": "1",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042077.1.24",
          "form": "근데 곧 긴소매로 갈아타야할듯요ㅜ",
          "original_form": "근데 곧 긴소매로 갈아타야할듯요ㅜ",
          "speaker_id": "1",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100042077.1.25",
          "form": "네ㅠ",
          "original_form": "네ㅠ",
          "speaker_id": "2",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100042077.1.26",
          "form": "환절기라 비염도 너무 심해져서",
          "original_form": "환절기라 비염도 너무 심해져서",
          "speaker_id": "2",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100042077.1.27",
          "form": "도대체 원인이 뭘까하다가 긴팔입으니까 좀 덜하더라구욬ㅋㅋㅋㅋㅋ",
          "original_form": "도대체 원인이 뭘까하다가 긴팔입으니까 좀 덜하더라구욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100042077.1.28",
          "form": "춥진않았는데... 따뜻함을 추구해야 몸이 안정이되는 ",
