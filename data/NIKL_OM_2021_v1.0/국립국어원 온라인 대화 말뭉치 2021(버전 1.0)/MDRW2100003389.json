      "utterance": [
        {
          "id": "MDRW2100003389.1.1",
          "form": "휴양림 가는거 말함???",
          "original_form": "휴양림 가는거 말함???",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003389.1.2",
          "form": "언제 예약했냐고하지??",
          "original_form": "언제 예약했냐고하지??",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003389.1.3",
          "form": "추석연휴지나고 23,24일 ㅋㅋ",
          "original_form": "추석연휴지나고 23,24일 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:56"
        },
        {
          "id": "MDRW2100003389.1.4",
          "form": "누구한테 말해? 회사?",
          "original_form": "누구한테 말해? 회사?",
          "speaker_id": "2",
          "time": "20210820 22:56"
        },
        {
          "id": "MDRW2100003389.1.5",
          "form": "아니 와이프 ㅋㅋㅋ 첫 여행 아냐??",
          "original_form": "아니 와이프 ㅋㅋㅋ 첫 여행 아냐??",
          "speaker_id": "1",
          "time": "20210820 23:00"
        },
        {
          "id": "MDRW2100003389.1.6",
          "form": "애기랑 셋이 가는",
          "original_form": "애기랑 셋이 가는",
          "speaker_id": "1",
          "time": "20210820 23:00"
        },
        {
          "id": "MDRW2100003389.1.7",
          "form": "말했지 ㅋㅋ 같이 가는데 서로 상의필수..",
          "original_form": "말했지 ㅋㅋ 같이 가는데 서로 상의필수..",
          "speaker_id": "2",
          "time": "20210820 23:03"
        },
        {
          "id": "MDRW2100003389.1.8",
          "form": "넌 언제 여행가?",
          "original_form": "넌 언제 여행가?",
          "speaker_id": "2",
          "time": "20210820 23:03"
        },
        {
          "id": "MDRW2100003389.1.9",
          "form": "아니 여행이 갑작스러울수있잖아 밖에서 케어하는게 ㅋㅋㅋㅋ",
          "original_form": "아니 여행이 갑작스러울수있잖아 밖에서 케어하는게 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:04"
        },
        {
          "id": "MDRW2100003389.1.10",
          "form": "동의해서 다행이네",
          "original_form": "동의해서 다행이네",
          "speaker_id": "1",
          "time": "20210820 23:04"
        },
        {
          "id": "MDRW2100003389.1.11",
          "form": "나는 9월 중순에..",
          "original_form": "나는 9월 중순에..",
          "speaker_id": "1",
          "time": "20210820 23:04"
        },
        {
          "id": "MDRW2100003389.1.12",
          "form": "ㅋㅋㅋ그치 9월이라 될거같아서",
          "original_form": "ㅋㅋㅋ그치 9월이라 될거같아서",
          "speaker_id": "2",
          "time": "20210820 23:04"
        },
        {
          "id": "MDRW2100003389.1.13",
          "form": "애기 많이 돌아댕겨줘야돼",
          "original_form": "애기 많이 돌아댕겨줘야돼",
          "speaker_id": "2",
          "time": "20210820 23:05"
        },
        {
          "id": "MDRW2100003389.1.14",
          "form": "맞아.. 애기한테도 좋을듯",
          "original_form": "맞아.. 애기한테도 좋을듯",
          "speaker_id": "1",
          "time": "20210820 23:05"
        },
        {
          "id": "MDRW2100003389.1.15",
          "form": "낯선 환경 경험 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "낯선 환경 경험 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:05"
        },
        {
          "id": "MDRW2100003389.1.16",
          "form": "ㅋㅋㅋ그래야 활발해진대",
          "original_form": "ㅋㅋㅋ그래야 활발해진대",
          "speaker_id": "2",
          "time": "20210820 23:06"
        },
        {
          "id": "MDRW2100003389.1.17",
          "form": "여기저기 싸돌아다녀야되는데",
          "original_form": "여기저기 싸돌아다녀야되는데",
          "speaker_id": "2",
          "time": "20210820 23:06"
        },
        {
          "id": "MDRW2100003389.1.18",
          "form": "코로나가 문제다 후",
          "original_form": "코로나가 문제다 후",
          "speaker_id": "2",
          "time": "20210820 23:07"
        },
        {
          "id": "MDRW2100003389.1.19",
          "form": "에휴 여름휴가 하나 보고 일년 버티는건데",
          "original_form": "에휴 여름휴가 하나 보고 일년 버티는건데",
          "speaker_id": "1",
          "time": "20210820 23:08"
        },
        {
          "id": "MDRW2100003389.1.20",
          "form": "여행계획 미리부터 세우고",
          "original_form": "여행계획 미리부터 세우고",
          "speaker_id": "1",
          "time": "20210820 23:08"
        },
        {
          "id": "MDRW2100003389.1.21",
          "form": "작년부터 글렀다...",
          "original_form": "작년부터 글렀다...",
          "speaker_id": "1",
          "time": "20210820 23:09"
        },
        {
          "id": "MDRW2100003389.1.22",
          "form": "벌써 이게 2년이 다되가네",
          "original_form": "벌써 이게 2년이 다되가네",
          "speaker_id": "2",
          "time": "20210820 23:09"
        },
        {
          "id": "MDRW2100003389.1.23",
          "form": "작년초에 러시아 다녀오길 잘했다",
          "original_form": "작년초에 러시아 다녀오길 잘했다",
          "speaker_id": "2",
          "time": "20210820 23:09"
        },
        {
          "id": "MDRW2100003389.1.24",
          "form": "ㅋㅋㅋㅋㅋ아 잘했다 그건",
          "original_form": "ㅋㅋㅋㅋㅋ아 잘했다 그건",
          "speaker_id": "1",
          "time": "20210820 23:10"
        },
        {
          "id": "MDRW2100003389.1.25",
          "form": "그때 발 다쳤었나 다녀와서 다친건가",
          "original_form": "그때 발 다쳤었나 다녀와서 다친건가",
          "speaker_id": "1",
          "time": "20210820 23:10"
        },
        {
          "id": "MDRW2100003389.1.26",
          "form": "다녀와서 다쳤을걸 ㅋㅋ",
          "original_form": "다녀와서 다쳤을걸 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003389.1.27",
          "form": "아직도 발목이 안돌아온다",
          "original_form": "아직도 발목이 안돌아온다",
          "speaker_id": "2",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003389.1.28",
          "form": "이대로살아야될듯",
          "original_form": "이대로살아야될듯",
          "speaker_id": "2",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003389.1.29",
          "form": "보드타러 또 여행가고싶다",
          "original_form": "보드타러 또 여행가고싶다",
          "speaker_id": "2",
          "time": "20210820 23:12"
        },
        {
          "id": "MDRW2100003389.1.30",
          "form": "헐 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003389.1.31",
          "form": "아직도 신기하다 보드 하나만 보고 간 여행",
          "original_form": "아직도 신기하다 보드 하나만 보고 간 여행",
          "speaker_id": "1",
          "time": "20210820 23:15"
        }
      ]
    }
