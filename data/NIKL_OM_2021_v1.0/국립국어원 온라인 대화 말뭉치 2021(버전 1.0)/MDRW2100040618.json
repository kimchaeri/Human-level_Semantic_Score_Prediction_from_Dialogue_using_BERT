        {
          "id": "MDRW2100040618.1.1",
          "form": "아침 드셨어요?",
          "original_form": "아침 드셨어요?",
          "speaker_id": "2",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100040618.1.2",
          "form": "넵!!!! 저녁 드셧어요?",
          "original_form": "넵!!!! 저녁 드셧어요?",
          "speaker_id": "1",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100040618.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ네네 벌써 저녁이 되었네요 ㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ네네 벌써 저녁이 되었네요 ㅋ",
          "speaker_id": "2",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100040618.1.4",
          "form": "수육 먹엇어요 ㅋㅋ",
          "original_form": "수육 먹엇어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100040618.1.5",
          "form": "맛있는거 드셨어요?",
          "original_form": "맛있는거 드셨어요?",
          "speaker_id": "2",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100040618.1.6",
          "form": "저는 폭립 먹었어요 ㅁㅁ",
          "original_form": "저는 폭립 먹었어요 ㅁㅁ",
          "speaker_id": "1",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100040618.1.7",
          "form": "맛집이라 그런지 웨이팅이 좀 있더라구요 ㅎㅎ",
          "original_form": "맛집이라 그런지 웨이팅이 좀 있더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100040618.1.8",
          "form": "와.........폭립.....ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와.........폭립.....ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:31"
        },
        {
          "id": "MDRW2100040618.1.9",
          "form": "진짜 못먹은지 좀 된것같네요 ㅋㅋㅋㅋㅋㅋㅋㅋ와,,,먹고싶네요 ㅋㅋ",
          "original_form": "진짜 못먹은지 좀 된것같네요 ㅋㅋㅋㅋㅋㅋㅋㅋ와,,,먹고싶네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:31"
        },
        {
          "id": "MDRW2100040618.1.10",
          "form": "맛집 가셨군요 ㅋ",
          "original_form": "맛집 가셨군요 ㅋ",
          "speaker_id": "2",
          "time": "20210905 20:31"
        },
        {
          "id": "MDRW2100040618.1.11",
          "form": "네네! 점심시간에 가서 그런지 ㅠㅠ",
          "original_form": "네네! 점심시간에 가서 그런지 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 20:31"
        },
        {
          "id": "MDRW2100040618.1.12",
          "form": "기다린만큼 맛있긴 하드라구요 ㅎㅎ",
          "original_form": "기다린만큼 맛있긴 하드라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:31"
        },
        {
          "id": "MDRW2100040618.1.13",
          "form": "얼마나 기다리신거에요?",
          "original_form": "얼마나 기다리신거에요?",
          "speaker_id": "2",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100040618.1.14",
          "form": "한 삼십분 기달렸어요!! 그래도 줄이 금방 금방 줄어서 ㅠㅜ",
          "original_form": "한 삼십분 기달렸어요!! 그래도 줄이 금방 금방 줄어서 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100040618.1.15",
          "form": "아 네네 ㅋㅋ 좀 기다리셨네요 ㅋㅋㅋ 30분도 소중하다며 ㅋㅋ",
          "original_form": "아 네네 ㅋㅋ 좀 기다리셨네요 ㅋㅋㅋ 30분도 소중하다며 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100040618.1.16",
          "form": "네네 ㅠㅠ 배고플때 가서 ㅠㅜ",
          "original_form": "네네 ㅠㅠ 배고플때 가서 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100040618.1.17",
          "form": "맛집 좋아하세요?",
          "original_form": "맛집 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100040618.1.18",
          "form": "네 맛집 너무 좋아하죠  ㅋ",
          "original_form": "네 맛집 너무 좋아하죠  ㅋ",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100040618.1.19",
          "form": "근데 신랑은 맛집에서 기다리는 그런걸 이해를 못해요",
          "original_form": "근데 신랑은 맛집에서 기다리는 그런걸 이해를 못해요",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100040618.1.20",
          "form": "와 저도 친구 아니였으면 다른곳 갔을거같아요 ㅋㅋㅋ",
          "original_form": "와 저도 친구 아니였으면 다른곳 갔을거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100040618.1.21",
          "form": "기다리다 먹으면 맛있긴 한데 힘들어서 ㅋㅋ",
          "original_form": "기다리다 먹으면 맛있긴 한데 힘들어서 ㅋㅋ",
