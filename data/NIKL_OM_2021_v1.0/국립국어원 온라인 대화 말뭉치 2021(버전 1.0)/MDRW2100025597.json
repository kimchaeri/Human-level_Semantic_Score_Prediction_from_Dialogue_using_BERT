      "utterance": [
        {
          "id": "MDRW2100025597.1.1",
          "form": "맞아요 반려동물은 진짜 책임감도 엄청중요하죠.!",
          "original_form": "맞아요 반려동물은 진짜 책임감도 엄청중요하죠.!",
          "speaker_id": "1",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100025597.1.2",
          "form": "name2님네 날씨는 어떤가용?",
          "original_form": "&name2&님네 날씨는 어떤가용?",
          "speaker_id": "1",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100025597.1.3",
          "form": "제가 사는 지역은 비가내리고있어요",
          "original_form": "제가 사는 지역은 비가내리고있어요",
          "speaker_id": "1",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100025597.1.4",
          "form": "여기도 어두컴컴해요",
          "original_form": "여기도 어두컴컴해요",
          "speaker_id": "2",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100025597.1.5",
          "form": "아직 비는 안오는가봐요~~",
          "original_form": "아직 비는 안오는가봐요~~",
          "speaker_id": "1",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100025597.1.6",
          "form": "비올 확률이 높다고 해요",
          "original_form": "비올 확률이 높다고 해요",
          "speaker_id": "2",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100025597.1.7",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100025597.1.8",
          "form": "우중충한 날엔 뭔가 우울해져서",
          "original_form": "우중충한 날엔 뭔가 우울해져서",
          "speaker_id": "2",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025597.1.9",
          "form": "커피를 많이 마시고 싶어요",
          "original_form": "커피를 많이 마시고 싶어요",
          "speaker_id": "2",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025597.1.10",
          "form": "비 많이 오나요?",
          "original_form": "비 많이 오나요?",
          "speaker_id": "2",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025597.1.11",
          "form": "이제 조금 줄어든거같아요",
          "original_form": "이제 조금 줄어든거같아요",
          "speaker_id": "1",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100025597.1.12",
          "form": "일기예보보니까",
          "original_form": "일기예보보니까",
          "speaker_id": "1",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100025597.1.13",
          "form": "쭈욱- 비소식이네요ㅜ",
          "original_form": "쭈욱- 비소식이네요ㅜ",
          "speaker_id": "1",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100025597.1.14",
          "form": "앗 ㅠㅠ",
          "original_form": "앗 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100025597.1.15",
          "form": "비올때 양말 젖는거 어렸을때부터 싫어했거든요",
          "original_form": "비올때 양말 젖는거 어렸을때부터 싫어했거든요",
          "speaker_id": "2",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100025597.1.16",
          "form": "집에서 비내리는거 구경하는건 좋지만",
          "original_form": "집에서 비내리는거 구경하는건 좋지만",
          "speaker_id": "2",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100025597.1.17",
          "form": "나가는건 싫어요 ㅠㅠ",
          "original_form": "나가는건 싫어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100025597.1.18",
          "form": "비내릴땐 장화가 필수품인거 같아요!",
          "original_form": "비내릴땐 장화가 필수품인거 같아요!",
          "speaker_id": "2",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100025597.1.19",
          "form": "ㅋㅋ저두요 비엄청엄청 좋아하는데",
          "original_form": "ㅋㅋ저두요 비엄청엄청 좋아하는데",
          "speaker_id": "1",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100025597.1.20",
          "form": "나가는건 딱질색..ㅎㅎ;",
          "original_form": "나가는건 딱질색..ㅎㅎ;",
          "speaker_id": "1",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100025597.1.21",
          "form": "집에있을때 오면 그렇게 좋더라구요",
          "original_form": "집에있을때 오면 그렇게 좋더라구요",
          "speaker_id": "1",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100025597.1.22",
          "form": "소리도 좋고 음악틀어놓으면 진짜 좋더라구요 ㅋㅋㅋ",
          "original_form": "소리도 좋고 음악틀어놓으면 진짜 좋더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100025597.1.23",
          "form": "빨리 맑은 날이 왔으면 좋겠어요",
          "original_form": "빨리 맑은 날이 왔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025597.1.24",
          "form": "ㅎㅎ가을장마라니  곧 끝나겠죠?ㅜㅜ",
          "original_form": "ㅎㅎ가을장마라니  곧 끝나겠죠?ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 14:23"
        },
        {
          "id": "MDRW2100025597.1.25",
          "form": "이장마가 끝나면",
          "original_form": "이장마가 끝나면",
          "speaker_id": "1",
          "time": "20210901 14:23"
        },
        {
          "id": "MDRW2100025597.1.26",
          "form": "낮에도 좀 시원해지려나요^^",
          "original_form": "낮에도 좀 시원해지려나요^^",
          "speaker_id": "1",
          "time": "20210901 14:23"
        },
        {
          "id": "MDRW2100025597.1.27",
          "form": "그러게요 얼른 시원해졌으면 좋겠어요",
          "original_form": "그러게요 얼른 시원해졌으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210901 14:31"
        },
        {
          "id": "MDRW2100025597.1.28",
          "form": "이번여름 더워서 에어컨 없었으면 어떻게 견뎠을까요",
          "original_form": "이번여름 더워서 에어컨 없었으면 어떻게 견뎠을까요",
          "speaker_id": "2",
          "time": "20210901 14:31"
        },
        {
          "id": "MDRW2100025597.1.29",
          "form": "맞아요 앞으로 갈수록 더워지겠죠?",
          "original_form": "맞아요 앞으로 갈수록 더워지겠죠?",
          "speaker_id": "1",
          "time": "20210901 16:24"
        },
        {
          "id": "MDRW2100025597.1.30",
          "form": "그냥 덥기만하면  모르겠는데",
          "original_form": "그냥 덥기만하면  모르겠는데",
          "speaker_id": "1",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100025597.1.31",
          "form": "습하고 더우니까  정말 불쾌지수가 ㅜㅞ",
          "original_form": "습하고 더우니까  정말 불쾌지수가 ㅜㅞ",
          "speaker_id": "1",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100025597.1.32",
          "form": "시원한 바람이 불어도",
          "original_form": "시원한 바람이 불어도",
          "speaker_id": "1",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100025597.1.33",
          "form": "습하니까 아직도 에어컨을 켜네요ㅜ",
          "original_form": "습하니까 아직도 에어컨을 켜네요ㅜ",
          "speaker_id": "1",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100025597.1.34",
          "form": "저두요 ㅠㅠ",
          "original_form": "저두요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:26"
        },
        {
          "id": "MDRW2100025597.1.35",
          "form": "이번여름이 더워서 에어컨끼고 살았머요",
          "original_form": "이번여름이 더워서 에어컨끼고 살았머요",
          "speaker_id": "2",
          "time": "20210901 16:26"
        },
        {
          "id": "MDRW2100025597.1.36",
          "form": "제습기 있으면 빨래가 뽀송하게 마르더라구요",
          "original_form": "제습기 있으면 빨래가 뽀송하게 마르더라구요",
          "speaker_id": "2",
          "time": "20210901 16:27"
        },
        {
          "id": "MDRW2100025597.1.37",
          "form": "저도 그래서",
          "original_form": "저도 그래서",
          "speaker_id": "1",
          "time": "20210901 16:27"
        },
        {
          "id": "MDRW2100025597.1.38",
          "form": "건조기로말리고",
          "original_form": "건조기로말리고",
          "speaker_id": "1",
          "time": "20210901 16:27"
        },
        {
          "id": "MDRW2100025597.1.39",
          "form": "건조기 못넣는건 다 제습기로말려요",
          "original_form": "건조기 못넣는건 다 제습기로말려요",
          "speaker_id": "1",
          "time": "20210901 16:27"
        },
        {
          "id": "MDRW2100025597.1.40",
          "form": "너무 습하니까 그냥은 잘 안말라서요ㅜ",
          "original_form": "너무 습하니까 그냥은 잘 안말라서요ㅜ",
          "speaker_id": "1",
          "time": "20210901 16:27"
        },
        {
          "id": "MDRW2100025597.1.41",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025597.1.42",
          "form": "여름이 좋으세요 겨울이 좋으세요?",
          "original_form": "여름이 좋으세요 겨울이 좋으세요?",
          "speaker_id": "2",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025597.1.43",
          "form": "전 추위를 많이 타는데 여름엔 벌래가 많이 나와서 겨울을 더 좋아해요",
          "original_form": "전 추위를 많이 타는데 여름엔 벌래가 많이 나와서 겨울을 더 좋아해요",
          "speaker_id": "2",
          "time": "20210901 16:41"
        },
        {
          "id": "MDRW2100025597.1.44",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100025597.1.45",
          "form": "전 여름에 팔다리 내놓는것도 싫어서",
          "original_form": "전 여름에 팔다리 내놓는것도 싫어서",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100025597.1.46",
          "form": "겨울이 더더 좋아요 ㅎ",
          "original_form": "겨울이 더더 좋아요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100025597.1.47",
          "form": "겨울엔 롱패딩 필수요!",
          "original_form": "겨울엔 롱패딩 필수요!",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100025597.1.48",
          "form": "ㅋㅋ 그래서 좋아요 꽁꽁 숨겨도되니까요 ㅍㅎㅎ",
          "original_form": "ㅋㅋ 그래서 좋아요 꽁꽁 숨겨도되니까요 ㅍㅎㅎ",
          "speaker_id": "1",
