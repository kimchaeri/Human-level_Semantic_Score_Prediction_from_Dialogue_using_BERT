      "utterance": [
        {
          "id": "MDRW2100030792.1.1",
          "form": "입소문이 중요하죠",
          "original_form": "입소문이 중요하죠",
          "speaker_id": "2",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100030792.1.2",
          "form": "전기차 관련주 만 해도 엄청납니다",
          "original_form": "전기차 관련주 만 해도 엄청납니다",
          "speaker_id": "1",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100030792.1.3",
          "form": "재밌는 주식 쇼핑",
          "original_form": "재밌는 주식 쇼핑",
          "speaker_id": "1",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100030792.1.4",
          "form": "맞아요 요즘에는 그게 하더군요",
          "original_form": "맞아요 요즘에는 그게 하더군요",
          "speaker_id": "2",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100030792.1.5",
          "form": "카메라 회사도 괜찮아요",
          "original_form": "카메라 회사도 괜찮아요",
          "speaker_id": "1",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100030792.1.6",
          "form": "전기차 수소 관련해서요",
          "original_form": "전기차 수소 관련해서요",
          "speaker_id": "2",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100030792.1.7",
          "form": "자율 주행 하려면 카메라도 많이 필요해요",
          "original_form": "자율 주행 하려면 카메라도 많이 필요해요",
          "speaker_id": "1",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100030792.1.8",
          "form": "빨리 쇼핑해 보고 싶은데",
          "original_form": "빨리 쇼핑해 보고 싶은데",
          "speaker_id": "2",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100030792.1.9",
          "form": "자신이 없어요",
          "original_form": "자신이 없어요",
          "speaker_id": "2",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100030792.1.10",
          "form": "휴대폰에도 카메라가 점점 많이 달려요",
          "original_form": "휴대폰에도 카메라가 점점 많이 달려요",
          "speaker_id": "1",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100030792.1.11",
          "form": "딱 한 주만 사서 지켜 보세요",
          "original_form": "딱 한 주만 사서 지켜 보세요",
          "speaker_id": "1",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030792.1.12",
          "form": "폴더블폰 인기 많으면서",
          "original_form": "폴더블폰 인기 많으면서",
          "speaker_id": "2",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030792.1.13",
          "form": "큰 돈 쓰지 마세요",
          "original_form": "큰 돈 쓰지 마세요",
          "speaker_id": "1",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030792.1.14",
          "form": "관련 주식 또 많이 오르더라구요",
          "original_form": "관련 주식 또 많이 오르더라구요",
          "speaker_id": "2",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030792.1.15",
          "form": "통 얼마 치료 쇼핑 해야 하나요",
          "original_form": "통 얼마 치료 쇼핑 해야 하나요",
          "speaker_id": "2",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030792.1.16",
          "form": "저는 슈피겐코리아를 샀는데",
          "original_form": "저는 슈피겐코리아를 샀는데",
          "speaker_id": "1",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030792.1.17",
          "form": "아시나요",
          "original_form": "아시나요",
          "speaker_id": "1",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030792.1.18",
          "form": "못 들어 봤어요",
          "original_form": "못 들어 봤어요",
          "speaker_id": "2",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030792.1.19",
          "form": "휴대폰케이스회사에요",
          "original_form": "휴대폰케이스회사에요",
          "speaker_id": "1",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030792.1.20",
          "form": "100만 원 정도 치를 사는게 낫나요",
          "original_form": "100만 원 정도 치를 사는게 낫나요",
          "speaker_id": "2",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030792.1.21",
          "form": "아마존 휴대폰케이스 1위",
          "original_form": "아마존 휴대폰케이스 1위",
          "speaker_id": "1",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030792.1.22",
          "form": "저점에서 사서 고점에서 8 자신이 없어서",
          "original_form": "저점에서 사서 고점에서 8 자신이 없어서",
          "speaker_id": "2",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030792.1.23",
          "form": "아이폰 관련주에요",
          "original_form": "아이폰 관련주에요",
          "speaker_id": "1",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030792.1.24",
          "form": "아이폰 13일 출시 되죠",
          "original_form": "아이폰 13일 출시 되죠",
          "speaker_id": "2",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030792.1.25",
          "form": "미국은 9월 개강이라",
          "original_form": "미국은 9월 개강이라",
          "speaker_id": "1",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030792.1.26",
          "form": "관련주 오르고 있는 거 보고 있습니다",
          "original_form": "관련주 오르고 있는 거 보고 있습니다",
          "speaker_id": "2",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030792.1.27",
          "form": "휴대폰주는 가을에 시세주더라구요",
          "original_form": "휴대폰주는 가을에 시세주더라구요",
          "speaker_id": "1",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030792.1.28",
          "form": "일단사서 기다려보고있어요",
          "original_form": "일단사서 기다려보고있어요",
          "speaker_id": "1",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030792.1.29",
          "form": "그럼 미리 사 놓으면 이득 보는 건가요",
          "original_form": "그럼 미리 사 놓으면 이득 보는 건가요",
          "speaker_id": "2",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030792.1.30",
