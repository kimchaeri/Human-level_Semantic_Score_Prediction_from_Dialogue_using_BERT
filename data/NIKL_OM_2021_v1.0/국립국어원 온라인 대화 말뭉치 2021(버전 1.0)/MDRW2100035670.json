      "utterance": [
        {
          "id": "MDRW2100035670.1.1",
          "form": "가사 진짜 심각하던데요",
          "original_form": "가사 진짜 심각하던데요",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035670.1.2",
          "form": "아 저두요 ㅋㅋㅋ 진짜 가사",
          "original_form": "아 저두요 ㅋㅋㅋ 진짜 가사",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035670.1.3",
          "form": "배우보다 더 배우",
          "original_form": "배우보다 더 배우",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035670.1.4",
          "form": "악 읽지마세요",
          "original_form": "악 읽지마세요",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035670.1.5",
          "form": "~~~~!!!",
          "original_form": "~~~~!!!",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035670.1.6",
          "form": "아 ~~~ 늑대 가지고 노는 여우 ~~",
          "original_form": "아 ~~~ 늑대 가지고 노는 여우 ~~",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035670.1.7",
          "form": "제 최애 파트 ..",
          "original_form": "제 최애 파트 ..",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035670.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 제발 진짜 2021년 맞냐고요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 제발 진짜 2021년 맞냐고요",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035670.1.9",
          "form": "이상한걸로 최애에요",
          "original_form": "이상한걸로 최애에요",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035670.1.10",
          "form": "오해마시길 ..",
          "original_form": "오해마시길 ..",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035670.1.11",
          "form": "아니 근데 심지어 그 노랜 멤버들이 무대를 잘해서 슬퍼요",
          "original_form": "아니 근데 심지어 그 노랜 멤버들이 무대를 잘해서 슬퍼요",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035670.1.12",
          "form": "맞아요 춤 잘추고...",
          "original_form": "맞아요 춤 잘추고...",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035670.1.13",
          "form": "그 이상망측한 노래에다가...",
          "original_form": "그 이상망측한 노래에다가...",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035670.1.14",
          "form": "라이브도 잘하고 ㅜㅜ 한명 빼구요 ...",
          "original_form": "라이브도 잘하고 ㅜㅜ 한명 빼구요 ...",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035670.1.15",
          "form": "한숨만 나오는...",
          "original_form": "한숨만 나오는...",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035670.1.16",
          "form": "아 춤도 한명 빼야죠 참...",
          "original_form": "아 춤도 한명 빼야죠 참...",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035670.1.17",
          "form": "아 맞다 ...",
          "original_form": "아 맞다 ...",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035670.1.18",
          "form": "그거 아세요 롤리폴리 ?",
          "original_form": "그거 아세요 롤리폴리 ?",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035670.1.19",
          "form": "예... 많이 늘긴 했던데",
          "original_form": "예... 많이 늘긴 했던데",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035670.1.20",
          "form": "롤리폴리가 머예요?",
          "original_form": "롤리폴리가 머예요?",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035670.1.21",
          "form": "연말무대 때",
          "original_form": "연말무대 때",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035670.1.22",
          "form": "있지 롤리폴리 췄거든요 티아라 노래",
          "original_form": "있지 롤리폴리 췄거든요 티아라 노래",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035670.1.23",
          "form": "아진짜요? 전 못본거같어요",
          "original_form": "아진짜요? 전 못본거같어요",
          "speaker_id": "2",
          "time": "20210829 22:15"
        }
      ]
