      "utterance": [
        {
          "id": "MDRW2100010531.1.1",
          "form": "가챠 알 와르르 나오는거 재밌져",
          "original_form": "가챠 알 와르르 나오는거 재밌져",
          "speaker_id": "1",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010531.1.2",
          "form": "게임은 거의 가챠땜에 계속 하게 되는거 같기두 하구ㅋㅋ",
          "original_form": "게임은 거의 가챠땜에 계속 하게 되는거 같기두 하구ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010531.1.3",
          "form": "맞아요 약간 도박 중독 같아요",
          "original_form": "맞아요 약간 도박 중독 같아요",
          "speaker_id": "2",
          "time": "20210823 19:30"
        },
        {
          "id": "MDRW2100010531.1.4",
          "form": "현질 유도하는 게임도 많으니까요",
          "original_form": "현질 유도하는 게임도 많으니까요",
          "speaker_id": "1",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010531.1.5",
          "form": "게임에 돈 많이 쓰시나요?",
          "original_form": "게임에 돈 많이 쓰시나요?",
          "speaker_id": "1",
          "time": "20210823 19:32"
        },
        {
          "id": "MDRW2100010531.1.6",
          "form": "돈을 써본적은 없어요 보상이 충분해서 그런가",
          "original_form": "돈을 써본적은 없어요 보상이 충분해서 그런가",
          "speaker_id": "2",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100010531.1.7",
          "form": "안 해도 즐길 수 있으면 좋은 게임이죠",
          "original_form": "안 해도 즐길 수 있으면 좋은 게임이죠",
          "speaker_id": "1",
          "time": "20210823 19:36"
        },
        {
          "id": "MDRW2100010531.1.8",
          "form": "그래도 전 가끔 쓰긴 해요",
          "original_form": "그래도 전 가끔 쓰긴 해요",
          "speaker_id": "1",
          "time": "20210823 19:36"
        },
        {
          "id": "MDRW2100010531.1.9",
          "form": "한번에 어느 정도 쓰세요?",
          "original_form": "한번에 어느 정도 쓰세요?",
          "speaker_id": "2",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100010531.1.10",
          "form": "보통 5천원 안쪽인거? 비쌀땐 몇만원 쓸때도 있긴 해요",
          "original_form": "보통 5천원 안쪽인거? 비쌀땐 몇만원 쓸때도 있긴 해요",
          "speaker_id": "1",
          "time": "20210823 20:46"
        },
        {
          "id": "MDRW2100010531.1.11",
          "form": "와 대단하시네요",
          "original_form": "와 대단하시네요",
          "speaker_id": "2",
          "time": "20210823 20:47"
        },
        {
          "id": "MDRW2100010531.1.12",
          "form": "그만큼 쓰면 보상도 세겠죠?",
          "original_form": "그만큼 쓰면 보상도 세겠죠?",
          "speaker_id": "2",
          "time": "20210823 20:47"
        },
        {
          "id": "MDRW2100010531.1.13",
          "form": "아무래도 그렇긴 하죠",
          "original_form": "아무래도 그렇긴 하죠",
          "speaker_id": "1",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100010531.1.14",
          "form": "다음달엔 덜 쓰려고 노력하지만요ㅋㅋ",
          "original_form": "다음달엔 덜 쓰려고 노력하지만요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100010531.1.15",
          "form": "덜 쓰려고 하시는구나",
          "original_form": "덜 쓰려고 하시는구나",
          "speaker_id": "2",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100010531.1.16",
          "form": "게임은 즐기려고 하는거니까",
          "original_form": "게임은 즐기려고 하는거니까",
          "speaker_id": "1",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100010531.1.17",
          "form": "돈써도 잘 안나오면 안쓰긴 하죠",
          "original_form": "돈써도 잘 안나오면 안쓰긴 하죠",
          "speaker_id": "1",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010531.1.18",
          "form": "맞죠 게임에 돈쓰는게 확실한건 아니기도 하고",
          "original_form": "맞죠 게임에 돈쓰는게 확실한건 아니기도 하고",
          "speaker_id": "2",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010531.1.19",
          "form": "맞아요 재밌게 하는게 중요하져",
          "original_form": "맞아요 재밌게 하는게 중요하져",
          "speaker_id": "1",
          "time": "20210823 22:28"
        }
      ]
