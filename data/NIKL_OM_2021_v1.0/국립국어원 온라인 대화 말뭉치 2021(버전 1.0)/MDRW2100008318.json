      "utterance": [
        {
          "id": "MDRW2100008318.1.1",
          "form": "뭔가 읽을 때 마다 느끼는게 새로운 거 같아요",
          "original_form": "뭔가 읽을 때 마다 느끼는게 새로운 거 같아요",
          "speaker_id": "1",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008318.1.2",
          "form": "오 무순 내용인가요?",
          "original_form": "오 무순 내용인가요?",
          "speaker_id": "2",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008318.1.3",
          "form": "처음 들어봐요",
          "original_form": "처음 들어봐요",
          "speaker_id": "2",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008318.1.4",
          "form": "약간 시간?에 대한 이야기인데 바쁘게 살아가는 현대사회 속 잃어버린 소중함을 말하는?",
          "original_form": "약간 시간?에 대한 이야기인데 바쁘게 살아가는 현대사회 속 잃어버린 소중함을 말하는?",
          "speaker_id": "1",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008318.1.5",
          "form": "그런 내용이에요",
          "original_form": "그런 내용이에요",
          "speaker_id": "1",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008318.1.6",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008318.1.7",
          "form": "재밌을것같아요",
          "original_form": "재밌을것같아요",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008318.1.8",
          "form": "저도 한번 빌려봐야겠네요",
          "original_form": "저도 한번 빌려봐야겠네요",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008318.1.9",
          "form": "저도 좋아하는 책 여러번 보는 편이애요",
          "original_form": "저도 좋아하는 책 여러번 보는 편이애요",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008318.1.10",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008318.1.11",
          "form": "뭔가 읽을 때 마다 새로운",
          "original_form": "뭔가 읽을 때 마다 새로운",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008318.1.12",
          "form": "무언가가있어요",
          "original_form": "무언가가있어요",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008318.1.13",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008318.1.14",
          "form": "마시멜로 이야기 아세요?",
          "original_form": "마시멜로 이야기 아세요?",
          "speaker_id": "2",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008318.1.15",
          "form": "들어봤는데 읽어보지는 못했어요",
          "original_form": "들어봤는데 읽어보지는 못했어요",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008318.1.16",
          "form": "저는 이 책만 한 5번 정도 읽은 것 같은데",
          "original_form": "저는 이 책만 한 5번 정도 읽은 것 같은데",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008318.1.17",
          "form": "5번이나요?",
          "original_form": "5번이나요?",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008318.1.18",
          "form": "되게 좋았나봐요",
          "original_form": "되게 좋았나봐요",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008318.1.19",
          "form": "다시 읽어도 좋더라고요",
          "original_form": "다시 읽어도 좋더라고요",
          "speaker_id": "2",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008318.1.20",
          "form": "어떤내용인가요?",
          "original_form": "어떤내용인가요?",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008318.1.21",
          "form": "되게 가벼운 책이에요",
          "original_form": "되게 가벼운 책이에요",
          "speaker_id": "2",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008318.1.22",
          "form": "청소년 권장도서 느낌?",
          "original_form": "청소년 권장도서 느낌?",
          "speaker_id": "2",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008318.1.23",
          "form": "마시멜로 실험이라고 들어보셨을까요?",
          "original_form": "마시멜로 실험이라고 들어보셨을까요?",
          "speaker_id": "2",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008318.1.24",
          "form": "저 그런 거 좋아해요ㅎㅎ",
          "original_form": "저 그런 거 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008318.1.25",
          "form": "네 그거 알아요",
          "original_form": "네 그거 알아요",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008318.1.26",
          "form": "아이한테 마시멜로 쥐어주고",
          "original_form": "아이한테 마시멜로 쥐어주고",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008318.1.27",
          "form": "양심 시험하는거",
          "original_form": "양심 시험하는거",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008318.1.28",
          "form": "맞죠",
          "original_form": "맞죠",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008318.1.29",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008318.1.30",
          "form": "양심이라고 할까",
          "original_form": "양심이라고 할까",
          "speaker_id": "2",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008318.1.31",
          "form": "인내심 테스트?",
          "original_form": "인내심 테스트?",
          "speaker_id": "2",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008318.1.32",
          "form": "그 표현이 더 잘 어울리겠어요",
          "original_form": "그 표현이 더 잘 어울리겠어요",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008318.1.33",
          "form": "마시멜로 하나주고 15분 기다리면 하나 더줄게!",
          "original_form": "마시멜로 하나주고 15분 기다리면 하나 더줄게!",
          "speaker_id": "2",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008318.1.34",
          "form": "맞아요 그거",
          "original_form": "맞아요 그거",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008318.1.35",
          "form": "하는 실험 거기서",
          "original_form": "하는 실험 거기서",
          "speaker_id": "2",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008318.1.36",
          "form": "인내하면 더큰 성공이 돌아올것이다",
          "original_form": "인내하면 더큰 성공이 돌아올것이다",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008318.1.37",
          "form": "하는 이야기를 하더라고요",
          "original_form": "하는 이야기를 하더라고요",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008318.1.38",
          "form": "커가면서 공감허지 못하는 내용도 생겼지만",
          "original_form": "커가면서 공감허지 못하는 내용도 생겼지만",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008318.1.39",
          "form": "열심히 살아야지, 하고 다시 의지를 다잡는 계기가 되주더라고요",
          "original_form": "열심히 살아야지, 하고 다시 의지를 다잡는 계기가 되주더라고요",
          "speaker_id": "2",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008318.1.40",
          "form": "되게 좋은 내용이네요",
          "original_form": "되게 좋은 내용이네요",
          "speaker_id": "1",
          "time": "20210822 15:14"
        }
      ]
    }
