        {
          "id": "MDRW2100027586.1.1",
          "form": "저는 알파고보고 놀랐어요",
          "original_form": "저는 알파고보고 놀랐어요",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100027586.1.2",
          "form": "오가뇩? 거기맛있더라구요",
          "original_form": "오가뇩? 거기맛있더라구요",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100027586.1.3",
          "form": "메뉴가뭐죠?",
          "original_form": "메뉴가뭐죠?",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100027586.1.4",
          "form": "조개랑 킹크랩요",
          "original_form": "조개랑 킹크랩요",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100027586.1.5",
          "form": "오..맛나겠네요",
          "original_form": "오..맛나겠네요",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100027586.1.6",
          "form": "네..넘맛있더군요",
          "original_form": "네..넘맛있더군요",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100027586.1.7",
          "form": "AI학과아세요?",
          "original_form": "AI학과아세요?",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100027586.1.8",
          "form": "저도 잘몰라요",
          "original_form": "저도 잘몰라요",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100027586.1.9",
          "form": "AI학과 아시는 학과있나요?",
          "original_form": "AI학과 아시는 학과있나요?",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100027586.1.10",
          "form": "아니요..",
          "original_form": "아니요..",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100027586.1.11",
          "form": "대학나온지오래라",
          "original_form": "대학나온지오래라",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100027586.1.12",
          "form": "요즘젊은애들한테 대세인듯해요",
          "original_form": "요즘젊은애들한테 대세인듯해요",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100027586.1.13",
          "form": "저는잘모르지만",
          "original_form": "저는잘모르지만",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100027586.1.14",
          "form": "취업이 잘되나봐요",
          "original_form": "취업이 잘되나봐요",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100027586.1.15",
          "form": "그렇죠?",
          "original_form": "그렇죠?",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100027586.1.16",
          "form": "인공지능이다보니 앞으로는 이런과가 좋을듯",
          "original_form": "인공지능이다보니 앞으로는 이런과가 좋을듯",
