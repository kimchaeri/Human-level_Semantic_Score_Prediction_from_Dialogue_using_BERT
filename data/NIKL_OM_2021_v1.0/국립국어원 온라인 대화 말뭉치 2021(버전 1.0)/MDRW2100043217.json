      "utterance": [
        {
          "id": "MDRW2100043217.1.1",
          "form": "일하시나요",
          "original_form": "일하시나요",
          "speaker_id": "1",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043217.1.2",
          "form": "저는 프리랜서인데",
          "original_form": "저는 프리랜서인데",
          "speaker_id": "2",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043217.1.3",
          "form": "지금은 쉬어요",
          "original_form": "지금은 쉬어요",
          "speaker_id": "2",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043217.1.4",
          "form": "쉬는거좋죠",
          "original_form": "쉬는거좋죠",
          "speaker_id": "1",
          "time": "20210907 15:58"
        },
        {
          "id": "MDRW2100043217.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100043217.1.6",
          "form": "저는직장다녀요",
          "original_form": "저는직장다녀요",
          "speaker_id": "1",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043217.1.7",
          "form": "업무 스트레스는 심하지 않으시고요?",
          "original_form": "업무 스트레스는 심하지 않으시고요?",
          "speaker_id": "2",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043217.1.8",
          "form": "전 번아웃 와서 ㅠ",
          "original_form": "전 번아웃 와서 ㅠ",
          "speaker_id": "2",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043217.1.9",
          "form": "저진짜왔는데",
          "original_form": "저진짜왔는데",
          "speaker_id": "1",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100043217.1.10",
          "form": "그냥존버하고있어요",
          "original_form": "그냥존버하고있어요",
          "speaker_id": "1",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100043217.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 16:08"
        },
        {
          "id": "MDRW2100043217.1.12",
          "form": "대단하세요",
          "original_form": "대단하세요",
          "speaker_id": "2",
          "time": "20210907 16:08"
        },
        {
          "id": "MDRW2100043217.1.13",
          "form": "그와중에 이 심심이까지 ㅋㅋ",
          "original_form": "그와중에 이 심심이까지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:08"
        },
        {
          "id": "MDRW2100043217.1.14",
          "form": "ㅎㅎㅎㅎ할거많은데이거하고있네요..",
          "original_form": "ㅎㅎㅎㅎ할거많은데이거하고있네요..",
          "speaker_id": "1",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100043217.1.15",
          "form": "직장에서 싫은 사람은 없어요?",
          "original_form": "직장에서 싫은 사람은 없어요?",
          "speaker_id": "2",
          "time": "20210907 16:12"
        },
        {
          "id": "MDRW2100043217.1.16",
          "form": "ㅎㅎ음 참을만해요",
          "original_form": "ㅎㅎ음 참을만해요",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043217.1.17",
          "form": "성격이 좋으시군요",
          "original_form": "성격이 좋으시군요",
          "speaker_id": "2",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043217.1.18",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043217.1.19",
          "form": "싫다가도좋은점도있고..",
          "original_form": "싫다가도좋은점도있고..",
          "speaker_id": "1",
