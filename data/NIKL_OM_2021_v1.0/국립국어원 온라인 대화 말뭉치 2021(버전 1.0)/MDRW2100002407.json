      "utterance": [
        {
          "id": "MDRW2100002407.1.1",
          "form": "나 아직",
          "original_form": "나 아직",
          "speaker_id": "1",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002407.1.2",
          "form": "사내 연애하고 싶냐?",
          "original_form": "사내 연애하고 싶냐?",
          "speaker_id": "2",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002407.1.3",
          "form": "사내 연애좋지",
          "original_form": "사내 연애좋지",
          "speaker_id": "1",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002407.1.4",
          "form": "바람둥이구만",
          "original_form": "바람둥이구만",
          "speaker_id": "2",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002407.1.5",
          "form": "퇴근하고 와이프랑같이",
          "original_form": "퇴근하고 와이프랑같이",
          "speaker_id": "1",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002407.1.6",
          "form": "아니 와이프랑 같이 일하고",
          "original_form": "아니 와이프랑 같이 일하고",
          "speaker_id": "1",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002407.1.7",
          "form": "놉",
          "original_form": "놉",
          "speaker_id": "2",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002407.1.8",
          "form": "나 아직",
          "original_form": "나 아직",
          "speaker_id": "1",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002407.1.9",
          "form": "그건 아님",
          "original_form": "그건 아님",
          "speaker_id": "2",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002407.1.10",
          "form": "회사에서도 집에서도 붙어있는건",
          "original_form": "회사에서도 집에서도 붙어있는건",
          "speaker_id": "2",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002407.1.11",
          "form": "정말 아님",
          "original_form": "정말 아님",
          "speaker_id": "2",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002407.1.12",
          "form": "부서가 다름 되지",
          "original_form": "부서가 다름 되지",
          "speaker_id": "1",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002407.1.13",
          "form": "나 아직 혼인신고안함",
          "original_form": "나 아직 혼인신고안함",
          "speaker_id": "1",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002407.1.14",
          "form": "올",
          "original_form": "올",
          "speaker_id": "2",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002407.1.15",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "2",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002407.1.16",
          "form": "일안하면",
          "original_form": "일안하면",
          "speaker_id": "2",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002407.1.17",
          "form": "쫓아내셈",
          "original_form": "쫓아내셈",
          "speaker_id": "2",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002407.1.18",
          "form": "에이 그건 아니지",
          "original_form": "에이 그건 아니지",
          "speaker_id": "1",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002407.1.19",
          "form": "기념 사진도 친구들 빼고 찍은 이유가 있었구만",
          "original_form": "기념 사진도 친구들 빼고 찍은 이유가 있었구만",
          "speaker_id": "2",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002407.1.20",
          "form": "사랑하냐?",
          "original_form": "사랑하냐?",
          "speaker_id": "2",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100002407.1.21",
          "form": "혼인신고 해택이 없어",
          "original_form": "혼인신고 해택이 없어",
          "speaker_id": "1",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100002407.1.22",
          "form": "신혼부부 대출",
          "original_form": "신혼부부 대출",
          "speaker_id": "2",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100002407.1.23",
          "form": "나 신용대출 회사대출 땡겼고",
          "original_form": "나 신용대출 회사대출 땡겼고",
          "speaker_id": "1",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100002407.1.24",
          "form": "소득공제",
          "original_form": "소득공제",
          "speaker_id": "2",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100002407.1.25",
          "form": "왜 없어",
          "original_form": "왜 없어",
          "speaker_id": "2",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100002407.1.26",
          "form": "그리고 낙인",
          "original_form": "그리고 낙인",
          "speaker_id": "2",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100002407.1.27",
          "form": "나 아파트 청약돼서",
          "original_form": "나 아파트 청약돼서",
          "speaker_id": "1",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100002407.1.28",
          "form": "와이프도 청약당첨시키려면",
          "original_form": "와이프도 청약당첨시키려면",
          "speaker_id": "1",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100002407.1.29",
          "form": "혼인신고 안돼",
          "original_form": "혼인신고 안돼",
          "speaker_id": "1",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100002407.1.30",
          "form": "투자도 하려는군",
          "original_form": "투자도 하려는군",
          "speaker_id": "2",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100002407.1.31",
          "form": "거긴 통상 얼마나 더 붙어?",
          "original_form": "거긴 통상 얼마나 더 붙어?",
          "speaker_id": "2",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100002407.1.32",
          "form": "분양가가 4억 5천이고 피가 지금1억 붙었는데",
          "original_form": "분양가가 4억 5천이고 피가 지금1억 붙었는데",
          "speaker_id": "1",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002407.1.33",
          "form": "거래는 안돼",
          "original_form": "거래는 안돼",
          "speaker_id": "1",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002407.1.34",
          "form": "그런데 시내쪽은 피가 훨씬 더 붙었어",
          "original_form": "그런데 시내쪽은 피가 훨씬 더 붙었어",
          "speaker_id": "1",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002407.1.35",
          "form": "구미에 비해서 많이 쎄네",
          "original_form": "구미에 비해서 많이 쎄네",
          "speaker_id": "2",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100002407.1.36",
          "form": "팔자! 팔면 월세 15년 2개월 낼 수 있고, 대출이자도 사라짐",
          "original_form": "팔자! 팔면 월세 15년 2개월 낼 수 있고, 대출이자도 사라짐",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002407.1.37",
          "form": "지금 팔면 발생 수익금에  60퍼센트가 세금일걸 거기다가 분양권이라 20퍼센트 가산되는걸로 알고있어",
          "original_form": "지금 팔면 발생 수익금에  60퍼센트가 세금일걸 거기다가 분양권이라 20퍼센트 가산되는걸로 알고있어",
          "speaker_id": "1",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002407.1.38",
          "form": "남는게 없어져",
          "original_form": "남는게 없어져",
          "speaker_id": "1",
          "time": "20210820 13:11"
        }
      ]
