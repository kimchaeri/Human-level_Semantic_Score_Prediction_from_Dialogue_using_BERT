      "utterance": [
        {
          "id": "MDRW2100043351.1.1",
          "form": "오늘 저녁 뭐먹을거얌?",
          "original_form": "오늘 저녁 뭐먹을거얌?",
          "speaker_id": "1",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043351.1.2",
          "form": "나 그릭요거트랑 크레이프케익 먹었어!",
          "original_form": "나 그릭요거트랑 크레이프케익 먹었어!",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043351.1.3",
          "form": "와 맛난거 먿었네",
          "original_form": "와 맛난거 먿었네",
          "speaker_id": "1",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043351.1.4",
          "form": "카페에서 먹었어?",
          "original_form": "카페에서 먹었어?",
          "speaker_id": "1",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043351.1.5",
          "form": "아니아니!",
          "original_form": "아니아니!",
          "speaker_id": "2",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043351.1.6",
          "form": "그릭요거트는 포장해오고",
          "original_form": "그릭요거트는 포장해오고",
          "speaker_id": "2",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043351.1.7",
          "form": "크레이프케익은 친구가 선물로 배송해준거",
          "original_form": "크레이프케익은 친구가 선물로 배송해준거",
          "speaker_id": "2",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043351.1.8",
          "form": "먹었어",
          "original_form": "먹었어",
          "speaker_id": "2",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043351.1.9",
          "form": "우와 생일이었어???",
          "original_form": "우와 생일이었어???",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100043351.1.10",
          "form": "웅!!! 8월 마지막날!!",
          "original_form": "웅!!! 8월 마지막날!!",
          "speaker_id": "2",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100043351.1.11",
          "form": "우와 축하해",
          "original_form": "우와 축하해",
          "speaker_id": "1",
          "time": "20210907 18:13"
        },
        {
          "id": "MDRW2100043351.1.12",
          "form": "그릭요거트 맛집 근처에있으면 조케따",
          "original_form": "그릭요거트 맛집 근처에있으면 조케따",
          "speaker_id": "1",
          "time": "20210907 18:13"
        },
        {
          "id": "MDRW2100043351.1.13",
          "form": "나 사는곳 주변에 내가 좋아하는 그릭요거트집있어서",
          "original_form": "나 사는곳 주변에 내가 좋아하는 그릭요거트집있어서",
          "speaker_id": "2",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100043351.1.14",
          "form": "애용중이다!!!",
          "original_form": "애용중이다!!!",
          "speaker_id": "2",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100043351.1.15",
          "form": "우오ㅏ 그니까ㅠㅠ부러워 어디야?",
          "original_form": "우오ㅏ 그니까ㅠㅠ부러워 어디야?",
          "speaker_id": "1",
          "time": "20210907 18:15"
        },
        {
          "id": "MDRW2100043351.1.16",
          "form": "부산은 아니지?",
          "original_form": "부산은 아니지?",
          "speaker_id": "1",
          "time": "20210907 18:15"
        },
        {
          "id": "MDRW2100043351.1.17",
          "form": "웅 부산은 아니야",
          "original_form": "웅 부산은 아니야",
          "speaker_id": "2",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100043351.1.18",
          "form": "멀다 부산ㅠ",
          "original_form": "멀다 부산ㅠ",
          "speaker_id": "2",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100043351.1.19",
          "form": "글쿠나",
          "original_form": "글쿠나",
          "speaker_id": "1",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100043351.1.20",
          "form": "요거트 맛집 부러워ㅠㅠ",
          "original_form": "요거트 맛집 부러워ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100043351.1.21",
          "form": "부산에도 유명한 맛집 많지않아??",
          "original_form": "부산에도 유명한 맛집 많지않아??",
          "speaker_id": "2",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100043351.1.22",
          "form": "많지 근데 요거트 집 너무 멀어",
          "original_form": "많지 근데 요거트 집 너무 멀어",
          "speaker_id": "1",
          "time": "20210907 18:18"
        },
        {
          "id": "MDRW2100043351.1.23",
          "form": "요거트 많이 좋아해?",
          "original_form": "요거트 많이 좋아해?",
          "speaker_id": "2",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100043351.1.24",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "1",
          "time": "20210907 18:19"
        }
      ]
