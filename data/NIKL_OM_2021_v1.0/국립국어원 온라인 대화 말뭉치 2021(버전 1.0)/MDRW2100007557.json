      "utterance": [
        {
          "id": "MDRW2100007557.1.1",
          "form": "최근에 어떤 물건 사셨어요?",
          "original_form": "최근에 어떤 물건 사셨어요?",
          "speaker_id": "1",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007557.1.2",
          "form": "마켓컬리에서 이것저것샀어요",
          "original_form": "마켓컬리에서 이것저것샀어요",
          "speaker_id": "2",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007557.1.3",
          "form": "새벽에 배송하니까 편하죠",
          "original_form": "새벽에 배송하니까 편하죠",
          "speaker_id": "1",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007557.1.4",
          "form": "네 가격도 비싸지않고 좋은거같아요",
          "original_form": "네 가격도 비싸지않고 좋은거같아요",
          "speaker_id": "2",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007557.1.5",
          "form": "품목이 좀 더 다양하면 좋을텐데",
          "original_form": "품목이 좀 더 다양하면 좋을텐데",
          "speaker_id": "1",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007557.1.6",
          "form": "맞아요 일반마트랑 품목이 좀 달라서..",
          "original_form": "맞아요 일반마트랑 품목이 좀 달라서..",
          "speaker_id": "2",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007557.1.7",
          "form": "근데 일반 마트에 안파는 제품도 많아서 좋아요",
          "original_form": "근데 일반 마트에 안파는 제품도 많아서 좋아요",
          "speaker_id": "2",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007557.1.8",
          "form": "밎아요 별거 다 팔더라구요",
          "original_form": "밎아요 별거 다 팔더라구요",
          "speaker_id": "1",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007557.1.9",
          "form": "요즘엔 핸드폰도 팔던데",
          "original_form": "요즘엔 핸드폰도 팔던데",
          "speaker_id": "2",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007557.1.10",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210822 03:15"
        },
        {
          "id": "MDRW2100007557.1.11",
          "form": "거기서 사면 좀 싼가요?",
          "original_form": "거기서 사면 좀 싼가요?",
          "speaker_id": "1",
          "time": "20210822 03:15"
        },
        {
          "id": "MDRW2100007557.1.12",
          "form": "싼건 모르겠는데 배송이 빨라서 많이 사더라구요",
          "original_form": "싼건 모르겠는데 배송이 빨라서 많이 사더라구요",
          "speaker_id": "2",
          "time": "20210822 03:15"
        },
        {
          "id": "MDRW2100007557.1.13",
          "form": "하루만에 오나요?",
          "original_form": "하루만에 오나요?",
          "speaker_id": "1",
          "time": "20210822 03:15"
        },
        {
          "id": "MDRW2100007557.1.14",
          "form": "네 새벽에 배송해준대요",
          "original_form": "네 새벽에 배송해준대요",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007557.1.15",
          "form": "그건 좋네요",
          "original_form": "그건 좋네요",
          "speaker_id": "1",
          "time": "20210822 03:16"
        }
