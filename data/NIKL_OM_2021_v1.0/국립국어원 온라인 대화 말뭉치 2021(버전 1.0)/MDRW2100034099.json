      "utterance": [
        {
          "id": "MDRW2100034099.1.1",
          "form": "그러니까요 ㅠㅠ",
          "original_form": "그러니까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100034099.1.2",
          "form": "혹시 마피아42 게임 아세요?",
          "original_form": "혹시 마피아42 게임 아세요?",
          "speaker_id": "2",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100034099.1.3",
          "form": "알아요! 수련회 가면 많이 했어여ㅎㅎ",
          "original_form": "알아요! 수련회 가면 많이 했어여ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100034099.1.4",
          "form": "핸드폰 게임으로도 있어요!",
          "original_form": "핸드폰 게임으로도 있어요!",
          "speaker_id": "2",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100034099.1.5",
          "form": "수련회나 수학여행 가면 폰 마피아로 했어요!",
          "original_form": "수련회나 수학여행 가면 폰 마피아로 했어요!",
          "speaker_id": "1",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100034099.1.6",
          "form": "그런거였군요 ㅋㅋㅋ",
          "original_form": "그런거였군요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:30"
        },
        {
          "id": "MDRW2100034099.1.7",
          "form": "마피아 42가 직업도 다양해서 재밌더라구요",
          "original_form": "마피아 42가 직업도 다양해서 재밌더라구요",
          "speaker_id": "2",
          "time": "20210829 15:30"
        },
        {
          "id": "MDRW2100034099.1.8",
          "form": "그리고 그냥 할 때보다 소리로 들킬 확률도 확 줄고요ㅋㅋ",
          "original_form": "그리고 그냥 할 때보다 소리로 들킬 확률도 확 줄고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100034099.1.9",
          "form": "ㅋㅋㅋㅋ 아니 그런 장점이 있었네요",
          "original_form": "ㅋㅋㅋㅋ 아니 그런 장점이 있었네요",
          "speaker_id": "2",
          "time": "20210829 15:33"
        },
        {
          "id": "MDRW2100034099.1.10",
          "form": "엎드리거나 눈을 감을 필요도 없구요ㅋㅋ",
          "original_form": "엎드리거나 눈을 감을 필요도 없구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100034099.1.11",
          "form": "ㅋㅋㅋㅋ 오랜만에 마피아42 하고 싶어지네요",
          "original_form": "ㅋㅋㅋㅋ 오랜만에 마피아42 하고 싶어지네요",
          "speaker_id": "2",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100034099.1.12",
          "form": "가끔 들가면 꽤나 많이들 하고 있더라구오ㅋㅌ",
          "original_form": "가끔 들가면 꽤나 많이들 하고 있더라구오ㅋㅌ",
          "speaker_id": "1",
          "time": "20210829 16:17"
        },
        {
          "id": "MDRW2100034099.1.13",
          "form": "아 저도 느꼈어요 ㅋㅋㅋ",
          "original_form": "아 저도 느꼈어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:17"
        },
        {
          "id": "MDRW2100034099.1.14",
          "form": "길게는 못하겠는데 가끔씩 들어가서 하면 재밌어요",
          "original_form": "길게는 못하겠는데 가끔씩 들어가서 하면 재밌어요",
          "speaker_id": "2",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100034099.1.15",
          "form": "맞아요ㅋㅋㅋㅋ 진짜 몇 달에 한 번 시간때우기ㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ 진짜 몇 달에 한 번 시간때우기ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100034099.1.16",
          "form": "ㅋㅋㅋㅋ 와 다들 비슷하네요",
          "original_form": "ㅋㅋㅋㅋ 와 다들 비슷하네요",
          "speaker_id": "2",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100034099.1.17",
          "form": "저도 이제 슬슬 할 때가 온 것 같아요 ㅋㅋ",
          "original_form": "저도 이제 슬슬 할 때가 온 것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:22"
        }
