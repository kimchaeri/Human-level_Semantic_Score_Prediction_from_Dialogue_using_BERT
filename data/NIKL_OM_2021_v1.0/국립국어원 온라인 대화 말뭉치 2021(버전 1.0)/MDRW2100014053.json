      "utterance": [
        {
          "id": "MDRW2100014053.1.1",
          "form": "안녕하세요~ 게임 하는거 있으신가요?",
          "original_form": "안녕하세요~ 게임 하는거 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014053.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014053.1.3",
          "form": "카트하고있어요",
          "original_form": "카트하고있어요",
          "speaker_id": "2",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014053.1.4",
          "form": "우와 저도 옛날에 했었는데ㅋㅋㅋ요즘엔 모바일도 있더라구요",
          "original_form": "우와 저도 옛날에 했었는데ㅋㅋㅋ요즘엔 모바일도 있더라구요",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014053.1.5",
          "form": "피시랑 모바일이랑 같이 하고있어요",
          "original_form": "피시랑 모바일이랑 같이 하고있어요",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014053.1.6",
          "form": "그렇구나 캐릭터는 뭐 주로 쓰세요? ?",
          "original_form": "그렇구나 캐릭터는 뭐 주로 쓰세요? ?",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014053.1.7",
          "form": "교복입은티아라?그캐릭 귀여워요",
          "original_form": "교복입은티아라?그캐릭 귀여워요",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014053.1.8",
          "form": "오 찾아봤는데 앞머리 귀여워요ㅋㅋㅋ 전 왠지 모르겠는데 맨날 배찌 했던거 같아요ㅋㅋㅋ",
          "original_form": "오 찾아봤는데 앞머리 귀여워요ㅋㅋㅋ 전 왠지 모르겠는데 맨날 배찌 했던거 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014053.1.9",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014053.1.10",
          "form": "배찌도인기많죠",
          "original_form": "배찌도인기많죠",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014053.1.11",
          "form": "모바일 카트는 첨시작할때 배찌 못고르는거 같더라구요? 그래서 다른캐릭터로 시작해서 아쉬웠던ㅋㅋㅋ",
          "original_form": "모바일 카트는 첨시작할때 배찌 못고르는거 같더라구요? 그래서 다른캐릭터로 시작해서 아쉬웠던ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014053.1.12",
          "form": "저는 피시로하다가 모바일하다가 번갈아가면서하고있어요",
          "original_form": "저는 피시로하다가 모바일하다가 번갈아가면서하고있어요",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014053.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014053.1.14",
          "form": "저는 요새는 다른게임에 꽂혀서 카트는 안하는데",
          "original_form": "저는 요새는 다른게임에 꽂혀서 카트는 안하는데",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014053.1.15",
          "form": "오락실 가면 카트라이더 꼭 해요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오락실 가면 카트라이더 꼭 해요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014053.1.16",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014053.1.17",
          "form": "실력은어때요??",
          "original_form": "실력은어때요??",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014053.1.18",
          "form": "음... 중상위?!?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "음... 중상위?!?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014053.1.19",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014053.1.20",
          "form": "나중에 알투비트리듬게임나온데요",
          "original_form": "나중에 알투비트리듬게임나온데요",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014053.1.21",
          "form": "가을에요",
          "original_form": "가을에요",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014053.1.22",
          "form": "저는 그거 기대중이에요",
          "original_form": "저는 그거 기대중이에요",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014053.1.23",
          "form": "근데 피씨로 할때 욕심부리다가 강에 떨어졌다가 올라오면 가끔 역주행하기도해요...ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 피씨로 할때 욕심부리다가 강에 떨어졌다가 올라오면 가끔 역주행하기도해요...ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014053.1.24",
          "form": "오 리듬게임 좋아해요ㅋㅋㅋ",
          "original_form": "오 리듬게임 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014053.1.25",
          "form": "새로나오는거 몰랐네요ㅋㅋㅋ",
          "original_form": "새로나오는거 몰랐네요ㅋㅋㅋ",
