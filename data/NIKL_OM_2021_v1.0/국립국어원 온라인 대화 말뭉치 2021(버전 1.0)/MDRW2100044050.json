      "utterance": [
        {
          "id": "MDRW2100044050.1.1",
          "form": "원래 된장이 돌려막기 음식인가",
          "original_form": "원래 된장이 돌려막기 음식인가",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044050.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044050.1.3",
          "form": "응 된장 넣어서 푹 끓여",
          "original_form": "응 된장 넣어서 푹 끓여",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044050.1.4",
          "form": "된장찌개는 싫은데 이상하게",
          "original_form": "된장찌개는 싫은데 이상하게",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044050.1.5",
          "form": "시래기국은 맛있네",
          "original_form": "시래기국은 맛있네",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044050.1.6",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044050.1.7",
          "form": "찌개는 찌개스러운 걸쭉함이 있는데",
          "original_form": "찌개는 찌개스러운 걸쭉함이 있는데",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044050.1.8",
          "form": "국은 시원해",
          "original_form": "국은 시원해",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044050.1.9",
          "form": "요즘 된장찌개가 싫어졌어",
          "original_form": "요즘 된장찌개가 싫어졌어",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044050.1.10",
          "form": "그 양파쩐내가 싫어져서",
          "original_form": "그 양파쩐내가 싫어져서",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044050.1.11",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044050.1.12",
          "form": "양파도 쩐내가 나..?",
          "original_form": "양파도 쩐내가 나..?",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044050.1.13",
          "form": "응 그 특유의 양파 푹 끓은 냄새",
          "original_form": "응 그 특유의 양파 푹 끓은 냄새",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044050.1.14",
          "form": "냄새에 민감해졌나",
          "original_form": "냄새에 민감해졌나",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044050.1.15",
          "form": "넌 그런거없니",
          "original_form": "넌 그런거없니",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044050.1.16",
          "form": "아직 모르겠어 아직까지는 야채에서 냄새를 못 느껴본 것 같아",
          "original_form": "아직 모르겠어 아직까지는 야채에서 냄새를 못 느껴본 것 같아",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044050.1.17",
          "form": "수박썩으면 냄새 고약한거 아냐",
          "original_form": "수박썩으면 냄새 고약한거 아냐",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044050.1.18",
          "form": "아 그건 ㅇㅈㅇㅈㅇㅈ",
          "original_form": "아 그건 ㅇㅈㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044050.1.19",
          "form": "쉰내나고 맛이 시큼해",
          "original_form": "쉰내나고 맛이 시큼해",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044050.1.20",
          "form": "왜 알고 있냐면 나도 그다지 알고싶지 않았어",
          "original_form": "왜 알고 있냐면 나도 그다지 알고싶지 않았어",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044050.1.21",
          "form": "먹었니...?",
          "original_form": "먹었니...?",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044050.1.22",
          "form": "어.....",
          "original_form": "어.....",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044050.1.23",
          "form": "배앓이좀 했겠는데",
          "original_form": "배앓이좀 했겠는데",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044050.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ다행히 먹자마자 쉰 맛에 놀라서 뱉었어",
          "original_form": "ㅋㅋㅋㅋㅋㅋ다행히 먹자마자 쉰 맛에 놀라서 뱉었어",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044050.1.25",
          "form": "삼키지는 않았고",
          "original_form": "삼키지는 않았고",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044050.1.26",
          "form": "다행이다 으악",
          "original_form": "다행이다 으악",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044050.1.27",
          "form": "그 특유의 썩은 맛이",
          "original_form": "그 특유의 썩은 맛이",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044050.1.28",
          "form": "근데 다시는 겪고싶지 않아",
          "original_form": "근데 다시는 겪고싶지 않아",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044050.1.29",
          "form": "최악이야",
          "original_form": "최악이야",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044050.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아 구려",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아 구려",
          "speaker_id": "2",
          "time": "20210907 21:31"
        }
      ]
    }
  ]
