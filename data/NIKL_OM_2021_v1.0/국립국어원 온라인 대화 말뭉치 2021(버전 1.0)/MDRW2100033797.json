      "utterance": [
        {
          "id": "MDRW2100033797.1.1",
          "form": "납작당면 기본이요",
          "original_form": "납작당면 기본이요",
          "speaker_id": "2",
          "time": "20210825 02:28"
        },
        {
          "id": "MDRW2100033797.1.2",
          "form": "옥수수면 같은건 너무 불어서ㅜ",
          "original_form": "옥수수면 같은건 너무 불어서ㅜ",
          "speaker_id": "2",
          "time": "20210825 02:28"
        },
        {
          "id": "MDRW2100033797.1.3",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210825 02:29"
        },
        {
          "id": "MDRW2100033797.1.4",
          "form": "당황했잖아요",
          "original_form": "당황했잖아요",
          "speaker_id": "1",
          "time": "20210825 02:29"
        },
        {
          "id": "MDRW2100033797.1.5",
          "form": "옥수수면 추가햇다가",
          "original_form": "옥수수면 추가햇다가",
          "speaker_id": "1",
          "time": "20210825 02:29"
        },
        {
          "id": "MDRW2100033797.1.6",
          "form": "ㅎㅎ 무시무시한 옥수수면ㅎ",
          "original_form": "ㅎㅎ 무시무시한 옥수수면ㅎ",
          "speaker_id": "2",
          "time": "20210825 02:30"
        },
        {
          "id": "MDRW2100033797.1.7",
          "form": "다불어왔더라고요?ㅎ..",
          "original_form": "다불어왔더라고요?ㅎ..",
          "speaker_id": "1",
          "time": "20210825 02:30"
        },
        {
          "id": "MDRW2100033797.1.8",
          "form": "아 심지어 가서 먹은 것도 아니고 배달이요;;?",
          "original_form": "아 심지어 가서 먹은 것도 아니고 배달이요;;?",
          "speaker_id": "2",
          "time": "20210825 02:31"
        },
        {
          "id": "MDRW2100033797.1.9",
          "form": "아이고야ㄷㄷㄷ",
          "original_form": "아이고야ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210825 02:31"
        },
        {
          "id": "MDRW2100033797.1.10",
          "form": "ㅋㅋㅋㅋ 배달이요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 배달이요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:33"
        },
        {
          "id": "MDRW2100033797.1.11",
          "form": "당황했어아ㅡ",
          "original_form": "당황했어아ㅡ",
          "speaker_id": "1",
          "time": "20210825 02:33"
        },
        {
          "id": "MDRW2100033797.1.12",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "1",
          "time": "20210825 02:33"
        },
        {
          "id": "MDRW2100033797.1.13",
          "form": "ㅎㅎ 그후론 뭘 주로 넣으시나요",
          "original_form": "ㅎㅎ 그후론 뭘 주로 넣으시나요",
          "speaker_id": "2",
          "time": "20210825 02:34"
        },
        {
          "id": "MDRW2100033797.1.14",
          "form": "그뒤론 그래서",
          "original_form": "그뒤론 그래서",
          "speaker_id": "1",
          "time": "20210825 02:38"
        },
        {
          "id": "MDRW2100033797.1.15",
          "form": "당면을 넣어요ㅋㅋㅋㅋ",
          "original_form": "당면을 넣어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:39"
        },
        {
          "id": "MDRW2100033797.1.16",
          "form": "아님 안넣거나",
          "original_form": "아님 안넣거나",
          "speaker_id": "1",
          "time": "20210825 02:39"
        },
        {
          "id": "MDRW2100033797.1.17",
          "form": "ㅎㅎ 사실 당면 없어도 되죠ㅎ",
          "original_form": "ㅎㅎ 사실 당면 없어도 되죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 02:39"
        },
        {
          "id": "MDRW2100033797.1.18",
          "form": "당면좋은데 불어오더라고요ㅠ",
          "original_form": "당면좋은데 불어오더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210825 02:40"
        },
        {
          "id": "MDRW2100033797.1.19",
          "form": "저는 완자가 최애예요ㅋㅋㅋ",
          "original_form": "저는 완자가 최애예요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:40"
        },
        {
          "id": "MDRW2100033797.1.20",
          "form": "그 중국당면도 불어서 와요?ㅜ",
          "original_form": "그 중국당면도 불어서 와요?ㅜ",
          "speaker_id": "2",
          "time": "20210825 02:40"
        },
        {
          "id": "MDRW2100033797.1.21",
          "form": "완자 꼬치 꼭 추가하시겠네요ㅎ",
          "original_form": "완자 꼬치 꼭 추가하시겠네요ㅎ",
          "speaker_id": "2",
          "time": "20210825 02:41"
        },
        {
          "id": "MDRW2100033797.1.22",
          "form": "네네 좀 불더라고요ㅠ",
          "original_form": "네네 좀 불더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210825 02:41"
        },
        {
          "id": "MDRW2100033797.1.23",
          "form": "넵ㅋㅋㅋ 젤좋아요",
          "original_form": "넵ㅋㅋㅋ 젤좋아요",
          "speaker_id": "1",
          "time": "20210825 02:41"
        },
        {
          "id": "MDRW2100033797.1.24",
          "form": "야채는 뭐넣으시나요?",
          "original_form": "야채는 뭐넣으시나요?",
          "speaker_id": "1",
          "time": "20210825 02:41"
        },
        {
          "id": "MDRW2100033797.1.25",
          "form": "청경채 목이버섯 죽순 배추",
          "original_form": "청경채 목이버섯 죽순 배추",
          "speaker_id": "2",
          "time": "20210825 02:42"
        },
        {
          "id": "MDRW2100033797.1.26",
          "form": "숙주 고수 이렇게 고정이요ㅎ",
          "original_form": "숙주 고수 이렇게 고정이요ㅎ",
          "speaker_id": "2",
          "time": "20210825 02:42"
        },
        {
          "id": "MDRW2100033797.1.27",
          "form": "ㅋㅋㅋ저는 버섯이요",
          "original_form": "ㅋㅋㅋ저는 버섯이요",
