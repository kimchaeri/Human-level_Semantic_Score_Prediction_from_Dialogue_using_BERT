      "utterance": [
        {
          "id": "MDRW2100007756.1.1",
          "form": "ㅋㅋㅋㅋ이언니 머리 좋음 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ이언니 머리 좋음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:14"
        },
        {
          "id": "MDRW2100007756.1.2",
          "form": "요즘 빨래 쌓아 놓는다고여 ㅋㅋㅋ",
          "original_form": "요즘 빨래 쌓아 놓는다고여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007756.1.3",
          "form": "예전 같지.않음 ㅋㅋㅋ",
          "original_form": "예전 같지.않음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007756.1.4",
          "form": "아 쌓아놓는다곡 ㅋㅋㅋㄱ",
          "original_form": "아 쌓아놓는다곡 ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007756.1.5",
          "form": "수건도 어쩔땐 한개도 없어가 ㅋㅋ",
          "original_form": "수건도 어쩔땐 한개도 없어가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007756.1.6",
          "form": "변했네",
          "original_form": "변했네",
          "speaker_id": "1",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007756.1.7",
          "form": "건조기서 꺼내씀 ㅋㅋ",
          "original_form": "건조기서 꺼내씀 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007756.1.8",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㅋㄱ건조기 없었음",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㅋㄱ건조기 없었음",
          "speaker_id": "1",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007756.1.9",
          "form": "우애살았겠노",
          "original_form": "우애살았겠노",
          "speaker_id": "1",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007756.1.10",
          "form": "빨래가 늠 많이 나와요 ㅠ",
          "original_form": "빨래가 늠 많이 나와요 ㅠ",
          "speaker_id": "2",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007756.1.11",
          "form": "나도 오늘 빨래 해야된다",
          "original_form": "나도 오늘 빨래 해야된다",
          "speaker_id": "1",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007756.1.12",
          "form": "그래 샤워할때 한명당",
          "original_form": "그래 샤워할때 한명당",
          "speaker_id": "1",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007756.1.13",
          "form": "온식구가 다 나댕기니..",
          "original_form": "온식구가 다 나댕기니..",
          "speaker_id": "2",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007756.1.14",
          "form": "2개씩 쓰니",
          "original_form": "2개씩 쓰니",
          "speaker_id": "1",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007756.1.15",
          "form": "하루빨래 못하면 수건 모자람",
          "original_form": "하루빨래 못하면 수건 모자람",
          "speaker_id": "1",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007756.1.16",
          "form": "그니깐요~끝이 없다 증말 ㅋㅋ",
          "original_form": "그니깐요~끝이 없다 증말 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007756.1.17",
          "form": "ㅋㅋㅋㅋㄱ 열심히해라",
          "original_form": "ㅋㅋㅋㅋㄱ 열심히해라",
          "speaker_id": "1",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007756.1.18",
          "form": "나 오늘 늦잠자는날이라",
          "original_form": "나 오늘 늦잠자는날이라",
          "speaker_id": "1",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007756.1.19",
          "form": "ㅋㅋㅋㅋ빨래는 와이래 싫을까여~",
          "original_form": "ㅋㅋㅋㅋ빨래는 와이래 싫을까여~",
          "speaker_id": "2",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007756.1.20",
          "form": "아직 침대에 누워있는데",
          "original_form": "아직 침대에 누워있는데",
          "speaker_id": "1",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007756.1.21",
          "form": "푸하하하",
          "original_form": "푸하하하",
          "speaker_id": "2",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007756.1.22",
          "form": "방에서",
          "original_form": "방에서",
          "speaker_id": "2",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007756.1.23",
          "form": "나가기가 싫으네",
          "original_form": "나가기가 싫으네",
          "speaker_id": "1",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007756.1.24",
          "form": "누워있다",
          "original_form": "누워있다",
          "speaker_id": "1",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007756.1.25",
          "form": "자는척",
          "original_form": "자는척",
          "speaker_id": "1",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007756.1.26",
          "form": "혼자 자는척 하고있아요 ㅋㅋㅋ",
          "original_form": "혼자 자는척 하고있아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007756.1.27",
          "form": "멋지다 늦잠자는날 ㅋㅋㅋ",
          "original_form": "멋지다 늦잠자는날 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007756.1.28",
          "form": "우린 내가 아침잠이 없으니 늘 아침엔 내몫임 ㅋㅋ",
          "original_form": "우린 내가 아침잠이 없으니 늘 아침엔 내몫임 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007756.1.29",
          "form": "울신랑은 기절상태;;",
          "original_form": "울신랑은 기절상태;;",
          "speaker_id": "2",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007756.1.30",
          "form": "그래 아침잠이 없는것도 대단하다",
          "original_form": "그래 아침잠이 없는것도 대단하다",
          "speaker_id": "1",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007756.1.31",
          "form": "우린 둘다 아침잠 많아서",
          "original_form": "우린 둘다 아침잠 많아서",
          "speaker_id": "1",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007756.1.32",
          "form": "이래 일찍 자는게 ㅋㅋㅋㅋ",
          "original_form": "이래 일찍 자는게 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007756.1.33",
          "form": "날 정해서 서로 늦잠잔다",
          "original_form": "날 정해서 서로 늦잠잔다",
          "speaker_id": "1",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007756.1.34",
          "form": "일찍자고 일찍 일어나고",
          "original_form": "일찍자고 일찍 일어나고",
          "speaker_id": "1",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007756.1.35",
          "form": "새나라의 어른이네",
          "original_form": "새나라의 어른이네",
          "speaker_id": "1",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007756.1.36",
          "form": "헌나라의.꼰대임 ㅋㅋㅋㅋㅋ",
          "original_form": "헌나라의.꼰대임 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:18"
        }
