      "utterance": [
        {
          "id": "MDRW2100044713.1.1",
          "form": "겨울간식 좋아하세요??",
          "original_form": "겨울간식 좋아하세요??",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044713.1.2",
          "form": "붕어빵 , 군고구마. 호빵.",
          "original_form": "붕어빵 , 군고구마. 호빵.",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044713.1.3",
          "form": "ㅠㅠㅠ 배고픈건 아닌데 왜이리 또 먹고 싶져",
          "original_form": "ㅠㅠㅠ 배고픈건 아닌데 왜이리 또 먹고 싶져",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044713.1.4",
          "form": "팥붕이세요 슈붕이세요",
          "original_form": "팥붕이세요 슈붕이세요",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044713.1.5",
          "form": "전 두개중에 팥",
          "original_form": "전 두개중에 팥",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044713.1.6",
          "form": "근데 가끔 슈도..맛있어여",
          "original_form": "근데 가끔 슈도..맛있어여",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044713.1.7",
          "form": "저도 팥에 가끔 슈 좋아요",
          "original_form": "저도 팥에 가끔 슈 좋아요",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044713.1.8",
          "form": "그게 딱 적절하죠? 역시",
          "original_form": "그게 딱 적절하죠? 역시",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044713.1.9",
          "form": "원조는 원조죠",
          "original_form": "원조는 원조죠",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044713.1.10",
          "form": "호빵은 어떤 호빵이신가요????",
          "original_form": "호빵은 어떤 호빵이신가요????",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044713.1.11",
          "form": "호빵은. 아... 어렵네요",
          "original_form": "호빵은. 아... 어렵네요",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044713.1.12",
          "form": "팥이맛있고 그담 야채. 그담 피자",
          "original_form": "팥이맛있고 그담 야채. 그담 피자",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044713.1.13",
          "form": "저는 피자여!!",
          "original_form": "저는 피자여!!",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044713.1.14",
          "form": "저는 호빵 팥은 좀 달더라고요",
          "original_form": "저는 호빵 팥은 좀 달더라고요",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044713.1.15",
          "form": "피자 팥 야채입니다!!!",
          "original_form": "피자 팥 야채입니다!!!",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044713.1.16",
          "form": "그나저나 붕어빵 요즘에는 치즈 들어간것도 판대요",
          "original_form": "그나저나 붕어빵 요즘에는 치즈 들어간것도 판대요",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044713.1.17",
          "form": "저랑 이야기한것 같은디 ㅎ",
          "original_form": "저랑 이야기한것 같은디 ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044713.1.18",
          "form": "근데 두개에 이천원!!",
          "original_form": "근데 두개에 이천원!!",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044713.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044713.1.20",
          "form": "맞아옄ㅋㅋㅋ",
          "original_form": "맞아옄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044713.1.21",
          "form": "네??? 두개 천원 아니고??",
          "original_form": "네??? 두개 천원 아니고??",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044713.1.22",
          "form": "네!!!",
          "original_form": "네!!!",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044713.1.23",
          "form": "왜 갑자기 두배나 받아요",
          "original_form": "왜 갑자기 두배나 받아요",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044713.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044713.1.25",
