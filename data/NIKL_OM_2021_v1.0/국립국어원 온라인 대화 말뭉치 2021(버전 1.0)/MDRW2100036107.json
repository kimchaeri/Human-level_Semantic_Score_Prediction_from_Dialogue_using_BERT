      "utterance": [
        {
          "id": "MDRW2100036107.1.1",
          "form": "저녁 뭐 드실거에요?",
          "original_form": "저녁 뭐 드실거에요?",
          "speaker_id": "2",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100036107.1.2",
          "form": "저요 그건 아직 모르겠어요 집 밥이랑 반찬?ㅋㅋㅋ",
          "original_form": "저요 그건 아직 모르겠어요 집 밥이랑 반찬?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:26"
        },
        {
          "id": "MDRW2100036107.1.3",
          "form": "ㅋㅋㅋㅋㅋ 해드실건가요?!",
          "original_form": "ㅋㅋㅋㅋㅋ 해드실건가요?!",
          "speaker_id": "2",
          "time": "20210830 16:28"
        },
        {
          "id": "MDRW2100036107.1.4",
          "form": "밥 정도는 해먹죠 ㅋㅋ 밥솥에 불올려서요",
          "original_form": "밥 정도는 해먹죠 ㅋㅋ 밥솥에 불올려서요",
          "speaker_id": "1",
          "time": "20210830 16:28"
        },
        {
          "id": "MDRW2100036107.1.5",
          "form": "오홍.. 요리는",
          "original_form": "오홍.. 요리는",
          "speaker_id": "2",
          "time": "20210830 16:29"
        },
        {
          "id": "MDRW2100036107.1.6",
          "form": "자주 하세여?",
          "original_form": "자주 하세여?",
          "speaker_id": "2",
          "time": "20210830 16:29"
        },
        {
          "id": "MDRW2100036107.1.7",
          "form": "집에서 밥먹을땐 하죠? 집에서도 저녁을 자주 먹어요",
          "original_form": "집에서 밥먹을땐 하죠? 집에서도 저녁을 자주 먹어요",
          "speaker_id": "1",
          "time": "20210830 16:30"
        },
        {
          "id": "MDRW2100036107.1.8",
          "form": "아하 좋아하세요?",
          "original_form": "아하 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 16:33"
        },
        {
          "id": "MDRW2100036107.1.9",
          "form": "노동자체는 모든 노동을 싫어하지만 맛있는 음식이란 결과물은 좋아하니 좋아하는건가 아닌가",
          "original_form": "노동자체는 모든 노동을 싫어하지만 맛있는 음식이란 결과물은 좋아하니 좋아하는건가 아닌가",
          "speaker_id": "1",
          "time": "20210830 16:33"
        },
        {
          "id": "MDRW2100036107.1.10",
          "form": "잘 모르겠지만 저정도 생각입니다 ㅋㅋ",
          "original_form": "잘 모르겠지만 저정도 생각입니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:34"
        },
        {
          "id": "MDRW2100036107.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아.. 그렇죠..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아.. 그렇죠..",
          "speaker_id": "2",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100036107.1.12",
          "form": "전 귀찮아서",
          "original_form": "전 귀찮아서",
          "speaker_id": "2",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100036107.1.13",
          "form": "잘 안하거든요",
          "original_form": "잘 안하거든요",
          "speaker_id": "2",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100036107.1.14",
          "form": "전 운동도 그래요 막상 함 몸은 상쾌하니 좋은데 하기싫은,,",
          "original_form": "전 운동도 그래요 막상 함 몸은 상쾌하니 좋은데 하기싫은,,",
          "speaker_id": "1",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100036107.1.15",
          "form": "아 맞아요 ㅠ",
          "original_form": "아 맞아요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 16:36"
        },
        {
          "id": "MDRW2100036107.1.16",
          "form": "그래서 전 항상 안하고",
          "original_form": "그래서 전 항상 안하고",
          "speaker_id": "2",
          "time": "20210830 16:36"
        },
        {
          "id": "MDRW2100036107.1.17",
          "form": "후회하는.....",
          "original_form": "후회하는.....",
          "speaker_id": "2",
          "time": "20210830 16:36"
        },
        {
          "id": "MDRW2100036107.1.18",
          "form": "그럴땐 강도를 낮춰보세요",
          "original_form": "그럴땐 강도를 낮춰보세요",
          "speaker_id": "1",
          "time": "20210830 16:37"
        },
        {
          "id": "MDRW2100036107.1.19",
          "form": "밥이 무리면 냉동만두라도 구워서 오이와 양파와 먹는다 같이요",
          "original_form": "밥이 무리면 냉동만두라도 구워서 오이와 양파와 먹는다 같이요",
          "speaker_id": "1",
          "time": "20210830 16:37"
        },
        {
          "id": "MDRW2100036107.1.20",
          "form": "그렇게 먹어도 배가 찰까 걱정이라서여",
          "original_form": "그렇게 먹어도 배가 찰까 걱정이라서여",
          "speaker_id": "2",
          "time": "20210830 16:39"
        },
        {
          "id": "MDRW2100036107.1.21",
          "form": "근데 냉동만두",
          "original_form": "근데 냉동만두",
          "speaker_id": "2",
          "time": "20210830 16:39"
        },
        {
          "id": "MDRW2100036107.1.22",
          "form": "되게 맛있을 거 같네요",
          "original_form": "되게 맛있을 거 같네요",
          "speaker_id": "2",
          "time": "20210830 16:39"
        },
        {
          "id": "MDRW2100036107.1.23",
          "form": "네 비비고 만두 같은거랑 야채먹음 되게 맛있고 든든해요",
          "original_form": "네 비비고 만두 같은거랑 야채먹음 되게 맛있고 든든해요",
          "speaker_id": "1",
          "time": "20210830 16:40"
        }
      ]
