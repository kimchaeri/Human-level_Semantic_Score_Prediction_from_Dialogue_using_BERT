      "utterance": [
        {
          "id": "MDRW2100033563.1.1",
          "form": "이따 파바 시키실껀가요?",
          "original_form": "이따 파바 시키실껀가요?",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100033563.1.2",
          "form": "네 파바는 써야될듯ㅋ",
          "original_form": "네 파바는 써야될듯ㅋ",
          "speaker_id": "1",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100033563.1.3",
          "form": "0원 결제는 안해봐서 기대하고있어요~",
          "original_form": "0원 결제는 안해봐서 기대하고있어요~",
          "speaker_id": "2",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100033563.1.4",
          "form": "슈퍼클럽 7번남았어요",
          "original_form": "슈퍼클럽 7번남았어요",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100033563.1.5",
          "form": "무슨빵좋아해요?",
          "original_form": "무슨빵좋아해요?",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100033563.1.6",
          "form": "ㅎㅎ 근데 가기싫을 시간이라 고민이~~",
          "original_form": "ㅎㅎ 근데 가기싫을 시간이라 고민이~~",
          "speaker_id": "2",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100033563.1.7",
          "form": "모카크림빵이요",
          "original_form": "모카크림빵이요",
          "speaker_id": "2",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100033563.1.8",
          "form": "한번도 안사봤는데 맛나요?",
          "original_form": "한번도 안사봤는데 맛나요?",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100033563.1.9",
          "form": "슈크림도넛? 요즘 이것도맛있더라구요",
          "original_form": "슈크림도넛? 요즘 이것도맛있더라구요",
          "speaker_id": "2",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100033563.1.10",
          "form": "전 고구마페스츄리 맛나던데",
          "original_form": "전 고구마페스츄리 맛나던데",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100033563.1.11",
          "form": "넘 비싸서 5500인가해요.",
          "original_form": "넘 비싸서 5500인가해요.",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100033563.1.12",
          "form": "저도요~~  팥도너츠 쫀득하니 하나먹음 힘나는거같아요",
          "original_form": "저도요~~  팥도너츠 쫀득하니 하나먹음 힘나는거같아요",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100033563.1.13",
          "form": "크림치즈도 맛있고",
          "original_form": "크림치즈도 맛있고",
          "speaker_id": "1",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100033563.1.14",
          "form": "리코타치즈 샐러드도 괜찮더라구요",
          "original_form": "리코타치즈 샐러드도 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100033563.1.15",
          "form": "샐러드는 만들기 귀찮을때 가끔 사먹어요",
          "original_form": "샐러드는 만들기 귀찮을때 가끔 사먹어요",
          "speaker_id": "2",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100033563.1.16",
          "form": "요기요할때 시켜먹음 좋더라구요",
          "original_form": "요기요할때 시켜먹음 좋더라구요",
          "speaker_id": "1",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100033563.1.17",
          "form": "마자요. 샐러드랑 우유한개하면 딱만원이요~",
          "original_form": "마자요. 샐러드랑 우유한개하면 딱만원이요~",
          "speaker_id": "2",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100033563.1.18",
          "form": "집매장은 샐러드가없어서 아쉽",
          "original_form": "집매장은 샐러드가없어서 아쉽",
          "speaker_id": "1",
