        },
        {
          "id": "MDRW2100028383.1.5",
          "form": "셔",
          "original_form": "셔",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028383.1.6",
          "form": "브리님 답이 늦어서 죄송합니다ㅠ 시약준비하느라...",
          "original_form": "브리님 답이 늦어서 죄송합니다ㅠ 시약준비하느라...",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100028383.1.7",
          "form": "아니 제가 커뮤는 저희 학교 커뮤만 하는데",
          "original_form": "아니 제가 커뮤는 저희 학교 커뮤만 하는데",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100028383.1.8",
          "form": "여대 특성상 커뮤가 되게 활발해요",
          "original_form": "여대 특성상 커뮤가 되게 활발해요",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100028383.1.9",
          "form": "그래서 페미니즘이 디폴트이긴 한데",
          "original_form": "그래서 페미니즘이 디폴트이긴 한데",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100028383.1.10",
          "form": "그래서 다들 기본적으로",
          "original_form": "그래서 다들 기본적으로",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100028383.1.11",
          "form": "당당한 여성상 정도는 추구하기는 하는데",
          "original_form": "당당한 여성상 정도는 추구하기는 하는데",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100028383.1.12",
          "form": "그거 아세요...? 되게",
          "original_form": "그거 아세요...? 되게",
          "speaker_id": "1",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100028383.1.13",
          "form": "탈코하고",
          "original_form": "탈코하고",
          "speaker_id": "1",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100028383.1.14",
          "form": "페미니스트인 것을 티내는 사람들을 내려다보는 그런 부류들이 있어요",
          "original_form": "페미니스트인 것을 티내는 사람들을 내려다보는 그런 부류들이 있어요",
          "speaker_id": "1",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100028383.1.15",
          "form": "자기들은 영악하게 페미니즘한다 이거예요 그니까",
          "original_form": "자기들은 영악하게 페미니즘한다 이거예요 그니까",
          "speaker_id": "1",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100028383.1.16",
          "form": "꾸밀 땐 꾸미고 남자들이랑 데이트도 하는데",
          "original_form": "꾸밀 땐 꾸미고 남자들이랑 데이트도 하는데",
          "speaker_id": "1",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100028383.1.17",
          "form": "상류층 남자랑 만나는 건 이대생 이미지에도 좋다",
          "original_form": "상류층 남자랑 만나는 건 이대생 이미지에도 좋다",
          "speaker_id": "1",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100028383.1.18",
          "form": "이런식으로 주장하면서 입시철만 되면",
          "original_form": "이런식으로 주장하면서 입시철만 되면",
          "speaker_id": "1",
          "time": "20210902 19:37"
        },
        {
          "id": "MDRW2100028383.1.19",
          "form": "탈코한 사람을 패는 플로우가 올라와요",
          "original_form": "탈코한 사람을 패는 플로우가 올라와요",
          "speaker_id": "1",
          "time": "20210902 19:37"
        },
        {
          "id": "MDRW2100028383.1.20",
          "form": "헐....진짜 몰랐어요",
          "original_form": "헐....진짜 몰랐어요",
          "speaker_id": "2",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100028383.1.21",
          "form": "일 보시면서 천천히 답장하세요!!",
          "original_form": "일 보시면서 천천히 답장하세요!!",
          "speaker_id": "2",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100028383.1.22",
          "form": "네네 그래서",
          "original_form": "네네 그래서",
          "speaker_id": "1",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100028383.1.23",
          "form": "하 이게 뭐라해야되지",
          "original_form": "하 이게 뭐라해야되지",
          "speaker_id": "1",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100028383.1.24",
          "form": "탈코한 사람도 많은데",
          "original_form": "탈코한 사람도 많은데",
          "speaker_id": "1",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100028383.1.25",
          "form": "진짜 참 뭐라할까 독기룩",
          "original_form": "진짜 참 뭐라할까 독기룩",
          "speaker_id": "2",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100028383.1.26",
          "form": "그만큼 헛똑똑이들도 많아요",
          "original_form": "그만큼 헛똑똑이들도 많아요",
          "speaker_id": "1",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100028383.1.27",
          "form": "같은 사람들",
          "original_form": "같은 사람들",
          "speaker_id": "2",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100028383.1.28",
          "form": "어 맞아요 독기룩 ㄹㅇ",
          "original_form": "어 맞아요 독기룩 ㄹㅇ",
          "speaker_id": "1",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100028383.1.29",
          "form": "근데 걔네가 실제로 또",
          "original_form": "근데 걔네가 실제로 또",
          "speaker_id": "1",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100028383.1.30",
          "form": "맞네요 헛똑똑",
          "original_form": "맞네요 헛똑똑",
          "speaker_id": "2",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100028383.1.31",
          "form": "집도 잘 살고",
          "original_form": "집도 잘 살고",
          "speaker_id": "1",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100028383.1.32",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100028383.1.33",
          "form": "흔히 말하는 갓생",
          "original_form": "흔히 말하는 갓생",
          "speaker_id": "1",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100028383.1.34",
          "form": "살기는 해요 또",
          "original_form": "살기는 해요 또",
          "speaker_id": "1",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100028383.1.35",
          "form": "제 지인 중에도 있거든요",
          "original_form": "제 지인 중에도 있거든요",
          "speaker_id": "1",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100028383.1.36",
          "form": "이대 졸업해서 치대 편입해갖고",
          "original_form": "이대 졸업해서 치대 편입해갖고",
          "speaker_id": "1",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100028383.1.37",
          "form": "집 줜나 잘살고",
          "original_form": "집 줜나 잘살고",
          "speaker_id": "1",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100028383.1.38",
          "form": "근데 남자 못잃는데 하루는 저한테",
          "original_form": "근데 남자 못잃는데 하루는 저한테",
          "speaker_id": "1",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100028383.1.39",
          "form": "학교 커뮤에 랟펨 글 올리는거 저 아니냐고 하더라구요",
          "original_form": "학교 커뮤에 랟펨 글 올리는거 저 아니냐고 하더라구요",
          "speaker_id": "1",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100028383.1.40",
          "form": "왜 비혼주의 강요하냐고 저한테 따지는거예욬ㅋㅋㅋㅋ",
          "original_form": "왜 비혼주의 강요하냐고 저한테 따지는거예욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100028383.1.41",
          "form": "ㅋㅋㅋㅋㅋ아 재",
          "original_form": "ㅋㅋㅋㅋㅋ아 재",
          "speaker_id": "2",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100028383.1.42",
          "form": "수",
          "original_form": "수",
          "speaker_id": "2",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100028383.1.43",
          "form": "없어요",
          "original_form": "없어요",
          "speaker_id": "2",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100028383.1.44",
          "form": "강요라니 지들이 괜히 찔리는거아닌가",
          "original_form": "강요라니 지들이 괜히 찔리는거아닌가",
          "speaker_id": "2",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100028383.1.45",
          "form": "그니까욬ㅋㅋㅋㅋㅋㅋ 백퍼 그냥 지들이 찔리는건데",
          "original_form": "그니까욬ㅋㅋㅋㅋㅋㅋ 백퍼 그냥 지들이 찔리는건데",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028383.1.46",
          "form": "근데 그런 인간들이 너무 많아서",
          "original_form": "근데 그런 인간들이 너무 많아서",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028383.1.47",
          "form": "문제예요",
          "original_form": "문제예요",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028383.1.48",
          "form": "아까는 어떤 탈코한 사람으로 추정되는 사람이",
          "original_form": "아까는 어떤 탈코한 사람으로 추정되는 사람이",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028383.1.49",
          "form": "탈코하고 나니까 긴머리가 너무 비위생적으로 보인다",
          "original_form": "탈코하고 나니까 긴머리가 너무 비위생적으로 보인다",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028383.1.50",
          "form": "이런 글을 썼는데",
          "original_form": "이런 글을 썼는데",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028383.1.51",
          "form": "진짜 300개 가까운 댓글이 달렸는데",
          "original_form": "진짜 300개 가까운 댓글이 달렸는데",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028383.1.52",
          "form": "그 중 절반 이상이",
          "original_form": "그 중 절반 이상이",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028383.1.53",
          "form": "왜 광역저격을 하냐,",
          "original_form": "왜 광역저격을 하냐,",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028383.1.54",
          "form": "먼저 탈코했다고 긴머리인 사람 무시하는거냐",
          "original_form": "먼저 탈코했다고 긴머리인 사람 무시하는거냐",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028383.1.55",
          "form": "이런게 와다다다 달린거예요",
          "original_form": "이런게 와다다다 달린거예요",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028383.1.56",
          "form": "정말 쥐뢀이네요",
          "original_form": "정말 쥐뢀이네요",
          "speaker_id": "2",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100028383.1.57",
          "form": "본인들이 이대생의 이미지를 망치는 거 아닌지..",
          "original_form": "본인들이 이대생의 이미지를 망치는 거 아닌지..",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028383.1.58",
          "form": "커뮤 에타 하시는건가요?",
          "original_form": "커뮤 에타 하시는건가요?",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028383.1.59",
          "form": "아 에타도 활발하긴 한데",
          "original_form": "아 에타도 활발하긴 한데",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028383.1.60",
          "form": "저희는 자체 커뮤니티 이화이언도 활발해요!!!",
          "original_form": "저희는 자체 커뮤니티 이화이언도 활발해요!!!",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028383.1.61",
          "form": "이게 더 인증이 까다로워서...",
          "original_form": "이게 더 인증이 까다로워서...",
          "speaker_id": "1",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100028383.1.62",
          "form": "네 그래서 저 글에,,",
          "original_form": "네 그래서 저 글에,,",
          "speaker_id": "1",
          "time": "20210902 20:49"
        },
