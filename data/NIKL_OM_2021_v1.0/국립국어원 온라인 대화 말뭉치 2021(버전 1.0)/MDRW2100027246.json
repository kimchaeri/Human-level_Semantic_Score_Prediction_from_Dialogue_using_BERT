        },
        {
          "id": "MDRW2100027246.1.3",
          "form": "피자는 어디가 갑이죠",
          "original_form": "피자는 어디가 갑이죠",
          "speaker_id": "1",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027246.1.4",
          "form": "쌀국수땡기는날이네요",
          "original_form": "쌀국수땡기는날이네요",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027246.1.5",
          "form": "피자는 반올림피자요",
          "original_form": "피자는 반올림피자요",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027246.1.6",
          "form": "가격대비 짱맛",
          "original_form": "가격대비 짱맛",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027246.1.7",
          "form": "처음들어요",
          "original_form": "처음들어요",
          "speaker_id": "1",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027246.1.8",
          "form": "존맛",
          "original_form": "존맛",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027246.1.9",
          "form": "싸나요",
          "original_form": "싸나요",
          "speaker_id": "1",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027246.1.10",
          "form": "대신동에있어요",
          "original_form": "대신동에있어요",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027246.1.11",
          "form": "요일할인있나요",
          "original_form": "요일할인있나요",
          "speaker_id": "1",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027246.1.12",
          "form": "네저렴한축이에요",
          "original_form": "네저렴한축이에요",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027246.1.13",
          "form": "요일할인은모르겠지만",
          "original_form": "요일할인은모르겠지만",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027246.1.14",
          "form": "피자스쿨과 비슷한가요",
          "original_form": "피자스쿨과 비슷한가요",
          "speaker_id": "1",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027246.1.15",
          "form": "배달어플에서할인행사행ㅂ",
          "original_form": "배달어플에서할인행사행ㅂ",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027246.1.16",
          "form": "고구마피자가 맛있습니다",
          "original_form": "고구마피자가 맛있습니다",
          "speaker_id": "1",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027246.1.17",
          "form": "그거보단 가격이있어요",
          "original_form": "그거보단 가격이있어요",
          "speaker_id": "2",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100027246.1.18",
          "form": "크러스트좋아해요",
          "original_form": "크러스트좋아해요",
