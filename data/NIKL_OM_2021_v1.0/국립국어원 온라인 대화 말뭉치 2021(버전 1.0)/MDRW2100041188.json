      "utterance": [
        {
          "id": "MDRW2100041188.1.1",
          "form": "마카롱 좋아하세요?",
          "original_form": "마카롱 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100041188.1.2",
          "form": "막 찾아먹진 않는데 있으면 먹어요 ㅋㅋ",
          "original_form": "막 찾아먹진 않는데 있으면 먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:51"
        },
        {
          "id": "MDRW2100041188.1.3",
          "form": "네 색깔이 예뻐서 좋아요",
          "original_form": "네 색깔이 예뻐서 좋아요",
          "speaker_id": "1",
          "time": "20210905 19:51"
        },
        {
          "id": "MDRW2100041188.1.4",
          "form": "먹음직스러워요 ㅎㅎ",
          "original_form": "먹음직스러워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 19:52"
        },
        {
          "id": "MDRW2100041188.1.5",
          "form": "무슨 맛 좋아하세요",
          "original_form": "무슨 맛 좋아하세요",
          "speaker_id": "2",
          "time": "20210905 19:52"
        },
        {
          "id": "MDRW2100041188.1.6",
          "form": "피스타치오맛? 그거 맛있던데요",
          "original_form": "피스타치오맛? 그거 맛있던데요",
          "speaker_id": "1",
          "time": "20210905 20:06"
        },
        {
          "id": "MDRW2100041188.1.7",
          "form": "오 그런 맛도 나오는군요 저는 녹차 좋아해요",
          "original_form": "오 그런 맛도 나오는군요 저는 녹차 좋아해요",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041188.1.8",
          "form": "아아 녹차맛 맛있겠어요",
          "original_form": "아아 녹차맛 맛있겠어요",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041188.1.9",
          "form": "마카롱은 쫀득한 식감이 좋아요",
          "original_form": "마카롱은 쫀득한 식감이 좋아요",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041188.1.10",
          "form": "한입 물었을때 쫀득한 그 느낌이 좋은거 같아요",
          "original_form": "한입 물었을때 쫀득한 그 느낌이 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100041188.1.11",
          "form": "네네 맞아요 그거요!!!",
          "original_form": "네네 맞아요 그거요!!!",
          "speaker_id": "1",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100041188.1.12",
          "form": "요새 뚱카롱도 많이 나오던데.. 드셔보셨나요 전 사진만 봐서 ㅋㅋ",
          "original_form": "요새 뚱카롱도 많이 나오던데.. 드셔보셨나요 전 사진만 봐서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100041188.1.13",
          "form": "아뇨 저도 사진으로만 봤어요",
          "original_form": "아뇨 저도 사진으로만 봤어요",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100041188.1.14",
          "form": "밸런스는 안맞아보이더러구요",
          "original_form": "밸런스는 안맞아보이더러구요",
          "speaker_id": "1",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100041188.1.15",
          "form": "그래도 안에 필링 많으면 맛있으려나요",
          "original_form": "그래도 안에 필링 많으면 맛있으려나요",
          "speaker_id": "1",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100041188.1.16",
          "form": "그쵸 근데 좀 느끼할거 같아요 ㅋㅋㅋㅋ",
          "original_form": "그쵸 근데 좀 느끼할거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100041188.1.17",
          "form": "한개먹으면.. 배부를듯한",
          "original_form": "한개먹으면.. 배부를듯한",
          "speaker_id": "2",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100041188.1.18",
          "form": "네 엄청 대형 마카롱 있더라구요 ㅋㅋㅋ",
          "original_form": "네 엄청 대형 마카롱 있더라구요 ㅋㅋㅋ",
