        },
        {
          "id": "MDRW2100013760.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013760.1.3",
          "form": "집안일 좋아하세요?",
          "original_form": "집안일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013760.1.4",
          "form": "왜인사하고 사라지세요?",
          "original_form": "왜인사하고 사라지세요?",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013760.1.5",
          "form": "일하고있습니다",
          "original_form": "일하고있습니다",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013760.1.6",
          "form": "집안일",
          "original_form": "집안일",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013760.1.7",
          "form": "청소좋아하는거같아요",
          "original_form": "청소좋아하는거같아요",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013760.1.8",
          "form": "청소 보통 뭐하세요? 집에서 집안일 거의 다하세요?",
          "original_form": "청소 보통 뭐하세요? 집에서 집안일 거의 다하세요?",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100013760.1.9",
          "form": "기냥 쓸고닦고해요ㅎㅎ",
          "original_form": "기냥 쓸고닦고해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 09:27"
        },
        {
          "id": "MDRW2100013760.1.10",
          "form": "식구가 둘이라 치울것도 별로 없네요",
          "original_form": "식구가 둘이라 치울것도 별로 없네요",
          "speaker_id": "1",
          "time": "20210826 09:27"
        },
        {
          "id": "MDRW2100013760.1.11",
          "form": "아 그래요? 결혼하시면 집안일 다 하실거예요?",
          "original_form": "아 그래요? 결혼하시면 집안일 다 하실거예요?",
          "speaker_id": "2",
          "time": "20210826 11:52"
        },
        {
          "id": "MDRW2100013760.1.12",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100013760.1.13",
          "form": "반반이죠",
          "original_form": "반반이죠",
          "speaker_id": "1",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100013760.1.14",
          "form": "제가 요리 상대방이 설거지",
          "original_form": "제가 요리 상대방이 설거지",
          "speaker_id": "1",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100013760.1.15",
          "form": "아네..",
          "original_form": "아네..",
          "speaker_id": "2",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100013760.1.16",
          "form": "요리좋아하시나봐요",
          "original_form": "요리좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100013760.1.17",
          "form": "넵 요리좋죠",
          "original_form": "넵 요리좋죠",
          "speaker_id": "1",
          "time": "20210827 09:11"
        },
        {
          "id": "MDRW2100013760.1.18",
          "form": "근데 주방이 좁아서요",
          "original_form": "근데 주방이 좁아서요",
          "speaker_id": "1",
          "time": "20210827 09:11"
        },
        {
          "id": "MDRW2100013760.1.19",
          "form": "ㅋㅋㅋㅋ어떤요리 제일 잘하세요!?",
          "original_form": "ㅋㅋㅋㅋ어떤요리 제일 잘하세요!?",
          "speaker_id": "2",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100013760.1.20",
          "form": "ㅋㅋ요즘은 마라샹궈잘합니다",
          "original_form": "ㅋㅋ요즘은 마라샹궈잘합니다",
          "speaker_id": "1",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100013760.1.21",
          "form": "우와 저 마랴샹궈 좋아해요!",
          "original_form": "우와 저 마랴샹궈 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100013760.1.22",
          "form": "기회가 되면 제가 해드릴게요",
          "original_form": "기회가 되면 제가 해드릴게요",
          "speaker_id": "1",
