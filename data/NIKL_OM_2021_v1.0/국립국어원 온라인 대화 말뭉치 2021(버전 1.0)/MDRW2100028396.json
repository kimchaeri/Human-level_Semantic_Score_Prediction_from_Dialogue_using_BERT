        },
        {
          "id": "MDRW2100028396.1.2",
          "form": "글케재밋나 디피",
          "original_form": "글케재밋나 디피",
          "speaker_id": "1",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100028396.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ차단 ㅠㅠ넘해",
          "original_form": "ㅋㅋㅋㅋㅋㅋ차단 ㅠㅠ넘해",
          "speaker_id": "2",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100028396.1.4",
          "form": "재미나 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "재미나 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100028396.1.5",
          "form": "4화보는중",
          "original_form": "4화보는중",
          "speaker_id": "2",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100028396.1.6",
          "form": "그 누구지 구씨",
          "original_form": "그 누구지 구씨",
          "speaker_id": "2",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100028396.1.7",
          "form": "익살스럽게 연기잘해",
          "original_form": "익살스럽게 연기잘해",
          "speaker_id": "2",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100028396.1.8",
          "form": "웨이브끝나면 넷플조져야지",
          "original_form": "웨이브끝나면 넷플조져야지",
          "speaker_id": "1",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100028396.1.9",
          "form": "웨브 나두 잘 안봄 요즘ㅋㅋ",
          "original_form": "웨브 나두 잘 안봄 요즘ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100028396.1.10",
          "form": "볼게없어",
          "original_form": "볼게없어",
          "speaker_id": "1",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028396.1.11",
          "form": "마자 볼거 없어",
          "original_form": "마자 볼거 없어",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028396.1.12",
          "form": "오 ㅏ 피부과 가따 친구랑 맘터치 먹고",
          "original_form": "오 ㅏ 피부과 가따 친구랑 맘터치 먹고",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028396.1.13",
          "form": "좌석버스 서서가떠니 자리 읍어서",
          "original_form": "좌석버스 서서가떠니 자리 읍어서",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
