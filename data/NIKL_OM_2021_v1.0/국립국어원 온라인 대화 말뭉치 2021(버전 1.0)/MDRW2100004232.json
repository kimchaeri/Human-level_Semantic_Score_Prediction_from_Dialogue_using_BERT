      "utterance": [
        {
          "id": "MDRW2100004232.1.1",
          "form": "최근에 영화 뭐 봤어?",
          "original_form": "최근에 영화 뭐 봤어?",
          "speaker_id": "1",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004232.1.2",
          "form": "최근에 크루엘라!",
          "original_form": "최근에 크루엘라!",
          "speaker_id": "2",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004232.1.3",
          "form": "너는",
          "original_form": "너는",
          "speaker_id": "2",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004232.1.4",
          "form": "나는 블랙위도우",
          "original_form": "나는 블랙위도우",
          "speaker_id": "1",
          "time": "20210821 15:43"
        },
        {
          "id": "MDRW2100004232.1.5",
          "form": "오 블랙위도우 보고싶었는데 어땠어",
          "original_form": "오 블랙위도우 보고싶었는데 어땠어",
          "speaker_id": "2",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004232.1.6",
          "form": "엄청 재미있었어",
          "original_form": "엄청 재미있었어",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004232.1.7",
          "form": "어벤져스 시리즈 하나도 안봐서 내용 이해 못하면 어쩌나 했는데",
          "original_form": "어벤져스 시리즈 하나도 안봐서 내용 이해 못하면 어쩌나 했는데",
          "speaker_id": "1",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004232.1.8",
          "form": "몰라도 재미있더라고",
          "original_form": "몰라도 재미있더라고",
          "speaker_id": "1",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004232.1.9",
          "form": "내가 그래서 안봤는데 시리즈를 다안봐가지고ㅜ",
          "original_form": "내가 그래서 안봤는데 시리즈를 다안봐가지고ㅜ",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004232.1.10",
          "form": "오 티비결제해서 한번봐봐야겠다",
          "original_form": "오 티비결제해서 한번봐봐야겠다",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004232.1.11",
          "form": "넷플봐?",
          "original_form": "넷플봐?",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004232.1.12",
          "form": "넷플 봐!!!",
          "original_form": "넷플 봐!!!",
          "speaker_id": "1",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004232.1.13",
          "form": "뭐 재밌게봤어??",
          "original_form": "뭐 재밌게봤어??",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004232.1.14",
          "form": "독립영화중에 소공녀라고 있는데",
          "original_form": "독립영화중에 소공녀라고 있는데",
          "speaker_id": "1",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004232.1.15",
          "form": "그거 재미있더라고",
          "original_form": "그거 재미있더라고",
          "speaker_id": "1",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004232.1.16",
          "form": "오 소공녀 옛날 이야기책소공녀는아는데 그거랑완전 다른거겠지? ㅋㅋㅋ",
          "original_form": "오 소공녀 옛날 이야기책소공녀는아는데 그거랑완전 다른거겠지? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004232.1.17",
          "form": "찾아봐야겠다",
          "original_form": "찾아봐야겠다",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004232.1.18",
          "form": "완전 다른건데",
          "original_form": "완전 다른건데",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004232.1.19",
          "form": "하루 일당 받아서 사는 여자가",
          "original_form": "하루 일당 받아서 사는 여자가",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004232.1.20",
          "form": "자기가 좋아하는 것들을 포기하기 싫어서 집을 포기하고 떠도는 얘기야",
          "original_form": "자기가 좋아하는 것들을 포기하기 싫어서 집을 포기하고 떠도는 얘기야",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004232.1.21",
          "form": "아하 한국영화인거지?",
          "original_form": "아하 한국영화인거지?",
          "speaker_id": "2",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004232.1.22",
          "form": "응응응",
          "original_form": "응응응",
          "speaker_id": "1",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004232.1.23",
          "form": "이솜 배우가 주연으로 출연했어",
          "original_form": "이솜 배우가 주연으로 출연했어",
          "speaker_id": "1",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004232.1.24",
          "form": "오케 한번찾아봐야겠다",
          "original_form": "오케 한번찾아봐야겠다",
          "speaker_id": "2",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004232.1.25",
          "form": "어 왠지 포스터본것같아",
          "original_form": "어 왠지 포스터본것같아",
          "speaker_id": "2",
          "time": "20210821 15:56"
        }
      ]
    }
  ]
