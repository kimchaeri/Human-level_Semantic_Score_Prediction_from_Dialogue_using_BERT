      "utterance": [
        {
          "id": "MDRW2100042330.1.1",
          "form": "별미당 약과는 안나오네요",
          "original_form": "별미당 약과는 안나오네요",
          "speaker_id": "2",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042330.1.2",
          "form": "아 별미담ㅋㅋㅋㅋㅋ",
          "original_form": "아 별미담ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042330.1.3",
          "form": "앗 ㅋㅋㅋㅋ",
          "original_form": "앗 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042330.1.4",
          "form": "담이네욬ㅋㅋㅋㅋㅋ",
          "original_form": "담이네욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042330.1.5",
          "form": "당인지 담인지 하여튼 검색해보니까 바로 나오네여!!ㅋㅋㅋ",
          "original_form": "당인지 담인지 하여튼 검색해보니까 바로 나오네여!!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042330.1.6",
          "form": "ㅎㅎㅎ네 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅎㅎㅎ네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042330.1.7",
          "form": "약과좋아하는데 딱 내입맛에 맞는건 찾기힘들더라구요",
          "original_form": "약과좋아하는데 딱 내입맛에 맞는건 찾기힘들더라구요",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042330.1.8",
          "form": "그래서 이것저것 이번기회레먹어볼까싶어서 ㅎㅎ",
          "original_form": "그래서 이것저것 이번기회레먹어볼까싶어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042330.1.9",
          "form": "맛있어보여요",
          "original_form": "맛있어보여요",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042330.1.10",
          "form": "맛있는거 많이 아시네요!!",
          "original_form": "맛있는거 많이 아시네요!!",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042330.1.11",
          "form": "저도 이번에 시켜볼까봐요ㅋㅋㅋㅋㅋ",
          "original_form": "저도 이번에 시켜볼까봐요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042330.1.12",
          "form": "그 중 먹어보고 추천한게 하나두 없는걸요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그 중 먹어보고 추천한게 하나두 없는걸요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042330.1.13",
          "form": "지금 name1님한테 자극받았어욬ㅋㅋㅋㅋ",
          "original_form": "지금 &name1&님한테 자극받았어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042330.1.14",
          "form": "입맛에 맞는 간식을 찾겠다는 마음!!!ㅋㅋㅋㅋ",
          "original_form": "입맛에 맞는 간식을 찾겠다는 마음!!!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042330.1.15",
          "form": "제가 입이 넘 짧아서 ㅠ",
          "original_form": "제가 입이 넘 짧아서 ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042330.1.16",
          "form": "맛있ㄴ느거만 먹고싶다보니 ㅎㅎㅎ",
          "original_form": "맛있ㄴ느거만 먹고싶다보니 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042330.1.17",
          "form": "라면 반개정도 드시는 정도인가요??",
          "original_form": "라면 반개정도 드시는 정도인가요??",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042330.1.18",
          "form": "음 그건아니에요 ㅋㅋ 하나는 먹긴한데",
          "original_form": "음 그건아니에요 ㅋㅋ 하나는 먹긴한데",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042330.1.19",
          "form": "질리는정도??",
          "original_form": "질리는정도??",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042330.1.20",
          "form": "그것고 마지막엔 넘 배부르지만 버리기아까워서먹는다",
          "original_form": "그것고 마지막엔 넘 배부르지만 버리기아까워서먹는다",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042330.1.21",
          "form": "하는정도?",
          "original_form": "하는정도?",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042330.1.22",
          "form": "저도 그래여 ㅋㅋㅋ",
          "original_form": "저도 그래여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042330.1.23",
          "form": "근데그것도넘배부르몬",
          "original_form": "근데그것도넘배부르몬",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042330.1.24",
          "form": "채해서",
          "original_form": "채해서",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042330.1.25",
          "form": "버리기도해요 ㅠ",
          "original_form": "버리기도해요 ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042330.1.26",
          "form": "아이크야",
          "original_form": "아이크야",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042330.1.27",
          "form": "맛있는거만 먹고 살고 싶어요 어차피 맛없는건 많이 사도",
          "original_form": "맛있는거만 먹고 살고 싶어요 어차피 맛없는건 많이 사도",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042330.1.28",
          "form": "다 안먹으니까 그게 그거??ㅋㅋㅋㅋ",
          "original_form": "다 안먹으니까 그게 그거??ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042330.1.29",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042330.1.30",
          "form": "결국 사서 안먹으면",
          "original_form": "결국 사서 안먹으면",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042330.1.31",
          "form": "버려야하니까 ㅠㅠ",
          "original_form": "버려야하니까 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042330.1.32",
          "form": "전 주로 식탁에버립니다",
          "original_form": "전 주로 식탁에버립니다",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042330.1.33",
          "form": "과일은 특히 좀 그래여",
          "original_form": "과일은 특히 좀 그래여",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042330.1.34",
          "form": "그럼 누군가 먹고 순삭했더라구요",
          "original_form": "그럼 누군가 먹고 순삭했더라구요",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042330.1.35",
