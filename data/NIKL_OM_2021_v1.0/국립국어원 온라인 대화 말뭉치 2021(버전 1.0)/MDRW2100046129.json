      "utterance": [
        {
          "id": "MDRW2100046129.1.1",
          "form": "ㅋㅋㅋㅋㅋ교수님이 한명이라고요????",
          "original_form": "ㅋㅋㅋㅋㅋ교수님이 한명이라고요????",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046129.1.2",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046129.1.3",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046129.1.4",
          "form": "신설된 과라서",
          "original_form": "신설된 과라서",
          "speaker_id": "1",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046129.1.5",
          "form": "그러면 전공수업 다 한분이하세요?",
          "original_form": "그러면 전공수업 다 한분이하세요?",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046129.1.6",
          "form": "넼ㅋㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046129.1.7",
          "form": "전공 3갠데",
          "original_form": "전공 3갠데",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100046129.1.8",
          "form": "3개다 같은 교수님",
          "original_form": "3개다 같은 교수님",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100046129.1.9",
          "form": "아그러면 잘보여야겠네요ㅋㅋㅋㅋㅋ",
          "original_form": "아그러면 잘보여야겠네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100046129.1.10",
          "form": "혹시 성적에 영향줄수도 있잖아요",
          "original_form": "혹시 성적에 영향줄수도 있잖아요",
          "speaker_id": "2",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100046129.1.11",
          "form": "저는 그나마 다행인게",
          "original_form": "저는 그나마 다행인게",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100046129.1.12",
          "form": "하나는 전선이라 안들어요",
          "original_form": "하나는 전선이라 안들어요",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100046129.1.13",
          "form": "두번만 마주쳐요",
          "original_form": "두번만 마주쳐요",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100046129.1.14",
          "form": "ㅋㅋㅋㅋ그거나 그거나인걸욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ그거나 그거나인걸욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046129.1.15",
          "form": "그러면 그분이 학과장이에요?",
          "original_form": "그러면 그분이 학과장이에요?",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046129.1.16",
          "form": "그럴걸요?",
          "original_form": "그럴걸요?",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046129.1.17",
          "form": "혼자시라...",
          "original_form": "혼자시라...",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046129.1.18",
          "form": "아니..언제 신설된과길래",
          "original_form": "아니..언제 신설된과길래",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046129.1.19",
          "form": "한명이지",
          "original_form": "한명이지",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046129.1.20",
          "form": "제가 알기로",
          "original_form": "제가 알기로",
          "speaker_id": "1",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100046129.1.21",
          "form": "작년에 생긴과예요",
          "original_form": "작년에 생긴과예요",
          "speaker_id": "1",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100046129.1.22",
          "form": "그래서 그렇구나~",
          "original_form": "그래서 그렇구나~",
          "speaker_id": "2",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100046129.1.23",
          "form": "저희는 10명 넘었어요",
          "original_form": "저희는 10명 넘었어요",
          "speaker_id": "2",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100046129.1.24",
          "form": "대신 다 은퇴하실나이대..ㅋㅋ",
          "original_form": "대신 다 은퇴하실나이대..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100046129.1.25",
          "form": "ㅋㅋㅋㅋ저희 교수님은",
          "original_form": "ㅋㅋㅋㅋ저희 교수님은",
          "speaker_id": "1",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100046129.1.26",
          "form": "젊으세요",
          "original_form": "젊으세요",
          "speaker_id": "1",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100046129.1.27",
          "form": "그것도 부럽다",
          "original_form": "그것도 부럽다",
          "speaker_id": "2",
          "time": "20210908 10:40"
        }
      ]
