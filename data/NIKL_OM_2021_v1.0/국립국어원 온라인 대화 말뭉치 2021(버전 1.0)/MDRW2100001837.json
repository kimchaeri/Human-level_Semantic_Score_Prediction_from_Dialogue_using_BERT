      "utterance": [
        {
          "id": "MDRW2100001837.1.1",
          "form": "혹시 반려동물 키우시나요?",
          "original_form": "혹시 반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100001837.1.2",
          "form": "네 전 키우고 있어요",
          "original_form": "네 전 키우고 있어요",
          "speaker_id": "2",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100001837.1.3",
          "form": "우와 진짜 부러워요ㅠㅠ자랑해주세요",
          "original_form": "우와 진짜 부러워요ㅠㅠ자랑해주세요",
          "speaker_id": "1",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100001837.1.4",
          "form": "전 진짜 키우고 싶은데 가족이 알레르기가 심해요",
          "original_form": "전 진짜 키우고 싶은데 가족이 알레르기가 심해요",
          "speaker_id": "1",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100001837.1.5",
          "form": "강아지 키우고 있는데",
          "original_form": "강아지 키우고 있는데",
          "speaker_id": "2",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100001837.1.6",
          "form": "원래 동생이 키우는 개예요",
          "original_form": "원래 동생이 키우는 개예요",
          "speaker_id": "2",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100001837.1.7",
          "form": "본가에 놀러오면 귀여워요",
          "original_form": "본가에 놀러오면 귀여워요",
          "speaker_id": "2",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100001837.1.8",
          "form": "진짜 귀여울 거 같아요",
          "original_form": "진짜 귀여울 거 같아요",
          "speaker_id": "1",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001837.1.9",
          "form": "어렸을 땐 강아지를 키웠었는데 주택에 살았어서 가능했거든요",
          "original_form": "어렸을 땐 강아지를 키웠었는데 주택에 살았어서 가능했거든요",
          "speaker_id": "1",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001837.1.10",
          "form": "갈색 푸들 키워요",
          "original_form": "갈색 푸들 키워요",
          "speaker_id": "2",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001837.1.11",
          "form": "똑똑한데 말을 잘 안 들어요",
          "original_form": "똑똑한데 말을 잘 안 들어요",
          "speaker_id": "2",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001837.1.12",
          "form": "자기도 나이 들었다는 건지",
          "original_form": "자기도 나이 들었다는 건지",
          "speaker_id": "2",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001837.1.13",
          "form": "헉 너무 귀여워요 푸들들이 진짜 똑똑하다면서요",
          "original_form": "헉 너무 귀여워요 푸들들이 진짜 똑똑하다면서요",
          "speaker_id": "1",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001837.1.14",
          "form": "제 친구도 갈색푸들 두마리 키우는데 진짜 귀엽고 똑똑했어요",
          "original_form": "제 친구도 갈색푸들 두마리 키우는데 진짜 귀엽고 똑똑했어요",
          "speaker_id": "1",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001837.1.15",
          "form": "무엇을 가르쳐주면 곧 잘해요",
          "original_form": "무엇을 가르쳐주면 곧 잘해요",
          "speaker_id": "2",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100001837.1.16",
          "form": "동물들은 눈빛이 참 이뻐요",
          "original_form": "동물들은 눈빛이 참 이뻐요",
          "speaker_id": "2",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100001837.1.17",
          "form": "그쵸 가끔 보면 어쩜 이렇게 올곧게 바라볼까 싶어요",
          "original_form": "그쵸 가끔 보면 어쩜 이렇게 올곧게 바라볼까 싶어요",
          "speaker_id": "1",
          "time": "20210819 21:43"
        },
        {
          "id": "MDRW2100001837.1.18",
          "form": "친구네 푸들 보러가면 계속 쓰다듬으라고 절 혼내요ㅋㅋㅋ",
          "original_form": "친구네 푸들 보러가면 계속 쓰다듬으라고 절 혼내요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:43"
        },
        {
          "id": "MDRW2100001837.1.19",
          "form": "주인 보는 눈빛이 있어요",
          "original_form": "주인 보는 눈빛이 있어요",
          "speaker_id": "2",
          "time": "20210819 21:43"
        },
        {
          "id": "MDRW2100001837.1.20",
          "form": "저는 주인은 아니지만",
          "original_form": "저는 주인은 아니지만",
          "speaker_id": "2",
          "time": "20210819 21:43"
        },
        {
          "id": "MDRW2100001837.1.21",
          "form": "자기 친구처럼 아는듯 해요",
          "original_form": "자기 친구처럼 아는듯 해요",
          "speaker_id": "2",
          "time": "20210819 21:44"
        },
        {
          "id": "MDRW2100001837.1.22",
          "form": "귀엽겠다",
          "original_form": "귀엽겠다",
          "speaker_id": "2",
          "time": "20210819 21:44"
        },
        {
          "id": "MDRW2100001837.1.23",
          "form": "우리집 개는 간식 달라고 발을 동동 구르던데",
          "original_form": "우리집 개는 간식 달라고 발을 동동 구르던데",
          "speaker_id": "2",
          "time": "20210819 21:44"
        },
        {
          "id": "MDRW2100001837.1.24",
          "form": "어우 벌써 귀엽다",
          "original_form": "어우 벌써 귀엽다",
          "speaker_id": "1",
          "time": "20210819 21:46"
        },
        {
          "id": "MDRW2100001837.1.25",
          "form": "간식이나 산책 관련된 말 알아듣는것도 너무 똑똑하고 귀여워요",
          "original_form": "간식이나 산책 관련된 말 알아듣는것도 너무 똑똑하고 귀여워요",
          "speaker_id": "1",
          "time": "20210819 21:46"
        },
        {
          "id": "MDRW2100001837.1.26",
          "form": "와 진짜",
          "original_form": "와 진짜",
          "speaker_id": "2",
          "time": "20210819 21:48"
        },
        {
          "id": "MDRW2100001837.1.27",
          "form": "산책 정말 좋아해요",
          "original_form": "산책 정말 좋아해요",
          "speaker_id": "2",
          "time": "20210819 21:48"
        },
        {
          "id": "MDRW2100001837.1.28",
          "form": "신난 궁둥이가 흔들흔들 귀엽거든요",
          "original_form": "신난 궁둥이가 흔들흔들 귀엽거든요",
          "speaker_id": "2",
          "time": "20210819 21:49"
        },
        {
          "id": "MDRW2100001837.1.29",
          "form": "와 저 진짜 강아지 산책 한 번 시켜봤는데",
          "original_form": "와 저 진짜 강아지 산책 한 번 시켜봤는데",
          "speaker_id": "1",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001837.1.30",
          "form": "그때 웰시코기여서 힘이 엄청 세서 제가 끌려다니는데도",
          "original_form": "그때 웰시코기여서 힘이 엄청 세서 제가 끌려다니는데도",
          "speaker_id": "1",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001837.1.31",
          "form": "행복했어요.. 신난 궁둥이를 보며..",
          "original_form": "행복했어요.. 신난 궁둥이를 보며..",
          "speaker_id": "1",
          "time": "20210819 21:53"
        }
      ]
