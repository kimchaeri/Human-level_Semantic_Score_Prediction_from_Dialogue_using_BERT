      "utterance": [
        {
          "id": "MDRW2100026780.1.1",
          "form": "아 전전전 여친과의 이별이야기죠",
          "original_form": "아 전전전 여친과의 이별이야기죠",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026780.1.2",
          "form": "잘살아라!",
          "original_form": "잘살아라!",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026780.1.3",
          "form": "들리냐!",
          "original_form": "들리냐!",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026780.1.4",
          "form": "? 전이 몇번 붙는 거죠?",
          "original_form": "? 전이 몇번 붙는 거죠?",
          "speaker_id": "1",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026780.1.5",
          "form": "많이 사귄척해봤습니다",
          "original_form": "많이 사귄척해봤습니다",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026780.1.6",
          "form": "ㅎㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026780.1.7",
          "form": "전여친은 이과 죽으란 말 안했나요ㅎㅎ",
          "original_form": "전여친은 이과 죽으란 말 안했나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026780.1.8",
          "form": "전여친은 제 수학실력을 좋아햇읍니다",
          "original_form": "전여친은 제 수학실력을 좋아햇읍니다",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026780.1.9",
          "form": "오 마갓",
          "original_form": "오 마갓",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026780.1.10",
          "form": "전여친 사람인가욪?",
          "original_form": "전여친 사람인가욪?",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026780.1.11",
          "form": "돌하르방 아니쥬?",
          "original_form": "돌하르방 아니쥬?",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026780.1.12",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026780.1.13",
          "form": "문제풀어주니 좋아하더라구요",
          "original_form": "문제풀어주니 좋아하더라구요",
          "speaker_id": "2",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026780.1.14",
          "form": "ㅎㅎ  사람이 아닌게 맞군요ㅎ",
          "original_form": "ㅎㅎ  사람이 아닌게 맞군요ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026780.1.15",
          "form": "여신이엇죠",
          "original_form": "여신이엇죠",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026780.1.16",
          "form": "제주서 헤어졌남요?",
          "original_form": "제주서 헤어졌남요?",
          "speaker_id": "1",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026780.1.17",
          "form": "제주돈ㅇㄴ",
          "original_form": "제주돈ㅇㄴ",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026780.1.18",
          "form": "갔다와서 ㅋㅋ",
          "original_form": "갔다와서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026780.1.19",
          "form": "성산일출봉 안올라간다자나요",
          "original_form": "성산일출봉 안올라간다자나요",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026780.1.20",
          "form": "부인은 천사고 전여친은 여신ㅎㅎㅎㅎ",
          "original_form": "부인은 천사고 전여친은 여신ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026780.1.21",
          "form": "?? 뭐가 문제쥬?",
          "original_form": "?? 뭐가 문제쥬?",
          "speaker_id": "1",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026780.1.22",
          "form": "이세상에 천사인 아내는 없어요 후하",
          "original_form": "이세상에 천사인 아내는 없어요 후하",
          "speaker_id": "2",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026780.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026780.1.24",
          "form": "님도 결혼하면 내재된 본성이 드러날 겁니다",
          "original_form": "님도 결혼하면 내재된 본성이 드러날 겁니다",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026780.1.25",
          "form": "아 제주도",
          "original_form": "아 제주도",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026780.1.26",
          "form": "그래도 좋네요",
          "original_form": "그래도 좋네요",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026780.1.27",
          "form": "연어솥밥만 만들어준다면",
          "original_form": "연어솥밥만 만들어준다면",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026780.1.28",
          "form": "영혼이라도 팔겠어욧!",
          "original_form": "영혼이라도 팔겠어욧!",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026780.1.29",
          "form": "직접하심이..",
          "original_form": "직접하심이..",
          "speaker_id": "2",
          "time": "20210902 00:08"
        }
      ]
    }
