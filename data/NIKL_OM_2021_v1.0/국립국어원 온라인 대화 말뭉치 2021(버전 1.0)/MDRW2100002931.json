      "utterance": [
        {
          "id": "MDRW2100002931.1.1",
          "form": "주변에 군대가서 살찐 친구 있니?",
          "original_form": "주변에 군대가서 살찐 친구 있니?",
          "speaker_id": "1",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002931.1.2",
          "form": "우리 오빠를 말하는구나.",
          "original_form": "우리 오빠를 말하는구나.",
          "speaker_id": "2",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002931.1.3",
          "form": "원래 삐쩍 말랐는데 엄마가 다행이라고 생각했어.",
          "original_form": "원래 삐쩍 말랐는데 엄마가 다행이라고 생각했어.",
          "speaker_id": "2",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002931.1.4",
          "form": "아니ㅠㅠ 그런 의도는 아니었어",
          "original_form": "아니ㅠㅠ 그런 의도는 아니었어",
          "speaker_id": "1",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002931.1.5",
          "form": "역시 좋은 식단과 피엑스의 조합으로 군대는 사람을 살찌게 하는구나.",
          "original_form": "역시 좋은 식단과 피엑스의 조합으로 군대는 사람을 살찌게 하는구나.",
          "speaker_id": "2",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002931.1.6",
          "form": "내친구들도 다 살쪄서 나오더라",
          "original_form": "내친구들도 다 살쪄서 나오더라",
          "speaker_id": "1",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002931.1.7",
          "form": "옥택연 봤니?",
          "original_form": "옥택연 봤니?",
          "speaker_id": "2",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002931.1.8",
          "form": "어떤 음식이 그렇게 맛있는걸까",
          "original_form": "어떤 음식이 그렇게 맛있는걸까",
          "speaker_id": "1",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002931.1.9",
          "form": "아 봤어",
          "original_form": "아 봤어",
          "speaker_id": "1",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002931.1.10",
          "form": "군대에서 완전 근육도 퉁퉁 되었더라.",
          "original_form": "군대에서 완전 근육도 퉁퉁 되었더라.",
          "speaker_id": "2",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002931.1.11",
          "form": "신기해",
          "original_form": "신기해",
          "speaker_id": "2",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002931.1.12",
          "form": "나는 꽤 멋있는 사진을 봤었는데",
          "original_form": "나는 꽤 멋있는 사진을 봤었는데",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002931.1.13",
          "form": "그 이후로 살이 더 찐걸까",
          "original_form": "그 이후로 살이 더 찐걸까",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002931.1.14",
          "form": "맞아 왜냐면 옥택연이니까",
          "original_form": "맞아 왜냐면 옥택연이니까",
          "speaker_id": "2",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002931.1.15",
          "form": "옥택연은 근육도 쪄서 멋있더라",
          "original_form": "옥택연은 근육도 쪄서 멋있더라",
          "speaker_id": "2",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002931.1.16",
          "form": "맞아 좀 미군 느낌도 나고",
          "original_form": "맞아 좀 미군 느낌도 나고",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002931.1.17",
          "form": "멋진 연예인의 군대는 그런건가봐",
          "original_form": "멋진 연예인의 군대는 그런건가봐",
          "speaker_id": "2",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002931.1.18",
          "form": "왜 우리학교 남자들은 안그럴까?",
          "original_form": "왜 우리학교 남자들은 안그럴까?",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002931.1.19",
          "form": "근데 악뮤의 찬혁이를 보면",
          "original_form": "근데 악뮤의 찬혁이를 보면",
          "speaker_id": "1",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002931.1.20",
          "form": "모든 연예인이 멋있어지는건 아닌가봐",
          "original_form": "모든 연예인이 멋있어지는건 아닌가봐",
          "speaker_id": "1",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002931.1.21",
          "form": "찬혁이는 얼굴에 각오가 가득하더라",
          "original_form": "찬혁이는 얼굴에 각오가 가득하더라",
          "speaker_id": "2",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002931.1.22",
          "form": "뭔가 의지가 가득하더라.",
          "original_form": "뭔가 의지가 가득하더라.",
          "speaker_id": "2",
          "time": "20210820 16:47"
        }
      ]
