        },
        {
          "id": "MDRW2100041705.1.4",
          "form": "벌써 3시다",
          "original_form": "벌써 3시다",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100041705.1.5",
          "form": "그러니깐 ㅠ",
          "original_form": "그러니깐 ㅠ",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100041705.1.6",
          "form": "곧 우리 아이도 오겠지 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "곧 우리 아이도 오겠지 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100041705.1.7",
          "form": "헬....ㅋㅋㅋㅋ",
          "original_form": "헬....ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100041705.1.8",
          "form": "또살짝 덥노",
          "original_form": "또살짝 덥노",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100041705.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ나두 선풍기켰다 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ나두 선풍기켰다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100041705.1.10",
          "form": "왜이래덥노 큰애 긴팔입고 갓는데",
          "original_form": "왜이래덥노 큰애 긴팔입고 갓는데",
          "speaker_id": "1",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100041705.1.11",
          "form": "그러니깐은 ㅋㅋ 나는 아직 반팔에 점퍼",
          "original_form": "그러니깐은 ㅋㅋ 나는 아직 반팔에 점퍼",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041705.1.12",
          "form": "나도 반팔 ㅋㅋㅋㅋ 입고 다니는데 애들은 또 달라서",
          "original_form": "나도 반팔 ㅋㅋㅋㅋ 입고 다니는데 애들은 또 달라서",
          "speaker_id": "1",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041705.1.13",
          "form": "ㅋㅋㅋ맞어 긴팔입는애들 많더라 ㅋㅋ",
          "original_form": "ㅋㅋㅋ맞어 긴팔입는애들 많더라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041705.1.14",
          "form": "애들은 잘때 긴팔입고 얇은거 ㅎㅎ 잣어",
          "original_form": "애들은 잘때 긴팔입고 얇은거 ㅎㅎ 잣어",
          "speaker_id": "1",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041705.1.15",
          "form": "맞어 맞어 ㅋㅋㅋ",
          "original_form": "맞어 맞어 ㅋㅋㅋ",
