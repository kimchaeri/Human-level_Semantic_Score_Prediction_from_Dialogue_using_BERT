      "utterance": [
        {
          "id": "MDRW2100032716.1.1",
          "form": "나폴리도우 먹어봤엉ㅅ",
          "original_form": "나폴리도우 먹어봤엉ㅅ",
          "speaker_id": "2",
          "time": "20210905 23:16"
        },
        {
          "id": "MDRW2100032716.1.2",
          "form": "전 근데 사실 막입이라 엄청 큰 차이는 잘 못느끼고",
          "original_form": "전 근데 사실 막입이라 엄청 큰 차이는 잘 못느끼고",
          "speaker_id": "2",
          "time": "20210905 23:16"
        },
        {
          "id": "MDRW2100032716.1.3",
          "form": "씬도우가 최고..",
          "original_form": "씬도우가 최고..",
          "speaker_id": "2",
          "time": "20210905 23:16"
        },
        {
          "id": "MDRW2100032716.1.4",
          "form": "얇으니까 더 맛이 잘(?) 느껴질 것 같아요",
          "original_form": "얇으니까 더 맛이 잘(?) 느껴질 것 같아요",
          "speaker_id": "1",
          "time": "20210905 23:33"
        },
        {
          "id": "MDRW2100032716.1.5",
          "form": "솔직히 피자 is 뭔들입니다..ㅠㅠㅠ 다 좋아요",
          "original_form": "솔직히 피자 is 뭔들입니다..ㅠㅠㅠ 다 좋아요",
          "speaker_id": "1",
          "time": "20210905 23:33"
        },
        {
          "id": "MDRW2100032716.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210906 08:01"
        },
        {
          "id": "MDRW2100032716.1.7",
          "form": "피자 vs 치킨 어느거 더 좋아하세요?",
          "original_form": "피자 vs 치킨 어느거 더 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 08:01"
        },
        {
          "id": "MDRW2100032716.1.8",
          "form": "저는... 치킨이 미세하게 더 좋은 것 같아요",
          "original_form": "저는... 치킨이 미세하게 더 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100032716.1.9",
          "form": "name2님은 어떠세요??",
          "original_form": "&name2&님은 어떠세요??",
          "speaker_id": "1",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100032716.1.10",
          "form": "전 ..",
          "original_form": "전 ..",
          "speaker_id": "2",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100032716.1.11",
          "form": "전 피자요..!!",
          "original_form": "전 피자요..!!",
          "speaker_id": "2",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100032716.1.12",
          "form": "오옷 피자도 너무 맛있죠ㅜㅜ",
          "original_form": "오옷 피자도 너무 맛있죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 12:46"
        },
        {
          "id": "MDRW2100032716.1.13",
          "form": "최애 피자는 어디인가요?",
          "original_form": "최애 피자는 어디인가요?",
          "speaker_id": "1",
          "time": "20210906 12:46"
        },
        {
          "id": "MDRW2100032716.1.14",
          "form": "저는 데일리로는 도미노피자가 재일 좋고",
          "original_form": "저는 데일리로는 도미노피자가 재일 좋고",
          "speaker_id": "2",
          "time": "20210906 13:05"
        },
        {
          "id": "MDRW2100032716.1.15",
          "form": "느끼한거 땡길때눈 피나치공이 제일 좋야요",
          "original_form": "느끼한거 땡길때눈 피나치공이 제일 좋야요",
          "speaker_id": "2",
          "time": "20210906 13:05"
        },
        {
          "id": "MDRW2100032716.1.16",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 13:05"
        },
        {
          "id": "MDRW2100032716.1.17",
          "form": "브랜드에 따라 나뉘는군요ㅋㅋㅋ",
          "original_form": "브랜드에 따라 나뉘는군요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:10"
        },
        {
          "id": "MDRW2100032716.1.18",
          "form": "도미노 블랙타이거 드셔보셨어요??",
          "original_form": "도미노 블랙타이거 드셔보셨어요??",
          "speaker_id": "1",
          "time": "20210906 13:10"
        },
        {
          "id": "MDRW2100032716.1.19",
          "form": "오 네.. 완전 맛있어요",
          "original_form": "오 네.. 완전 맛있어요",
          "speaker_id": "2",
          "time": "20210906 13:19"
        },
        {
          "id": "MDRW2100032716.1.20",
          "form": "근데 부동의 최애는 포테이토입니다",
          "original_form": "근데 부동의 최애는 포테이토입니다",
          "speaker_id": "2",
          "time": "20210906 13:20"
        },
        {
          "id": "MDRW2100032716.1.21",
          "form": "엌ㅋㅋㅋㅋ 새우를 이긴 감자라니",
          "original_form": "엌ㅋㅋㅋㅋ 새우를 이긴 감자라니",
          "speaker_id": "1",
          "time": "20210906 13:24"
        },
        {
          "id": "MDRW2100032716.1.22",
          "form": "저 진짜 포테이토없이 못살아요..",
          "original_form": "저 진짜 포테이토없이 못살아요..",
          "speaker_id": "2",
          "time": "20210906 13:24"
        },
        {
          "id": "MDRW2100032716.1.23",
          "form": "피자는 포테이토 최고~!",
          "original_form": "피자는 포테이토 최고~!",
          "speaker_id": "2",
          "time": "20210906 13:24"
        },
        {
          "id": "MDRW2100032716.1.24",
          "form": "스테디가 스테디인 이유는 있는 것 같아요ㅋㅋㅋㅋ",
          "original_form": "스테디가 스테디인 이유는 있는 것 같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:26"
        },
        {
          "id": "MDRW2100032716.1.25",
          "form": "도미노는 특히 포테이토가 유명하더라구요",
          "original_form": "도미노는 특히 포테이토가 유명하더라구요",
          "speaker_id": "1",
          "time": "20210906 13:26"
        },
        {
          "id": "MDRW2100032716.1.26",
          "form": "라인업 제목만 보면 엄청난데 완전 의외,,",
          "original_form": "라인업 제목만 보면 엄청난데 완전 의외,,",
          "speaker_id": "1",
          "time": "20210906 13:27"
        },
        {
          "id": "MDRW2100032716.1.27",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:28"
        },
        {
          "id": "MDRW2100032716.1.28",
          "form": "진짜 다른 메뉴 화려한데",
          "original_form": "진짜 다른 메뉴 화려한데",
          "speaker_id": "2",
          "time": "20210906 13:28"
        },
        {
          "id": "MDRW2100032716.1.29",
          "form": "포테이토로 다시 회귀할수밖에없어요",
          "original_form": "포테이토로 다시 회귀할수밖에없어요",
          "speaker_id": "2",
          "time": "20210906 13:28"
        },
        {
          "id": "MDRW2100032716.1.30",
          "form": "씬 도우로 먹어야해요",
          "original_form": "씬 도우로 먹어야해요",
          "speaker_id": "2",
