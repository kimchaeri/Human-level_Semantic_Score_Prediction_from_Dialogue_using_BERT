      "utterance": [
        {
          "id": "MDRW2100009500.1.1",
          "form": "ㅎㅎ 요리잘하세요?",
          "original_form": "ㅎㅎ 요리잘하세요?",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009500.1.2",
          "form": "그냥 간단한것만 해요ㅎ",
          "original_form": "그냥 간단한것만 해요ㅎ",
          "speaker_id": "1",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009500.1.3",
          "form": "요즘 밀키트 유행하잖아요ㅎㅎ",
          "original_form": "요즘 밀키트 유행하잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009500.1.4",
          "form": "지금 저녁안먹어서 급 배고파서 안주야 닭똥집 먹는중이요ㅎㅎ",
          "original_form": "지금 저녁안먹어서 급 배고파서 안주야 닭똥집 먹는중이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009500.1.5",
          "form": "우와ㅎㅎㅎ",
          "original_form": "우와ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009500.1.6",
          "form": "저도 먹어봤너요ㅎ",
          "original_form": "저도 먹어봤너요ㅎ",
          "speaker_id": "2",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009500.1.7",
          "form": "술도못하는데 안주라근가",
          "original_form": "술도못하는데 안주라근가",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009500.1.8",
          "form": "조금 느끼하긴한데ㅎ",
          "original_form": "조금 느끼하긴한데ㅎ",
          "speaker_id": "2",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009500.1.9",
          "form": "맥주랑 먹음 맛나겠어요",
          "original_form": "맥주랑 먹음 맛나겠어요",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009500.1.10",
          "form": "술좋아하시눈군요?",
          "original_form": "술좋아하시눈군요?",
          "speaker_id": "2",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009500.1.11",
          "form": "술좋아하시눈군요?",
          "original_form": "술좋아하시눈군요?",
          "speaker_id": "2",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009500.1.12",
          "form": "혼자사시는데 안주야ㅎㅎ 요즘은 한신포차도 배달이 되요ㅎㅎ 저 어제 시켜먹어봤어요ㅎ",
          "original_form": "혼자사시는데 안주야ㅎㅎ 요즘은 한신포차도 배달이 되요ㅎㅎ 저 어제 시켜먹어봤어요ㅎ",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009500.1.13",
          "form": "술은 못해요ㅋ",
          "original_form": "술은 못해요ㅋ",
          "speaker_id": "1",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009500.1.14",
          "form": "그래서 데미소다 탄산이랑   먹는중",
          "original_form": "그래서 데미소다 탄산이랑   먹는중",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009500.1.15",
          "form": "ㅎㅎ 술못하시구나ㅎㅎ",
          "original_form": "ㅎㅎ 술못하시구나ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009500.1.16",
          "form": "탄산도 좋죠ㅎ",
          "original_form": "탄산도 좋죠ㅎ",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009500.1.17",
          "form": "배달음식은 뭐좋아해요?",
          "original_form": "배달음식은 뭐좋아해요?",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009500.1.18",
          "form": "전 찜닭ㅎ",
          "original_form": "전 찜닭ㅎ",
          "speaker_id": "1",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009500.1.19",
          "form": "치킨도 좋고욪",
          "original_form": "치킨도 좋고욪",
          "speaker_id": "1",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009500.1.20",
          "form": "주로  뭐시켜드세요?",
          "original_form": "주로  뭐시켜드세요?",
          "speaker_id": "1",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009500.1.21",
          "form": "ㅎㅎ 닭류를 좋아하시나보네요ㅎ 지금 먹고잇는것도 똥집이네요ㅎㅎㅎ",
          "original_form": "ㅎㅎ 닭류를 좋아하시나보네요ㅎ 지금 먹고잇는것도 똥집이네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009500.1.22",
          "form": "전 다 좋아하는데 보통 한식위주에요ㅎ 김치찌게 종류 좋아해요ㅎ",
          "original_form": "전 다 좋아하는데 보통 한식위주에요ㅎ 김치찌게 종류 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009500.1.23",
          "form": "한식도 좋아해요ㅎ",
          "original_form": "한식도 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009500.1.24",
          "form": "근데 내맘대로 못먹는다는ㅠㅠ",
          "original_form": "근데 내맘대로 못먹는다는ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009500.1.25",
          "form": "갈비탕 해장국",
          "original_form": "갈비탕 해장국",
          "speaker_id": "1",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009500.1.26",
          "form": "오징어볶음ㅋ",
          "original_form": "오징어볶음ㅋ",
          "speaker_id": "1",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009500.1.27",
          "form": "오 좋죠ㅎㅎ",
          "original_form": "오 좋죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009500.1.28",
          "form": "왜못드세요?",
          "original_form": "왜못드세요?",
          "speaker_id": "1",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009500.1.29",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009500.1.30",
          "form": "아내분이 안해주시나ㅎ",
          "original_form": "아내분이 안해주시나ㅎ",
          "speaker_id": "1",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009500.1.31",
          "form": "제가 떡볶이 좋아하긴하는데 자주 먹지는 않는데",
          "original_form": "제가 떡볶이 좋아하긴하는데 자주 먹지는 않는데",
          "speaker_id": "2",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009500.1.32",
          "form": "배달시킬때는 제 의사가 없어요ㅎㅎㅎ",
          "original_form": "배달시킬때는 제 의사가 없어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009500.1.33",
          "form": "이미 시켜놓는데... ㅎㅎ",
          "original_form": "이미 시켜놓는데... ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009500.1.34",
          "form": "아ㅋ 아내분이 주로 떡볶이를?",
          "original_form": "아ㅋ 아내분이 주로 떡볶이를?",
          "speaker_id": "1",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100009500.1.35",
          "form": "네네ㅎ 떡볶이 보쌈 족발 등등ㅎ",
          "original_form": "네네ㅎ 떡볶이 보쌈 족발 등등ㅎ",
          "speaker_id": "2",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009500.1.36",
          "form": "배떡 드셔보셨어요?",
          "original_form": "배떡 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009500.1.37",
          "form": "로제떡볶이",
          "original_form": "로제떡볶이",
          "speaker_id": "1",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009500.1.38",
          "form": "배떡?ㅎ",
          "original_form": "배떡?ㅎ",
          "speaker_id": "2",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009500.1.39",
          "form": "로제떡볶이좋죠ㅎ",
          "original_form": "로제떡볶이좋죠ㅎ",
          "speaker_id": "2",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009500.1.40",
          "form": "정말 좋은데 많이먹으면 싫어져요ㅎㅎㄹ",
          "original_form": "정말 좋은데 많이먹으면 싫어져요ㅎㅎㄹ",
          "speaker_id": "2",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009500.1.41",
          "form": "돌아가면서 드셔야죠",
          "original_form": "돌아가면서 드셔야죠",
          "speaker_id": "1",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009500.1.42",
          "form": "진짜 근뎇치킨 보통 좋아하지않아요?",
          "original_form": "진짜 근뎇치킨 보통 좋아하지않아요?",
          "speaker_id": "2",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100009500.1.43",
          "form": "치킨은 대부분좋아하죠",
          "original_form": "치킨은 대부분좋아하죠",
          "speaker_id": "1",
          "time": "20210823 00:15"
        }
      ]
    }
  ]
