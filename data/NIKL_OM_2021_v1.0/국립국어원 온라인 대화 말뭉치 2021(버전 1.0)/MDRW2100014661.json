      "utterance": [
        {
          "id": "MDRW2100014661.1.1",
          "form": "동호회같은 것도 알아봤는데",
          "original_form": "동호회같은 것도 알아봤는데",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014661.1.2",
          "form": "시국이 시국인지라..!",
          "original_form": "시국이 시국인지라..!",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014661.1.3",
          "form": "ㅋㅋㅋ저는 완전 집순이라서",
          "original_form": "ㅋㅋㅋ저는 완전 집순이라서",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014661.1.4",
          "form": "동호회같은건 상상도안해봤어요",
          "original_form": "동호회같은건 상상도안해봤어요",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014661.1.5",
          "form": "저도 집순이ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 집순이ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014661.1.6",
          "form": "퇴근과 동시에 침대랑 한몸이에요",
          "original_form": "퇴근과 동시에 침대랑 한몸이에요",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014661.1.7",
          "form": "저도 침대 너무 좋아하는데",
          "original_form": "저도 침대 너무 좋아하는데",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014661.1.8",
          "form": "이러다간 일상이 회사, 침대밖에 없을 것 같아서ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이러다간 일상이 회사, 침대밖에 없을 것 같아서ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014661.1.9",
          "form": "아ㅜㅜㅜ그것도 맞아요 징짜ㅠㅠ",
          "original_form": "아ㅜㅜㅜ그것도 맞아요 징짜ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014661.1.10",
          "form": "코시국 인간적으로 나이 1년씩 빼줘야합니다ㅜㅜ",
          "original_form": "코시국 인간적으로 나이 1년씩 빼줘야합니다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014661.1.11",
          "form": "동호회가입한다면 관심있는 분야?",
          "original_form": "동호회가입한다면 관심있는 분야?",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014661.1.12",
          "form": "생각해보신거있으세용?",
          "original_form": "생각해보신거있으세용?",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014661.1.13",
          "form": "독서모임 생각하고 있어요!",
          "original_form": "독서모임 생각하고 있어요!",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014661.1.14",
          "form": "인문학적 소양이 너무 부족해서ㅋㅋㅋㅋㅋㅋ",
          "original_form": "인문학적 소양이 너무 부족해서ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014661.1.15",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014661.1.16",
          "form": "저는 에세이 읽는거 좋아해요",
          "original_form": "저는 에세이 읽는거 좋아해요",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014661.1.17",
          "form": "쉽게 후루룩 읽히는책!",
          "original_form": "쉽게 후루룩 읽히는책!",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014661.1.18",
          "form": "독서모임 좋으네요",
          "original_form": "독서모임 좋으네요",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014661.1.19",
          "form": "후루룩 책 좋아요!!",
          "original_form": "후루룩 책 좋아요!!",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014661.1.20",
          "form": "저 최근에는",
          "original_form": "저 최근에는",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014661.1.21",
          "form": "김초엽 씨 책 읽었는데 재밌었어요",
          "original_form": "김초엽 씨 책 읽었는데 재밌었어요",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014661.1.22",
          "form": "이건 소설!",
          "original_form": "이건 소설!",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014661.1.23",
          "form": "제목은어떻게되나요?",
          "original_form": "제목은어떻게되나요?",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014661.1.24",
          "form": "저는 독서에 1도 관심 없엇다가\r\n옛날에 고래로",
          "original_form": "저는 독서에 1도 관심 없엇다가\r\n옛날에 고래로",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014661.1.25",
          "form": "입문하게됐어요",
          "original_form": "입문하게됐어요",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014661.1.26",
          "form": "우리가 빛의 속도로 갈 수 없다면 이요!",
          "original_form": "우리가 빛의 속도로 갈 수 없다면 이요!",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014661.1.27",
          "form": "SF기반 소설인데 너무 좋더라고요",
          "original_form": "SF기반 소설인데 너무 좋더라고요",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014661.1.28",
          "form": "오 꼭 읽어볼게요~! 감사해요\r\n저는 오늘 친구기다리면서\r\n알라딘 중고서적에서",
          "original_form": "오 꼭 읽어볼게요~! 감사해요\r\n저는 오늘 친구기다리면서\r\n알라딘 중고서적에서",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014661.1.29",
          "form": "허지웅씨책 샀어요",
          "original_form": "허지웅씨책 샀어요",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014661.1.30",
          "form": "개인적으로 이분 책이 잘읽히고 좋더라구요",
          "original_form": "개인적으로 이분 책이 잘읽히고 좋더라구요",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014661.1.31",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "1",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014661.1.32",
          "form": "저도 찾아볼게요!",
          "original_form": "저도 찾아볼게요!",
          "speaker_id": "1",
          "time": "20210825 20:26"
        }
      ]
