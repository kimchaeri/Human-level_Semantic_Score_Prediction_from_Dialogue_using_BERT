      "utterance": [
        {
          "id": "MDRW2100003726.1.1",
          "form": "혼자가 최고에요",
          "original_form": "혼자가 최고에요",
          "speaker_id": "2",
          "time": "20210821 11:18"
        },
        {
          "id": "MDRW2100003726.1.2",
          "form": "결혼하신 언니들은 다 그얘기하시네요ㅎㅎ",
          "original_form": "결혼하신 언니들은 다 그얘기하시네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:19"
        },
        {
          "id": "MDRW2100003726.1.3",
          "form": "ㅎㅎ 결혼하기 전에 알려주는 사람이 없었어요ㅠ",
          "original_form": "ㅎㅎ 결혼하기 전에 알려주는 사람이 없었어요ㅠ",
          "speaker_id": "2",
          "time": "20210821 11:20"
        },
        {
          "id": "MDRW2100003726.1.4",
          "form": "그래서 다들 하나같이 입모아서 얘기하나봐요",
          "original_form": "그래서 다들 하나같이 입모아서 얘기하나봐요",
          "speaker_id": "1",
          "time": "20210821 11:20"
        },
        {
          "id": "MDRW2100003726.1.5",
          "form": "결혼하지마라/연애 많이해보고 결혼해라",
          "original_form": "결혼하지마라/연애 많이해보고 결혼해라",
          "speaker_id": "1",
          "time": "20210821 11:20"
        },
        {
          "id": "MDRW2100003726.1.6",
          "form": "여자에게 결혼은 정말 인생의 무덤인것 같아요",
          "original_form": "여자에게 결혼은 정말 인생의 무덤인것 같아요",
          "speaker_id": "2",
          "time": "20210821 11:21"
        },
        {
          "id": "MDRW2100003726.1.7",
          "form": "많이 후회되세요?ㅜㅜ",
          "original_form": "많이 후회되세요?ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 11:22"
        },
        {
          "id": "MDRW2100003726.1.8",
          "form": "네ㅠㅠ 되돌리고 싶어오 ㅎㅎ",
          "original_form": "네ㅠㅠ 되돌리고 싶어오 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:22"
        },
        {
          "id": "MDRW2100003726.1.9",
          "form": "그정도세요? 아이고...",
          "original_form": "그정도세요? 아이고...",
          "speaker_id": "1",
          "time": "20210821 11:23"
        },
        {
          "id": "MDRW2100003726.1.10",
          "form": "실례지만 결혼 몇년차세요?",
          "original_form": "실례지만 결혼 몇년차세요?",
          "speaker_id": "1",
          "time": "20210821 11:23"
        },
        {
          "id": "MDRW2100003726.1.11",
          "form": "네 ㅎㅎㅎname1님도 신중히 생각하세요",
          "original_form": "네 ㅎㅎㅎ&name1&님도 신중히 생각하세요",
          "speaker_id": "2",
          "time": "20210821 11:23"
        },
        {
          "id": "MDRW2100003726.1.12",
          "form": "저는 5년차에요",
          "original_form": "저는 5년차에요",
          "speaker_id": "2",
          "time": "20210821 11:23"
        },
        {
          "id": "MDRW2100003726.1.13",
          "form": "절대 안할거예요...ㅋㅋ",
          "original_form": "절대 안할거예요...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 11:24"
        },
        {
          "id": "MDRW2100003726.1.14",
          "form": "주변에 돌싱도 많거든요",
          "original_form": "주변에 돌싱도 많거든요",
          "speaker_id": "1",
          "time": "20210821 11:24"
        },
        {
          "id": "MDRW2100003726.1.15",
          "form": "다들 갔다와보면 왜 안하는지 알겠다고 쌍심지켜고 말리던데요",
          "original_form": "다들 갔다와보면 왜 안하는지 알겠다고 쌍심지켜고 말리던데요",
          "speaker_id": "1",
          "time": "20210821 11:24"
        },
        {
          "id": "MDRW2100003726.1.16",
          "form": "네!!저도 말리고 싶어요 ㅎㅎ",
          "original_form": "네!!저도 말리고 싶어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:25"
        },
        {
          "id": "MDRW2100003726.1.17",
          "form": "결혼은 포기한지 오래라서",
          "original_form": "결혼은 포기한지 오래라서",
          "speaker_id": "1",
          "time": "20210821 11:27"
        },
        {
          "id": "MDRW2100003726.1.18",
          "form": "아예 생각없어요ㅎㅎ",
          "original_form": "아예 생각없어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:27"
        },
        {
          "id": "MDRW2100003726.1.19",
          "form": "그나마 다행이죠",
          "original_form": "그나마 다행이죠",
          "speaker_id": "1",
          "time": "20210821 11:27"
        },
        {
          "id": "MDRW2100003726.1.20",
          "form": "네~혼자여도 요즘은 충분히 행복할수 있을것 같아요",
          "original_form": "네~혼자여도 요즘은 충분히 행복할수 있을것 같아요",
          "speaker_id": "2",
          "time": "20210821 11:28"
        },
        {
          "id": "MDRW2100003726.1.21",
          "form": "가끔 결혼할까 생각들때도 있는데",
          "original_form": "가끔 결혼할까 생각들때도 있는데",
          "speaker_id": "1",
          "time": "20210821 11:29"
        },
        {
          "id": "MDRW2100003726.1.22",
          "form": "그럴때마다 주변에서 기혼인 언니들이 다 말려줘서 다행이에요",
          "original_form": "그럴때마다 주변에서 기혼인 언니들이 다 말려줘서 다행이에요",
          "speaker_id": "1",
          "time": "20210821 11:30"
        },
        {
          "id": "MDRW2100003726.1.23",
          "form": "마침 name2님도 얘기해주시고",
          "original_form": "마침 &name2&님도 얘기해주시고",
          "speaker_id": "1",
          "time": "20210821 11:30"
        },
        {
          "id": "MDRW2100003726.1.24",
          "form": "네 ㅎㅎ 넣어두세요 ㅎㅎ",
          "original_form": "네 ㅎㅎ 넣어두세요 ㅎㅎ",
