      "utterance": [
        {
          "id": "MDRW2100045479.1.1",
          "form": "저 이종석 나온 드라마 그것도 봤어요 w ㅋㅋㅋㅋ웹툰하고 결합한 드라마",
          "original_form": "저 이종석 나온 드라마 그것도 봤어요 w ㅋㅋㅋㅋ웹툰하고 결합한 드라마",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100045479.1.2",
          "form": "더블유 저도 봤어요!!!!!!!!",
          "original_form": "더블유 저도 봤어요!!!!!!!!",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045479.1.3",
          "form": "재밌지않나오?",
          "original_form": "재밌지않나오?",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045479.1.4",
          "form": "마지막 회까지 보셨나요?",
          "original_form": "마지막 회까지 보셨나요?",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045479.1.5",
          "form": "네 저 다봤죠!!!",
          "original_form": "네 저 다봤죠!!!",
          "speaker_id": "1",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045479.1.6",
          "form": "신박했어요 ㅋㅋㅋㅋㅋ",
          "original_form": "신박했어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045479.1.7",
          "form": "마무리를 안좋아하는 분들도 계셔서ㅋㅋㅋㅋ",
          "original_form": "마무리를 안좋아하는 분들도 계셔서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045479.1.8",
          "form": "그 만화그리는 분 한효주 아빠요 ㅋㅋㅌㅋ 얼굴 없어 질 때는 좀 식겁했습니다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그 만화그리는 분 한효주 아빠요 ㅋㅋㅌㅋ 얼굴 없어 질 때는 좀 식겁했습니다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045479.1.9",
          "form": "그쵸ㅋㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045479.1.10",
          "form": "저도 깜놀",
          "original_form": "저도 깜놀",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045479.1.11",
          "form": "초반에는 제대로 못봤어오ㅋㅋㅋㅋ",
          "original_form": "초반에는 제대로 못봤어오ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045479.1.12",
          "form": "근데 만화에서 나온 강철은 진짜",
          "original_form": "근데 만화에서 나온 강철은 진짜",
          "speaker_id": "2",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100045479.1.13",
          "form": "멋있어요",
          "original_form": "멋있어요",
          "speaker_id": "2",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100045479.1.14",
          "form": "진짜요 ㅜㅜ 그리고 이종석이 좀 생긴것도 만화체에 잘 어울리게 생겨가지구 ㅋㅋㅋ",
          "original_form": "진짜요 ㅜㅜ 그리고 이종석이 좀 생긴것도 만화체에 잘 어울리게 생겨가지구 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045479.1.15",
          "form": "그것도 그렇고 그만화책소품도 잘만들었어요 진짜",
          "original_form": "그것도 그렇고 그만화책소품도 잘만들었어요 진짜",
          "speaker_id": "2",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045479.1.16",
          "form": "씨지도 잘구현하고..",
          "original_form": "씨지도 잘구현하고..",
          "speaker_id": "2",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045479.1.17",
          "form": "진짜요 ㅋㅋㅋㅋ그당시만해도 드라마에서 그정도 씨지 보여준 드라마가 없었던 것 같아요ㅋㅋㅋㅋ",
          "original_form": "진짜요 ㅋㅋㅋㅋ그당시만해도 드라마에서 그정도 씨지 보여준 드라마가 없었던 것 같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100045479.1.18",
          "form": "엠비씨에서 더블유를 한게 다행이에요ㅋㅋㅋㅋ",
          "original_form": "엠비씨에서 더블유를 한게 다행이에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100045479.1.19",
          "form": "이종석 한효주 합도 좋았구요",
          "original_form": "이종석 한효주 합도 좋았구요",
          "speaker_id": "2",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100045479.1.20",
          "form": "맞아요 ㅋㅋㅋㅋ둘이 얼굴도 겁나 쬐깐해서는... 길쭉길쭉하고 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ둘이 얼굴도 겁나 쬐깐해서는... 길쭉길쭉하고 ㅋㅋㅋㅋㅋ",
