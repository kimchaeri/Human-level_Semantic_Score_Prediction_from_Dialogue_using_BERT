      "utterance": [
        {
          "id": "MDRW2100018836.1.1",
          "form": "저도 단풍 구경 가고시퍼요",
          "original_form": "저도 단풍 구경 가고시퍼요",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018836.1.2",
          "form": "봄에 벚꽃구경은 하셨나요?",
          "original_form": "봄에 벚꽃구경은 하셨나요?",
          "speaker_id": "2",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018836.1.3",
          "form": "넹 그냥저냥 근처에서만요 ㅋㅋㅋ",
          "original_form": "넹 그냥저냥 근처에서만요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:43"
        },
        {
          "id": "MDRW2100018836.1.4",
          "form": "근데이번에 비때문에 넘 빨리떨어졋어서ㅠ",
          "original_form": "근데이번에 비때문에 넘 빨리떨어졋어서ㅠ",
          "speaker_id": "1",
          "time": "20210827 12:43"
        },
        {
          "id": "MDRW2100018836.1.5",
          "form": "아 맞아요 ㅠㅠ 저는 타이밍 좋게 봤네요",
          "original_form": "아 맞아요 ㅠㅠ 저는 타이밍 좋게 봤네요",
          "speaker_id": "2",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018836.1.6",
          "form": "출퇴근길에 벚꽃길이 엄청 이쁘게 폈더라구요",
          "original_form": "출퇴근길에 벚꽃길이 엄청 이쁘게 폈더라구요",
          "speaker_id": "2",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018836.1.7",
          "form": "크 ... 벚꽃여행도 가고싶어요",
          "original_form": "크 ... 벚꽃여행도 가고싶어요",
          "speaker_id": "1",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018836.1.8",
          "form": "그쵸.. 코로나 끝나면 우리 많이 다녀요",
          "original_form": "그쵸.. 코로나 끝나면 우리 많이 다녀요",
          "speaker_id": "2",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100018836.1.9",
          "form": "네네  ㅜㅡㅜ 언제쯤 괜차나질까용 ㅋㅋ",
          "original_form": "네네  ㅜㅡㅜ 언제쯤 괜차나질까용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100018836.1.10",
          "form": "그러게요 백신 맞으면 잠잠해지길 ㅠㅠ",
          "original_form": "그러게요 백신 맞으면 잠잠해지길 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100018836.1.11",
