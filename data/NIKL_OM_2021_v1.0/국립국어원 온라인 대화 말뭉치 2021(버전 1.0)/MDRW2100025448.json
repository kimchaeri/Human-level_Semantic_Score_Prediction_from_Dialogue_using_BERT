        },
        {
          "id": "MDRW2100025448.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100025448.1.3",
          "form": "요즘 자주 듣는 노래 있으세요?",
          "original_form": "요즘 자주 듣는 노래 있으세요?",
          "speaker_id": "2",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025448.1.4",
          "form": "요새 옛날 노래",
          "original_form": "요새 옛날 노래",
          "speaker_id": "1",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025448.1.5",
          "form": "많이 듣는거 같아요 ㅎㅎ",
          "original_form": "많이 듣는거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025448.1.6",
          "form": "코요테?",
          "original_form": "코요테?",
          "speaker_id": "1",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025448.1.7",
          "form": "앗ㅋㅋㅋ정말옛날이네요",
          "original_form": "앗ㅋㅋㅋ정말옛날이네요",
          "speaker_id": "2",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100025448.1.8",
          "form": "전 요즘 걸그룹노래 많이들어요",
          "original_form": "전 요즘 걸그룹노래 많이들어요",
          "speaker_id": "2",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100025448.1.9",
          "form": "여름이라 지쳐서 신나고싶어서",
          "original_form": "여름이라 지쳐서 신나고싶어서",
          "speaker_id": "2",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100025448.1.10",
          "form": "아 씨스타 이런건가요 ㅋㅋ",
          "original_form": "아 씨스타 이런건가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:41"
        },
        {
          "id": "MDRW2100025448.1.11",
          "form": "씨스타도 너무 예전인걸요",
          "original_form": "씨스타도 너무 예전인걸요",
          "speaker_id": "2",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025448.1.12",
          "form": "요즘!! 그룹이요!!",
          "original_form": "요즘!! 그룹이요!!",
          "speaker_id": "2",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025448.1.13",
          "form": "ㅋㅋㅋㅋ오마이걸 스테이씨 같은",
          "original_form": "ㅋㅋㅋㅋ오마이걸 스테이씨 같은",
          "speaker_id": "2",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025448.1.14",
          "form": "아 요즘....",
          "original_form": "아 요즘....",
          "speaker_id": "1",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025448.1.15",
          "form": "저도 알죠",
          "original_form": "저도 알죠",
          "speaker_id": "1",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025448.1.16",
          "form": "에스파 알아요",
          "original_form": "에스파 알아요",
          "speaker_id": "1",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025448.1.17",
          "form": "넥스트 레블",
          "original_form": "넥스트 레블",
          "speaker_id": "2",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025448.1.18",
          "form": "옛날노래도 좋죠 뭐ㅋㅋㅋㅋㅋ저도 자주 들어요",
          "original_form": "옛날노래도 좋죠 뭐ㅋㅋㅋㅋㅋ저도 자주 들어요",
          "speaker_id": "2",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025448.1.19",
          "form": "예능은 뭐 보세요 ㅋㅋ",
          "original_form": "예능은 뭐 보세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100025448.1.20",
          "form": "요즘 나오는건 거의다 봐요ㅋㅋㅋ",
          "original_form": "요즘 나오는건 거의다 봐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025448.1.21",
          "form": "뭐 보세요?",
          "original_form": "뭐 보세요?",
          "speaker_id": "2",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025448.1.22",
          "form": "최애는 대탈출이요ㅎㅎ",
          "original_form": "최애는 대탈출이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100025448.1.23",
          "form": "이번 시즌 1화였나",
          "original_form": "이번 시즌 1화였나",
          "speaker_id": "2",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025448.1.24",
          "form": "아한편은 되게 별로였는데",
          "original_form": "아한편은 되게 별로였는데",
          "speaker_id": "2",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025448.1.25",
          "form": "어떠셨어요?",
          "original_form": "어떠셨어요?",
          "speaker_id": "2",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025448.1.26",
          "form": "뭐 쏘쏘였는데",
          "original_form": "뭐 쏘쏘였는데",
          "speaker_id": "1",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025448.1.27",
          "form": "이번편",
          "original_form": "이번편",
          "speaker_id": "1",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025448.1.28",
          "form": "그 무의식이",
          "original_form": "그 무의식이",
          "speaker_id": "1",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025448.1.29",
          "form": "대박이던데요 ㅎㅎ",
          "original_form": "대박이던데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025448.1.30",
          "form": "맞아요!!!!",
          "original_form": "맞아요!!!!",
          "speaker_id": "2",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100025448.1.31",
          "form": "인셉션컨셉",
          "original_form": "인셉션컨셉",
          "speaker_id": "2",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100025448.1.32",
          "form": "퀄 진짜 대박인데",
          "original_form": "퀄 진짜 대박인데",
          "speaker_id": "2",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100025448.1.33",
          "form": "재밌긴했는데 좀 더 살렸으면 좋았을텐데ㅜ",
          "original_form": "재밌긴했는데 좀 더 살렸으면 좋았을텐데ㅜ",
