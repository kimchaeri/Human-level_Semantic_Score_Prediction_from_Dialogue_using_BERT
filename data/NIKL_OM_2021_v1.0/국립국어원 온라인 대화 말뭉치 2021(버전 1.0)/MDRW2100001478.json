        },
        {
          "id": "MDRW2100001478.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211016 00:49"
        },
        {
          "id": "MDRW2100001478.1.3",
          "form": "혹시 지금 자취하고 계신가요?",
          "original_form": "혹시 지금 자취하고 계신가요?",
          "speaker_id": "2",
          "time": "20211016 00:50"
        },
        {
          "id": "MDRW2100001478.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211016 00:50"
        },
        {
          "id": "MDRW2100001478.1.5",
          "form": "오 자취생이시구나",
          "original_form": "오 자취생이시구나",
          "speaker_id": "2",
          "time": "20211016 00:50"
        },
        {
          "id": "MDRW2100001478.1.6",
          "form": "출가라고 해주세요ㅋㅋㅋ",
          "original_form": "출가라고 해주세요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211016 00:50"
        },
        {
          "id": "MDRW2100001478.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜 출가죠 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜 출가죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211016 00:50"
        },
        {
          "id": "MDRW2100001478.1.8",
          "form": "내쫓김..인가요..",
          "original_form": "내쫓김..인가요..",
          "speaker_id": "2",
          "time": "20211016 00:50"
        },
        {
          "id": "MDRW2100001478.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211016 00:50"
        },
        {
          "id": "MDRW2100001478.1.10",
          "form": "네....",
          "original_form": "네....",
          "speaker_id": "1",
          "time": "20211016 00:50"
        },
        {
          "id": "MDRW2100001478.1.11",
          "form": "밥해주기 싫다고 나가라든데요",
          "original_form": "밥해주기 싫다고 나가라든데요",
          "speaker_id": "1",
          "time": "20211016 00:50"
        },
        {
          "id": "MDRW2100001478.1.12",
          "form": "저는 내쫓김이라도 좋으니.....",
          "original_form": "저는 내쫓김이라도 좋으니.....",
          "speaker_id": "2",
          "time": "20211016 00:50"
        },
        {
          "id": "MDRW2100001478.1.13",
          "form": "자취하고싶네요...",
          "original_form": "자취하고싶네요...",
          "speaker_id": "2",
          "time": "20211016 00:50"
        },
        {
          "id": "MDRW2100001478.1.14",
          "form": "아앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211016 00:50"
        },
        {
          "id": "MDRW2100001478.1.15",
          "form": "그럼 집안일 혼자 다 쳐내고 계시겠군요..",
          "original_form": "그럼 집안일 혼자 다 쳐내고 계시겠군요..",
          "speaker_id": "2",
          "time": "20211016 00:51"
        },
        {
          "id": "MDRW2100001478.1.16",
          "form": "이럴줄몰랏죠..",
          "original_form": "이럴줄몰랏죠..",
          "speaker_id": "1",
          "time": "20211016 00:51"
        },
        {
          "id": "MDRW2100001478.1.17",
          "form": "생각보다 일이 오래걸리네요",
          "original_form": "생각보다 일이 오래걸리네요",
          "speaker_id": "1",
          "time": "20211016 00:51"
        },
        {
          "id": "MDRW2100001478.1.18",
          "form": "빨래 설거지 청소 분리수거 요리",
          "original_form": "빨래 설거지 청소 분리수거 요리",
          "speaker_id": "1",
          "time": "20211016 00:51"
        },
        {
          "id": "MDRW2100001478.1.19",
          "form": "다할려니 부모님집에 살때처럼",
          "original_form": "다할려니 부모님집에 살때처럼",
          "speaker_id": "1",
          "time": "20211016 00:51"
        },
        {
          "id": "MDRW2100001478.1.20",
          "form": "하나하나 짧게짧게 안되요",
          "original_form": "하나하나 짧게짧게 안되요",
          "speaker_id": "1",
          "time": "20211016 00:52"
        },
        {
          "id": "MDRW2100001478.1.21",
          "form": "빨래랑 청소하면 주말이 다지나가요",
          "original_form": "빨래랑 청소하면 주말이 다지나가요",
          "speaker_id": "1",
          "time": "20211016 00:52"
        },
        {
          "id": "MDRW2100001478.1.22",
          "form": "그쵸 약간",
          "original_form": "그쵸 약간",
          "speaker_id": "2",
          "time": "20211016 00:52"
        },
        {
          "id": "MDRW2100001478.1.23",
          "form": "날잡고 대청소를 해야되는 느낌...",
          "original_form": "날잡고 대청소를 해야되는 느낌...",
          "speaker_id": "2",
          "time": "20211016 00:52"
        },
        {
          "id": "MDRW2100001478.1.24",
          "form": "빨리 기술이 발전해서",
          "original_form": "빨리 기술이 발전해서",
          "speaker_id": "2",
          "time": "20211016 00:52"
        },
        {
          "id": "MDRW2100001478.1.25",
          "form": "집안일로봇이",
          "original_form": "집안일로봇이",
          "speaker_id": "2",
          "time": "20211016 00:52"
        },
        {
          "id": "MDRW2100001478.1.26",
          "form": "나와야해요",
          "original_form": "나와야해요",
          "speaker_id": "2",
          "time": "20211016 00:52"
        },
        {
          "id": "MDRW2100001478.1.27",
          "form": "주말되면 뭔가 의무적으로 해야될거같아요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "주말되면 뭔가 의무적으로 해야될거같아요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211016 00:52"
        },
        {
          "id": "MDRW2100001478.1.28",
          "form": "언능 청소기를 구매해야..",
          "original_form": "언능 청소기를 구매해야..",
          "speaker_id": "1",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001478.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저는 자취생은 아니지만.. 주말은 약간",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저는 자취생은 아니지만.. 주말은 약간",
          "speaker_id": "2",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001478.1.30",
          "form": "암묵적인",
          "original_form": "암묵적인",
          "speaker_id": "2",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001478.1.31",
          "form": "청소하는날 아닌가요",
          "original_form": "청소하는날 아닌가요",
          "speaker_id": "2",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001478.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001478.1.33",
          "form": "청소기는 솔직히 비싼거 필요없고",
          "original_form": "청소기는 솔직히 비싼거 필요없고",
          "speaker_id": "2",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001478.1.34",
          "form": "싸고 흡입력 조은게 최고.",
          "original_form": "싸고 흡입력 조은게 최고.",
          "speaker_id": "2",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001478.1.35",
          "form": "이것저것 사다보니 돈이없어서 무릎끌고댕기며 청소하네여",
          "original_form": "이것저것 사다보니 돈이없어서 무릎끌고댕기며 청소하네여",
          "speaker_id": "1",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001478.1.36",
          "form": "무선으로 사야죠ㅋㅋ",
          "original_form": "무선으로 사야죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001478.1.37",
          "form": "허어 ㅠㅠ 무릎은 소중합니다...",
          "original_form": "허어 ㅠㅠ 무릎은 소중합니다...",
          "speaker_id": "2",
          "time": "20211016 00:53"
        },
        {
          "id": "MDRW2100001478.1.38",
          "form": "좋은걸로 구매하셔서 삶의질 높아지시길..기원합니다\r\n",
          "original_form": "좋은걸로 구매하셔서 삶의질 높아지시길..기원합니다\r\n",
