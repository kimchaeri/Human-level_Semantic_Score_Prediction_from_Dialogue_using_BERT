      "utterance": [
        {
          "id": "MDRW2100022481.1.1",
          "form": "안녕하세오",
          "original_form": "안녕하세오",
          "speaker_id": "1",
          "time": "20210829 14:49"
        },
        {
          "id": "MDRW2100022481.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100022481.1.3",
          "form": "영양제 드시나요?",
          "original_form": "영양제 드시나요?",
          "speaker_id": "1",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100022481.1.4",
          "form": "막상 챙겨먹는건없어요ㅠㅠ머그려고 사두고했등데",
          "original_form": "막상 챙겨먹는건없어요ㅠㅠ머그려고 사두고했등데",
          "speaker_id": "2",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100022481.1.5",
          "form": "꾸준히 먹으려면 귀찮죠 ㅋㅋ..",
          "original_form": "꾸준히 먹으려면 귀찮죠 ㅋㅋ..",
          "speaker_id": "1",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100022481.1.6",
          "form": "맞아요ㅠ 뭐챙겨드세요?",
          "original_form": "맞아요ㅠ 뭐챙겨드세요?",
          "speaker_id": "2",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100022481.1.7",
          "form": "전 자로우 락토페린이랑  자로우 유산균 먹어요",
          "original_form": "전 자로우 락토페린이랑  자로우 유산균 먹어요",
          "speaker_id": "1",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100022481.1.8",
          "form": "장건강을 챙기시는군요~~",
          "original_form": "장건강을 챙기시는군요~~",
          "speaker_id": "2",
          "time": "20210829 15:09"
        },
        {
          "id": "MDRW2100022481.1.9",
          "form": "장이 무엇보다 더 중요하니까요 ㅋㅋ",
          "original_form": "장이 무엇보다 더 중요하니까요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:10"
        },
        {
          "id": "MDRW2100022481.1.10",
          "form": "맞아요~ 다중요하지만 장도 잘챙겨야죠ㅠㅠ",
          "original_form": "맞아요~ 다중요하지만 장도 잘챙겨야죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022481.1.11",
          "form": "그렇죠 ㅎ",
          "original_form": "그렇죠 ㅎ",
          "speaker_id": "1",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022481.1.12",
          "form": "전 분말로되있는 유산균이잇는데 먹기힘들더라구여ㅠ 가루ㅠ",
          "original_form": "전 분말로되있는 유산균이잇는데 먹기힘들더라구여ㅠ 가루ㅠ",
          "speaker_id": "2",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100022481.1.13",
          "form": "분말이면 대부분 맛있지않나요",
          "original_form": "분말이면 대부분 맛있지않나요",
          "speaker_id": "1",
          "time": "20210829 15:15"
        },
        {
          "id": "MDRW2100022481.1.14",
          "form": "전 넘기기가힘들더라구여ㅠㅠ",
          "original_form": "전 넘기기가힘들더라구여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 15:15"
        },
        {
          "id": "MDRW2100022481.1.15",
          "form": "아 ㅜㅜ",
          "original_form": "아 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 15:16"
        },
        {
          "id": "MDRW2100022481.1.16",
          "form": "알약으로 된거 나중에 봐야겠어요!",
          "original_form": "알약으로 된거 나중에 봐야겠어요!",
          "speaker_id": "2",
          "time": "20210829 15:16"
        },
        {
          "id": "MDRW2100022481.1.17",
          "form": "자로우 제품 한번 드셔보세요",
          "original_form": "자로우 제품 한번 드셔보세요",
          "speaker_id": "1",
          "time": "20210829 15:16"
        },
        {
          "id": "MDRW2100022481.1.18",
          "form": "네 한번찾아볼께요!",
          "original_form": "네 한번찾아볼께요!",
          "speaker_id": "2",
          "time": "20210829 15:17"
        }
