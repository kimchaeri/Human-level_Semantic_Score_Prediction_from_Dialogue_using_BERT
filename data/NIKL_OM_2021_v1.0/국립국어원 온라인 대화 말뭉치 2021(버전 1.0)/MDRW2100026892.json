      "utterance": [
        {
          "id": "MDRW2100026892.1.1",
          "form": "저는 무직이에요",
          "original_form": "저는 무직이에요",
          "speaker_id": "2",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100026892.1.2",
          "form": "학생도 직업",
          "original_form": "학생도 직업",
          "speaker_id": "1",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100026892.1.3",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "1",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100026892.1.4",
          "form": "저는 고3이 아니에요....",
          "original_form": "저는 고3이 아니에요....",
          "speaker_id": "2",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100026892.1.5",
          "form": "네.....그러길바래요",
          "original_form": "네.....그러길바래요",
          "speaker_id": "1",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100026892.1.6",
          "form": "고 3.5인 기분",
          "original_form": "고 3.5인 기분",
          "speaker_id": "2",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100026892.1.7",
          "form": "네....대학생쿤",
          "original_form": "네....대학생쿤",
          "speaker_id": "2",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100026892.1.8",
          "form": "근데 대학 공대로가면",
          "original_form": "근데 대학 공대로가면",
          "speaker_id": "1",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100026892.1.9",
          "form": "님은 차피 고등학교 4학년이돼요",
          "original_form": "님은 차피 고등학교 4학년이돼요",
          "speaker_id": "1",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100026892.1.10",
          "form": "그래도 재수해서 고4 되는것보단",
          "original_form": "그래도 재수해서 고4 되는것보단",
          "speaker_id": "2",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100026892.1.11",
          "form": "낫다고 생각해요",
          "original_form": "낫다고 생각해요",
          "speaker_id": "2",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100026892.1.12",
          "form": "아나 갑분싸",
          "original_form": "아나 갑분싸",
          "speaker_id": "2",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100026892.1.13",
          "form": "알았어요 name1씨",
          "original_form": "알았어요 &name1&씨",
          "speaker_id": "2",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100026892.1.14",
          "form": "뭐요??? 어케 그런말을",
          "original_form": "뭐요??? 어케 그런말을",
          "speaker_id": "1",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100026892.1.15",
          "form": "흠 제가 경험상인데",
          "original_form": "흠 제가 경험상인데",
          "speaker_id": "1",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100026892.1.16",
          "form": "알바보다 과외가 돈벌기 편하긴하지만",
          "original_form": "알바보다 과외가 돈벌기 편하긴하지만",
          "speaker_id": "1",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100026892.1.17",
          "form": "감정소모가 넘 심햐서",
          "original_form": "감정소모가 넘 심햐서",
          "speaker_id": "1",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100026892.1.18",
          "form": "알바하면서 근육 키우는것도",
          "original_form": "알바하면서 근육 키우는것도",
          "speaker_id": "1",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100026892.1.19",
          "form": "ㄱㅊ은 선택일거예요",
          "original_form": "ㄱㅊ은 선택일거예요",
          "speaker_id": "1",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100026892.1.20",
          "form": "이열...",
          "original_form": "이열...",
          "speaker_id": "2",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100026892.1.21",
          "form": "알바도 감정소모 심하지않아요?",
          "original_form": "알바도 감정소모 심하지않아요?",
          "speaker_id": "2",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100026892.1.22",
          "form": "서비스쪽으로 가면 심하고",
          "original_form": "서비스쪽으로 가면 심하고",
          "speaker_id": "1",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100026892.1.23",
          "form": "대충 일만 하는 곳으로 가면",
          "original_form": "대충 일만 하는 곳으로 가면",
          "speaker_id": "1",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100026892.1.24",
          "form": "노예처럼 일만 해요",
          "original_form": "노예처럼 일만 해요",
          "speaker_id": "1",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100026892.1.25",
          "form": "왜 멈췄지",
          "original_form": "왜 멈췄지",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100026892.1.26",
          "form": "글게영",
          "original_form": "글게영",
          "speaker_id": "1",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100026892.1.27",
          "form": "대학가면...서비스 알바는 피해야겠군용...",
          "original_form": "대학가면...서비스 알바는 피해야겠군용...",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100026892.1.28",
          "form": "저 카페알바 한벙은 해보고 싶었는데",
          "original_form": "저 카페알바 한벙은 해보고 싶었는데",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100026892.1.29",
          "form": "카페절대",
          "original_form": "카페절대",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100026892.1.30",
          "form": "하지마세요진짜루",
          "original_form": "하지마세요진짜루",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100026892.1.31",
          "form": "그런말 많이들었어요....",
          "original_form": "그런말 많이들었어요....",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100026892.1.32",
          "form": "님이 커피에 진짜환장하지않는이상",
          "original_form": "님이 커피에 진짜환장하지않는이상",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100026892.1.33",
          "form": "어딜가도 지옥을보게될거예여",
          "original_form": "어딜가도 지옥을보게될거예여",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100026892.1.34",
          "form": "편의점은요??",
          "original_form": "편의점은요??",
          "speaker_id": "2",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100026892.1.35",
          "form": "편의점 알바도 해보셨나요",
          "original_form": "편의점 알바도 해보셨나요",
          "speaker_id": "2",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100026892.1.36",
          "form": "편의점 대타로 잠깐 해봤엇는데",
          "original_form": "편의점 대타로 잠깐 해봤엇는데",
          "speaker_id": "1",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100026892.1.37",
          "form": "음... 아파트쪽 아닌 편의점은 할만햇음",
          "original_form": "음... 아파트쪽 아닌 편의점은 할만햇음",
          "speaker_id": "1",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100026892.1.38",
          "form": "근데 혼자 해야하는 일이 진짜많아서",
          "original_form": "근데 혼자 해야하는 일이 진짜많아서",
          "speaker_id": "1",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100026892.1.39",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100026892.1.40",
          "form": "보통 한명이니까요",
          "original_form": "보통 한명이니까요",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100026892.1.41",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100026892.1.42",
          "form": "직원이 보통 한명인 가게들은",
          "original_form": "직원이 보통 한명인 가게들은",
          "speaker_id": "1",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100026892.1.43",
          "form": "피하세요",
          "original_form": "피하세요",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100026892.1.44",
          "form": "절대 도망가",
          "original_form": "절대 도망가",
          "speaker_id": "2",
          "time": "20210902 01:09"
        }
      ]
    }
