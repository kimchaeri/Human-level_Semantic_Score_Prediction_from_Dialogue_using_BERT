      "utterance": [
        {
          "id": "MDRW2100007399.1.1",
          "form": "꼬꼬무에 나왔었네요 이것도",
          "original_form": "꼬꼬무에 나왔었네요 이것도",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007399.1.2",
          "form": "네 진짜 기도안차고 어이가 없더라구요",
          "original_form": "네 진짜 기도안차고 어이가 없더라구요",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007399.1.3",
          "form": "또 기억에 남는건",
          "original_form": "또 기억에 남는건",
          "speaker_id": "2",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007399.1.4",
          "form": "필리핀 연쇄 납치 살인사건이요",
          "original_form": "필리핀 연쇄 납치 살인사건이요",
          "speaker_id": "2",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007399.1.5",
          "form": "처음에 이거보고 꼬꼬무를 알게됐어요",
          "original_form": "처음에 이거보고 꼬꼬무를 알게됐어요",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007399.1.6",
          "form": "찾아봤는데 약간 사이비같은 거였나 봐요",
          "original_form": "찾아봤는데 약간 사이비같은 거였나 봐요",
          "speaker_id": "1",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007399.1.7",
          "form": "헉 그것도 찾아봐야겠어요",
          "original_form": "헉 그것도 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007399.1.8",
          "form": "정말 속 천불나요",
          "original_form": "정말 속 천불나요",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007399.1.9",
          "form": "국가가 진짜 머하는 나라인지 ㅜㅠ",
          "original_form": "국가가 진짜 머하는 나라인지 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007399.1.10",
          "form": "헐...완전 무서워요",
          "original_form": "헐...완전 무서워요",
          "speaker_id": "1",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007399.1.11",
          "form": "이런 사건도 있었구나ㅠㅠ",
          "original_form": "이런 사건도 있었구나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007399.1.12",
          "form": "밤에 보시진 마셔요 ㅎ",
          "original_form": "밤에 보시진 마셔요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007399.1.13",
          "form": "자고 일어나서 봐야겠어요",
          "original_form": "자고 일어나서 봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007399.1.14",
          "form": "저 볼게 너무 많아졌어요ㅋㅋㅋㅋ",
          "original_form": "저 볼게 너무 많아졌어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007399.1.15",
          "form": "이러고보니 제가 티비엄청 많이보느사람같네요 ㅎㅎ",
          "original_form": "이러고보니 제가 티비엄청 많이보느사람같네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007399.1.16",
          "form": "저는 보는것만봐요  ㅎㅎ",
          "original_form": "저는 보는것만봐요  ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007399.1.17",
          "form": "드라마랑 꼬꼬무 온앤오프?",
          "original_form": "드라마랑 꼬꼬무 온앤오프?",
          "speaker_id": "1",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007399.1.18",
          "form": "재밌는 프로그램이 많긴해요 ㅎㅎ",
          "original_form": "재밌는 프로그램이 많긴해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007399.1.19",
          "form": "너무 방송들을 잘맨들어서 ㅎㅎ",
          "original_form": "너무 방송들을 잘맨들어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007399.1.20",
          "form": "꼬꼬무도 파일럿 프로그램이였데요",
          "original_form": "꼬꼬무도 파일럿 프로그램이였데요",
          "speaker_id": "2",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007399.1.21",
          "form": "너무 흥행해서 시즌2까지 간 잘된케이스죠",
          "original_form": "너무 흥행해서 시즌2까지 간 잘된케이스죠",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007399.1.22",
          "form": "헐 그건 몰랐어요",
          "original_form": "헐 그건 몰랐어요",
          "speaker_id": "1",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007399.1.23",
          "form": "진짜 잘만들긴 했어요!!",
          "original_form": "진짜 잘만들긴 했어요!!",
          "speaker_id": "1",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007399.1.24",
          "form": "계속 해줬으면",
          "original_form": "계속 해줬으면",
          "speaker_id": "1",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007399.1.25",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007399.1.26",
          "form": "또 사건들 수집해서 시즌3로 나오겧죠?",
          "original_form": "또 사건들 수집해서 시즌3로 나오겧죠?",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007399.1.27",
          "form": "워낙 잘됐으니 또 나올거 같아요!",
          "original_form": "워낙 잘됐으니 또 나올거 같아요!",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007399.1.28",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007399.1.29",
          "form": "지금 혹하는사이? 그것도 꼬꼬무비슷한거같던데요",
          "original_form": "지금 혹하는사이? 그것도 꼬꼬무비슷한거같던데요",
          "speaker_id": "2",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007399.1.30",
          "form": "헉 그런 프로그램도 있군요!",
          "original_form": "헉 그런 프로그램도 있군요!",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007399.1.31",
          "form": "찾아보니까 비슷해 보여요",
          "original_form": "찾아보니까 비슷해 보여요",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007399.1.32",
          "form": "약간 벤치마킹한건가봐요",
          "original_form": "약간 벤치마킹한건가봐요",
          "speaker_id": "1",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007399.1.33",
          "form": "네 따라한 느킴이 팍팍나더라구요",
          "original_form": "네 따라한 느킴이 팍팍나더라구요",
          "speaker_id": "2",
          "time": "20210822 02:13"
        }
      ]
