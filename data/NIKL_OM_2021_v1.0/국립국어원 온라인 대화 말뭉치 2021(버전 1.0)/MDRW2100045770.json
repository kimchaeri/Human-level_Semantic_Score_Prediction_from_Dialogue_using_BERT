        },
        {
          "id": "MDRW2100045770.1.2",
          "form": "샐러드 드신다네요",
          "original_form": "샐러드 드신다네요",
          "speaker_id": "1",
          "time": "20210908 06:52"
        },
        {
          "id": "MDRW2100045770.1.3",
          "form": "생고기 드시네ㅋㅋㅋㅋ",
          "original_form": "생고기 드시네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:53"
        },
        {
          "id": "MDRW2100045770.1.4",
          "form": "요리해주고 싶네요",
          "original_form": "요리해주고 싶네요",
          "speaker_id": "1",
          "time": "20210908 06:53"
        },
        {
          "id": "MDRW2100045770.1.5",
          "form": "꿀은 아니다 ㅜㅜ",
          "original_form": "꿀은 아니다 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 06:53"
        },
        {
          "id": "MDRW2100045770.1.6",
          "form": "뭐든 칼로리를 채워야 ㅋㅋㅋ",
