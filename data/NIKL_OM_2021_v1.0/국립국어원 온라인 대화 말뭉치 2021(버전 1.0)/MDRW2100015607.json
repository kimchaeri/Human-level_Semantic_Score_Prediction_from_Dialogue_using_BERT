      "utterance": [
        {
          "id": "MDRW2100015607.1.1",
          "form": "비슷해요...ㅋㅋㅋ",
          "original_form": "비슷해요...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015607.1.2",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015607.1.3",
          "form": "8만원? 정도 했던 것 같아요",
          "original_form": "8만원? 정도 했던 것 같아요",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015607.1.4",
          "form": "아 온라인인데도 막싸진않네요",
          "original_form": "아 온라인인데도 막싸진않네요",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015607.1.5",
          "form": "근데 직접보고싶을거같아요",
          "original_form": "근데 직접보고싶을거같아요",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015607.1.6",
          "form": "저도 막 반값 하고 그럴 줄 알았는데..ㅠㅠ",
          "original_form": "저도 막 반값 하고 그럴 줄 알았는데..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015607.1.7",
          "form": "너무신기해서 ㅋㅋㅋ그러게요",
          "original_form": "너무신기해서 ㅋㅋㅋ그러게요",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015607.1.8",
          "form": "그래도 좋은 점 있어요!",
          "original_form": "그래도 좋은 점 있어요!",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015607.1.9",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015607.1.10",
          "form": "온라인이라서 온 가족이 같이 볼 수 있어요 티비에 연결해서 ㅋㅋㅋㅋ",
          "original_form": "온라인이라서 온 가족이 같이 볼 수 있어요 티비에 연결해서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015607.1.11",
          "form": "그럼 1인당 2만원 이렇게 생각할 수도 있죵",
          "original_form": "그럼 1인당 2만원 이렇게 생각할 수도 있죵",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015607.1.12",
          "form": "오 진짜 그렇게 따지면 좋은데요",
          "original_form": "오 진짜 그렇게 따지면 좋은데요",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015607.1.13",
          "form": "친구들모여서봐도 될거같아요",
          "original_form": "친구들모여서봐도 될거같아요",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015607.1.14",
          "form": "집에서",
          "original_form": "집에서",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015607.1.15",
          "form": "전 혼자 봤는데.. 가족끼리 많이 보셨더라구욤",
          "original_form": "전 혼자 봤는데.. 가족끼리 많이 보셨더라구욤",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015607.1.16",
          "form": "맞아요 친구들끼리 모여있는 분도 계셨어요",
          "original_form": "맞아요 친구들끼리 모여있는 분도 계셨어요",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015607.1.17",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015607.1.18",
          "form": "콘서트는 가본적있나요??",
          "original_form": "콘서트는 가본적있나요??",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015607.1.19",
          "form": "가수 콘서트 같은거요?",
          "original_form": "가수 콘서트 같은거요?",
          "speaker_id": "2",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015607.1.20",
          "form": "가수나 그쵸 가수나 다른? 아무콘서트요",
          "original_form": "가수나 그쵸 가수나 다른? 아무콘서트요",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015607.1.21",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
