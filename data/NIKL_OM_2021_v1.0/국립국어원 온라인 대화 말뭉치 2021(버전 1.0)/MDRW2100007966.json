      "utterance": [
        {
          "id": "MDRW2100007966.1.1",
          "form": "여긴 안 와요",
          "original_form": "여긴 안 와요",
          "speaker_id": "2",
          "time": "20210822 12:20"
        },
        {
          "id": "MDRW2100007966.1.2",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "2",
          "time": "20210822 12:20"
        },
        {
          "id": "MDRW2100007966.1.3",
          "form": "더워요",
          "original_form": "더워요",
          "speaker_id": "1",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007966.1.4",
          "form": "어젠 비 오셨나요?",
          "original_form": "어젠 비 오셨나요?",
          "speaker_id": "2",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007966.1.5",
          "form": "네 많이요",
          "original_form": "네 많이요",
          "speaker_id": "1",
          "time": "20210822 12:23"
        },
        {
          "id": "MDRW2100007966.1.6",
          "form": "그래도 비오고 좀 시원해진 거 같아요",
          "original_form": "그래도 비오고 좀 시원해진 거 같아요",
          "speaker_id": "2",
          "time": "20210822 12:23"
        },
        {
          "id": "MDRW2100007966.1.7",
          "form": "바람은 좀 시원한가요?",
          "original_form": "바람은 좀 시원한가요?",
          "speaker_id": "2",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007966.1.8",
          "form": "습해요",
          "original_form": "습해요",
          "speaker_id": "1",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007966.1.9",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007966.1.10",
          "form": "어떤 계절 가장 좋아하세요?",
          "original_form": "어떤 계절 가장 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007966.1.11",
          "form": "봄이요",
          "original_form": "봄이요",
          "speaker_id": "1",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007966.1.12",
          "form": "여름이랑 겨울 중에서는 어떤 게 좋나요?",
          "original_form": "여름이랑 겨울 중에서는 어떤 게 좋나요?",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007966.1.13",
          "form": "겨울이요",
          "original_form": "겨울이요",
          "speaker_id": "1",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007966.1.14",
          "form": "여름 뜨거워요",
          "original_form": "여름 뜨거워요",
          "speaker_id": "1",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007966.1.15",
          "form": "이번 여름 진짜 덥더라고요",
          "original_form": "이번 여름 진짜 덥더라고요",
          "speaker_id": "2",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007966.1.16",
          "form": "에어컨을 안 틀 수가 없었어요",
          "original_form": "에어컨을 안 틀 수가 없었어요",
          "speaker_id": "2",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007966.1.17",
          "form": "네 지옥불",
          "original_form": "네 지옥불",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007966.1.18",
          "form": "내년이 걱정되네요",
          "original_form": "내년이 걱정되네요",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007966.1.19",
          "form": "전 다가오는 겨울이 더",
          "original_form": "전 다가오는 겨울이 더",
          "speaker_id": "1",
          "time": "20210822 12:31"
        },
        {
          "id": "MDRW2100007966.1.20",
          "form": "별로 안 더워요",
          "original_form": "별로 안 더워요",
