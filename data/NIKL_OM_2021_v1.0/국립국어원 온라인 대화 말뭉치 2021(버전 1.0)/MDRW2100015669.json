      "utterance": [
        {
          "id": "MDRW2100015669.1.1",
          "form": "만약 내가 원하는 직업 가질 수 있다면",
          "original_form": "만약 내가 원하는 직업 가질 수 있다면",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015669.1.2",
          "form": "어떤 직업 고르시겠어요?",
          "original_form": "어떤 직업 고르시겠어요?",
          "speaker_id": "2",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015669.1.3",
          "form": "직장 스트레스는 어딜 가나 다 있는 것 같아요 ㅎㅎ",
          "original_form": "직장 스트레스는 어딜 가나 다 있는 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015669.1.4",
          "form": "그쵸 ㅋㅋ사실 저는 꿈을 잘모르겠어요 ㅠ",
          "original_form": "그쵸 ㅋㅋ사실 저는 꿈을 잘모르겠어요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015669.1.5",
          "form": "특히 사람 대하는 일은 감정노동이죠 ㅠㅠ",
          "original_form": "특히 사람 대하는 일은 감정노동이죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015669.1.6",
          "form": "그래서 딱 못고르겠는",
          "original_form": "그래서 딱 못고르겠는",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015669.1.7",
          "form": "맞아요 서비스직 진짜",
          "original_form": "맞아요 서비스직 진짜",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015669.1.8",
          "form": "알바했었는데",
          "original_form": "알바했었는데",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015669.1.9",
          "form": "어떤 알바요?",
          "original_form": "어떤 알바요?",
          "speaker_id": "2",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015669.1.10",
          "form": "너무 적성아니에요",
          "original_form": "너무 적성아니에요",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015669.1.11",
          "form": "그냥 편의점 알바 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그냥 편의점 알바 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015669.1.12",
          "form": "서비스직이라기에는 약한가",
          "original_form": "서비스직이라기에는 약한가",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015669.1.13",
          "form": "아 편의점 ~ 진상 많죠?ㅋㅋㅋ",
          "original_form": "아 편의점 ~ 진상 많죠?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015669.1.14",
          "form": "전 친구 도와서 딱 하루 대타 해봤는데 넘 재밌었어요",
          "original_form": "전 친구 도와서 딱 하루 대타 해봤는데 넘 재밌었어요",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015669.1.15",
          "form": "네 저는 대학가에서 해서",
          "original_form": "네 저는 대학가에서 해서",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015669.1.16",
          "form": "술취한사람 진짜",
          "original_form": "술취한사람 진짜",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015669.1.17",
          "form": "많이오고 ㅋㅋㅋ",
          "original_form": "많이오고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015669.1.18",
          "form": "취객들은 어디 가서도 환영받지 못한답니다 ㅜㅜ",
          "original_form": "취객들은 어디 가서도 환영받지 못한답니다 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015669.1.19",
          "form": "알바해보신거있으세요?",
          "original_form": "알바해보신거있으세요?",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015669.1.20",
          "form": "저 웨딩홀 알바 했었어요!",
          "original_form": "저 웨딩홀 알바 했었어요!",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015669.1.21",
          "form": "일 잘한다고 고속승진 했어요 ㅋㅋㅋ",
          "original_form": "일 잘한다고 고속승진 했어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015669.1.22",
          "form": "옺ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "옺ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015669.1.23",
          "form": "재밌을거같아요!!",
          "original_form": "재밌을거같아요!!",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015669.1.24",
          "form": "3개월만에 매니저 달고 알바 관리 했어요",
          "original_form": "3개월만에 매니저 달고 알바 관리 했어요",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015669.1.25",
          "form": "알바하다가 적성에 맞으면 아예 직업삼는사람도 많더라구요",
          "original_form": "알바하다가 적성에 맞으면 아예 직업삼는사람도 많더라구요",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015669.1.26",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015669.1.27",
          "form": "그직업 하셔도 될뻔했네요 ㅋㅋㅋ",
          "original_form": "그직업 하셔도 될뻔했네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015669.1.28",
          "form": "맘 같아서 쭉 하고 싶었지만.. 전 가야할 길이 있었기에 ㅜㅜ",
          "original_form": "맘 같아서 쭉 하고 싶었지만.. 전 가야할 길이 있었기에 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015669.1.29",
          "form": "가장 좋은건 웨딩홀 뷔페 먹을 수 있었던 것..",
          "original_form": "가장 좋은건 웨딩홀 뷔페 먹을 수 있었던 것..",
          "speaker_id": "2",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015669.1.30",
          "form": "근데 하루 하객 1500명 넘게 상대해보면 할많하않..",
          "original_form": "근데 하루 하객 1500명 넘게 상대해보면 할많하않..",
          "speaker_id": "2",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015669.1.31",
          "form": "와 진짜 힘들었겠다...",
          "original_form": "와 진짜 힘들었겠다...",
          "speaker_id": "1",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015669.1.32",
          "form": "웨딩홀도 진상 많나요??",
          "original_form": "웨딩홀도 진상 많나요??",
