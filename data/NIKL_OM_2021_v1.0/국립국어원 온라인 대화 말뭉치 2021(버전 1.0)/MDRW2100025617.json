      "utterance": [
        {
          "id": "MDRW2100025617.1.1",
          "form": "저도 마찬가지예요!",
          "original_form": "저도 마찬가지예요!",
          "speaker_id": "1",
          "time": "20210901 14:25"
        },
        {
          "id": "MDRW2100025617.1.2",
          "form": "ㅋㅋㅋㅋㅋ증말 반갑네여",
          "original_form": "ㅋㅋㅋㅋㅋ증말 반갑네여",
          "speaker_id": "2",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100025617.1.3",
          "form": "흐흐",
          "original_form": "흐흐",
          "speaker_id": "1",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100025617.1.4",
          "form": "아니 그리고 남자들하고 연애할때도 참 뭐랄까 항상 신뢰가 없는데",
          "original_form": "아니 그리고 남자들하고 연애할때도 참 뭐랄까 항상 신뢰가 없는데",
          "speaker_id": "2",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100025617.1.5",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100025617.1.6",
          "form": "그냥 외로워서 이어갔던거같아요",
          "original_form": "그냥 외로워서 이어갔던거같아요",
          "speaker_id": "2",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100025617.1.7",
          "form": "아 그리고 다들 너무 못생겼어요",
          "original_form": "아 그리고 다들 너무 못생겼어요",
          "speaker_id": "1",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100025617.1.8",
          "form": "맞아요 그리고 저는 하;;;",
          "original_form": "맞아요 그리고 저는 하;;;",
          "speaker_id": "1",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100025617.1.9",
          "form": "남친 있는 애들이",
          "original_form": "남친 있는 애들이",
          "speaker_id": "1",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100025617.1.10",
          "form": "저 진짜 불쌍하게 봐가지고",
          "original_form": "저 진짜 불쌍하게 봐가지고",
          "speaker_id": "1",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100025617.1.11",
          "form": "근데 안맞으면 맞춰갈 생각안하고 헤어졋어요",
          "original_form": "근데 안맞으면 맞춰갈 생각안하고 헤어졋어요",
          "speaker_id": "2",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100025617.1.12",
          "form": "아 진짜 그런 오지랖",
          "original_form": "아 진짜 그런 오지랖",
          "speaker_id": "2",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100025617.1.13",
          "form": "저 성인이었을 때 첫연애가 26살이었나 그때 했거든요",
          "original_form": "저 성인이었을 때 첫연애가 26살이었나 그때 했거든요",
          "speaker_id": "1",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100025617.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100025617.1.15",
          "form": "오 그래도 안맞춰주고 헤어질 생각을 하시다니",
          "original_form": "오 그래도 안맞춰주고 헤어질 생각을 하시다니",
          "speaker_id": "1",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100025617.1.16",
          "form": "강단 있으셔요!!!",
          "original_form": "강단 있으셔요!!!",
          "speaker_id": "1",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100025617.1.17",
          "form": "아 왜 또 나쁜 말이라지",
          "original_form": "아 왜 또 나쁜 말이라지",
          "speaker_id": "1",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100025617.1.18",
          "form": "남자 만날 때도 그게",
          "original_form": "남자 만날 때도 그게",
          "speaker_id": "1",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100025617.1.19",
          "form": "성 애적 감정이라기 보단",
          "original_form": "성 애적 감정이라기 보단",
          "speaker_id": "1",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100025617.1.20",
          "form": "정드는거 있잖아요,,",
          "original_form": "정드는거 있잖아요,,",
          "speaker_id": "1",
          "time": "20210901 14:28"
        },
        {
          "id": "MDRW2100025617.1.21",
          "form": "그게 컸어요 원래도 정이 많아섴ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그게 컸어요 원래도 정이 많아섴ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:28"
        },
        {
          "id": "MDRW2100025617.1.22",
          "form": "ㅋㅋㅋㅋ그러신거같아요",
          "original_form": "ㅋㅋㅋㅋ그러신거같아요",
          "speaker_id": "2",
          "time": "20210901 14:31"
        },
        {
          "id": "MDRW2100025617.1.23",
          "form": "name1님은 참 따뜻한 분인거같아요",
          "original_form": "&name1&님은 참 따뜻한 분인거같아요",
          "speaker_id": "2",
          "time": "20210901 14:31"
        },
        {
          "id": "MDRW2100025617.1.24",
          "form": "그럼 name1님은 한번 연애하면 오래하시나요?",
          "original_form": "그럼 &name1&님은 한번 연애하면 오래하시나요?",
          "speaker_id": "2",
          "time": "20210901 14:31"
        },
        {
          "id": "MDRW2100025617.1.25",
          "form": "한 남 이 나쁜말이래여",
          "original_form": "한 남 이 나쁜말이래여",
          "speaker_id": "2",
          "time": "20210901 14:32"
        },
        {
          "id": "MDRW2100025617.1.26",
          "form": "들이 또...",
          "original_form": "{censored}들이 또...",
          "speaker_id": "2",
          "time": "20210901 14:32"
        },
        {
          "id": "MDRW2100025617.1.27",
          "form": "그성별한테는 짤없이 헤어졌지만 여성분들한테는 못하겟어요 ㅋㅋㅋ",
          "original_form": "그성별한테는 짤없이 헤어졌지만 여성분들한테는 못하겟어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:32"
        },
        {
          "id": "MDRW2100025617.1.28",
          "form": "여성분들은 대화가 통하고 맞춰갈 가치가 있는 분들이라 계속 대화를 하게 되더라구요",
          "original_form": "여성분들은 대화가 통하고 맞춰갈 가치가 있는 분들이라 계속 대화를 하게 되더라구요",
          "speaker_id": "2",
          "time": "20210901 14:32"
        },
        {
          "id": "MDRW2100025617.1.29",
          "form": "아 그쵸그쵸",
          "original_form": "아 그쵸그쵸",
          "speaker_id": "1",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100025617.1.30",
          "form": "name2님 지금 다른 분이랑도 얘기하고 계신거죠?!",
          "original_form": "&name2&님 지금 다른 분이랑도 얘기하고 계신거죠?!",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100025617.1.31",
          "form": "저는 음 일 년 정도....?",
          "original_form": "저는 음 일 년 정도....?",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100025617.1.32",
          "form": "그쵸 한 번 만나면 오래 가는 편 같아요!",
          "original_form": "그쵸 한 번 만나면 오래 가는 편 같아요!",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100025617.1.33",
          "form": "잘 못잊어요 8ㅅ8",
          "original_form": "잘 못잊어요 8ㅅ8",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100025617.1.34",
          "form": "네네 근데 랠리가 막 잘되진 않네요 ㅋㅋㅋ 다들 현생이 있으셔서 그런지",
          "original_form": "네네 근데 랠리가 막 잘되진 않네요 ㅋㅋㅋ 다들 현생이 있으셔서 그런지",
          "speaker_id": "2",
          "time": "20210901 14:52"
        },
        {
          "id": "MDRW2100025617.1.35",
          "form": "그냥 아무나 말걸고있어요 ㅋㅋㅋㅋ",
          "original_form": "그냥 아무나 말걸고있어요 ㅋㅋㅋㅋ",
