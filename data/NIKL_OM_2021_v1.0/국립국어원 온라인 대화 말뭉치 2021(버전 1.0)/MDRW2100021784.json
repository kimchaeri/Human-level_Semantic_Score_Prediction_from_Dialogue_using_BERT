      "utterance": [
        {
          "id": "MDRW2100021784.1.1",
          "form": "혹시나 직접 입고  옷핏 보여달라고 하면",
          "original_form": "혹시나 직접 입고  옷핏 보여달라고 하면",
          "speaker_id": "1",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021784.1.2",
          "form": "차단 하세요 ㅠㅠ",
          "original_form": "차단 하세요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021784.1.3",
          "form": "세상엔 진짜 이상한 사람들이 많은 것 같아요",
          "original_form": "세상엔 진짜 이상한 사람들이 많은 것 같아요",
          "speaker_id": "2",
          "time": "20210828 22:29"
        },
        {
          "id": "MDRW2100021784.1.4",
          "form": "특히 짧은 하의나 시스루요",
          "original_form": "특히 짧은 하의나 시스루요",
          "speaker_id": "1",
          "time": "20210828 22:29"
        },
        {
          "id": "MDRW2100021784.1.5",
          "form": "남자분들이 여자인척 사진요구 하더라구요",
          "original_form": "남자분들이 여자인척 사진요구 하더라구요",
          "speaker_id": "1",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100021784.1.6",
          "form": "어후 정말 왜 그렇게까지 해야되는지...",
          "original_form": "어후 정말 왜 그렇게까지 해야되는지...",
          "speaker_id": "2",
          "time": "20210828 22:32"
        },
        {
          "id": "MDRW2100021784.1.7",
          "form": "그러게요 후 넘 이상한 사람 많아서",
          "original_form": "그러게요 후 넘 이상한 사람 많아서",
          "speaker_id": "1",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021784.1.8",
          "form": "직거래 할때도 비대면으로 !!",
          "original_form": "직거래 할때도 비대면으로 !!",
          "speaker_id": "1",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021784.1.9",
          "form": "그쵸! 혹시 하게돼도 혼자 나가면 안될것 같아요",
          "original_form": "그쵸! 혹시 하게돼도 혼자 나가면 안될것 같아요",
          "speaker_id": "2",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100021784.1.10",
          "form": "네 아니면 문고리 비대면이 좋긴해요",
          "original_form": "네 아니면 문고리 비대면이 좋긴해요",
          "speaker_id": "1",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100021784.1.11",
          "form": "그렇겠네요! 꿀팁 감사합니다ㅎㅎ",
          "original_form": "그렇겠네요! 꿀팁 감사합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 22:47"
        },
        {
          "id": "MDRW2100021784.1.12",
          "form": "요즘 득템하신거 있으세요?",
          "original_form": "요즘 득템하신거 있으세요?",
          "speaker_id": "2",
          "time": "20210828 22:47"
        },
        {
          "id": "MDRW2100021784.1.13",
          "form": "아뇨 요즘에 코로나 때문에.거래 못해봤어요",
          "original_form": "아뇨 요즘에 코로나 때문에.거래 못해봤어요",
          "speaker_id": "1",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021784.1.14",
          "form": "아 말고 쇼핑으로 사신 건 없으세요?",
          "original_form": "아 말고 쇼핑으로 사신 건 없으세요?",
          "speaker_id": "2",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021784.1.15",
          "form": "쇼핑으로 음 타미힐피거 옷이랑 가방요 ㅎㅎ",
          "original_form": "쇼핑으로 음 타미힐피거 옷이랑 가방요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100021784.1.16",
          "form": "와 좋은거 사셨네요ㅎㅎ",
          "original_form": "와 좋은거 사셨네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100021784.1.17",
          "form": "아 쇼핑몰엣 이벤트 해서 배송비만 내고 삿어요 ㄹㄹ",
          "original_form": "아 쇼핑몰엣 이벤트 해서 배송비만 내고 삿어요 ㄹㄹ",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100021784.1.18",
          "form": "와 그런 이벤트는 어디서 한대요ㅋㅋ",
          "original_form": "와 그런 이벤트는 어디서 한대요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100021784.1.19",
          "form": "당첨되신거에요?",
          "original_form": "당첨되신거에요?",
          "speaker_id": "2",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100021784.1.20",
          "form": "출석이벤트였는데 ㅎㅎ 쿠폰이랑 조합하니  배송비만 나오더라구요 ㅎㄹ",
          "original_form": "출석이벤트였는데 ㅎㅎ 쿠폰이랑 조합하니  배송비만 나오더라구요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210828 23:00"
        },
        {
          "id": "MDRW2100021784.1.21",
