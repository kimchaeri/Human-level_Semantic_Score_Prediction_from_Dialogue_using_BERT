      "utterance": [
        {
          "id": "MDRW2100013160.1.1",
          "form": "요새 점심시간마다 아아메먹는다",
          "original_form": "요새 점심시간마다 아아메먹는다",
          "speaker_id": "2",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100013160.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100013160.1.3",
          "form": "월요일은 꼭 먹어야돼 월요병때문에",
          "original_form": "월요일은 꼭 먹어야돼 월요병때문에",
          "speaker_id": "2",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100013160.1.4",
          "form": "그치 딱 아아가 들어가줘야 머리가 돌아가",
          "original_form": "그치 딱 아아가 들어가줘야 머리가 돌아가",
          "speaker_id": "1",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100013160.1.5",
          "form": "맞아.....근데 난 딱 4-5시사이까지만 마셔",
          "original_form": "맞아.....근데 난 딱 4-5시사이까지만 마셔",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013160.1.6",
          "form": "그 이루로 마시면 잠못자ㅋㅋㅋ",
          "original_form": "그 이루로 마시면 잠못자ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013160.1.7",
          "form": "요새 뿌링클먹고싶은데 교정때문에 고민중..",
          "original_form": "요새 뿌링클먹고싶은데 교정때문에 고민중..",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013160.1.8",
          "form": "나는 이젠 아침에 먹어도 잠안와...커피 좋아하는데 억울해",
          "original_form": "나는 이젠 아침에 먹어도 잠안와...커피 좋아하는데 억울해",
          "speaker_id": "1",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013160.1.9",
          "form": "교정하면 뿌링클도 안되는거?",
          "original_form": "교정하면 뿌링클도 안되는거?",
          "speaker_id": "1",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013160.1.10",
          "form": "캬라멜도 아닌데?",
          "original_form": "캬라멜도 아닌데?",
          "speaker_id": "1",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100013160.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ그 고무줄 노랗게된다는데 나는 고무줄은 안했지만 브라켓 노랗게될까봐...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그 고무줄 노랗게된다는데 나는 고무줄은 안했지만 브라켓 노랗게될까봐...",
          "speaker_id": "2",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100013160.1.12",
          "form": "카레도 안된대 그 강황이랑 치즈가루가 노랗게만드나봐",
          "original_form": "카레도 안된대 그 강황이랑 치즈가루가 노랗게만드나봐",
          "speaker_id": "2",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100013160.1.13",
          "form": "카레안되는건 알았는데",
          "original_form": "카레안되는건 알았는데",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100013160.1.14",
          "form": "뿌링클에 강황이 들어가는구나",
          "original_form": "뿌링클에 강황이 들어가는구나",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100013160.1.15",
          "form": "와우...",
          "original_form": "와우...",
          "speaker_id": "1",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013160.1.16",
          "form": "뿌링클은 치즈가루?뭐가 노랗게만든대ㅋㅋㅋㅋ",
          "original_form": "뿌링클은 치즈가루?뭐가 노랗게만든대ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013160.1.17",
          "form": "근데 나는 고무줄한건없어가지구",
          "original_form": "근데 나는 고무줄한건없어가지구",
          "speaker_id": "2",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013160.1.18",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013160.1.19",
          "form": "한번먹어볼까 고민중...브라켓 노랗게될까봐..",
          "original_form": "한번먹어볼까 고민중...브라켓 노랗게될까봐..",
          "speaker_id": "2",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013160.1.20",
          "form": "아니야 좀 참아보자",
          "original_form": "아니야 좀 참아보자",
          "speaker_id": "1",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013160.1.21",
          "form": "노랗게되면 돌이킬수없잖아",
          "original_form": "노랗게되면 돌이킬수없잖아",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100013160.1.22",
          "form": "그니까ㅋㅋㅋㅋ",
          "original_form": "그니까ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100013160.1.23",
          "form": "고무줄은 빼도되는데",
          "original_form": "고무줄은 빼도되는데",
          "speaker_id": "2",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100013160.1.24",
          "form": "브라켓은 고정장치라ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "브라켓은 고정장치라ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100013160.1.25",
          "form": "참아보자..",
          "original_form": "참아보자..",
          "speaker_id": "1",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013160.1.26",
          "form": "너 교정 언제까지 하는거야?",
          "original_form": "너 교정 언제까지 하는거야?",
          "speaker_id": "1",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013160.1.27",
          "form": "아직모르나?",
          "original_form": "아직모르나?",
          "speaker_id": "1",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013160.1.28",
          "form": "교정끝나고 다 먹어...",
          "original_form": "교정끝나고 다 먹어...",
          "speaker_id": "1",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013160.1.29",
          "form": "너 점심은 뭐먹었어?",
          "original_form": "너 점심은 뭐먹었어?",
          "speaker_id": "1",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013160.1.30",
          "form": "나 점심 제육볶음ㅋㅋㅋ",
          "original_form": "나 점심 제육볶음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:26"
        }
      ]
