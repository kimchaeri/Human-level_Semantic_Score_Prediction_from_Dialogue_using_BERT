      "utterance": [
        {
          "id": "MDRW2100003096.1.1",
          "form": "정말.... 피드백 거지같이 주면..",
          "original_form": "정말.... 피드백 거지같이 주면..",
          "speaker_id": "1",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003096.1.2",
          "form": "멘탛 바사삭이닼ㅋㅋㅋ",
          "original_form": "멘탛 바사삭이닼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003096.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003096.1.4",
          "form": "다시 작업해야하는 거예요?",
          "original_form": "다시 작업해야하는 거예요?",
          "speaker_id": "2",
          "time": "20210820 18:36"
        },
        {
          "id": "MDRW2100003096.1.5",
          "form": "재작업은 아닌데...",
          "original_form": "재작업은 아닌데...",
          "speaker_id": "1",
          "time": "20210820 18:36"
        },
        {
          "id": "MDRW2100003096.1.6",
          "form": "처음부터 끝까지 수정..",
          "original_form": "처음부터 끝까지 수정..",
          "speaker_id": "1",
          "time": "20210820 18:36"
        },
        {
          "id": "MDRW2100003096.1.7",
          "form": "이모지.. 80몇개 작업했는데",
          "original_form": "이모지.. 80몇개 작업했는데",
          "speaker_id": "1",
          "time": "20210820 18:36"
        },
        {
          "id": "MDRW2100003096.1.8",
          "form": "그거 다ㅜ수정...",
          "original_form": "그거 다ㅜ수정...",
          "speaker_id": "1",
          "time": "20210820 18:36"
        },
        {
          "id": "MDRW2100003096.1.9",
          "form": "헉.....",
          "original_form": "헉.....",
          "speaker_id": "2",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003096.1.10",
          "form": "그럼 야근해야하는거야?",
          "original_form": "그럼 야근해야하는거야?",
          "speaker_id": "2",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003096.1.11",
          "form": "추가근무?",
          "original_form": "추가근무?",
          "speaker_id": "2",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003096.1.12",
          "form": "끔찍하다",
          "original_form": "끔찍하다",
          "speaker_id": "2",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003096.1.13",
          "form": "그렇지... 벌써부터 막막",
          "original_form": "그렇지... 벌써부터 막막",
          "speaker_id": "1",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003096.1.14",
          "form": "그것도 2주안에 해야함..ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그것도 2주안에 해야함..ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003096.1.15",
          "form": ".......너무하네?",
          "original_form": ".......너무하네?",
          "speaker_id": "2",
          "time": "20210820 18:39"
        },
        {
          "id": "MDRW2100003096.1.16",
          "form": "언니오늘 추가근무 없으면 같이 밥으려고 했는데",
          "original_form": "언니오늘 추가근무 없으면 같이 밥으려고 했는데",
          "speaker_id": "2",
          "time": "20210820 18:39"
        },
        {
          "id": "MDRW2100003096.1.17",
          "form": "아쉽..",
          "original_form": "아쉽..",
          "speaker_id": "2",
          "time": "20210820 18:39"
        },
        {
          "id": "MDRW2100003096.1.18",
          "form": "ㅠㅠ너무했지...",
          "original_form": "ㅠㅠ너무했지...",
          "speaker_id": "1",
          "time": "20210820 18:39"
        },
        {
          "id": "MDRW2100003096.1.19",
          "form": "원래 데드라인이 2주 후긴 했눈데",
          "original_form": "원래 데드라인이 2주 후긴 했눈데",
          "speaker_id": "1",
          "time": "20210820 18:39"
        },
        {
          "id": "MDRW2100003096.1.20",
          "form": "이제와서 수정이라니..",
          "original_form": "이제와서 수정이라니..",
          "speaker_id": "1",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003096.1.21",
          "form": "왜 80개나 만든후에",
          "original_form": "왜 80개나 만든후에",
          "speaker_id": "2",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003096.1.22",
          "form": "수정을 요구하는거죠?",
          "original_form": "수정을 요구하는거죠?",
          "speaker_id": "2",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003096.1.23",
          "form": "왜죠?",
          "original_form": "왜죠?",
          "speaker_id": "2",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003096.1.24",
          "form": "진짜 완전 고생이다",
          "original_form": "진짜 완전 고생이다",
          "speaker_id": "2",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003096.1.25",
          "form": "이제서야 임원보고 했답니다...휴우..",
          "original_form": "이제서야 임원보고 했답니다...휴우..",
          "speaker_id": "1",
          "time": "20210820 20:11"
        },
        {
          "id": "MDRW2100003096.1.26",
          "form": "참... 할말없을뮤",
          "original_form": "참... 할말없을뮤",
          "speaker_id": "1",
          "time": "20210820 20:11"
        },
        {
          "id": "MDRW2100003096.1.27",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210820 20:13"
        },
        {
          "id": "MDRW2100003096.1.28",
          "form": "이제야?",
          "original_form": "이제야?",
          "speaker_id": "2",
          "time": "20210820 20:13"
        },
        {
          "id": "MDRW2100003096.1.29",
          "form": "대단하네 대단해",
          "original_form": "대단하네 대단해",
          "speaker_id": "2",
          "time": "20210820 20:13"
        },
        {
          "id": "MDRW2100003096.1.30",
          "form": "언니는 그럼 언제퇴근해?",
          "original_form": "언니는 그럼 언제퇴근해?",
          "speaker_id": "2",
          "time": "20210820 20:13"
        },
        {
          "id": "MDRW2100003096.1.31",
          "form": "지금 막 나왔어~",
          "original_form": "지금 막 나왔어~",
          "speaker_id": "1",
          "time": "20210820 20:14"
        },
        {
          "id": "MDRW2100003096.1.32",
          "form": "아이고 고생했어요",
          "original_form": "아이고 고생했어요",
          "speaker_id": "2",
          "time": "20210820 20:15"
        },
        {
          "id": "MDRW2100003096.1.33",
          "form": "ㅠㅠ웅웅 고단하구먼..",
          "original_form": "ㅠㅠ웅웅 고단하구먼..",
          "speaker_id": "1",
          "time": "20210820 20:18"
        },
        {
          "id": "MDRW2100003096.1.34",
          "form": "난 이제집에갑니당",
          "original_form": "난 이제집에갑니당",
          "speaker_id": "2",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003096.1.35",
          "form": "고생했옹~~",
          "original_form": "고생했옹~~",
          "speaker_id": "1",
          "time": "20210820 22:23"
        },
        {
          "id": "MDRW2100003096.1.36",
          "form": "난 가까스로 비오기 전에 집 도착//",
          "original_form": "난 가까스로 비오기 전에 집 도착//",
