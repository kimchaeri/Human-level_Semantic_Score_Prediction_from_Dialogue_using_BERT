        {
          "id": "MDRW2100018507.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210827 07:09"
        },
        {
          "id": "MDRW2100018507.1.2",
          "form": "네 오네요",
          "original_form": "네 오네요",
          "speaker_id": "2",
          "time": "20210827 07:10"
        },
        {
          "id": "MDRW2100018507.1.3",
          "form": "거긴 오나요?",
          "original_form": "거긴 오나요?",
          "speaker_id": "2",
          "time": "20210827 07:10"
        },
        {
          "id": "MDRW2100018507.1.4",
          "form": "여기고 시작이여",
          "original_form": "여기고 시작이여",
          "speaker_id": "1",
          "time": "20210827 07:10"
        },
        {
          "id": "MDRW2100018507.1.5",
          "form": "어제 안와서 좋았는데",
          "original_form": "어제 안와서 좋았는데",
          "speaker_id": "2",
          "time": "20210827 07:10"
        },
        {
          "id": "MDRW2100018507.1.6",
          "form": "오늘부터 이틀온다니",
          "original_form": "오늘부터 이틀온다니",
          "speaker_id": "2",
          "time": "20210827 07:10"
        },
        {
          "id": "MDRW2100018507.1.7",
          "form": "또 춥겠네요",
          "original_form": "또 춥겠네요",
          "speaker_id": "1",
          "time": "20210827 07:11"
        },
        {
          "id": "MDRW2100018507.1.8",
          "form": "뭐  아침저녁 쌀쌀하긴하죠",
          "original_form": "뭐  아침저녁 쌀쌀하긴하죠",
          "speaker_id": "2",
          "time": "20210827 07:12"
        },
        {
          "id": "MDRW2100018507.1.9",
          "form": "이러다 가을인거죠",
          "original_form": "이러다 가을인거죠",
          "speaker_id": "2",
          "time": "20210827 07:12"
        },
        {
          "id": "MDRW2100018507.1.10",
          "form": "벌써요",
          "original_form": "벌써요",
          "speaker_id": "1",
          "time": "20210827 07:14"
        },
        {
          "id": "MDRW2100018507.1.11",
          "form": "가을이죠 입추 지나지 않았나요?",
          "original_form": "가을이죠 입추 지나지 않았나요?",
          "speaker_id": "2",
          "time": "20210827 07:15"
        },
        {
          "id": "MDRW2100018507.1.12",
          "form": "그러네요",
          "original_form": "그러네요",
          "speaker_id": "1",
          "time": "20210827 07:56"
        },
        {
          "id": "MDRW2100018507.1.13",
          "form": "이제 좀있음 추워지고 완전 가을이죠",
          "original_form": "이제 좀있음 추워지고 완전 가을이죠",
          "speaker_id": "2",
          "time": "20210827 07:58"
        },
        {
          "id": "MDRW2100018507.1.14",
          "form": "긴옷입어야래요",
          "original_form": "긴옷입어야래요",
          "speaker_id": "1",
          "time": "20210827 07:59"
        },
        {
          "id": "MDRW2100018507.1.15",
          "form": "네 좀있음 그래야죠 아직은 낮에 너무덥고",
          "original_form": "네 좀있음 그래야죠 아직은 낮에 너무덥고",
          "speaker_id": "2",
          "time": "20210827 07:59"
        },
        {
          "id": "MDRW2100018507.1.16",
          "form": "찝찝하고 그래서 아직은 반팔요 ㅎㅎ",
          "original_form": "찝찝하고 그래서 아직은 반팔요 ㅎㅎ",
