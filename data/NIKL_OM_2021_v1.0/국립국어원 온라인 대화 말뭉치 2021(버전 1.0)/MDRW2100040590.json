      "utterance": [
        {
          "id": "MDRW2100040590.1.1",
          "form": "네 저는 시도는 해보았는데",
          "original_form": "네 저는 시도는 해보았는데",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040590.1.2",
          "form": "그 특유의 냄새를 못견디겠더라구요...",
          "original_form": "그 특유의 냄새를 못견디겠더라구요...",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040590.1.3",
          "form": "그래서 진짜 맛집 가서 먹었는데도",
          "original_form": "그래서 진짜 맛집 가서 먹었는데도",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040590.1.4",
          "form": "다 친구 주고 저는 꿔바로우만 먹었어요",
          "original_form": "다 친구 주고 저는 꿔바로우만 먹었어요",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040590.1.5",
          "form": "호불호 많이 갈린다더니 진짜 그런가봐요",
          "original_form": "호불호 많이 갈린다더니 진짜 그런가봐요",
          "speaker_id": "1",
          "time": "20210904 23:02"
        },
        {
          "id": "MDRW2100040590.1.6",
          "form": "냄새가 어때요?",
          "original_form": "냄새가 어때요?",
          "speaker_id": "1",
          "time": "20210904 23:02"
        },
        {
          "id": "MDRW2100040590.1.7",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040590.1.8",
          "form": "인도사람 암내 아시나요...?",
          "original_form": "인도사람 암내 아시나요...?",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040590.1.9",
          "form": "아니요.....??????????ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니요.....??????????ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100040590.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100040590.1.11",
          "form": "하",
          "original_form": "하",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100040590.1.12",
          "form": "모르셔서 부럽습니다",
          "original_form": "모르셔서 부럽습니다",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100040590.1.13",
          "form": "저는 대학원에서 인도에서 온 포닥이랑 같이 지냈는데",
          "original_form": "저는 대학원에서 인도에서 온 포닥이랑 같이 지냈는데",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100040590.1.14",
          "form": "죽는 줄 알았거든요",
          "original_form": "죽는 줄 알았거든요",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100040590.1.15",
          "form": "하튼 양고기를 먹으면 비슷한 냄새가 입 안에서 확...",
          "original_form": "하튼 양고기를 먹으면 비슷한 냄새가 입 안에서 확...",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100040590.1.16",
          "form": "포닥은 뭐에요?",
          "original_form": "포닥은 뭐에요?",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100040590.1.17",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100040590.1.18",
          "form": "포스트 닥터 라고",
          "original_form": "포스트 닥터 라고",
          "speaker_id": "2",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100040590.1.19",
          "form": "박사후 연구과정",
          "original_form": "박사후 연구과정",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100040590.1.20",
          "form": "인데 박사과정 밟고나서 경력 쌓으려고 연구하는 분들이요!",
          "original_form": "인데 박사과정 밟고나서 경력 쌓으려고 연구하는 분들이요!",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100040590.1.21",
          "form": "오오 저는 대학만 졸업해서 몰랐어요",
          "original_form": "오오 저는 대학만 졸업해서 몰랐어요",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100040590.1.22",
          "form": "포닥 다음에 교수가 될 수 있고... 연구원 될수도 있고...",
          "original_form": "포닥 다음에 교수가 될 수 있고... 연구원 될수도 있고...",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100040590.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100040590.1.24",
          "form": "네네ㅠ 주로 우리나라는",
          "original_form": "네네ㅠ 주로 우리나라는",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100040590.1.25",
          "form": "인도에서 많이 오거든요",
          "original_form": "인도에서 많이 오거든요",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100040590.1.26",
          "form": "힘든 경험을 하셨네요",
          "original_form": "힘든 경험을 하셨네요",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100040590.1.27",
          "form": "인건비가 싸서...",
          "original_form": "인건비가 싸서...",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100040590.1.28",
          "form": "오 이유가 있어요?",
          "original_form": "오 이유가 있어요?",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100040590.1.29",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100040590.1.30",
          "form": "네네... 그분들이랑 있으면",
          "original_form": "네네... 그분들이랑 있으면",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100040590.1.31",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100040590.1.32",
          "form": "그분들 힌두교가 많아서",
          "original_form": "그분들 힌두교가 많아서",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100040590.1.33",
          "form": "소고기를 안드세요",
          "original_form": "소고기를 안드세요",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100040590.1.34",
          "form": "식생활 때문에 냄새가 그런걸까요?",
          "original_form": "식생활 때문에 냄새가 그런걸까요?",
          "speaker_id": "1",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100040590.1.35",
          "form": "네네 그런거 같아요",
          "original_form": "네네 그런거 같아요",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100040590.1.36",
          "form": "평소에도 향신료 강한",
          "original_form": "평소에도 향신료 강한",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100040590.1.37",
          "form": "음식을 먹으니까....ㅜ",
          "original_form": "음식을 먹으니까....ㅜ",
          "speaker_id": "2",
          "time": "20210904 23:18"
        }
      ]
    }
