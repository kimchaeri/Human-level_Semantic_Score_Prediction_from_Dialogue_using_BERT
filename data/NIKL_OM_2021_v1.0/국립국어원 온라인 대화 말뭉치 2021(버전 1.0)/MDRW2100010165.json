      "utterance": [
        {
          "id": "MDRW2100010165.1.1",
          "form": "언니 밖에 없네라는 퀴어 단편선도 있어",
          "original_form": "언니 밖에 없네라는 퀴어 단편선도 있어",
          "speaker_id": "2",
          "time": "20210823 14:59"
        },
        {
          "id": "MDRW2100010165.1.2",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210823 14:59"
        },
        {
          "id": "MDRW2100010165.1.3",
          "form": "근데나단편은",
          "original_form": "근데나단편은",
          "speaker_id": "1",
          "time": "20210823 14:59"
        },
        {
          "id": "MDRW2100010165.1.4",
          "form": "안좋아함",
          "original_form": "안좋아함",
          "speaker_id": "1",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010165.1.5",
          "form": "읽다 끊기는 느낌이라서??",
          "original_form": "읽다 끊기는 느낌이라서??",
          "speaker_id": "2",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010165.1.6",
          "form": "ㅇㅇㅇ뭔가단편은친절하지않은경우가",
          "original_form": "ㅇㅇㅇ뭔가단편은친절하지않은경우가",
          "speaker_id": "1",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010165.1.7",
          "form": "많은거같음",
          "original_form": "많은거같음",
          "speaker_id": "1",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010165.1.8",
          "form": "아 그치 후다닥 얍 완결 이럴때가 있지",
          "original_form": "아 그치 후다닥 얍 완결 이럴때가 있지",
          "speaker_id": "2",
          "time": "20210823 15:01"
        },
        {
          "id": "MDRW2100010165.1.9",
          "form": "맞아. 그리고 장편보다 숨겨진 의미이런게",
          "original_form": "맞아. 그리고 장편보다 숨겨진 의미이런게",
          "speaker_id": "1",
          "time": "20210823 15:02"
        },
        {
          "id": "MDRW2100010165.1.10",
          "form": "근데 이건 꽤 괜찮음",
          "original_form": "근데 이건 꽤 괜찮음",
          "speaker_id": "2",
          "time": "20210823 15:02"
        },
        {
          "id": "MDRW2100010165.1.11",
          "form": "더 많은느낌임",
          "original_form": "더 많은느낌임",
          "speaker_id": "1",
          "time": "20210823 15:02"
        },
        {
          "id": "MDRW2100010165.1.12",
          "form": "아 ㅇㅋㅇㅋ 한 번 읽어봄",
          "original_form": "아 ㅇㅋㅇㅋ 한 번 읽어봄",
          "speaker_id": "1",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010165.1.13",
          "form": "아 그치 풀어가는 것 보단 함축시켜야 하니까",
          "original_form": "아 그치 풀어가는 것 보단 함축시켜야 하니까",
          "speaker_id": "2",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010165.1.14",
          "form": "ㅇㅇㅇㅇ딱 그 느낌임",
          "original_form": "ㅇㅇㅇㅇ딱 그 느낌임",
          "speaker_id": "1",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010165.1.15",
          "form": "나중에 우리집 오면 빌려줄게",
          "original_form": "나중에 우리집 오면 빌려줄게",
          "speaker_id": "2",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010165.1.16",
          "form": "그니까 가끔 작가도 모르면서 막 쓰는 느낌리 들 때가 있음",
          "original_form": "그니까 가끔 작가도 모르면서 막 쓰는 느낌리 들 때가 있음",
          "speaker_id": "2",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010165.1.17",
          "form": "ㅇㅋㅇㅋ 어차피 개강전에 한 번 보기로 하지 않았음?",
          "original_form": "ㅇㅋㅇㅋ 어차피 개강전에 한 번 보기로 하지 않았음?",
          "speaker_id": "1",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010165.1.18",
          "form": "아 그거 우리집이야?",
          "original_form": "아 그거 우리집이야?",
          "speaker_id": "2",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010165.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅇㅇㅇ너네집이야",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅇㅇㅇ너네집이야",
          "speaker_id": "1",
          "time": "20210823 15:04"
        },
        {
          "id": "MDRW2100010165.1.20",
          "form": "너네 집 아니였냐",
          "original_form": "너네 집 아니였냐",
          "speaker_id": "2",
          "time": "20210823 15:04"
        },
