      "utterance": [
        {
          "id": "MDRW2100028207.1.1",
          "form": "다이어트 잘돼?",
          "original_form": "다이어트 잘돼?",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028207.1.2",
          "form": "유지만 하고있어 넌 단백질 보충 뭐로해",
          "original_form": "유지만 하고있어 넌 단백질 보충 뭐로해",
          "speaker_id": "1",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028207.1.3",
          "form": "소고기",
          "original_form": "소고기",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028207.1.4",
          "form": "대박이네 난 엄마가 다 구워먹어버림 내껀데",
          "original_form": "대박이네 난 엄마가 다 구워먹어버림 내껀데",
          "speaker_id": "1",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028207.1.5",
          "form": "삼겹살 먹고 싶다",
          "original_form": "삼겹살 먹고 싶다",
          "speaker_id": "2",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100028207.1.6",
          "form": "삼겹살에 김치돌돌말아",
          "original_form": "삼겹살에 김치돌돌말아",
          "speaker_id": "1",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100028207.1.7",
          "form": "콜라랑 같이 먹으면 최고지",
          "original_form": "콜라랑 같이 먹으면 최고지",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028207.1.8",
          "form": "먹고싶어 다음주에 먹을거야",
          "original_form": "먹고싶어 다음주에 먹을거야",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028207.1.9",
          "form": "나 이제 운전해 안녕",
          "original_form": "나 이제 운전해 안녕",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028207.1.10",
          "form": "그래 나도 이제 저녁 먹으러 가",
          "original_form": "그래 나도 이제 저녁 먹으러 가",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028207.1.11",
          "form": "맛있게 먹어",
          "original_form": "맛있게 먹어",
          "speaker_id": "1",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100028207.1.12",
          "form": "넌 뭐 먹었니?",
          "original_form": "넌 뭐 먹었니?",
          "speaker_id": "2",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028207.1.13",
          "form": "난 차에서 육포",
          "original_form": "난 차에서 육포",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028207.1.14",
          "form": "육포 너무 맛있어",
          "original_form": "육포 너무 맛있어",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028207.1.15",
          "form": "배고프겠다",
          "original_form": "배고프겠다",
          "speaker_id": "2",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028207.1.16",
          "form": "나는 밥 먹고 집에 가는 중",
          "original_form": "나는 밥 먹고 집에 가는 중",
          "speaker_id": "2",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028207.1.17",
          "form": "배고파 뭐먹었어",
          "original_form": "배고파 뭐먹었어",
          "speaker_id": "1",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028207.1.18",
          "form": "초밥 먹었지",
          "original_form": "초밥 먹었지",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028207.1.19",
