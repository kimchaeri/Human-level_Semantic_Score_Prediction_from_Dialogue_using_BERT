        {
          "id": "MDRW2100044294.1.1",
          "form": "어쩔 수 없는건가봐요 코로나는 흑흑..",
          "original_form": "어쩔 수 없는건가봐요 코로나는 흑흑..",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044294.1.2",
          "form": "그쵸ㅜㅜㅜ",
          "original_form": "그쵸ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044294.1.3",
          "form": "아근데",
          "original_form": "아근데",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044294.1.4",
          "form": "계속 찜닭쓰고있으니까",
          "original_form": "계속 찜닭쓰고있으니까",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044294.1.5",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044294.1.6",
          "form": "뭔가 재밌고 귀여워요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "뭔가 재밌고 귀여워요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044294.1.7",
          "form": "찜닭...일미리",
          "original_form": "찜닭...일미리",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044294.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ찜닭이 최고예요ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ찜닭이 최고예요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044294.1.9",
          "form": "내일꼭 시켜먹을거에요ㅋㅋㅋ",
          "original_form": "내일꼭 시켜먹을거에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044294.1.10",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044294.1.11",
          "form": "진찐 찜닭 짱이에요ㅜㅜ네네",
          "original_form": "진찐 찜닭 짱이에요ㅜㅜ네네",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044294.1.12",
          "form": "감기 걸리면 찜닭 꼭 먹어요",
          "original_form": "감기 걸리면 찜닭 꼭 먹어요",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044294.1.13",
          "form": "완전 소울푸드",
          "original_form": "완전 소울푸드",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044294.1.14",
          "form": "당면사리도...",
          "original_form": "당면사리도...",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044294.1.15",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044294.1.16",
          "form": "뜨끈뜨끈하고",
          "original_form": "뜨끈뜨끈하고",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044294.1.17",
          "form": "매콤하니까 일리있으세요!!",
          "original_form": "매콤하니까 일리있으세요!!",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044294.1.18",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044294.1.19",
          "form": "닭볶음탕은",
          "original_form": "닭볶음탕은",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044294.1.20",
          "form": "아픈속엔 너무 자극적이니까",
          "original_form": "아픈속엔 너무 자극적이니까",
