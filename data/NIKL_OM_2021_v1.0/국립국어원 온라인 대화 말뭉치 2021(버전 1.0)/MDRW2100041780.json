      "utterance": [
        {
          "id": "MDRW2100041780.1.1",
          "form": "앗 마지막에 하신 말씀을 못봤어요",
          "original_form": "앗 마지막에 하신 말씀을 못봤어요",
          "speaker_id": "1",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100041780.1.2",
          "form": "복권 1등돼도 못사는 한남동 주택...ㅋㅋㅋㅋ",
          "original_form": "복권 1등돼도 못사는 한남동 주택...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100041780.1.3",
          "form": "저는 문래쪽에 집있으면",
          "original_form": "저는 문래쪽에 집있으면",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041780.1.4",
          "form": "참 좋을 거같아요",
          "original_form": "참 좋을 거같아요",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041780.1.5",
          "form": "오오 맞아요",
          "original_form": "오오 맞아요",
          "speaker_id": "1",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041780.1.6",
          "form": "서울 어딜가도 멀지않고",
          "original_form": "서울 어딜가도 멀지않고",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041780.1.7",
          "form": "2호선이고",
          "original_form": "2호선이고",
          "speaker_id": "1",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041780.1.8",
          "form": "2호선에다가",
          "original_form": "2호선에다가",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041780.1.9",
          "form": "맞아요....",
          "original_form": "맞아요....",
          "speaker_id": "1",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041780.1.10",
          "form": "name2님 운전하실 줄 아세요?",
          "original_form": "&name2&님 운전하실 줄 아세요?",
          "speaker_id": "1",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041780.1.11",
          "form": "집에서 문래가 멀진 않지만..",
          "original_form": "집에서 문래가 멀진 않지만..",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041780.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041780.1.13",
          "form": "저는 면허도 없어요ㅠㅠ",
          "original_form": "저는 면허도 없어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041780.1.14",
          "form": "와 어른이다...",
          "original_form": "와 어른이다...",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041780.1.15",
          "form": "ㅋㅋㅋㅋ저는 운전 좋아해요 재밌어요",
          "original_form": "ㅋㅋㅋㅋ저는 운전 좋아해요 재밌어요",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041780.1.16",
          "form": "면허 따보세요! 완전 달라집니다",
          "original_form": "면허 따보세요! 완전 달라집니다",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041780.1.17",
          "form": "맞아요 그렇다면서요!",
          "original_form": "맞아요 그렇다면서요!",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041780.1.18",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041780.1.19",
          "form": "name2님은",
          "original_form": "&name2&님은",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041780.1.20",
          "form": "소뇌가 발달하셔서",
          "original_form": "소뇌가 발달하셔서",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041780.1.21",
          "form": "운전을 재미있다고 느끼시는 걸수도 있어요",
          "original_form": "운전을 재미있다고 느끼시는 걸수도 있어요",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041780.1.22",
          "form": "아녀유 저도 첨에 운전 ㅋㅋㅋㅋ 해보기 전에는 무서워가지고",
          "original_form": "아녀유 저도 첨에 운전 ㅋㅋㅋㅋ 해보기 전에는 무서워가지고",
          "speaker_id": "2",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041780.1.23",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041780.1.24",
          "form": "면허증 따면 취업에도 좀 유리한 것 같더라구요ㅠ",
          "original_form": "면허증 따면 취업에도 좀 유리한 것 같더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041780.1.25",
          "form": "면허 오랫동안 안땃거든요",
          "original_form": "면허 오랫동안 안땃거든요",
          "speaker_id": "2",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041780.1.26",
          "form": "오오 진짜요?!",
          "original_form": "오오 진짜요?!",
          "speaker_id": "1",
          "time": "20210906 15:47"
        }
      ]
