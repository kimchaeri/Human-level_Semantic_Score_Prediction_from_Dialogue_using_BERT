      "utterance": [
        {
          "id": "MDRW2100014414.1.1",
          "form": "안녕하세요! 저녁 드셨나요?",
          "original_form": "안녕하세요! 저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210825 18:53"
        },
        {
          "id": "MDRW2100014414.1.2",
          "form": "아직 안먹었습니다 점심을 많이먹어서 ㅎㅎ",
          "original_form": "아직 안먹었습니다 점심을 많이먹어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:53"
        },
        {
          "id": "MDRW2100014414.1.3",
          "form": "드셨나요",
          "original_form": "드셨나요",
          "speaker_id": "2",
          "time": "20210825 18:53"
        },
        {
          "id": "MDRW2100014414.1.4",
          "form": "저도 아직 안 먹었어요! 곧 먹으려구요",
          "original_form": "저도 아직 안 먹었어요! 곧 먹으려구요",
          "speaker_id": "1",
          "time": "20210825 18:53"
        },
        {
          "id": "MDRW2100014414.1.5",
          "form": "점심에 뭐드셨나요??",
          "original_form": "점심에 뭐드셨나요??",
          "speaker_id": "1",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014414.1.6",
          "form": "돈까스요!",
          "original_form": "돈까스요!",
          "speaker_id": "2",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014414.1.7",
          "form": "저녁뭐드세요?",
          "original_form": "저녁뭐드세요?",
          "speaker_id": "2",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014414.1.8",
          "form": "오 저는 오징어볶음 먹어요!",
          "original_form": "오 저는 오징어볶음 먹어요!",
          "speaker_id": "1",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014414.1.9",
          "form": "돈까스 너무 맛있었을 것 같아요",
          "original_form": "돈까스 너무 맛있었을 것 같아요",
          "speaker_id": "1",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014414.1.10",
          "form": "와 맛있겠다",
          "original_form": "와 맛있겠다",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014414.1.11",
          "form": "덕분에 배터지게 먹고 지금 그냥 라떼먹는중이에요 ㅎㅎ",
          "original_form": "덕분에 배터지게 먹고 지금 그냥 라떼먹는중이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014414.1.12",
          "form": "라떼 맛있죠!ㅋㅋㅋㅋ 달달한 거 좋아하세요?!",
          "original_form": "라떼 맛있죠!ㅋㅋㅋㅋ 달달한 거 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014414.1.13",
          "form": "네 좋아해요 ㅋㅋㅋ 좋아하시나요",
          "original_form": "네 좋아해요 ㅋㅋㅋ 좋아하시나요",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014414.1.14",
          "form": "네 ㅋㅋㅋ 저도 좋아해요 카페가면 무조건 달달한 거 먹어요",
          "original_form": "네 ㅋㅋㅋ 저도 좋아해요 카페가면 무조건 달달한 거 먹어요",
          "speaker_id": "1",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014414.1.15",
          "form": "전 라떼도 좋고 커피아니면ㅋㅋㅋ 자허블먹어요",
          "original_form": "전 라떼도 좋고 커피아니면ㅋㅋㅋ 자허블먹어요",
          "speaker_id": "2",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014414.1.16",
          "form": "아 자허블 맛있죠 저도 스벅 가면 2번에 한 번은 꼭 먹는 거 같아요ㅋㅋㅋㅋ",
          "original_form": "아 자허블 맛있죠 저도 스벅 가면 2번에 한 번은 꼭 먹는 거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100014414.1.17",
          "form": "마자요 클래식빼고 자몽으로!",
          "original_form": "마자요 클래식빼고 자몽으로!",
          "speaker_id": "2",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100014414.1.18",
          "form": "오 드실 줄 아시는 군요!ㅋㅋㅋㅋ 자몽 최대로 해서 먹어요 저는ㅎㅎㅎ",
          "original_form": "오 드실 줄 아시는 군요!ㅋㅋㅋㅋ 자몽 최대로 해서 먹어요 저는ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014414.1.19",
          "form": "ㅋㅋㅋㅋ 저는 약간 가책을 느껴서 2번만..",
          "original_form": "ㅋㅋㅋㅋ 저는 약간 가책을 느껴서 2번만..",
          "speaker_id": "2",
          "time": "20210825 19:16"
        }
      ]
