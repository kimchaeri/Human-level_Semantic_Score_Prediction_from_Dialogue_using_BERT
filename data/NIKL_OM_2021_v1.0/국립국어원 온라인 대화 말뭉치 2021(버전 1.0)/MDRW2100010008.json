      "utterance": [
        {
          "id": "MDRW2100010008.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210823 13:11"
        },
        {
          "id": "MDRW2100010008.1.2",
          "form": "요즘 랩실에서 실험은 잘 되어가?",
          "original_form": "요즘 랩실에서 실험은 잘 되어가?",
          "speaker_id": "1",
          "time": "20210823 13:11"
        },
        {
          "id": "MDRW2100010008.1.3",
          "form": "아니 주말에 한 실험이 모두 망해서 슬퍼",
          "original_form": "아니 주말에 한 실험이 모두 망해서 슬퍼",
          "speaker_id": "2",
          "time": "20210823 13:11"
        },
        {
          "id": "MDRW2100010008.1.4",
          "form": "언니 일은 어때?",
          "original_form": "언니 일은 어때?",
          "speaker_id": "2",
          "time": "20210823 13:12"
        },
        {
          "id": "MDRW2100010008.1.5",
          "form": "다 결과가 제대로 안나왔어?",
          "original_form": "다 결과가 제대로 안나왔어?",
          "speaker_id": "1",
          "time": "20210823 13:12"
        },
        {
          "id": "MDRW2100010008.1.6",
          "form": "무슨 일 하는지 설명해줘",
          "original_form": "무슨 일 하는지 설명해줘",
          "speaker_id": "2",
          "time": "20210823 13:12"
        },
        {
          "id": "MDRW2100010008.1.7",
          "form": "나는 뭐 요즘 접종때문에 바쁘지",
          "original_form": "나는 뭐 요즘 접종때문에 바쁘지",
          "speaker_id": "1",
          "time": "20210823 13:12"
        },
        {
          "id": "MDRW2100010008.1.8",
          "form": "응응 왜 이런 결과가 나왔나 모르겠네",
          "original_form": "응응 왜 이런 결과가 나왔나 모르겠네",
          "speaker_id": "2",
          "time": "20210823 13:12"
        },
        {
          "id": "MDRW2100010008.1.9",
          "form": "병원에서 접수받고 진행하는데",
          "original_form": "병원에서 접수받고 진행하는데",
          "speaker_id": "1",
          "time": "20210823 13:12"
        },
        {
          "id": "MDRW2100010008.1.10",
          "form": "계속 화이자 맞춰?",
          "original_form": "계속 화이자 맞춰?",
          "speaker_id": "2",
          "time": "20210823 13:12"
        },
        {
          "id": "MDRW2100010008.1.11",
          "form": "아닝 화이자 아스트라 전부 와",
          "original_form": "아닝 화이자 아스트라 전부 와",
          "speaker_id": "1",
          "time": "20210823 13:12"
        },
        {
          "id": "MDRW2100010008.1.12",
          "form": "근데 막",
          "original_form": "근데 막",
          "speaker_id": "1",
          "time": "20210823 13:12"
        },
        {
          "id": "MDRW2100010008.1.13",
          "form": "11시에 예약되서 왔는데",
          "original_form": "11시에 예약되서 왔는데",
          "speaker_id": "1",
          "time": "20210823 13:13"
        },
        {
          "id": "MDRW2100010008.1.14",
          "form": "11시 정각에 접종 안해준다고 화를 내는 사람이 많아ㅋㄱㄱ",
          "original_form": "11시 정각에 접종 안해준다고 화를 내는 사람이 많아ㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210823 13:13"
        },
        {
          "id": "MDRW2100010008.1.15",
          "form": "그럴때마다 짜증남;;",
          "original_form": "그럴때마다 짜증남;;",
          "speaker_id": "1",
          "time": "20210823 13:13"
        },
        {
          "id": "MDRW2100010008.1.16",
          "form": "그럴 것 같아.",
          "original_form": "그럴 것 같아.",
          "speaker_id": "2",
          "time": "20210823 13:13"
        },
        {
          "id": "MDRW2100010008.1.17",
          "form": "넌 랩실에서 어때?",
          "original_form": "넌 랩실에서 어때?",
          "speaker_id": "1",
          "time": "20210823 13:13"
        },
        {
          "id": "MDRW2100010008.1.18",
          "form": "접종 하루에 몇 명 정도해?",
          "original_form": "접종 하루에 몇 명 정도해?",
          "speaker_id": "2",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010008.1.19",
          "form": "그 언니 어케하기로 함?",
          "original_form": "그 언니 어케하기로 함?",
          "speaker_id": "1",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010008.1.20",
          "form": "여긴 뭐 시간마다 기본 열명하니까",
          "original_form": "여긴 뭐 시간마다 기본 열명하니까",
          "speaker_id": "1",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010008.1.21",
          "form": "9월까지 일단 가만히 지켜봐주기로 한 것 같아",
          "original_form": "9월까지 일단 가만히 지켜봐주기로 한 것 같아",
          "speaker_id": "2",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010008.1.22",
          "form": "하루에 대략 60명에서 70명 정도 하네",
          "original_form": "하루에 대략 60명에서 70명 정도 하네",
          "speaker_id": "1",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010008.1.23",
          "form": "그리고 내일 내가 일하는거 도와주기로 했는데",
          "original_form": "그리고 내일 내가 일하는거 도와주기로 했는데",
          "speaker_id": "2",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010008.1.24",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "1",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010008.1.25",
          "form": "짜증날 것을 예상하고",
          "original_form": "짜증날 것을 예상하고",
          "speaker_id": "2",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010008.1.26",
          "form": "하려고 ㅎㅎ",
          "original_form": "하려고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100010008.1.27",
          "form": "ㅋㅋㅋㄱㄱㄱ 어이구야 그 언니 되게",
          "original_form": "ㅋㅋㅋㄱㄱㄱ 어이구야 그 언니 되게",
          "speaker_id": "1",
          "time": "20210823 13:15"
        },
        {
          "id": "MDRW2100010008.1.28",
          "form": "듣기만 해도 스트레스 받던데",
          "original_form": "듣기만 해도 스트레스 받던데",
          "speaker_id": "1",
          "time": "20210823 13:15"
        }
      ]
    }
  ]
