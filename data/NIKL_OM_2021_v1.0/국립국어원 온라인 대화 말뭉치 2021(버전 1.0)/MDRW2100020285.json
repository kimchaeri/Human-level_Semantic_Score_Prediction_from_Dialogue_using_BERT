        },
        {
          "id": "MDRW2100020285.1.3",
          "form": "탄산수 고르는 것 좀 도와줄래",
          "original_form": "탄산수 고르는 것 좀 도와줄래",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020285.1.4",
          "form": "요즘 탄산수 맛이 다양하지요",
          "original_form": "요즘 탄산수 맛이 다양하지요",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020285.1.5",
          "form": "알고 있구나 그래서 고민이야",
          "original_form": "알고 있구나 그래서 고민이야",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020285.1.6",
          "form": "나는 키위 맛을 제일 좋아하는데",
          "original_form": "나는 키위 맛을 제일 좋아하는데",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020285.1.7",
          "form": "나는 원래는 포도를 제일 좋아했어",
          "original_form": "나는 원래는 포도를 제일 좋아했어",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020285.1.8",
          "form": "청포도 맛도 맛있는데",
          "original_form": "청포도 맛도 맛있는데",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020285.1.9",
          "form": "내가 지금 고민인 부분은 다른 새로운 맛도 있다는 거야",
          "original_form": "내가 지금 고민인 부분은 다른 새로운 맛도 있다는 거야",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020285.1.10",
          "form": "맛이 다양하구나",
          "original_form": "맛이 다양하구나",
          "speaker_id": "1",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020285.1.11",
          "form": "요즘은 배음악도 있대",
          "original_form": "요즘은 배음악도 있대",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020285.1.12",
          "form": "배맛? 맛있겠다",
          "original_form": "배맛? 맛있겠다",
          "speaker_id": "1",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020285.1.13",
          "form": "예전에 있는 아이스크림 맛이랑 비슷하다더라",
          "original_form": "예전에 있는 아이스크림 맛이랑 비슷하다더라",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020285.1.14",
          "form": "그거 사먹어봐야겠다",
          "original_form": "그거 사먹어봐야겠다",
          "speaker_id": "1",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020285.1.15",
          "form": "내가 사면은 너 하나 줄게",
          "original_form": "내가 사면은 너 하나 줄게",
