        {
          "id": "MDRW2100014463.1.1",
          "form": "좋아하는 과목이 뭔가요?",
          "original_form": "좋아하는 과목이 뭔가요?",
          "speaker_id": "1",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100014463.1.2",
          "form": "수학이여",
          "original_form": "수학이여",
          "speaker_id": "2",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014463.1.3",
          "form": "저도요^-^",
          "original_form": "저도요^-^",
          "speaker_id": "1",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100014463.1.4",
          "form": "수학의 명료함이 좋아요",
          "original_form": "수학의 명료함이 좋아요",
          "speaker_id": "2",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100014463.1.5",
          "form": "저도요. 그럼 수학중에서 뭐가 좋으셨나요?",
          "original_form": "저도요. 그럼 수학중에서 뭐가 좋으셨나요?",
          "speaker_id": "1",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014463.1.6",
          "form": "전 방정식요ㅋ",
          "original_form": "전 방정식요ㅋ",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014463.1.7",
          "form": "그게 명쾌하죠 ㅋㅋ",
          "original_form": "그게 명쾌하죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014463.1.8",
          "form": "딱떨어지는",
          "original_form": "딱떨어지는",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014463.1.9",
          "form": "그럼 국어는 별로셨나요?",
          "original_form": "그럼 국어는 별로셨나요?",
          "speaker_id": "1",
          "time": "20210826 08:06"
        },
        {
          "id": "MDRW2100014463.1.10",
          "form": "국어는 싫어요ㅋㅋ",
          "original_form": "국어는 싫어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:20"
        },
        {
          "id": "MDRW2100014463.1.11",
          "form": "지문잔뜩",
          "original_form": "지문잔뜩",
          "speaker_id": "2",
          "time": "20210826 08:20"
        },
        {
          "id": "MDRW2100014463.1.12",
          "form": "저도요 ㅠ 그래도 비문학이랑 문법이 좀 낫더라고요",
          "original_form": "저도요 ㅠ 그래도 비문학이랑 문법이 좀 낫더라고요",
          "speaker_id": "1",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100014463.1.13",
          "form": "그냥 숫자가 좋은ㅋ",
          "original_form": "그냥 숫자가 좋은ㅋ",
          "speaker_id": "2",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100014463.1.14",
