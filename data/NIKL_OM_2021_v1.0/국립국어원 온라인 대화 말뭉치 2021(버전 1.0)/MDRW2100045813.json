      "utterance": [
        {
          "id": "MDRW2100045813.1.1",
          "form": "정수기 다채운 의지의 사나이 ㄷㄷ",
          "original_form": "정수기 다채운 의지의 사나이 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100045813.1.2",
          "form": "물은 많이 있어야 안전한 ㄷㄷ",
          "original_form": "물은 많이 있어야 안전한 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100045813.1.3",
          "form": "욕조가 없어서 아쉽네요",
          "original_form": "욕조가 없어서 아쉽네요",
          "speaker_id": "2",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100045813.1.4",
          "form": "비 안오면 큰일인",
          "original_form": "비 안오면 큰일인",
          "speaker_id": "1",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100045813.1.5",
          "form": "이사가는게 낫지않을ㅋ가요",
          "original_form": "이사가는게 낫지않을ㅋ가요",
          "speaker_id": "1",
          "time": "20210908 07:37"
        },
        {
          "id": "MDRW2100045813.1.6",
          "form": "트럭도 있으신데 여기가 좋은가바요",
          "original_form": "트럭도 있으신데 여기가 좋은가바요",
          "speaker_id": "1",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100045813.1.7",
          "form": "그런가봐요 ㅋㅋ",
          "original_form": "그런가봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100045813.1.8",
          "form": "아이템이 많긴하다는",
          "original_form": "아이템이 많긴하다는",
          "speaker_id": "1",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100045813.1.9",
          "form": "맘껏먹는 베리파티 ㅋㅋㅋ",
          "original_form": "맘껏먹는 베리파티 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:47"
        },
        {
          "id": "MDRW2100045813.1.10",
          "form": "살빠지긴하나바요",
          "original_form": "살빠지긴하나바요",
          "speaker_id": "2",
          "time": "20210908 07:47"
        },
        {
          "id": "MDRW2100045813.1.11",
          "form": "저는 첨봣다는 ㄷㄷ",
          "original_form": "저는 첨봣다는 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210908 07:47"
        },
        {
          "id": "MDRW2100045813.1.12",
          "form": "살빠지는거요?",
          "original_form": "살빠지는거요?",
          "speaker_id": "1",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100045813.1.13",
          "form": "베리만 먹으니 빠질수밖에 ㅋㅋ",
          "original_form": "베리만 먹으니 빠질수밖에 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045813.1.14",
          "form": "채집렙 진짜 높을듯",
          "original_form": "채집렙 진짜 높을듯",
          "speaker_id": "1",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045813.1.15",
          "form": "고기도 먹어줘야 하는데",
          "original_form": "고기도 먹어줘야 하는데",
          "speaker_id": "2",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100045813.1.16",
          "form": "이거 칼로리도 있던데요",
          "original_form": "이거 칼로리도 있던데요",
          "speaker_id": "1",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100045813.1.17",
          "form": "맞아요 ㄷㄷ 진짜 섬세한게임",
          "original_form": "맞아요 ㄷㄷ 진짜 섬세한게임",
          "speaker_id": "2",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100045813.1.18",
          "form": "은근 살만하네요",
          "original_form": "은근 살만하네요",
          "speaker_id": "1",
          "time": "20210908 07:52"
        }
      ]
