      "utterance": [
        {
          "id": "MDRW2100037067.1.1",
          "form": "안녕하세요~~",
          "original_form": "안녕하세요~~",
          "speaker_id": "1",
          "time": "20210831 18:40"
        },
        {
          "id": "MDRW2100037067.1.2",
          "form": "안녕하세요 저녁드셨어요?",
          "original_form": "안녕하세요 저녁드셨어요?",
          "speaker_id": "2",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100037067.1.3",
          "form": "아뇨...아직..!! 누워서 빈둥대고 있네요 ㅋㅋㅋㅋ",
          "original_form": "아뇨...아직..!! 누워서 빈둥대고 있네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100037067.1.4",
          "form": "name2님은 진토닉 좋아하시나요",
          "original_form": "&name2&님은 진토닉 좋아하시나요",
          "speaker_id": "1",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100037067.1.5",
          "form": "진토닉정말 좋아합니다!",
          "original_form": "진토닉정말 좋아합니다!",
          "speaker_id": "2",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100037067.1.6",
          "form": "근데 맛집가서 먹으면",
          "original_form": "근데 맛집가서 먹으면",
          "speaker_id": "2",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100037067.1.7",
          "form": "돈이먛이나와서",
          "original_form": "돈이먛이나와서",
          "speaker_id": "2",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100037067.1.8",
          "form": "이제 만들어먹어야되나 고민중입니다",
          "original_form": "이제 만들어먹어야되나 고민중입니다",
          "speaker_id": "2",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100037067.1.9",
          "form": "오호...! 저도 그런 깔끔 상큼한 맛이 좋아요...!!",
          "original_form": "오호...! 저도 그런 깔끔 상큼한 맛이 좋아요...!!",
          "speaker_id": "1",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100037067.1.10",
          "form": "직접 만들어먹으면 하루에 한잔씩ㅋㅋㅋㅋㅋ마실듯여 ㅋㅋㅋㅋㅋ",
          "original_form": "직접 만들어먹으면 하루에 한잔씩ㅋㅋㅋㅋㅋ마실듯여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100037067.1.11",
          "form": "ㅋㅋㄱㅋㅋㅋㅋ맛집의 조리법을 알아오고싶어요",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋ맛집의 조리법을 알아오고싶어요",
          "speaker_id": "2",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100037067.1.12",
          "form": "저는 많이 마셔본건 아니라 ㅋㅋㅋ차이점은 잘모르는디 ㅋㅋㅋㅋ",
          "original_form": "저는 많이 마셔본건 아니라 ㅋㅋㅋ차이점은 잘모르는디 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:38"
        },
        {
          "id": "MDRW2100037067.1.13",
          "form": "맛집은 확실히 다른가요??",
          "original_form": "맛집은 확실히 다른가요??",
          "speaker_id": "1",
          "time": "20210831 19:38"
        },
        {
          "id": "MDRW2100037067.1.14",
          "form": "문래에 있는 한가게",
          "original_form": "문래에 있는 한가게",
          "speaker_id": "2",
          "time": "20210831 19:38"
        },
        {
          "id": "MDRW2100037067.1.15",
          "form": "진토닉이 정말맛있었어요",
          "original_form": "진토닉이 정말맛있었어요",
          "speaker_id": "2",
          "time": "20210831 19:38"
        },
        {
          "id": "MDRW2100037067.1.16",
          "form": "뭘쓰는건지 궁금ㅠ",
          "original_form": "뭘쓰는건지 궁금ㅠ",
          "speaker_id": "2",
          "time": "20210831 19:38"
        },
        {
          "id": "MDRW2100037067.1.17",
          "form": "으앙 진짜 ㅋㅋㅋㅋㅋ비법좀 알려달라고 하고싶네요ㅋㅋㅋㅋ",
          "original_form": "으앙 진짜 ㅋㅋㅋㅋㅋ비법좀 알려달라고 하고싶네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100037067.1.18",
          "form": "그럼 술은 진토닉이 최애고",
          "original_form": "그럼 술은 진토닉이 최애고",
          "speaker_id": "1",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100037067.1.19",
          "form": "맥주도 좋아해요",
          "original_form": "맥주도 좋아해요",
          "speaker_id": "2",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100037067.1.20",
          "form": "맥주나 소주도 좋아하세요??",
          "original_form": "맥주나 소주도 좋아하세요??",
          "speaker_id": "1",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100037067.1.21",
          "form": "소토닉이랑",
          "original_form": "소토닉이랑",
          "speaker_id": "2",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100037067.1.22",
          "form": "청하도 ,,.",
          "original_form": "청하도 ,,.",
          "speaker_id": "2",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100037067.1.23",
          "form": "헐 소토닉은 소주+토닉?!?!?!!!",
          "original_form": "헐 소토닉은 소주+토닉?!?!?!!!",
          "speaker_id": "1",
          "time": "20210831 19:50"
        },
        {
          "id": "MDRW2100037067.1.24",
          "form": "이거도 맛있겠다요 ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "이거도 맛있겠다요 ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 19:50"
        },
        {
          "id": "MDRW2100037067.1.25",
          "form": "소주만 단독으로 마시면 다음날 멍들어있어서 자제해야됩니다,,",
          "original_form": "소주만 단독으로 마시면 다음날 멍들어있어서 자제해야됩니다,,",
          "speaker_id": "2",
          "time": "20210831 19:50"
        },
        {
          "id": "MDRW2100037067.1.26",
          "form": "네 소토닉에 레몬즙넣어서 마시면",
          "original_form": "네 소토닉에 레몬즙넣어서 마시면",
          "speaker_id": "2",
          "time": "20210831 19:50"
        },
        {
          "id": "MDRW2100037067.1.27",
          "form": "최고에요!",
          "original_form": "최고에요!",
          "speaker_id": "2",
          "time": "20210831 19:50"
        }
      ]
    }
