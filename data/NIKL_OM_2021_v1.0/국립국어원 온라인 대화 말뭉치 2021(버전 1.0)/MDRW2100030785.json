      "utterance": [
        {
          "id": "MDRW2100030785.1.1",
          "form": "음료는 어떤 것을 좋아하세요?",
          "original_form": "음료는 어떤 것을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 12:31"
        },
        {
          "id": "MDRW2100030785.1.2",
          "form": "저는 커피종류를 좋아해요",
          "original_form": "저는 커피종류를 좋아해요",
          "speaker_id": "2",
          "time": "20210904 12:31"
        },
        {
          "id": "MDRW2100030785.1.3",
          "form": "아메리카노 좋아하시나요?",
          "original_form": "아메리카노 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 12:31"
        },
        {
          "id": "MDRW2100030785.1.4",
          "form": "피곤할 때 챙겨 마시는 편이에요",
          "original_form": "피곤할 때 챙겨 마시는 편이에요",
          "speaker_id": "2",
          "time": "20210904 12:32"
        },
        {
          "id": "MDRW2100030785.1.5",
          "form": "피곤할 때 마시기에 좋죠! 배달로도 자주 드시나요?",
          "original_form": "피곤할 때 마시기에 좋죠! 배달로도 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030785.1.6",
          "form": "배달로는 자주 안마시는 편이에요",
          "original_form": "배달로는 자주 안마시는 편이에요",
          "speaker_id": "2",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030785.1.7",
          "form": "유명한 카페를 가는 걸 좋아해요",
          "original_form": "유명한 카페를 가는 걸 좋아해요",
          "speaker_id": "2",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100030785.1.8",
          "form": "디저트도 같이 드시나요?",
          "original_form": "디저트도 같이 드시나요?",
          "speaker_id": "1",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030785.1.9",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030785.1.10",
          "form": "식사는 집에서 만들어 드시나요?",
          "original_form": "식사는 집에서 만들어 드시나요?",
          "speaker_id": "2",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100030785.1.11",
          "form": "주로 만들어 먹지만 만들 수 없는 것들은 배달시켜 먹기도 해요",
          "original_form": "주로 만들어 먹지만 만들 수 없는 것들은 배달시켜 먹기도 해요",
          "speaker_id": "1",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030785.1.12",
          "form": "어떤것들을 배달시켜드시나요?",
          "original_form": "어떤것들을 배달시켜드시나요?",
          "speaker_id": "2",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030785.1.13",
          "form": "요즘은 마라탕에 빠져있어요! 부릉님은 배달 음식을 자주 드시나요?",
          "original_form": "요즘은 마라탕에 빠져있어요! 부릉님은 배달 음식을 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210904 12:49"
        },
        {
          "id": "MDRW2100030785.1.14",
          "form": "네 자주 먹어요",
          "original_form": "네 자주 먹어요",
          "speaker_id": "2",
          "time": "20210904 12:50"
        },
        {
          "id": "MDRW2100030785.1.15",
          "form": "요즘은 만들어먹는것보다 배달이 더 양도 많고 간단해서 자주 시켜 먹어요",
          "original_form": "요즘은 만들어먹는것보다 배달이 더 양도 많고 간단해서 자주 시켜 먹어요",
          "speaker_id": "2",
          "time": "20210904 12:50"
        },
        {
          "id": "MDRW2100030785.1.16",
          "form": "배달이 양이 많기도 하지만, 가격이 조금 비싸지 않나요?",
          "original_form": "배달이 양이 많기도 하지만, 가격이 조금 비싸지 않나요?",
          "speaker_id": "1",
          "time": "20210904 13:06"
        },
        {
          "id": "MDRW2100030785.1.17",
          "form": "맞아요 가격이 비싸긴해요",
          "original_form": "맞아요 가격이 비싸긴해요",
          "speaker_id": "2",
          "time": "20210904 13:15"
        },
        {
          "id": "MDRW2100030785.1.18",
          "form": "배달 음식은 주로 뭘 시켜드세요?",
          "original_form": "배달 음식은 주로 뭘 시켜드세요?",
          "speaker_id": "1",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030785.1.19",
          "form": "다양한걸 시켜먹는편이에요",
          "original_form": "다양한걸 시켜먹는편이에요",
          "speaker_id": "2",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030785.1.20",
          "form": "조리하기 어려운 것들을 시켜드시나요?",
          "original_form": "조리하기 어려운 것들을 시켜드시나요?",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030785.1.21",
          "form": "네 육개장 같은 음식들을 시켜먹어요",
          "original_form": "네 육개장 같은 음식들을 시켜먹어요",
          "speaker_id": "2",
          "time": "20210904 13:34"
        }
      ]
