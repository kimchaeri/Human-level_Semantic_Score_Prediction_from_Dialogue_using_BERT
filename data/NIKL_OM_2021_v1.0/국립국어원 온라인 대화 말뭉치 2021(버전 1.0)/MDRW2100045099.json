      "utterance": [
        {
          "id": "MDRW2100045099.1.1",
          "form": "엉 거의 복수극 아니면 악역",
          "original_form": "엉 거의 복수극 아니면 악역",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045099.1.2",
          "form": "갑자기 연기를 잘함",
          "original_form": "갑자기 연기를 잘함",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045099.1.3",
          "form": "결혼하고 더잘함",
          "original_form": "결혼하고 더잘함",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045099.1.4",
          "form": "맞아 예능도 많이 하고",
          "original_form": "맞아 예능도 많이 하고",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045099.1.5",
          "form": "성격도 좋은듯",
          "original_form": "성격도 좋은듯",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045099.1.6",
          "form": "응 요리도 잘하는듯",
          "original_form": "응 요리도 잘하는듯",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045099.1.7",
          "form": "펀레스트롱에서 출연",
          "original_form": "펀레스트롱에서 출연",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045099.1.8",
          "form": "판소리도 배운다고 하던데",
          "original_form": "판소리도 배운다고 하던데",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045099.1.9",
          "form": "엉 영화인가 했잖아",
          "original_form": "엉 영화인가 했잖아",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045099.1.10",
          "form": "맞다 광대",
          "original_form": "맞다 광대",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045099.1.11",
          "form": "거의 관객이 없었다",
          "original_form": "거의 관객이 없었다",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
