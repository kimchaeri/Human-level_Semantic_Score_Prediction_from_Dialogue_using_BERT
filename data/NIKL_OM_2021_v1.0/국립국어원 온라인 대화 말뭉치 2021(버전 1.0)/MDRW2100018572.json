        {
          "id": "MDRW2100018572.1.1",
          "form": "재택근무하시나요?",
          "original_form": "재택근무하시나요?",
          "speaker_id": "2",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018572.1.2",
          "form": "예전에 했는데 지금은 안해요",
          "original_form": "예전에 했는데 지금은 안해요",
          "speaker_id": "1",
          "time": "20210827 10:15"
        },
        {
          "id": "MDRW2100018572.1.3",
          "form": "재택근무 하셨을 때 어떠셨어요?",
          "original_form": "재택근무 하셨을 때 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210827 10:23"
        },
        {
          "id": "MDRW2100018572.1.4",
          "form": "좋을줄알았는데 별로",
          "original_form": "좋을줄알았는데 별로",
          "speaker_id": "1",
          "time": "20210827 11:13"
        },
        {
          "id": "MDRW2100018572.1.5",
          "form": "어떤점이요?",
          "original_form": "어떤점이요?",
          "speaker_id": "2",
          "time": "20210827 11:14"
        },
        {
          "id": "MDRW2100018572.1.6",
          "form": "육아 집안일을 같이하는ㅠ",
          "original_form": "육아 집안일을 같이하는ㅠ",
          "speaker_id": "1",
          "time": "20210827 11:14"
        },
        {
          "id": "MDRW2100018572.1.7",
          "form": "아..ㅠ 배로 힘드셨겠네요",
          "original_form": "아..ㅠ 배로 힘드셨겠네요",
          "speaker_id": "2",
          "time": "20210827 11:18"
        },
        {
          "id": "MDRW2100018572.1.8",
          "form": "그래서 출근해요ㅋ",
          "original_form": "그래서 출근해요ㅋ",
          "speaker_id": "1",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018572.1.9",
          "form": "ㅋㅋ그게 나으시군요",
          "original_form": "ㅋㅋ그게 나으시군요",
          "speaker_id": "2",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018572.1.10",
          "form": "여긴 커피도 있구ㅠㅠ흑",
          "original_form": "여긴 커피도 있구ㅠㅠ흑",
          "speaker_id": "1",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018572.1.11",
          "form": "흡ㅜ 그쵸",
          "original_form": "흡ㅜ 그쵸",
          "speaker_id": "2",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018572.1.12",
          "form": "병행하는건 힘들어요",
          "original_form": "병행하는건 힘들어요",
          "speaker_id": "2",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018572.1.13",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018572.1.14",
          "form": "그래도 코로나라 어쩔 수 없이 하는경우가 많은거 같더라구요",
