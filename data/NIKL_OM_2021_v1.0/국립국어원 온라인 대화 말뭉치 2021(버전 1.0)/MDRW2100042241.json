      "utterance": [
        {
          "id": "MDRW2100042241.1.1",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100042241.1.2",
          "form": "네 갈치구이 해서 먹었답니다",
          "original_form": "네 갈치구이 해서 먹었답니다",
          "speaker_id": "2",
          "time": "20210906 21:02"
        },
        {
          "id": "MDRW2100042241.1.3",
          "form": "name1님은 뭐 드셨어요?",
          "original_form": "&name1&님은 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210906 21:02"
        },
        {
          "id": "MDRW2100042241.1.4",
          "form": "저는 치킨 먹었어요",
          "original_form": "저는 치킨 먹었어요",
          "speaker_id": "1",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100042241.1.5",
          "form": "맛난거 드셨네요ㅠ 갈치 좋아하세요?",
          "original_form": "맛난거 드셨네요ㅠ 갈치 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100042241.1.6",
          "form": "치킨이 더 맛있는 거 아닌가요? ㅎㅎ",
          "original_form": "치킨이 더 맛있는 거 아닌가요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:04"
        },
        {
          "id": "MDRW2100042241.1.7",
          "form": "가리는 음식 없어서 갈치도 좋아해요",
          "original_form": "가리는 음식 없어서 갈치도 좋아해요",
          "speaker_id": "2",
          "time": "20210906 21:04"
        },
        {
          "id": "MDRW2100042241.1.8",
          "form": "사실 남편이 워낙 좋아해서 남편 위해 했어요",
          "original_form": "사실 남편이 워낙 좋아해서 남편 위해 했어요",
          "speaker_id": "2",
          "time": "20210906 21:04"
        },
        {
          "id": "MDRW2100042241.1.9",
          "form": "근데 전 생선 엄청 좋아하거든요 치킨도 맛있지만ㅎㅎ...",
          "original_form": "근데 전 생선 엄청 좋아하거든요 치킨도 맛있지만ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042241.1.10",
          "form": "갈치말고 또 좋아하는 음식있으세요?",
          "original_form": "갈치말고 또 좋아하는 음식있으세요?",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042241.1.11",
          "form": "저는 물고기보다 육고기를 좋아해서요 ㅎㅎ",
          "original_form": "저는 물고기보다 육고기를 좋아해서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042241.1.12",
          "form": "남편은 해산물을 좋아하고",
          "original_form": "남편은 해산물을 좋아하고",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042241.1.13",
          "form": "근데 제가 요리하니까 보통 돼지 소 닭 오리 이런게 올라와요",
          "original_form": "근데 제가 요리하니까 보통 돼지 소 닭 오리 이런게 올라와요",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042241.1.14",
          "form": "가끔 생선 올리구요 ㅎㅎ",
          "original_form": "가끔 생선 올리구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042241.1.15",
          "form": "요리하는 자의 특권이네요 저도 육고기 엄청좋아해요!",
          "original_form": "요리하는 자의 특권이네요 저도 육고기 엄청좋아해요!",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042241.1.16",
          "form": "사실 싫어하는 사람이 있을까싶은ㅜ",
          "original_form": "사실 싫어하는 사람이 있을까싶은ㅜ",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042241.1.17",
          "form": "그렇죠 요리하는 사람 맘이죠 ㅎㅎ",
          "original_form": "그렇죠 요리하는 사람 맘이죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042241.1.18",
          "form": "name1님은 배달음식도 종종 시켜먹나요?",
          "original_form": "&name1&님은 배달음식도 종종 시켜먹나요?",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042241.1.19",
          "form": "네 종종 시켜먹어요!",
          "original_form": "네 종종 시켜먹어요!",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042241.1.20",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042241.1.21",
          "form": "저는 배달음식보다 제가 요리하는 걸 좋아해서 잘 안시키는데",
          "original_form": "저는 배달음식보다 제가 요리하는 걸 좋아해서 잘 안시키는데",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042241.1.22",
          "form": "가족들이 먹고 싶다고 특히 딸이 그럴때 가끔 시켜요",
          "original_form": "가족들이 먹고 싶다고 특히 딸이 그럴때 가끔 시켜요",
          "speaker_id": "2",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042241.1.23",
          "form": "아하 따님분 배달음식 뭐뭐좋아하세요?",
          "original_form": "아하 따님분 배달음식 뭐뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042241.1.24",
          "form": "너무나 평범하게 치킨 피자 같은거요",
          "original_form": "너무나 평범하게 치킨 피자 같은거요",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042241.1.25",
          "form": "가끔 마라탕",
          "original_form": "가끔 마라탕",
