      "utterance": [
        {
          "id": "MDRW2100030243.1.1",
          "form": "좋아하는 음료 있으신가오?",
          "original_form": "좋아하는 음료 있으신가오?",
          "speaker_id": "1",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030243.1.2",
          "form": "이번에 생겼어요",
          "original_form": "이번에 생겼어요",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030243.1.3",
          "form": "탄산음료인데",
          "original_form": "탄산음료인데",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030243.1.4",
          "form": "잠시만요",
          "original_form": "잠시만요",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030243.1.5",
          "form": "분다버그",
          "original_form": "분다버그",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030243.1.6",
          "form": "향도 진하고",
          "original_form": "향도 진하고",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030243.1.7",
          "form": "유리병에 들어있는 음료에요",
          "original_form": "유리병에 들어있는 음료에요",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030243.1.8",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030243.1.9",
          "form": "요즘 좋아하는 음료가 어떤건가요??",
          "original_form": "요즘 좋아하는 음료가 어떤건가요??",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030243.1.10",
          "form": "오오 문다버그 무슨색이요?",
          "original_form": "오오 문다버그 무슨색이요?",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030243.1.11",
          "form": "그 진저랑 핑크 있지 않나??",
          "original_form": "그 진저랑 핑크 있지 않나??",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030243.1.12",
          "form": "요즘 제로콜라에 빠졌슴니댜..",
          "original_form": "요즘 제로콜라에 빠졌슴니댜..",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030243.1.13",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030243.1.14",
          "form": "진저는 맛없어요",
          "original_form": "진저는 맛없어요",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030243.1.15",
          "form": "핑크랑 라임 맛있어요",
          "original_form": "핑크랑 라임 맛있어요",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030243.1.16",
          "form": "제로콜라?",
          "original_form": "제로콜라?",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030243.1.17",
          "form": "코카콜라 제로???",
          "original_form": "코카콜라 제로???",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100030243.1.18",
          "form": "그냥 콜라랑 차이가 있나요??",
          "original_form": "그냥 콜라랑 차이가 있나요??",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030243.1.19",
          "form": "네넹 제로는 그 칼로리가 0이라 먹을때 죄책감 없이 묵어도 되오ㅋㅋㅋ",
          "original_form": "네넹 제로는 그 칼로리가 0이라 먹을때 죄책감 없이 묵어도 되오ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030243.1.20",
          "form": "그래서 맘껏 먹는ㅋㅋ",
          "original_form": "그래서 맘껏 먹는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030243.1.21",
          "form": "오 라임은 안묵어 봤엉",
          "original_form": "오 라임은 안묵어 봤엉",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030243.1.22",
          "form": "맛있나오?",
          "original_form": "맛있나오?",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030243.1.23",
          "form": "오늘 먹어볼꺼예요",
          "original_form": "오늘 먹어볼꺼예요",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030243.1.24",
          "form": "핑크는 몇일전에  먹었는데 탄산도 많은것같고",
          "original_form": "핑크는 몇일전에  먹었는데 탄산도 많은것같고",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030243.1.25",
          "form": "오오 먹고 카톡으로 알려주세오ㅋㅋ",
          "original_form": "오오 먹고 카톡으로 알려주세오ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030243.1.26",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030243.1.27",
          "form": "핑크는 무슨 맛이징?",
          "original_form": "핑크는 무슨 맛이징?",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030243.1.28",
          "form": "네~~~~~~",
          "original_form": "네~~~~~~",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030243.1.29",
          "form": "안먹은지 오래됨",
          "original_form": "안먹은지 오래됨",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030243.1.30",
          "form": "핑크는 자몽입니다",
          "original_form": "핑크는 자몽입니다",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030243.1.31",
          "form": "처음 먹었는데 맛있었어",
          "original_form": "처음 먹었는데 맛있었어",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030243.1.32",
          "form": "오오 맛있겠당",
          "original_form": "오오 맛있겠당",
          "speaker_id": "1",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030243.1.33",
          "form": "담에 카페가서 발견하면 묵겠어ㅋㅋ",
          "original_form": "담에 카페가서 발견하면 묵겠어ㅋㅋ",
