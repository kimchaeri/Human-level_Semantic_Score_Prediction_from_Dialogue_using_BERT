      "utterance": [
        {
          "id": "MDRW2100006234.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006234.1.2",
          "form": "게임하세요?",
          "original_form": "게임하세요?",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006234.1.3",
          "form": "네 저 게임 좋아합니다!",
          "original_form": "네 저 게임 좋아합니다!",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006234.1.4",
          "form": "요새 폰으로 원신합니다",
          "original_form": "요새 폰으로 원신합니다",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006234.1.5",
          "form": "헉? 어떤게임이에요?",
          "original_form": "헉? 어떤게임이에요?",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006234.1.6",
          "form": "약간.제한적인 오픈월드 게임이고 대체적으로 스토리빼고는 평범한 rpg게임 이에요",
          "original_form": "약간.제한적인 오픈월드 게임이고 대체적으로 스토리빼고는 평범한 rpg게임 이에요",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100006234.1.7",
          "form": "2D느낌이 있어서 좋아합니다",
          "original_form": "2D느낌이 있어서 좋아합니다",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100006234.1.8",
          "form": "오오 저도 2d rpg인 메이플 하고있는데",
          "original_form": "오오 저도 2d rpg인 메이플 하고있는데",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006234.1.9",
          "form": "rpg좋아하시나봐요",
          "original_form": "rpg좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006234.1.10",
          "form": "네 레벨업하고 모험하는거 좋아해요 메이플도 재미있는 게임이죠 추억의 게임이네요 ㅋㅋㅋ",
          "original_form": "네 레벨업하고 모험하는거 좋아해요 메이플도 재미있는 게임이죠 추억의 게임이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100006234.1.11",
          "form": "ㅋㅋㅋ맞아요 해보셨어요?",
          "original_form": "ㅋㅋㅋ맞아요 해보셨어요?",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006234.1.12",
          "form": "예전에 초등학교때 했었어요 ㅋㅋㅋ 지금도 엄청 유저들이 많다고 들었어요~",
          "original_form": "예전에 초등학교때 했었어요 ㅋㅋㅋ 지금도 엄청 유저들이 많다고 들었어요~",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006234.1.13",
          "form": "맞아요 근데 좀 많이 떠나긴했어요ㅠㅜ",
          "original_form": "맞아요 근데 좀 많이 떠나긴했어요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100006234.1.14",
          "form": "맞아요 ㅠㅜ",
          "original_form": "맞아요 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006234.1.15",
          "form": "근데 좀 위태한거같아요",
          "original_form": "근데 좀 위태한거같아요",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006234.1.16",
          "form": "그래도 아직 인기게임이지않나요 메이플스토리?ㅋ",
          "original_form": "그래도 아직 인기게임이지않나요 메이플스토리?ㅋ",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100006234.1.17",
          "form": "그런가요?? 요즘 다른 게임에 많이 밀린것같던데",
          "original_form": "그런가요?? 요즘 다른 게임에 많이 밀린것같던데",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100006234.1.18",
          "form": "요새는 로스트아크가 인기더라구요",
          "original_form": "요새는 로스트아크가 인기더라구요",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100006234.1.19",
          "form": "로아도 해보셨어요?",
          "original_form": "로아도 해보셨어요?",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100006234.1.20",
          "form": "아뇨 로아는 방송으로만 봤어요 3D게임 안좋아해요",
          "original_form": "아뇨 로아는 방송으로만 봤어요 3D게임 안좋아해요",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100006234.1.21",
          "form": "비슷한 블소는 해본적있습니다 블레이드앤소울",
          "original_form": "비슷한 블소는 해본적있습니다 블레이드앤소울",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100006234.1.22",
          "form": "블소도 유명하잖아요 많이 들어봤어요",
          "original_form": "블소도 유명하잖아요 많이 들어봤어요",
