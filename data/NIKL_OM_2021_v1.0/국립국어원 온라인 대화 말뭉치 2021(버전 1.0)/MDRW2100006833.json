      "utterance": [
        {
          "id": "MDRW2100006833.1.1",
          "form": "마지막으로 본 영화는요?",
          "original_form": "마지막으로 본 영화는요?",
          "speaker_id": "2",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100006833.1.2",
          "form": "넷플릭스로 바람의 검신봤어요.",
          "original_form": "넷플릭스로 바람의 검신봤어요.",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006833.1.3",
          "form": "모르는영화네요...",
          "original_form": "모르는영화네요...",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006833.1.4",
          "form": "일본 영화에요!!",
          "original_form": "일본 영화에요!!",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006833.1.5",
          "form": "전에 봤었는데 이전에 마지막으로 나왔어요",
          "original_form": "전에 봤었는데 이전에 마지막으로 나왔어요",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006833.1.6",
          "form": "그러시군요 극장은요?",
          "original_form": "그러시군요 극장은요?",
          "speaker_id": "2",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100006833.1.7",
          "form": "극장은 안간지 꽤 됐네요ㅠㅠ",
          "original_form": "극장은 안간지 꽤 됐네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100006833.1.8",
          "form": "코로나 터지고 한 번을 안갔으니ㅠㅠ",
          "original_form": "코로나 터지고 한 번을 안갔으니ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100006833.1.9",
          "form": "그러시군요 2년 다되가겠어요",
          "original_form": "그러시군요 2년 다되가겠어요",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100006833.1.10",
          "form": "네ㅠㅠ 거의 2년 전후로 된것같아요ㅠㅠ",
          "original_form": "네ㅠㅠ 거의 2년 전후로 된것같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100006833.1.11",
          "form": "기억도 안나겠어요",
          "original_form": "기억도 안나겠어요",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100006833.1.12",
          "form": "넼ㅋㅋㅋㅋ 영화관가서 크게 큰 소리로 영화보고 싶어요ㅠㅠ",
          "original_form": "넼ㅋㅋㅋㅋ 영화관가서 크게 큰 소리로 영화보고 싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006833.1.13",
          "form": "콘서트도 마찬가지겠어요",
          "original_form": "콘서트도 마찬가지겠어요",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006833.1.14",
          "form": "네ㅠㅠㅠㅠㅠ",
          "original_form": "네ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006833.1.15",
          "form": "작년에 좋아하는 밴드 내한온다고해서 좋은자리 잘 잡았는뎈ㅋㅋㅋ 그대로 취소돼서 너무 슬펐어요ㅠㅠ",
          "original_form": "작년에 좋아하는 밴드 내한온다고해서 좋은자리 잘 잡았는뎈ㅋㅋㅋ 그대로 취소돼서 너무 슬펐어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006833.1.16",
          "form": "누구인가요ㅡ 모르겠지만 ㅎㅎ",
          "original_form": "누구인가요ㅡ 모르겠지만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006833.1.17",
          "form": "one ok rock이라고 하는 밴드에요.",
          "original_form": "one ok rock이라고 하는 밴드에요.",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006833.1.18",
          "form": "불매운동에 참여는 하지만 이 밴드 만큼은 못하겠도라구요ㅠㅍ",
          "original_form": "불매운동에 참여는 하지만 이 밴드 만큼은 못하겠도라구요ㅠㅍ",
