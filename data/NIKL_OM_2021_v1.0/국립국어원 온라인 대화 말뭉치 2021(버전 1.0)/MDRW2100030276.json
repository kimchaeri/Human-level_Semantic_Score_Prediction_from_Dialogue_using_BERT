      "utterance": [
        {
          "id": "MDRW2100030276.1.1",
          "form": "고양이 좋아하세요?",
          "original_form": "고양이 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030276.1.2",
          "form": "전 고양이파입니다 ㅋㅋㅋㅋㅋ",
          "original_form": "전 고양이파입니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030276.1.3",
          "form": "저도 고양이파에요 ㅋㅋㅋ",
          "original_form": "저도 고양이파에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030276.1.4",
          "form": "근데 키울순 없어요",
          "original_form": "근데 키울순 없어요",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030276.1.5",
          "form": "그쵸 저도 키울 순 없어요",
          "original_form": "그쵸 저도 키울 순 없어요",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030276.1.6",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030276.1.7",
          "form": "집에 알레르기 있는 사람이 있어서",
          "original_form": "집에 알레르기 있는 사람이 있어서",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030276.1.8",
          "form": "저는 털을 감당할수가 없을거 같아요",
          "original_form": "저는 털을 감당할수가 없을거 같아요",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030276.1.9",
          "form": "아 그것도 그래요",
          "original_form": "아 그것도 그래요",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030276.1.10",
          "form": "제 털도 감당이안되긴해요",
          "original_form": "제 털도 감당이안되긴해요",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030276.1.11",
          "form": "근데 반려동물 키우면 안외롭다고하기도 하더라구요",
          "original_form": "근데 반려동물 키우면 안외롭다고하기도 하더라구요",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030276.1.12",
          "form": "ㅋㅋㅋㅋㅋ 맞아요 제털도 힘들어요",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요 제털도 힘들어요",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030276.1.13",
          "form": "키우는 친구를 봤었는데",
          "original_form": "키우는 친구를 봤었는데",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030276.1.14",
          "form": "검은옷을 입을수가 없더라고요",
          "original_form": "검은옷을 입을수가 없더라고요",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030276.1.15",
          "form": "아...맞아요",
          "original_form": "아...맞아요",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030276.1.16",
          "form": "친구집에놀러갔는데",
          "original_form": "친구집에놀러갔는데",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030276.1.17",
          "form": "청소기돌려도",
          "original_form": "청소기돌려도",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030276.1.18",
          "form": "발에 털이 밟혀요",
          "original_form": "발에 털이 밟혀요",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030276.1.19",
          "form": "제털도 밟히긴하지만",
          "original_form": "제털도 밟히긴하지만",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030276.1.20",
          "form": "암튼 청소를 두배로 열심히 해야항거같아요",
          "original_form": "암튼 청소를 두배로 열심히 해야항거같아요",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030276.1.21",
          "form": "악 절대",
          "original_form": "악 절대",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030276.1.22",
          "form": "저는 못키우겠네요",
          "original_form": "저는 못키우겠네요",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030276.1.23",
          "form": "역시랜선집사가체질인가봐요",
          "original_form": "역시랜선집사가체질인가봐요",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030276.1.24",
          "form": "사진보는정도가 딱 ㅋㅋㅋ",
          "original_form": "사진보는정도가 딱 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030276.1.25",
          "form": "생명을 키우는게 보통일이 아니니까",
          "original_form": "생명을 키우는게 보통일이 아니니까",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030276.1.26",
          "form": "그쵸... 제 생명 책임지기도 힘들어요",
          "original_form": "그쵸... 제 생명 책임지기도 힘들어요",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030276.1.27",
          "form": "아니 계속 너무 비관적인가요..?ㅎㅎㅎ",
          "original_form": "아니 계속 너무 비관적인가요..?ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030276.1.28",
          "form": "ㅋㅋㅋ 맞는 말이십니다",
          "original_form": "ㅋㅋㅋ 맞는 말이십니다",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030276.1.29",
          "form": "틀린말 안하시는데요",
          "original_form": "틀린말 안하시는데요",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030276.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ name1님이랑 잘 맞는거 같아서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ &name1&님이랑 잘 맞는거 같아서",
          "speaker_id": "2",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030276.1.31",
          "form": "대화가 즐겁네요 ㅎㅎ",
          "original_form": "대화가 즐겁네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030276.1.32",
          "form": "저도 잘 맞는 분이랑 오래 대화해서 즐거워요ㅋㅋㅋ",
          "original_form": "저도 잘 맞는 분이랑 오래 대화해서 즐거워요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030276.1.33",
          "form": "벌써 하루가 갔네욬ㅋㅋ",
          "original_form": "벌써 하루가 갔네욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030276.1.34",
          "form": "그러니까욬ㅋㅋㅋㅋ 벌써 12시 ㅎㅎ",
          "original_form": "그러니까욬ㅋㅋㅋㅋ 벌써 12시 ㅎㅎ",
