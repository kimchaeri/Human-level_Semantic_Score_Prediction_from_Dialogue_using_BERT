      "utterance": [
        {
          "id": "MDRW2100010173.1.1",
          "form": "최근에 뭐 산 거 있어?",
          "original_form": "최근에 뭐 산 거 있어?",
          "speaker_id": "1",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100010173.1.2",
          "form": "나는 사실 물욕이 잘 없어",
          "original_form": "나는 사실 물욕이 잘 없어",
          "speaker_id": "2",
          "time": "20210823 15:09"
        },
        {
          "id": "MDRW2100010173.1.3",
          "form": "율 너는 뭐 산 거 있어?",
          "original_form": "율 너는 뭐 산 거 있어?",
          "speaker_id": "2",
          "time": "20210823 15:10"
        },
        {
          "id": "MDRW2100010173.1.4",
          "form": "나 세탁기에 버즈라이브 넣고 돌려서",
          "original_form": "나 세탁기에 버즈라이브 넣고 돌려서",
          "speaker_id": "1",
          "time": "20210823 15:12"
        },
        {
          "id": "MDRW2100010173.1.5",
          "form": "케이스 다시 새로 샀어 5만원에",
          "original_form": "케이스 다시 새로 샀어 5만원에",
          "speaker_id": "1",
          "time": "20210823 15:12"
        },
        {
          "id": "MDRW2100010173.1.6",
          "form": "와우 마음이 너무 아팠겠구나",
          "original_form": "와우 마음이 너무 아팠겠구나",
          "speaker_id": "2",
          "time": "20210823 15:12"
        },
        {
          "id": "MDRW2100010173.1.7",
          "form": "케이스는 어때 마음에 들어 ?",
          "original_form": "케이스는 어때 마음에 들어 ?",
          "speaker_id": "2",
          "time": "20210823 15:13"
        },
        {
          "id": "MDRW2100010173.1.8",
          "form": "같은 걸 산 거니까",
          "original_form": "같은 걸 산 거니까",
          "speaker_id": "1",
          "time": "20210823 15:17"
        },
        {
          "id": "MDRW2100010173.1.9",
          "form": "마음에 들지만 통장은 울고 있지",
          "original_form": "마음에 들지만 통장은 울고 있지",
          "speaker_id": "1",
          "time": "20210823 15:18"
        },
        {
          "id": "MDRW2100010173.1.10",
          "form": "마음이 아프구나",
          "original_form": "마음이 아프구나",
          "speaker_id": "2",
          "time": "20210823 15:18"
        },
        {
          "id": "MDRW2100010173.1.11",
          "form": "중고거래 이런 거 해본 적 없어?",
          "original_form": "중고거래 이런 거 해본 적 없어?",
          "speaker_id": "1",
          "time": "20210823 15:18"
        },
        {
          "id": "MDRW2100010173.1.12",
          "form": "나는 판매 자주 해",
          "original_form": "나는 판매 자주 해",
          "speaker_id": "2",
          "time": "20210823 15:18"
        },
        {
          "id": "MDRW2100010173.1.13",
          "form": "너는 어때",
          "original_form": "너는 어때",
          "speaker_id": "2",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010173.1.14",
          "form": "나는 판매도 하고 구매도 해",
          "original_form": "나는 판매도 하고 구매도 해",
          "speaker_id": "1",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010173.1.15",
          "form": "당근 많이 써",
          "original_form": "당근 많이 써",
          "speaker_id": "1",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010173.1.16",
          "form": "나도 당근은 많이 있어",
          "original_form": "나도 당근은 많이 있어",
          "speaker_id": "2",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010173.1.17",
          "form": "무나 받을 때 특히",
          "original_form": "무나 받을 때 특히",
          "speaker_id": "1",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010173.1.18",
          "form": "많이 써",
          "original_form": "많이 써",
          "speaker_id": "2",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010173.1.19",
          "form": "싸게 올라와서 좋아",
          "original_form": "싸게 올라와서 좋아",
          "speaker_id": "1",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010173.1.20",
          "form": "그것도 있고 약간 온도 오르면 뿌듯하지않니",
          "original_form": "그것도 있고 약간 온도 오르면 뿌듯하지않니",
          "speaker_id": "2",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010173.1.21",
          "form": "맞아 그거 때문에 친절도 신경 쓰게 되는 거 같아ㅋㅋㄱ",
          "original_form": "맞아 그거 때문에 친절도 신경 쓰게 되는 거 같아ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210823 15:20"
        }
      ]
