      "utterance": [
        {
          "id": "MDRW2100015624.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015624.1.2",
          "form": "여행 많이 다니시나여?",
          "original_form": "여행 많이 다니시나여?",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015624.1.3",
          "form": "저는 부산으로 여행 자주 가는것 같아요!",
          "original_form": "저는 부산으로 여행 자주 가는것 같아요!",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015624.1.4",
          "form": "부산이 가까워서요?",
          "original_form": "부산이 가까워서요?",
          "speaker_id": "1",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015624.1.5",
          "form": "부산이 멀긴하지만 바다도 있고 맛집도 많아서 좋은것 같아서요",
          "original_form": "부산이 멀긴하지만 바다도 있고 맛집도 많아서 좋은것 같아서요",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015624.1.6",
          "form": "바다도 들어가시나여?",
          "original_form": "바다도 들어가시나여?",
          "speaker_id": "1",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015624.1.7",
          "form": "남천동도 가보셧나여? ㅋㅋㅋ",
          "original_form": "남천동도 가보셧나여? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015624.1.8",
          "form": "오 남천동은 잘 안가본것 같아요 추천해주실만한 관광지 있으신가요!",
          "original_form": "오 남천동은 잘 안가본것 같아요 추천해주실만한 관광지 있으신가요!",
          "speaker_id": "2",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015624.1.9",
          "form": "전 집돌이라.. 잘몰라요... \n부산은 배타러 가본거 같네요..",
          "original_form": "전 집돌이라.. 잘몰라요... \n부산은 배타러 가본거 같네요..",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015624.1.10",
          "form": "일본갈때",
          "original_form": "일본갈때",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015624.1.11",
          "form": "ㅋㅋㅋ 아 그럼 대마도도 가보신건가요?",
          "original_form": "ㅋㅋㅋ 아 그럼 대마도도 가보신건가요?",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015624.1.12",
          "form": "아뇨.. 후쿠오카 갈때요... \n학생때라 돈 아낄려고",
          "original_form": "아뇨.. 후쿠오카 갈때요... \n학생때라 돈 아낄려고",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015624.1.13",
          "form": "기차타고 부산가서 배로 ㅎㅎㅎ",
          "original_form": "기차타고 부산가서 배로 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015624.1.14",
          "form": "오 저도 후쿠오카 부산에서 가본적 있어요",
          "original_form": "오 저도 후쿠오카 부산에서 가본적 있어요",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015624.1.15",
          "form": "해외도 자주다니시나여?",
          "original_form": "해외도 자주다니시나여?",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015624.1.16",
          "form": "최근에는 해외여항ㅣ 가본지 오래인것같아요",
          "original_form": "최근에는 해외여항ㅣ 가본지 오래인것같아요",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015624.1.17",
          "form": "다들 그렇죠... 코로나땜시",
          "original_form": "다들 그렇죠... 코로나땜시",
          "speaker_id": "1",
