      "utterance": [
        {
          "id": "MDRW2100037417.1.1",
          "form": "저도! 거침없이 때 티비 잘 안 봐서",
          "original_form": "저도! 거침없이 때 티비 잘 안 봐서",
          "speaker_id": "1",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100037417.1.2",
          "form": "못 봤고 지붕뚫고를 엄청 열심히 봤어요",
          "original_form": "못 봤고 지붕뚫고를 엄청 열심히 봤어요",
          "speaker_id": "1",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100037417.1.3",
          "form": "3은 별로 못 봤자먼..!",
          "original_form": "3은 별로 못 봤자먼..!",
          "speaker_id": "1",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100037417.1.4",
          "form": "저두요 ! 3은..",
          "original_form": "저두요 ! 3은..",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100037417.1.5",
          "form": "재미없어요 ㅎㅎㅎ",
          "original_form": "재미없어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100037417.1.6",
          "form": "박하선만 좀 웃기더라고요 ㅠ.^",
          "original_form": "박하선만 좀 웃기더라고요 ㅠ.^",
          "speaker_id": "1",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100037417.1.7",
          "form": "아 마자욬ㅋㅋㅋ",
          "original_form": "아 마자욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:30"
        },
        {
          "id": "MDRW2100037417.1.8",
          "form": "박하선이 다 살린 ㅜㅜ",
          "original_form": "박하선이 다 살린 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 23:30"
        },
        {
          "id": "MDRW2100037417.1.9",
          "form": "지붕뚫고 누구 좋아하셨어요?",
          "original_form": "지붕뚫고 누구 좋아하셨어요?",
          "speaker_id": "2",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100037417.1.10",
          "form": "저는 아역들이 너무 좋았어요 ㅠ.^",
          "original_form": "저는 아역들이 너무 좋았어요 ㅠ.^",
          "speaker_id": "1",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100037417.1.11",
          "form": "둘 케미가 너무 눗겨서!!",
          "original_form": "둘 케미가 너무 눗겨서!!",
          "speaker_id": "1",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100037417.1.12",
          "form": "아아 빵꾸똥꾸!",
          "original_form": "아아 빵꾸똥꾸!",
          "speaker_id": "2",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100037417.1.13",
          "form": "그 마지막에 슬펐어요",
          "original_form": "그 마지막에 슬펐어요",
          "speaker_id": "2",
          "time": "20210831 23:32"
        },
        {
          "id": "MDRW2100037417.1.14",
          "form": "이민가지말라구 ㅠ 인형주는거",
          "original_form": "이민가지말라구 ㅠ 인형주는거",
          "speaker_id": "2",
          "time": "20210831 23:32"
        },
        {
          "id": "MDRW2100037417.1.15",
          "form": "헐 저도요 ㅠㅠ 그거랑 학교 다녀왔나? 그 뒤로",
          "original_form": "헐 저도요 ㅠㅠ 그거랑 학교 다녀왔나? 그 뒤로",
          "speaker_id": "1",
          "time": "20210831 23:50"
        },
        {
          "id": "MDRW2100037417.1.16",
          "form": "없어져서 혼자 엉엉 울던거 ㅠㅠ",
          "original_form": "없어져서 혼자 엉엉 울던거 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 23:50"
        },
        {
          "id": "MDRW2100037417.1.17",
          "form": "아 맞아요 ㅜㅜ",
          "original_form": "아 맞아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100037417.1.18",
          "form": "지붕뚫은 결말이.. 별로였죠",
          "original_form": "지붕뚫은 결말이.. 별로였죠",
          "speaker_id": "2",
          "time": "20210831 23:58"
        },
        {
          "id": "MDRW2100037417.1.19",
          "form": "네!!!! 아니 진짜 왜 거기서",
          "original_form": "네!!!! 아니 진짜 왜 거기서",
          "speaker_id": "1",
          "time": "20210901 00:08"
        },
        {
          "id": "MDRW2100037417.1.20",
          "form": "갑자기 다니엘이랑 신세경을 죽이는지",
          "original_form": "갑자기 다니엘이랑 신세경을 죽이는지",
          "speaker_id": "1",
          "time": "20210901 00:08"
        },
        {
          "id": "MDRW2100037417.1.21",
          "form": "솔직히 신세경 너무 불쌍한 인생 아니냐고요 ㅠㅠ",
          "original_form": "솔직히 신세경 너무 불쌍한 인생 아니냐고요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 00:08"
        },
        {
          "id": "MDRW2100037417.1.22",
          "form": "맞아요!!! 하 진짜",
          "original_form": "맞아요!!! 하 진짜",
          "speaker_id": "2",
          "time": "20210901 00:18"
        },
        {
          "id": "MDRW2100037417.1.23",
          "form": "그 작가는 새드를 넘 좋아해여...",
          "original_form": "그 작가는 새드를 넘 좋아해여...",
          "speaker_id": "2",
          "time": "20210901 00:18"
        },
        {
          "id": "MDRW2100037417.1.24",
          "form": "아 그니까요 그리고 막 이유있믄 새드가 아니라",
          "original_form": "아 그니까요 그리고 막 이유있믄 새드가 아니라",
          "speaker_id": "1",
          "time": "20210901 00:27"
        },
        {
          "id": "MDRW2100037417.1.25",
          "form": "너무 뜬금없는 새드에요 -.-",
          "original_form": "너무 뜬금없는 새드에요 -.-",
          "speaker_id": "1",
          "time": "20210901 00:27"
        },
        {
          "id": "MDRW2100037417.1.26",
          "form": "마자요 ㅋㄱㄲ",
          "original_form": "마자요 ㅋㄱㄲ",
          "speaker_id": "2",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100037417.1.27",
          "form": "가끔 무섭기도 한 결말이에요 !",
          "original_form": "가끔 무섭기도 한 결말이에요 !",
          "speaker_id": "2",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100037417.1.28",
          "form": "거침없이도 이상했던거같은데...",
          "original_form": "거침없이도 이상했던거같은데...",
          "speaker_id": "2",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100037417.1.29",
          "form": "맞아요 거침없이 진짜",
          "original_form": "맞아요 거침없이 진짜",
          "speaker_id": "1",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100037417.1.30",
          "form": "무서운 장면 나오잖아요",
          "original_form": "무서운 장면 나오잖아요",
          "speaker_id": "1",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100037417.1.31",
          "form": "그 할머니? 탈옥이랑",
          "original_form": "그 할머니? 탈옥이랑",
          "speaker_id": "1",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100037417.1.32",
          "form": "박민영이 간처1인거..",
          "original_form": "박민영이 간처1인거..",
          "speaker_id": "1",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100037417.1.33",
          "form": "어라라 간  첩",
          "original_form": "어라라 간  첩",
          "speaker_id": "1",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100037417.1.34",
          "form": "박민영 아빤가 좀 범인 의심이였자나여",
          "original_form": "박민영 아빤가 좀 범인 의심이였자나여",
