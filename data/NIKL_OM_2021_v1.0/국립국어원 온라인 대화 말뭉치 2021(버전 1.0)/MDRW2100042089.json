      "utterance": [
        {
          "id": "MDRW2100042089.1.1",
          "form": "영양제 드시는거 있나요",
          "original_form": "영양제 드시는거 있나요",
          "speaker_id": "1",
          "time": "20210906 19:35"
        },
        {
          "id": "MDRW2100042089.1.2",
          "form": "영양제드세요?",
          "original_form": "영양제드세요?",
          "speaker_id": "1",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042089.1.3",
          "form": "비타민만 먹고있어요",
          "original_form": "비타민만 먹고있어요",
          "speaker_id": "2",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042089.1.4",
          "form": "저는 비터민 콜라겐 루테인 오메가",
          "original_form": "저는 비터민 콜라겐 루테인 오메가",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100042089.1.5",
          "form": "넘 많아서 안먹는 날이 더 많아요 ㅋㅋ",
          "original_form": "넘 많아서 안먹는 날이 더 많아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100042089.1.6",
          "form": "ㅋㅋㅋ더 챙겨먹어야된다는데 귀찮더라구요",
          "original_form": "ㅋㅋㅋ더 챙겨먹어야된다는데 귀찮더라구요",
          "speaker_id": "2",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100042089.1.7",
          "form": "맞아요 ㅋㅋ 하나라도 잘 먹어여하는데",
          "original_form": "맞아요 ㅋㅋ 하나라도 잘 먹어여하는데",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042089.1.8",
          "form": "비타민도 찾아보려니 너무 많아서 그냥 광고하는거 샀어요ㅋㅋ",
          "original_form": "비타민도 찾아보려니 너무 많아서 그냥 광고하는거 샀어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100042089.1.9",
          "form": "저는 유재석 광고 비타민 먹어요",
          "original_form": "저는 유재석 광고 비타민 먹어요",
          "speaker_id": "1",
          "time": "20210906 19:44"
        },
        {
          "id": "MDRW2100042089.1.10",
          "form": "아 맞아요ㅋㅋ 저두 그걸로 샀어요",
          "original_form": "아 맞아요ㅋㅋ 저두 그걸로 샀어요",
          "speaker_id": "2",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100042089.1.11",
          "form": "오 주로 고려은단 마니 먹더라고요",
          "original_form": "오 주로 고려은단 마니 먹더라고요",
          "speaker_id": "1",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100042089.1.12",
          "form": "제일 유명한것 같더라구요",
          "original_form": "제일 유명한것 같더라구요",
          "speaker_id": "2",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042089.1.13",
          "form": "가볍고 좋아요",
          "original_form": "가볍고 좋아요",
          "speaker_id": "1",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100042089.1.14",
          "form": "외국엔 비타민젤리도 있다는데 너무 실것같아요",
          "original_form": "외국엔 비타민젤리도 있다는데 너무 실것같아요",
          "speaker_id": "2",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100042089.1.15",
          "form": "아 그거 저 앉은자리에서 다먹었어요 ㅋㅋ",
          "original_form": "아 그거 저 앉은자리에서 다먹었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
