      "utterance": [
        {
          "id": "MDRW2100002950.1.1",
          "form": "최근에 산 물건중에 가장 만족도가 높았던 물건 뭐에요?",
          "original_form": "최근에 산 물건중에 가장 만족도가 높았던 물건 뭐에요?",
          "speaker_id": "1",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002950.1.2",
          "form": "저는 폴라로이드요! 사진찍을때마다 만족스러워요!",
          "original_form": "저는 폴라로이드요! 사진찍을때마다 만족스러워요!",
          "speaker_id": "2",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002950.1.3",
          "form": "우와 신기해요",
          "original_form": "우와 신기해요",
          "speaker_id": "1",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002950.1.4",
          "form": "name1님은 어떤것이 만족스러우신가요?",
          "original_form": "&name1&님은 어떤것이 만족스러우신가요?",
          "speaker_id": "2",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002950.1.5",
          "form": "저는 갤럭시탭s7을 샀는데",
          "original_form": "저는 갤럭시탭s7을 샀는데",
          "speaker_id": "1",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002950.1.6",
          "form": "만족하고 잘 쓰고 있어요",
          "original_form": "만족하고 잘 쓰고 있어요",
          "speaker_id": "1",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002950.1.7",
          "form": "오 탭도 사고싶어용",
          "original_form": "오 탭도 사고싶어용",
          "speaker_id": "2",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002950.1.8",
          "form": "탭 있으니까 좋더라고요",
          "original_form": "탭 있으니까 좋더라고요",
          "speaker_id": "1",
          "time": "20210820 17:01"
        },
        {
          "id": "MDRW2100002950.1.9",
          "form": "패드 류도 사고싶더라구요ㅠㅠ 그림그리는분들 보니까 취미생활해보고 싶어요",
          "original_form": "패드 류도 사고싶더라구요ㅠㅠ 그림그리는분들 보니까 취미생활해보고 싶어요",
          "speaker_id": "2",
          "time": "20210820 17:01"
        },
        {
          "id": "MDRW2100002950.1.10",
          "form": "그림 취미 간지나서",
          "original_form": "그림 취미 간지나서",
          "speaker_id": "1",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002950.1.11",
          "form": "예전에 아이패드 샀는데",
          "original_form": "예전에 아이패드 샀는데",
          "speaker_id": "1",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002950.1.12",
          "form": "아이패드 없는 그림 못그리는 사람에서 아이패드 있는 그림 못그리는 사람 됐어요",
          "original_form": "아이패드 없는 그림 못그리는 사람에서 아이패드 있는 그림 못그리는 사람 됐어요",
          "speaker_id": "1",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002950.1.13",
          "form": "ㅋㅋㅠㅠ 그림 그냥 잘그리고 싶지 연습을 하고 싶지 않더라고요",
          "original_form": "ㅋㅋㅠㅠ 그림 그냥 잘그리고 싶지 연습을 하고 싶지 않더라고요",
          "speaker_id": "1",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002950.1.14",
          "form": "ㅋㅋㅋㅋㅋ그래도 좀더 있어보이는 못그리는 사람이라서 괜찮아요!ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ그래도 좀더 있어보이는 못그리는 사람이라서 괜찮아요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002950.1.15",
          "form": "아님 패드로 영화보는것도 좋을것같아용",
          "original_form": "아님 패드로 영화보는것도 좋을것같아용",
          "speaker_id": "2",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002950.1.16",
          "form": "화면크게!",
          "original_form": "화면크게!",
          "speaker_id": "2",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002950.1.17",
          "form": "그건 진짜 좋아요",
          "original_form": "그건 진짜 좋아요",
          "speaker_id": "1",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002950.1.18",
          "form": "넷플 같은거 틀어두고",
          "original_form": "넷플 같은거 틀어두고",
          "speaker_id": "1",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002950.1.19",
          "form": "보고 있으면 편-안",
          "original_form": "보고 있으면 편-안",
          "speaker_id": "1",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002950.1.20",
          "form": "ㅋㅋㅋ폰으로보면 눈빠질것같아요ㅠㅠㅋㅋ패드도 사고싶어지네용",
          "original_form": "ㅋㅋㅋ폰으로보면 눈빠질것같아요ㅠㅠㅋㅋ패드도 사고싶어지네용",
          "speaker_id": "2",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002950.1.21",
          "form": "이렇게 탕진을..!",
          "original_form": "이렇게 탕진을..!",
