      "utterance": [
        {
          "id": "MDRW2100018377.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210827 00:38"
        },
        {
          "id": "MDRW2100018377.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210827 00:39"
        },
        {
          "id": "MDRW2100018377.1.3",
          "form": "엇 어떤 동물 키우시나요?",
          "original_form": "엇 어떤 동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100018377.1.4",
          "form": "고양이 두분을 모시고 있답니다",
          "original_form": "고양이 두분을 모시고 있답니다",
          "speaker_id": "2",
          "time": "20210827 00:42"
        },
        {
          "id": "MDRW2100018377.1.5",
          "form": "저도 고양이 키워요!ㅋㅋㅋ",
          "original_form": "저도 고양이 키워요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:42"
        },
        {
          "id": "MDRW2100018377.1.6",
          "form": "반갑네요",
          "original_form": "반갑네요",
          "speaker_id": "1",
          "time": "20210827 00:42"
        },
        {
          "id": "MDRW2100018377.1.7",
          "form": "저희집 애들은 7살 name3와 4살 name4에요 둘다 코숏이에요",
          "original_form": "저희집 애들은 7살 &name3&와 4살 &name4&에요 둘다 코숏이에요",
          "speaker_id": "2",
          "time": "20210827 00:43"
        },
        {
          "id": "MDRW2100018377.1.8",
          "form": "지금도 옆에서 잘 자고 있네요",
          "original_form": "지금도 옆에서 잘 자고 있네요",
          "speaker_id": "2",
          "time": "20210827 00:43"
        },
        {
          "id": "MDRW2100018377.1.9",
          "form": "저희 고양이는 깜냥인데",
          "original_form": "저희 고양이는 깜냥인데",
          "speaker_id": "1",
          "time": "20210827 00:44"
        },
        {
          "id": "MDRW2100018377.1.10",
          "form": "지금 자기얘기하는거 어떻게 알고 옆으로 슥 왔네요",
          "original_form": "지금 자기얘기하는거 어떻게 알고 옆으로 슥 왔네요",
          "speaker_id": "1",
          "time": "20210827 00:44"
        },
        {
          "id": "MDRW2100018377.1.11",
          "form": "이제 담달에 한살이에요",
          "original_form": "이제 담달에 한살이에요",
          "speaker_id": "1",
          "time": "20210827 00:44"
        },
        {
          "id": "MDRW2100018377.1.12",
          "form": "ㅋㅋ이제 다 컸네요",
          "original_form": "ㅋㅋ이제 다 컸네요",
          "speaker_id": "2",
          "time": "20210827 00:54"
        },
        {
          "id": "MDRW2100018377.1.13",
          "form": "너무 귀엽겠어요",
          "original_form": "너무 귀엽겠어요",
          "speaker_id": "2",
          "time": "20210827 00:54"
        },
        {
          "id": "MDRW2100018377.1.14",
          "form": "네 ㅠㅠㅠ",
          "original_form": "네 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 00:54"
        },
        {
          "id": "MDRW2100018377.1.15",
          "form": "사료는 뭐 먹이세요?",
          "original_form": "사료는 뭐 먹이세요?",
          "speaker_id": "1",
          "time": "20210827 00:54"
        },
        {
          "id": "MDRW2100018377.1.16",
          "form": "사료가....  언니가 사먹여서...",
          "original_form": "사료가....  언니가 사먹여서...",
          "speaker_id": "2",
          "time": "20210827 00:54"
        },
        {
          "id": "MDRW2100018377.1.17",
          "form": "지금은 로얄캐닌 이벤트로 산거 줬는데",
          "original_form": "지금은 로얄캐닌 이벤트로 산거 줬는데",
          "speaker_id": "2",
          "time": "20210827 00:55"
        },
        {
          "id": "MDRW2100018377.1.18",
          "form": "아주 잘먹네요",
          "original_form": "아주 잘먹네요",
          "speaker_id": "2",
          "time": "20210827 00:55"
        },
        {
          "id": "MDRW2100018377.1.19",
          "form": "로얄캐닌이 기호성이 좋다더니..",
          "original_form": "로얄캐닌이 기호성이 좋다더니..",
          "speaker_id": "2",
          "time": "20210827 00:55"
        },
        {
          "id": "MDRW2100018377.1.20",
          "form": "앟ㅎㅎ",
          "original_form": "앟ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100018377.1.21",
          "form": "로얄캐닌이 기호성 갑이죠 역시",
          "original_form": "로얄캐닌이 기호성 갑이죠 역시",
          "speaker_id": "1",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100018377.1.22",
          "form": "저희고양이는 여러개 돌려가며 먹어요",
          "original_form": "저희고양이는 여러개 돌려가며 먹어요",
          "speaker_id": "1",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100018377.1.23",
          "form": "네 그게 좋은거 같아요",
          "original_form": "네 그게 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100018377.1.24",
          "form": "애들도 질려하는 것 같더라구요",
          "original_form": "애들도 질려하는 것 같더라구요",
          "speaker_id": "2",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100018377.1.25",
          "form": "고양이는 미각이 약하다는거 거짓말 같아요",
          "original_form": "고양이는 미각이 약하다는거 거짓말 같아요",
          "speaker_id": "2",
          "time": "20210827 00:57"
        },
        {
          "id": "MDRW2100018377.1.26",
          "form": "맞아요 얼마나 까다로운지 ㅡㅡ",
          "original_form": "맞아요 얼마나 까다로운지 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100018377.1.27",
          "form": "캔을 안먹어서 요즘 골치아파요",
          "original_form": "캔을 안먹어서 요즘 골치아파요",
          "speaker_id": "1",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100018377.1.28",
          "form": "저런...",
          "original_form": "저런...",
          "speaker_id": "2",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100018377.1.29",
          "form": "입맛이 고급인가봐요",
          "original_form": "입맛이 고급인가봐요",
          "speaker_id": "2",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100018377.1.30",
          "form": "오늘 고양이 음료수라하나? 액체로 된거 먹였는데",
          "original_form": "오늘 고양이 음료수라하나? 액체로 된거 먹였는데",
          "speaker_id": "2",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100018377.1.31",
          "form": "아주 잘먹더라구요",
          "original_form": "아주 잘먹더라구요",
          "speaker_id": "2",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100018377.1.32",
          "form": "이게 나트륨이 높은편이라",
          "original_form": "이게 나트륨이 높은편이라",
          "speaker_id": "2",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100018377.1.33",
          "form": "자극적이라 애들이 아주 좋아한다던데",
          "original_form": "자극적이라 애들이 아주 좋아한다던데",
          "speaker_id": "2",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100018377.1.34",
          "form": "그래서 다들 물로 희석해서 준다더라구요",
          "original_form": "그래서 다들 물로 희석해서 준다더라구요",
          "speaker_id": "2",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100018377.1.35",
          "form": "아 저럭",
          "original_form": "아 저럭",
          "speaker_id": "1",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018377.1.36",
          "form": "저도 물이랑 2:1 비율로 타줬는데도",
          "original_form": "저도 물이랑 2:1 비율로 타줬는데도",
          "speaker_id": "2",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018377.1.37",
          "form": "아주 잘먹었어요",
          "original_form": "아주 잘먹었어요",
          "speaker_id": "2",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018377.1.38",
          "form": "저도 그런거 먹이는데 나트륨이 높았구나 ㅠㅠ",
          "original_form": "저도 그런거 먹이는데 나트륨이 높았구나 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018377.1.39",
          "form": "저도 물 타서 줘야겠어요",
          "original_form": "저도 물 타서 줘야겠어요",
          "speaker_id": "1",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018377.1.40",
          "form": "근데 이게 주황색인데..",
          "original_form": "근데 이게 주황색인데..",
          "speaker_id": "2",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018377.1.41",
          "form": "저희집 애 하나는",
          "original_form": "저희집 애 하나는",
          "speaker_id": "2",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018377.1.42",
          "form": "이제까지 그냥 줬는데...ㅠㅠ",
          "original_form": "이제까지 그냥 줬는데...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018377.1.43",
          "form": "그게 무섭다고",
          "original_form": "그게 무섭다고",
          "speaker_id": "2",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018377.1.44",
          "form": "겁먹고 물그릇 공격하더라구요...",
          "original_form": "겁먹고 물그릇 공격하더라구요...",
          "speaker_id": "2",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018377.1.45",
          "form": "예....?",
          "original_form": "예....?",
          "speaker_id": "1",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018377.1.46",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ귀여운 고양이네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ귀여운 고양이네요",
          "speaker_id": "1",
          "time": "20210827 01:01"
        },
        {
          "id": "MDRW2100018377.1.47",
          "form": "음 이건 습식캔은 아니고 음료수 같은거였어요",
          "original_form": "음 이건 습식캔은 아니고 음료수 같은거였어요",
          "speaker_id": "2",
          "time": "20210827 01:01"
        },
        {
          "id": "MDRW2100018377.1.48",
          "form": "겁많은 냥이ㅋㅋㅋㅋㅋㅋ",
          "original_form": "겁많은 냥이ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:01"
        },
        {
          "id": "MDRW2100018377.1.49",
          "form": "하..  갈수록 겁이 많아져서 큰일이에요",
          "original_form": "하..  갈수록 겁이 많아져서 큰일이에요",
          "speaker_id": "2",
          "time": "20210827 01:01"
        },
        {
          "id": "MDRW2100018377.1.50",
          "form": "ㅋㅋㅋㅠㅠ 저희 애도 사람만 오면 숨네요",
          "original_form": "ㅋㅋㅋㅠㅠ 저희 애도 사람만 오면 숨네요",
          "speaker_id": "1",
          "time": "20210827 01:02"
        }
