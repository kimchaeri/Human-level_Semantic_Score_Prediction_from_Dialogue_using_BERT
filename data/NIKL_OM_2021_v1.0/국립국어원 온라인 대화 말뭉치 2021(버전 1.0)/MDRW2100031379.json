      "utterance": [
        {
          "id": "MDRW2100031379.1.1",
          "form": "커피 좋아하세요?^^",
          "original_form": "커피 좋아하세요?^^",
          "speaker_id": "1",
          "time": "20210904 20:56"
        },
        {
          "id": "MDRW2100031379.1.2",
          "form": "커피 좋아하죠",
          "original_form": "커피 좋아하죠",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100031379.1.3",
          "form": "특히 아이스요",
          "original_form": "특히 아이스요",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100031379.1.4",
          "form": "위가 튼튼하신가 봐요~",
          "original_form": "위가 튼튼하신가 봐요~",
          "speaker_id": "1",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100031379.1.5",
          "form": "저도 커피 좋아하는데 잠을 방해해서 자주 못마시거든요",
          "original_form": "저도 커피 좋아하는데 잠을 방해해서 자주 못마시거든요",
          "speaker_id": "1",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100031379.1.6",
          "form": "아주 아쉬워요",
          "original_form": "아주 아쉬워요",
          "speaker_id": "1",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100031379.1.7",
          "form": "저는 위는 별 문제가 없어요",
          "original_form": "저는 위는 별 문제가 없어요",
          "speaker_id": "2",
          "time": "20210905 17:14"
        },
        {
          "id": "MDRW2100031379.1.8",
          "form": "잠도.. 오히려 커피 먹어도 잠이 잘 안깨서 맨날",
          "original_form": "잠도.. 오히려 커피 먹어도 잠이 잘 안깨서 맨날",
          "speaker_id": "2",
          "time": "20210905 17:15"
        },
        {
          "id": "MDRW2100031379.1.9",
          "form": "어떻게하면 카페인 농도를 높일까 궁리합니다 ㅎ",
          "original_form": "어떻게하면 카페인 농도를 높일까 궁리합니다 ㅎ",
          "speaker_id": "2",
          "time": "20210905 17:15"
        },
        {
          "id": "MDRW2100031379.1.10",
          "form": "콜드브루만 먹고",
          "original_form": "콜드브루만 먹고",
          "speaker_id": "2",
          "time": "20210905 17:15"
        },
        {
          "id": "MDRW2100031379.1.11",
          "form": "한편으론 부럽네요",
          "original_form": "한편으론 부럽네요",
          "speaker_id": "1",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100031379.1.12",
          "form": "전 마시고 싶어도 진짜 밤에 난리나서 못마시거든요",
          "original_form": "전 마시고 싶어도 진짜 밤에 난리나서 못마시거든요",
          "speaker_id": "1",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100031379.1.13",
          "form": "그런 친구들도 있더라구요",
          "original_form": "그런 친구들도 있더라구요",
          "speaker_id": "2",
          "time": "20210905 17:22"
        },
        {
          "id": "MDRW2100031379.1.14",
          "form": "사실 전 커피를 잠깨는 용도로만 마셔서 맛 자체를 글케 좋아하는건 아니랍니다",
          "original_form": "사실 전 커피를 잠깨는 용도로만 마셔서 맛 자체를 글케 좋아하는건 아니랍니다",
          "speaker_id": "2",
          "time": "20210905 17:22"
        },
        {
          "id": "MDRW2100031379.1.15",
          "form": "커피를 진짜 즐기는데 카페인이 안받으면 좀 슬플것 같긴 하네요..",
          "original_form": "커피를 진짜 즐기는데 카페인이 안받으면 좀 슬플것 같긴 하네요..",
          "speaker_id": "2",
          "time": "20210905 17:23"
        },
        {
          "id": "MDRW2100031379.1.16",
          "form": "네 그래서 디카페인에 도전을 해보려고 해요",
          "original_form": "네 그래서 디카페인에 도전을 해보려고 해요",
          "speaker_id": "1",
          "time": "20210905 17:30"
        },
        {
          "id": "MDRW2100031379.1.17",
          "form": "많은 분들이 디카페인 커피 마셔보라고 권해주셨어요",
          "original_form": "많은 분들이 디카페인 커피 마셔보라고 권해주셨어요",
          "speaker_id": "1",
          "time": "20210905 17:30"
        },
        {
          "id": "MDRW2100031379.1.18",
          "form": "오 디카페인 전 맛 구분도 안가더라구요",
          "original_form": "오 디카페인 전 맛 구분도 안가더라구요",
          "speaker_id": "2",
          "time": "20210905 17:37"
        },
        {
          "id": "MDRW2100031379.1.19",
          "form": "디카페인도 좋죠 저희 엄마도 자주 드세요",
          "original_form": "디카페인도 좋죠 저희 엄마도 자주 드세요",
          "speaker_id": "2",
          "time": "20210905 17:37"
        },
        {
          "id": "MDRW2100031379.1.20",
          "form": "그렇군요.. 진작 마셔볼걸",
          "original_form": "그렇군요.. 진작 마셔볼걸",
          "speaker_id": "1",
          "time": "20210905 17:41"
        },
        {
          "id": "MDRW2100031379.1.21",
          "form": "저희집은 네스프레소 캡슐을 쓴답니다",
          "original_form": "저희집은 네스프레소 캡슐을 쓴답니다",
          "speaker_id": "2",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100031379.1.22",
          "form": "name1님은 집에서 커피 뭐드세요?",
          "original_form": "&name1&님은 집에서 커피 뭐드세요?",
          "speaker_id": "2",
          "time": "20210905 19:46"
        },
        {
          "id": "MDRW2100031379.1.23",
          "form": "저희 가족은 원두 내려 마셔요",
          "original_form": "저희 가족은 원두 내려 마셔요",
          "speaker_id": "1",
          "time": "20210905 20:15"
        },
        {
          "id": "MDRW2100031379.1.24",
          "form": "그렇군요 저희집도 옛날엔 그랬어요ㅋㅋ",
          "original_form": "그렇군요 저희집도 옛날엔 그랬어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100031379.1.25",
          "form": "캡슐은 아직 도전 안해봤는데",
          "original_form": "캡슐은 아직 도전 안해봤는데",
          "speaker_id": "1",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100031379.1.26",
          "form": "원두보다 낫나요?",
          "original_form": "원두보다 낫나요?",
