      "utterance": [
        {
          "id": "MDRW2100002682.1.1",
          "form": "밥은 주로 해드세요?",
          "original_form": "밥은 주로 해드세요?",
          "speaker_id": "1",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002682.1.2",
          "form": "네 집에서는 거의 저녁만 먹어서 간단히 해서 먹어요",
          "original_form": "네 집에서는 거의 저녁만 먹어서 간단히 해서 먹어요",
          "speaker_id": "2",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002682.1.3",
          "form": "아~저는 해먹기도 하고 배달도 자주해요 ㅎ",
          "original_form": "아~저는 해먹기도 하고 배달도 자주해요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:45"
        },
        {
          "id": "MDRW2100002682.1.4",
          "form": "어떤거 주로 배달하세요?",
          "original_form": "어떤거 주로 배달하세요?",
          "speaker_id": "2",
          "time": "20210820 14:47"
        },
        {
          "id": "MDRW2100002682.1.5",
          "form": "식사류도 하고 야식도 하고 간식도 배달하고요 ㅎ",
          "original_form": "식사류도 하고 야식도 하고 간식도 배달하고요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:47"
        },
        {
          "id": "MDRW2100002682.1.6",
          "form": "골고루 하시네요 ㅋㅋ",
          "original_form": "골고루 하시네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:48"
        },
        {
          "id": "MDRW2100002682.1.7",
          "form": "저는 배달은 주로 식사만해요",
          "original_form": "저는 배달은 주로 식사만해요",
          "speaker_id": "2",
          "time": "20210820 14:49"
        },
        {
          "id": "MDRW2100002682.1.8",
          "form": "고기종류로요",
          "original_form": "고기종류로요",
          "speaker_id": "2",
          "time": "20210820 14:49"
        },
        {
          "id": "MDRW2100002682.1.9",
          "form": "아~어떤 고기요?",
          "original_form": "아~어떤 고기요?",
          "speaker_id": "1",
          "time": "20210820 14:49"
        },
        {
          "id": "MDRW2100002682.1.10",
          "form": "족발 보쌈 감자탕 이런거요",
          "original_form": "족발 보쌈 감자탕 이런거요",
          "speaker_id": "2",
          "time": "20210820 14:50"
        },
        {
          "id": "MDRW2100002682.1.11",
          "form": "고기를 좋아해서ㅋㅋ",
          "original_form": "고기를 좋아해서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:50"
        },
        {
          "id": "MDRW2100002682.1.12",
          "form": "치킨도 많이 시키고요",
          "original_form": "치킨도 많이 시키고요",
          "speaker_id": "2",
          "time": "20210820 14:50"
        },
        {
          "id": "MDRW2100002682.1.13",
          "form": "아~~ㅎㅎ 저도 고기 좋아해요 ㅎ",
          "original_form": "아~~ㅎㅎ 저도 고기 좋아해요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:50"
        },
        {
          "id": "MDRW2100002682.1.14",
          "form": "저도 치킨 자주시켜요 ㅎ",
          "original_form": "저도 치킨 자주시켜요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:51"
        },
        {
          "id": "MDRW2100002682.1.15",
          "form": "치킨은 역시 허니콤보",
          "original_form": "치킨은 역시 허니콤보",
          "speaker_id": "2",
          "time": "20210820 14:51"
        },
        {
          "id": "MDRW2100002682.1.16",
          "form": "그거 맛있나요?",
          "original_form": "그거 맛있나요?",
          "speaker_id": "1",
          "time": "20210820 14:51"
        },
        {
          "id": "MDRW2100002682.1.17",
          "form": "오늘먹어도 내일또 먹고싶은 맛이에요",
          "original_form": "오늘먹어도 내일또 먹고싶은 맛이에요",
          "speaker_id": "2",
          "time": "20210820 14:52"
        },
        {
          "id": "MDRW2100002682.1.18",
          "form": "아 진짜요?너무 달기만 할까봐 아직 안 먹어봤어요",
          "original_form": "아 진짜요?너무 달기만 할까봐 아직 안 먹어봤어요",
          "speaker_id": "1",
          "time": "20210820 14:53"
        },
        {
          "id": "MDRW2100002682.1.19",
          "form": "꼭 드셔보세요 중독입니다",
          "original_form": "꼭 드셔보세요 중독입니다",
          "speaker_id": "2",
          "time": "20210820 14:53"
        },
        {
          "id": "MDRW2100002682.1.20",
          "form": "아~네~~콤보면 닭다리랑 윙인가요?",
          "original_form": "아~네~~콤보면 닭다리랑 윙인가요?",
          "speaker_id": "1",
          "time": "20210820 14:54"
        },
        {
          "id": "MDRW2100002682.1.21",
          "form": "네 먹기도 편하고 순삭이에요",
          "original_form": "네 먹기도 편하고 순삭이에요",
