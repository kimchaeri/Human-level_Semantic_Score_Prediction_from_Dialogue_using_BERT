        {
          "id": "MDRW2100046159.1.1",
          "form": "쇼핑가자",
          "original_form": "쇼핑가자",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046159.1.2",
          "form": "나 오늘 아침에 삼다수 시켰어",
          "original_form": "나 오늘 아침에 삼다수 시켰어",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046159.1.3",
          "form": "물을 주문해서먹는구나",
          "original_form": "물을 주문해서먹는구나",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046159.1.4",
          "form": "새벽에 시켰는데 오늘 올거래",
          "original_form": "새벽에 시켰는데 오늘 올거래",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046159.1.5",
          "form": "와 우리나라 택배최고네",
          "original_form": "와 우리나라 택배최고네",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046159.1.6",
          "form": "너는 쇼핑 뭐할건데?",
          "original_form": "너는 쇼핑 뭐할건데?",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046159.1.7",
          "form": "나는 필기구살거야",
          "original_form": "나는 필기구살거야",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046159.1.8",
          "form": "열공하려고 하는구나",
          "original_form": "열공하려고 하는구나",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046159.1.9",
          "form": "나는 다이어리 꾸미는걸좋아해서",
          "original_form": "나는 다이어리 꾸미는걸좋아해서",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046159.1.10",
          "form": "오 다꾸 하는구나",
          "original_form": "오 다꾸 하는구나",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046159.1.11",
          "form": "형광펜살거야",
          "original_form": "형광펜살거야",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046159.1.12",
          "form": "마일드? 뭐 그런게 유명하다던데",
          "original_form": "마일드? 뭐 그런게 유명하다던데",
