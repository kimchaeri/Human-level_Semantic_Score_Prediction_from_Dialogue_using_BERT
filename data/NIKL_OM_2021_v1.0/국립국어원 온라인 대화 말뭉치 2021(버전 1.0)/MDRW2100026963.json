      "utterance": [
        {
          "id": "MDRW2100026963.1.1",
          "form": "놀러가고싶다",
          "original_form": "놀러가고싶다",
          "speaker_id": "1",
          "time": "20210902 01:58"
        },
        {
          "id": "MDRW2100026963.1.2",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "2",
          "time": "20210902 02:00"
        },
        {
          "id": "MDRW2100026963.1.3",
          "form": "같이가면조켓다",
          "original_form": "같이가면조켓다",
          "speaker_id": "1",
          "time": "20210902 02:00"
        },
        {
          "id": "MDRW2100026963.1.4",
          "form": "기억나니..? 우리 일본여행..",
          "original_form": "기억나니..? 우리 일본여행..",
          "speaker_id": "2",
          "time": "20210902 02:00"
        },
        {
          "id": "MDRW2100026963.1.5",
          "form": "당연하지...",
          "original_form": "당연하지...",
          "speaker_id": "1",
          "time": "20210902 02:01"
        },
        {
          "id": "MDRW2100026963.1.6",
          "form": "도쿄도가고 오사카도가고ㅜ",
          "original_form": "도쿄도가고 오사카도가고ㅜ",
          "speaker_id": "1",
          "time": "20210902 02:02"
        },
        {
          "id": "MDRW2100026963.1.7",
          "form": "ㅠㅠㅠ추억이야...",
          "original_form": "ㅠㅠㅠ추억이야...",
          "speaker_id": "2",
          "time": "20210902 02:03"
        },
        {
          "id": "MDRW2100026963.1.8",
          "form": "또 같이 가서 맛난거먹고 온천도 가고",
          "original_form": "또 같이 가서 맛난거먹고 온천도 가고",
          "speaker_id": "2",
          "time": "20210902 02:03"
        },
        {
          "id": "MDRW2100026963.1.9",
          "form": "요새 겨울에 삿포로 가고싶더라",
          "original_form": "요새 겨울에 삿포로 가고싶더라",
          "speaker_id": "2",
          "time": "20210902 02:03"
        },
        {
          "id": "MDRW2100026963.1.10",
          "form": "좋아.... 취직하면 그돈으로 함땡겨버리자",
          "original_form": "좋아.... 취직하면 그돈으로 함땡겨버리자",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100026963.1.11",
          "form": "삿포로 겨울온천에 흥미잇다",
          "original_form": "삿포로 겨울온천에 흥미잇다",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100026963.1.12",
          "form": "하 죠아",
          "original_form": "하 죠아",
          "speaker_id": "2",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100026963.1.13",
          "form": "우리 한 번 고급여관으로~~~~",
          "original_form": "우리 한 번 고급여관으로~~~~",
          "speaker_id": "2",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100026963.1.14",
          "form": "눈도 많이 밟고싶다룽",
          "original_form": "눈도 많이 밟고싶다룽",
          "speaker_id": "2",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100026963.1.15",
          "form": "마저...",
          "original_form": "마저...",
          "speaker_id": "1",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100026963.1.16",
          "form": "근데 료칸비싸대",
          "original_form": "근데 료칸비싸대",
          "speaker_id": "1",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100026963.1.17",
          "form": "맞아 나 일본브이로그 보는데 겨울 훗카이도 여행봤는데",
          "original_form": "맞아 나 일본브이로그 보는데 겨울 훗카이도 여행봤는데",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100026963.1.18",
          "form": "료칸 완전 좋아보여서 가격봤었는데",
          "original_form": "료칸 완전 좋아보여서 가격봤었는데",
