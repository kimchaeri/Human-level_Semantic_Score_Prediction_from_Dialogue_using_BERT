      "utterance": [
        {
          "id": "MDRW2100007525.1.1",
          "form": "동물 키우시나용?",
          "original_form": "동물 키우시나용?",
          "speaker_id": "1",
          "time": "20210822 02:56"
        },
        {
          "id": "MDRW2100007525.1.2",
          "form": "아뇽",
          "original_form": "아뇽",
          "speaker_id": "2",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007525.1.3",
          "form": "키우세요?",
          "original_form": "키우세요?",
          "speaker_id": "2",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007525.1.4",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007525.1.5",
          "form": "랜선 이모입니다",
          "original_form": "랜선 이모입니다",
          "speaker_id": "1",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007525.1.6",
          "form": "키운다면 뭐키우고싶으세요?",
          "original_form": "키운다면 뭐키우고싶으세요?",
          "speaker_id": "2",
          "time": "20210822 02:57"
        },
        {
          "id": "MDRW2100007525.1.7",
          "form": "전 강아지요 너무 귀야우ㅏ요",
          "original_form": "전 강아지요 너무 귀야우ㅏ요",
          "speaker_id": "1",
          "time": "20210822 03:01"
        },
        {
          "id": "MDRW2100007525.1.8",
          "form": "짛님은요??",
          "original_form": "짛님은요??",
          "speaker_id": "1",
          "time": "20210822 03:01"
        },
        {
          "id": "MDRW2100007525.1.9",
          "form": "저는 요즘 저희 어머니께서 고양이가 귀엽다고 하시더라구요",
          "original_form": "저는 요즘 저희 어머니께서 고양이가 귀엽다고 하시더라구요",
          "speaker_id": "2",
          "time": "20210822 03:01"
        },
        {
          "id": "MDRW2100007525.1.10",
          "form": "저도 고양이 좋아해요",
          "original_form": "저도 고양이 좋아해요",
          "speaker_id": "2",
          "time": "20210822 03:01"
        },
        {
          "id": "MDRW2100007525.1.11",
          "form": "어머니와 함께 사시는군요",
          "original_form": "어머니와 함께 사시는군요",
          "speaker_id": "1",
          "time": "20210822 03:01"
        },
        {
          "id": "MDRW2100007525.1.12",
          "form": "고양이도 귀엽죠",
          "original_form": "고양이도 귀엽죠",
          "speaker_id": "1",
          "time": "20210822 03:01"
        },
        {
          "id": "MDRW2100007525.1.13",
          "form": "알러지는 없으세요?",
          "original_form": "알러지는 없으세요?",
          "speaker_id": "1",
          "time": "20210822 03:02"
        },
        {
          "id": "MDRW2100007525.1.14",
          "form": "예전에 고양이카페가서 알러지 없었으니",
          "original_form": "예전에 고양이카페가서 알러지 없었으니",
          "speaker_id": "2",
          "time": "20210822 03:02"
        },
        {
          "id": "MDRW2100007525.1.15",
          "form": "괜찮은가봐요",
          "original_form": "괜찮은가봐요",
          "speaker_id": "2",
          "time": "20210822 03:02"
        },
        {
          "id": "MDRW2100007525.1.16",
          "form": "강아지도 귀엽죠",
          "original_form": "강아지도 귀엽죠",
          "speaker_id": "2",
          "time": "20210822 03:02"
        },
        {
          "id": "MDRW2100007525.1.17",
          "form": "헐 다행이네요",
          "original_form": "헐 다행이네요",
          "speaker_id": "1",
          "time": "20210822 03:02"
        },
        {
          "id": "MDRW2100007525.1.18",
          "form": "말잘듣는 강아지들",
          "original_form": "말잘듣는 강아지들",
          "speaker_id": "2",
          "time": "20210822 03:02"
        },
        {
          "id": "MDRW2100007525.1.19",
          "form": "강아지는 넘 똑똑",
          "original_form": "강아지는 넘 똑똑",
          "speaker_id": "1",
          "time": "20210822 03:02"
        },
        {
          "id": "MDRW2100007525.1.20",
          "form": "사람같아요",
          "original_form": "사람같아요",
          "speaker_id": "1",
          "time": "20210822 03:02"
        },
        {
          "id": "MDRW2100007525.1.21",
          "form": "종종보면",
          "original_form": "종종보면",
          "speaker_id": "1",
          "time": "20210822 03:02"
        },
        {
          "id": "MDRW2100007525.1.22",
          "form": "맞아요 가끔식 보면 너무 사람같아요",
          "original_form": "맞아요 가끔식 보면 너무 사람같아요",
          "speaker_id": "2",
          "time": "20210822 03:03"
        },
        {
          "id": "MDRW2100007525.1.23",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210822 03:03"
        },
        {
          "id": "MDRW2100007525.1.24",
          "form": "유투브로 보세요??",
          "original_form": "유투브로 보세요??",
          "speaker_id": "1",
          "time": "20210822 03:03"
        },
        {
          "id": "MDRW2100007525.1.25",
          "form": "네 거의 유튜브로 봐요",
          "original_form": "네 거의 유튜브로 봐요",
          "speaker_id": "2",
          "time": "20210822 03:03"
        },
        {
          "id": "MDRW2100007525.1.26",
          "form": "고양이 쓰다듬는영상",
          "original_form": "고양이 쓰다듬는영상",
          "speaker_id": "2",
          "time": "20210822 03:03"
        },
        {
          "id": "MDRW2100007525.1.27",
          "form": "저두요 인스타랑 유투브",
          "original_form": "저두요 인스타랑 유투브",
          "speaker_id": "1",
          "time": "20210822 03:03"
        },
        {
          "id": "MDRW2100007525.1.28",
          "form": "뭉땡뭉땡 이라고 채널있는데",
          "original_form": "뭉땡뭉땡 이라고 채널있는데",
          "speaker_id": "1",
          "time": "20210822 03:04"
        },
        {
          "id": "MDRW2100007525.1.29",
          "form": "뭉이 진짜 똑똑해요",
          "original_form": "뭉이 진짜 똑똑해요",
          "speaker_id": "1",
          "time": "20210822 03:04"
        },
        {
          "id": "MDRW2100007525.1.30",
          "form": "거의 사람",
          "original_form": "거의 사람",
          "speaker_id": "1",
          "time": "20210822 03:04"
        }
      ]
    }
  ]
