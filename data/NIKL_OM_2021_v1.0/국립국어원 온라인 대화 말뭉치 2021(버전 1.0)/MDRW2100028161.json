      "utterance": [
        {
          "id": "MDRW2100028161.1.1",
          "form": "엉 누텔라도 들어가면 헤븐이야",
          "original_form": "엉 누텔라도 들어가면 헤븐이야",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028161.1.2",
          "form": "와 누텔라는 게임끝이지 누텔라 숟가락으로 퍼먹고싶다",
          "original_form": "와 누텔라는 게임끝이지 누텔라 숟가락으로 퍼먹고싶다",
          "speaker_id": "1",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028161.1.3",
          "form": "거기에 바나나",
          "original_form": "거기에 바나나",
          "speaker_id": "2",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028161.1.4",
          "form": "광주에 퐁당와플 맛있다",
          "original_form": "광주에 퐁당와플 맛있다",
          "speaker_id": "1",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028161.1.5",
          "form": "알지알지",
          "original_form": "알지알지",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028161.1.6",
          "form": "우리 버터핑거팬케잌먹으러 가자",
          "original_form": "우리 버터핑거팬케잌먹으러 가자",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028161.1.7",
          "form": "너무좋다 그거 어디에있어",
          "original_form": "너무좋다 그거 어디에있어",
          "speaker_id": "1",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028161.1.8",
          "form": "정자동",
          "original_form": "정자동",
          "speaker_id": "2",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028161.1.9",
          "form": "아 거기어딘지 알아",
          "original_form": "아 거기어딘지 알아",
          "speaker_id": "1",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028161.1.10",
          "form": "유명했는데 이제 거의 없어",
          "original_form": "유명했는데 이제 거의 없어",
          "speaker_id": "2",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028161.1.11",
          "form": "정자 하나 있는거지? 다음에 가자",
          "original_form": "정자 하나 있는거지? 다음에 가자",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028161.1.12",
          "form": "맞아 가자 팬케잌도 맛있음",
          "original_form": "맞아 가자 팬케잌도 맛있음",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028161.1.13",
          "form": "와플도 있고",
          "original_form": "와플도 있고",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028161.1.14",
          "form": "브런치도 있어",
          "original_form": "브런치도 있어",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028161.1.15",
          "form": "너무좋다",
          "original_form": "너무좋다",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028161.1.16",
          "form": "브런치 맛있어",
          "original_form": "브런치 맛있어",
          "speaker_id": "1",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028161.1.17",
          "form": "근데 느끼해서 피클 필수야",
          "original_form": "근데 느끼해서 피클 필수야",
