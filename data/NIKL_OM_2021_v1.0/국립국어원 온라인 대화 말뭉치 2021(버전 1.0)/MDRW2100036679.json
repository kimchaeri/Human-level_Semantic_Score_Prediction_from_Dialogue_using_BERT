      "utterance": [
        {
          "id": "MDRW2100036679.1.1",
          "form": "네! 오늘도 아침 걷기 하고 왔어요~",
          "original_form": "네! 오늘도 아침 걷기 하고 왔어요~",
          "speaker_id": "2",
          "time": "20210831 08:40"
        },
        {
          "id": "MDRW2100036679.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 08:40"
        },
        {
          "id": "MDRW2100036679.1.3",
          "form": "저도 걷기 시작했는데 요새 비와서 또 못하고 있어요",
          "original_form": "저도 걷기 시작했는데 요새 비와서 또 못하고 있어요",
          "speaker_id": "1",
          "time": "20210831 08:41"
        },
        {
          "id": "MDRW2100036679.1.4",
          "form": "아ㅠ 그쵸ㅠㅡ 저도 걷다가 비와서 들어왔어요ㅠ",
          "original_form": "아ㅠ 그쵸ㅠㅡ 저도 걷다가 비와서 들어왔어요ㅠ",
          "speaker_id": "2",
          "time": "20210831 08:41"
        },
        {
          "id": "MDRW2100036679.1.5",
          "form": "지난주에 몇번 당했어요",
          "original_form": "지난주에 몇번 당했어요",
          "speaker_id": "1",
          "time": "20210831 08:43"
        },
        {
          "id": "MDRW2100036679.1.6",
          "form": "비 안와서 나갔다가 쫄딱 맞고 들어왔어요",
          "original_form": "비 안와서 나갔다가 쫄딱 맞고 들어왔어요",
          "speaker_id": "1",
          "time": "20210831 08:43"
        },
        {
          "id": "MDRW2100036679.1.7",
          "form": "당하셨군요ㅠ",
          "original_form": "당하셨군요ㅠ",
          "speaker_id": "2",
          "time": "20210831 09:04"
        },
        {
          "id": "MDRW2100036679.1.8",
          "form": "한두방울이 아니라 후두둑이더라구요",
          "original_form": "한두방울이 아니라 후두둑이더라구요",
          "speaker_id": "2",
          "time": "20210831 09:04"
        },
        {
          "id": "MDRW2100036679.1.9",
          "form": "그니까요  원래 걷기운동이었는데",
          "original_form": "그니까요  원래 걷기운동이었는데",
          "speaker_id": "1",
          "time": "20210831 09:04"
        },
        {
          "id": "MDRW2100036679.1.10",
          "form": "비오면서 달리기로 바꼈어요",
          "original_form": "비오면서 달리기로 바꼈어요",
          "speaker_id": "1",
          "time": "20210831 09:05"
        },
        {
          "id": "MDRW2100036679.1.11",
          "form": "어휴 그러셨겠어요",
          "original_form": "어휴 그러셨겠어요",
          "speaker_id": "2",
          "time": "20210831 09:05"
        },
        {
          "id": "MDRW2100036679.1.12",
          "form": "달리기는 준비 운동이 필요한데!",
          "original_form": "달리기는 준비 운동이 필요한데!",
          "speaker_id": "2",
          "time": "20210831 09:06"
        },
        {
          "id": "MDRW2100036679.1.13",
          "form": "걸으면서 준비운동이 됐겠죠?",
          "original_form": "걸으면서 준비운동이 됐겠죠?",
          "speaker_id": "1",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100036679.1.14",
          "form": "암튼 운동하기 안좋은 날씨에요",
          "original_form": "암튼 운동하기 안좋은 날씨에요",
          "speaker_id": "1",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100036679.1.15",
          "form": "오ㅋㅋ그렇네요",
          "original_form": "오ㅋㅋ그렇네요",
          "speaker_id": "2",
          "time": "20210831 09:16"
        },
        {
          "id": "MDRW2100036679.1.16",
          "form": "맞아요 진짜 안좋아요",
          "original_form": "맞아요 진짜 안좋아요",
          "speaker_id": "2",
          "time": "20210831 09:16"
        },
        {
          "id": "MDRW2100036679.1.17",
          "form": "홈트하면 날씨 상관없이 운동할수 있을텐데",
          "original_form": "홈트하면 날씨 상관없이 운동할수 있을텐데",
          "speaker_id": "1",
          "time": "20210831 09:19"
        },
        {
          "id": "MDRW2100036679.1.18",
          "form": "맞아요 근데 홈트는 정을 못붙이겠어요",
          "original_form": "맞아요 근데 홈트는 정을 못붙이겠어요",
          "speaker_id": "2",
          "time": "20210831 09:23"
        },
        {
          "id": "MDRW2100036679.1.19",
          "form": "맞아요 못하겠어요",
          "original_form": "맞아요 못하겠어요",
          "speaker_id": "1",
          "time": "20210831 09:23"
        },
        {
          "id": "MDRW2100036679.1.20",
          "form": "헬스하는거랑 느낌이 같아융",
          "original_form": "헬스하는거랑 느낌이 같아융",
          "speaker_id": "2",
          "time": "20210831 09:24"
        },
        {
          "id": "MDRW2100036679.1.21",
          "form": "맞아요.. 저 헬스장 연회원 끊고도 거의 안갔어요..",
          "original_form": "맞아요.. 저 헬스장 연회원 끊고도 거의 안갔어요..",
          "speaker_id": "1",
          "time": "20210831 09:29"
        }
      ]
