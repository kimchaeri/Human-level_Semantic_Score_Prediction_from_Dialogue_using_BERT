      "utterance": [
        {
          "id": "MDRW2100041878.1.1",
          "form": "저는 떡볶이를 끝으로 오늘의 음식섭취 끝..!!",
          "original_form": "저는 떡볶이를 끝으로 오늘의 음식섭취 끝..!!",
          "speaker_id": "2",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100041878.1.2",
          "form": "저녁 뭐드시려구요?",
          "original_form": "저녁 뭐드시려구요?",
          "speaker_id": "2",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100041878.1.3",
          "form": "저 고민이예요",
          "original_form": "저 고민이예요",
          "speaker_id": "1",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100041878.1.4",
          "form": "고민이에요 !!!",
          "original_form": "고민이에요 !!!",
          "speaker_id": "1",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100041878.1.5",
          "form": "ㅋㅋ맛있는거 드세요 고민고민",
          "original_form": "ㅋㅋ맛있는거 드세요 고민고민",
          "speaker_id": "2",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100041878.1.6",
          "form": "ㅋㅋ 밥 고르는게.은근 힘들어요",
          "original_form": "ㅋㅋ 밥 고르는게.은근 힘들어요",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041878.1.7",
          "form": "ㅠㅠ 저만 그런가요",
          "original_form": "ㅠㅠ 저만 그런가요",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041878.1.8",
          "form": "저두에요  요리해먹을때 머리속으로 한참 시뮬레이션 돌려보고 한답니다",
          "original_form": "저두에요  요리해먹을때 머리속으로 한참 시뮬레이션 돌려보고 한답니다",
          "speaker_id": "2",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100041878.1.9",
          "form": "ㅌㅋㅋㅋㅋㅋㅋ 어려워요 ㅠ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋ 어려워요 ㅠ",
          "speaker_id": "1",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100041878.1.10",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "1",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100041878.1.11",
          "form": "그냥 인터넷 레시피 찾아보고 하는거 좋아해요ㅋㅋ",
          "original_form": "그냥 인터넷 레시피 찾아보고 하는거 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100041878.1.12",
          "form": "독창성은 없어요!!",
          "original_form": "독창성은 없어요!!",
          "speaker_id": "2",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100041878.1.13",
          "form": "요리사 될 거 아니면",
          "original_form": "요리사 될 거 아니면",
          "speaker_id": "1",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100041878.1.14",
          "form": "뭐 독창성 따위!!",
          "original_form": "뭐 독창성 따위!!",
          "speaker_id": "1",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100041878.1.15",
          "form": "그건 그렇네요ㅋㅋ!! 먹을만하면 된거겠죠",
          "original_form": "그건 그렇네요ㅋㅋ!! 먹을만하면 된거겠죠",
          "speaker_id": "2",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100041878.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ 맛있는게 최고죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 맛있는게 최고죠",
          "speaker_id": "1",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100041878.1.17",
          "form": "자주 해먹는 요리 있으세요?",
          "original_form": "자주 해먹는 요리 있으세요?",
          "speaker_id": "2",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100041878.1.18",
          "form": "아뇨 .. 요리를 못해서",
          "original_form": "아뇨 .. 요리를 못해서",
          "speaker_id": "1",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100041878.1.19",
          "form": "전 주로 시켜먹어요 흑",
          "original_form": "전 주로 시켜먹어요 흑",
          "speaker_id": "1",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100041878.1.20",
          "form": "아! 배만 채우면 되져!ㅋㅋㅋ",
