      "utterance": [
        {
          "id": "MDRW2100012839.1.1",
          "form": "저는 무교예요",
          "original_form": "저는 무교예요",
          "speaker_id": "2",
          "time": "20210825 02:29"
        },
        {
          "id": "MDRW2100012839.1.2",
          "form": "아 저도 무교예요. 세례명은 있지만",
          "original_form": "아 저도 무교예요. 세례명은 있지만",
          "speaker_id": "1",
          "time": "20210825 02:30"
        },
        {
          "id": "MDRW2100012839.1.3",
          "form": "교회나 성당 절 한번도 안 가보셨나요",
          "original_form": "교회나 성당 절 한번도 안 가보셨나요",
          "speaker_id": "1",
          "time": "20210825 02:31"
        },
        {
          "id": "MDRW2100012839.1.4",
          "form": "모태신앙이세요?",
          "original_form": "모태신앙이세요?",
          "speaker_id": "2",
          "time": "20210825 02:31"
        },
        {
          "id": "MDRW2100012839.1.5",
          "form": "교회랑 절 가봤어요",
          "original_form": "교회랑 절 가봤어요",
          "speaker_id": "2",
          "time": "20210825 02:31"
        },
        {
          "id": "MDRW2100012839.1.6",
          "form": "네 온가족이 세례명이 있어요ㅎ",
          "original_form": "네 온가족이 세례명이 있어요ㅎ",
          "speaker_id": "1",
          "time": "20210825 02:32"
        },
        {
          "id": "MDRW2100012839.1.7",
          "form": "저도 무교인데 셋 다 가봤네요",
          "original_form": "저도 무교인데 셋 다 가봤네요",
          "speaker_id": "1",
          "time": "20210825 02:32"
        },
        {
          "id": "MDRW2100012839.1.8",
          "form": "절은 봉은사 가봤어요",
          "original_form": "절은 봉은사 가봤어요",
          "speaker_id": "2",
          "time": "20210825 02:33"
        },
        {
          "id": "MDRW2100012839.1.9",
          "form": "템플스테이로 간 건가요?",
          "original_form": "템플스테이로 간 건가요?",
          "speaker_id": "1",
          "time": "20210825 02:33"
        },
        {
          "id": "MDRW2100012839.1.10",
          "form": "아니요 우연히 가봤어요~",
          "original_form": "아니요 우연히 가봤어요~",
          "speaker_id": "2",
          "time": "20210825 02:35"
        },
        {
          "id": "MDRW2100012839.1.11",
          "form": "유명하다고 해서",
          "original_form": "유명하다고 해서",
          "speaker_id": "2",
          "time": "20210825 02:35"
        },
        {
          "id": "MDRW2100012839.1.12",
          "form": "봉은사는 어떻던가요",
          "original_form": "봉은사는 어떻던가요",
          "speaker_id": "1",
          "time": "20210825 02:36"
        },
        {
          "id": "MDRW2100012839.1.13",
          "form": "이름만 들어본 곳이라 궁금하네요",
          "original_form": "이름만 들어본 곳이라 궁금하네요",
          "speaker_id": "1",
          "time": "20210825 02:36"
        },
        {
          "id": "MDRW2100012839.1.14",
          "form": "절이 크고 불상도 컸어요",
          "original_form": "절이 크고 불상도 컸어요",
          "speaker_id": "2",
          "time": "20210825 02:36"
        },
        {
          "id": "MDRW2100012839.1.15",
          "form": "교회 가보셨나요?",
          "original_form": "교회 가보셨나요?",
          "speaker_id": "2",
          "time": "20210825 02:37"
        },
        {
          "id": "MDRW2100012839.1.16",
          "form": "아 대학교 때 선배한테 끌려서요",
          "original_form": "아 대학교 때 선배한테 끌려서요",
          "speaker_id": "1",
          "time": "20210825 02:38"
        },
        {
          "id": "MDRW2100012839.1.17",
          "form": "그런데 너무 저와 안 맞더라고요",
          "original_form": "그런데 너무 저와 안 맞더라고요",
          "speaker_id": "1",
          "time": "20210825 02:38"
        },
        {
          "id": "MDRW2100012839.1.18",
          "form": "교회는 어쩌다 가보셨어요?",
          "original_form": "교회는 어쩌다 가보셨어요?",
          "speaker_id": "1",
          "time": "20210825 02:39"
        },
        {
          "id": "MDRW2100012839.1.19",
          "form": "선배가 전도사였나요?",
          "original_form": "선배가 전도사였나요?",
          "speaker_id": "2",
          "time": "20210825 02:40"
        },
        {
          "id": "MDRW2100012839.1.20",
          "form": "강남에 갔다가 가까워서 가보게되었어요",
          "original_form": "강남에 갔다가 가까워서 가보게되었어요",
          "speaker_id": "2",
          "time": "20210825 02:40"
        },
        {
          "id": "MDRW2100012839.1.21",
          "form": "아 저처럼 끌려서 가셨나 했어요ㅎ",
          "original_form": "아 저처럼 끌려서 가셨나 했어요ㅎ",
          "speaker_id": "1",
          "time": "20210825 02:43"
        },
        {
          "id": "MDRW2100012839.1.22",
          "form": "선배는 전도사는 아녔는데 워낙 신실하셨죠",
          "original_form": "선배는 전도사는 아녔는데 워낙 신실하셨죠",
          "speaker_id": "1",
          "time": "20210825 02:44"
        },
        {
          "id": "MDRW2100012839.1.23",
          "form": "아ㅋㅋ",
          "original_form": "아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 02:45"
        }
      ]
    }
