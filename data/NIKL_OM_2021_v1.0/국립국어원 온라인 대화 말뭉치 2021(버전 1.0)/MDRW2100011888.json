      "utterance": [
        {
          "id": "MDRW2100011888.1.1",
          "form": "그러니까 산책 갈려 했는데",
          "original_form": "그러니까 산책 갈려 했는데",
          "speaker_id": "2",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100011888.1.2",
          "form": "오늘도 가려고 했었어?",
          "original_form": "오늘도 가려고 했었어?",
          "speaker_id": "1",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100011888.1.3",
          "form": "응 날씨만 좋으면",
          "original_form": "응 날씨만 좋으면",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011888.1.4",
          "form": "몇 시쯤 가려 그랬는데?",
          "original_form": "몇 시쯤 가려 그랬는데?",
          "speaker_id": "1",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011888.1.5",
          "form": "6시쯤?",
          "original_form": "6시쯤?",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011888.1.6",
          "form": "그때 나가면 산책하기 딱 좋아?",
          "original_form": "그때 나가면 산책하기 딱 좋아?",
          "speaker_id": "1",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011888.1.7",
          "form": "응 좋아",
          "original_form": "응 좋아",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011888.1.8",
          "form": "요즘도 중앙공원까지 갔다 오나?",
          "original_form": "요즘도 중앙공원까지 갔다 오나?",
          "speaker_id": "1",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100011888.1.9",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100011888.1.10",
          "form": "꽤 먼 데 안 힘들어?",
          "original_form": "꽤 먼 데 안 힘들어?",
          "speaker_id": "1",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100011888.1.11",
          "form": "괜찮아 할만해",
          "original_form": "괜찮아 할만해",
          "speaker_id": "2",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100011888.1.12",
          "form": "거기까지 갔다 오는 시간 얼마나 걸리지?",
          "original_form": "거기까지 갔다 오는 시간 얼마나 걸리지?",
          "speaker_id": "1",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100011888.1.13",
          "form": "한 2시간?",
          "original_form": "한 2시간?",
          "speaker_id": "2",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100011888.1.14",
          "form": "어 꽤 오래 걸린다",
          "original_form": "어 꽤 오래 걸린다",
