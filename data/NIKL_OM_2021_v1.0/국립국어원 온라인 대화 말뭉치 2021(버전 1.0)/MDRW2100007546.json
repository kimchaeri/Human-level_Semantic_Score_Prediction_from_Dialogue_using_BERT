      "utterance": [
        {
          "id": "MDRW2100007546.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007546.1.2",
          "form": "오늘 맛있는거 드셨나여",
          "original_form": "오늘 맛있는거 드셨나여",
          "speaker_id": "1",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007546.1.3",
          "form": "마라탕 먹었어용",
          "original_form": "마라탕 먹었어용",
          "speaker_id": "2",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007546.1.4",
          "form": "마라탕 좋아하세요?",
          "original_form": "마라탕 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007546.1.5",
          "form": "엄청 좋아하져ㅠ",
          "original_form": "엄청 좋아하져ㅠ",
          "speaker_id": "1",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007546.1.6",
          "form": "없어서 못먹는ㅠㅠ",
          "original_form": "없어서 못먹는ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007546.1.7",
          "form": "맵게 드세요??",
          "original_form": "맵게 드세요??",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007546.1.8",
          "form": "저 완전 맵찔이라서 순한맛으로 먹어요...",
          "original_form": "저 완전 맵찔이라서 순한맛으로 먹어요...",
          "speaker_id": "2",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007546.1.9",
          "form": "매운거 잘드세요?",
          "original_form": "매운거 잘드세요?",
          "speaker_id": "2",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007546.1.10",
          "form": "아아 저도 맵찔인데 마라탕은 얼얼하게 먹는걸 좋아해요!",
          "original_form": "아아 저도 맵찔인데 마라탕은 얼얼하게 먹는걸 좋아해요!",
          "speaker_id": "1",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007546.1.11",
          "form": "오늘 저는 요즘 핫한 그릭복숭아를 만들어 먹었답니당",
          "original_form": "오늘 저는 요즘 핫한 그릭복숭아를 만들어 먹었답니당",
          "speaker_id": "1",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007546.1.12",
          "form": "맛있으셨나요? 저도 그릭 복숭아 먹어보고싶어요",
          "original_form": "맛있으셨나요? 저도 그릭 복숭아 먹어보고싶어요",
          "speaker_id": "2",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007546.1.13",
          "form": "맛잇었어요!",
          "original_form": "맛잇었어요!",
          "speaker_id": "1",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007546.1.14",
          "form": "씨 파내는게 어렵더라구요ㅠㅠ",
          "original_form": "씨 파내는게 어렵더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007546.1.15",
          "form": "잘 안떨어져나와서ㅠ",
          "original_form": "잘 안떨어져나와서ㅠ",
          "speaker_id": "1",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007546.1.16",
          "form": "헉 그렇군요ㅠㅠㅠ 저도 다음에 꼭 해먹어야겠네요",
          "original_form": "헉 그렇군요ㅠㅠㅠ 저도 다음에 꼭 해먹어야겠네요",
          "speaker_id": "2",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007546.1.17",
          "form": "네네 추천해요!",
          "original_form": "네네 추천해요!",
          "speaker_id": "1",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007546.1.18",
          "form": "꿀 안뿌려도 맛있어요^^",
          "original_form": "꿀 안뿌려도 맛있어요^^",
          "speaker_id": "1",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007546.1.19",
          "form": "그릭 복숭아는 뭔가 인스타 감성...뭔지 아시죠 아무튼 그런 느낌이에요",
          "original_form": "그릭 복숭아는 뭔가 인스타 감성...뭔지 아시죠 아무튼 그런 느낌이에요",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007546.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007546.1.21",
          "form": "저도 유행이라기에 그냥 만들어봤어요ㅎㅎ",
          "original_form": "저도 유행이라기에 그냥 만들어봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007546.1.22",
          "form": "저도 유행 따라 그릭 복숭아 만들어서 시리얼 위에 올려먹어야겠어요!",
          "original_form": "저도 유행 따라 그릭 복숭아 만들어서 시리얼 위에 올려먹어야겠어요!",
          "speaker_id": "2",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007546.1.23",
          "form": "ㅎㅎ 카페에서도 곧 팔것같아요",
          "original_form": "ㅎㅎ 카페에서도 곧 팔것같아요",
          "speaker_id": "1",
          "time": "20210822 03:20"
        }
      ]
    }
