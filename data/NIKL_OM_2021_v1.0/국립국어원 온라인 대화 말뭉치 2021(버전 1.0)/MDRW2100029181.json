      "utterance": [
        {
          "id": "MDRW2100029181.1.1",
          "form": "저 아는 언니는 초밥을 되게 좋아했는데",
          "original_form": "저 아는 언니는 초밥을 되게 좋아했는데",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029181.1.2",
          "form": "갑자기 날거를 못먹게됐다고 하더라고욮",
          "original_form": "갑자기 날거를 못먹게됐다고 하더라고욮",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029181.1.3",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029181.1.4",
          "form": "헉...흐아악... 고통스러워요.. 어떻게 그런..",
          "original_form": "헉...흐아악... 고통스러워요.. 어떻게 그런..",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029181.1.5",
          "form": "심지어 초밥인데...ㅠ",
          "original_form": "심지어 초밥인데...ㅠ",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029181.1.6",
          "form": "저도 초밥 좋아해서 이거 못먹게 되면 슬플거같아요",
          "original_form": "저도 초밥 좋아해서 이거 못먹게 되면 슬플거같아요",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029181.1.7",
          "form": "저도 초밥 좋아해서 그 얘기듣고 많이 속상하더라고요ㅠㅠ",
          "original_form": "저도 초밥 좋아해서 그 얘기듣고 많이 속상하더라고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029181.1.8",
          "form": "흡..... 혹시 어떤 초밥 좋아하세요..?",
          "original_form": "흡..... 혹시 어떤 초밥 좋아하세요..?",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029181.1.9",
          "form": "홍대 여우골의 참치초밥 연어초밥이 진짜 맛있어요..",
          "original_form": "홍대 여우골의 참치초밥 연어초밥이 진짜 맛있어요..",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029181.1.10",
          "form": "저는 은행골 자주가요",
          "original_form": "저는 은행골 자주가요",
          "speaker_id": "2",
          "time": "20210903 06:32"
        },
        {
          "id": "MDRW2100029181.1.11",
          "form": "여우골 되게 맛있다면서요",
          "original_form": "여우골 되게 맛있다면서요",
          "speaker_id": "2",
          "time": "20210903 06:32"
        },
        {
          "id": "MDRW2100029181.1.12",
          "form": "친구들한테 얘기 들었던걱같아요",
          "original_form": "친구들한테 얘기 들었던걱같아요",
          "speaker_id": "2",
          "time": "20210903 06:32"
        },
        {
          "id": "MDRW2100029181.1.13",
          "form": "맞아요ㅠㅠ 아마 여우골이 원조일거예요",
          "original_form": "맞아요ㅠㅠ 아마 여우골이 원조일거예요",
          "speaker_id": "1",
          "time": "20210903 10:15"
        },
        {
          "id": "MDRW2100029181.1.14",
          "form": "헉 그렇군요 담에 한번 먹어볼게요",
          "original_form": "헉 그렇군요 담에 한번 먹어볼게요",
          "speaker_id": "2",
          "time": "20210903 10:21"
        },
        {
          "id": "MDRW2100029181.1.15",
          "form": "아침드셨어요?",
          "original_form": "아침드셨어요?",
          "speaker_id": "2",
          "time": "20210903 10:21"
        },
        {
          "id": "MDRW2100029181.1.16",
          "form": "아직이요! 저는 아침 안먹고 살아서 점심 저녁만 먹어요!",
          "original_form": "아직이요! 저는 아침 안먹고 살아서 점심 저녁만 먹어요!",
          "speaker_id": "1",
          "time": "20210903 10:22"
        },
        {
          "id": "MDRW2100029181.1.17",
          "form": "핫 저두요 ㅎㅎ",
          "original_form": "핫 저두요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100029181.1.18",
          "form": "아침 먹을 시간에 조금이라도 자고싶어서",
          "original_form": "아침 먹을 시간에 조금이라도 자고싶어서",
          "speaker_id": "2",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100029181.1.19",
          "form": "배가 너무 고프네여..",
          "original_form": "배가 너무 고프네여..",
          "speaker_id": "2",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100029181.1.20",
          "form": "간단하게 요거트 같은 건 먹지만 그래도 점심까지 엄청 허기지지 않나요ㅠ?",
          "original_form": "간단하게 요거트 같은 건 먹지만 그래도 점심까지 엄청 허기지지 않나요ㅠ?",
          "speaker_id": "1",
          "time": "20210903 10:33"
        },
        {
          "id": "MDRW2100029181.1.21",
          "form": "그래서 일하더라도 점심시간만 기다리고 있는 거 같아요",
          "original_form": "그래서 일하더라도 점심시간만 기다리고 있는 거 같아요",
          "speaker_id": "1",
          "time": "20210903 10:33"
        },
        {
          "id": "MDRW2100029181.1.22",
          "form": "지금 너무 배고파서 ㅋㅋㅋ 머리가 멈췄어요",
          "original_form": "지금 너무 배고파서 ㅋㅋㅋ 머리가 멈췄어요",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029181.1.23",
          "form": "점심 뭐드세요?",
          "original_form": "점심 뭐드세요?",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029181.1.24",
          "form": "저는 김치찜이랑 계란말이 먹을 거 같아요!!",
          "original_form": "저는 김치찜이랑 계란말이 먹을 거 같아요!!",
          "speaker_id": "1",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100029181.1.25",
          "form": "허억 너무 맛있겠어요ㅠ",
          "original_form": "허억 너무 맛있겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210903 11:26"
        },
        {
          "id": "MDRW2100029181.1.26",
          "form": "김치찜에 계란말이 ㅠㅠ 벌써 침나오는데.... 혼밥이라 김치찜 먹을 수가 없네요",
          "original_form": "김치찜에 계란말이 ㅠㅠ 벌써 침나오는데.... 혼밥이라 김치찜 먹을 수가 없네요",
          "speaker_id": "2",
          "time": "20210903 11:26"
        }
      ]
    }
