      "utterance": [
        {
          "id": "MDRW2100026052.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210901 19:23"
        },
        {
          "id": "MDRW2100026052.1.2",
          "form": "당근마켓에서 기프티콘 팔아봤어요!",
          "original_form": "당근마켓에서 기프티콘 팔아봤어요!",
          "speaker_id": "2",
          "time": "20210901 19:25"
        },
        {
          "id": "MDRW2100026052.1.3",
          "form": "요즘 당근 많이 하시더라구요",
          "original_form": "요즘 당근 많이 하시더라구요",
          "speaker_id": "2",
          "time": "20210901 19:25"
        },
        {
          "id": "MDRW2100026052.1.4",
          "form": "저도 당근",
          "original_form": "저도 당근",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026052.1.5",
          "form": "오 자주 거래하시나요?",
          "original_form": "오 자주 거래하시나요?",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026052.1.6",
          "form": "가끔씩요",
          "original_form": "가끔씩요",
          "speaker_id": "1",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100026052.1.7",
          "form": "최근 구매 물건은 뭐가 있으신가요?",
          "original_form": "최근 구매 물건은 뭐가 있으신가요?",
          "speaker_id": "2",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026052.1.8",
          "form": "팔기만요",
          "original_form": "팔기만요",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026052.1.9",
          "form": "아하 저는 홈플러스에서 장봐왔어요",
          "original_form": "아하 저는 홈플러스에서 장봐왔어요",
          "speaker_id": "2",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100026052.1.10",
          "form": "요즘 꽃게가 철이더라구요",
          "original_form": "요즘 꽃게가 철이더라구요",
          "speaker_id": "2",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100026052.1.11",
          "form": "오 맛나겠네녀",
          "original_form": "오 맛나겠네녀",
          "speaker_id": "1",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100026052.1.12",
          "form": "살아있는 꽃게를 팔더라구요",
          "original_form": "살아있는 꽃게를 팔더라구요",
          "speaker_id": "2",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100026052.1.13",
          "form": "헉 요리가",
          "original_form": "헉 요리가",
          "speaker_id": "1",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100026052.1.14",
          "form": "저도 겨우 손질하고 그냥 쪄먹고있어요",
          "original_form": "저도 겨우 손질하고 그냥 쪄먹고있어요",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
