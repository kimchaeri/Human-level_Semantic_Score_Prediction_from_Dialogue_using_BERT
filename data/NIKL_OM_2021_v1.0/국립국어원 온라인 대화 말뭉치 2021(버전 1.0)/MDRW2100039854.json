      "utterance": [
        {
          "id": "MDRW2100039854.1.1",
          "form": "동아리 안해요! 편입 준비중이라ㅜㅜ",
          "original_form": "동아리 안해요! 편입 준비중이라ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039854.1.2",
          "form": "오래있다가 가요ㅜㅠ",
          "original_form": "오래있다가 가요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100039854.1.3",
          "form": "날씨오떠세요",
          "original_form": "날씨오떠세요",
          "speaker_id": "2",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100039854.1.4",
          "form": "좋아요ㅋㅋㅋㅋㅋ",
          "original_form": "좋아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039854.1.5",
          "form": "며칠동안 비오다가 오늘 진짜 맑아서",
          "original_form": "며칠동안 비오다가 오늘 진짜 맑아서",
          "speaker_id": "1",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039854.1.6",
          "form": "너무 좋았어요!",
          "original_form": "너무 좋았어요!",
          "speaker_id": "1",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039854.1.7",
          "form": "오 저도 그래요 오늘 선선하고 비도안오고",
          "original_form": "오 저도 그래요 오늘 선선하고 비도안오고",
          "speaker_id": "2",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039854.1.8",
          "form": "맞아요 문 열어놓으니까 길가던 사람이랑 꼭 사랑에 빠질거같은 날씨였어요",
          "original_form": "맞아요 문 열어놓으니까 길가던 사람이랑 꼭 사랑에 빠질거같은 날씨였어요",
          "speaker_id": "1",
          "time": "20210903 22:13"
        },
        {
          "id": "MDRW2100039854.1.9",
          "form": "우와 이쁜 표현이네요",
          "original_form": "우와 이쁜 표현이네요",
          "speaker_id": "2",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039854.1.10",
          "form": "저 가을옷도 샀어요! 짱이죠",
          "original_form": "저 가을옷도 샀어요! 짱이죠",
          "speaker_id": "1",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039854.1.11",
          "form": "저도 친구랑 산책다녀왔어요 밤에",
          "original_form": "저도 친구랑 산책다녀왔어요 밤에",
          "speaker_id": "2",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039854.1.12",
          "form": "헉 감사합니다ㅋㅋㅋㅋ",
          "original_form": "헉 감사합니다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039854.1.13",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039854.1.14",
          "form": "저는 여기 친구가 없어요ㅠㅠ 산책 저도 가고싶어요",
          "original_form": "저는 여기 친구가 없어요ㅠㅠ 산책 저도 가고싶어요",
          "speaker_id": "1",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039854.1.15",
          "form": "와 저도 가을옷 고민중인데",
          "original_form": "와 저도 가을옷 고민중인데",
          "speaker_id": "2",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039854.1.16",
          "form": "어떤거 샀어요",
          "original_form": "어떤거 샀어요",
          "speaker_id": "2",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039854.1.17",
          "form": "아직 안왔어요..",
          "original_form": "아직 안왔어요..",
          "speaker_id": "1",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039854.1.18",
          "form": "오잉",
          "original_form": "오잉",
          "speaker_id": "1",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039854.1.19",
          "form": "바지랑 치마랑",
          "original_form": "바지랑 치마랑",
          "speaker_id": "1",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039854.1.20",
          "form": "티 두 개랑..?",
          "original_form": "티 두 개랑..?",
          "speaker_id": "1",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039854.1.21",
          "form": "어디가 나쁜말이죠ㅋㅋㅋㅋㅠㅠㅜ으앙",
          "original_form": "어디가 나쁜말이죠ㅋㅋㅋㅋㅠㅠㅜ으앙",
          "speaker_id": "1",
          "time": "20210903 22:15"
        },
        {
          "id": "MDRW2100039854.1.22",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 22:15"
        },
        {
          "id": "MDRW2100039854.1.23",
          "form": "저도 신발 흰바지 검은니트 고민중이에여",
          "original_form": "저도 신발 흰바지 검은니트 고민중이에여",
          "speaker_id": "2",
          "time": "20210903 22:15"
        },
        {
          "id": "MDRW2100039854.1.24",
          "form": "흰바지!! 저 샀어요!!!",
          "original_form": "흰바지!! 저 샀어요!!!",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039854.1.25",
          "form": "여태 검은색 하의만 입다가ㅋㅋㅋ 여름옷은 거의 흰색 검정인데",
          "original_form": "여태 검은색 하의만 입다가ㅋㅋㅋ 여름옷은 거의 흰색 검정인데",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039854.1.26",
          "form": "가을~겨울옷만 알록달록하네요...",
          "original_form": "가을~겨울옷만 알록달록하네요...",
          "speaker_id": "1",
          "time": "20210903 22:16"
        }
      ]
    }
