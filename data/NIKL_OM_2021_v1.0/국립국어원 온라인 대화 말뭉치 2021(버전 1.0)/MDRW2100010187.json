      "utterance": [
        {
          "id": "MDRW2100010187.1.1",
          "form": "오늘 점심 메뉴는 뭐였어?",
          "original_form": "오늘 점심 메뉴는 뭐였어?",
          "speaker_id": "1",
          "time": "20210823 15:21"
        },
        {
          "id": "MDRW2100010187.1.2",
          "form": "샐러드 ㅋㅋㄱ다이어트시작했다",
          "original_form": "샐러드 ㅋㅋㄱ다이어트시작했다",
          "speaker_id": "2",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010187.1.3",
          "form": "헐 화이팅이야",
          "original_form": "헐 화이팅이야",
          "speaker_id": "1",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010187.1.4",
          "form": "나는 김치찌개 먹었어",
          "original_form": "나는 김치찌개 먹었어",
          "speaker_id": "1",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010187.1.5",
          "form": "ㅋㅋㄱ맛있었겠다 직장에서 먹은거야?",
          "original_form": "ㅋㅋㄱ맛있었겠다 직장에서 먹은거야?",
          "speaker_id": "2",
          "time": "20210823 15:24"
        },
        {
          "id": "MDRW2100010187.1.6",
          "form": "아니아니 집에서ㅋㅋㅋ 대학생이거든",
          "original_form": "아니아니 집에서ㅋㅋㅋ 대학생이거든",
          "speaker_id": "1",
          "time": "20210823 15:28"
        },
        {
          "id": "MDRW2100010187.1.7",
          "form": "직장인이야?",
          "original_form": "직장인이야?",
          "speaker_id": "1",
          "time": "20210823 15:28"
        },
        {
          "id": "MDRW2100010187.1.8",
          "form": "응 나는 직장인 ㅋㅋ",
          "original_form": "응 나는 직장인 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:30"
        },
        {
          "id": "MDRW2100010187.1.9",
          "form": "나도 대학생때로 돌아가고싶다 ㅠ",
          "original_form": "나도 대학생때로 돌아가고싶다 ㅠ",
          "speaker_id": "2",
          "time": "20210823 15:30"
        },
        {
          "id": "MDRW2100010187.1.10",
          "form": "다시 대학교 다니자",
          "original_form": "다시 대학교 다니자",
          "speaker_id": "1",
          "time": "20210823 15:33"
        },
        {
          "id": "MDRW2100010187.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:33"
        },
        {
          "id": "MDRW2100010187.1.12",
          "form": "그럼 거기서 샐러드 도시락을 싸 간 거야?",
          "original_form": "그럼 거기서 샐러드 도시락을 싸 간 거야?",
          "speaker_id": "1",
          "time": "20210823 15:33"
        },
        {
          "id": "MDRW2100010187.1.13",
          "form": "이 나이에? 오바야 ㅋㅋ",
          "original_form": "이 나이에? 오바야 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:33"
        },
        {
          "id": "MDRW2100010187.1.14",
          "form": "아니 여기 샐러드전문점가서 7천원주고 사 먹었어 ㅋㅋ",
          "original_form": "아니 여기 샐러드전문점가서 7천원주고 사 먹었어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:33"
        },
        {
          "id": "MDRW2100010187.1.15",
          "form": "밥보다 더비싸 ㅋㅋ",
          "original_form": "밥보다 더비싸 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:33"
        },
        {
          "id": "MDRW2100010187.1.16",
          "form": "와ㅋㅋㅋㅋ",
          "original_form": "와ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100010187.1.17",
          "form": "주변에 전문점이 있다는 게 신기해",
          "original_form": "주변에 전문점이 있다는 게 신기해",
          "speaker_id": "1",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100010187.1.18",
          "form": "1시에 먹었는데 벌써 배고픔 ㅋㅋㄱ",
          "original_form": "1시에 먹었는데 벌써 배고픔 ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100010187.1.19",
          "form": "그것도 샐러드전문점이",
          "original_form": "그것도 샐러드전문점이",
          "speaker_id": "1",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100010187.1.20",
          "form": "여기 서울이야 ㅋㅋ너 사는곳은 어디야?",
          "original_form": "여기 서울이야 ㅋㅋ너 사는곳은 어디야?",
          "speaker_id": "2",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100010187.1.21",
          "form": "풀때기 먹었으니 배고플만두 하다..",
          "original_form": "풀때기 먹었으니 배고플만두 하다..",
          "speaker_id": "1",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100010187.1.22",
          "form": "나 사는 곳 경기!ㅋㅋ",
          "original_form": "나 사는 곳 경기!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100010187.1.23",
          "form": "오리고기샐러드먹었어 ㅋㅋ고기도 있었음",
          "original_form": "오리고기샐러드먹었어 ㅋㅋ고기도 있었음",
          "speaker_id": "2",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100010187.1.24",
          "form": "삼겹살 집을 가도 상추는 주는데",
          "original_form": "삼겹살 집을 가도 상추는 주는데",
          "speaker_id": "1",
          "time": "20210823 15:35"
        },
        {
          "id": "MDRW2100010187.1.25",
          "form": "샐러드에 고기가 곁들어간 걸 먹다니",
          "original_form": "샐러드에 고기가 곁들어간 걸 먹다니",
          "speaker_id": "1",
          "time": "20210823 15:35"
        },
        {
          "id": "MDRW2100010187.1.26",
          "form": "하긴 다이어트랬지",
          "original_form": "하긴 다이어트랬지",
          "speaker_id": "1",
          "time": "20210823 15:35"
        },
        {
          "id": "MDRW2100010187.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ근데 말로만 다이어트야 항상",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ근데 말로만 다이어트야 항상",
          "speaker_id": "2",
          "time": "20210823 15:36"
        },
        {
          "id": "MDRW2100010187.1.28",
          "form": "이건 식문화의 붕괴야..!",
          "original_form": "이건 식문화의 붕괴야..!",
          "speaker_id": "1",
          "time": "20210823 15:36"
        },
        {
          "id": "MDRW2100010187.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜 그냥 해야겠는데 귀찮은 거?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜 그냥 해야겠는데 귀찮은 거?",
          "speaker_id": "1",
          "time": "20210823 15:36"
        },
        {
          "id": "MDRW2100010187.1.30",
          "form": "ㅠㅠ 운동도해야되고 ㅜ아 저녁을 안 먹어야되는데 저녁은 항상 치킨 족발 ㅋㅋㅋ",
          "original_form": "ㅠㅠ 운동도해야되고 ㅜ아 저녁을 안 먹어야되는데 저녁은 항상 치킨 족발 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:36"
        },
        {
          "id": "MDRW2100010187.1.31",
          "form": "훌륭한 선택이야",
          "original_form": "훌륭한 선택이야",
          "speaker_id": "1",
          "time": "20210823 15:37"
        },
        {
          "id": "MDRW2100010187.1.32",
          "form": "치킨 족발 나두 먹고 싶다",
          "original_form": "치킨 족발 나두 먹고 싶다",
          "speaker_id": "1",
          "time": "20210823 15:37"
        },
        {
          "id": "MDRW2100010187.1.33",
          "form": "오늘 뿌링클을 먹을까",
          "original_form": "오늘 뿌링클을 먹을까",
          "speaker_id": "1",
          "time": "20210823 15:37"
        },
        {
          "id": "MDRW2100010187.1.34",
          "form": "ㅋㄱㄱ오 탁월한선택이야 ㅋㅋ",
          "original_form": "ㅋㄱㄱ오 탁월한선택이야 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:40"
        }
      ]
