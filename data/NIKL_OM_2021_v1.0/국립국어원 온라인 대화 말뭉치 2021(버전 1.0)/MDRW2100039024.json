      "utterance": [
        {
          "id": "MDRW2100039024.1.1",
          "form": "디피 드라마 시작햇어요 저",
          "original_form": "디피 드라마 시작햇어요 저",
          "speaker_id": "2",
          "time": "20210903 02:16"
        },
        {
          "id": "MDRW2100039024.1.2",
          "form": "디피 영화아니고 드라마였어요???",
          "original_form": "디피 영화아니고 드라마였어요???",
          "speaker_id": "1",
          "time": "20210903 02:17"
        },
        {
          "id": "MDRW2100039024.1.3",
          "form": "그거 맨날 넷플 1위더만요",
          "original_form": "그거 맨날 넷플 1위더만요",
          "speaker_id": "1",
          "time": "20210903 02:17"
        },
        {
          "id": "MDRW2100039024.1.4",
          "form": "들마ㅇㅇ",
          "original_form": "들마ㅇㅇ",
          "speaker_id": "2",
          "time": "20210903 02:18"
        },
        {
          "id": "MDRW2100039024.1.5",
          "form": "정해인이랑 요즘 핫해진 누구 나옴",
          "original_form": "정해인이랑 요즘 핫해진 누구 나옴",
          "speaker_id": "2",
          "time": "20210903 02:18"
        },
        {
          "id": "MDRW2100039024.1.6",
          "form": "아아 구씨였만데 구기환인가 구교환인가",
          "original_form": "아아 구씨였만데 구기환인가 구교환인가",
          "speaker_id": "1",
          "time": "20210903 02:20"
        },
        {
          "id": "MDRW2100039024.1.7",
          "form": "아마자요 구기환은 제 학교 사람중에 그런사람잇엇는데...",
          "original_form": "아마자요 구기환은 제 학교 사람중에 그런사람잇엇는데...",
          "speaker_id": "2",
          "time": "20210903 02:20"
        },
        {
          "id": "MDRW2100039024.1.8",
          "form": "아미고 구교환이요",
          "original_form": "아미고 구교환이요",
          "speaker_id": "2",
          "time": "20210903 02:20"
        },
        {
          "id": "MDRW2100039024.1.9",
          "form": "익스체인지",
          "original_form": "익스체인지",
          "speaker_id": "2",
          "time": "20210903 02:20"
        },
        {
          "id": "MDRW2100039024.1.10",
          "form": "아앗 익스체인지....넵..^&^",
          "original_form": "아앗 익스체인지....넵..^&^",
          "speaker_id": "1",
          "time": "20210903 02:21"
        },
        {
          "id": "MDRW2100039024.1.11",
          "form": "디피 어때요",
          "original_form": "디피 어때요",
          "speaker_id": "1",
          "time": "20210903 02:21"
        },
        {
          "id": "MDRW2100039024.1.12",
          "form": "추천 잼던데요 근데좀 어둡",
          "original_form": "추천 잼던데요 근데좀 어둡",
          "speaker_id": "2",
          "time": "20210903 02:21"
        },
        {
          "id": "MDRW2100039024.1.13",
          "form": "몇화 까지 봤는데요?",
          "original_form": "몇화 까지 봤는데요?",
          "speaker_id": "1",
          "time": "20210903 02:22"
        },
        {
          "id": "MDRW2100039024.1.14",
          "form": "구교환..흠 좀 매력 잇을 유",
          "original_form": "구교환..흠 좀 매력 잇을 유",
          "speaker_id": "2",
          "time": "20210903 02:22"
        },
        {
          "id": "MDRW2100039024.1.15",
          "form": "3화",
          "original_form": "3화",
          "speaker_id": "2",
          "time": "20210903 02:22"
        },
        {
          "id": "MDRW2100039024.1.16",
          "form": "원작잇대여",
          "original_form": "원작잇대여",
          "speaker_id": "2",
          "time": "20210903 02:22"
        },
        {
          "id": "MDRW2100039024.1.17",
          "form": "아 저 구교환 한번 본적있긴해요 영화 매기에서",
          "original_form": "아 저 구교환 한번 본적있긴해요 영화 매기에서",
          "speaker_id": "1",
          "time": "20210903 02:24"
        },
        {
          "id": "MDRW2100039024.1.18",
          "form": "오잉 그건 머지언제나온거요",
          "original_form": "오잉 그건 머지언제나온거요",
          "speaker_id": "2",
          "time": "20210903 02:24"
        },
        {
          "id": "MDRW2100039024.1.19",
          "form": "거기서 좀 ㅋㅋㅋ 또라이같았는데 ㅋㅋㅋㅋ",
          "original_form": "거기서 좀 ㅋㅋㅋ 또라이같았는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 02:24"
        },
        {
          "id": "MDRW2100039024.1.20",
          "form": "ㅋㅋㅋㅋ 디피에서도 약간 그럼",
          "original_form": "ㅋㅋㅋㅋ 디피에서도 약간 그럼",
          "speaker_id": "2",
          "time": "20210903 02:24"
        },
        {
          "id": "MDRW2100039024.1.21",
          "form": "영화는 나온지 좀 됐을걸요??ㅌㅋㅋㅋㅋ",
