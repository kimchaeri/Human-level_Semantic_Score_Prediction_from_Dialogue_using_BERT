      "utterance": [
        {
          "id": "MDRW2100004507.1.1",
          "form": "전 집에서 소소한 알바해요",
          "original_form": "전 집에서 소소한 알바해요",
          "speaker_id": "2",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100004507.1.2",
          "form": "무슨일 하세요?",
          "original_form": "무슨일 하세요?",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100004507.1.3",
          "form": "저는 평범한 회사원이요",
          "original_form": "저는 평범한 회사원이요",
          "speaker_id": "1",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100004507.1.4",
          "form": "회사원이 최고예요",
          "original_form": "회사원이 최고예요",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100004507.1.5",
          "form": "전요새 나가서 알바 할까해도 마땅치 않더 라구요",
          "original_form": "전요새 나가서 알바 할까해도 마땅치 않더 라구요",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100004507.1.6",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100004507.1.7",
          "form": "네 월급은 사장이 걱정해주고 얼마나 좋아요",
          "original_form": "네 월급은 사장이 걱정해주고 얼마나 좋아요",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100004507.1.8",
          "form": "연초에 월급 인상해줬나요?",
          "original_form": "연초에 월급 인상해줬나요?",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100004507.1.9",
          "form": "저희 애아빠는 뭐 팍 올려준다고 뻥치고 쥐꼬리만큼 올렸네요",
          "original_form": "저희 애아빠는 뭐 팍 올려준다고 뻥치고 쥐꼬리만큼 올렸네요",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100004507.1.10",
          "form": "전 집에서 유투브 블로그 보고 상장하는 공모주 청약해서 소소히 벌어요",
          "original_form": "전 집에서 유투브 블로그 보고 상장하는 공모주 청약해서 소소히 벌어요",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100004507.1.11",
          "form": "에고 월급 팍 인상해줘라  좀!!",
          "original_form": "에고 월급 팍 인상해줘라  좀!!",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100004507.1.12",
          "form": "얼마 안되지만 요새 왠지 활력소 되서 좋아요",
          "original_form": "얼마 안되지만 요새 왠지 활력소 되서 좋아요",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100004507.1.13",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100004507.1.14",
          "form": "오오 재테크하시는구나",
          "original_form": "오오 재테크하시는구나",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100004507.1.15",
          "form": "다비이모좀 불러야 겠어요",
          "original_form": "다비이모좀 불러야 겠어요",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100004507.1.16",
          "form": "네 조금해요",
          "original_form": "네 조금해요",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100004507.1.17",
          "form": "ㅎㅎ주변에 급여만으로는 경제적으로 부족하다고",
          "original_form": "ㅎㅎ주변에 급여만으로는 경제적으로 부족하다고",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100004507.1.18",
          "form": "주식하는 분들 많더라고요",
          "original_form": "주식하는 분들 많더라고요",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100004507.1.19",
          "form": "저희 여동생도 알혀주니 회사에서 몰래해요 ㅋㅋ",
          "original_form": "저희 여동생도 알혀주니 회사에서 몰래해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100004507.1.20",
          "form": "전 주식은 망해서 공모주만해요",
          "original_form": "전 주식은 망해서 공모주만해요",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100004507.1.21",
          "form": "요번에 수익 괜찮았어요",
          "original_form": "요번에 수익 괜찮았어요",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100004507.1.22",
          "form": "어제도 따상가까이 가고요 ㅎㅎ",
          "original_form": "어제도 따상가까이 가고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100004507.1.23",
          "form": "오오 축하드려요ㅎㅎ",
          "original_form": "오오 축하드려요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100004507.1.24",
          "form": "네 요번에ㅋㅋ계속 좋았어요",
          "original_form": "네 요번에ㅋㅋ계속 좋았어요",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100004507.1.25",
          "form": "주식 투자한건 ㅡㅡ계속 기다리고요",
          "original_form": "주식 투자한건 ㅡㅡ계속 기다리고요",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100004507.1.26",
          "form": "굳이 직장생활 하지 않으셔도",
          "original_form": "굳이 직장생활 하지 않으셔도",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100004507.1.27",
          "form": "괜찮으시겠는걸요?",
          "original_form": "괜찮으시겠는걸요?",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100004507.1.28",
          "form": "소소해요 ㅋㅋ",
          "original_form": "소소해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100004507.1.29",
          "form": "정말 학원비요",
          "original_form": "정말 학원비요",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100004507.1.30",
          "form": "큰돈으로 안해서요",
          "original_form": "큰돈으로 안해서요",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100004507.1.31",
          "form": "그래도요~",
          "original_form": "그래도요~",
          "speaker_id": "1",
          "time": "20210821 21:44"
        }
      ]
    }
