      "utterance": [
        {
          "id": "MDRW2100036266.1.1",
          "form": "마라탕 한달에 한번씩 땡겨요ㅠㅠ ㅋㅋㅋ",
          "original_form": "마라탕 한달에 한번씩 땡겨요ㅠㅠ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100036266.1.2",
          "form": "근데 매운맛하면후회하는데 또 매운맛시키죠ㅠㅠ",
          "original_form": "근데 매운맛하면후회하는데 또 매운맛시키죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100036266.1.3",
          "form": "매운게 중독이라고 하더라구용",
          "original_form": "매운게 중독이라고 하더라구용",
          "speaker_id": "1",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100036266.1.4",
          "form": "그래서 한번 먹기시작하면 습관이 된다구ㅜㅜ",
          "original_form": "그래서 한번 먹기시작하면 습관이 된다구ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100036266.1.5",
          "form": "고통을 즐기는건거같아요ㅠ",
          "original_form": "고통을 즐기는건거같아요ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100036266.1.6",
          "form": "우유 드시고 드셔요!ㅠ",
          "original_form": "우유 드시고 드셔요!ㅠ",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100036266.1.7",
          "form": "우유는 후에 마시면 더 독이래요!!",
          "original_form": "우유는 후에 마시면 더 독이래요!!",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100036266.1.8",
          "form": "전에마셔야지ㅠ",
          "original_form": "전에마셔야지ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100036266.1.9",
          "form": "넹넹 어쩔수없이 매운거 먹는날",
          "original_form": "넹넹 어쩔수없이 매운거 먹는날",
          "speaker_id": "1",
          "time": "20210830 20:35"
        },
        {
          "id": "MDRW2100036266.1.10",
          "form": "저도 항상 우유 먹고 먹어요",
          "original_form": "저도 항상 우유 먹고 먹어요",
          "speaker_id": "1",
          "time": "20210830 20:35"
        },
        {
          "id": "MDRW2100036266.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:35"
        },
        {
          "id": "MDRW2100036266.1.12",
          "form": "ㅋㅋㅋ그러시군요 !!",
          "original_form": "ㅋㅋㅋ그러시군요 !!",
          "speaker_id": "2",
          "time": "20210830 20:37"
        },
        {
          "id": "MDRW2100036266.1.13",
          "form": "미리준비해둬야겟어요 ~~",
          "original_form": "미리준비해둬야겟어요 ~~",
          "speaker_id": "2",
          "time": "20210830 20:37"
        },
        {
          "id": "MDRW2100036266.1.14",
          "form": "신림동에 엄~청 매운걸로 유명한 짬뽕집이 있는데",
          "original_form": "신림동에 엄~청 매운걸로 유명한 짬뽕집이 있는데",
          "speaker_id": "1",
          "time": "20210830 20:37"
        },
        {
          "id": "MDRW2100036266.1.15",
          "form": "혹시 들어보셨어요?",
          "original_form": "혹시 들어보셨어요?",
          "speaker_id": "1",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100036266.1.16",
          "form": "티비에서봤어요!!",
          "original_form": "티비에서봤어요!!",
          "speaker_id": "2",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100036266.1.17",
          "form": "가본적은없지만 ㅠㅠ",
          "original_form": "가본적은없지만 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100036266.1.18",
          "form": "전 저기 가봤는데ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 저기 가봤는데ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:39"
        },
        {
          "id": "MDRW2100036266.1.19",
          "form": "친구가 먹는거 진짜 한 숟가락만 떠먹어봤거든용",
          "original_form": "친구가 먹는거 진짜 한 숟가락만 떠먹어봤거든용",
          "speaker_id": "1",
          "time": "20210830 20:39"
        },
        {
          "id": "MDRW2100036266.1.20",
          "form": "근데 빈종이컵에 그대로 뱉었어욬ㅋㅋㅋㅋ",
          "original_form": "근데 빈종이컵에 그대로 뱉었어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
