      "utterance": [
        {
          "id": "MDRW2100042298.1.1",
          "form": "사골육수 좋네요 ! 이금기 ~",
          "original_form": "사골육수 좋네요 ! 이금기 ~",
          "speaker_id": "1",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042298.1.2",
          "form": "샤인머그캣 첨사머거봣는데",
          "original_form": "샤인머그캣 첨사머거봣는데",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042298.1.3",
          "form": "먀싯네요",
          "original_form": "먀싯네요",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042298.1.4",
          "form": "아 진짜 맛있죠?",
          "original_form": "아 진짜 맛있죠?",
          "speaker_id": "2",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042298.1.5",
          "form": "비싼만큼 과일은 맛잇는거 같아요",
          "original_form": "비싼만큼 과일은 맛잇는거 같아요",
          "speaker_id": "2",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042298.1.6",
          "form": "넹넹 짱맛.....",
          "original_form": "넹넹 짱맛.....",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042298.1.7",
          "form": "과일머조아하세요",
          "original_form": "과일머조아하세요",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042298.1.8",
          "form": "샤인도 좋구 복숭아 너무 좋고 단감도 귤도!!!",
          "original_form": "샤인도 좋구 복숭아 너무 좋고 단감도 귤도!!!",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042298.1.9",
          "form": "복숭아~",
          "original_form": "복숭아~",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042298.1.10",
          "form": "딸기는용 ~",
          "original_form": "딸기는용 ~",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042298.1.11",
          "form": "당근 좋아하죠ㅜㅜ",
          "original_form": "당근 좋아하죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042298.1.12",
          "form": "싫어하는 과일있으세요?",
          "original_form": "싫어하는 과일있으세요?",
          "speaker_id": "2",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042298.1.13",
          "form": "ㅋㅋㅋ 음딱히없어요",
          "original_form": "ㅋㅋㅋ 음딱히없어요",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042298.1.14",
          "form": "그죠??",
          "original_form": "그죠??",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042298.1.15",
          "form": "과일다 맛잇죵...",
          "original_form": "과일다 맛잇죵...",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042298.1.16",
          "form": "알러지있음 바나나가 시대요",
          "original_form": "알러지있음 바나나가 시대요",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042298.1.17",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042298.1.18",
          "form": "ㅋㅋㅋㅋㅋ 시다구요",
          "original_form": "ㅋㅋㅋㅋㅋ 시다구요",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042298.1.19",
          "form": "키위처럼",
          "original_form": "키위처럼",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042298.1.20",
          "form": "새콤",
          "original_form": "새콤",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042298.1.21",
