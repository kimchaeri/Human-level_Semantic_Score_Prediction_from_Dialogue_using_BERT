      "utterance": [
        {
          "id": "MDRW2100004020.1.1",
          "form": "안녕~",
          "original_form": "안녕~",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100004020.1.2",
          "form": "책 좋아해??",
          "original_form": "책 좋아해??",
          "speaker_id": "2",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100004020.1.3",
          "form": "나.. 만화책은 좋아하는데",
          "original_form": "나.. 만화책은 좋아하는데",
          "speaker_id": "1",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100004020.1.4",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100004020.1.5",
          "form": "name2는 책 좋아해?",
          "original_form": "&name2&는 책 좋아해?",
          "speaker_id": "1",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100004020.1.6",
          "form": "ㅋㅋㅋㅋㅋ 책 좋아하긴 하는데! 자주 안읽네 ㅋㅋㅋㅋㅋ 웃기다...",
          "original_form": "ㅋㅋㅋㅋㅋ 책 좋아하긴 하는데! 자주 안읽네 ㅋㅋㅋㅋㅋ 웃기다...",
          "speaker_id": "2",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100004020.1.7",
          "form": "ㅋㅋㅋ제일 좋아하는 책이 뭐야?",
          "original_form": "ㅋㅋㅋ제일 좋아하는 책이 뭐야?",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004020.1.8",
          "form": "한권만 고르기 힘들다면 세권정도..ㅎㅎ",
          "original_form": "한권만 고르기 힘들다면 세권정도..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004020.1.9",
          "form": "아 ㅋㅋㅋㅋㅋ 음.....  막상 생각하니 떠오르는게 없네",
          "original_form": "아 ㅋㅋㅋㅋㅋ 음.....  막상 생각하니 떠오르는게 없네",
          "speaker_id": "2",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004020.1.10",
          "form": "혹시 시집도 좋아해? 나는 이제니시인의 시를 좋아하거든!",
          "original_form": "혹시 시집도 좋아해? 나는 이제니시인의 시를 좋아하거든!",
          "speaker_id": "2",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004020.1.11",
          "form": "헉 내가 시에 조예가없어서..",
          "original_form": "헉 내가 시에 조예가없어서..",
          "speaker_id": "1",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004020.1.12",
          "form": "근데 좋아한다니까 궁금하다",
          "original_form": "근데 좋아한다니까 궁금하다",
          "speaker_id": "1",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004020.1.13",
          "form": "다음에 서점에 가게된다면 사볼게",
          "original_form": "다음에 서점에 가게된다면 사볼게",
          "speaker_id": "1",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004020.1.14",
          "form": "아냐아냐 사지는 말구",
          "original_form": "아냐아냐 사지는 말구",
          "speaker_id": "2",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004020.1.15",
          "form": "하나만 읽어봐 !",
          "original_form": "하나만 읽어봐 !",
          "speaker_id": "2",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004020.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ혹시 모르잖아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ혹시 모르잖아",
          "speaker_id": "2",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004020.1.17",
          "form": "아냐",
          "original_form": "아냐",
          "speaker_id": "1",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004020.1.18",
          "form": "난 name2의 추천을 믿어",
          "original_form": "난 &name2&의 추천을 믿어",
          "speaker_id": "1",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004020.1.19",
          "form": "분명 멋진 책일거야",
          "original_form": "분명 멋진 책일거야",
          "speaker_id": "1",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004020.1.20",
          "form": "고마워!!!",
          "original_form": "고마워!!!",
          "speaker_id": "2",
          "time": "20210821 14:46"
        },
        {
          "id": "MDRW2100004020.1.21",
          "form": "녹차는 무슨 만화책 좋아해??",
          "original_form": "녹차는 무슨 만화책 좋아해??",
          "speaker_id": "2",
          "time": "20210821 14:46"
        },
        {
          "id": "MDRW2100004020.1.22",
          "form": "음... 최근에 본것중에서는",
          "original_form": "음... 최근에 본것중에서는",
          "speaker_id": "1",
          "time": "20210821 14:48"
        },
        {
          "id": "MDRW2100004020.1.23",
          "form": "'빠졌어 너에게'?",
          "original_form": "'빠졌어 너에게'?",
          "speaker_id": "1",
          "time": "20210821 14:48"
        },
        {
          "id": "MDRW2100004020.1.24",
          "form": "ㅋㅋㅋ그냥 소소하게 웃긴 단편집이야",
          "original_form": "ㅋㅋㅋ그냥 소소하게 웃긴 단편집이야",
          "speaker_id": "1",
          "time": "20210821 14:48"
        },
        {
          "id": "MDRW2100004020.1.25",
          "form": "방금 검색해봤어! 드라마로도 나왔네??",
          "original_form": "방금 검색해봤어! 드라마로도 나왔네??",
          "speaker_id": "2",
          "time": "20210821 14:49"
        },
        {
          "id": "MDRW2100004020.1.26",
          "form": "나도 다음에 놀숲 (ㅋㅋ) 가게 되면 볼게!!!",
          "original_form": "나도 다음에 놀숲 (ㅋㅋ) 가게 되면 볼게!!!",
          "speaker_id": "2",
          "time": "20210821 14:49"
        },
        {
          "id": "MDRW2100004020.1.27",
          "form": "드..드라마가있어?",
          "original_form": "드..드라마가있어?",
          "speaker_id": "1",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004020.1.28",
          "form": "처음알았어",
          "original_form": "처음알았어",
          "speaker_id": "1",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004020.1.29",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004020.1.30",
          "form": "웨이브에 한 편씩 올라온대!ㅋㅋㅋㅋㅋ",
          "original_form": "웨이브에 한 편씩 올라온대!ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004020.1.31",
          "form": "코로나때문에 만화카페 못간지 오래됐네... 그립다...",
          "original_form": "코로나때문에 만화카페 못간지 오래됐네... 그립다...",
          "speaker_id": "1",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004020.1.32",
          "form": "헐!! 담에 봐야겠다...",
          "original_form": "헐!! 담에 봐야겠다...",
