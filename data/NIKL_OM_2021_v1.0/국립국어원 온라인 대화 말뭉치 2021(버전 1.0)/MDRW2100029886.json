      "utterance": [
        {
          "id": "MDRW2100029886.1.1",
          "form": "점심은 잘 챙겨 드셨나요?",
          "original_form": "점심은 잘 챙겨 드셨나요?",
          "speaker_id": "1",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100029886.1.2",
          "form": "네 오랜만에 집밥",
          "original_form": "네 오랜만에 집밥",
          "speaker_id": "2",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100029886.1.3",
          "form": "본가가서 먹었어요 ㅎㅎ",
          "original_form": "본가가서 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100029886.1.4",
          "form": "오 !! 본가가 가까우세요?",
          "original_form": "오 !! 본가가 가까우세요?",
          "speaker_id": "1",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100029886.1.5",
          "form": "네 지하철로 10정거장정도?",
          "original_form": "네 지하철로 10정거장정도?",
          "speaker_id": "2",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029886.1.6",
          "form": "환승 없이요 ㅋㅋ",
          "original_form": "환승 없이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029886.1.7",
          "form": "헐 완전 가깝죠 그정도면!!!!!",
          "original_form": "헐 완전 가깝죠 그정도면!!!!!",
          "speaker_id": "1",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029886.1.8",
          "form": "저는 같은 서울인데도 끝과 끝이라서 지하철로는 1시간 40분걸려요 ㅋㅋㅋㅋㅋㅋㅠㅠ",
          "original_form": "저는 같은 서울인데도 끝과 끝이라서 지하철로는 1시간 40분걸려요 ㅋㅋㅋㅋㅋㅋㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029886.1.9",
          "form": "집밥 어떤거 드셨나요?",
          "original_form": "집밥 어떤거 드셨나요?",
          "speaker_id": "1",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029886.1.10",
          "form": "돼지고기 불백 느낌?",
          "original_form": "돼지고기 불백 느낌?",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100029886.1.11",
          "form": "자취하세요??",
          "original_form": "자취하세요??",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100029886.1.12",
          "form": "네 자취해서 맨날 대충먹어요 ㅋㅋㅋ 냉동음식 ㅠㅠ",
          "original_form": "네 자취해서 맨날 대충먹어요 ㅋㅋㅋ 냉동음식 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100029886.1.13",
          "form": "에이 냉동은!",
          "original_form": "에이 냉동은!",
          "speaker_id": "2",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100029886.1.14",
          "form": "건강에 안좋을텐데",
          "original_form": "건강에 안좋을텐데",
          "speaker_id": "2",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100029886.1.15",
          "form": "맞아요 근데 퇴근하고나면 뭐 해먹을 힘이 잘 안나더라구요 ㅠ",
          "original_form": "맞아요 근데 퇴근하고나면 뭐 해먹을 힘이 잘 안나더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100029886.1.16",
          "form": "그래서 진짜 자주 시켜먹다가",
          "original_form": "그래서 진짜 자주 시켜먹다가",
          "speaker_id": "1",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100029886.1.17",
          "form": "이제 덜 시켜먹자구 해서 냉동까지 왔어요 ㅋㅋㅋ 좀 더 가다보면 해... 먹겠죠? ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "이제 덜 시켜먹자구 해서 냉동까지 왔어요 ㅋㅋㅋ 좀 더 가다보면 해... 먹겠죠? ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100029886.1.18",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100029886.1.19",
          "form": "제 와이프 보니까",
          "original_form": "제 와이프 보니까",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100029886.1.20",
          "form": "한번 안하니까",
          "original_form": "한번 안하니까",
