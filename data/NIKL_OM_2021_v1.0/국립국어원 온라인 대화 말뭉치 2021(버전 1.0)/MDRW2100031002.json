      "utterance": [
        {
          "id": "MDRW2100031002.1.1",
          "form": "알바 하는거 있으세요?",
          "original_form": "알바 하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100031002.1.2",
          "form": "네 지금 알바하고 있어요",
          "original_form": "네 지금 알바하고 있어요",
          "speaker_id": "2",
          "time": "20210904 18:55"
        },
        {
          "id": "MDRW2100031002.1.3",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210904 18:55"
        },
        {
          "id": "MDRW2100031002.1.4",
          "form": "아 저는 옛날에 한 적 있어요",
          "original_form": "아 저는 옛날에 한 적 있어요",
          "speaker_id": "1",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100031002.1.5",
          "form": "어떤 알바하세요?",
          "original_form": "어떤 알바하세요?",
          "speaker_id": "1",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100031002.1.6",
          "form": "빵집 알바 하고있어요!",
          "original_form": "빵집 알바 하고있어요!",
          "speaker_id": "2",
          "time": "20210905 00:26"
        },
        {
          "id": "MDRW2100031002.1.7",
          "form": "ㅇ어떤 알바 해보셨나요??",
          "original_form": "ㅇ어떤 알바 해보셨나요??",
          "speaker_id": "2",
          "time": "20210905 00:26"
        },
        {
          "id": "MDRW2100031002.1.8",
          "form": "저는 과외알바했어요 빵집알바는 가끔 요기요같은데서 이벤트하면 많이 바쁘지요?",
          "original_form": "저는 과외알바했어요 빵집알바는 가끔 요기요같은데서 이벤트하면 많이 바쁘지요?",
          "speaker_id": "1",
          "time": "20210905 00:31"
        },
        {
          "id": "MDRW2100031002.1.9",
          "form": "맞아오ㅠㅠ  번화가에 있지 않아도 5천원씩 크게 행사하면 바쁜데",
          "original_form": "맞아오ㅠㅠ  번화가에 있지 않아도 5천원씩 크게 행사하면 바쁜데",
          "speaker_id": "2",
          "time": "20210905 00:43"
        },
        {
          "id": "MDRW2100031002.1.10",
          "form": "번화가나 대학가는..얼마나 바쁠지...",
          "original_form": "번화가나 대학가는..얼마나 바쁠지...",
          "speaker_id": "2",
          "time": "20210905 00:43"
        },
        {
          "id": "MDRW2100031002.1.11",
          "form": "실례가 안된다면 어떤 과목 과외하셨는지 물어봐도 괜찮을까요??",
          "original_form": "실례가 안된다면 어떤 과목 과외하셨는지 물어봐도 괜찮을까요??",
          "speaker_id": "2",
          "time": "20210905 00:43"
        },
        {
          "id": "MDRW2100031002.1.12",
          "form": "과외라니 멋있으세요,,,!",
          "original_form": "과외라니 멋있으세요,,,!",
          "speaker_id": "2",
          "time": "20210905 00:43"
        },
        {
          "id": "MDRW2100031002.1.13",
          "form": "아 저는 수학이랑 영어요",
          "original_form": "아 저는 수학이랑 영어요",
          "speaker_id": "1",
          "time": "20210906 13:42"
        },
        {
          "id": "MDRW2100031002.1.14",
          "form": "친구나 선후배 하던거 넘겨받고 그랬네요",
          "original_form": "친구나 선후배 하던거 넘겨받고 그랬네요",
          "speaker_id": "1",
          "time": "20210906 13:42"
        },
        {
          "id": "MDRW2100031002.1.15",
          "form": "우왕 수학이랑 영어 젬병이라...",
          "original_form": "우왕 수학이랑 영어 젬병이라...",
          "speaker_id": "2",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100031002.1.16",
          "form": "진짜 멋지세요...!",
          "original_form": "진짜 멋지세요...!",
          "speaker_id": "2",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100031002.1.17",
          "form": "예전에 카페 갔을때 카페에서 과외하는거 보고 조금 신기했어요",
          "original_form": "예전에 카페 갔을때 카페에서 과외하는거 보고 조금 신기했어요",
          "speaker_id": "2",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100031002.1.18",
          "form": "아 저도 그거 신기하더라구요",
          "original_form": "아 저도 그거 신기하더라구요",
          "speaker_id": "1",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100031002.1.19",
          "form": "장소가 마땅치 않아서 하는건가?",
          "original_form": "장소가 마땅치 않아서 하는건가?",
          "speaker_id": "1",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100031002.1.20",
          "form": "공부가 잘돼서 하는지 궁금했어요",
          "original_form": "공부가 잘돼서 하는지 궁금했어요",
          "speaker_id": "1",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100031002.1.21",
          "form": "보통 과외는 선생님이 집에 오는데",
          "original_form": "보통 과외는 선생님이 집에 오는데",
          "speaker_id": "2",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100031002.1.22",
          "form": "요즘 카페서 하니깐 신기쓰...",
          "original_form": "요즘 카페서 하니깐 신기쓰...",
          "speaker_id": "2",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100031002.1.23",
          "form": "넘 시끄럽지 않으면 어느정도 되지않을까요??!",
          "original_form": "넘 시끄럽지 않으면 어느정도 되지않을까요??!",
          "speaker_id": "2",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100031002.1.24",
          "form": "그렇겠죠? 백색소음이 집중이 잘되는경우도 있다고하니",
          "original_form": "그렇겠죠? 백색소음이 집중이 잘되는경우도 있다고하니",
          "speaker_id": "1",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100031002.1.25",
          "form": "알바는 진상들 많이 없나요?",
          "original_form": "알바는 진상들 많이 없나요?",
          "speaker_id": "1",
          "time": "20210906 18:57"
        },
        {
          "id": "MDRW2100031002.1.26",
          "form": "빵 살테니 마스크 서비스 달라던 술취한아저씨였습니다...",
          "original_form": "빵 살테니 마스크 서비스 달라던 술취한아저씨였습니다...",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100031002.1.27",
          "form": "최근에 기억나는 진상이",
          "original_form": "최근에 기억나는 진상이",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100031002.1.28",
          "form": "노마스크셨어요ㅎㅎ",
          "original_form": "노마스크셨어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100031002.1.29",
          "form": "으으 빵집에 마스크도 안쓰고",
          "original_form": "으으 빵집에 마스크도 안쓰고",
          "speaker_id": "1",
          "time": "20210907 12:32"
        },
        {
          "id": "MDRW2100031002.1.30",
          "form": "진짜 일이힘든게 아니라",
          "original_form": "진짜 일이힘든게 아니라",
          "speaker_id": "1",
          "time": "20210907 12:33"
        },
        {
          "id": "MDRW2100031002.1.31",
          "form": "진상땜에 힘들겠네요",
          "original_form": "진상땜에 힘들겠네요",
          "speaker_id": "1",
          "time": "20210907 12:33"
        }
      ]
