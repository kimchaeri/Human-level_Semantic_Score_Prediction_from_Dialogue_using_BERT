      "utterance": [
        {
          "id": "MDRW2100031976.1.1",
          "form": "백신 10부제 예약하신거죠?",
          "original_form": "백신 10부제 예약하신거죠?",
          "speaker_id": "1",
          "time": "20210905 10:18"
        },
        {
          "id": "MDRW2100031976.1.2",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031976.1.3",
          "form": "편한 마음으로 들어갔는데 정시에는 사람 많더라구요",
          "original_form": "편한 마음으로 들어갔는데 정시에는 사람 많더라구요",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031976.1.4",
          "form": "네 저도 언니 도와주려고 접속했는데 시작시간엔 인증도 늦고",
          "original_form": "네 저도 언니 도와주려고 접속했는데 시작시간엔 인증도 늦고",
          "speaker_id": "1",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031976.1.5",
          "form": "사람들이 몰리더라고요",
          "original_form": "사람들이 몰리더라고요",
          "speaker_id": "1",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031976.1.6",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031976.1.7",
          "form": "그리고 인증이 자꾸 튕겨서 더 짜증났어요...",
          "original_form": "그리고 인증이 자꾸 튕겨서 더 짜증났어요...",
          "speaker_id": "2",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031976.1.8",
          "form": "네 인증단계가 쉽지 않더라고요",
          "original_form": "네 인증단계가 쉽지 않더라고요",
          "speaker_id": "1",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031976.1.9",
          "form": "인증 방법만 바꿔서 네다섯번 시도한거같아요 ㅋㅋㅋ",
          "original_form": "인증 방법만 바꿔서 네다섯번 시도한거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031976.1.10",
          "form": "에고 사전예약 하느라 고생하셨네요",
          "original_form": "에고 사전예약 하느라 고생하셨네요",
          "speaker_id": "1",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031976.1.11",
          "form": "이번에 예약하는 분들은 다 고생하신거같아요..",
          "original_form": "이번에 예약하는 분들은 다 고생하신거같아요..",
          "speaker_id": "2",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100031976.1.12",
          "form": "네 백신 맞는 것도 이렇게 힘들어서야.",
          "original_form": "네 백신 맞는 것도 이렇게 힘들어서야.",
          "speaker_id": "1",
          "time": "20210905 10:47"
        },
        {
          "id": "MDRW2100031976.1.13",
          "form": "맞고나서 후유증도 걱정해야 하고ㅠ",
          "original_form": "맞고나서 후유증도 걱정해야 하고ㅠ",
          "speaker_id": "1",
          "time": "20210905 10:47"
        },
        {
          "id": "MDRW2100031976.1.14",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100031976.1.15",
          "form": "전 2주 뒤에 1차인데 벌써 걱정이에요",
          "original_form": "전 2주 뒤에 1차인데 벌써 걱정이에요",
          "speaker_id": "2",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100031976.1.16",
          "form": "네 가기 전에 식사 든든히 하고",
          "original_form": "네 가기 전에 식사 든든히 하고",
          "speaker_id": "1",
          "time": "20210905 10:50"
        },
        {
          "id": "MDRW2100031976.1.17",
          "form": "맞고 조금이라도 이상하면 바로 약 드셔요",
          "original_form": "맞고 조금이라도 이상하면 바로 약 드셔요",
          "speaker_id": "1",
          "time": "20210905 10:50"
        },
        {
          "id": "MDRW2100031976.1.18",
          "form": "백신맞고 병원에서 좀 있다가 귀가하셨나요?",
          "original_form": "백신맞고 병원에서 좀 있다가 귀가하셨나요?",
          "speaker_id": "2",
          "time": "20210905 10:51"
        },
        {
          "id": "MDRW2100031976.1.19",
          "form": "네 30분 대기했다가 왔어요",
          "original_form": "네 30분 대기했다가 왔어요",
          "speaker_id": "1",
          "time": "20210905 10:51"
        },
        {
          "id": "MDRW2100031976.1.20",
          "form": "병원에서 있으라는 시간만큼 있다가 오는 게 좋을 듯해서",
          "original_form": "병원에서 있으라는 시간만큼 있다가 오는 게 좋을 듯해서",
          "speaker_id": "1",
          "time": "20210905 10:51"
        }
