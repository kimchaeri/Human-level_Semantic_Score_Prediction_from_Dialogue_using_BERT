      "utterance": [
        {
          "id": "MDRW2100001014.1.1",
          "form": "토욜에 공연 기대됨",
          "original_form": "토욜에 공연 기대됨",
          "speaker_id": "1",
          "time": "20210820 11:51"
        },
        {
          "id": "MDRW2100001014.1.2",
          "form": "저도여",
          "original_form": "저도여",
          "speaker_id": "2",
          "time": "20210820 11:51"
        },
        {
          "id": "MDRW2100001014.1.3",
          "form": "전통 춤이죠?",
          "original_form": "전통 춤이죠?",
          "speaker_id": "2",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100001014.1.4",
          "form": "국악인데 플라잉이여",
          "original_form": "국악인데 플라잉이여",
          "speaker_id": "1",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100001014.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ전통춤아녀",
          "original_form": "ㅋㅋㅋㅋㅋㅋ전통춤아녀",
          "speaker_id": "1",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100001014.1.6",
          "form": "다 잘못 알고 있었네ㅋㅋㅋㅋㅋ",
          "original_form": "다 잘못 알고 있었네ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 11:54"
        },
        {
          "id": "MDRW2100001014.1.7",
          "form": "국악은 뭐 왠만해서 재밌죠",
          "original_form": "국악은 뭐 왠만해서 재밌죠",
          "speaker_id": "2",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100001014.1.8",
          "form": "엉 신박한 것 같아서 기대기대",
          "original_form": "엉 신박한 것 같아서 기대기대",
          "speaker_id": "1",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100001014.1.9",
          "form": "제목이 국악 플라잉 퍼포먼스 였네ㅋㅋㅋㅋㅋ",
          "original_form": "제목이 국악 플라잉 퍼포먼스 였네ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 11:57"
        },
        {
          "id": "MDRW2100001014.1.10",
          "form": "엉ㅋㅋㅋㅋㅋ근데 몇분짜리 공연인지는 안나오더라고",
          "original_form": "엉ㅋㅋㅋㅋㅋ근데 몇분짜리 공연인지는 안나오더라고",
          "speaker_id": "1",
          "time": "20210820 12:00"
        },
        {
          "id": "MDRW2100001014.1.11",
          "form": "한 1시간 넘게 하지 않을까요?",
          "original_form": "한 1시간 넘게 하지 않을까요?",
          "speaker_id": "2",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100001014.1.12",
          "form": "요즘공연 거의 60~80분인 것 같어",
          "original_form": "요즘공연 거의 60~80분인 것 같어",
          "speaker_id": "1",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100001014.1.13",
          "form": "티켓은 받은거에여?",
          "original_form": "티켓은 받은거에여?",
          "speaker_id": "2",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100001014.1.14",
          "form": "아님 거기 가서 받아요?",
          "original_form": "아님 거기 가서 받아요?",
          "speaker_id": "2",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100001014.1.15",
          "form": "거기가서 받아야해",
          "original_form": "거기가서 받아야해",
          "speaker_id": "1",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100001014.1.16",
          "form": "미리 30분 전까지만 가면 될듯",
          "original_form": "미리 30분 전까지만 가면 될듯",
          "speaker_id": "1",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100001014.1.17",
          "form": "좋네요 날씨만 좋으면 될꺼같아요",
          "original_form": "좋네요 날씨만 좋으면 될꺼같아요",
          "speaker_id": "2",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100001014.1.18",
          "form": "앙 간만에 너 국악보는기지?",
          "original_form": "앙 간만에 너 국악보는기지?",
          "speaker_id": "1",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100001014.1.19",
          "form": "네 공연 자체를 간만에 보죠",
          "original_form": "네 공연 자체를 간만에 보죠",
          "speaker_id": "2",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100001014.1.20",
          "form": "그 전에 우리 여우락 간게 마지막인가?",
          "original_form": "그 전에 우리 여우락 간게 마지막인가?",
          "speaker_id": "1",
          "time": "20210820 13:53"
        }
      ]
