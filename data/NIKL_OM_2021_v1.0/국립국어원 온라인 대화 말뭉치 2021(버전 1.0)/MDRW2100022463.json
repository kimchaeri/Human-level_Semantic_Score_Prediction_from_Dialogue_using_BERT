      "utterance": [
        {
          "id": "MDRW2100022463.1.1",
          "form": "ㅋㅋㅋ어떤 사고 주로 쳐요? 허스키가",
          "original_form": "ㅋㅋㅋ어떤 사고 주로 쳐요? 허스키가",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100022463.1.2",
          "form": "에어팟 십어먹고.",
          "original_form": "에어팟 십어먹고.",
          "speaker_id": "1",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100022463.1.3",
          "form": "허걱 에어팟을요?ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "허걱 에어팟을요?ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100022463.1.4",
          "form": "음.. 휴지 뜯어놓고",
          "original_form": "음.. 휴지 뜯어놓고",
          "speaker_id": "1",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100022463.1.5",
          "form": "미쳤다 힘이 엄청 좋네요",
          "original_form": "미쳤다 힘이 엄청 좋네요",
          "speaker_id": "2",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100022463.1.6",
          "form": "휴지는 소형견도 자주 뜯더라구요",
          "original_form": "휴지는 소형견도 자주 뜯더라구요",
          "speaker_id": "2",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100022463.1.7",
          "form": "ㅋㅋㅋ 그냥 다 뜯어요",
          "original_form": "ㅋㅋㅋ 그냥 다 뜯어요",
          "speaker_id": "1",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100022463.1.8",
          "form": "물어뜯는 걸 좋아하나보요?",
          "original_form": "물어뜯는 걸 좋아하나보요?",
          "speaker_id": "2",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100022463.1.9",
          "form": "소형견도그래요?",
          "original_form": "소형견도그래요?",
          "speaker_id": "1",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100022463.1.10",
          "form": "약간?",
          "original_form": "약간?",
          "speaker_id": "1",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100022463.1.11",
          "form": "네 소형견도 그런겆 ㅗㅎ아해요",
          "original_form": "네 소형견도 그런겆 ㅗㅎ아해요",
          "speaker_id": "2",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100022463.1.12",
          "form": "그리고막 소파도 뜯어버리고",
          "original_form": "그리고막 소파도 뜯어버리고",
          "speaker_id": "2",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100022463.1.13",
          "form": "사실 본가에 진돗개는 얌전하거든요",
          "original_form": "사실 본가에 진돗개는 얌전하거든요",
          "speaker_id": "1",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100022463.1.14",
          "form": "저희 누나도 에어팟 뜯겼어요 ㅋㅋㅋ",
          "original_form": "저희 누나도 에어팟 뜯겼어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100022463.1.15",
          "form": "명품 신발도 물어뜯어놨더라구요",
          "original_form": "명품 신발도 물어뜯어놨더라구요",
          "speaker_id": "2",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100022463.1.16",
          "form": "헉 에어팟 뜯기는거 집사의 국룰인가봐요",
          "original_form": "헉 에어팟 뜯기는거 집사의 국룰인가봐요",
          "speaker_id": "1",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100022463.1.17",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100022463.1.18",
          "form": "ㅋㅋㅋ 진짜 핵웃겨요",
          "original_form": "ㅋㅋㅋ 진짜 핵웃겨요",
          "speaker_id": "2",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100022463.1.19",
          "form": "근데 진돗개는 진짜 영리하잖아요",
          "original_form": "근데 진돗개는 진짜 영리하잖아요",
          "speaker_id": "2",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100022463.1.20",
          "form": "강아지 이름은 뭐에요",
