      "utterance": [
        {
          "id": "MDRW2100001920.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001920.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001920.1.3",
          "form": "무슨 일하시나요?",
          "original_form": "무슨 일하시나요?",
          "speaker_id": "1",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001920.1.4",
          "form": "개인사업해요ㅜㅜ",
          "original_form": "개인사업해요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001920.1.5",
          "form": "무슨일하시나요~",
          "original_form": "무슨일하시나요~",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001920.1.6",
          "form": "저는 카페에서 일해요",
          "original_form": "저는 카페에서 일해요",
          "speaker_id": "1",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001920.1.7",
          "form": "요즘 일이 힘들지 않나요 ㅠ",
          "original_form": "요즘 일이 힘들지 않나요 ㅠ",
          "speaker_id": "1",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001920.1.8",
          "form": "힘들고 지쳐요..",
          "original_form": "힘들고 지쳐요..",
          "speaker_id": "2",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001920.1.9",
          "form": "사람상대하시는일은 더 힘드시죠ㅜㅜ",
          "original_form": "사람상대하시는일은 더 힘드시죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001920.1.10",
          "form": "네 쉽지만은 않아요 ㅠㅠ",
          "original_form": "네 쉽지만은 않아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001920.1.11",
          "form": "퇴사를 고려할 정도로...",
          "original_form": "퇴사를 고려할 정도로...",
          "speaker_id": "1",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001920.1.12",
          "form": "예전에 알바할때 진짜 별이상한사람 생각외로 너무 많았어요ㅜ",
          "original_form": "예전에 알바할때 진짜 별이상한사람 생각외로 너무 많았어요ㅜ",
          "speaker_id": "2",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001920.1.13",
          "form": "네 일하다 보면 인류애가 사라져요 ㅎㅎ",
          "original_form": "네 일하다 보면 인류애가 사라져요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001920.1.14",
          "form": "맞아요.ㅎ 별별인류다보고나면 정떨어져요ㅜㅜ",
          "original_form": "맞아요.ㅎ 별별인류다보고나면 정떨어져요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001920.1.15",
          "form": "근데 또 막상 다른 일을 하려고 하면 막막해요 ㅠㅠ",
          "original_form": "근데 또 막상 다른 일을 하려고 하면 막막해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 22:21"
        },
        {
          "id": "MDRW2100001920.1.16",
          "form": "새로운일의 두려움이 있지요ㅜㅜ",
          "original_form": "새로운일의 두려움이 있지요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001920.1.17",
          "form": "네 근데 이제 나이 때문에 취업하기가 어려울 것 같아서요...",
          "original_form": "네 근데 이제 나이 때문에 취업하기가 어려울 것 같아서요...",
          "speaker_id": "1",
          "time": "20210819 22:25"
        },
        {
          "id": "MDRW2100001920.1.18",
          "form": "돈벌기가 힘들어요ㅜ",
          "original_form": "돈벌기가 힘들어요ㅜ",
          "speaker_id": "2",
          "time": "20210819 22:26"
        }
      ]
