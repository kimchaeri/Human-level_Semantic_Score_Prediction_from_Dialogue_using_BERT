      "utterance": [
        {
          "id": "MDRW2100039747.1.1",
          "form": "요즘.. 따로챙겨보는건 없는거같아요",
          "original_form": "요즘.. 따로챙겨보는건 없는거같아요",
          "speaker_id": "2",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039747.1.2",
          "form": "슬의생보시잖아요?!",
          "original_form": "슬의생보시잖아요?!",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039747.1.3",
          "form": "아 그렇네요",
          "original_form": "아 그렇네요",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039747.1.4",
          "form": "슬의생 좋아하세요?",
          "original_form": "슬의생 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039747.1.5",
          "form": "네!!!! 이번주 안해서 눈물 뚝뚝..",
          "original_form": "네!!!! 이번주 안해서 눈물 뚝뚝..",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039747.1.6",
          "form": "아이고.. 벌써 금요일이네요",
          "original_form": "아이고.. 벌써 금요일이네요",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100039747.1.7",
          "form": "다른건 안보세요?",
          "original_form": "다른건 안보세요?",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100039747.1.8",
          "form": "네...요즘 볼게 없더라구요",
          "original_form": "네...요즘 볼게 없더라구요",
          "speaker_id": "1",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039747.1.9",
          "form": "홍천기도 볼려고 도전했는데",
          "original_form": "홍천기도 볼려고 도전했는데",
          "speaker_id": "1",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039747.1.10",
          "form": "1화부터 실망해서 ㅠ",
          "original_form": "1화부터 실망해서 ㅠ",
          "speaker_id": "1",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039747.1.11",
          "form": "아하 cg가 특이한거같다고 듣긴했어요",
          "original_form": "아하 cg가 특이한거같다고 듣긴했어요",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039747.1.12",
          "form": "섬마을 어쩌고를 한번 봐보세요",
          "original_form": "섬마을 어쩌고를 한번 봐보세요",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039747.1.13",
          "form": "좀 유치하긴한데 뭐..",
          "original_form": "좀 유치하긴한데 뭐..",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039747.1.14",
          "form": "아.....넷플올라오드라구요",
          "original_form": "아.....넷플올라오드라구요",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039747.1.15",
          "form": "디피보러갓다가봤어요",
          "original_form": "디피보러갓다가봤어요",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039747.1.16",
          "form": "거긴 얼굴맛집아닌가요?",
          "original_form": "거긴 얼굴맛집아닌가요?",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039747.1.17",
          "form": "그런가요 ㅎㅎ",
          "original_form": "그런가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039747.1.18",
          "form": "얼굴합이맞는진 아직모르겠어요",
          "original_form": "얼굴합이맞는진 아직모르겠어요",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039747.1.19",
          "form": "아.. 아직 몇번 방영 안했잖아요 ㅎㅎ",
          "original_form": "아.. 아직 몇번 방영 안했잖아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039747.1.20",
          "form": "나중에 한번 봐보세요",
          "original_form": "나중에 한번 봐보세요",
          "speaker_id": "2",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039747.1.21",
          "form": "넴넴",
          "original_form": "넴넴",
          "speaker_id": "1",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039747.1.22",
          "form": "그럴께요",
          "original_form": "그럴께요",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039747.1.23",
          "form": "다른거 추천해줄만한게 있나요?",
          "original_form": "다른거 추천해줄만한게 있나요?",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039747.1.24",
          "form": "요즘 디피가 유명하지 않나요?",
          "original_form": "요즘 디피가 유명하지 않나요?",
          "speaker_id": "1",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039747.1.25",
          "form": "전 안봒지만..",
          "original_form": "전 안봒지만..",
