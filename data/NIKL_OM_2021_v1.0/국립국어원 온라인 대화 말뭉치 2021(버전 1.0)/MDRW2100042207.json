      "utterance": [
        {
          "id": "MDRW2100042207.1.1",
          "form": "아침 두번 ㅋ 저녘도 2번 차려요 ㅋ",
          "original_form": "아침 두번 ㅋ 저녘도 2번 차려요 ㅋ",
          "speaker_id": "1",
          "time": "20210906 20:42"
        },
        {
          "id": "MDRW2100042207.1.2",
          "form": "두번씩요?",
          "original_form": "두번씩요?",
          "speaker_id": "2",
          "time": "20210906 20:42"
        },
        {
          "id": "MDRW2100042207.1.3",
          "form": "가족이 한번에 안일어나나 보네요",
          "original_form": "가족이 한번에 안일어나나 보네요",
          "speaker_id": "2",
          "time": "20210906 20:42"
        },
        {
          "id": "MDRW2100042207.1.4",
          "form": "아뇨 남편이 출근이 빨라서 6시",
          "original_form": "아뇨 남편이 출근이 빨라서 6시",
          "speaker_id": "1",
          "time": "20210906 20:43"
        },
        {
          "id": "MDRW2100042207.1.5",
          "form": "아이랑 저는 7시반",
          "original_form": "아이랑 저는 7시반",
          "speaker_id": "1",
          "time": "20210906 20:43"
        },
        {
          "id": "MDRW2100042207.1.6",
          "form": "남편이 빨리 나가네요",
          "original_form": "남편이 빨리 나가네요",
          "speaker_id": "2",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042207.1.7",
          "form": "name1님은 아이랑 같이 먹으려고 늦게 먹는  군요",
          "original_form": "&name1&님은 아이랑 같이 먹으려고 늦게 먹는  군요",
          "speaker_id": "2",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042207.1.8",
          "form": "출근거리가 있는데",
          "original_form": "출근거리가 있는데",
          "speaker_id": "1",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042207.1.9",
          "form": "7시 반도 늦은 건 아닌데 남편 출근이 빠른거네요",
          "original_form": "7시 반도 늦은 건 아닌데 남편 출근이 빠른거네요",
          "speaker_id": "2",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042207.1.10",
          "form": "걑은시간에 나감 엄천 막혀서요",
          "original_form": "걑은시간에 나감 엄천 막혀서요",
          "speaker_id": "1",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042207.1.11",
          "form": "일찍 출근해요",
          "original_form": "일찍 출근해요",
          "speaker_id": "1",
          "time": "20210906 20:47"
        },
        {
          "id": "MDRW2100042207.1.12",
          "form": "막히는 길은 정말 스트레스죠",
          "original_form": "막히는 길은 정말 스트레스죠",
          "speaker_id": "2",
          "time": "20210906 20:49"
        },
        {
          "id": "MDRW2100042207.1.13",
          "form": "그래도 아침 먹고 나가는 모습이 든든하네요",
          "original_form": "그래도 아침 먹고 나가는 모습이 든든하네요",
          "speaker_id": "2",
          "time": "20210906 20:49"
        },
        {
          "id": "MDRW2100042207.1.14",
          "form": "아침을 먹어야 하루 에너지가 생기는 것 같아요",
          "original_form": "아침을 먹어야 하루 에너지가 생기는 것 같아요",
          "speaker_id": "2",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042207.1.15",
          "form": "아침 안먹으면 큰일나는지 알아요 ㅎㅎ 습관이 무섭 ㅎ",
          "original_form": "아침 안먹으면 큰일나는지 알아요 ㅎㅎ 습관이 무섭 ㅎ",
          "speaker_id": "1",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042207.1.16",
          "form": "습관 무섭죠",
          "original_form": "습관 무섭죠",
          "speaker_id": "2",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042207.1.17",
          "form": "저도 아침 안먹이면 큰일나는 줄 알고 가족들 꼭 먹게 하거든요",
          "original_form": "저도 아침 안먹이면 큰일나는 줄 알고 가족들 꼭 먹게 하거든요",
          "speaker_id": "2",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042207.1.18",
          "form": "그래서 애들도 아침은 당연히 먹는 줄 알아요",
          "original_form": "그래서 애들도 아침은 당연히 먹는 줄 알아요",
          "speaker_id": "2",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042207.1.19",
          "form": "ㅎㅎ 그게 좋으넉 같아요",
          "original_form": "ㅎㅎ 그게 좋으넉 같아요",
          "speaker_id": "1",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100042207.1.20",
          "form": "아침먹어야 두뇌회전도 좋다하자나요 ㅎ",
          "original_form": "아침먹어야 두뇌회전도 좋다하자나요 ㅎ",
          "speaker_id": "1",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100042207.1.21",
          "form": "저도 그 이야기 하면서 먹여요",
          "original_form": "저도 그 이야기 하면서 먹여요",
          "speaker_id": "2",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042207.1.22",
          "form": "애들 학교 다닐때 지각하더라고 아침은 먹고가라고 먹였네요",
          "original_form": "애들 학교 다닐때 지각하더라고 아침은 먹고가라고 먹였네요",
          "speaker_id": "2",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042207.1.23",
          "form": "ㅎㅎㅎ 전 아이기 아직 쪼꼬미긴 한데",
          "original_form": "ㅎㅎㅎ 전 아이기 아직 쪼꼬미긴 한데",
          "speaker_id": "1",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100042207.1.24",
          "form": "밥을 잘 안먹어서 ㅜㅁ ㅜ",
          "original_form": "밥을 잘 안먹어서 ㅜㅁ ㅜ",
          "speaker_id": "1",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100042207.1.25",
          "form": "습관 들이려고 그래도 같이 먹어요",
          "original_form": "습관 들이려고 그래도 같이 먹어요",
          "speaker_id": "1",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100042207.1.26",
          "form": "아기들은 원래 밥을 잘 안먹죠",
          "original_form": "아기들은 원래 밥을 잘 안먹죠",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042207.1.27",
          "form": "저희 아이들도 그랬어요",
          "original_form": "저희 아이들도 그랬어요",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042207.1.28",
          "form": "참 밥 먹이는게 곤욕이었죠",
          "original_form": "참 밥 먹이는게 곤욕이었죠",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042207.1.29",
          "form": "흑 먹는것듀 손에 꼽아요 예민해서",
