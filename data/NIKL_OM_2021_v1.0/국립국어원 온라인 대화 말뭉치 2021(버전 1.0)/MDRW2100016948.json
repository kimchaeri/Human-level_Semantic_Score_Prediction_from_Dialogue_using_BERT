      "utterance": [
        {
          "id": "MDRW2100016948.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210826 16:39"
        },
        {
          "id": "MDRW2100016948.1.2",
          "form": "안녕하세용ㅎㅎ",
          "original_form": "안녕하세용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016948.1.3",
          "form": "점심 맛있는거 드셨나요?",
          "original_form": "점심 맛있는거 드셨나요?",
          "speaker_id": "1",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100016948.1.4",
          "form": "점심은 수육먹었어요!!ㅎㅎ",
          "original_form": "점심은 수육먹었어요!!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:16"
        },
        {
          "id": "MDRW2100016948.1.5",
          "form": "name1은 뭐 드셨나요?",
          "original_form": "&name1&은 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210826 17:16"
        },
        {
          "id": "MDRW2100016948.1.6",
          "form": "전 엽떡이요",
          "original_form": "전 엽떡이요",
          "speaker_id": "1",
          "time": "20210826 17:16"
        },
        {
          "id": "MDRW2100016948.1.7",
          "form": "수육 맛잇었겠어요..ㅡ",
          "original_form": "수육 맛잇었겠어요..ㅡ",
          "speaker_id": "1",
          "time": "20210826 17:16"
        },
        {
          "id": "MDRW2100016948.1.8",
          "form": "와... 수육",
          "original_form": "와... 수육",
          "speaker_id": "1",
          "time": "20210826 17:16"
        },
        {
          "id": "MDRW2100016948.1.9",
          "form": "수육진짜 맛있게 먹었어용ㅎㅎ",
          "original_form": "수육진짜 맛있게 먹었어용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100016948.1.10",
          "form": "오 떡볶이 좋아하시나요?",
          "original_form": "오 떡볶이 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100016948.1.11",
          "form": "네! 엄청요!",
          "original_form": "네! 엄청요!",
          "speaker_id": "1",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100016948.1.12",
          "form": "매운것도 좋아하시나요?",
          "original_form": "매운것도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100016948.1.13",
          "form": "저는 매운걸 잘 못먹어서 엽떡은 잘 못먹어요ㅠ",
          "original_form": "저는 매운걸 잘 못먹어서 엽떡은 잘 못먹어요ㅠ",
          "speaker_id": "2",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100016948.1.14",
          "form": "네! 완전 많이 좋아해요",
          "original_form": "네! 완전 많이 좋아해요",
          "speaker_id": "1",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100016948.1.15",
          "form": "저도 잘 못먹어서",
          "original_form": "저도 잘 못먹어서",
          "speaker_id": "1",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100016948.1.16",
          "form": "울면서 먹어요..ㅎㅎ",
          "original_form": "울면서 먹어요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100016948.1.17",
          "form": "아하 그러시구나ㅋㅋㅋ울면서 까지 먹으시다니ㅠㅠㅠ",
          "original_form": "아하 그러시구나ㅋㅋㅋ울면서 까지 먹으시다니ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100016948.1.18",
          "form": "저도 매운거 먹으면 진짜 눈물 콧물 다나더라구요ㅠㅠ",
          "original_form": "저도 매운거 먹으면 진짜 눈물 콧물 다나더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100016948.1.19",
          "form": "오늘도 울면서 드셨어요?ㅋㅋㅋ",
          "original_form": "오늘도 울면서 드셨어요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100016948.1.20",
          "form": "아뇨 ㅋㅋㅋㅋㅋ 오늘은 착한맛 먹었어요",
          "original_form": "아뇨 ㅋㅋㅋㅋㅋ 오늘은 착한맛 먹었어요",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100016948.1.21",
          "form": "저말고 다른사람들이 더 못먹어서 ㅎㅎㅎ",
          "original_form": "저말고 다른사람들이 더 못먹어서 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100016948.1.22",
          "form": "아 그러셨군요ㅋㅋㅋㅋ 맛있게 드셨다면 됐죠ㅎㅎ",
          "original_form": "아 그러셨군요ㅋㅋㅋㅋ 맛있게 드셨다면 됐죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100016948.1.23",
          "form": "저는 청년다방 떡볶이는 엄청 좋아해요!!",
          "original_form": "저는 청년다방 떡볶이는 엄청 좋아해요!!",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100016948.1.24",
          "form": "엄청 맛있죠!",
          "original_form": "엄청 맛있죠!",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100016948.1.25",
          "form": "청다 감튀도 엄청 맛있구요",
          "original_form": "청다 감튀도 엄청 맛있구요",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100016948.1.26",
          "form": "맞아요!! 청년다방이 전체적으로 제 입맛에 맞는느낌이에요!ㅎㅎ",
          "original_form": "맞아요!! 청년다방이 전체적으로 제 입맛에 맞는느낌이에요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100016948.1.27",
          "form": "최근에 신전에서 로제떡볶이 먹어봤는데 로제떡볶이도 맛있더라구용",
          "original_form": "최근에 신전에서 로제떡볶이 먹어봤는데 로제떡볶이도 맛있더라구용",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100016948.1.28",
          "form": "오 한번도 안먹어봤는데",
          "original_form": "오 한번도 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100016948.1.29",
          "form": "먹어봐야겠네요!",
          "original_form": "먹어봐야겠네요!",
          "speaker_id": "1",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100016948.1.30",
          "form": "로제떡볶이가 근데 반응이 많이 갈리더라구요! 로제파스타같은거는 좋아하세요?",
          "original_form": "로제떡볶이가 근데 반응이 많이 갈리더라구요! 로제파스타같은거는 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100016948.1.31",
          "form": "좋아해요!",
          "original_form": "좋아해요!",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100016948.1.32",
          "form": "오 그러면 좋아하실수도 있을거같아요!!",
          "original_form": "오 그러면 좋아하실수도 있을거같아요!!",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100016948.1.33",
          "form": "꼭 먹어봐야겠어요",
          "original_form": "꼭 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 18:46"
        }
      ]
