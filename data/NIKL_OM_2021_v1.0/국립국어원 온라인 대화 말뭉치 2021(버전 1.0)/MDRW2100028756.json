      "utterance": [
        {
          "id": "MDRW2100028756.1.1",
          "form": "건강관리 하시는 거 있으세요?",
          "original_form": "건강관리 하시는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028756.1.2",
          "form": "영양제 챙겨먹고 있습니다",
          "original_form": "영양제 챙겨먹고 있습니다",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028756.1.3",
          "form": "있으신가요?",
          "original_form": "있으신가요?",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028756.1.4",
          "form": "저두 영양제 요ㅋㅋ",
          "original_form": "저두 영양제 요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028756.1.5",
          "form": "운동은 늘 생각뿐입니다;;",
          "original_form": "운동은 늘 생각뿐입니다;;",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028756.1.6",
          "form": "저도 운동은 항상 계획만 잘짜요ㅠㅋㅋ",
          "original_form": "저도 운동은 항상 계획만 잘짜요ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028756.1.7",
          "form": "실행하는게 힘드네요ㅎ",
          "original_form": "실행하는게 힘드네요ㅎ",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028756.1.8",
          "form": "운동 실행하는 분은 정말 대단한 분ㅋㅋㅋㅋ",
          "original_form": "운동 실행하는 분은 정말 대단한 분ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028756.1.9",
          "form": "운동과 다이어트는 내일부터죠ㅋㅋ",
          "original_form": "운동과 다이어트는 내일부터죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028756.1.10",
          "form": "그쵸ㅋㅋ 영양제는 잘챙겨 드시나요?",
          "original_form": "그쵸ㅋㅋ 영양제는 잘챙겨 드시나요?",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028756.1.11",
          "form": "챙겨 먹으려고 노력은 해요",
          "original_form": "챙겨 먹으려고 노력은 해요",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028756.1.12",
          "form": "아 그런데 자꾸 까먹게 되네요;;",
          "original_form": "아 그런데 자꾸 까먹게 되네요;;",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028756.1.13",
          "form": "네 저도 피곤할때만 챙겨먹게 되요",
          "original_form": "네 저도 피곤할때만 챙겨먹게 되요",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028756.1.14",
          "form": "그러면 안되는데ㅠ",
          "original_form": "그러면 안되는데ㅠ",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028756.1.15",
          "form": "확실히 근데 영양제 챙겨 먹을 때랑 안 먹을 때랑 차이가 나지 않나요??",
          "original_form": "확실히 근데 영양제 챙겨 먹을 때랑 안 먹을 때랑 차이가 나지 않나요??",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028756.1.16",
          "form": "네 너무 나요...",
          "original_form": "네 너무 나요...",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028756.1.17",
          "form": "그래서 챙겨 먹을려고 노력중입니다",
          "original_form": "그래서 챙겨 먹을려고 노력중입니다",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028756.1.18",
          "form": "한 해 한 해 지날수록 더 느껴지는 거 같아요",
          "original_form": "한 해 한 해 지날수록 더 느껴지는 거 같아요",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028756.1.19",
          "form": "언제 챙겨 드세요? 오전 오후?",
          "original_form": "언제 챙겨 드세요? 오전 오후?",
          "speaker_id": "2",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028756.1.20",
          "form": "전 오전에 먹으려고 해요 오 뭐",
          "original_form": "전 오전에 먹으려고 해요 오 뭐",
          "speaker_id": "1",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028756.1.21",
          "form": "그래야 까먹어도 저녁에 먹을 수 있으니",
          "original_form": "그래야 까먹어도 저녁에 먹을 수 있으니",
          "speaker_id": "1",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028756.1.22",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
