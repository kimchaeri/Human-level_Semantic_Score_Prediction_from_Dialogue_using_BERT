      "utterance": [
        {
          "id": "MDRW2100044078.1.1",
          "form": "저요 다요",
          "original_form": "저요 다요",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044078.1.2",
          "form": "데식 조아요",
          "original_form": "데식 조아요",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044078.1.3",
          "form": "님은",
          "original_form": "님은",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044078.1.4",
          "form": "데식 어느노래요?",
          "original_form": "데식 어느노래요?",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044078.1.5",
          "form": "저는 요즘 뚫고지나가요 많이들어요",
          "original_form": "저는 요즘 뚫고지나가요 많이들어요",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044078.1.6",
          "form": "저도저도",
          "original_form": "저도저도",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044078.1.7",
          "form": "그거 슬프지않나요",
          "original_form": "그거 슬프지않나요",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044078.1.8",
          "form": "오늘날씨엔 땡스투도 생각나네요",
          "original_form": "오늘날씨엔 땡스투도 생각나네요",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044078.1.9",
          "form": "크 죽이죠 또 뭐요",
          "original_form": "크 죽이죠 또 뭐요",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044078.1.10",
          "form": "전 콜그레출레이션 그노래로 가장처음알았어요",
          "original_form": "전 콜그레출레이션 그노래로 가장처음알았어요",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044078.1.11",
          "form": "저두요 콘 가심 ?",
          "original_form": "저두요 콘 가심 ?",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100044078.1.12",
          "form": "아뇨ㅠ 가보셧어요?",
          "original_form": "아뇨ㅠ 가보셧어요?",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044078.1.13",
          "form": "네네 조아요",
          "original_form": "네네 조아요",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044078.1.14",
          "form": "에데식.노래 좋앗죠?",
          "original_form": "에데식.노래 좋앗죠?",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044078.1.15",
          "form": "네 너무좋았어요ㅠ 빨리 다음앨범 듣구싶어요ㅠ",
          "original_form": "네 너무좋았어요ㅠ 빨리 다음앨범 듣구싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044078.1.16",
          "form": "군대언제와요ㅠ",
          "original_form": "군대언제와요ㅠ",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044078.1.17",
          "form": "생각보다 금방이에요 ㅋㅋㅋ",
          "original_form": "생각보다 금방이에요 ㅋㅋㅋ",
