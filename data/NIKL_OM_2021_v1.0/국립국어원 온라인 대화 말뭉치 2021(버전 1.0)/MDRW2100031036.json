      "utterance": [
        {
          "id": "MDRW2100031036.1.1",
          "form": "다음에 찾아봐야겠네요",
          "original_form": "다음에 찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100031036.1.2",
          "form": "거의 없더라구요",
          "original_form": "거의 없더라구요",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100031036.1.3",
          "form": "치킨 어디꺼 자주드세요?",
          "original_form": "치킨 어디꺼 자주드세요?",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100031036.1.4",
          "form": "저희동네는 없을수도 있을거같아요ㅜ",
          "original_form": "저희동네는 없을수도 있을거같아요ㅜ",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100031036.1.5",
          "form": "주로 교촌아니면 호치킨이요",
          "original_form": "주로 교촌아니면 호치킨이요",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100031036.1.6",
          "form": "호치킨은 첨들어봐요",
          "original_form": "호치킨은 첨들어봐요",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100031036.1.7",
          "form": "특별한 메뉴가있나요",
          "original_form": "특별한 메뉴가있나요",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100031036.1.8",
          "form": "저렴한데예요",
          "original_form": "저렴한데예요",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100031036.1.9",
          "form": "아뇨 싸서 먹어요",
          "original_form": "아뇨 싸서 먹어요",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100031036.1.10",
          "form": "아 저렴이 좋아합니다",
          "original_form": "아 저렴이 좋아합니다",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100031036.1.11",
          "form": "두마리치킨같은건가요?",
          "original_form": "두마리치킨같은건가요?",
          "speaker_id": "1",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100031036.1.12",
          "form": "후라이드가 저렴해요",
          "original_form": "후라이드가 저렴해요",
          "speaker_id": "2",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100031036.1.13",
          "form": "아뇨 한마리인데 가격이 싸요",
          "original_form": "아뇨 한마리인데 가격이 싸요",
          "speaker_id": "2",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100031036.1.14",
          "form": "아 후라이드 바삭한타입인가요?",
          "original_form": "아 후라이드 바삭한타입인가요?",
          "speaker_id": "1",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100031036.1.15",
          "form": "네네 완전바삭ㅜ",
          "original_form": "네네 완전바삭ㅜ",
          "speaker_id": "2",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100031036.1.16",
          "form": "근데 싼대신에",
          "original_form": "근데 싼대신에",
          "speaker_id": "2",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100031036.1.17",
          "form": "양이 조금 작긴해요",
          "original_form": "양이 조금 작긴해요",
          "speaker_id": "2",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100031036.1.18",
          "form": "아 바삭이 맛있을거같아요",
          "original_form": "아 바삭이 맛있을거같아요",
          "speaker_id": "1",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100031036.1.19",
          "form": "네네 전양 작아서 괜찮은데",
          "original_form": "네네 전양 작아서 괜찮은데",
          "speaker_id": "2",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100031036.1.20",
          "form": "많이 드심 비추예요",
          "original_form": "많이 드심 비추예요",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100031036.1.21",
          "form": "치킨은 딱 기분좋은 만큼 먹는게",
          "original_form": "치킨은 딱 기분좋은 만큼 먹는게",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100031036.1.22",
          "form": "뒤탈이 없는거같아요",
          "original_form": "뒤탈이 없는거같아요",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100031036.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 15:48"
        }
      ]
