      "utterance": [
        {
          "id": "MDRW2100040073.1.1",
          "form": "좋아하는 계절 있나요?^^",
          "original_form": "좋아하는 계절 있나요?^^",
          "speaker_id": "1",
          "time": "20210904 05:36"
        },
        {
          "id": "MDRW2100040073.1.2",
          "form": "가을이 좋은데 넘 짧죠",
          "original_form": "가을이 좋은데 넘 짧죠",
          "speaker_id": "2",
          "time": "20210904 18:43"
        },
        {
          "id": "MDRW2100040073.1.3",
          "form": "저도 가을 가장 좋아해요",
          "original_form": "저도 가을 가장 좋아해요",
          "speaker_id": "1",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100040073.1.4",
          "form": "갈수록 짧아져서 정말 속상하답니다",
          "original_form": "갈수록 짧아져서 정말 속상하답니다",
          "speaker_id": "1",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100040073.1.5",
          "form": "그동안 비가 너무 오래와서 봄에서 가을 넘어가는 느낌이 없었는데",
          "original_form": "그동안 비가 너무 오래와서 봄에서 가을 넘어가는 느낌이 없었는데",
          "speaker_id": "1",
          "time": "20210904 20:43"
        },
        {
          "id": "MDRW2100040073.1.6",
          "form": "그쵸 가을에 사고 싶은 옷도 망설여져요",
          "original_form": "그쵸 가을에 사고 싶은 옷도 망설여져요",
          "speaker_id": "2",
          "time": "20210904 20:43"
        },
        {
          "id": "MDRW2100040073.1.7",
          "form": "어제부터 해가 떠서 가을 느낌이 조금 났네요",
          "original_form": "어제부터 해가 떠서 가을 느낌이 조금 났네요",
          "speaker_id": "1",
          "time": "20210904 20:43"
        },
        {
          "id": "MDRW2100040073.1.8",
          "form": "그쵸 오늘 하늘이 정말 가을이더라고요",
          "original_form": "그쵸 오늘 하늘이 정말 가을이더라고요",
          "speaker_id": "2",
          "time": "20210904 20:43"
        },
        {
          "id": "MDRW2100040073.1.9",
          "form": "네~ 높고 푸른하늘",
          "original_form": "네~ 높고 푸른하늘",
          "speaker_id": "1",
          "time": "20210904 20:43"
        },
        {
          "id": "MDRW2100040073.1.10",
          "form": "얼마만인지요",
          "original_form": "얼마만인지요",
          "speaker_id": "1",
          "time": "20210904 20:43"
        },
        {
          "id": "MDRW2100040073.1.11",
          "form": "가을은 짧아도 옷은 있어야하고",
          "original_form": "가을은 짧아도 옷은 있어야하고",
          "speaker_id": "1",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100040073.1.12",
          "form": "오늘은 미세먼지도 별로 없었대요",
          "original_form": "오늘은 미세먼지도 별로 없었대요",
          "speaker_id": "2",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100040073.1.13",
          "form": "한국은 4계절이라 기본적으로 필요한 옷들만 쳐도 많아져요",
          "original_form": "한국은 4계절이라 기본적으로 필요한 옷들만 쳐도 많아져요",
          "speaker_id": "1",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100040073.1.14",
          "form": "미세먼지가 없어서 더 파랬군요",
          "original_form": "미세먼지가 없어서 더 파랬군요",
          "speaker_id": "1",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100040073.1.15",
          "form": "네 저는 작년부터 트렌치코트 살까하다가도",
          "original_form": "네 저는 작년부터 트렌치코트 살까하다가도",
          "speaker_id": "2",
          "time": "20210904 20:45"
        },
        {
          "id": "MDRW2100040073.1.16",
          "form": "금방 ㅊ추워지니까 시기를 놓쳤어요",
          "original_form": "금방 ㅊ추워지니까 시기를 놓쳤어요",
          "speaker_id": "2",
          "time": "20210904 20:45"
        },
        {
          "id": "MDRW2100040073.1.17",
          "form": "트렌치코트는 진짜 그렇죠",
          "original_form": "트렌치코트는 진짜 그렇죠",
          "speaker_id": "1",
          "time": "20210904 21:13"
        },
        {
          "id": "MDRW2100040073.1.18",
          "form": "가을에 입어줘야하긴하는데",
          "original_form": "가을에 입어줘야하긴하는데",
          "speaker_id": "1",
          "time": "20210904 21:13"
        },
        {
          "id": "MDRW2100040073.1.19",
          "form": "살 시기를 잡는게 너무 어려워요",
          "original_form": "살 시기를 잡는게 너무 어려워요",
          "speaker_id": "1",
          "time": "20210904 21:13"
        },
        {
          "id": "MDRW2100040073.1.20",
          "form": "네 근데 두번 입으면 패딩입어야 하는 날씨죠",
          "original_form": "네 근데 두번 입으면 패딩입어야 하는 날씨죠",
          "speaker_id": "2",
          "time": "20210904 21:14"
        },
        {
          "id": "MDRW2100040073.1.21",
          "form": "그래서 고민이죠.. 입고는 싶은데 너무 횟수가 적으니까",
          "original_form": "그래서 고민이죠.. 입고는 싶은데 너무 횟수가 적으니까",
          "speaker_id": "1",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100040073.1.22",
          "form": "근데 또 그건 봄에 입기도 그러고 딱 가을에 맞는 옷인데",
          "original_form": "근데 또 그건 봄에 입기도 그러고 딱 가을에 맞는 옷인데",
          "speaker_id": "1",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100040073.1.23",
          "form": "일년에 한두번 입겠다고 사기가.. 가격도 만만치않고",
          "original_form": "일년에 한두번 입겠다고 사기가.. 가격도 만만치않고",
          "speaker_id": "1",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100040073.1.24",
          "form": "맞아요 그래서 이번에도 적당한 걸 찾아보다가 또 계절을 넘기지 않을까 싶어요",
          "original_form": "맞아요 그래서 이번에도 적당한 걸 찾아보다가 또 계절을 넘기지 않을까 싶어요",
          "speaker_id": "2",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100040073.1.25",
          "form": "봄 여름 가을 겨울이 딱 4등분이었으면 좋겠어요",
          "original_form": "봄 여름 가을 겨울이 딱 4등분이었으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210904 21:25"
        }
      ]
