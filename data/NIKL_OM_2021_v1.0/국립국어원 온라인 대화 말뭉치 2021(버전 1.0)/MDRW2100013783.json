      "utterance": [
        {
          "id": "MDRW2100013783.1.1",
          "form": "혹시 게임 하시나요??",
          "original_form": "혹시 게임 하시나요??",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013783.1.2",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013783.1.3",
          "form": "오 무슨게임 좋아하세요??",
          "original_form": "오 무슨게임 좋아하세요??",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013783.1.4",
          "form": "저는 요즘 플래시게임 해요",
          "original_form": "저는 요즘 플래시게임 해요",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013783.1.5",
          "form": "오 예를들어 어떤거요??",
          "original_form": "오 예를들어 어떤거요??",
          "speaker_id": "1",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013783.1.6",
          "form": "퍼즐버블 아세요??",
          "original_form": "퍼즐버블 아세요??",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013783.1.7",
          "form": "오 재밌어요?? 그런건 어디서 하는거에요?",
          "original_form": "오 재밌어요?? 그런건 어디서 하는거에요?",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013783.1.8",
          "form": "와플래시",
          "original_form": "와플래시",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013783.1.9",
          "form": "라고 요즘 플래시게임이 다 없어져서",
          "original_form": "라고 요즘 플래시게임이 다 없어져서",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013783.1.10",
          "form": "여기 티스토리에 누가 복원해놧어요",
          "original_form": "여기 티스토리에 누가 복원해놧어요",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013783.1.11",
          "form": "저는 슈게임 많이 했는데",
          "original_form": "저는 슈게임 많이 했는데",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013783.1.12",
          "form": "지금 봐도 재밌어보이네요",
          "original_form": "지금 봐도 재밌어보이네요",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100013783.1.13",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100013783.1.14",
          "form": "슈게임도 재밌어요",
          "original_form": "슈게임도 재밌어요",
          "speaker_id": "2",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100013783.1.15",
          "form": "저는 폰으로 쿠키런이랑 배그하는데 재밌더라고요",
          "original_form": "저는 폰으로 쿠키런이랑 배그하는데 재밌더라고요",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100013783.1.16",
          "form": "폰배그 하시는군요! 저는 컴배그 해용",
          "original_form": "폰배그 하시는군요! 저는 컴배그 해용",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100013783.1.17",
          "form": "아 컴퓨터로도 했었네요",
          "original_form": "아 컴퓨터로도 했었네요",
          "speaker_id": "1",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100013783.1.18",
          "form": "배그는 인기 지금도 많나요?",
          "original_form": "배그는 인기 지금도 많나요?",
          "speaker_id": "1",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100013783.1.19",
          "form": "아뇨ㅠ 한 2년 전에 인기 다 식은 것 같아요",
          "original_form": "아뇨ㅠ 한 2년 전에 인기 다 식은 것 같아요",
          "speaker_id": "2",
          "time": "20210827 23:01"
        }
      ]
