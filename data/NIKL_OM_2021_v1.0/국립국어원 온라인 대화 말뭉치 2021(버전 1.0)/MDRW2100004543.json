      "utterance": [
        {
          "id": "MDRW2100004543.1.1",
          "form": "요즘 배달음식을 정말 많이 먹게 됐어요",
          "original_form": "요즘 배달음식을 정말 많이 먹게 됐어요",
          "speaker_id": "1",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004543.1.2",
          "form": "name2님은 어떠신가요?",
          "original_form": "&name2&님은 어떠신가요?",
          "speaker_id": "1",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004543.1.3",
          "form": "저희도요",
          "original_form": "저희도요",
          "speaker_id": "2",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004543.1.4",
          "form": "전에 비해 많이늘엇어요",
          "original_form": "전에 비해 많이늘엇어요",
          "speaker_id": "2",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004543.1.5",
          "form": "주로 어떤 음식을 시켜드세요?",
          "original_form": "주로 어떤 음식을 시켜드세요?",
          "speaker_id": "1",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004543.1.6",
          "form": "고기류요?",
          "original_form": "고기류요?",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004543.1.7",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "1",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004543.1.8",
          "form": "저는 정말 다양하게 시켜먹었어요",
          "original_form": "저는 정말 다양하게 시켜먹었어요",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004543.1.9",
          "form": "매운음식이나요",
          "original_form": "매운음식이나요",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004543.1.10",
          "form": "피자 족발 막국수...",
          "original_form": "피자 족발 막국수...",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004543.1.11",
          "form": "매운음식도 종종 먹었네요",
          "original_form": "매운음식도 종종 먹었네요",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004543.1.12",
          "form": "배달음식 어플 살펴보니 정말 자주 시켜먹었어요ㅜ",
          "original_form": "배달음식 어플 살펴보니 정말 자주 시켜먹었어요ㅜ",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004543.1.13",
          "form": "ㅋㅋ비슷하네요",
          "original_form": "ㅋㅋ비슷하네요",
          "speaker_id": "2",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004543.1.14",
          "form": "요즘은 냉면도요",
          "original_form": "요즘은 냉면도요",
          "speaker_id": "2",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004543.1.15",
          "form": "냉면도 좋죠!!",
          "original_form": "냉면도 좋죠!!",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004543.1.16",
          "form": "저는 택배로 시켜서 먹었어요",
          "original_form": "저는 택배로 시켜서 먹었어요",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004543.1.17",
          "form": "오 그것도 괜찮죠",
          "original_form": "오 그것도 괜찮죠",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004543.1.18",
          "form": "네 근데 너무 자주 시켜먹은 것 같아서",
          "original_form": "네 근데 너무 자주 시켜먹은 것 같아서",
          "speaker_id": "1",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004543.1.19",
          "form": "이제는 좀 자제하려고요",
          "original_form": "이제는 좀 자제하려고요",
          "speaker_id": "1",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004543.1.20",
          "form": "애기재우고보니 이제네요ㅠ 저도 오늘은 해먹엇어요ㅎ",
          "original_form": "애기재우고보니 이제네요ㅠ 저도 오늘은 해먹엇어요ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100004543.1.21",
          "form": "에고 애기가 늦게 잤나봐요",
          "original_form": "에고 애기가 늦게 잤나봐요",
          "speaker_id": "1",
          "time": "20210822 10:09"
        },
        {
          "id": "MDRW2100004543.1.22",
          "form": "애기 키우느라 바쁘실텐데 음식까지 해드시다니 대단해요~",
          "original_form": "애기 키우느라 바쁘실텐데 음식까지 해드시다니 대단해요~",
          "speaker_id": "1",
          "time": "20210822 10:09"
        },
        {
          "id": "MDRW2100004543.1.23",
          "form": "너무 배달해먹으면 눈치보여서요ㅎㅎ",
          "original_form": "너무 배달해먹으면 눈치보여서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100004543.1.24",
          "form": "아아 그렇죠ㅎㅎ",
          "original_form": "아아 그렇죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100004543.1.25",
          "form": "아침 식사는 하셨어요?",
          "original_form": "아침 식사는 하셨어요?",
          "speaker_id": "1",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100004543.1.26",
          "form": "네 아침은 시리얼정도 간단히요ㅎ",
          "original_form": "네 아침은 시리얼정도 간단히요ㅎ",
          "speaker_id": "2",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100004543.1.27",
          "form": "저는 식빵 구워서 미리 만들어놓은 샐러드랑 먹었어요",
          "original_form": "저는 식빵 구워서 미리 만들어놓은 샐러드랑 먹었어요",
          "speaker_id": "1",
          "time": "20210822 10:11"
        },
        {
          "id": "MDRW2100004543.1.28",
          "form": "아침은 간단히 먹게 되죠ㅎㅎ",
          "original_form": "아침은 간단히 먹게 되죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:11"
        },
        {
          "id": "MDRW2100004543.1.29",
          "form": "오 맛있는거 드셨네요ㅎ",
          "original_form": "오 맛있는거 드셨네요ㅎ",
          "speaker_id": "2",
          "time": "20210822 10:11"
        },
        {
          "id": "MDRW2100004543.1.30",
          "form": "네~ 집에 양배추가 많아서",
          "original_form": "네~ 집에 양배추가 많아서",
          "speaker_id": "1",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100004543.1.31",
          "form": "코울슬로를 잔뜩 만들었는데",
          "original_form": "코울슬로를 잔뜩 만들었는데",
          "speaker_id": "1",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100004543.1.32",
          "form": "저희도 한통사다두고",
          "original_form": "저희도 한통사다두고",
          "speaker_id": "2",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100004543.1.33",
          "form": "덕분에 며칠 아침식사를 해결했어요",
          "original_form": "덕분에 며칠 아침식사를 해결했어요",
          "speaker_id": "1",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100004543.1.34",
          "form": "몆날며칠을 먹네요ㅎ",
          "original_form": "몆날며칠을 먹네요ㅎ",
          "speaker_id": "2",
          "time": "20210822 10:12"
        }
      ]
