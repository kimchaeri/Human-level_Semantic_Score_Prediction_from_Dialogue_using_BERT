      "utterance": [
        {
          "id": "MDRW2100027872.1.1",
          "form": "오 영상 좋은것 ㅎㅎ",
          "original_form": "오 영상 좋은것 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027872.1.2",
          "form": "유튜브 때매 잠을 못자게써요 ㅎㅎ",
          "original_form": "유튜브 때매 잠을 못자게써요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027872.1.3",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027872.1.4",
          "form": "저는 영화덕후라서",
          "original_form": "저는 영화덕후라서",
          "speaker_id": "1",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027872.1.5",
          "form": "유툽보단",
          "original_form": "유툽보단",
          "speaker_id": "1",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027872.1.6",
          "form": "왓챠넷플봐요",
          "original_form": "왓챠넷플봐요",
          "speaker_id": "1",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027872.1.7",
          "form": "왓챠 오",
          "original_form": "왓챠 오",
          "speaker_id": "2",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100027872.1.8",
          "form": "저는 유튭프리미엄이랑 티빙 넷플인데",
          "original_form": "저는 유튭프리미엄이랑 티빙 넷플인데",
          "speaker_id": "2",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100027872.1.9",
          "form": "애기 있어서 맨날 공룡이랑",
          "original_form": "애기 있어서 맨날 공룡이랑",
          "speaker_id": "2",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100027872.1.10",
          "form": "뽀로로랑",
          "original_form": "뽀로로랑",
          "speaker_id": "2",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100027872.1.11",
          "form": "콩순오 봄니다 ㅠ",
          "original_form": "콩순오 봄니다 ㅠ",
          "speaker_id": "2",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100027872.1.12",
          "form": "ㅋㅋㅋ저는 조카 주말에 봐주는데",
          "original_form": "ㅋㅋㅋ저는 조카 주말에 봐주는데",
          "speaker_id": "1",
          "time": "20210903 12:48"
        },
        {
          "id": "MDRW2100027872.1.13",
          "form": "시크릿쥬쥬 지겹도록 봐여ㅠㅠ",
          "original_form": "시크릿쥬쥬 지겹도록 봐여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 12:49"
        },
        {
          "id": "MDRW2100027872.1.14",
          "form": "ㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100027872.1.15",
          "form": "오 좋은 삼촌이네요",
          "original_form": "오 좋은 삼촌이네요",
          "speaker_id": "2",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100027872.1.16",
          "form": "죽겟어여ㅋㅋㅋㅋ",
          "original_form": "죽겟어여ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100027872.1.17",
          "form": "자주 봐주시나요",
          "original_form": "자주 봐주시나요",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100027872.1.18",
          "form": "일주일에 한번정도 ㅠㅠ",
          "original_form": "일주일에 한번정도 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 12:57"
        },
        {
          "id": "MDRW2100027872.1.19",
          "form": "부모님들 대단하심다",
          "original_form": "부모님들 대단하심다",
          "speaker_id": "1",
          "time": "20210903 12:57"
        },
        {
          "id": "MDRW2100027872.1.20",
          "form": "유튜브 돌려야되요",
          "original_form": "유튜브 돌려야되요",
          "speaker_id": "2",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100027872.1.21",
          "form": "아님 정신병 걸려요 ㅠ",
          "original_form": "아님 정신병 걸려요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100027872.1.22",
          "form": "맞아요 유툽찬스 써도 너무너무너무 힘들것같아요",
          "original_form": "맞아요 유툽찬스 써도 너무너무너무 힘들것같아요",
          "speaker_id": "1",
          "time": "20210903 12:59"
        },
        {
          "id": "MDRW2100027872.1.23",
          "form": "맞습니다 ㅋㅋ지금도 아기상어가 귓가에 울리네요",
          "original_form": "맞습니다 ㅋㅋ지금도 아기상어가 귓가에 울리네요",
          "speaker_id": "2",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100027872.1.24",
          "form": "ㅋㅋ애기가 아직 어린가바요",
          "original_form": "ㅋㅋ애기가 아직 어린가바요",
          "speaker_id": "1",
          "time": "20210903 15:22"
        },
        {
          "id": "MDRW2100027872.1.25",
          "form": "2살요 ㅋㅋ",
          "original_form": "2살요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:22"
        },
        {
          "id": "MDRW2100027872.1.26",
          "form": "영상보고 상어 춤춥니다 ㅋㅋ",
