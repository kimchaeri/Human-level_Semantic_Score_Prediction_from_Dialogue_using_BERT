      "utterance": [
        {
          "id": "MDRW2100009898.1.1",
          "form": "연락은 자주하세요?",
          "original_form": "연락은 자주하세요?",
          "speaker_id": "2",
          "time": "20210824 08:01"
        },
        {
          "id": "MDRW2100009898.1.2",
          "form": "요새는 영상통화 같은게 있으니까요",
          "original_form": "요새는 영상통화 같은게 있으니까요",
          "speaker_id": "1",
          "time": "20210824 08:02"
        },
        {
          "id": "MDRW2100009898.1.3",
          "form": "커뮤니티라면",
          "original_form": "커뮤니티라면",
          "speaker_id": "1",
          "time": "20210824 08:02"
        },
        {
          "id": "MDRW2100009898.1.4",
          "form": "오프라인은 아니니데",
          "original_form": "오프라인은 아니니데",
          "speaker_id": "1",
          "time": "20210824 08:02"
        },
        {
          "id": "MDRW2100009898.1.5",
          "form": "아닌데",
          "original_form": "아닌데",
          "speaker_id": "1",
          "time": "20210824 08:02"
        },
        {
          "id": "MDRW2100009898.1.6",
          "form": "네이버카페 긴급헌혈단 얼마전에 가입했어요",
          "original_form": "네이버카페 긴급헌혈단 얼마전에 가입했어요",
          "speaker_id": "1",
          "time": "20210824 08:03"
        },
        {
          "id": "MDRW2100009898.1.7",
          "form": "아 그런것도 있어요",
          "original_form": "아 그런것도 있어요",
          "speaker_id": "2",
          "time": "20210824 08:03"
        },
        {
          "id": "MDRW2100009898.1.8",
          "form": "저는 영국에서 6개월이상 지낸적 있는데",
          "original_form": "저는 영국에서 6개월이상 지낸적 있는데",
          "speaker_id": "2",
          "time": "20210824 08:03"
        },
        {
          "id": "MDRW2100009898.1.9",
          "form": "혈우병 위험으로",
          "original_form": "혈우병 위험으로",
          "speaker_id": "2",
          "time": "20210824 08:03"
        },
        {
          "id": "MDRW2100009898.1.10",
          "form": "30년간 헌혈이 안된대요",
          "original_form": "30년간 헌혈이 안된대요",
          "speaker_id": "2",
          "time": "20210824 08:03"
        },
        {
          "id": "MDRW2100009898.1.11",
          "form": "아하 ㅜ 저런 거의 평생이네요",
          "original_form": "아하 ㅜ 저런 거의 평생이네요",
          "speaker_id": "1",
          "time": "20210824 08:04"
        },
        {
          "id": "MDRW2100009898.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210824 08:04"
        },
        {
          "id": "MDRW2100009898.1.13",
          "form": "저는 커뮤니티 활동은 잘 안하는데",
          "original_form": "저는 커뮤니티 활동은 잘 안하는데",
          "speaker_id": "2",
          "time": "20210824 08:04"
        },
        {
          "id": "MDRW2100009898.1.14",
          "form": "활동하면",
          "original_form": "활동하면",
          "speaker_id": "2",
          "time": "20210824 08:04"
        },
        {
          "id": "MDRW2100009898.1.15",
          "form": "요새 코로나라서 혈소판이 많이 부족하대요",
          "original_form": "요새 코로나라서 혈소판이 많이 부족하대요",
          "speaker_id": "1",
          "time": "20210824 08:04"
        },
        {
          "id": "MDRW2100009898.1.16",
          "form": "오프라인 모임도 하나요?",
          "original_form": "오프라인 모임도 하나요?",
          "speaker_id": "2",
          "time": "20210824 08:04"
        },
        {
          "id": "MDRW2100009898.1.17",
          "form": "아 혈소판",
          "original_form": "아 혈소판",
          "speaker_id": "2",
          "time": "20210824 08:04"
        },
        {
          "id": "MDRW2100009898.1.18",
          "form": "백혈병 환자들이 10개 맞아야할거 3개만 아껴맞고 그런다고",
          "original_form": "백혈병 환자들이 10개 맞아야할거 3개만 아껴맞고 그런다고",
          "speaker_id": "1",
          "time": "20210824 08:05"
        },
        {
          "id": "MDRW2100009898.1.19",
          "form": "오프라인 모임은 아니에요 ㅎㅎ",
          "original_form": "오프라인 모임은 아니에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 08:05"
        },
        {
          "id": "MDRW2100009898.1.20",
          "form": "참여하시는 모임 있으세요",
          "original_form": "참여하시는 모임 있으세요",
          "speaker_id": "1",
          "time": "20210824 08:06"
        },
        {
          "id": "MDRW2100009898.1.21",
          "form": "친구들끼리 먹자계하네요 생각해보니",
          "original_form": "친구들끼리 먹자계하네요 생각해보니",
          "speaker_id": "2",
          "time": "20210824 08:07"
        },
        {
          "id": "MDRW2100009898.1.22",
          "form": "다달이 돈모아서 맛있는거 먹으러",
          "original_form": "다달이 돈모아서 맛있는거 먹으러",
          "speaker_id": "2",
          "time": "20210824 08:07"
        },
        {
          "id": "MDRW2100009898.1.23",
          "form": "오 나쁘지 않네요",
          "original_form": "오 나쁘지 않네요",
          "speaker_id": "1",
          "time": "20210824 08:10"
        },
        {
          "id": "MDRW2100009898.1.24",
          "form": "코로나만 조심하면ㅜ",
          "original_form": "코로나만 조심하면ㅜ",
          "speaker_id": "1",
          "time": "20210824 08:10"
        },
        {
          "id": "MDRW2100009898.1.25",
          "form": "아이고 이놈의 코로나 진짜ㅜㅜ",
          "original_form": "아이고 이놈의 코로나 진짜ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 08:10"
        },
        {
          "id": "MDRW2100009898.1.26",
          "form": "사실 코로나때문에 6개월째 모임 안해서",
          "original_form": "사실 코로나때문에 6개월째 모임 안해서",
          "speaker_id": "2",
          "time": "20210824 08:11"
        },
        {
          "id": "MDRW2100009898.1.27",
          "form": "모인돈 돌려주기도 했어요",
          "original_form": "모인돈 돌려주기도 했어요",
          "speaker_id": "2",
          "time": "20210824 08:11"
        },
        {
          "id": "MDRW2100009898.1.28",
          "form": "비말 감염 이라니까 먹는거나 술마시는게 젤 걱정되죠",
          "original_form": "비말 감염 이라니까 먹는거나 술마시는게 젤 걱정되죠",
          "speaker_id": "1",
          "time": "20210824 08:12"
        },
        {
          "id": "MDRW2100009898.1.29",
          "form": "4인이상 모임금지 풀렸나요",
          "original_form": "4인이상 모임금지 풀렸나요",
          "speaker_id": "1",
          "time": "20210824 08:13"
        },
        {
          "id": "MDRW2100009898.1.30",
          "form": "잘 모르겠네요",
          "original_form": "잘 모르겠네요",
          "speaker_id": "2",
          "time": "20210824 08:13"
        },
        {
          "id": "MDRW2100009898.1.31",
          "form": "접종률 올라가면",
          "original_form": "접종률 올라가면",
          "speaker_id": "2",
          "time": "20210824 08:13"
        },
        {
          "id": "MDRW2100009898.1.32",
          "form": "위드코로나로 바뀔거라던데",
