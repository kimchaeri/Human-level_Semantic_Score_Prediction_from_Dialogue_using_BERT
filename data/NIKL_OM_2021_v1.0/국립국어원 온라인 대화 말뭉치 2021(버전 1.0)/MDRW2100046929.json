      "utterance": [
        {
          "id": "MDRW2100046929.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211108 13:00"
        },
        {
          "id": "MDRW2100046929.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211108 13:08"
        },
        {
          "id": "MDRW2100046929.1.3",
          "form": "혹시 MBTI 테스트 해보셨나요?",
          "original_form": "혹시 MBTI 테스트 해보셨나요?",
          "speaker_id": "2",
          "time": "20211108 13:08"
        },
        {
          "id": "MDRW2100046929.1.4",
          "form": "네 해봤어요",
          "original_form": "네 해봤어요",
          "speaker_id": "1",
          "time": "20211108 13:22"
        },
        {
          "id": "MDRW2100046929.1.5",
          "form": "오호 결과가 어떻게 나왔나요?",
          "original_form": "오호 결과가 어떻게 나왔나요?",
          "speaker_id": "2",
          "time": "20211108 13:24"
        },
        {
          "id": "MDRW2100046929.1.6",
          "form": "저는 ESTJ 나왔어요",
          "original_form": "저는 ESTJ 나왔어요",
          "speaker_id": "1",
          "time": "20211108 13:27"
        },
        {
          "id": "MDRW2100046929.1.7",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20211108 13:34"
        },
        {
          "id": "MDRW2100046929.1.8",
          "form": "저는 ENFP에요!",
          "original_form": "저는 ENFP에요!",
          "speaker_id": "2",
          "time": "20211108 13:34"
        },
        {
          "id": "MDRW2100046929.1.9",
          "form": "하나 빼고 다 다르네용",
          "original_form": "하나 빼고 다 다르네용",
          "speaker_id": "2",
          "time": "20211108 13:34"
        },
        {
          "id": "MDRW2100046929.1.10",
          "form": "저는 다르게 나왔으면 하는 마음에",
          "original_form": "저는 다르게 나왔으면 하는 마음에",
          "speaker_id": "2",
          "time": "20211108 13:34"
        },
        {
          "id": "MDRW2100046929.1.11",
          "form": "엄청 여러번 해보고",
          "original_form": "엄청 여러번 해보고",
          "speaker_id": "2",
          "time": "20211108 13:34"
        },
        {
          "id": "MDRW2100046929.1.12",
          "form": "영문버전으로도",
          "original_form": "영문버전으로도",
          "speaker_id": "2",
          "time": "20211108 13:34"
        },
        {
          "id": "MDRW2100046929.1.13",
          "form": "해봤는데..",
          "original_form": "해봤는데..",
          "speaker_id": "2",
          "time": "20211108 13:34"
        },
        {
          "id": "MDRW2100046929.1.14",
          "form": "ENFP만 나오더라구요",
          "original_form": "ENFP만 나오더라구요",
          "speaker_id": "2",
          "time": "20211108 13:34"
        },
        {
          "id": "MDRW2100046929.1.15",
          "form": "그래서 받아들였습니다..",
          "original_form": "그래서 받아들였습니다..",
          "speaker_id": "2",
          "time": "20211108 13:34"
        },
        {
          "id": "MDRW2100046929.1.16",
          "form": "영문버전!",
          "original_form": "영문버전!",
          "speaker_id": "1",
          "time": "20211108 13:34"
        },
        {
          "id": "MDRW2100046929.1.17",
          "form": "영어 잘하시나봐요",
          "original_form": "영어 잘하시나봐요",
          "speaker_id": "1",
          "time": "20211108 13:35"
        },
        {
          "id": "MDRW2100046929.1.18",
          "form": "엇",
          "original_form": "엇",
          "speaker_id": "2",
          "time": "20211108 13:35"
        },
        {
          "id": "MDRW2100046929.1.19",
          "form": "아닙니다 ㅋㅋㅋㅋ",
          "original_form": "아닙니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 13:35"
        },
        {
          "id": "MDRW2100046929.1.20",
          "form": "모르겠는건 파파고 돌려가면서",
          "original_form": "모르겠는건 파파고 돌려가면서",
          "speaker_id": "2",
          "time": "20211108 13:35"
        },
        {
          "id": "MDRW2100046929.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅎㅎ",
          "speaker_id": "2",
          "time": "20211108 13:35"
        },
        {
          "id": "MDRW2100046929.1.22",
          "form": "영문버전이 좀 더 ",
          "original_form": "영문버전이 좀 더 ",
          "speaker_id": "2",
          "time": "20211108 13:35"
        },
        {
          "id": "MDRW2100046929.1.23",
          "form": "파파고!",
          "original_form": "파파고!",
          "speaker_id": "1",
          "time": "20211108 13:35"
        },
        {
          "id": "MDRW2100046929.1.24",
          "form": "질문의 의도를 정확하게 파악하기",
          "original_form": "질문의 의도를 정확하게 파악하기",
          "speaker_id": "2",
          "time": "20211108 13:35"
        },
        {
          "id": "MDRW2100046929.1.25",
          "form": "좋다고 해서",
          "original_form": "좋다고 해서",
          "speaker_id": "2",
          "time": "20211108 13:35"
        },
        {
          "id": "MDRW2100046929.1.26",
          "form": "해봤어요 ㅎㅎ",
          "original_form": "해봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211108 13:35"
        },
        {
          "id": "MDRW2100046929.1.27",
          "form": "한국어로 된 건 가끔 해석이 이상하게 되서 질문 의도를 모르겠는게 있더라구요",
          "original_form": "한국어로 된 건 가끔 해석이 이상하게 되서 질문 의도를 모르겠는게 있더라구요",
          "speaker_id": "2",
          "time": "20211108 13:35"
        },
        {
          "id": "MDRW2100046929.1.28",
          "form": "파파고가 구글보다 더 좋나요?",
          "original_form": "파파고가 구글보다 더 좋나요?",
          "speaker_id": "1",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046929.1.29",
          "form": "저는 개인적으로 파파고를 더 선호해요!",
          "original_form": "저는 개인적으로 파파고를 더 선호해요!",
          "speaker_id": "2",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046929.1.30",
          "form": "번역이 더 매끄러워요",
          "original_form": "번역이 더 매끄러워요",
          "speaker_id": "2",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046929.1.31",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046929.1.32",
          "form": "그래도 둘 다 훌륭합니다... 많이 발전했어요",
          "original_form": "그래도 둘 다 훌륭합니다... 많이 발전했어요",
          "speaker_id": "2",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046929.1.33",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046929.1.34",
          "form": "저도 파파고 써봐야겠어요",
          "original_form": "저도 파파고 써봐야겠어요",
          "speaker_id": "1",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046929.1.35",
          "form": "네네 번역기 중에 제일 자연스럽게 잘되는거 같아요",
          "original_form": "네네 번역기 중에 제일 자연스럽게 잘되는거 같아요",
          "speaker_id": "2",
          "time": "20211108 13:37"
        },
        {
          "id": "MDRW2100046929.1.36",
          "form": "근데 저는같은 문항을 해도 그때그때 조금씩 그날 기분에 따라",
          "original_form": "근데 저는같은 문항을 해도 그때그때 조금씩 그날 기분에 따라",
          "speaker_id": "1",
          "time": "20211108 13:37"
        },
        {
          "id": "MDRW2100046929.1.37",
          "form": "답이 달라지는거같아요",
          "original_form": "답이 달라지는거같아요",
          "speaker_id": "1",
          "time": "20211108 13:37"
        },
        {
          "id": "MDRW2100046929.1.38",
          "form": "기분과 컨디션에 따라 조금씩",
          "original_form": "기분과 컨디션에 따라 조금씩",
          "speaker_id": "1",
          "time": "20211108 13:37"
        },
        {
          "id": "MDRW2100046929.1.39",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 13:38"
        },
        {
          "id": "MDRW2100046929.1.40",
          "form": "그래도 큰 틀은 비슷한거같더라구요 ㅎㅎ\r\n",
          "original_form": "그래도 큰 틀은 비슷한거같더라구요 ㅎㅎ\r\n",
          "speaker_id": "2",
          "time": "20211108 13:38"
        }
      ]
    }
