      "utterance": [
        {
          "id": "MDRW2100026343.1.1",
          "form": "쇼핑 좋아하시나요??",
          "original_form": "쇼핑 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100026343.1.2",
          "form": "쇼핑을 안좋아항수없죠!",
          "original_form": "쇼핑을 안좋아항수없죠!",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026343.1.3",
          "form": "오늘도 택배가 셋이나..ㅎㅎ",
          "original_form": "오늘도 택배가 셋이나..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026343.1.4",
          "form": "ㅋㅋㅋㅋㅋ 맞아요 코로나 때문에 재미있는게 쇼핑인 것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요 코로나 때문에 재미있는게 쇼핑인 것 같아요",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026343.1.5",
          "form": "주로 뭐 구매 하세요?",
          "original_form": "주로 뭐 구매 하세요?",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026343.1.6",
          "form": "최근에는 가방",
          "original_form": "최근에는 가방",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026343.1.7",
          "form": "구매햇는데요",
          "original_form": "구매햇는데요",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026343.1.8",
          "form": "요즘은 디퓨저 관심이 잔득있어요",
          "original_form": "요즘은 디퓨저 관심이 잔득있어요",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026343.1.9",
          "form": "오 디퓨저 어디것이 좋나요? 저는 향에 예민해서 향수는 힘든데",
          "original_form": "오 디퓨저 어디것이 좋나요? 저는 향에 예민해서 향수는 힘든데",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026343.1.10",
          "form": "디퓨저는 괜찮더라구요!",
          "original_form": "디퓨저는 괜찮더라구요!",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026343.1.11",
          "form": "저도 엄청 예민한데",
          "original_form": "저도 엄청 예민한데",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026343.1.12",
          "form": "그 메르시엔코 플라워마켓은",
          "original_form": "그 메르시엔코 플라워마켓은",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026343.1.13",
          "form": "괜찮더라구요",
          "original_form": "괜찮더라구요",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026343.1.14",
          "form": "그래서 그거만 사용하는데",
          "original_form": "그래서 그거만 사용하는데",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026343.1.15",
          "form": "다른 디퓨저파는곳에서도 플라워하우스라던지",
          "original_form": "다른 디퓨저파는곳에서도 플라워하우스라던지",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026343.1.16",
          "form": "플라워디퓨저나오길래",
          "original_form": "플라워디퓨저나오길래",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026343.1.17",
          "form": "사볼까 고민해요",
          "original_form": "사볼까 고민해요",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026343.1.18",
          "form": "우와 이렇게 자세하게 알려주시다니 너무 감사해요 ㅎㅎ",
          "original_form": "우와 이렇게 자세하게 알려주시다니 너무 감사해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026343.1.19",
          "form": "꼭 찾아봐야겠어요!",
          "original_form": "꼭 찾아봐야겠어요!",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026343.1.20",
          "form": "직구도 하시나요?",
          "original_form": "직구도 하시나요?",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026343.1.21",
          "form": "저는 요새 직구 재미에 빠져서",
          "original_form": "저는 요새 직구 재미에 빠져서",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026343.1.22",
          "form": "아뇨 직구는 안해요 ㅠ",
          "original_form": "아뇨 직구는 안해요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026343.1.23",
          "form": "요즘 코로나라",
          "original_form": "요즘 코로나라",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026343.1.24",
          "form": "가격이엄청오르더라구요 ㅠㅜ",
          "original_form": "가격이엄청오르더라구요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026343.1.25",
          "form": "아 그쵸 ㅠㅠㅠ 저는 옷 좋아해서 직구 선호하는데",
          "original_form": "아 그쵸 ㅠㅠㅠ 저는 옷 좋아해서 직구 선호하는데",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026343.1.26",
          "form": "끊을 수가 없더라구요 ㅎㅎ",
          "original_form": "끊을 수가 없더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026343.1.27",
          "form": "직구하면 이쁜게 많나요?",
          "original_form": "직구하면 이쁜게 많나요?",
          "speaker_id": "2",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026343.1.28",
          "form": "음 가격이 너무 좋아요 폴로나 타미 같은것도 가격이 너무 좋고",
          "original_form": "음 가격이 너무 좋아요 폴로나 타미 같은것도 가격이 너무 좋고",
          "speaker_id": "1",
          "time": "20210901 21:54"
        }
      ]
