      "utterance": [
        {
          "id": "MDRW2100016767.1.1",
          "form": "당연하지~",
          "original_form": "당연하지~",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016767.1.2",
          "form": "당근마켓 알아?",
          "original_form": "당근마켓 알아?",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016767.1.3",
          "form": "거기서 물건도 사봤는걸",
          "original_form": "거기서 물건도 사봤는걸",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016767.1.4",
          "form": "많이 하는 편이야?",
          "original_form": "많이 하는 편이야?",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016767.1.5",
          "form": "자주는 안하고 그냥 한두번씩 확인만 해ㅋㅋ",
          "original_form": "자주는 안하고 그냥 한두번씩 확인만 해ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016767.1.6",
          "form": "너는 자주 해?",
          "original_form": "너는 자주 해?",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016767.1.7",
          "form": "아니 나도 한두번 해봤어 ㅋㅋ",
          "original_form": "아니 나도 한두번 해봤어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016767.1.8",
          "form": "뭐사봤어?",
          "original_form": "뭐사봤어?",
          "speaker_id": "1",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016767.1.9",
          "form": "나 프린터!",
          "original_form": "나 프린터!",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016767.1.10",
          "form": "잉크도 많아서 완전 싸게 잘샀어ㅋㅋ",
          "original_form": "잉크도 많아서 완전 싸게 잘샀어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016767.1.11",
          "form": "오 큰거 샀네?! 직접 가서 들고왔어?",
          "original_form": "오 큰거 샀네?! 직접 가서 들고왔어?",
          "speaker_id": "1",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100016767.1.12",
          "form": "그쪽 집 근처에서 거래하길 희망해서 주말에 아빠 차 타고 다녀왂어ㅋㅋ",
          "original_form": "그쪽 집 근처에서 거래하길 희망해서 주말에 아빠 차 타고 다녀왂어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100016767.1.13",
          "form": "너는 어떤 거 거래해봤어?",
          "original_form": "너는 어떤 거 거래해봤어?",
          "speaker_id": "2",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100016767.1.14",
          "form": "타이밍이좋았구나",
          "original_form": "타이밍이좋았구나",
          "speaker_id": "1",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100016767.1.15",
          "form": "난 모니터 받침대 ㅋㅋ",
          "original_form": "난 모니터 받침대 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016767.1.16",
          "form": "버스타고가서..버스비 추가요 ㅎ",
          "original_form": "버스타고가서..버스비 추가요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016767.1.17",
          "form": "물건은 괜찮았어?",
          "original_form": "물건은 괜찮았어?",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016767.1.18",
          "form": "ㅎㅎ그래도 새로 사는거보단 나았을거야",
          "original_form": "ㅎㅎ그래도 새로 사는거보단 나았을거야",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016767.1.19",
          "form": "완전 새상품 같았어",
          "original_form": "완전 새상품 같았어",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016767.1.20",
          "form": "만족!",
          "original_form": "만족!",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016767.1.21",
          "form": "ㅋㅋ맞아 돈도 아끼고 상품 재활용도하고 !",
          "original_form": "ㅋㅋ맞아 돈도 아끼고 상품 재활용도하고 !",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016767.1.22",
          "form": "나도 만족했어",
          "original_form": "나도 만족했어",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016767.1.23",
          "form": "다음에도 기회되면 자주 이용할 것 같아ㅋㅋ",
          "original_form": "다음에도 기회되면 자주 이용할 것 같아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:20"
        }
      ]
