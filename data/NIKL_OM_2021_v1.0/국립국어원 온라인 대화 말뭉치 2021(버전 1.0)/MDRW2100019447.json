      "utterance": [
        {
          "id": "MDRW2100019447.1.1",
          "form": "학교 다니는 학생인가요?",
          "original_form": "학교 다니는 학생인가요?",
          "speaker_id": "1",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019447.1.2",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019447.1.3",
          "form": "2학기가 곧 시작되는데 수강신청은 잘하셨나요",
          "original_form": "2학기가 곧 시작되는데 수강신청은 잘하셨나요",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100019447.1.4",
          "form": "다행히 올클 했습니다!",
          "original_form": "다행히 올클 했습니다!",
          "speaker_id": "2",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100019447.1.5",
          "form": "와우 대박! 쉽지 않은데 어떻게 하셨어요?",
          "original_form": "와우 대박! 쉽지 않은데 어떻게 하셨어요?",
          "speaker_id": "1",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100019447.1.6",
          "form": "전공이 많아서 쉬웟어요",
          "original_form": "전공이 많아서 쉬웟어요",
          "speaker_id": "2",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100019447.1.7",
          "form": "슬픈 이유네요ㅜ",
          "original_form": "슬픈 이유네요ㅜ",
          "speaker_id": "2",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100019447.1.8",
          "form": "그럼 하루 풀공강 만드셨나요? 금요일?ㅎㅎ",
          "original_form": "그럼 하루 풀공강 만드셨나요? 금요일?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100019447.1.9",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100019447.1.10",
          "form": "금공이에요",
          "original_form": "금공이에요",
          "speaker_id": "2",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100019447.1.11",
          "form": "벌써부터 행복하죠",
          "original_form": "벌써부터 행복하죠",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100019447.1.12",
          "form": "그대신 다른 날들로 몰렸지만",
          "original_form": "그대신 다른 날들로 몰렸지만",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100019447.1.13",
          "form": "시험이 걱정이네요",
          "original_form": "시험이 걱정이네요",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100019447.1.14",
          "form": "name1님도 학생이신가요?",
          "original_form": "&name1&님도 학생이신가요?",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100019447.1.15",
          "form": "저는 졸업한지 오래된 화석입니다 ㅜㅜ",
          "original_form": "저는 졸업한지 오래된 화석입니다 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100019447.1.16",
          "form": "저는 대체로 성공했지만 한두과목은 실패하고 그랬어요 ㅋㅋ",
          "original_form": "저는 대체로 성공했지만 한두과목은 실패하고 그랬어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100019447.1.17",
          "form": "성공률 높으시네요!",
          "original_form": "성공률 높으시네요!",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100019447.1.18",
          "form": "전 올클 이번이 처음이에요 ㅋㅋㅋ",
          "original_form": "전 올클 이번이 처음이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100019447.1.19",
          "form": "그전까진 저도ㅜ",
          "original_form": "그전까진 저도ㅜ",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100019447.1.20",
          "form": "인기 많은것들 포기하니 쉽긴하더라구요",
          "original_form": "인기 많은것들 포기하니 쉽긴하더라구요",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100019447.1.21",
          "form": "ㅋㅋㅋㅋ안돼요 인기 많은 것부터 수강신청 호다닥 담아야해요",
          "original_form": "ㅋㅋㅋㅋ안돼요 인기 많은 것부터 수강신청 호다닥 담아야해요",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100019447.1.22",
          "form": "저 그래서 스키수업 들은 적도 있어요!!",
          "original_form": "저 그래서 스키수업 들은 적도 있어요!!",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100019447.1.23",
          "form": "스키수업이요?",
          "original_form": "스키수업이요?",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100019447.1.24",
          "form": "직접 스키장에 가신건가요?",
          "original_form": "직접 스키장에 가신건가요?",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100019447.1.25",
          "form": "와 엄청 재밌겠어요",
          "original_form": "와 엄청 재밌겠어요",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100019447.1.26",
          "form": "네 맞아요!! 그 수업 듣는 학생들 전부 스키장 갔어요~",
          "original_form": "네 맞아요!! 그 수업 듣는 학생들 전부 스키장 갔어요~",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100019447.1.27",
          "form": "그 스키장 가기 전에는 스케이트장에서 수업했구요!",
          "original_form": "그 스키장 가기 전에는 스케이트장에서 수업했구요!",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100019447.1.28",
          "form": "우와 신박한 수업이네요",
          "original_form": "우와 신박한 수업이네요",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100019447.1.29",
          "form": "거기서 성적을 어떻게 나눴나요?",
          "original_form": "거기서 성적을 어떻게 나눴나요?",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100019447.1.30",
          "form": "궁금해요",
          "original_form": "궁금해요",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100019447.1.31",
          "form": "저도 궁금하네요 교수님...ㅋㅋㅋㅋ",
          "original_form": "저도 궁금하네요 교수님...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100019447.1.32",
          "form": "그냥 자세를 위주로 보고 채점하시는 것 같더라구요",
          "original_form": "그냥 자세를 위주로 보고 채점하시는 것 같더라구요",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100019447.1.33",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100019447.1.34",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100019447.1.35",
          "form": "뭔가 중고등학생 체육 수행평가 같기도하네요",
          "original_form": "뭔가 중고등학생 체육 수행평가 같기도하네요",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100019447.1.36",
          "form": "맞죠 ㅋㅋㅋ 그래서 뭔가 깨름칙해요",
          "original_form": "맞죠 ㅋㅋㅋ 그래서 뭔가 깨름칙해요",
          "speaker_id": "1",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100019447.1.37",
          "form": "스키는 그 기술이 여러가지 있거든요 아세용?",
          "original_form": "스키는 그 기술이 여러가지 있거든요 아세용?",
          "speaker_id": "1",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100019447.1.38",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "2",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100019447.1.39",
          "form": "스키에 대해 하나도 모르는 인간입니다",
          "original_form": "스키에 대해 하나도 모르는 인간입니다",
