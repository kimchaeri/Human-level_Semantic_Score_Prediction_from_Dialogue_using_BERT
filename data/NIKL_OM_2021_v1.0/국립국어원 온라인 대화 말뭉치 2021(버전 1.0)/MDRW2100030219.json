      "utterance": [
        {
          "id": "MDRW2100030219.1.1",
          "form": "된듯..반려동물 한번만 더 이야기해서",
          "original_form": "된듯..반려동물 한번만 더 이야기해서",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030219.1.2",
          "form": "또 내볼까",
          "original_form": "또 내볼까",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030219.1.3",
          "form": "같은 주제여도 괜찮은 것 같던데",
          "original_form": "같은 주제여도 괜찮은 것 같던데",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030219.1.4",
          "form": "그래 !!!!!!!",
          "original_form": "그래 !!!!!!!",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030219.1.5",
          "form": "좋아!!!",
          "original_form": "좋아!!!",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030219.1.6",
          "form": "아 나는 친칠라도",
          "original_form": "아 나는 친칠라도",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030219.1.7",
          "form": "조금 꿈의 반려동물 이랄까..",
          "original_form": "조금 꿈의 반려동물 이랄까..",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030219.1.8",
          "form": "원래 아무생각 없었는데",
          "original_form": "원래 아무생각 없었는데",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030219.1.9",
          "form": "최근에 갑자기 빡.. 좋아졌어",
          "original_form": "최근에 갑자기 빡.. 좋아졌어",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030219.1.10",
          "form": "오~~~ 친칠라",
          "original_form": "오~~~ 친칠라",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030219.1.11",
          "form": "뭔가 그 뭉실뭉실한 궁뎅이가 너무 귀여워",
          "original_form": "뭔가 그 뭉실뭉실한 궁뎅이가 너무 귀여워",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030219.1.12",
          "form": "ㅠㅠ뭉실궁댕 뭉실이",
          "original_form": "ㅠㅠ뭉실궁댕 뭉실이",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030219.1.13",
          "form": "유튜브 보니까",
          "original_form": "유튜브 보니까",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030219.1.14",
          "form": "반려 여우 기르는 사람들도 있더라..",
          "original_form": "반려 여우 기르는 사람들도 있더라..",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030219.1.15",
          "form": "정말 큰 용기가 필요한것같아..",
          "original_form": "정말 큰 용기가 필요한것같아..",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030219.1.16",
          "form": "헐... 여우 어케 기르지",
          "original_form": "헐... 여우 어케 기르지",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030219.1.17",
          "form": "습성이나 상식도 부족해가지구...나같은 얼레벌레 사람은",
          "original_form": "습성이나 상식도 부족해가지구...나같은 얼레벌레 사람은",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030219.1.18",
          "form": "절대 못 기를듯.",
          "original_form": "절대 못 기를듯.",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030219.1.19",
          "form": "여우.. 아주 귀여웠어요.",
          "original_form": "여우.. 아주 귀여웠어요.",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030219.1.20",
          "form": "ㄹㅇ..",
          "original_form": "ㄹㅇ..",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030219.1.21",
          "form": "여우.. 귀엽지만 키우라고 한다면 예???? 제가요??? 상태 되는거지",
          "original_form": "여우.. 귀엽지만 키우라고 한다면 예???? 제가요??? 상태 되는거지",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030219.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그렇지 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그렇지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030219.1.23",
          "form": "아 이번에 식물들 떠나보내면서 생각하는건데",
          "original_form": "아 이번에 식물들 떠나보내면서 생각하는건데",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030219.1.24",
          "form": "뱀 진짜 기르고싶은데 파충류들은 조명,온도,습도",
          "original_form": "뱀 진짜 기르고싶은데 파충류들은 조명,온도,습도",
          "speaker_id": "2",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030219.1.25",
          "form": "예민한 편이라서",
          "original_form": "예민한 편이라서",
          "speaker_id": "2",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030219.1.26",
          "form": "많이... 두려워졌어.",
          "original_form": "많이... 두려워졌어.",
          "speaker_id": "2",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030219.1.27",
          "form": "뱀.....진짜 기르고싶은데 길렀다가 죽으면 진짜 충격일듯",
          "original_form": "뱀.....진짜 기르고싶은데 길렀다가 죽으면 진짜 충격일듯",
          "speaker_id": "2",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030219.1.28",
          "form": "아 ㅠㅠㅠㅠㅠㅠ 진짜.......",
          "original_form": "아 ㅠㅠㅠㅠㅠㅠ 진짜.......",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030219.1.29",
          "form": "식물도 그렇다마는 동물은 보내는 순간이 있다는게",
          "original_form": "식물도 그렇다마는 동물은 보내는 순간이 있다는게",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030219.1.30",
          "form": "생명을 책임지는건 정말 쉽지않아..",
          "original_form": "생명을 책임지는건 정말 쉽지않아..",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030219.1.31",
          "form": "넘... 무서운거같야",
          "original_form": "넘... 무서운거같야",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030219.1.32",
          "form": "아윽...",
          "original_form": "아윽...",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030219.1.33",
          "form": "마자...",
          "original_form": "마자...",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030219.1.34",
          "form": "맞아.. 나도 어릴때",
          "original_form": "맞아.. 나도 어릴때",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030219.1.35",
          "form": "코찔찔이처럼 길렀던 동물들",
          "original_form": "코찔찔이처럼 길렀던 동물들",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030219.1.36",
          "form": "죽었을때...아직두 기억에 남아있어..",
          "original_form": "죽었을때...아직두 기억에 남아있어..",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030219.1.37",
          "form": "헐........ 넘 마음아퍼",
          "original_form": "헐........ 넘 마음아퍼",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030219.1.38",
          "form": "기니피그랑 토끼랑 햄스터랑 강아지랑...진짜 많이도 길렀네",
          "original_form": "기니피그랑 토끼랑 햄스터랑 강아지랑...진짜 많이도 길렀네",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030219.1.39",
          "form": "아니 진짜 많이키웠잖아",
          "original_form": "아니 진짜 많이키웠잖아",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030219.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그런데 다 초등학교때까지만 이야기구",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그런데 다 초등학교때까지만 이야기구",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030219.1.41",
          "form": "중학교 들어가선 하쭌이 생겨서",
          "original_form": "중학교 들어가선 하쭌이 생겨서",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030219.1.42",
          "form": "그 이후론 아예 못길렀어",
          "original_form": "그 이후론 아예 못길렀어",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030219.1.43",
          "form": "헐 그랫엇구나",
          "original_form": "헐 그랫엇구나",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030219.1.44",
          "form": "하긴 집에 갓난아기가 있으면",
          "original_form": "하긴 집에 갓난아기가 있으면",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030219.1.45",
          "form": "그래서 솔직히 내가 책임지고 기른 애들은 없다싶이해서",
          "original_form": "그래서 솔직히 내가 책임지고 기른 애들은 없다싶이해서",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030219.1.46",
          "form": "그럴만도",
          "original_form": "그럴만도",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030219.1.47",
          "form": "부모님은 아직도..내가 동물 기르고싶다 말하면 좀 회의적이시더라",
          "original_form": "부모님은 아직도..내가 동물 기르고싶다 말하면 좀 회의적이시더라",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030219.1.48",
          "form": "하지만 저는 독립했으니 말 안하고 기를 것임",
          "original_form": "하지만 저는 독립했으니 말 안하고 기를 것임",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
