      "utterance": [
        {
          "id": "MDRW2100022418.1.1",
          "form": "와 진짜 많네요!! ㅎㅎ",
          "original_form": "와 진짜 많네요!! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100022418.1.2",
          "form": "수영 저도 배우고 싶어요 ㅠㅠ",
          "original_form": "수영 저도 배우고 싶어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100022418.1.3",
          "form": "그쵸 수영 진짜 배우고 싶어요 저도ㅠ",
          "original_form": "그쵸 수영 진짜 배우고 싶어요 저도ㅠ",
          "speaker_id": "1",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100022418.1.4",
          "form": "코로나인데 운영은 하더라고요 수영장",
          "original_form": "코로나인데 운영은 하더라고요 수영장",
          "speaker_id": "1",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100022418.1.5",
          "form": "그치만 무서워서 꺼려지네요 ㅠㅠㅠ",
          "original_form": "그치만 무서워서 꺼려지네요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100022418.1.6",
          "form": "코로나 끝나기전엔 못갈거같아요",
          "original_form": "코로나 끝나기전엔 못갈거같아요",
          "speaker_id": "2",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100022418.1.7",
          "form": "그쵸.. ㅜㅜ 얼른 종식됐으면 좋겠어요",
          "original_form": "그쵸.. ㅜㅜ 얼른 종식됐으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100022418.1.8",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100022418.1.9",
          "form": "저는 9월중에 맞아요",
          "original_form": "저는 9월중에 맞아요",
          "speaker_id": "1",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100022418.1.10",
          "form": "화이자로",
          "original_form": "화이자로",
          "speaker_id": "1",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100022418.1.11",
          "form": "저두 9월 중에 화이자 또는 모더나에요!",
          "original_form": "저두 9월 중에 화이자 또는 모더나에요!",
          "speaker_id": "2",
          "time": "20210829 14:05"
        },
        {
          "id": "MDRW2100022418.1.12",
          "form": "아마도 화이자일 확률이 높네요!",
          "original_form": "아마도 화이자일 확률이 높네요!",
          "speaker_id": "2",
          "time": "20210829 14:05"
        },
        {
          "id": "MDRW2100022418.1.13",
          "form": "오 그렇죠",
          "original_form": "오 그렇죠",
          "speaker_id": "1",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100022418.1.14",
          "form": "뉴스보니까 화이자가 fda정식 승인 났다고 하더라고요",
          "original_form": "뉴스보니까 화이자가 fda정식 승인 났다고 하더라고요",
          "speaker_id": "1",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100022418.1.15",
          "form": "그래서 조금 믿을만한데 부작용이 가장 많은게 화이자라죠",
          "original_form": "그래서 조금 믿을만한데 부작용이 가장 많은게 화이자라죠",
          "speaker_id": "1",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100022418.1.16",
          "form": "주변에서도 알레르기 반응만 몇명인지..참 무서워요",
          "original_form": "주변에서도 알레르기 반응만 몇명인지..참 무서워요",
          "speaker_id": "1",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100022418.1.17",
          "form": "주변에 많으신가요? ㅠㅠ 전 아직 주변에는 없어서 ㅠㅠ",
          "original_form": "주변에 많으신가요? ㅠㅠ 전 아직 주변에는 없어서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100022418.1.18",
          "form": "네.. 2차에서 좀 많이 그러더라고요",
          "original_form": "네.. 2차에서 좀 많이 그러더라고요",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100022418.1.19",
          "form": "겁나긴 하는데 안 맞으면 위험해서.. 맞으려구요",
          "original_form": "겁나긴 하는데 안 맞으면 위험해서.. 맞으려구요",
          "speaker_id": "1",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100022418.1.20",
          "form": "맞아요ㅠㅠ 별일없이 잘 지나가길 바래야죠",
          "original_form": "맞아요ㅠㅠ 별일없이 잘 지나가길 바래야죠",
          "speaker_id": "2",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100022418.1.21",
          "form": "옷은 어디서 사시나요?",
          "original_form": "옷은 어디서 사시나요?",
          "speaker_id": "1",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022418.1.22",
          "form": "저는 인터넷에서 많이샀었는데 사는 족족 망해서 밖에나가서 사봤거든요",
          "original_form": "저는 인터넷에서 많이샀었는데 사는 족족 망해서 밖에나가서 사봤거든요",
          "speaker_id": "1",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022418.1.23",
          "form": "밖에서 직접 입고 사니까 무조건 성공이더라고요...하",
          "original_form": "밖에서 직접 입고 사니까 무조건 성공이더라고요...하",
          "speaker_id": "1",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022418.1.24",
          "form": "이제 인터넷에서는 잘 못사겠어요",
          "original_form": "이제 인터넷에서는 잘 못사겠어요",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022418.1.25",
          "form": "맞아요 인터넷은 정말 보정도 많이 하고 아예 사진 자체도 속여서 찍는게 많아서 믿을 게 못되죠 ㅠㅠ",
          "original_form": "맞아요 인터넷은 정말 보정도 많이 하고 아예 사진 자체도 속여서 찍는게 많아서 믿을 게 못되죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022418.1.26",
          "form": "그리고 인터넷은 유독 한 철 밖에 못입는 옷이 많은거같아요 ㅠ",
          "original_form": "그리고 인터넷은 유독 한 철 밖에 못입는 옷이 많은거같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022418.1.27",
          "form": "헐 맞아요 저의 무의식에 있던 생각이네요 한 철 입는 거요",
          "original_form": "헐 맞아요 저의 무의식에 있던 생각이네요 한 철 입는 거요",
          "speaker_id": "1",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022418.1.28",
          "form": "옷 사러 나가기 귀찮은데 막상 또 좋은 옷 건지면 기분은 좋죠 하하",
          "original_form": "옷 사러 나가기 귀찮은데 막상 또 좋은 옷 건지면 기분은 좋죠 하하",
          "speaker_id": "1",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022418.1.29",
          "form": "옷 살 계획 있으신가요?",
          "original_form": "옷 살 계획 있으신가요?",
          "speaker_id": "1",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022418.1.30",
          "form": "옷은 사도 늘 없는 것 같아요..ㅠ 매번 뭐 입지 고민하게 되는 옷장이네요 ㅠㅠ",
          "original_form": "옷은 사도 늘 없는 것 같아요..ㅠ 매번 뭐 입지 고민하게 되는 옷장이네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100022418.1.31",
          "form": "이제 또 추워지니까 셔츠 사고싶네요 ㅎㅎ",
          "original_form": "이제 또 추워지니까 셔츠 사고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100022418.1.32",
          "form": "맞아요 입는 것 보다 안 입는게 더 많은 것 같네요..",
          "original_form": "맞아요 입는 것 보다 안 입는게 더 많은 것 같네요..",
          "speaker_id": "1",
          "time": "20210829 14:33"
        },
        {
          "id": "MDRW2100022418.1.33",
          "form": "셔츠 좋죠",
          "original_form": "셔츠 좋죠",
          "speaker_id": "1",
          "time": "20210829 14:33"
        },
        {
          "id": "MDRW2100022418.1.34",
