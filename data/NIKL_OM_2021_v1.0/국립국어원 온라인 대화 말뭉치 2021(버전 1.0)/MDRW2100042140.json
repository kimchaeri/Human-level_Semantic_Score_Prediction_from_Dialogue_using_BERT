      "utterance": [
        {
          "id": "MDRW2100042140.1.1",
          "form": "왔다갔다한거빼곤좋앗던거같아",
          "original_form": "왔다갔다한거빼곤좋앗던거같아",
          "speaker_id": "2",
          "time": "20210906 20:03"
        },
        {
          "id": "MDRW2100042140.1.2",
          "form": "맞아 요즘 비가 왜케 왔다리갔다리 할까",
          "original_form": "맞아 요즘 비가 왜케 왔다리갔다리 할까",
          "speaker_id": "1",
          "time": "20210906 20:03"
        },
        {
          "id": "MDRW2100042140.1.3",
          "form": "그러게 힝",
          "original_form": "그러게 힝",
          "speaker_id": "2",
          "time": "20210906 20:04"
        },
        {
          "id": "MDRW2100042140.1.4",
          "form": "여우비라고하니그걸?맞나?",
          "original_form": "여우비라고하니그걸?맞나?",
          "speaker_id": "2",
          "time": "20210906 20:04"
        },
        {
          "id": "MDRW2100042140.1.5",
          "form": "맞아 호랑이 장가가는날",
          "original_form": "맞아 호랑이 장가가는날",
          "speaker_id": "1",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100042140.1.6",
          "form": "여우가 시집을 가는지 먼지 ㅋㅋ",
          "original_form": "여우가 시집을 가는지 먼지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100042140.1.7",
          "form": "호랑이장가가는날ㅋㅋㄱㅋ",
          "original_form": "호랑이장가가는날ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100042140.1.8",
          "form": "흐흐",
          "original_form": "흐흐",
          "speaker_id": "2",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100042140.1.9",
          "form": "긴옷입구싶다",
          "original_form": "긴옷입구싶다",
          "speaker_id": "2",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100042140.1.10",
          "form": "추워져라 얍",
          "original_form": "추워져라 얍",
          "speaker_id": "2",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100042140.1.11",
          "form": "오늘 나 긴옷입얼어",
          "original_form": "오늘 나 긴옷입얼어",
          "speaker_id": "1",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100042140.1.12",
          "form": "낼 입어봐",
          "original_form": "낼 입어봐",
          "speaker_id": "1",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100042140.1.13",
          "form": "저는아직덥습니다호잇",
          "original_form": "저는아직덥습니다호잇",
          "speaker_id": "2",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042140.1.14",
          "form": "그렇군요 맞아요 빡치면 덥더라고요",
          "original_form": "그렇군요 맞아요 빡치면 덥더라고요",
          "speaker_id": "1",
          "time": "20210906 20:22"
        },
        {
          "id": "MDRW2100042140.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042140.1.16",
          "form": "빡치면덥죠. 태양보다덥죠.",
          "original_form": "빡치면덥죠. 태양보다덥죠.",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042140.1.17",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042140.1.18",
          "form": "낼은 또 뭘 입나",
          "original_form": "낼은 또 뭘 입나",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042140.1.19",
          "form": "낼은뭐입지 그러게요",
          "original_form": "낼은뭐입지 그러게요",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042140.1.20",
