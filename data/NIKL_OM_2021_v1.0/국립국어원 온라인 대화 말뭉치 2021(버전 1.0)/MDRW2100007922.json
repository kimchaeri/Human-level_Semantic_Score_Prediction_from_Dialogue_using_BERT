        {
          "id": "MDRW2100007922.1.9",
          "form": "맞아요 가끔 교회에서 그런 행사하면 재미있었죠",
          "original_form": "맞아요 가끔 교회에서 그런 행사하면 재미있었죠",
          "speaker_id": "2",
          "time": "20210822 12:20"
        },
        {
          "id": "MDRW2100007922.1.10",
          "form": "그재미에 가는데",
          "original_form": "그재미에 가는데",
