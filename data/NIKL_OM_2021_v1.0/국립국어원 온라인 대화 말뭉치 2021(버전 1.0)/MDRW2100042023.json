      "utterance": [
        {
          "id": "MDRW2100042023.1.1",
          "form": "저녁 뭐 먹니ㅋㅋ",
          "original_form": "저녁 뭐 먹니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100042023.1.2",
          "form": "낮에 먹은 파스타 남은거 데워 먹을 듯 ㅋㅋ",
          "original_form": "낮에 먹은 파스타 남은거 데워 먹을 듯 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:50"
        },
        {
          "id": "MDRW2100042023.1.3",
          "form": "낮에 무슨파스타 했는데?",
          "original_form": "낮에 무슨파스타 했는데?",
          "speaker_id": "1",
          "time": "20210906 18:51"
        },
        {
          "id": "MDRW2100042023.1.4",
          "form": "토마토 베이컨 파스타",
          "original_form": "토마토 베이컨 파스타",
          "speaker_id": "2",
          "time": "20210906 19:02"
        },
        {
          "id": "MDRW2100042023.1.5",
          "form": "오 베이컨 좋다ㅋㅋ 난 제육볶음해서 먹었다ㅋ",
          "original_form": "오 베이컨 좋다ㅋㅋ 난 제육볶음해서 먹었다ㅋ",
          "speaker_id": "1",
          "time": "20210906 19:25"
        },
        {
          "id": "MDRW2100042023.1.6",
          "form": "상추 쌈 쌌니...",
          "original_form": "상추 쌈 쌌니...",
          "speaker_id": "2",
          "time": "20210906 19:25"
        },
        {
          "id": "MDRW2100042023.1.7",
          "form": "깻잎쌈 쌌다ㅋㅋㅋ 우리밭에 이제 상추가 없네ㅋㅋ",
          "original_form": "깻잎쌈 쌌다ㅋㅋㅋ 우리밭에 이제 상추가 없네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042023.1.8",
          "form": "사실 나도 깻잎파여 ㅋㅋㅋ",
          "original_form": "사실 나도 깻잎파여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:38"
        },
        {
          "id": "MDRW2100042023.1.9",
          "form": "깻잎 장아찌도 함께 했다ㅋㅋㅋ",
          "original_form": "깻잎 장아찌도 함께 했다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:42"
        },
        {
          "id": "MDRW2100042023.1.10",
          "form": "됴타됴타!",
          "original_form": "됴타됴타!",
          "speaker_id": "2",
          "time": "20210906 21:11"
        },
        {
          "id": "MDRW2100042023.1.11",
          "form": "낼아침엔  남은 제육에 김치 쪼사넣고 밥볶아 먹어야디ㅋㅋ",
          "original_form": "낼아침엔  남은 제육에 김치 쪼사넣고 밥볶아 먹어야디ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100042023.1.12",
          "form": "맛있겠군요..",
          "original_form": "맛있겠군요..",
          "speaker_id": "2",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100042023.1.13",
          "form": "요즘엔 먹는게 낙이다ㅋㅋ",
          "original_form": "요즘엔 먹는게 낙이다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042023.1.14",
          "form": "그치 나도 요즘 식욕 좀 올라올때 열심히 먹으련다 ㅋㅋㅋㅋ",
          "original_form": "그치 나도 요즘 식욕 좀 올라올때 열심히 먹으련다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042023.1.15",
          "form": "먹고싶은거 있을때 잘 먹어야지",
          "original_form": "먹고싶은거 있을때 잘 먹어야지",
          "speaker_id": "2",
          "time": "20210906 21:22"
        }
