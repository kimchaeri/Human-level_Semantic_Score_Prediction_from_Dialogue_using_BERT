        {
          "id": "MDRW2100046002.1.2",
          "form": "기막힌 칼국수집이 있지",
          "original_form": "기막힌 칼국수집이 있지",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100046002.1.3",
          "form": "어디에 있는데",
          "original_form": "어디에 있는데",
          "speaker_id": "2",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100046002.1.4",
          "form": "용산",
          "original_form": "용산",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100046002.1.5",
          "form": "무슨 칼국수야",
          "original_form": "무슨 칼국수야",
          "speaker_id": "2",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100046002.1.6",
          "form": "보말칼국수",
          "original_form": "보말칼국수",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100046002.1.7",
          "form": "먼 제주도에서 파는 거잖아",
          "original_form": "먼 제주도에서 파는 거잖아",
          "speaker_id": "2",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100046002.1.8",
          "form": "맞아 그런데 서울에도 있어",
          "original_form": "맞아 그런데 서울에도 있어",
          "speaker_id": "1",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100046002.1.9",
          "form": "이날 시에 따끈따끈 칼국수 딱 좋다",
          "original_form": "이날 시에 따끈따끈 칼국수 딱 좋다",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100046002.1.10",
          "form": "맞아 뜨끈한 거 좋아",
          "original_form": "맞아 뜨끈한 거 좋아",
          "speaker_id": "1",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100046002.1.11",
          "form": "나는 모든 칼국수를 좋아해",
          "original_form": "나는 모든 칼국수를 좋아해",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100046002.1.12",
          "form": "팥칼국수도 좋아하니",
          "original_form": "팥칼국수도 좋아하니",
          "speaker_id": "1",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100046002.1.13",
          "form": "그거는 안 먹어 봤어",
          "original_form": "그거는 안 먹어 봤어",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100046002.1.14",
          "form": "칼국수 좋아한다면",
          "original_form": "칼국수 좋아한다면",
          "speaker_id": "1",
          "time": "20210908 09:30"
