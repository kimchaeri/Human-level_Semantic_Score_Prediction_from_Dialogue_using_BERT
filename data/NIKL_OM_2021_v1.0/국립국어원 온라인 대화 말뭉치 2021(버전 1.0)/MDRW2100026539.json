      "utterance": [
        {
          "id": "MDRW2100026539.1.1",
          "form": "냉장고 파먹기ㅋ",
          "original_form": "냉장고 파먹기ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026539.1.2",
          "form": "절약 할때 좋아요 ㅋ",
          "original_form": "절약 할때 좋아요 ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026539.1.3",
          "form": "음쓰가 싫어서",
          "original_form": "음쓰가 싫어서",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026539.1.4",
          "form": "다먹는게 중요",
          "original_form": "다먹는게 중요",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026539.1.5",
          "form": "냉파 안함 다 음쓰 ㅋ",
          "original_form": "냉파 안함 다 음쓰 ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026539.1.6",
          "form": "오~ 정답이신듯^^",
          "original_form": "오~ 정답이신듯^^",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026539.1.7",
          "form": "근데 저도 열심히 한다고 하는데 그래도 가끔 나옴 ㅠㅠ",
          "original_form": "근데 저도 열심히 한다고 하는데 그래도 가끔 나옴 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026539.1.8",
          "form": "그정도는 적을듯하네요",
          "original_form": "그정도는 적을듯하네요",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026539.1.9",
          "form": "음 가끔은 많이더 나옴 ㅋ",
          "original_form": "음 가끔은 많이더 나옴 ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026539.1.10",
          "form": "음., 그럼 잡탕",
          "original_form": "음., 그럼 잡탕",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026539.1.11",
          "form": "ㅋㅋㅋㅋㅋ 잡다 항서 많을땐 카레가 짱",
          "original_form": "ㅋㅋㅋㅋㅋ 잡다 항서 많을땐 카레가 짱",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026539.1.12",
          "form": "카레는 설겆이가 ㅠ",
          "original_form": "카레는 설겆이가 ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026539.1.13",
          "form": "색이 다 배서",
          "original_form": "색이 다 배서",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026539.1.14",
          "form": "ㅎㅎㅎㅎ전괜찮던데",
          "original_form": "ㅎㅎㅎㅎ전괜찮던데",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026539.1.15",
          "form": "수세미도 버려야함ㅋ 초벌 설겆이 안하면ㅋ",
          "original_form": "수세미도 버려야함ㅋ 초벌 설겆이 안하면ㅋ",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026539.1.16",
          "form": "ㅡㅁ ㅡ//멜라민 그릇 사용하시나봐요",
          "original_form": "ㅡㅁ ㅡ//멜라민 그릇 사용하시나봐요",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026539.1.17",
          "form": "아 그런건 몰라서ㅋ",
          "original_form": "아 그런건 몰라서ㅋ",
