      "utterance": [
        {
          "id": "MDRW2100030916.1.1",
          "form": "혼자 할수있게 교웍시켜야지",
          "original_form": "혼자 할수있게 교웍시켜야지",
          "speaker_id": "2",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030916.1.2",
          "form": "입 크다고 구박하지 말고 양치를 깨끗이 시켜줘라",
          "original_form": "입 크다고 구박하지 말고 양치를 깨끗이 시켜줘라",
          "speaker_id": "1",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030916.1.3",
          "form": "가만있을땐 입이 작아 이쁜데 입벌림 넘 무셔 ㅅㄴ",
          "original_form": "가만있을땐 입이 작아 이쁜데 입벌림 넘 무셔 ㅅㄴ",
          "speaker_id": "2",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030916.1.4",
          "form": "며칠 동안 양치를 안 시켜준 거야",
          "original_form": "며칠 동안 양치를 안 시켜준 거야",
          "speaker_id": "1",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030916.1.5",
          "form": "오늘 삼일째 안시켰지 오늘 시켜야지 충치 안생긴게",
          "original_form": "오늘 삼일째 안시켰지 오늘 시켜야지 충치 안생긴게",
          "speaker_id": "2",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030916.1.6",
          "form": "오면서 눈곱빛도 사와",
          "original_form": "오면서 눈곱빛도 사와",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030916.1.7",
          "form": "눈곱빗",
          "original_form": "눈곱빗",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030916.1.8",
          "form": "강아지 눈꼽빗? 알써",
          "original_form": "강아지 눈꼽빗? 알써",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030916.1.9",
          "form": "새벽부터 얼굴 핥고 ㅠ",
          "original_form": "새벽부터 얼굴 핥고 ㅠ",
          "speaker_id": "2",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030916.1.10",
          "form": "나 낮잠 잘때도",
          "original_form": "나 낮잠 잘때도",
          "speaker_id": "1",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100030916.1.11",
          "form": "계속 뽀뽀하더라고",
          "original_form": "계속 뽀뽀하더라고",
          "speaker_id": "1",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100030916.1.12",
          "form": "그니까 심심하니 놀아달란거지",
          "original_form": "그니까 심심하니 놀아달란거지",
          "speaker_id": "2",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100030916.1.13",
          "form": "아까는 엉덩이",
          "original_form": "아까는 엉덩이",
          "speaker_id": "1",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100030916.1.14",
          "form": "썰매타더라",
          "original_form": "썰매타더라",
          "speaker_id": "1",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100030916.1.15",
          "form": "엉덩이 썰매",
          "original_form": "엉덩이 썰매",
          "speaker_id": "1",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100030916.1.16",
          "form": "항  문  낭 짜줄게",
          "original_form": "항  문  낭 짜줄게",
          "speaker_id": "2",
          "time": "20210904 14:21"
        },
        {
          "id": "MDRW2100030916.1.17",
          "form": "냄새가 고약해",
          "original_form": "냄새가 고약해",
          "speaker_id": "1",
          "time": "20210904 14:21"
        },
        {
          "id": "MDRW2100030916.1.18",
          "form": "맞아 생선 비린내 나 ㅠ",
          "original_form": "맞아 생선 비린내 나 ㅠ",
          "speaker_id": "2",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100030916.1.19",
          "form": "빨리 강아지 간식 만들어주지",
          "original_form": "빨리 강아지 간식 만들어주지",
