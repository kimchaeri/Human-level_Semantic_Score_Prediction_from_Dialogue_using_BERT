      "utterance": [
        {
          "id": "MDRW2100040518.1.1",
          "form": "주말에 가족이랑 뭐 하셨나요?",
          "original_form": "주말에 가족이랑 뭐 하셨나요?",
          "speaker_id": "1",
          "time": "20210904 22:12"
        },
        {
          "id": "MDRW2100040518.1.2",
          "form": "전 아직 미혼이고 ㅋㅋ 부모님이랑 살아요ㅋㅋ 아침 저녁 함께했어요",
          "original_form": "전 아직 미혼이고 ㅋㅋ 부모님이랑 살아요ㅋㅋ 아침 저녁 함께했어요",
          "speaker_id": "2",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100040518.1.3",
          "form": "좋겠네요 엄마가 해주는.밥 먹고~",
          "original_form": "좋겠네요 엄마가 해주는.밥 먹고~",
          "speaker_id": "1",
          "time": "20210904 22:28"
        },
        {
          "id": "MDRW2100040518.1.4",
          "form": "전 온종일 육아 했어요. 공포의 주말...",
          "original_form": "전 온종일 육아 했어요. 공포의 주말...",
          "speaker_id": "1",
          "time": "20210904 22:28"
        },
        {
          "id": "MDRW2100040518.1.5",
          "form": "아...ㅜㅠ",
          "original_form": "아...ㅜㅠ",
          "speaker_id": "2",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100040518.1.6",
          "form": "제 친구가 요즘 매일 전쟁",
          "original_form": "제 친구가 요즘 매일 전쟁",
          "speaker_id": "2",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100040518.1.7",
          "form": "몇살이에요?",
          "original_form": "몇살이에요?",
          "speaker_id": "2",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100040518.1.8",
          "form": "친구가 육아중이군요",
          "original_form": "친구가 육아중이군요",
          "speaker_id": "1",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100040518.1.9",
          "form": "전 5,7살이요",
          "original_form": "전 5,7살이요",
          "speaker_id": "1",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100040518.1.10",
          "form": "그나마 좀 키워서 신생아때보단 나아요",
          "original_form": "그나마 좀 키워서 신생아때보단 나아요",
          "speaker_id": "1",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100040518.1.11",
          "form": "제 친구 4살인데 요즘 진짜 전쟁...",
          "original_form": "제 친구 4살인데 요즘 진짜 전쟁...",
          "speaker_id": "2",
          "time": "20210904 22:31"
        },
        {
          "id": "MDRW2100040518.1.12",
          "form": "고민이 많은데 누구한테 말도 못하고",
          "original_form": "고민이 많은데 누구한테 말도 못하고",
          "speaker_id": "2",
          "time": "20210904 22:32"
        },
        {
          "id": "MDRW2100040518.1.13",
          "form": "5살 되면 애가 갑자기 사람이 되긴 해요 ㅠ",
          "original_form": "5살 되면 애가 갑자기 사람이 되긴 해요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100040518.1.14",
          "form": "4살이면 아직 36개월이 안됐을수도 있고.. 36개월 미만은 아직 사람이 아니거든요 ㅋㅋㅋ",
          "original_form": "4살이면 아직 36개월이 안됐을수도 있고.. 36개월 미만은 아직 사람이 아니거든요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:36"
        },
        {
          "id": "MDRW2100040518.1.15",
          "form": "요즘 맨날 소리지르고 화낸대요 이유없이",
          "original_form": "요즘 맨날 소리지르고 화낸대요 이유없이",
          "speaker_id": "2",
          "time": "20210904 22:36"
        },
        {
          "id": "MDRW2100040518.1.16",
          "form": "그래서 상담을 받아야하나 예약하더라고요",
          "original_form": "그래서 상담을 받아야하나 예약하더라고요",
          "speaker_id": "2",
          "time": "20210904 22:36"
        },
        {
          "id": "MDRW2100040518.1.17",
          "form": "앗.. 엄청 고생이네요 ㅠ 우리집도 맨날 둘이 싸우는데요 ㅠ",
          "original_form": "앗.. 엄청 고생이네요 ㅠ 우리집도 맨날 둘이 싸우는데요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100040518.1.18",
          "form": "때리기도 하나요?ㅜ",
          "original_form": "때리기도 하나요?ㅜ",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040518.1.19",
          "form": "둘이 서로 때리죠 ㅠ",
          "original_form": "둘이 서로 때리죠 ㅠ",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040518.1.20",
          "form": "그래서 공포의 주말입니다.. 둘이 몇번을 싸워야 하루가 가련지...",
          "original_form": "그래서 공포의 주말입니다.. 둘이 몇번을 싸워야 하루가 가련지...",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040518.1.21",
          "form": "친구딸인데 어린이집에서 친구때린다고ㅜㅠ 이게 심각한거라고 해서 ㅜㅠ",
          "original_form": "친구딸인데 어린이집에서 친구때린다고ㅜㅠ 이게 심각한거라고 해서 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100040518.1.22",
          "form": "진짜 육아 너무 어려운거 같아요",
          "original_form": "진짜 육아 너무 어려운거 같아요",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100040518.1.23",
          "form": "딸이라고요? 딸은 보통 안그러거든요 ㅠ",
          "original_form": "딸이라고요? 딸은 보통 안그러거든요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 22:46"
        },
        {
          "id": "MDRW2100040518.1.24",
          "form": "저희집은 아들 둘이라 ㅠ",
          "original_form": "저희집은 아들 둘이라 ㅠ",
          "speaker_id": "1",
          "time": "20210904 22:46"
        }
      ]
    }
  ]
