        },
        {
          "id": "MDRW2100001046.1.3",
          "form": "엄청 좋아해!!",
          "original_form": "엄청 좋아해!!",
          "speaker_id": "2",
          "time": "20210722 16:35"
        },
        {
          "id": "MDRW2100001046.1.4",
          "form": "키우고 싶어..",
          "original_form": "키우고 싶어..",
          "speaker_id": "2",
          "time": "20210722 16:35"
        },
        {
          "id": "MDRW2100001046.1.5",
          "form": "뭘 젤키우고싶어?",
          "original_form": "뭘 젤키우고싶어?",
          "speaker_id": "1",
          "time": "20210722 16:36"
        },
        {
          "id": "MDRW2100001046.1.6",
          "form": "난 강아지",
          "original_form": "난 강아지",
          "speaker_id": "2",
          "time": "20210722 16:36"
        },
        {
          "id": "MDRW2100001046.1.7",
          "form": "난 강아지 키우고있어!",
          "original_form": "난 강아지 키우고있어!",
          "speaker_id": "1",
          "time": "20210722 16:37"
        },
        {
          "id": "MDRW2100001046.1.8",
