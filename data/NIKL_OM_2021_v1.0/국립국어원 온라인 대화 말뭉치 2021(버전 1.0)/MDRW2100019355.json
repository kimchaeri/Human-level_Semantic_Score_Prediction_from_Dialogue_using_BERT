      "utterance": [
        {
          "id": "MDRW2100019355.1.1",
          "form": "저녁 먹는중",
          "original_form": "저녁 먹는중",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019355.1.2",
          "form": "저녁시간이네 저녁 먹었나",
          "original_form": "저녁시간이네 저녁 먹었나",
          "speaker_id": "1",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019355.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019355.1.4",
          "form": "오 마침 치니까 ㅋㅋ",
          "original_form": "오 마침 치니까 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019355.1.5",
          "form": "나 방금 먹고옴",
          "original_form": "나 방금 먹고옴",
          "speaker_id": "1",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019355.1.6",
          "form": "뭐 먹었어?",
          "original_form": "뭐 먹었어?",
          "speaker_id": "1",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019355.1.7",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019355.1.8",
          "form": "빠르게 먹었네",
          "original_form": "빠르게 먹었네",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019355.1.9",
          "form": "나 짜파게티 먹는중",
          "original_form": "나 짜파게티 먹는중",
          "speaker_id": "2",
          "time": "20210827 18:15"
        },
        {
          "id": "MDRW2100019355.1.10",
          "form": "컨디션 별로라 후딱 먹는중",
          "original_form": "컨디션 별로라 후딱 먹는중",
          "speaker_id": "2",
          "time": "20210827 18:15"
        },
        {
          "id": "MDRW2100019355.1.11",
          "form": "오 짜파게티 맛있겠다 나도 며칠전에 먹었어",
          "original_form": "오 짜파게티 맛있겠다 나도 며칠전에 먹었어",
          "speaker_id": "1",
          "time": "20210827 18:16"
        },
        {
          "id": "MDRW2100019355.1.12",
          "form": "난 디기 오랜만",
          "original_form": "난 디기 오랜만",
          "speaker_id": "2",
          "time": "20210827 18:16"
        },
        {
          "id": "MDRW2100019355.1.13",
          "form": "ㅇㅇ 이상하게 잘 안먹어짐 ㅋㅋ",
          "original_form": "ㅇㅇ 이상하게 잘 안먹어짐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:17"
        },
        {
          "id": "MDRW2100019355.1.14",
          "form": "한번씩 먹음 맛나",
          "original_form": "한번씩 먹음 맛나",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019355.1.15",
          "form": "특히 파김치랑 맛있는데\r\n파김지가 나왔는데 너무 익어서 맛이 요상해",
          "original_form": "특히 파김치랑 맛있는데\r\n파김지가 나왔는데 너무 익어서 맛이 요상해",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019355.1.16",
          "form": "ㅇㅇ맛있엉 양파 많이 볶아서 넣음 맛있더라",
          "original_form": "ㅇㅇ맛있엉 양파 많이 볶아서 넣음 맛있더라",
          "speaker_id": "1",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019355.1.17",
          "form": "그대로 안끓이고 양파 볶아?",
          "original_form": "그대로 안끓이고 양파 볶아?",
          "speaker_id": "2",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100019355.1.18",
          "form": "ㄴㄴ",
          "original_form": "ㄴㄴ",
          "speaker_id": "1",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100019355.1.19",
          "form": "끓이고 따로",
          "original_form": "끓이고 따로",
          "speaker_id": "1",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100019355.1.20",
          "form": "양파볶은뒤 소스넣어 볶아",
          "original_form": "양파볶은뒤 소스넣어 볶아",
          "speaker_id": "1",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100019355.1.21",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100019355.1.22",
          "form": "안먹어봤어",
          "original_form": "안먹어봤어",
          "speaker_id": "2",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100019355.1.23",
          "form": "간짜장느끼민가ㅎ",
          "original_form": "간짜장느끼민가ㅎ",
          "speaker_id": "2",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100019355.1.24",
          "form": "느낌인가",
          "original_form": "느낌인가",
          "speaker_id": "2",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100019355.1.25",
          "form": "고기넣음 더 맛있는갑더라",
          "original_form": "고기넣음 더 맛있는갑더라",
          "speaker_id": "1",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100019355.1.26",
          "form": "맛이 더 풍부해지는 느낌",
          "original_form": "맛이 더 풍부해지는 느낌",
          "speaker_id": "1",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100019355.1.27",
          "form": "원재료가 드가니 그렇겠다ㅎ",
          "original_form": "원재료가 드가니 그렇겠다ㅎ",
          "speaker_id": "2",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100019355.1.28",
          "form": "ㅇㅇ 난 고기는 안넣어봤지만",
          "original_form": "ㅇㅇ 난 고기는 안넣어봤지만",
          "speaker_id": "1",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100019355.1.29",
          "form": "한번 먹어보고싶긴한데\r\n언젠가 먹어볼께~",
          "original_form": "한번 먹어보고싶긴한데\r\n언젠가 먹어볼께~",
          "speaker_id": "2",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019355.1.30",
          "form": "ㅇㅇ집에갔을때 먹어봐~~",
          "original_form": "ㅇㅇ집에갔을때 먹어봐~~",
          "speaker_id": "1",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019355.1.31",
          "form": "요리하기 귀찮ㅎ\r\n막상 가면 대충 먹더라고ㅋㄱㄲ",
          "original_form": "요리하기 귀찮ㅎ\r\n막상 가면 대충 먹더라고ㅋㄱㄲ",
          "speaker_id": "2",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019355.1.32",
          "form": "제출할게",
          "original_form": "제출할게",
          "speaker_id": "1",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019355.1.33",
          "form": "저녁 만두 먹었어!!",
          "original_form": "저녁 만두 먹었어!!",
