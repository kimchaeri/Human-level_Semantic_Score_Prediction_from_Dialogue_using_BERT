      "utterance": [
        {
          "id": "MDRW2100025776.1.1",
          "form": "음악 어플 뭐 쓰세요?^^",
          "original_form": "음악 어플 뭐 쓰세요?^^",
          "speaker_id": "1",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100025776.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100025776.1.3",
          "form": "플로씁니다",
          "original_form": "플로씁니다",
          "speaker_id": "2",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100025776.1.4",
          "form": "오 저도 플로 쓰다 갈아탔어요 ㅋㅋ 유투브 뮤직으로요",
          "original_form": "오 저도 플로 쓰다 갈아탔어요 ㅋㅋ 유투브 뮤직으로요",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025776.1.5",
          "form": "오 좋나요?",
          "original_form": "오 좋나요?",
          "speaker_id": "2",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100025776.1.6",
          "form": "네~ 관련곡 잘 찾아주고 ㅋㅋ 유투브라이브영상 고대로 노래로만 들을수도 있고.. 만족하고 있어요",
          "original_form": "네~ 관련곡 잘 찾아주고 ㅋㅋ 유투브라이브영상 고대로 노래로만 들을수도 있고.. 만족하고 있어요",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100025776.1.7",
          "form": "오 대박이네요",
          "original_form": "오 대박이네요",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025776.1.8",
          "form": "외국곡을 많이 들어서 유툽이 더 좋은거같더라구요",
          "original_form": "외국곡을 많이 들어서 유툽이 더 좋은거같더라구요",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025776.1.9",
          "form": "가격은 얼마에 사용하시나요?",
          "original_form": "가격은 얼마에 사용하시나요?",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025776.1.10",
          "form": "오~ 외국곡 많이 들으시구나",
          "original_form": "오~ 외국곡 많이 들으시구나",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100025776.1.11",
          "form": "전 유투브 프리미엄 끊어서 유투브+유투브 뮤직까지 한번에 써요 ㅎㅎ",
          "original_form": "전 유투브 프리미엄 끊어서 유투브+유투브 뮤직까지 한번에 써요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025776.1.12",
          "form": "오오 좋네요",
          "original_form": "오오 좋네요",
          "speaker_id": "2",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025776.1.13",
          "form": "다른 ott는 구독 하시나요?",
          "original_form": "다른 ott는 구독 하시나요?",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025776.1.14",
          "form": "네 유투브 말고는 딱히... 근데 넷플은 간혹 끊어서 봐요 ㅋㅋㅋ 님은요",
          "original_form": "네 유투브 말고는 딱히... 근데 넷플은 간혹 끊어서 봐요 ㅋㅋㅋ 님은요",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100025776.1.15",
          "form": "저는 왓챠랑 웨이브요!",
          "original_form": "저는 왓챠랑 웨이브요!",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025776.1.16",
          "form": "오... 두개 뭐가 다른가요?!",
          "original_form": "오... 두개 뭐가 다른가요?!",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025776.1.17",
          "form": "거의 비슷한데",
          "original_form": "거의 비슷한데",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025776.1.18",
          "form": "왓챠가 영화보기에 더 좋은거같아요",
          "original_form": "왓챠가 영화보기에 더 좋은거같아요",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025776.1.19",
          "form": "으음~ 왓챠는 영화 ㅎㅎㅎ 기억해둬야겠어요 \r\n워낙 많은데 장단점을 모르겠더라고요",
          "original_form": "으음~ 왓챠는 영화 ㅎㅎㅎ 기억해둬야겠어요 \r\n워낙 많은데 장단점을 모르겠더라고요",
          "speaker_id": "1",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100025776.1.20",
          "form": "근데 영상 보는 거 좋아하는 사람들은 넷플 왓챠 웨이브 티빙 한꺼번에 다 해두더라고요 ㅋㅋㅋㅋ 신기!!",
          "original_form": "근데 영상 보는 거 좋아하는 사람들은 넷플 왓챠 웨이브 티빙 한꺼번에 다 해두더라고요 ㅋㅋㅋㅋ 신기!!",
          "speaker_id": "1",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100025776.1.21",
          "form": "우와ㅋㅋㅋㅋ 시간이 부족하겠어요ㅋㅋ",
          "original_form": "우와ㅋㅋㅋㅋ 시간이 부족하겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:23"
        },
        {
          "id": "MDRW2100025776.1.22",
          "form": "미드 좋아하시면 그레이아나토미 추천요!",
          "original_form": "미드 좋아하시면 그레이아나토미 추천요!",
          "speaker_id": "1",
          "time": "20210901 19:33"
        }
      ]
