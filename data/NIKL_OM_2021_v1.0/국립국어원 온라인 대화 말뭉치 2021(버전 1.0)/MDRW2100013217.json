      "utterance": [
        {
          "id": "MDRW2100013217.1.1",
          "form": "name2은 종교가 있으신가요?",
          "original_form": "&name2&은 종교가 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013217.1.2",
          "form": "기독교예요!",
          "original_form": "기독교예요!",
          "speaker_id": "2",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100013217.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100013217.1.4",
          "form": "저는 천주교요!",
          "original_form": "저는 천주교요!",
          "speaker_id": "1",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013217.1.5",
          "form": "기독교에서 연례적으로 하는 활동이나 모임있으세요?",
          "original_form": "기독교에서 연례적으로 하는 활동이나 모임있으세요?",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100013217.1.6",
          "form": "모임은 딱히 참석해본적 없어요 !",
          "original_form": "모임은 딱히 참석해본적 없어요 !",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100013217.1.7",
          "form": "성단은 모임 같은 게 다양한 편인가요?",
          "original_form": "성단은 모임 같은 게 다양한 편인가요?",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100013217.1.8",
          "form": "저는 모태신앙이라 어릴때부터 다녀서",
          "original_form": "저는 모태신앙이라 어릴때부터 다녀서",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013217.1.9",
          "form": "초등부나 중고등부 교리를 했었는데",
          "original_form": "초등부나 중고등부 교리를 했었는데",
          "speaker_id": "1",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100013217.1.10",
          "form": "거기서 같이 캠프가거나 했었던 것같아요",
          "original_form": "거기서 같이 캠프가거나 했었던 것같아요",
          "speaker_id": "1",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100013217.1.11",
          "form": "학생회도 해보구요",
          "original_form": "학생회도 해보구요",
          "speaker_id": "1",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100013217.1.12",
          "form": "오오 좋은 것 같아요!",
          "original_form": "오오 좋은 것 같아요!",
          "speaker_id": "2",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100013217.1.13",
          "form": "잉? 가운데 저게 뭘까요 ㅠㅠㅠ 이상한 거 안 보냈어요 ㅠㅠㅠㅠ",
          "original_form": "잉? 가운데 저게 뭘까요 ㅠㅠㅠ 이상한 거 안 보냈어요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013217.1.14",
          "form": "조금만 용어를 잘못 보내도 부적절한 단어라고 제재하더라구요ㅎㅎ",
          "original_form": "조금만 용어를 잘못 보내도 부적절한 단어라고 제재하더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013217.1.15",
          "form": "저한텐 안보여요!",
          "original_form": "저한텐 안보여요!",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013217.1.16",
          "form": "다행이에요 ㅠㅠㅠ 깜짝놀랐어요 ㅠㅠㅠ",
          "original_form": "다행이에요 ㅠㅠㅠ 깜짝놀랐어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013217.1.17",
          "form": "저도 부모님 따라 가진 종교라 유치부 때까지는 막 달란트 행사 이런 거 해본 적 있는데",
          "original_form": "저도 부모님 따라 가진 종교라 유치부 때까지는 막 달란트 행사 이런 거 해본 적 있는데",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013217.1.18",
          "form": "성당도 비슷한가요?",
          "original_form": "성당도 비슷한가요?",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013217.1.19",
          "form": "초등학생때부터는 따로 초등부 예배에 안 나가서 기억이 흐릿하긴 한데 가끔씩 달란트로 바꿔 먹던 떡볶이가 생각나더라고요  ㅎㅎ",
          "original_form": "초등학생때부터는 따로 초등부 예배에 안 나가서 기억이 흐릿하긴 한데 가끔씩 달란트로 바꿔 먹던 떡볶이가 생각나더라고요  ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013217.1.20",
          "form": "아아 기독교 친구한테 들어봤던 것같아요!",
          "original_form": "아아 기독교 친구한테 들어봤던 것같아요!",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013217.1.21",
          "form": "저희는 캠프 다같이 가거나 학생회하고 이랬어요",
          "original_form": "저희는 캠프 다같이 가거나 학생회하고 이랬어요",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013217.1.22",
          "form": "학생회에선  어떤 활동을 하나요?",
          "original_form": "학생회에선  어떤 활동을 하나요?",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013217.1.23",
          "form": "중고등부 성당 행사 주도가 주인 것같아요 각종 부서 부장도 해서 이끌구요",
          "original_form": "중고등부 성당 행사 주도가 주인 것같아요 각종 부서 부장도 해서 이끌구요",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100013217.1.24",
          "form": "직접 행사를 주도한다는 게 좋네요!",
          "original_form": "직접 행사를 주도한다는 게 좋네요!",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100013217.1.25",
          "form": "그쵸 그만큼 책임감도 느껴서 쉽지만은 않았어요ㅋㅋㅋ",
          "original_form": "그쵸 그만큼 책임감도 느껴서 쉽지만은 않았어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:01"
        }
      ]
