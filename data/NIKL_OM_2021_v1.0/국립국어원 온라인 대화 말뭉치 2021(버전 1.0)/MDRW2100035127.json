      "utterance": [
        {
          "id": "MDRW2100035127.1.1",
          "form": "저도 좀 그래요 ㅋㅋ",
          "original_form": "저도 좀 그래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:34"
        },
        {
          "id": "MDRW2100035127.1.2",
          "form": "고백부부 추천드려용",
          "original_form": "고백부부 추천드려용",
          "speaker_id": "1",
          "time": "20210829 03:34"
        },
        {
          "id": "MDRW2100035127.1.3",
          "form": "보다말았는데",
          "original_form": "보다말았는데",
          "speaker_id": "2",
          "time": "20210829 03:34"
        },
        {
          "id": "MDRW2100035127.1.4",
          "form": "결말 후기 찾아보고 결말이 별로다 하면 시도도 안해요ㅋㅋ",
          "original_form": "결말 후기 찾아보고 결말이 별로다 하면 시도도 안해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:34"
        },
        {
          "id": "MDRW2100035127.1.5",
          "form": "이거 장나라랑 손호준이랑 주연인 드라만데",
          "original_form": "이거 장나라랑 손호준이랑 주연인 드라만데",
          "speaker_id": "1",
          "time": "20210829 03:34"
        },
        {
          "id": "MDRW2100035127.1.6",
          "form": "둘이 현실에선 부부였다가 갑자기 백투더 대학생활로 시간이동해서",
          "original_form": "둘이 현실에선 부부였다가 갑자기 백투더 대학생활로 시간이동해서",
          "speaker_id": "1",
          "time": "20210829 03:34"
        },
        {
          "id": "MDRW2100035127.1.7",
          "form": "저도 2화인가",
          "original_form": "저도 2화인가",
          "speaker_id": "2",
          "time": "20210829 03:35"
        },
        {
          "id": "MDRW2100035127.1.8",
          "form": "까지봤는데!",
          "original_form": "까지봤는데!",
          "speaker_id": "2",
          "time": "20210829 03:35"
        },
        {
          "id": "MDRW2100035127.1.9",
          "form": "싱글일때로 돌아가는 드라마에용",
          "original_form": "싱글일때로 돌아가는 드라마에용",
          "speaker_id": "1",
          "time": "20210829 03:35"
        },
        {
          "id": "MDRW2100035127.1.10",
          "form": "결말이 넘 맘에 들더라구요",
          "original_form": "결말이 넘 맘에 들더라구요",
          "speaker_id": "1",
          "time": "20210829 03:35"
        },
        {
          "id": "MDRW2100035127.1.11",
          "form": "그리고 장나라 엄마 나오는 장면이 진짜 슬펐어요ㅠㅠ",
          "original_form": "그리고 장나라 엄마 나오는 장면이 진짜 슬펐어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 03:35"
        },
        {
          "id": "MDRW2100035127.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210829 03:35"
        },
        {
          "id": "MDRW2100035127.1.13",
          "form": "저좀더 봣나봐요ㅠㅠ",
          "original_form": "저좀더 봣나봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:35"
        },
        {
          "id": "MDRW2100035127.1.14",
          "form": "아아 보셨죵?ㅋㅋㅋ",
          "original_form": "아아 보셨죵?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:35"
        },
        {
          "id": "MDRW2100035127.1.15",
          "form": "고딩때도 가서 엄마랑만나서 막 울고 이런거",
          "original_form": "고딩때도 가서 엄마랑만나서 막 울고 이런거",
          "speaker_id": "2",
          "time": "20210829 03:35"
        },
        {
          "id": "MDRW2100035127.1.16",
          "form": "기억이 새록새록 나시나보당",
          "original_form": "기억이 새록새록 나시나보당",
          "speaker_id": "1",
          "time": "20210829 03:36"
        },
        {
          "id": "MDRW2100035127.1.17",
          "form": "기억나여ㅠㅠ",
          "original_form": "기억나여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:36"
        },
        {
          "id": "MDRW2100035127.1.18",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 03:36"
        },
        {
          "id": "MDRW2100035127.1.19",
          "form": "그나마 최근 작품은",
          "original_form": "그나마 최근 작품은",
          "speaker_id": "1",
          "time": "20210829 03:36"
        },
        {
          "id": "MDRW2100035127.1.20",
          "form": "마우스 봤는데",
          "original_form": "마우스 봤는데",
          "speaker_id": "1",
          "time": "20210829 03:36"
        },
        {
          "id": "MDRW2100035127.1.21",
          "form": "전 좀 감정으로 절 괴롭히는걸 싫어하긴해여ㅠ ㅅ",
          "original_form": "전 좀 감정으로 절 괴롭히는걸 싫어하긴해여ㅠ ㅅ",
          "speaker_id": "2",
          "time": "20210829 03:36"
        },
        {
          "id": "MDRW2100035127.1.22",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210829 03:36"
        },
        {
          "id": "MDRW2100035127.1.23",
          "form": "그럼 이 드라마 보지마세여ㅠㅠ",
          "original_form": "그럼 이 드라마 보지마세여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 03:36"
        },
        {
          "id": "MDRW2100035127.1.24",
          "form": "사람들이 슬프자하는건 좀피하능편 ㅠㅠ",
          "original_form": "사람들이 슬프자하는건 좀피하능편 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:36"
        },
        {
          "id": "MDRW2100035127.1.25",
          "form": "완전 기빨려요",
          "original_form": "완전 기빨려요",
          "speaker_id": "1",
          "time": "20210829 03:36"
        },
        {
          "id": "MDRW2100035127.1.26",
          "form": "아ㅠㅠ 마음이 여리신가봐용",
          "original_form": "아ㅠㅠ 마음이 여리신가봐용",
          "speaker_id": "1",
          "time": "20210829 03:36"
        }
      ]
    }
