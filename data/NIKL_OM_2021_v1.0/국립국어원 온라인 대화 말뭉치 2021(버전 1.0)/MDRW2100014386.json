      "utterance": [
        {
          "id": "MDRW2100014386.1.1",
          "form": "나도봤어",
          "original_form": "나도봤어",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014386.1.2",
          "form": "롯시에서",
          "original_form": "롯시에서",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014386.1.3",
          "form": "처넌딜",
          "original_form": "처넌딜",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014386.1.4",
          "form": "라이언 레이놀즈 연기 잘하더라",
          "original_form": "라이언 레이놀즈 연기 잘하더라",
          "speaker_id": "1",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014386.1.5",
          "form": "나는 놓쳐서 다주고봤어",
          "original_form": "나는 놓쳐서 다주고봤어",
          "speaker_id": "1",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014386.1.6",
          "form": "진짜 웃겨 라이언좋아",
          "original_form": "진짜 웃겨 라이언좋아",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014386.1.7",
          "form": "자연스러운 연기가 아주 좋았어",
          "original_form": "자연스러운 연기가 아주 좋았어",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014386.1.8",
          "form": "영화 주제도 신선하고",
          "original_form": "영화 주제도 신선하고",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014386.1.9",
          "form": "ㅋㅋㅋ그런연기잘해",
          "original_form": "ㅋㅋㅋ그런연기잘해",
          "speaker_id": "2",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014386.1.10",
          "form": "약간 가볍게 보기 좋았어",
          "original_form": "약간 가볍게 보기 좋았어",
          "speaker_id": "2",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014386.1.11",
          "form": "데드풀같은 게 진짜 잘어울려",
          "original_form": "데드풀같은 게 진짜 잘어울려",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014386.1.12",
          "form": "맞아 불편하지도않고",
          "original_form": "맞아 불편하지도않고",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014386.1.13",
          "form": "프로포즈봐쥬새오",
          "original_form": "프로포즈봐쥬새오",
          "speaker_id": "2",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014386.1.14",
          "form": "로맨틱코미디인데 재밌다구",
          "original_form": "로맨틱코미디인데 재밌다구",
          "speaker_id": "2",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014386.1.15",
          "form": "그런거 안보는데",
          "original_form": "그런거 안보는데",
          "speaker_id": "1",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014386.1.16",
          "form": "로코는 스파이같은거만 좋어",
          "original_form": "로코는 스파이같은거만 좋어",
          "speaker_id": "1",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014386.1.17",
          "form": "재밌는데 흑..",
          "original_form": "재밌는데 흑..",
          "speaker_id": "2",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014386.1.18",
          "form": "나를차버린스파이",
          "original_form": "나를차버린스파이",
          "speaker_id": "1",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014386.1.19",
          "form": "그것도 병맛이라 괜찮아",
          "original_form": "그것도 병맛이라 괜찮아",
          "speaker_id": "2",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014386.1.20",
          "form": "코미디가 더커",
          "original_form": "코미디가 더커",
          "speaker_id": "2",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014386.1.21",
          "form": "요즘은 그런 영화가 좋더라",
          "original_form": "요즘은 그런 영화가 좋더라",
          "speaker_id": "1",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014386.1.22",
          "form": "안힘든 거",
          "original_form": "안힘든 거",
          "speaker_id": "1",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014386.1.23",
          "form": "ㅋㅋㅋㅋㅋ가볍게 보는게조아",
          "original_form": "ㅋㅋㅋㅋㅋ가볍게 보는게조아",
          "speaker_id": "2",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014386.1.24",
          "form": "인생이 팍팍해서그렇죠",
          "original_form": "인생이 팍팍해서그렇죠",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014386.1.25",
          "form": "맞아 무거운 건 힘들어",
          "original_form": "맞아 무거운 건 힘들어",
          "speaker_id": "1",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014386.1.26",
          "form": "갑자기?",
          "original_form": "갑자기?",
          "speaker_id": "1",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014386.1.27",
          "form": "콘서트도 가고싶어",
          "original_form": "콘서트도 가고싶어",
          "speaker_id": "2",
          "time": "20210825 18:47"
        }
      ]
    }
  ]
