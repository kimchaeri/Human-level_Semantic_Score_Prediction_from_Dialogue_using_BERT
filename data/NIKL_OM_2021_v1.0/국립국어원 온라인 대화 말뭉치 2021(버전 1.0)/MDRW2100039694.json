      "utterance": [
        {
          "id": "MDRW2100039694.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100039694.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039694.1.3",
          "form": "요새 잘 드시는 음식 있으신가요?",
          "original_form": "요새 잘 드시는 음식 있으신가요?",
          "speaker_id": "2",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039694.1.4",
          "form": "음 요즘 닭강정이요",
          "original_form": "음 요즘 닭강정이요",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039694.1.5",
          "form": "저녁드셨어요?",
          "original_form": "저녁드셨어요?",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039694.1.6",
          "form": "지금 먹고있어요!",
          "original_form": "지금 먹고있어요!",
          "speaker_id": "2",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100039694.1.7",
          "form": "저는 닭가슴살 스테이크  먹고있어요",
          "original_form": "저는 닭가슴살 스테이크  먹고있어요",
          "speaker_id": "2",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100039694.1.8",
          "form": "다이어트 한답시고ㅎㅎ",
          "original_form": "다이어트 한답시고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100039694.1.9",
          "form": "닭강정 너무 맛있겠어요!",
          "original_form": "닭강정 너무 맛있겠어요!",
          "speaker_id": "2",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100039694.1.10",
          "form": "보통 어디서 사드시나요?",
          "original_form": "보통 어디서 사드시나요?",
          "speaker_id": "2",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100039694.1.11",
          "form": "와 다이어트 너무 힘드시겠어요ㅜ",
          "original_form": "와 다이어트 너무 힘드시겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100039694.1.12",
          "form": "저는 가마로요",
          "original_form": "저는 가마로요",
          "speaker_id": "1",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100039694.1.13",
          "form": "닭가슴살 스테이크  맛 괜찮나요?",
          "original_form": "닭가슴살 스테이크  맛 괜찮나요?",
          "speaker_id": "1",
          "time": "20210903 20:49"
        },
        {
          "id": "MDRW2100039694.1.14",
          "form": "밍밍해서 허브솔트 잔뜩 뿌려먹고 있어요ㅋㅋㅋ",
          "original_form": "밍밍해서 허브솔트 잔뜩 뿌려먹고 있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100039694.1.15",
          "form": "가마로 너무 맛있겠어요 메뉴가 여러가지 있나요?",
          "original_form": "가마로 너무 맛있겠어요 메뉴가 여러가지 있나요?",
          "speaker_id": "2",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100039694.1.16",
          "form": "엄청 많진않은데",
          "original_form": "엄청 많진않은데",
          "speaker_id": "1",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100039694.1.17",
          "form": "맛이 좋아요",
          "original_form": "맛이 좋아요",
          "speaker_id": "1",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100039694.1.18",
          "form": "바삭바삭하구요",
          "original_form": "바삭바삭하구요",
          "speaker_id": "1",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100039694.1.19",
          "form": "가마로 추천입니다",
          "original_form": "가마로 추천입니다",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039694.1.20",
          "form": "바삭바삭",
          "original_form": "바삭바삭",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039694.1.21",
          "form": "중요하죠",
          "original_form": "중요하죠",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039694.1.22",
          "form": "그쵸 집앞에도 닭강정집 있는데",
          "original_form": "그쵸 집앞에도 닭강정집 있는데",
          "speaker_id": "1",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039694.1.23",
          "form": "강정 양념을 묻히고도 바삭함을 잃지 않다니",
          "original_form": "강정 양념을 묻히고도 바삭함을 잃지 않다니",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039694.1.24",
          "form": "덜 바삭해서 아쉬워요",
          "original_form": "덜 바삭해서 아쉬워요",
          "speaker_id": "1",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039694.1.25",
          "form": "닭강정의 귀감이네요",
          "original_form": "닭강정의 귀감이네요",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039694.1.26",
          "form": "네네 다음에 한번 드셔보세요",
          "original_form": "네네 다음에 한번 드셔보세요",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039694.1.27",
          "form": "닭강정이 만들기 참 어려운 음식인 것 같아오ㅡ",
          "original_form": "닭강정이 만들기 참 어려운 음식인 것 같아오ㅡ",
          "speaker_id": "2",
          "time": "20210903 20:54"
        }
      ]
