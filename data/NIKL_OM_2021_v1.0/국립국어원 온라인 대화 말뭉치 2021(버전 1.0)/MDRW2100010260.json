      "utterance": [
        {
          "id": "MDRW2100010260.1.1",
          "form": "어떤음식 좋아하시나요?",
          "original_form": "어떤음식 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010260.1.2",
          "form": "저는 빵종류도 다 좋아해요ㅎㅎ 뭐좋아하세요?",
          "original_form": "저는 빵종류도 다 좋아해요ㅎㅎ 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 16:12"
        },
        {
          "id": "MDRW2100010260.1.3",
          "form": "저 날것 좋아해요ㅋㅋㅋ",
          "original_form": "저 날것 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100010260.1.4",
          "form": "회종류요",
          "original_form": "회종류요",
          "speaker_id": "1",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100010260.1.5",
          "form": "아 저도 좋아해요 초밥도 그렇구ㅎㅎ",
          "original_form": "아 저도 좋아해요 초밥도 그렇구ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100010260.1.6",
          "form": "전 육회도 좋아해요ㅋㅋㅋ",
          "original_form": "전 육회도 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100010260.1.7",
          "form": "너무맛있어요",
          "original_form": "너무맛있어요",
          "speaker_id": "1",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100010260.1.8",
          "form": "아 저도 좋아해요ㅠㅠㅠ진짜 많이먹는..",
          "original_form": "아 저도 좋아해요ㅠㅠㅠ진짜 많이먹는..",
          "speaker_id": "2",
          "time": "20210823 16:16"
        },
        {
          "id": "MDRW2100010260.1.9",
          "form": "육사시미도",
          "original_form": "육사시미도",
          "speaker_id": "1",
          "time": "20210823 16:18"
        },
        {
          "id": "MDRW2100010260.1.10",
          "form": "드시나요?",
          "original_form": "드시나요?",
          "speaker_id": "1",
          "time": "20210823 16:18"
        },
        {
          "id": "MDRW2100010260.1.11",
          "form": "네ㅎㅎㅎ처음엔 무슨차인가 했는데 진짜 다르더라구요",
          "original_form": "네ㅎㅎㅎ처음엔 무슨차인가 했는데 진짜 다르더라구요",
          "speaker_id": "2",
          "time": "20210823 16:18"
        },
        {
          "id": "MDRW2100010260.1.12",
          "form": "맞아요ㅋㅋㅋ 아 회먹고싶네요",
          "original_form": "맞아요ㅋㅋㅋ 아 회먹고싶네요",
          "speaker_id": "1",
          "time": "20210823 16:19"
        },
        {
          "id": "MDRW2100010260.1.13",
          "form": "광어파인가요 연어파인가요",
          "original_form": "광어파인가요 연어파인가요",
          "speaker_id": "1",
          "time": "20210823 16:19"
        },
        {
          "id": "MDRW2100010260.1.14",
          "form": "저는 기름진것보다 식감이 좋아서 광어에요ㅎㅎ",
          "original_form": "저는 기름진것보다 식감이 좋아서 광어에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:20"
        },
        {
          "id": "MDRW2100010260.1.15",
          "form": "저랑같네요ㅋㅋㅋ",
          "original_form": "저랑같네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:22"
        },
        {
          "id": "MDRW2100010260.1.16",
          "form": "저 광어지느러미 환장해요",
          "original_form": "저 광어지느러미 환장해요",
          "speaker_id": "1",
          "time": "20210823 16:22"
        },
        {
          "id": "MDRW2100010260.1.17",
          "form": "아 쫄깃하죠ㅠㅠㅠ",
          "original_form": "아 쫄깃하죠ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 16:23"
        },
        {
          "id": "MDRW2100010260.1.18",
          "form": "맞아요ㅠ 먹고싶네요",
          "original_form": "맞아요ㅠ 먹고싶네요",
          "speaker_id": "1",
          "time": "20210823 16:26"
        },
        {
          "id": "MDRW2100010260.1.19",
          "form": "그러게요ㅠㅠ",
          "original_form": "그러게요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 16:27"
        }
      ]
