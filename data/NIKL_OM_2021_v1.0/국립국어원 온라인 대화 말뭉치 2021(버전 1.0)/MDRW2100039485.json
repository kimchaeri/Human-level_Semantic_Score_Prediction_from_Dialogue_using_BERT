      "utterance": [
        {
          "id": "MDRW2100039485.1.1",
          "form": "저녁은 제때 먹겠네 ㅋㅋㅋ",
          "original_form": "저녁은 제때 먹겠네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:09"
        },
        {
          "id": "MDRW2100039485.1.2",
          "form": "ㅋㅋㅋ 맞아! 저녁에 비빔밥 해먹었어",
          "original_form": "ㅋㅋㅋ 맞아! 저녁에 비빔밥 해먹었어",
          "speaker_id": "2",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039485.1.3",
          "form": "너는 저녁 뭐 먹었어??",
          "original_form": "너는 저녁 뭐 먹었어??",
          "speaker_id": "2",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100039485.1.4",
          "form": "비빔밥 맛있겠다!!",
          "original_form": "비빔밥 맛있겠다!!",
          "speaker_id": "1",
          "time": "20210903 21:03"
        },
        {
          "id": "MDRW2100039485.1.5",
          "form": "국밥? 정확힌 모르겠는데 그런 거 먹었어",
          "original_form": "국밥? 정확힌 모르겠는데 그런 거 먹었어",
          "speaker_id": "1",
          "time": "20210903 21:03"
        },
        {
          "id": "MDRW2100039485.1.6",
          "form": "국밥 너무 좋지!",
          "original_form": "국밥 너무 좋지!",
          "speaker_id": "2",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100039485.1.7",
          "form": "요즘 좋아하는 노래 있어??",
          "original_form": "요즘 좋아하는 노래 있어??",
          "speaker_id": "2",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100039485.1.8",
          "form": "아 ㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039485.1.9",
          "form": "요즘 퀸덤 들어!",
          "original_form": "요즘 퀸덤 들어!",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039485.1.10",
          "form": "오홍! 나도 퀸덤 엄청 재밌게 봤어",
          "original_form": "오홍! 나도 퀸덤 엄청 재밌게 봤어",
          "speaker_id": "2",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039485.1.11",
          "form": "어떤 노래가 제일 좋았어??",
          "original_form": "어떤 노래가 제일 좋았어??",
          "speaker_id": "2",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039485.1.12",
          "form": "응? 레드벨벳 퀸덤 말한 거였어 ㅋㅋㅋㅋㅋ",
          "original_form": "응? 레드벨벳 퀸덤 말한 거였어 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100039485.1.13",
          "form": "퀸덤에서는 덕분에 오마이걸을 좋아하게 됐다 ㅎㅎ",
          "original_form": "퀸덤에서는 덕분에 오마이걸을 좋아하게 됐다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100039485.1.14",
          "form": "아 맞아ㅋㅋㅋㅋ 레벨 신곡 퀸덤이지!",
