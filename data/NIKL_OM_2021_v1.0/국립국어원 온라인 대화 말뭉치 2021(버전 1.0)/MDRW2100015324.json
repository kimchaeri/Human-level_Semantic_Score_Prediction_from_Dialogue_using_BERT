        {
          "id": "MDRW2100015324.1.1",
          "form": "기억나는 여행지 있으세요?",
          "original_form": "기억나는 여행지 있으세요?",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015324.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015324.1.3",
          "form": "어디가 기억에 남으세요",
          "original_form": "어디가 기억에 남으세요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015324.1.4",
          "form": "단양이요",
          "original_form": "단양이요",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015324.1.5",
          "form": "제주도라고 하실 줄 알았는데요",
          "original_form": "제주도라고 하실 줄 알았는데요",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015324.1.6",
          "form": "하려다 말았어요",
          "original_form": "하려다 말았어요",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015324.1.7",
          "form": "왜 그렇게 생각하셨어요?",
          "original_form": "왜 그렇게 생각하셨어요?",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015324.1.8",
          "form": "왜 하려다 마셨을까요?",
          "original_form": "왜 하려다 마셨을까요?",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015324.1.9",
          "form": "전 제주도거든요^^",
          "original_form": "전 제주도거든요^^",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015324.1.10",
          "form": "왜냐하면",
          "original_form": "왜냐하면",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015324.1.11",
          "form": "제주도는 너무 흔하잖아요",
          "original_form": "제주도는 너무 흔하잖아요",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015324.1.12",
          "form": "재밌었고 좋았긴했는데",
          "original_form": "재밌었고 좋았긴했는데",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015324.1.13",
          "form": "제주도는 다 아는 여행지라서",
          "original_form": "제주도는 다 아는 여행지라서",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015324.1.14",
          "form": "다른 걸 얘기하고 싶었어요",
          "original_form": "다른 걸 얘기하고 싶었어요",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015324.1.15",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015324.1.16",
          "form": "하지만 소중한 친구와 간 여행은 흔하지 않죠?",
          "original_form": "하지만 소중한 친구와 간 여행은 흔하지 않죠?",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015324.1.17",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015324.1.18",
          "form": "그래서 고민했었어요",
          "original_form": "그래서 고민했었어요",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015324.1.19",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015324.1.20",
          "form": "담양에서 뭐하셨나요",
          "original_form": "담양에서 뭐하셨나요",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015324.1.21",
          "form": "단양이요",
          "original_form": "단양이요",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015324.1.22",
          "form": "패러글라이딩탔어요",
          "original_form": "패러글라이딩탔어요",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015324.1.23",
          "form": "제주도 어디가 제일 맘에 드셨나요?",
          "original_form": "제주도 어디가 제일 맘에 드셨나요?",
