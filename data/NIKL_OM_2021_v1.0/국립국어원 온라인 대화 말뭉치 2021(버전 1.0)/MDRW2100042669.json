      "utterance": [
        {
          "id": "MDRW2100042669.1.1",
          "form": "또 어떤 아이돌들 좋아하세요?",
          "original_form": "또 어떤 아이돌들 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 00:51"
        },
        {
          "id": "MDRW2100042669.1.2",
          "form": "저도 간잽많이하는편이라 ㅎㅎ",
          "original_form": "저도 간잽많이하는편이라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 00:51"
        },
        {
          "id": "MDRW2100042669.1.3",
          "form": "전 슴돌 많이 좋아해요!",
          "original_form": "전 슴돌 많이 좋아해요!",
          "speaker_id": "2",
          "time": "20210907 00:52"
        },
        {
          "id": "MDRW2100042669.1.4",
          "form": "요즘은 에스파랑 스테이씨가 좋더라구요",
          "original_form": "요즘은 에스파랑 스테이씨가 좋더라구요",
          "speaker_id": "2",
          "time": "20210907 00:52"
        },
        {
          "id": "MDRW2100042669.1.5",
          "form": "슴돌은 세계관이나 이런배경이 탄탄한게좋더라구여",
          "original_form": "슴돌은 세계관이나 이런배경이 탄탄한게좋더라구여",
          "speaker_id": "1",
          "time": "20210907 00:58"
        },
        {
          "id": "MDRW2100042669.1.6",
          "form": "유튜브 자주보시는거 있으신가여",
          "original_form": "유튜브 자주보시는거 있으신가여",
          "speaker_id": "1",
          "time": "20210907 00:58"
        },
        {
          "id": "MDRW2100042669.1.7",
          "form": "맞아요! 그런 매력이 또 있죠",
          "original_form": "맞아요! 그런 매력이 또 있죠",
          "speaker_id": "2",
          "time": "20210907 00:58"
        },
        {
          "id": "MDRW2100042669.1.8",
          "form": "저는 아이돌 교차편집 자주 봐요",
          "original_form": "저는 아이돌 교차편집 자주 봐요",
          "speaker_id": "2",
          "time": "20210907 01:09"
        },
        {
          "id": "MDRW2100042669.1.9",
          "form": "교차편집 보는맛 있더라구여 팬들 정성진짜 대단한것같아요",
          "original_form": "교차편집 보는맛 있더라구여 팬들 정성진짜 대단한것같아요",
          "speaker_id": "1",
          "time": "20210907 01:11"
        },
        {
          "id": "MDRW2100042669.1.10",
          "form": "구독하시는 유튜버있으신가요",
          "original_form": "구독하시는 유튜버있으신가요",
          "speaker_id": "1",
          "time": "20210907 01:11"
        },
        {
          "id": "MDRW2100042669.1.11",
          "form": "교차편집 요즘 마술 같지 않나요?",
          "original_form": "교차편집 요즘 마술 같지 않나요?",
          "speaker_id": "2",
          "time": "20210907 01:12"
        },
        {
          "id": "MDRW2100042669.1.12",
          "form": "전 딱히 구독하는 유튜버는 없고 좋아하는 아이돌들 계정 구독해놓고 있어요!",
          "original_form": "전 딱히 구독하는 유튜버는 없고 좋아하는 아이돌들 계정 구독해놓고 있어요!",
          "speaker_id": "2",
          "time": "20210907 01:13"
        },
        {
          "id": "MDRW2100042669.1.13",
          "form": "맞아요 옷색만 바뀌는마법마냥 ㅋㅋㅋ어떻게 그렇게딱딱 맞는지",
          "original_form": "맞아요 옷색만 바뀌는마법마냥 ㅋㅋㅋ어떻게 그렇게딱딱 맞는지",
          "speaker_id": "1",
          "time": "20210907 01:14"
        },
        {
          "id": "MDRW2100042669.1.14",
          "form": "너무 신기해요... 어떻게 하는지도 궁금하고",
          "original_form": "너무 신기해요... 어떻게 하는지도 궁금하고",
          "speaker_id": "2",
          "time": "20210907 01:16"
        },
        {
          "id": "MDRW2100042669.1.15",
          "form": "맞아요 편집도엄청오래걸리는것같던데 진짜 팬심없음못할거같아요",
          "original_form": "맞아요 편집도엄청오래걸리는것같던데 진짜 팬심없음못할거같아요",
          "speaker_id": "1",
          "time": "20210907 01:18"
        },
        {
          "id": "MDRW2100042669.1.16",
          "form": "그쵸! 아무나 할 수 있는 게 아닌 것 같아요",
          "original_form": "그쵸! 아무나 할 수 있는 게 아닌 것 같아요",
          "speaker_id": "2",
          "time": "20210907 01:19"
        },
        {
          "id": "MDRW2100042669.1.17",
          "form": "진짜 기획사에서 돈줘도 될 퀄리티!",
          "original_form": "진짜 기획사에서 돈줘도 될 퀄리티!",
          "speaker_id": "1",
          "time": "20210907 01:21"
        },
        {
          "id": "MDRW2100042669.1.18",
          "form": "티비프로는 뭐 자주보세요",
          "original_form": "티비프로는 뭐 자주보세요",
          "speaker_id": "1",
          "time": "20210907 01:21"
        },
        {
          "id": "MDRW2100042669.1.19",
          "form": "저 놀라운 토요일이요!",
          "original_form": "저 놀라운 토요일이요!",
          "speaker_id": "2",
          "time": "20210907 01:21"
        },
        {
          "id": "MDRW2100042669.1.20",
          "form": "저도 놀토 자주봐요 멤버들 합도좋고",
          "original_form": "저도 놀토 자주봐요 멤버들 합도좋고",
          "speaker_id": "1",
          "time": "20210907 01:23"
        }
      ]
    }
  ]
