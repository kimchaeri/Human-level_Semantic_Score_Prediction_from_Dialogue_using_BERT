      "utterance": [
        {
          "id": "MDRW2100009896.1.1",
          "form": "지금 계신 지역은 비가 오나요?",
          "original_form": "지금 계신 지역은 비가 오나요?",
          "speaker_id": "2",
          "time": "20210823 11:27"
        },
        {
          "id": "MDRW2100009896.1.2",
          "form": "네 비가 좀 오네요",
          "original_form": "네 비가 좀 오네요",
          "speaker_id": "1",
          "time": "20210823 11:27"
        },
        {
          "id": "MDRW2100009896.1.3",
          "form": "거기도 오나요?",
          "original_form": "거기도 오나요?",
          "speaker_id": "1",
          "time": "20210823 11:27"
        },
        {
          "id": "MDRW2100009896.1.4",
          "form": "네 오네요",
          "original_form": "네 오네요",
          "speaker_id": "2",
          "time": "20210823 11:28"
        },
        {
          "id": "MDRW2100009896.1.5",
          "form": "제가 딱 외출하니 비오기 시작하네요 아놔 진짜",
          "original_form": "제가 딱 외출하니 비오기 시작하네요 아놔 진짜",
          "speaker_id": "2",
          "time": "20210823 11:29"
        },
        {
          "id": "MDRW2100009896.1.6",
          "form": "ㅎㅎ 이런 날엔 집에만 있고싶어요",
          "original_form": "ㅎㅎ 이런 날엔 집에만 있고싶어요",
          "speaker_id": "1",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009896.1.7",
          "form": "저는 아침에 출근하기싫더라구요",
          "original_form": "저는 아침에 출근하기싫더라구요",
          "speaker_id": "1",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009896.1.8",
          "form": "저도 막 그런거조차 짜증나요",
          "original_form": "저도 막 그런거조차 짜증나요",
          "speaker_id": "2",
          "time": "20210823 11:35"
        },
        {
          "id": "MDRW2100009896.1.9",
          "form": "왜 비올때 나는 실내에 있지 못하는가 ㅋㅋㅋ",
          "original_form": "왜 비올때 나는 실내에 있지 못하는가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:35"
        },
        {
          "id": "MDRW2100009896.1.10",
          "form": "태풍도 온다고하고 회사에서는 계속 문자보내고 ㅎ",
          "original_form": "태풍도 온다고하고 회사에서는 계속 문자보내고 ㅎ",
          "speaker_id": "1",
          "time": "20210823 11:39"
        },
        {
          "id": "MDRW2100009896.1.11",
          "form": "태풍을 우리가 막을것도아닌데 어쩌라는건지 ㅋ",
          "original_form": "태풍을 우리가 막을것도아닌데 어쩌라는건지 ㅋ",
          "speaker_id": "1",
          "time": "20210823 11:39"
        },
        {
          "id": "MDRW2100009896.1.12",
          "form": "아 ㅋㅋ 저도 뭐 어쩌라는거야",
          "original_form": "아 ㅋㅋ 저도 뭐 어쩌라는거야",
          "speaker_id": "2",
          "time": "20210823 11:40"
        },
        {
          "id": "MDRW2100009896.1.13",
          "form": "나도 쉴팔자가 되면 쉬고싶다고 이런 생각만했어요",
          "original_form": "나도 쉴팔자가 되면 쉬고싶다고 이런 생각만했어요",
          "speaker_id": "2",
          "time": "20210823 11:40"
        },
        {
          "id": "MDRW2100009896.1.14",
          "form": "안된다며 긍정적으로 살아야한다며 ㅋㅋ",
          "original_form": "안된다며 긍정적으로 살아야한다며 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:41"
        },
        {
          "id": "MDRW2100009896.1.15",
          "form": "글쵸 ㅎㅎ",
          "original_form": "글쵸 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 11:42"
        },
        {
          "id": "MDRW2100009896.1.16",
          "form": "뉴스보니  오늘밤내일이 고비같네요",
          "original_form": "뉴스보니  오늘밤내일이 고비같네요",
          "speaker_id": "1",
          "time": "20210823 11:44"
        },
        {
          "id": "MDRW2100009896.1.17",
          "form": "동해안으로잘 빠져나가길 빌어봐야겠어요",
          "original_form": "동해안으로잘 빠져나가길 빌어봐야겠어요",
          "speaker_id": "1",
          "time": "20210823 11:44"
        },
        {
          "id": "MDRW2100009896.1.18",
          "form": "기상이변이 진짜 좀 있는것같기도하네요",
          "original_form": "기상이변이 진짜 좀 있는것같기도하네요",
          "speaker_id": "2",
          "time": "20210823 11:45"
        },
        {
          "id": "MDRW2100009896.1.19",
          "form": "뭔가 내가 어릴땐 이렇게 덥지않았던것 같은데..",
          "original_form": "뭔가 내가 어릴땐 이렇게 덥지않았던것 같은데..",
          "speaker_id": "2",
          "time": "20210823 11:45"
        },
        {
          "id": "MDRW2100009896.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210823 11:45"
        },
        {
          "id": "MDRW2100009896.1.21",
          "form": "온난화가 심해진거같기도하구요",
          "original_form": "온난화가 심해진거같기도하구요",
          "speaker_id": "1",
          "time": "20210823 11:45"
        },
        {
          "id": "MDRW2100009896.1.22",
          "form": "앞으로 세대들이 걱정이지요ㅜ",
          "original_form": "앞으로 세대들이 걱정이지요ㅜ",
          "speaker_id": "2",
          "time": "20210823 11:47"
        },
        {
          "id": "MDRW2100009896.1.23",
          "form": "지구야 미안해ㅜㅜ",
          "original_form": "지구야 미안해ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 11:48"
        },
        {
          "id": "MDRW2100009896.1.24",
          "form": "저도 갑자기 지구한테 미안해지네요",
          "original_form": "저도 갑자기 지구한테 미안해지네요",
