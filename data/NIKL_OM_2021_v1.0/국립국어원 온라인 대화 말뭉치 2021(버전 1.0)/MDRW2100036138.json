      "utterance": [
        {
          "id": "MDRW2100036138.1.1",
          "form": "저번에 분짜 먹어봤어요!",
          "original_form": "저번에 분짜 먹어봤어요!",
          "speaker_id": "1",
          "time": "20210830 17:06"
        },
        {
          "id": "MDRW2100036138.1.2",
          "form": "맛있으셨나요!?",
          "original_form": "맛있으셨나요!?",
          "speaker_id": "2",
          "time": "20210830 17:09"
        },
        {
          "id": "MDRW2100036138.1.3",
          "form": "네 엄청 입에 맞았어요!",
          "original_form": "네 엄청 입에 맞았어요!",
          "speaker_id": "1",
          "time": "20210830 17:11"
        },
        {
          "id": "MDRW2100036138.1.4",
          "form": "그래서 오늘도 딴 거 먹으러 가보려구요",
          "original_form": "그래서 오늘도 딴 거 먹으러 가보려구요",
          "speaker_id": "1",
          "time": "20210830 17:11"
        },
        {
          "id": "MDRW2100036138.1.5",
          "form": "새로운 음식을 개척하시는 편이군요!",
          "original_form": "새로운 음식을 개척하시는 편이군요!",
          "speaker_id": "2",
          "time": "20210830 17:19"
        },
        {
          "id": "MDRW2100036138.1.6",
          "form": "저는 혼자서는 거의 먹던 것만 먹어서ㅎㅎ",
          "original_form": "저는 혼자서는 거의 먹던 것만 먹어서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 17:19"
        },
        {
          "id": "MDRW2100036138.1.7",
          "form": "새로운 음식 보면 넘 궁금해서 먹어보거 싶어요",
          "original_form": "새로운 음식 보면 넘 궁금해서 먹어보거 싶어요",
          "speaker_id": "1",
          "time": "20210830 17:19"
        },
        {
          "id": "MDRW2100036138.1.8",
          "form": "너무 괴조합만 아니라면 좋아요 ㅎㅎ",
          "original_form": "너무 괴조합만 아니라면 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 17:20"
        },
        {
          "id": "MDRW2100036138.1.9",
          "form": "저는 쌀국수도 마라탕도 주변에서 데리고 가줘서 처음 먹어봤어요ㅋㅋㅋ",
          "original_form": "저는 쌀국수도 마라탕도 주변에서 데리고 가줘서 처음 먹어봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:23"
        },
        {
          "id": "MDRW2100036138.1.10",
          "form": "가자 그럴 때 가면 좋죠",
          "original_form": "가자 그럴 때 가면 좋죠",
          "speaker_id": "1",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100036138.1.11",
          "form": "제 동생은 가자고 해도 싫대요",
          "original_form": "제 동생은 가자고 해도 싫대요",
          "speaker_id": "1",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100036138.1.12",
          "form": "아 정말요~?",
          "original_form": "아 정말요~?",
          "speaker_id": "2",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100036138.1.13",
          "form": "동생분도 도전 잘 안 하시는 스타일인가요?",
          "original_form": "동생분도 도전 잘 안 하시는 스타일인가요?",
          "speaker_id": "2",
          "time": "20210830 17:27"
        },
        {
          "id": "MDRW2100036138.1.14",
          "form": "네 초딩입맛이라 딱 치킨 피자만 좋아해욬ㅋㅋㅋ",
          "original_form": "네 초딩입맛이라 딱 치킨 피자만 좋아해욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:28"
        },
        {
          "id": "MDRW2100036138.1.15",
          "form": "앗ㅋㅋㅋ 치킨 피자도 맛있죠ㅎㅎ",
          "original_form": "앗ㅋㅋㅋ 치킨 피자도 맛있죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 17:28"
        },
        {
          "id": "MDRW2100036138.1.16",
          "form": "치킨 시키시면 어디서 시켜 드시나요?",
          "original_form": "치킨 시키시면 어디서 시켜 드시나요?",
          "speaker_id": "2",
          "time": "20210830 17:28"
        },
        {
          "id": "MDRW2100036138.1.17",
          "form": "비에치씨랑 푸라닭 자주 시켜요",
          "original_form": "비에치씨랑 푸라닭 자주 시켜요",
          "speaker_id": "1",
          "time": "20210830 17:30"
        },
        {
          "id": "MDRW2100036138.1.18",
          "form": "푸라닭 진짜 맛있더라고요",
          "original_form": "푸라닭 진짜 맛있더라고요",
          "speaker_id": "2",
          "time": "20210830 17:31"
        },
        {
          "id": "MDRW2100036138.1.19",
          "form": "비싼 만큼 맛있어요..",
          "original_form": "비싼 만큼 맛있어요..",
          "speaker_id": "2",
          "time": "20210830 17:31"
        },
        {
          "id": "MDRW2100036138.1.20",
          "form": "맞아요 잘되면 잘되는 이유가 다 있어요",
          "original_form": "맞아요 잘되면 잘되는 이유가 다 있어요",
