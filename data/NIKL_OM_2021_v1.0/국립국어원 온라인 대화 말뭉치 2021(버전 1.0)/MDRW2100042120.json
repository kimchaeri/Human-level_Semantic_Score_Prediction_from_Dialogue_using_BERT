      "utterance": [
        {
          "id": "MDRW2100042120.1.1",
          "form": "엄마가 그래도 얼굴 이쁘게 낳아줬잖아",
          "original_form": "엄마가 그래도 얼굴 이쁘게 낳아줬잖아",
          "speaker_id": "2",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100042120.1.2",
          "form": "그건 만족스럽긴 하지만 ㅎㅎ",
          "original_form": "그건 만족스럽긴 하지만 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042120.1.3",
          "form": "이쁘긴 한데 잘난척 쩐다 우리 딸",
          "original_form": "이쁘긴 한데 잘난척 쩐다 우리 딸",
          "speaker_id": "2",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042120.1.4",
          "form": "참나 ㅠ 사실인데",
          "original_form": "참나 ㅠ 사실인데",
          "speaker_id": "1",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042120.1.5",
          "form": "내려오면 매직 하자 거기가 제일 잘하니까",
          "original_form": "내려오면 매직 하자 거기가 제일 잘하니까",
          "speaker_id": "2",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042120.1.6",
          "form": "저도 다른곳에서 해봤는데 사흘지나면 풀리더라고요",
          "original_form": "저도 다른곳에서 해봤는데 사흘지나면 풀리더라고요",
          "speaker_id": "1",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042120.1.7",
          "form": "돈아깝게 다들 왜이렇게 못하니",
          "original_form": "돈아깝게 다들 왜이렇게 못하니",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042120.1.8",
          "form": "그니까요 거기처럼 공들여서 잘 펴줘야 하는데",
          "original_form": "그니까요 거기처럼 공들여서 잘 펴줘야 하는데",
          "speaker_id": "1",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042120.1.9",
          "form": "다들 시간 빨리 끝내려고 대충 하고 끝내는거같아요",
          "original_form": "다들 시간 빨리 끝내려고 대충 하고 끝내는거같아요",
          "speaker_id": "1",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042120.1.10",
          "form": "사흘이면 고데기질만 해도 유지되겠다",
          "original_form": "사흘이면 고데기질만 해도 유지되겠다",
          "speaker_id": "2",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100042120.1.11",
          "form": "그러니까요 ㅋㅋㅋㅋ 돈을 꽁으로 받아가려고해 나쁜놈들",
          "original_form": "그러니까요 ㅋㅋㅋㅋ 돈을 꽁으로 받아가려고해 나쁜놈들",
          "speaker_id": "1",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100042120.1.12",
          "form": "그러고도 미용 자격증 나오나",
          "original_form": "그러고도 미용 자격증 나오나",
          "speaker_id": "2",
          "time": "20210906 20:00"
        },
        {
          "id": "MDRW2100042120.1.13",
          "form": "자격증을 의심하게 만드는 실력 ㅡㅡ",
          "original_form": "자격증을 의심하게 만드는 실력 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210906 20:00"
        },
        {
          "id": "MDRW2100042120.1.14",
          "form": "하여튼 내려와서 머리해",
          "original_form": "하여튼 내려와서 머리해",
          "speaker_id": "2",
          "time": "20210906 20:00"
        },
        {
          "id": "MDRW2100042120.1.15",
          "form": "펴는건 거기가 잘하니까",
          "original_form": "펴는건 거기가 잘하니까",
          "speaker_id": "2",
          "time": "20210906 20:00"
        },
        {
          "id": "MDRW2100042120.1.16",
