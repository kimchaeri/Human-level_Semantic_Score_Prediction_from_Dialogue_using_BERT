      "utterance": [
        {
          "id": "MDRW2100041687.1.1",
          "form": "언니 요리를 잘하는게 아니구",
          "original_form": "언니 요리를 잘하는게 아니구",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041687.1.2",
          "form": "어쩔수 없이 한다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어쩔수 없이 한다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041687.1.3",
          "form": "케이스 마트에 콩나물이랑 두부 싸더라고",
          "original_form": "케이스 마트에 콩나물이랑 두부 싸더라고",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041687.1.4",
          "form": "그래도 하는게 어디고 ㅋㅋ",
          "original_form": "그래도 하는게 어디고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041687.1.5",
          "form": "잘한다",
          "original_form": "잘한다",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041687.1.6",
          "form": "집에 있으니 해야지뭐",
          "original_form": "집에 있으니 해야지뭐",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041687.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ 우리 목욜날 케이스마트 가는거제??",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 우리 목욜날 케이스마트 가는거제??",
          "speaker_id": "1",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041687.1.8",
          "form": "언니처럼 평일에 매일 일 나가는것도 아니구",
          "original_form": "언니처럼 평일에 매일 일 나가는것도 아니구",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041687.1.9",
          "form": "응응 목욜날 고고 합세 ㅋㅋ",
          "original_form": "응응 목욜날 고고 합세 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041687.1.10",
          "form": "name3이 4시45분쯤 태권도에서 오거든",
          "original_form": "&name3&이 4시45분쯤 태권도에서 오거든",
          "speaker_id": "1",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041687.1.11",
          "form": "니 몇시쯤 출발할꺼고??",
          "original_form": "니 몇시쯤 출발할꺼고??",
          "speaker_id": "1",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041687.1.12",
          "form": "name4이 5시20분에 오는데",
          "original_form": "&name4&이 5시20분에 오는데",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041687.1.13",
          "form": "너무 늦나?",
          "original_form": "너무 늦나?",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100041687.1.14",
          "form": "ㅋㅋㅋㅋㅋㄱ 더늦노 ㅋㅋㅋㅋㄱ 아니 괜찮다 좀 늦게까지 놀다오지뭐 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱ 더늦노 ㅋㅋㅋㅋㄱ 아니 괜찮다 좀 늦게까지 놀다오지뭐 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100041687.1.15",
          "form": "그럼 바로 픽업해서 출발하자",
          "original_form": "그럼 바로 픽업해서 출발하자",
          "speaker_id": "1",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100041687.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그래그래",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그래그래",
          "speaker_id": "2",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100041687.1.17",
          "form": "좋구마잉 ㅋㅋ",
          "original_form": "좋구마잉 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100041687.1.18",
          "form": "늦게까지 놀다가 밥먹고 바로씻기고 자면되겠네",
          "original_form": "늦게까지 놀다가 밥먹고 바로씻기고 자면되겠네",
          "speaker_id": "2",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041687.1.19",
          "form": "그날도 꿀잠 자겠구만",
          "original_form": "그날도 꿀잠 자겠구만",
          "speaker_id": "2",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041687.1.20",
          "form": "좋다 일곱시쯤 와서 밥 먹고 바로 재울 수 있도록",
          "original_form": "좋다 일곱시쯤 와서 밥 먹고 바로 재울 수 있도록",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041687.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041687.1.22",
          "form": "응응 그러니깐은 그날은 먹기 쉬운걸 먹이고 ㅋㅋ 재워야지",
          "original_form": "응응 그러니깐은 그날은 먹기 쉬운걸 먹이고 ㅋㅋ 재워야지",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041687.1.23",
          "form": "그날은 짜장밥이다ㅋㅋㅋㅋㄱ",
          "original_form": "그날은 짜장밥이다ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100041687.1.24",
          "form": "제일 쉬운거 ㅋㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "제일 쉬운거 ㅋㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100041687.1.25",
          "form": "그러니깐 나도 ㅋㅋ",
          "original_form": "그러니깐 나도 ㅋㅋ",
