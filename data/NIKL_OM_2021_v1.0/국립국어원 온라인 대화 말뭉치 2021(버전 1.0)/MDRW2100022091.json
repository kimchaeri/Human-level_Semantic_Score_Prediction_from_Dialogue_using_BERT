      "utterance": [
        {
          "id": "MDRW2100022091.1.1",
          "form": "집에서 사서 씻어서 만들기도 되긴 하는데",
          "original_form": "집에서 사서 씻어서 만들기도 되긴 하는데",
          "speaker_id": "2",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100022091.1.2",
          "form": "손이 너무 가서 ㅋㅋㅋㅋ",
          "original_form": "손이 너무 가서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100022091.1.3",
          "form": "근데 그러면 안심은 되긴 하져 ㅎㅎ",
          "original_form": "근데 그러면 안심은 되긴 하져 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100022091.1.4",
          "form": "아무래도 손질도 어렵고",
          "original_form": "아무래도 손질도 어렵고",
          "speaker_id": "1",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100022091.1.5",
          "form": "저는 요리 가끔하는편인데",
          "original_form": "저는 요리 가끔하는편인데",
          "speaker_id": "1",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100022091.1.6",
          "form": "씨앗 일일이 다 빼야하고 ㅋㅋ",
          "original_form": "씨앗 일일이 다 빼야하고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100022091.1.7",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "2",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100022091.1.8",
          "form": "저도 최근에 집에 감자가 너무 많인서 소진하려고",
          "original_form": "저도 최근에 집에 감자가 너무 많인서 소진하려고",
          "speaker_id": "1",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100022091.1.9",
          "form": "감자수프만들고",
          "original_form": "감자수프만들고",
          "speaker_id": "1",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100022091.1.10",
          "form": "와아! 감자!!",
          "original_form": "와아! 감자!!",
          "speaker_id": "2",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100022091.1.11",
          "form": "두부 유통기한이 조금 지났길래",
          "original_form": "두부 유통기한이 조금 지났길래",
          "speaker_id": "1",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100022091.1.12",
          "form": "감자 소진 디게 어렵져 ㅠㅠ 배가 금방 차버리고",
          "original_form": "감자 소진 디게 어렵져 ㅠㅠ 배가 금방 차버리고",
          "speaker_id": "2",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100022091.1.13",
          "form": "두부강정 만들었어요",
          "original_form": "두부강정 만들었어요",
          "speaker_id": "1",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100022091.1.14",
          "form": "왁 두부 두부 냉동하세요!",
          "original_form": "왁 두부 두부 냉동하세요!",
          "speaker_id": "2",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100022091.1.15",
          "form": "두부 냉동 잘되요",
          "original_form": "두부 냉동 잘되요",
          "speaker_id": "2",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100022091.1.16",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100022091.1.17",
          "form": "냉동두부 갖구 마파두부나 두부된장국이나 강된장 이런거 하면 완전 좋아여",
          "original_form": "냉동두부 갖구 마파두부나 두부된장국이나 강된장 이런거 하면 완전 좋아여",
          "speaker_id": "2",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100022091.1.18",
          "form": "두부냉동하고",
          "original_form": "두부냉동하고",
          "speaker_id": "1",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100022091.1.19",
          "form": "손으로 눗느면",
          "original_form": "손으로 눗느면",
          "speaker_id": "1",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100022091.1.20",
          "form": "물기가 쏙빠지죠",
          "original_form": "물기가 쏙빠지죠",
          "speaker_id": "1",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100022091.1.21",
          "form": "물 쭈우욱 ㅋㅋㅋㅋㅋ",
          "original_form": "물 쭈우욱 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100022091.1.22",
          "form": "아 맞다 있죠 묵이요",
          "original_form": "아 맞다 있죠 묵이요",
          "speaker_id": "2",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100022091.1.23",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100022091.1.24",
          "form": "묵 절대 냉동하지 마세요....^ㅡ^",
          "original_form": "묵 절대 냉동하지 마세요....^ㅡ^",
          "speaker_id": "2",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100022091.1.25",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100022091.1.26",
          "form": "와 완전 ... 스펀지 됐어요..",
          "original_form": "와 완전 ... 스펀지 됐어요..",
          "speaker_id": "2",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100022091.1.27",
          "form": "커져요?",
          "original_form": "커져요?",
          "speaker_id": "1",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100022091.1.28",
          "form": "묵 섬유질 킹왕짱이구나를 배운 경험.. 재생이 안되요 ㅋㅋㅋㅋ",
          "original_form": "묵 섬유질 킹왕짱이구나를 배운 경험.. 재생이 안되요 ㅋㅋㅋㅋ",
