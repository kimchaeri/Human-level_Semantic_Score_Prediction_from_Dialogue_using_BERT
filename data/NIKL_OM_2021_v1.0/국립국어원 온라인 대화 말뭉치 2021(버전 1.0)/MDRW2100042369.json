      "utterance": [
        {
          "id": "MDRW2100042369.1.1",
          "form": "저녁 뭐 먹었댔어?",
          "original_form": "저녁 뭐 먹었댔어?",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042369.1.2",
          "form": "고기 먹었어요",
          "original_form": "고기 먹었어요",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042369.1.3",
          "form": "혹시 목살과 김치?!?!",
          "original_form": "혹시 목살과 김치?!?!",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042369.1.4",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042369.1.5",
          "form": "앜ㅋㅋㅋ글쿤요",
          "original_form": "앜ㅋㅋㅋ글쿤요",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042369.1.6",
          "form": "내일은 또 뭘먹져",
          "original_form": "내일은 또 뭘먹져",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042369.1.7",
          "form": "마라샹궈 조아하세여?",
          "original_form": "마라샹궈 조아하세여?",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042369.1.8",
          "form": "마라탕은 먹어봤는데 그건 안먹어봤어요",
          "original_form": "마라탕은 먹어봤는데 그건 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042369.1.9",
          "form": "아아 샹궈 오늘 고민하다가 그냥 집밥 먹었어요",
          "original_form": "아아 샹궈 오늘 고민하다가 그냥 집밥 먹었어요",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042369.1.10",
          "form": "음... 샹궈 맛있나요? 궁금하네요",
          "original_form": "음... 샹궈 맛있나요? 궁금하네요",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042369.1.11",
          "form": "네네 야채볶음이라 밥 되게 마니먹게돼요",
          "original_form": "네네 야채볶음이라 밥 되게 마니먹게돼요",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042369.1.12",
          "form": "마라는 향이 너무 세서 집에서 먹기 좀 그렇더라구요",
          "original_form": "마라는 향이 너무 세서 집에서 먹기 좀 그렇더라구요",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042369.1.13",
          "form": "아아 그쵸 저도 그래서 젤 순한맛으로 먹어요",
          "original_form": "아아 그쵸 저도 그래서 젤 순한맛으로 먹어요",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042369.1.14",
          "form": "전 가서 먹는데 옷에 냄새 다 배는거 같아요ㅋㅋ",
          "original_form": "전 가서 먹는데 옷에 냄새 다 배는거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042369.1.15",
          "form": "맞아요 머리에도 나고",
          "original_form": "맞아요 머리에도 나고",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042369.1.16",
          "form": "다음날에도 나여",
          "original_form": "다음날에도 나여",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042369.1.17",
          "form": "피부에 베여요 ㅋㅋㅋㅋ",
          "original_form": "피부에 베여요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:19"
        }
