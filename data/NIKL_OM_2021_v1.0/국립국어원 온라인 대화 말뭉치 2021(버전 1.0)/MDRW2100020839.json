      "utterance": [
        {
          "id": "MDRW2100020839.1.1",
          "form": "해외나 국내여행 가보신 곳 있으신가요?",
          "original_form": "해외나 국내여행 가보신 곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020839.1.2",
          "form": "네 유럽이랑 일본 가봤어요",
          "original_form": "네 유럽이랑 일본 가봤어요",
          "speaker_id": "2",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020839.1.3",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020839.1.4",
          "form": "우와 네 좋아해요! 저는 말레이시아랑 미국 가봤어요",
          "original_form": "우와 네 좋아해요! 저는 말레이시아랑 미국 가봤어요",
          "speaker_id": "1",
          "time": "20210828 13:21"
        },
        {
          "id": "MDRW2100020839.1.5",
          "form": "여행경험 들려주세요",
          "original_form": "여행경험 들려주세요",
          "speaker_id": "1",
          "time": "20210828 13:21"
        },
        {
          "id": "MDRW2100020839.1.6",
          "form": "학생때 갔던거라 고생을 많이해서..ㅋㅋㅋ",
          "original_form": "학생때 갔던거라 고생을 많이해서..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:23"
        },
        {
          "id": "MDRW2100020839.1.7",
          "form": "회사 다니면 다시 가보고 싶었는데 코로나때문에 못가고 있네요 ㅠ",
          "original_form": "회사 다니면 다시 가보고 싶었는데 코로나때문에 못가고 있네요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 13:24"
        },
        {
          "id": "MDRW2100020839.1.8",
          "form": "미국은 동부 가신건가요?",
          "original_form": "미국은 동부 가신건가요?",
          "speaker_id": "2",
          "time": "20210828 13:24"
        },
        {
          "id": "MDRW2100020839.1.9",
          "form": "우와 학생때 가셨구나.. 네 동부요!",
          "original_form": "우와 학생때 가셨구나.. 네 동부요!",
          "speaker_id": "1",
          "time": "20210828 13:28"
        },
        {
          "id": "MDRW2100020839.1.10",
          "form": "지인이 있어서 갔다왔어요",
          "original_form": "지인이 있어서 갔다왔어요",
          "speaker_id": "1",
          "time": "20210828 13:28"
        },
        {
          "id": "MDRW2100020839.1.11",
          "form": "와 그럼 더 현지인 시점으로 보셨겠네요",
          "original_form": "와 그럼 더 현지인 시점으로 보셨겠네요",
          "speaker_id": "2",
          "time": "20210828 13:28"
        },
        {
          "id": "MDRW2100020839.1.12",
          "form": "아니에요ㅋㅋㅋㅋ유명지만 왔다갔다 했답니다",
          "original_form": "아니에요ㅋㅋㅋㅋ유명지만 왔다갔다 했답니다",
          "speaker_id": "1",
          "time": "20210828 13:29"
        },
        {
          "id": "MDRW2100020839.1.13",
          "form": "여행갔다오면 시야가 트이긴 하는 것 같아요",
          "original_form": "여행갔다오면 시야가 트이긴 하는 것 같아요",
          "speaker_id": "1",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100020839.1.14",
          "form": "맞아요 새로운 환경을 보니까 확실히 다양한 경험이 되더라구요",
          "original_form": "맞아요 새로운 환경을 보니까 확실히 다양한 경험이 되더라구요",
          "speaker_id": "2",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020839.1.15",
          "form": "그쵸 경험 중요하죠",
          "original_form": "그쵸 경험 중요하죠",
          "speaker_id": "1",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100020839.1.16",
          "form": "워킹홀리데이 가 보셨나요?",
          "original_form": "워킹홀리데이 가 보셨나요?",
          "speaker_id": "1",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100020839.1.17",
          "form": "아뇨 교환학생이랑 워홀 안가봤어요",
          "original_form": "아뇨 교환학생이랑 워홀 안가봤어요",
          "speaker_id": "2",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100020839.1.18",
          "form": "가보셨나여?",
          "original_form": "가보셨나여?",
          "speaker_id": "2",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100020839.1.19",
          "form": "아뇨 안가봤어요.. 그냥 유튜브로만 알아보고있네요 하하",
          "original_form": "아뇨 안가봤어요.. 그냥 유튜브로만 알아보고있네요 하하",
          "speaker_id": "1",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020839.1.20",
          "form": "님은 워홀 하실 생각있나요?",
          "original_form": "님은 워홀 하실 생각있나요?",
          "speaker_id": "1",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100020839.1.21",
          "form": "전 엄두가 안나서 못갈거같아요",
          "original_form": "전 엄두가 안나서 못갈거같아요",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020839.1.22",
          "form": "주변에서 워홀 갔는데 일자리 없어서 돌아오는 경우도 봤어요",
          "original_form": "주변에서 워홀 갔는데 일자리 없어서 돌아오는 경우도 봤어요",
          "speaker_id": "2",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100020839.1.23",
          "form": "헐 그렇군요..역시 힘드네요",
          "original_form": "헐 그렇군요..역시 힘드네요",
          "speaker_id": "1",
          "time": "20210828 13:48"
        }
      ]
    }
