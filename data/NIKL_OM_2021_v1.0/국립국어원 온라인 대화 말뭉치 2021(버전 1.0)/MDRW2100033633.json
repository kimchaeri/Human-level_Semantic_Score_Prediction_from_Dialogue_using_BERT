      "utterance": [
        {
          "id": "MDRW2100033633.1.1",
          "form": "여행좋아하시나요?",
          "original_form": "여행좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100033633.1.2",
          "form": "네 여행 좋아해요ㅎㅎ 여행 좋아하세요?",
          "original_form": "네 여행 좋아해요ㅎㅎ 여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100033633.1.3",
          "form": "네 저도 여행좋아해요",
          "original_form": "네 저도 여행좋아해요",
          "speaker_id": "1",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100033633.1.4",
          "form": "국내 좋아하는 여행지있으신가요",
          "original_form": "국내 좋아하는 여행지있으신가요",
          "speaker_id": "1",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100033633.1.5",
          "form": "제주도ㅎㅎ 강원도 바다도 좋아해요ㅋㅋ",
          "original_form": "제주도ㅎㅎ 강원도 바다도 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100033633.1.6",
          "form": "저도 바다있는곳좋아해서 제주도 넘좋아해요",
          "original_form": "저도 바다있는곳좋아해서 제주도 넘좋아해요",
          "speaker_id": "1",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100033633.1.7",
          "form": "제주도 어디가 가장좋으셨어요",
          "original_form": "제주도 어디가 가장좋으셨어요",
          "speaker_id": "1",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100033633.1.8",
          "form": "몽돌이었나..사람많았는데 좋았어요ㅠㅠ",
          "original_form": "몽돌이었나..사람많았는데 좋았어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 19:55"
        },
        {
          "id": "MDRW2100033633.1.9",
          "form": "오 그 해변이 모래사장말고 몽돌깔린거말이죠?",
          "original_form": "오 그 해변이 모래사장말고 몽돌깔린거말이죠?",
          "speaker_id": "1",
          "time": "20210824 21:51"
        },
        {
          "id": "MDRW2100033633.1.10",
          "form": "네 귀엽더라구요ㅎㅎ",
          "original_form": "네 귀엽더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 21:52"
        },
        {
          "id": "MDRW2100033633.1.11",
          "form": "맞아요 다른매력이있져 전 제주도에 우도가 가장좋더라구요",
          "original_form": "맞아요 다른매력이있져 전 제주도에 우도가 가장좋더라구요",
          "speaker_id": "1",
          "time": "20210824 21:53"
        },
        {
          "id": "MDRW2100033633.1.12",
          "form": "우도가보셨나요?",
          "original_form": "우도가보셨나요?",
          "speaker_id": "1",
          "time": "20210824 21:53"
        },
        {
          "id": "MDRW2100033633.1.13",
          "form": "아 네ㅎㅎ진짜 이쁘더라구요 안갔으면 후회할뻔했어요",
          "original_form": "아 네ㅎㅎ진짜 이쁘더라구요 안갔으면 후회할뻔했어요",
          "speaker_id": "2",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100033633.1.14",
          "form": "그쵸 바다 너무맑고이쁘고",
          "original_form": "그쵸 바다 너무맑고이쁘고",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100033633.1.15",
          "form": "거기에 보트체험있는데 타보셨나요",
          "original_form": "거기에 보트체험있는데 타보셨나요",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100033633.1.16",
          "form": "아뇨ㅠㅠ다음에 와서 타자했는데...ㅎㅎ 재밌어보였는데 타셨어요?",
          "original_form": "아뇨ㅠㅠ다음에 와서 타자했는데...ㅎㅎ 재밌어보였는데 타셨어요?",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100033633.1.17",
          "form": "네 제주도에 보트체험하는곳중에 우도가 젤 재밌어요",
          "original_form": "네 제주도에 보트체험하는곳중에 우도가 젤 재밌어요",
          "speaker_id": "1",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100033633.1.18",
          "form": "우도 서빈백사 보트장이요!",
          "original_form": "우도 서빈백사 보트장이요!",
          "speaker_id": "1",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100033633.1.19",
          "form": "다음에 가면 꼭 해야겠어요ㅠㅠ후회안하게 ㅜㅋㅋㅋ",
          "original_form": "다음에 가면 꼭 해야겠어요ㅠㅠ후회안하게 ㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:58"
        }
      ]
