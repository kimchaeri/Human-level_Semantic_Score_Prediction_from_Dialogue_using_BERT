      "utterance": [
        {
          "id": "MDRW2100025543.1.1",
          "form": "어떤 음료 좋아하세요?",
          "original_form": "어떤 음료 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025543.1.2",
          "form": "탄산 좋아해요",
          "original_form": "탄산 좋아해요",
          "speaker_id": "2",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025543.1.3",
          "form": "커피도 가끔 마시구요",
          "original_form": "커피도 가끔 마시구요",
          "speaker_id": "2",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025543.1.4",
          "form": "탄산 요즘 제로 시리즈 좋더라구요",
          "original_form": "탄산 요즘 제로 시리즈 좋더라구요",
          "speaker_id": "1",
          "time": "20210901 13:05"
        },
        {
          "id": "MDRW2100025543.1.5",
          "form": "약간 밍밍하긴하지만",
          "original_form": "약간 밍밍하긴하지만",
          "speaker_id": "1",
          "time": "20210901 13:05"
        },
        {
          "id": "MDRW2100025543.1.6",
          "form": "펩시 제로 맛있더라구요",
          "original_form": "펩시 제로 맛있더라구요",
          "speaker_id": "2",
          "time": "20210901 13:06"
        },
        {
          "id": "MDRW2100025543.1.7",
          "form": "큰 병은 안나오긴 하지만요",
          "original_form": "큰 병은 안나오긴 하지만요",
          "speaker_id": "2",
          "time": "20210901 13:06"
        },
        {
          "id": "MDRW2100025543.1.8",
          "form": "오 저는 칠성제로밖에 못먹어봣어요",
          "original_form": "오 저는 칠성제로밖에 못먹어봣어요",
          "speaker_id": "1",
          "time": "20210901 13:06"
        },
        {
          "id": "MDRW2100025543.1.9",
          "form": "맛이 궁금해지네요",
          "original_form": "맛이 궁금해지네요",
          "speaker_id": "1",
          "time": "20210901 13:07"
        },
        {
          "id": "MDRW2100025543.1.10",
          "form": "커피는 달달한 카라멜 같은거만 먹다가 요즘은 아메리카노만 먹네요",
          "original_form": "커피는 달달한 카라멜 같은거만 먹다가 요즘은 아메리카노만 먹네요",
          "speaker_id": "1",
          "time": "20210901 13:07"
        },
        {
          "id": "MDRW2100025543.1.11",
          "form": "잠 깨는데는 아메리카노가 좋져ㅎ",
          "original_form": "잠 깨는데는 아메리카노가 좋져ㅎ",
          "speaker_id": "2",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025543.1.12",
          "form": "전 더워서 시원한맛에 먹는거 같아요 ㅎㅎ 구수한맛이랑",
          "original_form": "전 더워서 시원한맛에 먹는거 같아요 ㅎㅎ 구수한맛이랑",
          "speaker_id": "1",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025543.1.13",
          "form": "배달 음식 자주 드시나요?",
          "original_form": "배달 음식 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025543.1.14",
          "form": "네 자주 먹어요",
          "original_form": "네 자주 먹어요",
          "speaker_id": "2",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025543.1.15",
          "form": "치킨 자주 먹는거 같아요",
          "original_form": "치킨 자주 먹는거 같아요",
          "speaker_id": "2",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025543.1.16",
          "form": "오 저두요 어디 치킨 제일 좋아하세요",
          "original_form": "오 저두요 어디 치킨 제일 좋아하세요",
          "speaker_id": "1",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025543.1.17",
          "form": "노랑통닭이요",
          "original_form": "노랑통닭이요",
          "speaker_id": "2",
          "time": "20210901 13:28"
        },
        {
          "id": "MDRW2100025543.1.18",
          "form": "후라이드가 맛있더라구요",
          "original_form": "후라이드가 맛있더라구요",
          "speaker_id": "2",
          "time": "20210901 13:28"
        },
        {
          "id": "MDRW2100025543.1.19",
          "form": "오 기본 바삭파 이시군요",
          "original_form": "오 기본 바삭파 이시군요",
          "speaker_id": "1",
          "time": "20210901 13:30"
        },
        {
          "id": "MDRW2100025543.1.20",
          "form": "저는 교촌 허니콤보 좋아해요",
          "original_form": "저는 교촌 허니콤보 좋아해요",
          "speaker_id": "1",
          "time": "20210901 13:30"
        },
        {
          "id": "MDRW2100025543.1.21",
          "form": "허니콤보 맛있죠",
          "original_form": "허니콤보 맛있죠",
          "speaker_id": "2",
          "time": "20210901 13:34"
        },
        {
          "id": "MDRW2100025543.1.22",
          "form": "뿌링클 같은 메뉴도 좋아해요",
          "original_form": "뿌링클 같은 메뉴도 좋아해요",
          "speaker_id": "2",
          "time": "20210901 13:34"
        },
        {
          "id": "MDRW2100025543.1.23",
          "form": "뿌링클 좋죠 소스 듬뿍찍어서 ㅎㅎ",
          "original_form": "뿌링클 좋죠 소스 듬뿍찍어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:35"
        }
      ]
