      "utterance": [
        {
          "id": "MDRW2100015490.1.1",
          "form": "군대는 맨날 남친들한테만 들어왓는데",
          "original_form": "군대는 맨날 남친들한테만 들어왓는데",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015490.1.2",
          "form": "이번 남친은 의경 출신이라 잘 모르겠네요",
          "original_form": "이번 남친은 의경 출신이라 잘 모르겠네요",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015490.1.3",
          "form": "우와 신기하다",
          "original_form": "우와 신기하다",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015490.1.4",
          "form": "나름대로 특전사, 육군, 공군, 상근, 의경을 만났습니다",
          "original_form": "나름대로 특전사, 육군, 공군, 상근, 의경을 만났습니다",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015490.1.5",
          "form": "군대 컬렉터가 되었죠",
          "original_form": "군대 컬렉터가 되었죠",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015490.1.6",
          "form": "의경컬렉션이 완성되었어요",
          "original_form": "의경컬렉션이 완성되었어요",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015490.1.7",
          "form": "아 저는 남친 해군이래요",
          "original_form": "아 저는 남친 해군이래요",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015490.1.8",
          "form": "승선면제이지만 해군이라네요?",
          "original_form": "승선면제이지만 해군이라네요?",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015490.1.9",
          "form": "그게다에요 ㅋㅋㅋㅋㅋ",
          "original_form": "그게다에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015490.1.10",
          "form": "오 해군으로 분류되는군요",
          "original_form": "오 해군으로 분류되는군요",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015490.1.11",
          "form": "제 대학동기들은 전부 gis병으로 가서 지도를 그렸습니다",
          "original_form": "제 대학동기들은 전부 gis병으로 가서 지도를 그렸습니다",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015490.1.12",
          "form": "우와 그런게잇나요 신기합니다",
          "original_form": "우와 그런게잇나요 신기합니다",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015490.1.13",
          "form": "지도를 보며 그분들께 감사를",
          "original_form": "지도를 보며 그분들께 감사를",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015490.1.14",
          "form": "ㅋㅋㅋㅋㅋ군대 정말 대단합니다",
          "original_form": "ㅋㅋㅋㅋㅋ군대 정말 대단합니다",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015490.1.15",
          "form": "요즘 애들이 맨날 여성징병제 얘기하는게",
          "original_form": "요즘 애들이 맨날 여성징병제 얘기하는게",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015490.1.16",
          "form": "짜증이 나지만 들어주고 있어요",
          "original_form": "짜증이 나지만 들어주고 있어요",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015490.1.17",
          "form": "아 무슨 논리를 펼치나요 궁금해요",
          "original_form": "아 무슨 논리를 펼치나요 궁금해요",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015490.1.18",
          "form": "여성들도 군대 가야한다고들 하죠",
          "original_form": "여성들도 군대 가야한다고들 하죠",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015490.1.19",
          "form": "걍 별논리는 없어요",
          "original_form": "걍 별논리는 없어요",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015490.1.20",
          "form": "걍 자기들만 가는게 억울하다는 건데",
          "original_form": "걍 자기들만 가는게 억울하다는 건데",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015490.1.21",
          "form": "억울하다 생각하는군요ㅠㅠ",
          "original_form": "억울하다 생각하는군요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015490.1.22",
          "form": "그래요 뭐 여성도 가도되지만 여성에겐 주어진 역할이 너무 많은데ㅠㅠ",
          "original_form": "그래요 뭐 여성도 가도되지만 여성에겐 주어진 역할이 너무 많은데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015490.1.23",
          "form": "맞아요 자기들은 뭘 하지도 않으면서 왜 억울해하지...",
          "original_form": "맞아요 자기들은 뭘 하지도 않으면서 왜 억울해하지...",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015490.1.24",
          "form": "암튼 근데 총쏴보고는 싶네요",
          "original_form": "암튼 근데 총쏴보고는 싶네요",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015490.1.25",
          "form": "힘들것같긴하지만서도 성별로 구분지어 가야한다는 건 좀  그래요",
          "original_form": "힘들것같긴하지만서도 성별로 구분지어 가야한다는 건 좀  그래요",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015490.1.26",
          "form": "그러게요 군대 문제는 참 어렵습니다",
          "original_form": "그러게요 군대 문제는 참 어렵습니다",
          "speaker_id": "2",
          "time": "20210825 23:22"
        }
      ]
