      "utterance": [
        {
          "id": "MDRW2100041823.1.1",
          "form": "그래서 영화 언제나온다고?",
          "original_form": "그래서 영화 언제나온다고?",
          "speaker_id": "1",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100041823.1.2",
          "form": "ㅋㅋㅋ너무 이르게 찾는거 아녀?",
          "original_form": "ㅋㅋㅋ너무 이르게 찾는거 아녀?",
          "speaker_id": "2",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100041823.1.3",
          "form": "책 내용 기반으로 그래도 가면 좋겠는데",
          "original_form": "책 내용 기반으로 그래도 가면 좋겠는데",
          "speaker_id": "2",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100041823.1.4",
          "form": "각색 적당히 제발...",
          "original_form": "각색 적당히 제발...",
          "speaker_id": "2",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100041823.1.5",
          "form": "웹툰은 멀쩡히 잘하고 있어서 좋은데",
          "original_form": "웹툰은 멀쩡히 잘하고 있어서 좋은데",
          "speaker_id": "2",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100041823.1.6",
          "form": "영화는 아무래도 어려울걸 알아서",
          "original_form": "영화는 아무래도 어려울걸 알아서",
          "speaker_id": "2",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100041823.1.7",
          "form": "차라리 애니메이션으로 더 빨리 나와주면 좋겠어",
          "original_form": "차라리 애니메이션으로 더 빨리 나와주면 좋겠어",
          "speaker_id": "2",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100041823.1.8",
          "form": "제발 각색 하지말고 내줬으면..",
          "original_form": "제발 각색 하지말고 내줬으면..",
          "speaker_id": "1",
          "time": "20210906 16:21"
        },
        {
          "id": "MDRW2100041823.1.9",
          "form": "상영시간 3시간 이래도 괜찮으니까",
          "original_form": "상영시간 3시간 이래도 괜찮으니까",
          "speaker_id": "1",
          "time": "20210906 16:21"
        },
        {
          "id": "MDRW2100041823.1.10",
          "form": "어차피 5부로 낼거면 최대한 꾹꾹 눌러담아서 내줘 ㅋㅋㅋ쿠ㅜ",
          "original_form": "어차피 5부로 낼거면 최대한 꾹꾹 눌러담아서 내줘 ㅋㅋㅋ쿠ㅜ",
          "speaker_id": "1",
          "time": "20210906 16:21"
        },
        {
          "id": "MDRW2100041823.1.11",
          "form": "어... 아.... 그건 좀",
          "original_form": "어... 아.... 그건 좀",
          "speaker_id": "2",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100041823.1.12",
          "form": "3시간...",
          "original_form": "3시간...",
          "speaker_id": "2",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100041823.1.13",
          "form": "그럴거면 차라리 드라마를...",
          "original_form": "그럴거면 차라리 드라마를...",
          "speaker_id": "2",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100041823.1.14",
          "form": "왜 타이타닉도 러닝타임 3시간 나오는걸ㅋㅋㅋ",
          "original_form": "왜 타이타닉도 러닝타임 3시간 나오는걸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100041823.1.15",
          "form": "다른 부분은 몰라도 1부는 2시간 채우려먄 많이 쳐내야할것 같단 말여ㅜ",
          "original_form": "다른 부분은 몰라도 1부는 2시간 채우려먄 많이 쳐내야할것 같단 말여ㅜ",
          "speaker_id": "1",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100041823.1.16",
          "form": "본적 없어서 몰랐네",
          "original_form": "본적 없어서 몰랐네",
          "speaker_id": "2",
          "time": "20210906 16:34"
        },
        {
          "id": "MDRW2100041823.1.17",
          "form": "영화 대부 라고 알아?",
          "original_form": "영화 대부 라고 알아?",
          "speaker_id": "2",
          "time": "20210906 16:34"
        },
        {
          "id": "MDRW2100041823.1.18",
          "form": "그 영화는 러닝타임중에 중간에 쉬는 시간도 있어",
          "original_form": "그 영화는 러닝타임중에 중간에 쉬는 시간도 있어",
          "speaker_id": "2",
          "time": "20210906 16:34"
        },
        {
          "id": "MDRW2100041823.1.19",
          "form": "영화는 아는데 안봤어",
          "original_form": "영화는 아는데 안봤어",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100041823.1.20",
          "form": "77년도 영화넼ㅋㅋ",
          "original_form": "77년도 영화넼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041823.1.21",
          "form": "그럼 영화 코요테 어글리 봄?",
          "original_form": "그럼 영화 코요테 어글리 봄?",
          "speaker_id": "2",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100041823.1.22",
          "form": "난 이거도 재미지게 봤는데",
          "original_form": "난 이거도 재미지게 봤는데",
          "speaker_id": "2",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100041823.1.23",
          "form": "놉..",
          "original_form": "놉..",
          "speaker_id": "1",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100041823.1.24",
          "form": "영화 머바써요...",
          "original_form": "영화 머바써요...",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100041823.1.25",
          "form": "어바웃 타임 봤니?",
          "original_form": "어바웃 타임 봤니?",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100041823.1.26",
          "form": "ㅇㅇ 그건 봄",
          "original_form": "ㅇㅇ 그건 봄",
          "speaker_id": "1",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100041823.1.27",
          "form": "영화관에서 봤다우",
          "original_form": "영화관에서 봤다우",
          "speaker_id": "1",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100041823.1.28",
          "form": "그렇다면...",
