      "utterance": [
        {
          "id": "MDRW2100040941.1.1",
          "form": "샤이니는 정말 전무후무한 것 같아요",
          "original_form": "샤이니는 정말 전무후무한 것 같아요",
          "speaker_id": "2",
          "time": "20210905 13:23"
        },
        {
          "id": "MDRW2100040941.1.2",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210905 13:23"
        },
        {
          "id": "MDRW2100040941.1.3",
          "form": "태민이 엄청 말라서",
          "original_form": "태민이 엄청 말라서",
          "speaker_id": "2",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100040941.1.4",
          "form": "뭐든 소화해내는 거 같지 않나요",
          "original_form": "뭐든 소화해내는 거 같지 않나요",
          "speaker_id": "2",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100040941.1.5",
          "form": "헐 맞아요 ㅠ.^ 솔직히 아이돌들 엄청 마른 거 보묜",
          "original_form": "헐 맞아요 ㅠ.^ 솔직히 아이돌들 엄청 마른 거 보묜",
          "speaker_id": "1",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100040941.1.6",
          "form": "마음 아파서 좀 그런데 확실히 무대할 때",
          "original_form": "마음 아파서 좀 그런데 확실히 무대할 때",
          "speaker_id": "1",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100040941.1.7",
          "form": "선이 예쁘더라고요!",
          "original_form": "선이 예쁘더라고요!",
          "speaker_id": "1",
          "time": "20210905 13:25"
        },
        {
          "id": "MDRW2100040941.1.8",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210905 13:26"
        },
        {
          "id": "MDRW2100040941.1.9",
          "form": "무용수들이 괜히 마른 게 아니구나 생각하게 되는...?",
          "original_form": "무용수들이 괜히 마른 게 아니구나 생각하게 되는...?",
          "speaker_id": "2",
          "time": "20210905 13:26"
        },
        {
          "id": "MDRW2100040941.1.10",
          "form": "별로 먹지도 못할텐데 어떻게 그렇게",
          "original_form": "별로 먹지도 못할텐데 어떻게 그렇게",
          "speaker_id": "2",
          "time": "20210905 13:26"
        },
        {
          "id": "MDRW2100040941.1.11",
          "form": "춤을 부숴져라 추는지!!!",
          "original_form": "춤을 부숴져라 추는지!!!",
          "speaker_id": "2",
          "time": "20210905 13:26"
        },
        {
          "id": "MDRW2100040941.1.12",
          "form": "그니까요 저엿음 3분 추고 30분 누워있었을 걸요 -.- ?",
          "original_form": "그니까요 저엿음 3분 추고 30분 누워있었을 걸요 -.- ?",
          "speaker_id": "1",
          "time": "20210905 13:28"
        },
        {
          "id": "MDRW2100040941.1.13",
          "form": "아 ㅋㅋㅋㅋㅋ 맞아요 저도요!!!!!",
          "original_form": "아 ㅋㅋㅋㅋㅋ 맞아요 저도요!!!!!",
          "speaker_id": "2",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100040941.1.14",
          "form": "물론 반의 반의 반도 흉내 못내지만...",
          "original_form": "물론 반의 반의 반도 흉내 못내지만...{emoji:☆}",
          "speaker_id": "2",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100040941.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ ㅠ.^",
          "original_form": "ㅋㅋㅋㅋㅋㅋ ㅠ.^",
          "speaker_id": "1",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100040941.1.16",
          "form": "name2님 춤추는 영상 보는 것도",
          "original_form": "&name2&님 춤추는 영상 보는 것도",
          "speaker_id": "1",
          "time": "20210905 13:30"
        },
        {
