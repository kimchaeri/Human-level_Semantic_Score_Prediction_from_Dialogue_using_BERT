        },
        {
          "id": "MDRW2100045124.1.2",
          "form": "ㅎㅎ...",
          "original_form": "ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045124.1.3",
          "form": "마크 좋아하시려나",
          "original_form": "마크 좋아하시려나",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045124.1.4",
          "form": "ㄴㄴ",
          "original_form": "ㄴㄴ",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045124.1.5",
          "form": "재현",
          "original_form": "재현",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045124.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045124.1.7",
          "form": "을 제일 좋아하는거같으",
          "original_form": "을 제일 좋아하는거같으",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045124.1.8",
          "form": "걔가 잘생긴 걘가",
          "original_form": "걔가 잘생긴 걘가",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045124.1.9",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045124.1.10",
          "form": "ㅣ성이랑",
          "original_form": "ㅣ성이랑",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045124.1.11",
          "form": "지성",
          "original_form": "지성",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045124.1.12",
          "form": "ㅋㅋㅋㅋㅋ그친구가",
          "original_form": "ㅋㅋㅋㅋㅋ그친구가",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045124.1.13",
          "form": "신화는 떡밥이없으니",
          "original_form": "신화는 떡밥이없으니",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045124.1.14",
          "form": "빠진게 스맙의 섹시존인데",
          "original_form": "빠진게 스맙의 섹시존인데",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045124.1.15",
          "form": "거기도 만만치않은 가시밭길이라",
          "original_form": "거기도 만만치않은 가시밭길이라",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045124.1.16",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045124.1.17",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045124.1.18",
          "form": "쉬운길은",
          "original_form": "쉬운길은",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045124.1.19",
          "form": "택하지 않으시는구나",
          "original_form": "택하지 않으시는구나",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045124.1.20",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045124.1.21",
          "form": "거기너무힘든거같더라",
          "original_form": "거기너무힘든거같더라",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045124.1.22",
          "form": "그 일아이돌은",
          "original_form": "그 일아이돌은",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045124.1.23",
          "form": "일본 방송이 너무 애들 소비하는게",
          "original_form": "일본 방송이 너무 애들 소비하는게",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045124.1.24",
          "form": "저렴해서 스트레스받는거같았음",
          "original_form": "저렴해서 스트레스받는거같았음",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045124.1.25",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045124.1.26",
          "form": "거기에 어떤애가",
          "original_form": "거기에 어떤애가",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045124.1.27",
          "form": "여돌이 제일 헬이긴 하지만",
          "original_form": "여돌이 제일 헬이긴 하지만",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045124.1.28",
          "form": "그렇다 해서 남돌도",
          "original_form": "그렇다 해서 남돌도",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045124.1.29",
          "form": "몸이 좋은데",
          "original_form": "몸이 좋은데",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045124.1.30",
          "form": "취급이 좋은건 아닐테니",
          "original_form": "취급이 좋은건 아닐테니",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045124.1.31",
          "form": "근육바보 캐를 부여해서",
          "original_form": "근육바보 캐를 부여해서",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045124.1.32",
          "form": "스트레슬ㄹ 받아했대",
          "original_form": "스트레슬ㄹ 받아했대",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045124.1.33",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045124.1.34",
          "form": "그러니까 자꾸 몸좋은거 거자고",
          "original_form": "그러니까 자꾸 몸좋은거 거자고",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045124.1.35",
          "form": "가지고",
          "original_form": "가지고",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045124.1.36",
          "form": "걔네 캐릭터 하나 정해주고",
          "original_form": "걔네 캐릭터 하나 정해주고",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045124.1.37",
          "form": "애를 이상한 밥ㅇ향으로 소비함",
          "original_form": "애를 이상한 밥ㅇ향으로 소비함",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045124.1.38",
          "form": "뇌절하는거",
          "original_form": "뇌절하는거",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045124.1.39",
          "form": "그 일본 몰카도 엽기적으로 하잖아여",
          "original_form": "그 일본 몰카도 엽기적으로 하잖아여",
          "speaker_id": "1",
          "time": "20210908 01:00"
        }
      ]
    }
