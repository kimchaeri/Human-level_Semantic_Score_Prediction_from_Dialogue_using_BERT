      "utterance": [
        {
          "id": "MDRW2100013445.1.1",
          "form": "요즘 날씨 어떤가요...",
          "original_form": "요즘 날씨 어떤가요...",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013445.1.2",
          "form": "너무 오락가락하는거 같지 않아요?",
          "original_form": "너무 오락가락하는거 같지 않아요?",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013445.1.3",
          "form": "맞아요 더웠다가 비오고 ㅋㅋ",
          "original_form": "맞아요 더웠다가 비오고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013445.1.4",
          "form": "자꾸 그게 반복되는까 좀 짜증나요 ㅋㅋㅋ",
          "original_form": "자꾸 그게 반복되는까 좀 짜증나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013445.1.5",
          "form": "동감해요 우산챙기는게 참...",
          "original_form": "동감해요 우산챙기는게 참...",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013445.1.6",
          "form": "ㅋㅋㅋ 저두 그 생각했어요!",
          "original_form": "ㅋㅋㅋ 저두 그 생각했어요!",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013445.1.7",
          "form": "ㅋㅋㅋㅋㅋ그렇군요 우산무겁잖아요 ㅜ",
          "original_form": "ㅋㅋㅋㅋㅋ그렇군요 우산무겁잖아요 ㅜ",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013445.1.8",
          "form": "맞아요 그렇다고 너무 작은거 챙기면 비 다맞고",
          "original_form": "맞아요 그렇다고 너무 작은거 챙기면 비 다맞고",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013445.1.9",
          "form": "저는 그래서 경량우산 사용중",
          "original_form": "저는 그래서 경량우산 사용중",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013445.1.10",
          "form": "오 그거 괜찮나요???",
          "original_form": "오 그거 괜찮나요???",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013445.1.11",
          "form": "네 ㅋㅋㅋㅋ 가벼우니까 훨씬 좋죠",
          "original_form": "네 ㅋㅋㅋㅋ 가벼우니까 훨씬 좋죠",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013445.1.12",
          "form": "그럼 저도 대비해서 하나 장만해야겠네요",
          "original_form": "그럼 저도 대비해서 하나 장만해야겠네요",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013445.1.13",
          "form": "넵 좋은거 사시길 바랄게요 ㅋㅋㅋ",
          "original_form": "넵 좋은거 사시길 바랄게요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:35"
        }
