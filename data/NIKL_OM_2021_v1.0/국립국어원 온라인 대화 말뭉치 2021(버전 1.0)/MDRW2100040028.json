      "utterance": [
        {
          "id": "MDRW2100040028.1.1",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100040028.1.2",
          "form": "야에가",
          "original_form": "야에가",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100040028.1.3",
          "form": "여기서 나온다고???",
          "original_form": "여기서 나온다고???",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100040028.1.4",
          "form": "그치 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그치 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100040028.1.5",
          "form": "내 반응이 이해되지 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "내 반응이 이해되지 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100040028.1.6",
          "form": "왜 얘가 전대라는 추측있는지",
          "original_form": "왜 얘가 전대라는 추측있는지",
          "speaker_id": "2",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100040028.1.7",
          "form": "이해 가네",
          "original_form": "이해 가네",
          "speaker_id": "2",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100040028.1.8",
          "form": "종려랑 라이덴 둘 다한테 반말까네",
          "original_form": "종려랑 라이덴 둘 다한테 반말까네",
          "speaker_id": "2",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100040028.1.9",
          "form": "그리고 종려도 본적 있는것같고",
          "original_form": "그리고 종려도 본적 있는것같고",
          "speaker_id": "2",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100040028.1.10",
          "form": "근데 설정이 너무",
          "original_form": "근데 설정이 너무",
          "speaker_id": "2",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100040028.1.11",
          "form": "터무니없는데요.....",
          "original_form": "터무니없는데요.....",
          "speaker_id": "2",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100040028.1.12",
          "form": "갑자기 라이덴이",
          "original_form": "갑자기 라이덴이",
          "speaker_id": "2",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100040028.1.13",
          "form": "자아는 어디 틀어박혀있고",
          "original_form": "자아는 어디 틀어박혀있고",
          "speaker_id": "2",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100040028.1.14",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋ 그니까",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋ 그니까",
          "speaker_id": "1",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100040028.1.15",
          "form": "움직이는건 인형이라고?",
          "original_form": "움직이는건 인형이라고?",
          "speaker_id": "2",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100040028.1.16",
          "form": "얼탱.....",
          "original_form": "얼탱.....",
          "speaker_id": "2",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100040028.1.17",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100040028.1.18",
          "form": "이게 바로 세탁기...?",
          "original_form": "이게 바로 세탁기...?",
          "speaker_id": "1",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100040028.1.19",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100040028.1.20",
          "form": "누가봐도 세탁기",
          "original_form": "누가봐도 세탁기",
          "speaker_id": "2",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100040028.1.21",
          "form": "플레이어블로 팔아야해서 그러나?",
          "original_form": "플레이어블로 팔아야해서 그러나?",
          "speaker_id": "1",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100040028.1.22",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100040028.1.23",
          "form": "사실 죄는 껍데기가 지은거고",
          "original_form": "사실 죄는 껍데기가 지은거고",
          "speaker_id": "2",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100040028.1.24",
          "form": "알맹이는 심상수련이나 하고있었다?",
          "original_form": "알맹이는 심상수련이나 하고있었다?",
          "speaker_id": "2",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100040028.1.25",
          "form": "차라리 껍데기가 본체인게",
          "original_form": "차라리 껍데기가 본체인게",
          "speaker_id": "1",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100040028.1.26",
          "form": "독특함이나 있을듯",
          "original_form": "독특함이나 있을듯",
          "speaker_id": "1",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100040028.1.27",
          "form": "ㅇㅇ차라리 진짜 우인단에게 넘어가서",
          "original_form": "ㅇㅇ차라리 진짜 우인단에게 넘어가서",
          "speaker_id": "2",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100040028.1.28",
          "form": "초기 삼봉행을 앗아간 시간이 싫어서",
          "original_form": "초기 삼봉행을 앗아간 시간이 싫어서",
          "speaker_id": "2",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100040028.1.29",
          "form": "영원을 추구하기 위해 안수령 내렸단게",
          "original_form": "영원을 추구하기 위해 안수령 내렸단게",
          "speaker_id": "2",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100040028.1.30",
          "form": "더 매력있었을듯",
          "original_form": "더 매력있었을듯",
          "speaker_id": "2",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100040028.1.31",
          "form": "이게 무슨 신이야",
          "original_form": "이게 무슨 신이야",
          "speaker_id": "2",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100040028.1.32",
          "form": "ㅇㅇ 그리고 그게 더 통일성 있지 않나",
          "original_form": "ㅇㅇ 그리고 그게 더 통일성 있지 않나",
          "speaker_id": "1",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100040028.1.33",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100040028.1.34",
          "form": "놀랍게도",
          "original_form": "놀랍게도",
          "speaker_id": "1",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100040028.1.35",
          "form": "막장은 이제부터 시작임",
          "original_form": "막장은 이제부터 시작임",
          "speaker_id": "1",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100040028.1.36",
          "form": "ㄹㅇ멘탈 상태",
          "original_form": "ㄹㅇ멘탈 상태",
          "speaker_id": "2",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100040028.1.37",
          "form": "겁나 쓔렉이네",
          "original_form": "겁나 쓔렉이네",
          "speaker_id": "2",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100040028.1.38",
          "form": "일단 이 특훈이 그",
          "original_form": "일단 이 특훈이 그",
          "speaker_id": "2",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100040028.1.39",
          "form": "안맞기인건",
          "original_form": "안맞기인건",
          "speaker_id": "2",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100040028.1.40",
          "form": "벌써 알것같아",
          "original_form": "벌써 알것같아",
          "speaker_id": "2",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100040028.1.41",
          "form": "ㅇㅇ 라이덴이랑 붙는거",
          "original_form": "ㅇㅇ 라이덴이랑 붙는거",
          "speaker_id": "1",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100040028.1.42",
          "form": "1차는 주인공 포함 캐들의 체력이 일정수준 이하로 까이면서 버티는거같고",
          "original_form": "1차는 주인공 포함 캐들의 체력이 일정수준 이하로 까이면서 버티는거같고",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100040028.1.43",
          "form": "2차는 ㅎ",
          "original_form": "2차는 ㅎ",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100040028.1.44",
          "form": "신경 안써도 됨",
          "original_form": "신경 안써도 됨",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100040028.1.45",
          "form": "이럴거면 차라리 팔지 말고",
          "original_form": "이럴거면 차라리 팔지 말고",
          "speaker_id": "2",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100040028.1.46",
          "form": "근데 스토리가 너무 막장이라",
          "original_form": "근데 스토리가 너무 막장이라",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100040028.1.47",
          "form": "전투에 신경 쓸 시간이 없어짐",
          "original_form": "전투에 신경 쓸 시간이 없어짐",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100040028.1.48",
          "form": "주인공이 라이덴 끌어내려서",
          "original_form": "주인공이 라이덴 끌어내려서",
          "speaker_id": "2",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100040028.1.49",
          "form": "처단하는",
          "original_form": "처단하는",
          "speaker_id": "2",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100040028.1.50",
          "form": "전개가",
          "original_form": "전개가",
          "speaker_id": "2",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100040028.1.51",
          "form": "더 나았을것같은데",
          "original_form": "더 나았을것같은데",
          "speaker_id": "2",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100040028.1.52",
          "form": "ㅋㅋㅋㅋㅋㅋ 그러기엔 주인공이",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그러기엔 주인공이",
          "speaker_id": "1",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100040028.1.53",
          "form": "넘 약함...",
          "original_form": "넘 약함...",
          "speaker_id": "1",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100040028.1.54",
          "form": "그니까 수련을 시켜야지",
          "original_form": "그니까 수련을 시켜야지",
          "speaker_id": "2",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100040028.1.55",
          "form": "아니 아무리 그래도 원소를 세개를 쓰는데",
          "original_form": "아니 아무리 그래도 원소를 세개를 쓰는데",
          "speaker_id": "2",
          "time": "20210904 01:02"
        }
      ]
