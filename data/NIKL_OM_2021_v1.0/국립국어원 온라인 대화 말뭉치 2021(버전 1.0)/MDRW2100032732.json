      "utterance": [
        {
          "id": "MDRW2100032732.1.1",
          "form": "요즘 직장생활 어떠니",
          "original_form": "요즘 직장생활 어떠니",
          "speaker_id": "1",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100032732.1.2",
          "form": "저번주? 이번주?... 너무 서터레스었어",
          "original_form": "저번주? 이번주?... 너무 서터레스었어",
          "speaker_id": "2",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032732.1.3",
          "form": "와이 ㅜ",
          "original_form": "와이 ㅜ",
          "speaker_id": "1",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032732.1.4",
          "form": "원래 스근하게 넘어가는 일인데",
          "original_form": "원래 스근하게 넘어가는 일인데",
          "speaker_id": "2",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032732.1.5",
          "form": "새로 들어온 사람이 사사건건 태클 걸어서 ㅋㅋㅋ",
          "original_form": "새로 들어온 사람이 사사건건 태클 걸어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032732.1.6",
          "form": "헐ㅋㅋㅋㅋㅋ태클?",
          "original_form": "헐ㅋㅋㅋㅋㅋ태클?",
          "speaker_id": "1",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032732.1.7",
          "form": "ㅇㅇ...내가 하는게 회사 규정 검토하는 거거든",
          "original_form": "ㅇㅇ...내가 하는게 회사 규정 검토하는 거거든",
          "speaker_id": "2",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032732.1.8",
          "form": "그 사람은 감사실에서 최종 검토하는데",
          "original_form": "그 사람은 감사실에서 최종 검토하는데",
          "speaker_id": "2",
          "time": "20210905 22:55"
        },
        {
          "id": "MDRW2100032732.1.9",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210905 22:55"
        },
        {
          "id": "MDRW2100032732.1.10",
          "form": "거의 법률 수준으로 검토함ㅋㅋㅋㅋ",
          "original_form": "거의 법률 수준으로 검토함ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:55"
        },
        {
          "id": "MDRW2100032732.1.11",
          "form": "연관잇는일을하능군",
          "original_form": "연관잇는일을하능군",
          "speaker_id": "1",
          "time": "20210905 22:55"
        },
        {
          "id": "MDRW2100032732.1.12",
          "form": "헐 법률 ㅋㅋㅋㅋ",
          "original_form": "헐 법률 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:55"
        },
        {
          "id": "MDRW2100032732.1.13",
          "form": "전문적인 업무이신가",
          "original_form": "전문적인 업무이신가",
          "speaker_id": "1",
          "time": "20210905 22:55"
        },
        {
          "id": "MDRW2100032732.1.14",
          "form": "ㅇㅇ 그사람은 변호사",
          "original_form": "ㅇㅇ 그사람은 변호사",
          "speaker_id": "2",
          "time": "20210905 22:55"
        },
        {
          "id": "MDRW2100032732.1.15",
          "form": "그랴서 요즘 변호사 혐오생김",
          "original_form": "그랴서 요즘 변호사 혐오생김",
          "speaker_id": "2",
          "time": "20210905 22:56"
        },
        {
          "id": "MDRW2100032732.1.16",
          "form": "헐ㅋㅋㅋㅋ근데 원래하는 업무아녓슴??",
          "original_form": "헐ㅋㅋㅋㅋ근데 원래하는 업무아녓슴??",
          "speaker_id": "1",
          "time": "20210905 22:56"
        },
        {
          "id": "MDRW2100032732.1.17",
          "form": "그 전 사람은 좀 덜 태클?",
          "original_form": "그 전 사람은 좀 덜 태클?",
          "speaker_id": "1",
          "time": "20210905 22:56"
        },
        {
          "id": "MDRW2100032732.1.18",
          "form": "그 사람 오기전에는 그냥 대충",
          "original_form": "그 사람 오기전에는 그냥 대충",
          "speaker_id": "2",
          "time": "20210905 22:56"
        },
        {
          "id": "MDRW2100032732.1.19",
          "form": "크게 이상한 것만 지적하는 수준?",
          "original_form": "크게 이상한 것만 지적하는 수준?",
          "speaker_id": "2",
          "time": "20210905 22:56"
        },
        {
          "id": "MDRW2100032732.1.20",
          "form": "하 그럼 이번사람은 좀 에프엠적으로 디테일하게보나보다...으",
          "original_form": "하 그럼 이번사람은 좀 에프엠적으로 디테일하게보나보다...으",
          "speaker_id": "1",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100032732.1.21",
          "form": "근데 너무 심하게 해서...내 말고도",
          "original_form": "근데 너무 심하게 해서...내 말고도",
          "speaker_id": "2",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100032732.1.22",
          "form": "몇사람 그 사람한테 잡힌거 같도라구",
          "original_form": "몇사람 그 사람한테 잡힌거 같도라구",
          "speaker_id": "2",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100032732.1.23",
          "form": "헐 진짜 깐깐한가보다",
          "original_form": "헐 진짜 깐깐한가보다",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032732.1.24",
          "form": "어엉...그래서 이번에는 대응자료 모아서 팀장님이 직접 말햐본대...",
          "original_form": "어엉...그래서 이번에는 대응자료 모아서 팀장님이 직접 말햐본대...",
          "speaker_id": "2",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032732.1.25",
          "form": "그 사람 아니었으면 이미 지난 월요일에 끝났을 일인데 에혀 ....",
          "original_form": "그 사람 아니었으면 이미 지난 월요일에 끝났을 일인데 에혀 ....",
          "speaker_id": "2",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032732.1.26",
          "form": "아낰ㅋㅋㅋㅋ팀장급이 나서서 해결해야하나보네",
          "original_form": "아낰ㅋㅋㅋㅋ팀장급이 나서서 해결해야하나보네",
          "speaker_id": "1",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032732.1.27",
          "form": "내가 가면 또 어버버하다가 빠꾸먹을까봐 그런듯 ㅋㅋㅋㅋ",
          "original_form": "내가 가면 또 어버버하다가 빠꾸먹을까봐 그런듯 ㅋㅋㅋㅋ",
          "speaker_id": "2",
