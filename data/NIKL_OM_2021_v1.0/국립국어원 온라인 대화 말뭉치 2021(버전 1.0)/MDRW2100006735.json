      "utterance": [
        {
          "id": "MDRW2100006735.1.1",
          "form": "포르투갈이 좀 옛날 유럽 느낌이라 좋았어요.",
          "original_form": "포르투갈이 좀 옛날 유럽 느낌이라 좋았어요.",
          "speaker_id": "2",
          "time": "20210908 08:07"
        },
        {
          "id": "MDRW2100006735.1.2",
          "form": "발전되기전 유럽느낌 ㅎㅎ",
          "original_form": "발전되기전 유럽느낌 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 08:08"
        },
        {
          "id": "MDRW2100006735.1.3",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210908 08:08"
        },
        {
          "id": "MDRW2100006735.1.4",
          "form": "파리는 뭐가 좋으셧나요?",
          "original_form": "파리는 뭐가 좋으셧나요?",
          "speaker_id": "2",
          "time": "20210908 08:08"
        },
        {
          "id": "MDRW2100006735.1.5",
          "form": "직항이 있던가",
          "original_form": "직항이 있던가",
          "speaker_id": "1",
          "time": "20210908 08:08"
        },
        {
          "id": "MDRW2100006735.1.6",
          "form": "역시 도시 자체가 옛모습으로 보존된점?",
          "original_form": "역시 도시 자체가 옛모습으로 보존된점?",
          "speaker_id": "1",
          "time": "20210908 08:08"
        },
        {
          "id": "MDRW2100006735.1.7",
          "form": "유럽은 보존을 잘해놔서 그게 좋더라구요",
          "original_form": "유럽은 보존을 잘해놔서 그게 좋더라구요",
          "speaker_id": "2",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100006735.1.8",
          "form": "어떻게 보면 너무 불편한데ㅋㅋ",
          "original_form": "어떻게 보면 너무 불편한데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:11"
        },
        {
          "id": "MDRW2100006735.1.9",
          "form": "화장실ㅋㅋ",
          "original_form": "화장실ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:11"
        },
        {
          "id": "MDRW2100006735.1.10",
          "form": "네그건잘못된보존같아요",
          "original_form": "네그건잘못된보존같아요",
          "speaker_id": "2",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100006735.1.11",
          "form": "한국도 지하철 개조하는데 거긴 왜그러는지",
          "original_form": "한국도 지하철 개조하는데 거긴 왜그러는지",
          "speaker_id": "2",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100006735.1.12",
          "form": "한국이 그런 면에서는 확실히 매력적인 부분도 있는듯해요",
          "original_form": "한국이 그런 면에서는 확실히 매력적인 부분도 있는듯해요",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100006735.1.13",
          "form": "자주보면매력을 못느끼니 ㅎㅎ",
          "original_form": "자주보면매력을 못느끼니 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100006735.1.14",
          "form": "외국사람 입장에서는 매력있을 것 같은데ㅎㅎ",
          "original_form": "외국사람 입장에서는 매력있을 것 같은데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100006735.1.15",
          "form": "교통 좋고 편의점 좋고ㅋㅋ",
          "original_form": "교통 좋고 편의점 좋고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100006735.1.16",
          "form": "그쵸 여행하기편하고",
          "original_form": "그쵸 여행하기편하고",
          "speaker_id": "2",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100006735.1.17",
          "form": "치안도좋구요",
          "original_form": "치안도좋구요",
          "speaker_id": "2",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100006735.1.18",
          "form": "국민성도 이정도면 훌륭한 듯한데ㅎㅎ",
          "original_form": "국민성도 이정도면 훌륭한 듯한데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100006735.1.19",
          "form": "이번에 코로나 터지니 국민성도 확실히 보이더라구요",
          "original_form": "이번에 코로나 터지니 국민성도 확실히 보이더라구요",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100006735.1.20",
          "form": "외국은 거지가 많아서 깜짝놀랐어요",
          "original_form": "외국은 거지가 많아서 깜짝놀랐어요",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100006735.1.21",
          "form": "야바위꾼이랑ㅋ",
          "original_form": "야바위꾼이랑ㅋ",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100006735.1.22",
          "form": "진짜많아요",
          "original_form": "진짜많아요",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100006735.1.23",
          "form": "요새는 난민때메 더 심한듯",
          "original_form": "요새는 난민때메 더 심한듯",
          "speaker_id": "2",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100006735.1.24",
          "form": "예전엔 이정도까지는 아니었는데",
          "original_form": "예전엔 이정도까지는 아니었는데",
          "speaker_id": "2",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100006735.1.25",
          "form": "아 난민도 논란이 많지요",
          "original_form": "아 난민도 논란이 많지요",
