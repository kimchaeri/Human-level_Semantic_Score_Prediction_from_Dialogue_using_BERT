      "utterance": [
        {
          "id": "MDRW2100044417.1.1",
          "form": "나 배고파서 호두파이 또 잘라서 먹었다",
          "original_form": "나 배고파서 호두파이 또 잘라서 먹었다",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044417.1.2",
          "form": "배가 왜이리 자주 고픈거야",
          "original_form": "배가 왜이리 자주 고픈거야",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044417.1.3",
          "form": "ㅋㅋㅋ나도 샤인 또 먹음",
          "original_form": "ㅋㅋㅋ나도 샤인 또 먹음",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044417.1.4",
          "form": "그리고 유튜브서 음식 영상보다가",
          "original_form": "그리고 유튜브서 음식 영상보다가",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044417.1.5",
          "form": "유튜브 음식 영상 보다가",
          "original_form": "유튜브 음식 영상 보다가",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044417.1.6",
          "form": "샤인머스켓 식당에서받은거?",
          "original_form": "샤인머스켓 식당에서받은거?",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044417.1.7",
          "form": "아니 아는동생이 줬어ㅎ",
          "original_form": "아니 아는동생이 줬어ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044417.1.8",
          "form": "한송이 먹으라구ㅋㅋ",
          "original_form": "한송이 먹으라구ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044417.1.9",
          "form": "나 과일 돟아하는거 아는 애라ㅎ",
          "original_form": "나 과일 돟아하는거 아는 애라ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044417.1.10",
          "form": "오 좋네 ㅋㅋ",
          "original_form": "오 좋네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044417.1.11",
          "form": "과일도 유행이 있나봐 진짜",
          "original_form": "과일도 유행이 있나봐 진짜",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044417.1.12",
          "form": "샤인이 너무 비싸져서 미워",
          "original_form": "샤인이 너무 비싸져서 미워",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044417.1.13",
          "form": "뭐 하나뜨면 여기저기서 팔아 ㅋㅋ",
          "original_form": "뭐 하나뜨면 여기저기서 팔아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044417.1.14",
          "form": "자주 먹고싶은데",
          "original_form": "자주 먹고싶은데",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044417.1.15",
          "form": "그냥 수입산 포도랑 달라?",
          "original_form": "그냥 수입산 포도랑 달라?",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044417.1.16",
          "form": "맞아ㅋㅋㅋ울 나라만 그런지 모르겠는데 유행 심함ㅋ",
          "original_form": "맞아ㅋㅋㅋ울 나라만 그런지 모르겠는데 유행 심함ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044417.1.17",
          "form": "포도 식감이 당도가 다르긴해",
          "original_form": "포도 식감이 당도가 다르긴해",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044417.1.18",
          "form": "ㅇㅇ장차치들 장사잇속일듯 ㅋㅋ 마케팅",
          "original_form": "ㅇㅇ장차치들 장사잇속일듯 ㅋㅋ 마케팅",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044417.1.19",
          "form": "포도마다 다른것처럼",
          "original_form": "포도마다 다른것처럼",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044417.1.20",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044417.1.21",
          "form": "난 그래도 샤인보단 머루포도 좋아",
          "original_form": "난 그래도 샤인보단 머루포도 좋아",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044417.1.22",
          "form": "난 수입포도도 충분히 달던데",
          "original_form": "난 수입포도도 충분히 달던데",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044417.1.23",
          "form": "내 최애 머루포도ㅋ",
          "original_form": "내 최애 머루포도ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044417.1.24",
          "form": "샤인은 얼마나달기에",
          "original_form": "샤인은 얼마나달기에",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044417.1.25",
          "form": "머루포도 맛나지",
          "original_form": "머루포도 맛나지",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044417.1.26",
          "form": "나도 좋아해",
          "original_form": "나도 좋아해",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044417.1.27",
          "form": "수입 껍질 질기던데 내가 먹은게 그랬는가",
          "original_form": "수입 껍질 질기던데 내가 먹은게 그랬는가",
          "speaker_id": "2",
          "time": "20210907 22:45"
        }
      ]
    }
