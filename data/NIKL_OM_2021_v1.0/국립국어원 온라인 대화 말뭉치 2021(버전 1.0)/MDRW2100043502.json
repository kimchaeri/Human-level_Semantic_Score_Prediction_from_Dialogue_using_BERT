      "utterance": [
        {
          "id": "MDRW2100043502.1.1",
          "form": "누구랑 사시나요?",
          "original_form": "누구랑 사시나요?",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043502.1.2",
          "form": "부모님과 자매형제요ㅋㅋ",
          "original_form": "부모님과 자매형제요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043502.1.3",
          "form": "다들 독립전이에요ㅋㅋ",
          "original_form": "다들 독립전이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043502.1.4",
          "form": "엄마가 해주는 밥 먹고 다니시는군요 부럽네요",
          "original_form": "엄마가 해주는 밥 먹고 다니시는군요 부럽네요",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043502.1.5",
          "form": "ㅋㅋ넵ㅋㅋ",
          "original_form": "ㅋㅋ넵ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043502.1.6",
          "form": "주말엔 딸들이 하려고하는데 대부분 엄마가ㅋㅋ",
          "original_form": "주말엔 딸들이 하려고하는데 대부분 엄마가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043502.1.7",
          "form": "노노~ 독립하면 평생 해 드셔야 하니 지금은 엄마 밥 드세요",
          "original_form": "노노~ 독립하면 평생 해 드셔야 하니 지금은 엄마 밥 드세요",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043502.1.8",
          "form": "대신 설거지 도와드리시면 되고요",
          "original_form": "대신 설거지 도와드리시면 되고요",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043502.1.9",
          "form": "ㅋㅋ기혼친구들이 다 부러워하더라고요",
          "original_form": "ㅋㅋ기혼친구들이 다 부러워하더라고요",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043502.1.10",
          "form": "설거지는 대부분 저희가해요ㅋㅋ",
          "original_form": "설거지는 대부분 저희가해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043502.1.11",
          "form": "착한 딸들이네요 ㅎㅎ",
          "original_form": "착한 딸들이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043502.1.12",
          "form": "식세기 사고싶어요",
          "original_form": "식세기 사고싶어요",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043502.1.13",
          "form": "식세기는 독립하며 선물로 드리는건 어떠세요?",
          "original_form": "식세기는 독립하며 선물로 드리는건 어떠세요?",
          "speaker_id": "1",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043502.1.14",
          "form": "양심상ㅋㅋ 설거지는ㅋㅋ",
          "original_form": "양심상ㅋㅋ 설거지는ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043502.1.15",
          "form": "아마 결혼을... ㅋㅋ 안할거같아서ㅋㅋ",
          "original_form": "아마 결혼을... ㅋㅋ 안할거같아서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043502.1.16",
          "form": "왜요? 육아의 고통을 이미 아셨군요",
          "original_form": "왜요? 육아의 고통을 이미 아셨군요",
          "speaker_id": "1",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043502.1.17",
          "form": "ㅋㅋ익히 들어서ㅋㅋ",
          "original_form": "ㅋㅋ익히 들어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043502.1.18",
          "form": "어머니는 위대하죠ㅋㅋ",
          "original_form": "어머니는 위대하죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043502.1.19",
          "form": "하.. 그러니까요 저 일하며 육아하려니 넘 힘들어요",
          "original_form": "하.. 그러니까요 저 일하며 육아하려니 넘 힘들어요",
          "speaker_id": "1",
          "time": "20210907 20:00"
        }
