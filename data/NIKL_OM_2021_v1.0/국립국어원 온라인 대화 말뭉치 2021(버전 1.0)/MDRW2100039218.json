      "utterance": [
        {
          "id": "MDRW2100039218.1.1",
          "form": "류준열은 팔뚝이 최고에요ㅋㅋㅋ",
          "original_form": "류준열은 팔뚝이 최고에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:21"
        },
        {
          "id": "MDRW2100039218.1.2",
          "form": "ㅋㅋㅋㅋㅋㄱ 팔뚝 그렇죠\r\nㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱ 팔뚝 그렇죠\r\nㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039218.1.3",
          "form": "몸매가 정말 이쁜듯",
          "original_form": "몸매가 정말 이쁜듯",
          "speaker_id": "2",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039218.1.4",
          "form": "핏줄이 섹시할 줄이야ㅋㅋㅋ",
          "original_form": "핏줄이 섹시할 줄이야ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:37"
        },
        {
          "id": "MDRW2100039218.1.5",
          "form": "ㅋㅋㅋㅋ 그러게요",
          "original_form": "ㅋㅋㅋㅋ 그러게요",
          "speaker_id": "2",
          "time": "20210903 14:46"
        },
        {
          "id": "MDRW2100039218.1.6",
          "form": "다시 봐도 명장면 ㅎㅎ",
          "original_form": "다시 봐도 명장면 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 14:46"
        },
        {
          "id": "MDRW2100039218.1.7",
          "form": "무심한듯",
          "original_form": "무심한듯",
          "speaker_id": "2",
          "time": "20210903 14:46"
        },
        {
          "id": "MDRW2100039218.1.8",
          "form": "어우 그런게 포인트죠",
          "original_form": "어우 그런게 포인트죠",
          "speaker_id": "1",
          "time": "20210903 14:47"
        },
        {
          "id": "MDRW2100039218.1.9",
          "form": "심쿵하는ㅋㄱㅋ",
          "original_form": "심쿵하는ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210903 14:47"
        },
        {
          "id": "MDRW2100039218.1.10",
          "form": "류준열은 목소리랑 몸매가 매력적이라",
          "original_form": "류준열은 목소리랑 몸매가 매력적이라",
          "speaker_id": "2",
          "time": "20210903 14:48"
        },
        {
          "id": "MDRW2100039218.1.11",
          "form": "정말 잘생긴연기를 하는듯 ㅎㅎ",
          "original_form": "정말 잘생긴연기를 하는듯 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 14:48"
        },
        {
          "id": "MDRW2100039218.1.12",
          "form": "잘생겨보여요 ㅎㅎ",
          "original_form": "잘생겨보여요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 14:48"
        },
        {
          "id": "MDRW2100039218.1.13",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210903 14:48"
        },
        {
          "id": "MDRW2100039218.1.14",
          "form": "잘생김을 연기중이라",
          "original_form": "잘생김을 연기중이라",
          "speaker_id": "1",
          "time": "20210903 14:49"
        },
        {
          "id": "MDRW2100039218.1.15",
          "form": "취향이 아닌데도 갑자기 취향같고ㅋㅋㅋ",
          "original_form": "취향이 아닌데도 갑자기 취향같고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:49"
        },
        {
          "id": "MDRW2100039218.1.16",
          "form": "ㅋㅋㅋㅋㅋ 맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100039218.1.17",
          "form": "늘 멋있어보이는 ㅋㅋㅋㅋㄱ",
          "original_form": "늘 멋있어보이는 ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100039218.1.18",
          "form": "봉오동전투 보셨어요?? 영화",
          "original_form": "봉오동전투 보셨어요?? 영화",
          "speaker_id": "2",
          "time": "20210903 14:53"
        },
        {
          "id": "MDRW2100039218.1.19",
          "form": "아뇨! 그거 진짜보고 싶은데",
          "original_form": "아뇨! 그거 진짜보고 싶은데",
          "speaker_id": "1",
          "time": "20210903 14:53"
        },
        {
          "id": "MDRW2100039218.1.20",
          "form": "한국사공부할때 배운 부분이라",
          "original_form": "한국사공부할때 배운 부분이라",
          "speaker_id": "1",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100039218.1.21",
          "form": "봉오동 전투도 재밌게 봤어요",
          "original_form": "봉오동 전투도 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100039218.1.22",
          "form": "거기서도 류준열 은 잘생김을 연기해요",
          "original_form": "거기서도 류준열 은 잘생김을 연기해요",
          "speaker_id": "2",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100039218.1.23",
          "form": "ㅇㅋㅋㅋㅋㄱㅋㅋ대단한대요ㅋㅋㅋ",
          "original_form": "ㅇㅋㅋㅋㅋㄱㅋㅋ대단한대요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:00"
        },
        {
          "id": "MDRW2100039218.1.24",
          "form": "ㅋㅋㅋㅋㅋ 전쟁중에서도 잘생김을 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 전쟁중에서도 잘생김을 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:02"
        },
        {
          "id": "MDRW2100039218.1.25",
