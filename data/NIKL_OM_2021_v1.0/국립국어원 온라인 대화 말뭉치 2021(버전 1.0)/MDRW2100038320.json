      "utterance": [
        {
          "id": "MDRW2100038320.1.1",
          "form": "그게 또 애매한테 잘 먹던 캔도 어느날부터 안먹고 그래요 ^_ㅜ",
          "original_form": "그게 또 애매한테 잘 먹던 캔도 어느날부터 안먹고 그래요 ^_ㅜ",
          "speaker_id": "2",
          "time": "20210902 09:56"
        },
        {
          "id": "MDRW2100038320.1.2",
          "form": "입맛 까다롭기가 아주 그냥.....",
          "original_form": "입맛 까다롭기가 아주 그냥.....",
          "speaker_id": "2",
          "time": "20210902 09:56"
        },
        {
          "id": "MDRW2100038320.1.3",
          "form": "ㅠㅠ 사기가 힘드시겠어요",
          "original_form": "ㅠㅠ 사기가 힘드시겠어요",
          "speaker_id": "1",
          "time": "20210902 09:57"
        },
        {
          "id": "MDRW2100038320.1.4",
          "form": "캔",
          "original_form": "캔",
          "speaker_id": "1",
          "time": "20210902 09:57"
        },
        {
          "id": "MDRW2100038320.1.5",
          "form": "네ㅠㅠ 그래서 박람회 열리면 자주자주 가요...",
          "original_form": "네ㅠㅠ 그래서 박람회 열리면 자주자주 가요...",
          "speaker_id": "2",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100038320.1.6",
          "form": "새로운 맛 쓸어 오느라...ㅠㅠ",
          "original_form": "새로운 맛 쓸어 오느라...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100038320.1.7",
          "form": "부산에는 처음으로",
          "original_form": "부산에는 처음으로",
          "speaker_id": "1",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100038320.1.8",
          "form": "궁팡했어요",
          "original_form": "궁팡했어요",
          "speaker_id": "1",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100038320.1.9",
          "form": "가보셨나요?",
          "original_form": "가보셨나요?",
          "speaker_id": "1",
          "time": "20210902 09:59"
        },
        {
          "id": "MDRW2100038320.1.10",
          "form": "네! 저 부산 살아서요 ㅎㅎ",
          "original_form": "네! 저 부산 살아서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:00"
        },
        {
          "id": "MDRW2100038320.1.11",
          "form": "가보셨어요?",
          "original_form": "가보셨어요?",
          "speaker_id": "2",
          "time": "20210902 10:00"
        },
        {
          "id": "MDRW2100038320.1.12",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100038320.1.13",
          "form": "저도 부산살아요",
          "original_form": "저도 부산살아요",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100038320.1.14",
          "form": "궁팡에서 마주쳤을수도 있겠네요 ㅎ",
          "original_form": "궁팡에서 마주쳤을수도 있겠네요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038320.1.15",
          "form": "저는 토욜갔어요",
          "original_form": "저는 토욜갔어요",
          "speaker_id": "1",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038320.1.16",
          "form": "비가 와서 ㅋㅋㅋ",
          "original_form": "비가 와서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038320.1.17",
          "form": "두 마리라서 양도 두배로 사오셨겠네요 ㅎㅎ",
          "original_form": "두 마리라서 양도 두배로 사오셨겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:07"
        },
        {
          "id": "MDRW2100038320.1.18",
          "form": "맞아요 금방 없어졌어요ㅜㅋㅋ",
          "original_form": "맞아요 금방 없어졌어요ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100038320.1.19",
          "form": "카페보니 나중에 아플때 대비해서 잘먹는거 찾아놓으래요",
          "original_form": "카페보니 나중에 아플때 대비해서 잘먹는거 찾아놓으래요",
          "speaker_id": "1",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100038320.1.20",
          "form": "찾아도 바뀌고 그러니 캔 따줄 때마다 복권 긁는 기분이에요 ㅋㅋㅋ",
          "original_form": "찾아도 바뀌고 그러니 캔 따줄 때마다 복권 긁는 기분이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100038320.1.21",
          "form": "애기들은 습, 건식 안 가리고 잘 먹나요??",
          "original_form": "애기들은 습, 건식 안 가리고 잘 먹나요??",
          "speaker_id": "2",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100038320.1.22",
          "form": "헉 잘먹다가도 안먹나봐요 ㅠㅠ",
          "original_form": "헉 잘먹다가도 안먹나봐요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100038320.1.23",
          "form": "그 태국산 네모난 과자뭐죠 ㅋㅋㅋ",
          "original_form": "그 태국산 네모난 과자뭐죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100038320.1.24",
          "form": "그건 귀신같이 안먹어요 ㅋㅋ",
          "original_form": "그건 귀신같이 안먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100038320.1.25",
          "form": "태국산만",
          "original_form": "태국산만",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100038320.1.26",
          "form": "템테이션 일까요? ㅋㅋㅋㅋㅋ",
          "original_form": "템테이션 일까요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100038320.1.27",
          "form": "저희집 고양이는 근데 변비가 있어요",
