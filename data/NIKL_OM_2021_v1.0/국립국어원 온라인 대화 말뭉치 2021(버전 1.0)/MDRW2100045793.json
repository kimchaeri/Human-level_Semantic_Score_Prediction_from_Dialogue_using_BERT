      "utterance": [
        {
          "id": "MDRW2100045793.1.1",
          "form": "아침에 밥은 안 먹혀서",
          "original_form": "아침에 밥은 안 먹혀서",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100045793.1.2",
          "form": "그럼 빈속으로 출근해요",
          "original_form": "그럼 빈속으로 출근해요",
          "speaker_id": "1",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100045793.1.3",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100045793.1.4",
          "form": "응 간단하게라도 먹나요",
          "original_form": "응 간단하게라도 먹나요",
          "speaker_id": "1",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100045793.1.5",
          "form": "네 그런 편이에요",
          "original_form": "네 그런 편이에요",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100045793.1.6",
          "form": "잘 하고 있네요",
          "original_form": "잘 하고 있네요",
          "speaker_id": "1",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100045793.1.7",
          "form": "그냥 과일 먹어요",
          "original_form": "그냥 과일 먹어요",
          "speaker_id": "2",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100045793.1.8",
          "form": "식전에 과일도 당수치가 올라가서 안 좋다던데",
          "original_form": "식전에 과일도 당수치가 올라가서 안 좋다던데",
          "speaker_id": "1",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100045793.1.9",
          "form": "헉 그런가요",
          "original_form": "헉 그런가요",
          "speaker_id": "2",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100045793.1.10",
          "form": "간단한 오트밀 같은 건 어때요",
          "original_form": "간단한 오트밀 같은 건 어때요",
          "speaker_id": "1",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100045793.1.11",
          "form": "식감을 싫어해요",
          "original_form": "식감을 싫어해요",
          "speaker_id": "2",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100045793.1.12",
          "form": "그럼 야채생식",
          "original_form": "그럼 야채생식",
          "speaker_id": "1",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100045793.1.13",
          "form": "아 한때 채소는 몇 번 먹은 적이 있는데 안맞더라구요",
          "original_form": "아 한때 채소는 몇 번 먹은 적이 있는데 안맞더라구요",
