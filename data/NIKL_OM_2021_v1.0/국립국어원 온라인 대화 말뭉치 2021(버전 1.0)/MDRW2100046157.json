      "utterance": [
        {
          "id": "MDRW2100046157.1.1",
          "form": "안녕하세요 식사하셨나요?",
          "original_form": "안녕하세요 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046157.1.2",
          "form": "아직 ㅠ 안먹었어용 식사 하셨나염?",
          "original_form": "아직 ㅠ 안먹었어용 식사 하셨나염?",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046157.1.3",
          "form": "네 저는 아침 먹었어요",
          "original_form": "네 저는 아침 먹었어요",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046157.1.4",
          "form": "뭐 드셨나염??",
          "original_form": "뭐 드셨나염??",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046157.1.5",
          "form": "저는 아침 잘 안챙겨먹게 되네요ㅠㅋㅋ",
          "original_form": "저는 아침 잘 안챙겨먹게 되네요ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046157.1.6",
          "form": "저는 카레랑 두부조림 먹었어요ㅋㅋㅋ",
          "original_form": "저는 카레랑 두부조림 먹었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046157.1.7",
          "form": "오 아침부터 좋은거 드셨네요",
          "original_form": "오 아침부터 좋은거 드셨네요",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046157.1.8",
          "form": "요리 하신건가요?",
          "original_form": "요리 하신건가요?",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046157.1.9",
          "form": "네! 저는 주로 요리해서 먹어요",
          "original_form": "네! 저는 주로 요리해서 먹어요",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046157.1.10",
          "form": "와우웅 요리 잘하시나봐여!!!",
          "original_form": "와우웅 요리 잘하시나봐여!!!",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046157.1.11",
          "form": "그것도 그렇구 아침부터 카레라니!",
          "original_form": "그것도 그렇구 아침부터 카레라니!",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046157.1.12",
          "form": "야채썰고 하는게 여간 귀찮은게 아니잖아요ㅠ",
          "original_form": "야채썰고 하는게 여간 귀찮은게 아니잖아요ㅠ",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046157.1.13",
          "form": "그래서 항상 저녁에 만들어 놓고 아침에는 끓여서 먹어요ㅋㅋㅋ",
          "original_form": "그래서 항상 저녁에 만들어 놓고 아침에는 끓여서 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046157.1.14",
          "form": "앗앗 그러시구나 근데 저녁에 만들어 두는것도 진짜",
          "original_form": "앗앗 그러시구나 근데 저녁에 만들어 두는것도 진짜",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046157.1.15",
          "form": "대단하십니다..ㅎㅎ",
          "original_form": "대단하십니다..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046157.1.16",
          "form": "앗ㅋㅋㅋㅋ 감사해요",
          "original_form": "앗ㅋㅋㅋㅋ 감사해요",
          "speaker_id": "1",
