      "utterance": [
        {
          "id": "MDRW2100034150.1.1",
          "form": "저는 조이도 좋아해요 ㅋㅋ 키도 크고 ㅎㅎ 이뻐요",
          "original_form": "저는 조이도 좋아해요 ㅋㅋ 키도 크고 ㅎㅎ 이뻐요",
          "speaker_id": "1",
          "time": "20210827 18:32"
        },
        {
          "id": "MDRW2100034150.1.2",
          "form": "오 맞아요 조이 웃는거도 너무 이뻐요",
          "original_form": "오 맞아요 조이 웃는거도 너무 이뻐요",
          "speaker_id": "2",
          "time": "20210827 18:32"
        },
        {
          "id": "MDRW2100034150.1.3",
          "form": "데뷔초때 웃는게 너무 이뻐서 눈이 확 갔었어요",
          "original_form": "데뷔초때 웃는게 너무 이뻐서 눈이 확 갔었어요",
          "speaker_id": "2",
          "time": "20210827 18:32"
        },
        {
          "id": "MDRW2100034150.1.4",
          "form": "맞아요 웃는게 진짜 이뻐요 ㅎㅎ",
          "original_form": "맞아요 웃는게 진짜 이뻐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100034150.1.5",
          "form": "과즙상 ㅋㅋ 이에요",
          "original_form": "과즙상 ㅋㅋ 이에요",
          "speaker_id": "1",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100034150.1.6",
          "form": "맞아요 대표적인 과즙상 아이돌인거같아요 ㅋㅋㅋ",
          "original_form": "맞아요 대표적인 과즙상 아이돌인거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100034150.1.7",
          "form": "신곡 들어보셨어요??",
          "original_form": "신곡 들어보셨어요??",
          "speaker_id": "2",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100034150.1.8",
          "form": "네 들어봤어요! 퀸덤 ㅎㅎ",
          "original_form": "네 들어봤어요! 퀸덤 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100034150.1.9",
          "form": "가볍게 듣기 좋은거같아요 ㅎㅎ",
          "original_form": "가볍게 듣기 좋은거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100034150.1.10",
          "form": "노래 좋더라구요 ㅋㅋㅋ",
          "original_form": "노래 좋더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100034150.1.11",
          "form": "레드벨벳 좋아하는 노래 있으세요??",
          "original_form": "레드벨벳 좋아하는 노래 있으세요??",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100034150.1.12",
          "form": "전 싸이코 좋아해요 ㅎㅎ 젤 많이 들었어요",
          "original_form": "전 싸이코 좋아해요 ㅎㅎ 젤 많이 들었어요",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100034150.1.13",
          "form": "맞아요 저도 레벨 노래중에서 그게 제일 좋아요 ㅋㅋㅋ",
          "original_form": "맞아요 저도 레벨 노래중에서 그게 제일 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100034150.1.14",
          "form": "맞죠 진짜 안질리고 ㅋㅋ 겨울에 듣기 좋아요",
          "original_form": "맞죠 진짜 안질리고 ㅋㅋ 겨울에 듣기 좋아요",
          "speaker_id": "1",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100034150.1.15",
          "form": "웬디 목소리 좋아해서 ㅠㅠ 활동 못해서 아쉽더라구요",
          "original_form": "웬디 목소리 좋아해서 ㅠㅠ 활동 못해서 아쉽더라구요",
          "speaker_id": "1",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100034150.1.16",
          "form": "맞아요 가장 완벽한 컨셉이었는데 개인적으로 ㅠㅠㅠㅠ",
          "original_form": "맞아요 가장 완벽한 컨셉이었는데 개인적으로 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100034150.1.17",
          "form": "너무 안타까워요",
          "original_form": "너무 안타까워요",
          "speaker_id": "2",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100034150.1.18",
          "form": "진짜요 ㅠㅠ 머리도 넘 잘어울렸는데 !",
          "original_form": "진짜요 ㅠㅠ 머리도 넘 잘어울렸는데 !",
          "speaker_id": "1",
          "time": "20210827 18:39"
        },
        {
          "id": "MDRW2100034150.1.19",
          "form": "허쉬컷 하고싶었어요 ㅋㅋ",
          "original_form": "허쉬컷 하고싶었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:40"
        },
        {
          "id": "MDRW2100034150.1.20",
          "form": "맞아요 의상도 이뻤었구 ㅠㅠㅠ",
          "original_form": "맞아요 의상도 이뻤었구 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 18:40"
        },
        {
          "id": "MDRW2100034150.1.21",
          "form": "완전 유행이죠 그머리 ㅋㅋㅋ",
