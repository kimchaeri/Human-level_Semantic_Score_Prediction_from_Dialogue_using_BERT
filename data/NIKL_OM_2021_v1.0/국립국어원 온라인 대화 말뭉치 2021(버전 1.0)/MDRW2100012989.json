      "utterance": [
        {
          "id": "MDRW2100012989.1.1",
          "form": "다이어트 해요?",
          "original_form": "다이어트 해요?",
          "speaker_id": "1",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012989.1.2",
          "form": "네 건강 때문에 감량하고 있어요!",
          "original_form": "네 건강 때문에 감량하고 있어요!",
          "speaker_id": "2",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012989.1.3",
          "form": "저도 복직 때문에 다이어트 해요.ㅎㅎ",
          "original_form": "저도 복직 때문에 다이어트 해요.ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012989.1.4",
          "form": "근데....안 빠지네요ㅠㅠ",
          "original_form": "근데....안 빠지네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012989.1.5",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012989.1.6",
          "form": "운동은 홈트하거나 러닝 정도 하는데",
          "original_form": "운동은 홈트하거나 러닝 정도 하는데",
          "speaker_id": "2",
          "time": "20210825 10:21"
        },
        {
          "id": "MDRW2100012989.1.7",
          "form": "하는 운동 있으세요?",
          "original_form": "하는 운동 있으세요?",
          "speaker_id": "2",
          "time": "20210825 10:21"
        },
        {
          "id": "MDRW2100012989.1.8",
          "form": "필라테스 했다가 지금은 그냥 홈트ㅎㅎ",
          "original_form": "필라테스 했다가 지금은 그냥 홈트ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100012989.1.9",
          "form": "홈트가 돈도 안 들고 편해서 좋아요",
          "original_form": "홈트가 돈도 안 들고 편해서 좋아요",
          "speaker_id": "2",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100012989.1.10",
          "form": "맞아요~근데 확실히 누가 잡아주면 자세가 더 잘 잡아지긴 하더라고요  ㅎ",
          "original_form": "맞아요~근데 확실히 누가 잡아주면 자세가 더 잘 잡아지긴 하더라고요  ㅎ",
          "speaker_id": "1",
          "time": "20210825 11:53"
        },
        {
          "id": "MDRW2100012989.1.11",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "2",
          "time": "20210825 11:53"
        },
        {
          "id": "MDRW2100012989.1.12",
          "form": "그리고 혼자서는 오래 못 하는 거 같아요ㅋㅋㅋ",
          "original_form": "그리고 혼자서는 오래 못 하는 거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:53"
        },
        {
          "id": "MDRW2100012989.1.13",
          "form": "누가 시켜야 하는 편이라",
          "original_form": "누가 시켜야 하는 편이라",
          "speaker_id": "2",
          "time": "20210825 11:53"
        },
        {
          "id": "MDRW2100012989.1.14",
          "form": "ㅎㅎ전 애기낳고 인생 최대치 되고 찍은 사진을 봤는데 다이어트 자극이 되더라고요",
          "original_form": "ㅎㅎ전 애기낳고 인생 최대치 되고 찍은 사진을 봤는데 다이어트 자극이 되더라고요",
          "speaker_id": "1",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100012989.1.15",
          "form": "아 나 진짜 빼야겠다 싶더라고요",
          "original_form": "아 나 진짜 빼야겠다 싶더라고요",
          "speaker_id": "1",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100012989.1.16",
          "form": "저도 수험생활동안 최대치 찍고 방학에 운동 시작했어요ㅠㅠ",
          "original_form": "저도 수험생활동안 최대치 찍고 방학에 운동 시작했어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 12:13"
        },
        {
          "id": "MDRW2100012989.1.17",
          "form": "꾸준히 식단이랑 운동하면서 빼려고요",
          "original_form": "꾸준히 식단이랑 운동하면서 빼려고요",
          "speaker_id": "2",
          "time": "20210825 12:13"
        },
        {
          "id": "MDRW2100012989.1.18",
          "form": "잘할 수 있을 거에요.\r\n아가씨땐 금방 빠지더라고요:)",
          "original_form": "잘할 수 있을 거에요.\r\n아가씨땐 금방 빠지더라고요:)",
          "speaker_id": "1",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100012989.1.19",
          "form": "응원 감사해요!",
          "original_form": "응원 감사해요!",
          "speaker_id": "2",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100012989.1.20",
          "form": "영양제 같은 건 챙겨 드시나요?",
          "original_form": "영양제 같은 건 챙겨 드시나요?",
          "speaker_id": "2",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100012989.1.21",
          "form": "안 먹다가 그냥 다이어트제에 같이 들어있는 유산균 같은 거 먹어요ㅎㅎ",
          "original_form": "안 먹다가 그냥 다이어트제에 같이 들어있는 유산균 같은 거 먹어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:39"
        },
        {
          "id": "MDRW2100012989.1.22",
          "form": "저도 질유산균이랑 비타민 정도 먹고 있어요",
          "original_form": "저도 질유산균이랑 비타민 정도 먹고 있어요",
          "speaker_id": "2",
          "time": "20210825 12:40"
        },
        {
          "id": "MDRW2100012989.1.23",
          "form": "둘 다 꽤 좋은 거 같아요",
          "original_form": "둘 다 꽤 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210825 12:40"
        }
      ]
