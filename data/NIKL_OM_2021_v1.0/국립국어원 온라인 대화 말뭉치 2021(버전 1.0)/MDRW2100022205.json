      "utterance": [
        {
          "id": "MDRW2100022205.1.1",
          "form": "오늘 아침 드셨어요?",
          "original_form": "오늘 아침 드셨어요?",
          "speaker_id": "1",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022205.1.2",
          "form": "네 아침 먹었어요",
          "original_form": "네 아침 먹었어요",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100022205.1.3",
          "form": "먹고 움직여야하거든요",
          "original_form": "먹고 움직여야하거든요",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100022205.1.4",
          "form": "오~아침 뭐 드셨는데요?",
          "original_form": "오~아침 뭐 드셨는데요?",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100022205.1.5",
          "form": "간단하게 시리얼먹었어요",
          "original_form": "간단하게 시리얼먹었어요",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022205.1.6",
          "form": "시리얼도 종류가 다양하던데 어떤 시리얼이요?",
          "original_form": "시리얼도 종류가 다양하던데 어떤 시리얼이요?",
          "speaker_id": "1",
          "time": "20210829 11:42"
        },
        {
          "id": "MDRW2100022205.1.7",
          "form": "시리얼 오리오먹었어요",
          "original_form": "시리얼 오리오먹었어요",
          "speaker_id": "2",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022205.1.8",
          "form": "근데 지금은 점심먹을시간이 다되서ㅋㅋㅋ",
          "original_form": "근데 지금은 점심먹을시간이 다되서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022205.1.9",
          "form": "아 오레오요? 저도 그거 좋아해요",
          "original_form": "아 오레오요? 저도 그거 좋아해요",
          "speaker_id": "1",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022205.1.10",
          "form": "점심 뭐 드셨어요?",
          "original_form": "점심 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100022205.1.11",
          "form": "저는 햄버거 먹었어욪",
          "original_form": "저는 햄버거 먹었어욪",
          "speaker_id": "2",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100022205.1.12",
          "form": "와 어디꺼요?",
          "original_form": "와 어디꺼요?",
          "speaker_id": "1",
          "time": "20210829 14:47"
        },
        {
          "id": "MDRW2100022205.1.13",
          "form": "맥도날드갔어요",
          "original_form": "맥도날드갔어요",
          "speaker_id": "2",
          "time": "20210829 14:47"
        },
        {
          "id": "MDRW2100022205.1.14",
          "form": "오 맥도날드 그 무슨 올더데이?",
          "original_form": "오 맥도날드 그 무슨 올더데이?",
          "speaker_id": "1",
          "time": "20210829 14:47"
        },
        {
          "id": "MDRW2100022205.1.15",
          "form": "그걸로 사신거에요?",
          "original_form": "그걸로 사신거에요?",
          "speaker_id": "1",
          "time": "20210829 14:47"
        },
        {
          "id": "MDRW2100022205.1.16",
          "form": "아니요 그냥 가서 사먹었어요",
          "original_form": "아니요 그냥 가서 사먹었어요",
          "speaker_id": "2",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100022205.1.17",
          "form": "올더데이가머에요?",
          "original_form": "올더데이가머에요?",
          "speaker_id": "2",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100022205.1.18",
          "form": "하루종일 4900원에 파는거요!",
          "original_form": "하루종일 4900원에 파는거요!",
          "speaker_id": "1",
          "time": "20210829 14:56"
        },
        {
          "id": "MDRW2100022205.1.19",
          "form": "우와!!되게  신기하네요",
          "original_form": "우와!!되게  신기하네요",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100022205.1.20",
          "form": "무조건 다 4900원인거에요?",
          "original_form": "무조건 다 4900원인거에요?",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100022205.1.21",
          "form": "어떤 메뉴에요??",
          "original_form": "어떤 메뉴에요??",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100022205.1.22",
          "form": "전 잘 모르는데 아마 빅맥일거에요 빅맥이란 다른 햄버거 한두개 정도?",
          "original_form": "전 잘 모르는데 아마 빅맥일거에요 빅맥이란 다른 햄버거 한두개 정도?",
          "speaker_id": "1",
          "time": "20210829 23:56"
        },
        {
          "id": "MDRW2100022205.1.23",
          "form": "하루종일 세트로 그렇게 팔아요",
          "original_form": "하루종일 세트로 그렇게 팔아요",
