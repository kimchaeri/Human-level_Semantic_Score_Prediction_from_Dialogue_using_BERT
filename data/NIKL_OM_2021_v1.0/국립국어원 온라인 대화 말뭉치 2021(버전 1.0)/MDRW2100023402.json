      "utterance": [
        {
          "id": "MDRW2100023402.1.1",
          "form": "해외여행 다녀오신적 있으세용?",
          "original_form": "해외여행 다녀오신적 있으세용?",
          "speaker_id": "1",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100023402.1.2",
          "form": "아 여행? 은 아닌데",
          "original_form": "아 여행? 은 아닌데",
          "speaker_id": "2",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023402.1.3",
          "form": "미국에 1년정도 살다 온 적은 있어요",
          "original_form": "미국에 1년정도 살다 온 적은 있어요",
          "speaker_id": "2",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023402.1.4",
          "form": "헐 어디에용??",
          "original_form": "헐 어디에용??",
          "speaker_id": "1",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023402.1.5",
          "form": "여행 많이 다니세요?",
          "original_form": "여행 많이 다니세요?",
          "speaker_id": "2",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023402.1.6",
          "form": "그것도 전 장기여행이라고 생각해요!",
          "original_form": "그것도 전 장기여행이라고 생각해요!",
          "speaker_id": "1",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023402.1.7",
          "form": "시애틀이요",
          "original_form": "시애틀이요",
          "speaker_id": "2",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023402.1.8",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023402.1.9",
          "form": "저는 자주는 못다녔어요ㅠㅠㅠ",
          "original_form": "저는 자주는 못다녔어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023402.1.10",
          "form": "우와 시애틀은 뭐가 유명한가용?",
          "original_form": "우와 시애틀은 뭐가 유명한가용?",
          "speaker_id": "1",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023402.1.11",
          "form": "스벅이요~~ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "스벅이요~~ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023402.1.12",
          "form": "지금은 트와일라잇이지 않을까요..",
          "original_form": "지금은 트와일라잇이지 않을까요..",
          "speaker_id": "2",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023402.1.13",
          "form": "옼ㅋㅋㅋ",
          "original_form": "옼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023402.1.14",
          "form": "아 트와일라잇을",
          "original_form": "아 트와일라잇을",
          "speaker_id": "1",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023402.1.15",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023402.1.16",
          "form": "시애틀에서 촬영했어여??",
          "original_form": "시애틀에서 촬영했어여??",
          "speaker_id": "1",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023402.1.17",
          "form": "음 시애틀 위인가 시애틀 안에인가 포크스라고",
          "original_form": "음 시애틀 위인가 시애틀 안에인가 포크스라고",
          "speaker_id": "2",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023402.1.18",
          "form": "맞나 지명이 헷갈리는데",
          "original_form": "맞나 지명이 헷갈리는데",
          "speaker_id": "2",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023402.1.19",
          "form": "저 이 영화 심심할때마다봐서 세번은 정주행한것같은데 와 여기 배경이 시애틀이었구나",
          "original_form": "저 이 영화 심심할때마다봐서 세번은 정주행한것같은데 와 여기 배경이 시애틀이었구나",
          "speaker_id": "1",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023402.1.20",
          "form": "거기가 건기우기",
          "original_form": "거기가 건기우기",
          "speaker_id": "2",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023402.1.21",
          "form": "날씨가",
          "original_form": "날씨가",
          "speaker_id": "2",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023402.1.22",
          "form": "여름건기 겨울우기 해서 겨울내내 비가와요",
          "original_form": "여름건기 겨울우기 해서 겨울내내 비가와요",
          "speaker_id": "2",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023402.1.23",
          "form": "아아 영화 대사에서도 그러더라구여!",
          "original_form": "아아 영화 대사에서도 그러더라구여!",
          "speaker_id": "1",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023402.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023402.1.25",
          "form": "비가 많이 온다구ㅋㅋㅋㅋ",
          "original_form": "비가 많이 온다구ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100023402.1.26",
          "form": "우왕ㅋㅋㅋㅋ",
          "original_form": "우왕ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:36"
        },
        {
          "id": "MDRW2100023402.1.27",
          "form": "칙칙해서거기산다구",
          "original_form": "칙칙해서거기산다구",
          "speaker_id": "2",
          "time": "20210830 01:36"
        },
        {
          "id": "MDRW2100023402.1.28",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:36"
        },
        {
          "id": "MDRW2100023402.1.29",
          "form": "와 완전 신기해요ㅋㅋㅋㅋ",
          "original_form": "와 완전 신기해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:36"
        },
        {
          "id": "MDRW2100023402.1.30",
          "form": "저 약간 미국에 대한 로망있어요!",
          "original_form": "저 약간 미국에 대한 로망있어요!",
          "speaker_id": "1",
          "time": "20210830 01:36"
        }
      ]
    }
