      "utterance": [
        {
          "id": "MDRW2100015377.1.1",
          "form": "맞아요 먹는 얘기로는 밤도 샐 수 있어요",
          "original_form": "맞아요 먹는 얘기로는 밤도 샐 수 있어요",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015377.1.2",
          "form": "밀키트로 또 뭐드셔보셨어요?",
          "original_form": "밀키트로 또 뭐드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015377.1.3",
          "form": "애슐리 거는 거의 다 먹어본거 같아요",
          "original_form": "애슐리 거는 거의 다 먹어본거 같아요",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015377.1.4",
          "form": "저도 그럼 에슐리로",
          "original_form": "저도 그럼 에슐리로",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015377.1.5",
          "form": "먹어보겠습니다",
          "original_form": "먹어보겠습니다",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015377.1.6",
          "form": "치킨 좋아하시죠?",
          "original_form": "치킨 좋아하시죠?",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015377.1.7",
          "form": "치킨 안좋아하는 사람은 없죠",
          "original_form": "치킨 안좋아하는 사람은 없죠",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015377.1.8",
          "form": "저 교촌이 땡겨요",
          "original_form": "저 교촌이 땡겨요",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015377.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015377.1.10",
          "form": "교촌이 허니콤보였던가요",
          "original_form": "교촌이 허니콤보였던가요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015377.1.11",
          "form": "네 허니콤보요",
          "original_form": "네 허니콤보요",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015377.1.12",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015377.1.13",
          "form": "ㅠ 내일은 진짜 치킨 먹고 싶어요",
          "original_form": "ㅠ 내일은 진짜 치킨 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015377.1.14",
          "form": "먹고 싶은거 왜이리 많지",
          "original_form": "먹고 싶은거 왜이리 많지",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015377.1.15",
          "form": "치킨드시죠 내일 ㅋㅋ",
          "original_form": "치킨드시죠 내일 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015377.1.16",
          "form": "메뉴 정해드릴게요",
          "original_form": "메뉴 정해드릴게요",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015377.1.17",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015377.1.18",
          "form": "네ㅋㅋ 치킨 가겠습니다",
          "original_form": "네ㅋㅋ 치킨 가겠습니다",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015377.1.19",
          "form": "내일 점심 추천좀",
          "original_form": "내일 점심 추천좀",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015377.1.20",
          "form": "낼 점심은... 족발 1인분만 시켜먹을 수 있다면 족발이요",
          "original_form": "낼 점심은... 족발 1인분만 시켜먹을 수 있다면 족발이요",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015377.1.21",
          "form": "그럴수 없으니....",
          "original_form": "그럴수 없으니....",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015377.1.22",
          "form": "족발덮밥 같은거 있음 좋은데",
          "original_form": "족발덮밥 같은거 있음 좋은데",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015377.1.23",
          "form": "족발은 왜 다른 요리가 없는지...",
          "original_form": "족발은 왜 다른 요리가 없는지...",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015377.1.24",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015377.1.25",
          "form": "진짜 족발뿐",
          "original_form": "진짜 족발뿐",
          "speaker_id": "1",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015377.1.26",
          "form": "족발 응용요리 하면 성공하려나요ㅋㅋ",
          "original_form": "족발 응용요리 하면 성공하려나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015377.1.27",
          "form": "집에서 시켜먹고 남은 족발로",
          "original_form": "집에서 시켜먹고 남은 족발로",
          "speaker_id": "1",
          "time": "20210826 08:51"
        },
        {
          "id": "MDRW2100015377.1.28",
          "form": "레시피 연구하면?",
          "original_form": "레시피 연구하면?",
          "speaker_id": "1",
          "time": "20210826 08:52"
        },
        {
          "id": "MDRW2100015377.1.29",
          "form": "할만 할거 같은데요...?",
          "original_form": "할만 할거 같은데요...?",
          "speaker_id": "2",
          "time": "20210826 11:14"
        },
        {
          "id": "MDRW2100015377.1.30",
          "form": "해보세요 ㅋㅋㅋ",
          "original_form": "해보세요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100015377.1.31",
          "form": "레시피개발",
          "original_form": "레시피개발",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100015377.1.32",
          "form": "일단 족발을 사야겠네요ㅋㅋ",
          "original_form": "일단 족발을 사야겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:07"
        }
      ]
    }
