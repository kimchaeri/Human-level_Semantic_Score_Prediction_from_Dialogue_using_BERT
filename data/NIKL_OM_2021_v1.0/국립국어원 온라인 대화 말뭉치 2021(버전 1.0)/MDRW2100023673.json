      "utterance": [
        {
          "id": "MDRW2100023673.1.1",
          "form": "스포츠 좋아하시나봐여?",
          "original_form": "스포츠 좋아하시나봐여?",
          "speaker_id": "1",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023673.1.2",
          "form": "스포츠를 보는건 좋아해요!!! 그래서 올림픽때 즐거웠어요 ㅋㅋ",
          "original_form": "스포츠를 보는건 좋아해요!!! 그래서 올림픽때 즐거웠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023673.1.3",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023673.1.4",
          "form": "전 예전에는 nba봣는데.. 이젠 암거도 안봐요",
          "original_form": "전 예전에는 nba봣는데.. 이젠 암거도 안봐요",
          "speaker_id": "1",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023673.1.5",
          "form": "nba면 미국농구리그여?",
          "original_form": "nba면 미국농구리그여?",
          "speaker_id": "2",
          "time": "20210830 15:25"
        },
        {
          "id": "MDRW2100023673.1.6",
          "form": "오.. 네.. 미국 마이클조던 시절...",
          "original_form": "오.. 네.. 미국 마이클조던 시절...",
          "speaker_id": "1",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100023673.1.7",
          "form": "어떤 장르 좋아하세요?",
          "original_form": "어떤 장르 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100023673.1.8",
          "form": "전 국대경기 다 재밌게 봅니다..",
          "original_form": "전 국대경기 다 재밌게 봅니다..",
          "speaker_id": "2",
          "time": "20210830 16:37"
        },
        {
          "id": "MDRW2100023673.1.9",
          "form": "올림픽은 뭐가 젤 재밋엇어요?",
          "original_form": "올림픽은 뭐가 젤 재밋엇어요?",
          "speaker_id": "1",
          "time": "20210830 16:39"
        },
        {
          "id": "MDRW2100023673.1.10",
          "form": "양궁이랑 배구요!!!!!!! 펜싱이랑 클라이밍도 ㅋㅋㅋㄱ",
          "original_form": "양궁이랑 배구요!!!!!!! 펜싱이랑 클라이밍도 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100023673.1.11",
          "form": "펜싱이 생각보다 재밋더라구여.. \n멋잇기도하고",
          "original_form": "펜싱이 생각보다 재밋더라구여.. \n멋잇기도하고",
          "speaker_id": "1",
          "time": "20210830 16:41"
        },
        {
          "id": "MDRW2100023673.1.12",
          "form": "펜싱 멋있더라구요 ㅋㅋㅋ",
          "original_form": "펜싱 멋있더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023673.1.13",
          "form": "모든 스포츠가 그렇지만 룰을 알면 더 재밌는거 같아요",
          "original_form": "모든 스포츠가 그렇지만 룰을 알면 더 재밌는거 같아요",
          "speaker_id": "2",
          "time": "20210830 20:57"
        },
        {
          "id": "MDRW2100023673.1.14",
          "form": "그쵸.. 알면 알수록 더 잘보이니까",
          "original_form": "그쵸.. 알면 알수록 더 잘보이니까",
