          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004281.1.10",
          "form": "언제 와???",
          "original_form": "언제 와???",
          "speaker_id": "1",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004281.1.11",
          "form": "개욱겨",
          "original_form": "개욱겨",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004281.1.12",
          "form": "내 폰케이스",
          "original_form": "내 폰케이스",
          "speaker_id": "1",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004281.1.13",
          "form": "언제와",
          "original_form": "언제와",
          "speaker_id": "1",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004281.1.14",
          "form": "그거언제와",
          "original_form": "그거언제와",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004281.1.15",
          "form": "왜 발송 안시켜줘",
          "original_form": "왜 발송 안시켜줘",
          "speaker_id": "1",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004281.1.16",
          "form": "에 그건좀",
          "original_form": "에 그건좀",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004281.1.17",
          "form": "아 나 사고싶은거너무많아서",
          "original_form": "아 나 사고싶은거너무많아서",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004281.1.18",
          "form": "쿠팡가야되나 싶은데아바가 쿠팡가지말래",
          "original_form": "쿠팡가야되나 싶은데아바가 쿠팡가지말래",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004281.1.19",
          "form": "왱?",
          "original_form": "왱?",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004281.1.20",
          "form": "확진자가 많이 나와서 그래?",
          "original_form": "확진자가 많이 나와서 그래?",
          "speaker_id": "1",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004281.1.21",
          "form": "아니 거기 갓다와서",
          "original_form": "아니 거기 갓다와서",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004281.1.22",
          "form": "건강에 안좋을거가ㅏㅌ다구",
          "original_form": "건강에 안좋을거가ㅏㅌ다구",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004281.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅈㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅈㅋㅋㅋㅋ",
