      "utterance": [
        {
          "id": "MDRW2100017204.1.1",
          "form": "사람들이",
          "original_form": "사람들이",
          "speaker_id": "2",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100017204.1.2",
          "form": "작업할때 퀄이 달라지지ㅋㅋㅋㅋㅋㅋ",
          "original_form": "작업할때 퀄이 달라지지ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100017204.1.3",
          "form": "부리는 노예가 많은거라고 하던데",
          "original_form": "부리는 노예가 많은거라고 하던데",
          "speaker_id": "2",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100017204.1.4",
          "form": "나지금 이노트북으로는",
          "original_form": "나지금 이노트북으로는",
          "speaker_id": "1",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100017204.1.5",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100017204.1.6",
          "form": "테런도 렌더링 50으로 돌리거든",
          "original_form": "테런도 렌더링 50으로 돌리거든",
          "speaker_id": "1",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100017204.1.7",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017204.1.8",
          "form": "그래서 진짜좋은거 사서 마크나 옵치 짱짱하게 돌리고싶음",
          "original_form": "그래서 진짜좋은거 사서 마크나 옵치 짱짱하게 돌리고싶음",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017204.1.9",
          "form": "아 이북리더기도 사고싶다",
          "original_form": "아 이북리더기도 사고싶다",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017204.1.10",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017204.1.11",
          "form": "근데 나도 전자책 읽는거",
          "original_form": "근데 나도 전자책 읽는거",
          "speaker_id": "2",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100017204.1.12",
          "form": "사려고 했었거든",
          "original_form": "사려고 했었거든",
          "speaker_id": "2",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100017204.1.13",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100017204.1.14",
          "form": "알라딘에서 파는거",
          "original_form": "알라딘에서 파는거",
          "speaker_id": "2",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100017204.1.15",
          "form": "크레마?",
          "original_form": "크레마?",
          "speaker_id": "1",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100017204.1.16",
          "form": "오 맞아맞아",
          "original_form": "오 맞아맞아",
          "speaker_id": "2",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100017204.1.17",
          "form": "근데 난 약간 종이를",
          "original_form": "근데 난 약간 종이를",
          "speaker_id": "2",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100017204.1.18",
          "form": "손에 쥐고 있어야해",
          "original_form": "손에 쥐고 있어야해",
          "speaker_id": "2",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100017204.1.19",
          "form": "ㅇㅎㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅎㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100017204.1.20",
          "form": "그리고 요즘 좋은 문장에",
          "original_form": "그리고 요즘 좋은 문장에",
          "speaker_id": "2",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017204.1.21",
          "form": "인덱스 붙이는거에 재미 들려서",
          "original_form": "인덱스 붙이는거에 재미 들려서",
          "speaker_id": "2",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017204.1.22",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017204.1.23",
          "form": "인덱스도 빨리 다 쓰고 이쁜거 사고 싶다",
          "original_form": "인덱스도 빨리 다 쓰고 이쁜거 사고 싶다",
          "speaker_id": "2",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017204.1.24",
          "form": "나 인덱스 아직도 그거씀",
          "original_form": "나 인덱스 아직도 그거씀",
          "speaker_id": "1",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017204.1.25",
          "form": "고딩때 다이소랑 마블 콜라보했을때 삿던",
          "original_form": "고딩때 다이소랑 마블 콜라보했을때 삿던",
          "speaker_id": "1",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017204.1.26",
          "form": "마블인덱스ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "마블인덱스ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
