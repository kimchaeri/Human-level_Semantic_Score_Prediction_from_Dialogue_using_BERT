      "utterance": [
        {
          "id": "MDRW2100015675.1.1",
          "form": "식사는 주로 어떻게?",
          "original_form": "식사는 주로 어떻게?",
          "speaker_id": "2",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015675.1.2",
          "form": "아침은 챙겨드세요?",
          "original_form": "아침은 챙겨드세요?",
          "speaker_id": "2",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015675.1.3",
          "form": "잘 안 먹는 편이에요",
          "original_form": "잘 안 먹는 편이에요",
          "speaker_id": "1",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015675.1.4",
          "form": "그래도 간단히 먹을 수 있는 거 챙겨요. 아예 안 먹으면 속쓰려서",
          "original_form": "그래도 간단히 먹을 수 있는 거 챙겨요. 아예 안 먹으면 속쓰려서",
          "speaker_id": "1",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015675.1.5",
          "form": "아침은 챙겨먹기가 그렇됴",
          "original_form": "아침은 챙겨먹기가 그렇됴",
          "speaker_id": "2",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015675.1.6",
          "form": "아침에 바쁘기도하고ㄹㅎ",
          "original_form": "아침에 바쁘기도하고ㄹㅎ",
          "speaker_id": "2",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015675.1.7",
          "form": "그래도 다행이시네요ㅎ 챙겨드셔서ㅎㅎ",
          "original_form": "그래도 다행이시네요ㅎ 챙겨드셔서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015675.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015675.1.9",
          "form": "근데 공복에 바나나 토마토가 안 좋다는 얘길 들었어요",
          "original_form": "근데 공복에 바나나 토마토가 안 좋다는 얘길 들었어요",
          "speaker_id": "1",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015675.1.10",
          "form": "아마 산성이 좋다고 그랬던것같애요ㅎ",
          "original_form": "아마 산성이 좋다고 그랬던것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015675.1.11",
          "form": "공복에 레몬즙먹으면 좋대요ㅎ",
          "original_form": "공복에 레몬즙먹으면 좋대요ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015675.1.12",
          "form": "몸에 산성이 들어오면 다이어트에 도움되기도한다던데ㅎㅎ",
          "original_form": "몸에 산성이 들어오면 다이어트에 도움되기도한다던데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015675.1.13",
          "form": "토마토도 그런거 아닐까요?ㅎ",
          "original_form": "토마토도 그런거 아닐까요?ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015675.1.14",
          "form": "맛과 건강, 편리함까지 동시에 챙기기 참 어렵네요ㅋㅋ",
          "original_form": "맛과 건강, 편리함까지 동시에 챙기기 참 어렵네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015675.1.15",
          "form": "하나는 포기해야죠ㅎㅎㅎ",
          "original_form": "하나는 포기해야죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015675.1.16",
          "form": "하나를택하면ㅎㅎ",
          "original_form": "하나를택하면ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015675.1.17",
          "form": "대체당 샀는데 요리할 때 쓰면 설탕이랑 별 다를 거 없더라구요",
          "original_form": "대체당 샀는데 요리할 때 쓰면 설탕이랑 별 다를 거 없더라구요",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015675.1.18",
          "form": "오ㅎㅎ 그런것도 있군요ㄹ",
          "original_form": "오ㅎㅎ 그런것도 있군요ㄹ",
          "speaker_id": "2",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015675.1.19",
          "form": "설탕이랑 차이가 뭐에요? 그럼?",
          "original_form": "설탕이랑 차이가 뭐에요? 그럼?",
          "speaker_id": "2",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015675.1.20",
          "form": "칼로리가 낮나요?",
          "original_form": "칼로리가 낮나요?",
          "speaker_id": "2",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015675.1.21",
          "form": "스테비아라고 단맛은 나는데 당은 아닌 거여서 당뇨 있다거나 하면 좋아요",
          "original_form": "스테비아라고 단맛은 나는데 당은 아닌 거여서 당뇨 있다거나 하면 좋아요",
          "speaker_id": "1",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100015675.1.22",
          "form": "요즘 제로콜라나 비슷한 음료들 많이 나왔잖아요",
          "original_form": "요즘 제로콜라나 비슷한 음료들 많이 나왔잖아요",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015675.1.23",
          "form": "아아ㅎ 뭔지 알겟어요ㅎㅎ",
          "original_form": "아아ㅎ 뭔지 알겟어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015675.1.24",
          "form": "맛만 안변하면 좋죠ㅎㅎ",
          "original_form": "맛만 안변하면 좋죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015675.1.25",
          "form": "근데 제로콜라는 맛이 좀 틀려서ㅠ",
          "original_form": "근데 제로콜라는 맛이 좀 틀려서ㅠ",
          "speaker_id": "2",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015675.1.26",
          "form": "2프로 부족한맛이라할까요?",
          "original_form": "2프로 부족한맛이라할까요?",
          "speaker_id": "2",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015675.1.27",
          "form": "스테비아는 어때요?",
          "original_form": "스테비아는 어때요?",
          "speaker_id": "2",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015675.1.28",
          "form": "약간 다른 맛이긴 하죠ㅋㅋ",
          "original_form": "약간 다른 맛이긴 하죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015675.1.29",
          "form": "스테비아만 살짝 먹어봤을 땐 확실히 설탕이랑 다른 맛인데",
          "original_form": "스테비아만 살짝 먹어봤을 땐 확실히 설탕이랑 다른 맛인데",
          "speaker_id": "1",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015675.1.30",
          "form": "요리에 들어가면 티 안 나더라구요",
          "original_form": "요리에 들어가면 티 안 나더라구요",
          "speaker_id": "1",
          "time": "20210826 00:53"
        }
      ]
    }
