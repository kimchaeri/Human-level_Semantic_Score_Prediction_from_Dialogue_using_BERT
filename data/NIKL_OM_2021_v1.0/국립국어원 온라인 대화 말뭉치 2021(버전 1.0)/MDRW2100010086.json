      "utterance": [
        {
          "id": "MDRW2100010086.1.1",
          "form": "좋아하는 음식 있으세요???",
          "original_form": "좋아하는 음식 있으세요???",
          "speaker_id": "1",
          "time": "20210823 14:04"
        },
        {
          "id": "MDRW2100010086.1.2",
          "form": "저 요새 과일빙수 좋아해요 ㅋㅋㅋ 얼마전에 메론빙수 먹었는데 좋더라구요 좋아하시나요",
          "original_form": "저 요새 과일빙수 좋아해요 ㅋㅋㅋ 얼마전에 메론빙수 먹었는데 좋더라구요 좋아하시나요",
          "speaker_id": "2",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100010086.1.3",
          "form": "헉 맛있겠네요 여름 지나기 전에 설빙쿠폰 써야 되는데 까먹을뻔...",
          "original_form": "헉 맛있겠네요 여름 지나기 전에 설빙쿠폰 써야 되는데 까먹을뻔...",
          "speaker_id": "1",
          "time": "20210823 14:46"
        },
        {
          "id": "MDRW2100010086.1.4",
          "form": "빙수 좋아합니다~~~",
          "original_form": "빙수 좋아합니다~~~",
          "speaker_id": "1",
          "time": "20210823 14:46"
        },
        {
          "id": "MDRW2100010086.1.5",
          "form": "헐 설빙 다녀오세요",
          "original_form": "헐 설빙 다녀오세요",
          "speaker_id": "2",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100010086.1.6",
          "form": "지금 메론 박스채 쌓아놧어요",
          "original_form": "지금 메론 박스채 쌓아놧어요",
          "speaker_id": "2",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100010086.1.7",
          "form": "ㅋㅋ멋지네요!!",
          "original_form": "ㅋㅋ멋지네요!!",
          "speaker_id": "1",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100010086.1.8",
          "form": "어디서 사신건가요?",
          "original_form": "어디서 사신건가요?",
          "speaker_id": "1",
          "time": "20210823 15:28"
        },
        {
          "id": "MDRW2100010086.1.9",
          "form": "아 설빙에서 메론을 쌓아놨어요 ㅋㅋㅋㅋ",
          "original_form": "아 설빙에서 메론을 쌓아놨어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:42"
        },
        {
          "id": "MDRW2100010086.1.10",
          "form": "메론 빙수가 잘 나가나보더라구요",
          "original_form": "메론 빙수가 잘 나가나보더라구요",
          "speaker_id": "2",
          "time": "20210823 21:42"
        },
        {
          "id": "MDRW2100010086.1.11",
          "form": "아하ㅋㅋㅋㅋ메론 최고죠",
          "original_form": "아하ㅋㅋㅋㅋ메론 최고죠",
          "speaker_id": "1",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100010086.1.12",
          "form": "복숭아빙수인가도 있더라고요",
          "original_form": "복숭아빙수인가도 있더라고요",
          "speaker_id": "1",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100010086.1.13",
          "form": "헐 그것도 맛있을거같아요!",
          "original_form": "헐 그것도 맛있을거같아요!",
          "speaker_id": "2",
          "time": "20210824 08:49"
        },
        {
          "id": "MDRW2100010086.1.14",
          "form": "설빙 한번 더 가야겠어요 ㅋㅋㅋㅋ",
          "original_form": "설빙 한번 더 가야겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 08:49"
        },
        {
          "id": "MDRW2100010086.1.15",
          "form": "쿠폰으로 뭐 드실건가요?",
          "original_form": "쿠폰으로 뭐 드실건가요?",
          "speaker_id": "2",
          "time": "20210824 08:49"
        },
        {
          "id": "MDRW2100010086.1.16",
          "form": "음 녹차빙수? 고민중입니다ㅋㅋㅋ",
          "original_form": "음 녹차빙수? 고민중입니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:13"
        },
        {
          "id": "MDRW2100010086.1.17",
          "form": "녹차빙수 좋아하세요?",
          "original_form": "녹차빙수 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 09:13"
        },
        {
          "id": "MDRW2100010086.1.18",
          "form": "녹차빙수 안먹어봤어요!",
          "original_form": "녹차빙수 안먹어봤어요!",
          "speaker_id": "2",
          "time": "20210824 13:11"
        },
        {
          "id": "MDRW2100010086.1.19",
          "form": "녹차아이스크림 올라간건가요?",
          "original_form": "녹차아이스크림 올라간건가요?",
          "speaker_id": "2",
          "time": "20210824 13:11"
        },
        {
          "id": "MDRW2100010086.1.20",
          "form": "네! 맛있더라고요",
          "original_form": "네! 맛있더라고요",
          "speaker_id": "1",
          "time": "20210824 13:53"
        },
        {
          "id": "MDRW2100010086.1.21",
          "form": "녹차를 별로 안 좋아하시나요?",
          "original_form": "녹차를 별로 안 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 13:53"
        },
        {
          "id": "MDRW2100010086.1.22",
          "form": "저 지금도 녹차라떼 먹고있어요 ㅎㅎ",
          "original_form": "저 지금도 녹차라떼 먹고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100010086.1.23",
          "form": "녹차아이스크림에 우유얼음인가요?",
          "original_form": "녹차아이스크림에 우유얼음인가요?",
          "speaker_id": "2",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100010086.1.24",
          "form": "앜ㅋㅋ그렇군요  네 그랬던거 같아요!",
          "original_form": "앜ㅋㅋ그렇군요  네 그랬던거 같아요!",
          "speaker_id": "1",
          "time": "20210824 15:33"
        },
        {
          "id": "MDRW2100010086.1.25",
          "form": "저도 먹은지 오래돼서ㅋㅋ",
          "original_form": "저도 먹은지 오래돼서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:33"
        },
        {
          "id": "MDRW2100010086.1.26",
          "form": "맛있겠네요 ㅜㅜ",
