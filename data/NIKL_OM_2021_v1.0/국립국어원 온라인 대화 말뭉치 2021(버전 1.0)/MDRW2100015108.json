      "utterance": [
        {
          "id": "MDRW2100015108.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015108.1.2",
          "form": "음료수 뭐좋아하세여",
          "original_form": "음료수 뭐좋아하세여",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015108.1.3",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015108.1.4",
          "form": "음 유자 음료수 레몬에이드요.",
          "original_form": "음 유자 음료수 레몬에이드요.",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015108.1.5",
          "form": "오 저도 그런류 좋아해요",
          "original_form": "오 저도 그런류 좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015108.1.6",
          "form": "새콤이로요",
          "original_form": "새콤이로요",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015108.1.7",
          "form": "스벅 자몽허니블랙티도 좋아하시나요?",
          "original_form": "스벅 자몽허니블랙티도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015108.1.8",
          "form": "저둨ㅋ",
          "original_form": "저둨ㅋ",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015108.1.9",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015108.1.10",
          "form": "고런거 다좋아요",
          "original_form": "고런거 다좋아요",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015108.1.11",
          "form": "저 사실 음료먹으러 스벅가요 ㅋㅋ",
          "original_form": "저 사실 음료먹으러 스벅가요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015108.1.12",
          "form": "커피아니고",
          "original_form": "커피아니고",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015108.1.13",
          "form": "저두 커피보단 역시 달콤한 음료가",
          "original_form": "저두 커피보단 역시 달콤한 음료가",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015108.1.14",
          "form": "이번에 새로나온 자허블비슷한 음료도 있던데",
          "original_form": "이번에 새로나온 자허블비슷한 음료도 있던데",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015108.1.15",
          "form": "역시 자허블이 진리인거같아요",
          "original_form": "역시 자허블이 진리인거같아요",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015108.1.16",
          "form": "오 저도 그래요 ㅎㅎ",
          "original_form": "오 저도 그래요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015108.1.17",
          "form": "ㅎㅎㅎ근데 자허블도 지점마다",
          "original_form": "ㅎㅎㅎ근데 자허블도 지점마다",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015108.1.18",
          "form": "와 진짜 맛없게 만드는 직원도 있더라구요",
          "original_form": "와 진짜 맛없게 만드는 직원도 있더라구요",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015108.1.19",
          "form": "잘 보고 가야해서ㅠㅠ저번에 저 완전 맹탕 자허블먹었어요",
          "original_form": "잘 보고 가야해서ㅠㅠ저번에 저 완전 맹탕 자허블먹었어요",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015108.1.20",
          "form": "맞아요 그게 하는사럼따러 틀리데요",
          "original_form": "맞아요 그게 하는사럼따러 틀리데요",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015108.1.21",
          "form": "저도요 ㅡㅡ",
          "original_form": "저도요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015108.1.22",
          "form": "ㅠㅠㅠㅠ 다른카페는 어디어디가세요",
          "original_form": "ㅠㅠㅠㅠ 다른카페는 어디어디가세요",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015108.1.23",
          "form": "저 외곽에 개인카페요",
          "original_form": "저 외곽에 개인카페요",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015108.1.24",
          "form": "오 개인카페 잘만 고르면 맛있더라구요",
          "original_form": "오 개인카페 잘만 고르면 맛있더라구요",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015108.1.25",
          "form": "전 요새 레몬차 맛있는집 찾아서 종종가요",
          "original_form": "전 요새 레몬차 맛있는집 찾아서 종종가요",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015108.1.26",
          "form": "아 레몬차 넘좋아요",
          "original_form": "아 레몬차 넘좋아요",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015108.1.27",
          "form": "맞아요 전 자몽에이드요 ㅋㅋ",
          "original_form": "맞아요 전 자몽에이드요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015108.1.28",
          "form": "오 저두요",
          "original_form": "오 저두요",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015108.1.29",
          "form": "에이드는 진짜 다 맛있어요",
          "original_form": "에이드는 진짜 다 맛있어요",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015108.1.30",
          "form": "초코류는 안좋아하세요?",
          "original_form": "초코류는 안좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015108.1.31",
          "form": "좋아하죠",
          "original_form": "좋아하죠",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015108.1.32",
          "form": "쪼코 케이크도 음료도",
          "original_form": "쪼코 케이크도 음료도",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015108.1.33",
          "form": "전 초코는 스벅 자바칩 젤 맛있던데",
          "original_form": "전 초코는 스벅 자바칩 젤 맛있던데",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015108.1.34",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015108.1.35",
          "form": "전 그 특유의 맛이 있는게 좋아요",
          "original_form": "전 그 특유의 맛이 있는게 좋아요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015108.1.36",
          "form": "네 ㅎㅎ딸이랑 먹어요",
          "original_form": "네 ㅎㅎ딸이랑 먹어요",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015108.1.37",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:42"
        }
      ]
    }
