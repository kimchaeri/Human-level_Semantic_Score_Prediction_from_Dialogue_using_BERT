      "utterance": [
        {
          "id": "MDRW2100005921.1.1",
          "form": "위드 코로나 어떻게생각하세요?",
          "original_form": "위드 코로나 어떻게생각하세요?",
          "speaker_id": "1",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100005921.1.2",
          "form": "언젠가 해야한다고 생각하지만 지금은 이르지않나 생각해요",
          "original_form": "언젠가 해야한다고 생각하지만 지금은 이르지않나 생각해요",
          "speaker_id": "2",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100005921.1.3",
          "form": "어떻게 생각하세요?",
          "original_form": "어떻게 생각하세요?",
          "speaker_id": "2",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100005921.1.4",
          "form": "저도 일단 2차접종을 모두 완료해야한다고 생각해요",
          "original_form": "저도 일단 2차접종을 모두 완료해야한다고 생각해요",
          "speaker_id": "1",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100005921.1.5",
          "form": "하지만 정말 지치는게 맞긴해요..",
          "original_form": "하지만 정말 지치는게 맞긴해요..",
          "speaker_id": "1",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100005921.1.6",
          "form": "그죠 지치긴해요...",
          "original_form": "그죠 지치긴해요...",
          "speaker_id": "2",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005921.1.7",
          "form": "생활고로 고통받는분도 많고",
          "original_form": "생활고로 고통받는분도 많고",
          "speaker_id": "2",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005921.1.8",
          "form": "백신맞으셨나요?",
          "original_form": "백신맞으셨나요?",
          "speaker_id": "1",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005921.1.9",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "1",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005921.1.10",
          "form": "이번에 지원금이나오면 저같은 대학생은 좋지만",
          "original_form": "이번에 지원금이나오면 저같은 대학생은 좋지만",
          "speaker_id": "1",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005921.1.11",
          "form": "네 일차맞았습ㄴ대",
          "original_form": "네 일차맞았습ㄴ대",
          "speaker_id": "2",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005921.1.12",
          "form": "소상공인분들은 사실 타격이 엄청크니까요",
          "original_form": "소상공인분들은 사실 타격이 엄청크니까요",
          "speaker_id": "1",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005921.1.13",
          "form": "그죠....",
          "original_form": "그죠....",
          "speaker_id": "2",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005921.1.14",
          "form": "오오 저도 화이자1차까지맞았습니다",
          "original_form": "오오 저도 화이자1차까지맞았습니다",
          "speaker_id": "1",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005921.1.15",
          "form": "경제로 사람이 죽나...코로나로 죽나",
          "original_form": "경제로 사람이 죽나...코로나로 죽나",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100005921.1.16",
          "form": "사실 경중을 따질수없긴하니까요",
          "original_form": "사실 경중을 따질수없긴하니까요",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100005921.1.17",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "1",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100005921.1.18",
          "form": "독감처럼",
          "original_form": "독감처럼",
          "speaker_id": "1",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100005921.1.19",
          "form": "치사율이 낮아지고 백신트로 후유증이 많이 사라진다고하면",
          "original_form": "치사율이 낮아지고 백신트로 후유증이 많이 사라진다고하면",
          "speaker_id": "1",
          "time": "20210907 16:30"
        },
        {
          "id": "MDRW2100005921.1.20",
          "form": "그땐 위드코로나해야한다규생각해요",
          "original_form": "그땐 위드코로나해야한다규생각해요",
          "speaker_id": "1",
          "time": "20210907 16:30"
        },
        {
          "id": "MDRW2100005921.1.21",
          "form": "내년에는 해야하지않을까 생각하는데요",
          "original_form": "내년에는 해야하지않을까 생각하는데요",
          "speaker_id": "2",
          "time": "20210907 16:30"
        },
        {
          "id": "MDRW2100005921.1.22",
          "form": "아직은 ...의료업계분들에게 부담을 넘기는게 아닌가싶어요",
          "original_form": "아직은 ...의료업계분들에게 부담을 넘기는게 아닌가싶어요",
          "speaker_id": "2",
          "time": "20210907 16:30"
        },
        {
          "id": "MDRW2100005921.1.23",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "1",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100005921.1.24",
          "form": "아니면 백신접종률을 높여서",
          "original_form": "아니면 백신접종률을 높여서",
          "speaker_id": "1",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100005921.1.25",
          "form": "올해말정도는 괜찮을 것 같어요",
          "original_form": "올해말정도는 괜찮을 것 같어요",
          "speaker_id": "1",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100005921.1.26",
          "form": "할로윈때도 이렇게 많이 확진다가 나올까료?",
          "original_form": "할로윈때도 이렇게 많이 확진다가 나올까료?",
          "speaker_id": "1",
          "time": "20210907 16:32"
        },
        {
          "id": "MDRW2100005921.1.27",
          "form": "글쎄요 백신 많이들 맞았으니까 작년보다는",
          "original_form": "글쎄요 백신 많이들 맞았으니까 작년보다는",
          "speaker_id": "2",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100005921.1.28",
          "form": "덜하지않을까요...",
          "original_form": "덜하지않을까요...",
