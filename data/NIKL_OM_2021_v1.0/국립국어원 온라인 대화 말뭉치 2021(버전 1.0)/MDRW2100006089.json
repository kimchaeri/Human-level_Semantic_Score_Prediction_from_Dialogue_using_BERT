      "utterance": [
        {
          "id": "MDRW2100006089.1.1",
          "form": "기억에 남는 공연 있나요",
          "original_form": "기억에 남는 공연 있나요",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100006089.1.2",
          "form": "저는 레미제라블요",
          "original_form": "저는 레미제라블요",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100006089.1.3",
          "form": "진짜 감동적이랍니다",
          "original_form": "진짜 감동적이랍니다",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100006089.1.4",
          "form": "뮤지컬로 보신건가요?",
          "original_form": "뮤지컬로 보신건가요?",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100006089.1.5",
          "form": "넵 대학생뮤지컬대회가있는데",
          "original_form": "넵 대학생뮤지컬대회가있는데",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100006089.1.6",
          "form": "거기서 대상받은팀 공연이었거든요",
          "original_form": "거기서 대상받은팀 공연이었거든요",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100006089.1.7",
          "form": "프로못지않게 잘하더라구요",
          "original_form": "프로못지않게 잘하더라구요",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100006089.1.8",
          "form": "저는 영화로만 본지라 궁금하네요 뮤지컬도...",
          "original_form": "저는 영화로만 본지라 궁금하네요 뮤지컬도...",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006089.1.9",
          "form": "영화도 참 좋지만 뮤지컬도 뭉클하더라구요",
          "original_form": "영화도 참 좋지만 뮤지컬도 뭉클하더라구요",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100006089.1.10",
          "form": "참 좋은작품인거같아요",
          "original_form": "참 좋은작품인거같아요",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100006089.1.11",
          "form": "원작도 그렇구요",
          "original_form": "원작도 그렇구요",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100006089.1.12",
          "form": "그렇죠 원작 자체가 훌륭하다보니...",
          "original_form": "그렇죠 원작 자체가 훌륭하다보니...",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006089.1.13",
          "form": "영화 자주보시나요?",
          "original_form": "영화 자주보시나요?",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100006089.1.14",
          "form": "좋아하는데 코로나로 영화관 가기가 어렵네요",
          "original_form": "좋아하는데 코로나로 영화관 가기가 어렵네요",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100006089.1.15",
          "form": "그쵸 우리동네 아이맥스관도 문 닫았더라구요",
          "original_form": "그쵸 우리동네 아이맥스관도 문 닫았더라구요",
          "speaker_id": "2",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100006089.1.16",
          "form": "온라인 공연같은건 유료라도 보시나요?",
          "original_form": "온라인 공연같은건 유료라도 보시나요?",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006089.1.17",
          "form": "전 직접보는데 의의를 둬서 안보게되네요",
          "original_form": "전 직접보는데 의의를 둬서 안보게되네요",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006089.1.18",
          "form": "저도요..ㅡ온라인공연은 집중을 못하겠어요",
          "original_form": "저도요..ㅡ온라인공연은 집중을 못하겠어요",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100006089.1.19",
          "form": "네 그리고 재미도 덜하구요",
          "original_form": "네 그리고 재미도 덜하구요",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100006089.1.20",
          "form": "직접 보는만큼의 흥분이없네요",
          "original_form": "직접 보는만큼의 흥분이없네요",
          "speaker_id": "2",
          "time": "20210907 20:38"
        }
