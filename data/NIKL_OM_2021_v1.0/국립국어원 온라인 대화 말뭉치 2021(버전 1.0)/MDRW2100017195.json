        {
          "id": "MDRW2100017195.1.1",
          "form": "여행지 어디가 좋으세요?",
          "original_form": "여행지 어디가 좋으세요?",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017195.1.2",
          "form": "대만이요!",
          "original_form": "대만이요!",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017195.1.3",
          "form": "아하 전 태국이여",
          "original_form": "아하 전 태국이여",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017195.1.4",
          "form": "오 태국 가보고싶어요",
          "original_form": "오 태국 가보고싶어요",
          "speaker_id": "1",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017195.1.5",
          "form": "푸켓 좋았어요",
          "original_form": "푸켓 좋았어요",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017195.1.6",
          "form": "오오 음식도맛있나요",
          "original_form": "오오 음식도맛있나요",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100017195.1.7",
          "form": "음식은 별루ㅠㅠ 환락가가 좋았어요ㅋ",
          "original_form": "음식은 별루ㅠㅠ 환락가가 좋았어요ㅋ",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100017195.1.8",
          "form": "헉 유흥이요.?",
          "original_form": "헉 유흥이요.?",
          "speaker_id": "1",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100017195.1.9",
          "form": "우리랑 다른 문화가 신기하더라구요",
          "original_form": "우리랑 다른 문화가 신기하더라구요",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100017195.1.10",
          "form": "코로나끝나면 가보고싶네요",
          "original_form": "코로나끝나면 가보고싶네요",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100017195.1.11",
          "form": "네 좋아요",
          "original_form": "네 좋아요",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100017195.1.12",
          "form": "언제끝날지ㅠ",
          "original_form": "언제끝날지ㅠ",
