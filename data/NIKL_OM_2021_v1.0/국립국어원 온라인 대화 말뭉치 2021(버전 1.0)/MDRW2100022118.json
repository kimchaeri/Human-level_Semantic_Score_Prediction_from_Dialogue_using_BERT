      "utterance": [
        {
          "id": "MDRW2100022118.1.1",
          "form": "벌써 점심시간이네요",
          "original_form": "벌써 점심시간이네요",
          "speaker_id": "1",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100022118.1.2",
          "form": "전 2시에 밥먹어서.. \r\n점심뭐드시마요?",
          "original_form": "전 2시에 밥먹어서.. \r\n점심뭐드시마요?",
          "speaker_id": "2",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022118.1.3",
          "form": "파스타를 해먹을까하고 있어오!",
          "original_form": "파스타를 해먹을까하고 있어오!",
          "speaker_id": "1",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022118.1.4",
          "form": "어떤 종류요? 하얀거?뻘건거? ㅋㅋㅋ",
          "original_form": "어떤 종류요? 하얀거?뻘건거? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022118.1.5",
          "form": "그걸 못골랐어요!!ㅋㅋ",
          "original_form": "그걸 못골랐어요!!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022118.1.6",
          "form": "아직도 눈씨름 중이예요",
          "original_form": "아직도 눈씨름 중이예요",
          "speaker_id": "1",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022118.1.7",
          "form": "오늘은 비오니까.. 빨간거로..ㅋㅋㅋ \r\n낮부터 알콜 한잔이랑.. ㅋㅋㅋ",
          "original_form": "오늘은 비오니까.. 빨간거로..ㅋㅋㅋ \r\n낮부터 알콜 한잔이랑.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022118.1.8",
          "form": "ㅋㅋㅋ아 혹하네요ㅋㅋ",
          "original_form": "ㅋㅋㅋ아 혹하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:03"
        },
        {
          "id": "MDRW2100022118.1.9",
          "form": "면종류 좋아하시나여?",
          "original_form": "면종류 좋아하시나여?",
          "speaker_id": "2",
          "time": "20210829 12:04"
        },
        {
          "id": "MDRW2100022118.1.10",
          "form": "네! 완전좋아해요",
          "original_form": "네! 완전좋아해요",
          "speaker_id": "1",
          "time": "20210829 12:06"
        },
        {
          "id": "MDRW2100022118.1.11",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 12:06"
        },
        {
          "id": "MDRW2100022118.1.12",
          "form": "전 점심은 거의 면으로.. ㅋㅋㅋ",
          "original_form": "전 점심은 거의 면으로.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:06"
        },
        {
          "id": "MDRW2100022118.1.13",
          "form": "쌀보다 더 좋아요",
          "original_form": "쌀보다 더 좋아요",
          "speaker_id": "2",
          "time": "20210829 12:06"
        },
        {
          "id": "MDRW2100022118.1.14",
          "form": "저랑 같으시군요!!",
          "original_form": "저랑 같으시군요!!",
          "speaker_id": "1",
          "time": "20210829 12:07"
        },
        {
          "id": "MDRW2100022118.1.15",
          "form": "밥은 안먹어도 면안먹음 안돼요ㅋㄱㅋ",
          "original_form": "밥은 안먹어도 면안먹음 안돼요ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210829 12:07"
        },
        {
          "id": "MDRW2100022118.1.16",
          "form": "맞아요.. 전 그 면의 쫀득한 식감?이 넘 좋아요.. \r\n그래서 우동이나 파스타류가 최애..",
          "original_form": "맞아요.. 전 그 면의 쫀득한 식감?이 넘 좋아요.. \r\n그래서 우동이나 파스타류가 최애..",
          "speaker_id": "2",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022118.1.17",
          "form": "크으 저도요!",
          "original_form": "크으 저도요!",
          "speaker_id": "1",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022118.1.18",
          "form": "이야기하니 더 배고프네요ㅋㅋㅋ",
