      "utterance": [
        {
          "id": "MDRW2100002829.1.1",
          "form": "여행 좋아요?",
          "original_form": "여행 좋아요?",
          "speaker_id": "1",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002829.1.2",
          "form": "네 좋아하는 편이에요",
          "original_form": "네 좋아하는 편이에요",
          "speaker_id": "2",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002829.1.3",
          "form": "최근에 여행하신적 있어요?",
          "original_form": "최근에 여행하신적 있어요?",
          "speaker_id": "2",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002829.1.4",
          "form": "남해요",
          "original_form": "남해요",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002829.1.5",
          "form": "이번 여름에 다녀오신건가요?",
          "original_form": "이번 여름에 다녀오신건가요?",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002829.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002829.1.7",
          "form": "좋으셨겠다 저는 이달말일에 부산으로 여행 계획중이에요",
          "original_form": "좋으셨겠다 저는 이달말일에 부산으로 여행 계획중이에요",
          "speaker_id": "2",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002829.1.8",
          "form": "부산더 좋죠",
          "original_form": "부산더 좋죠",
          "speaker_id": "1",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002829.1.9",
          "form": "요즘은 코로나때문에",
          "original_form": "요즘은 코로나때문에",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002829.1.10",
          "form": "여행하기가 너무 어려워요",
          "original_form": "여행하기가 너무 어려워요",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002829.1.11",
          "form": "네 시국이",
          "original_form": "네 시국이",
          "speaker_id": "1",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002829.1.12",
          "form": "저 작년에 결혼했는데 신혼여행을 제주도로 다녀왔어요",
          "original_form": "저 작년에 결혼했는데 신혼여행을 제주도로 다녀왔어요",
          "speaker_id": "2",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002829.1.13",
          "form": "아내가 많이 실망했었어요",
          "original_form": "아내가 많이 실망했었어요",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002829.1.14",
          "form": "해외못가서요?",
          "original_form": "해외못가서요?",
          "speaker_id": "1",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002829.1.15",
          "form": "그런데 제주도 가는 비행기에 신혼부부들 정말 많더라고요",
          "original_form": "그런데 제주도 가는 비행기에 신혼부부들 정말 많더라고요",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002829.1.16",
          "form": "네 해외 못가서 실망하더라고요",
          "original_form": "네 해외 못가서 실망하더라고요",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002829.1.17",
          "form": "저는 특별히 싫거나 하지는 않았는데",
          "original_form": "저는 특별히 싫거나 하지는 않았는데",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002829.1.18",
          "form": "한번뿐인데 시국이",
          "original_form": "한번뿐인데 시국이",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002829.1.19",
          "form": "그쵸 어쩔수 없었죠",
          "original_form": "그쵸 어쩔수 없었죠",
          "speaker_id": "2",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002829.1.20",
          "form": "사실 결혼식 무사히 치른것도 다행이에요",
          "original_form": "사실 결혼식 무사히 치른것도 다행이에요",
