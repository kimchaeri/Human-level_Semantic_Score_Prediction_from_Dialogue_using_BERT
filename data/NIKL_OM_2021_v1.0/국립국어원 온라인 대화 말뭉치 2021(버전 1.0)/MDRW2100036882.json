      "utterance": [
        {
          "id": "MDRW2100036882.1.1",
          "form": "힐데스 우리 집에서 보이잖아요",
          "original_form": "힐데스 우리 집에서 보이잖아요",
          "speaker_id": "2",
          "time": "20210831 14:27"
        },
        {
          "id": "MDRW2100036882.1.2",
          "form": "땀 흘리면서 게임 함",
          "original_form": "땀 흘리면서 게임 함",
          "speaker_id": "2",
          "time": "20210831 14:27"
        },
        {
          "id": "MDRW2100036882.1.3",
          "form": "그 작은애도",
          "original_form": "그 작은애도",
          "speaker_id": "1",
          "time": "20210831 14:27"
        },
        {
          "id": "MDRW2100036882.1.4",
          "form": "힐데스가??",
          "original_form": "힐데스가??",
          "speaker_id": "1",
          "time": "20210831 14:27"
        },
        {
          "id": "MDRW2100036882.1.5",
          "form": "백십 동 살아요ㅋㅋㅋ",
          "original_form": "백십 동 살아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:27"
        },
        {
          "id": "MDRW2100036882.1.6",
          "form": "땀 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "땀 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210831 14:27"
        },
        {
          "id": "MDRW2100036882.1.7",
          "form": "ㅋㅋㅋㄱㄱㅋ 웃기노",
          "original_form": "ㅋㅋㅋㄱㄱㅋ 웃기노",
          "speaker_id": "1",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100036882.1.8",
          "form": "알고 보니 우리 20층 살 때 15층 살던 아이인",
          "original_form": "알고 보니 우리 20층 살 때 15층 살던 아이인",
          "speaker_id": "2",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100036882.1.9",
          "form": "ㅏ 그럼 그때 사진에 있었던 네 명 중에",
          "original_form": "ㅏ 그럼 그때 사진에 있었던 네 명 중에",
          "speaker_id": "1",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100036882.1.10",
          "form": "반도에 사는 애는 아무도 없네",
          "original_form": "반도에 사는 애는 아무도 없네",
          "speaker_id": "1",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100036882.1.11",
          "form": "근데 다 반도에서 놀고",
          "original_form": "근데 다 반도에서 놀고",
          "speaker_id": "1",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100036882.1.12",
          "form": "아 15층",
          "original_form": "아 15층",
          "speaker_id": "1",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100036882.1.13",
          "form": "반도는 다 거쳐 가는 곳ㅋㅋㅋ",
          "original_form": "반도는 다 거쳐 가는 곳ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100036882.1.14",
          "form": "거기가 그늘져서 시원 하더라",
          "original_form": "거기가 그늘져서 시원 하더라",
          "speaker_id": "1",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100036882.1.15",
          "form": "예전 우미린 같은 곳이죠",
          "original_form": "예전 우미린 같은 곳이죠",
          "speaker_id": "2",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100036882.1.16",
          "form": "다른 놀이터는 다 햇볕 쨍쨍 임",
          "original_form": "다른 놀이터는 다 햇볕 쨍쨍 임",
          "speaker_id": "1",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100036882.1.17",
          "form": "거기가 약간 구석 지고 사람도 없고 그늘지고",
          "original_form": "거기가 약간 구석 지고 사람도 없고 그늘지고",
          "speaker_id": "1",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100036882.1.18",
          "form": "둘이 피아노 같이 다녀서",
          "original_form": "둘이 피아노 같이 다녀서",
          "speaker_id": "2",
          "time": "20210831 14:29"
        },
        {
          "id": "MDRW2100036882.1.19",
          "form": "마치면 늘 게임 함ㅋㅋㅋㅋ",
          "original_form": "마치면 늘 게임 함ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:29"
        },
        {
          "id": "MDRW2100036882.1.20",
          "form": "한 날은 7시 돼도 안 들어오길래",
          "original_form": "한 날은 7시 돼도 안 들어오길래",
          "speaker_id": "2",
          "time": "20210831 14:29"
        },
        {
          "id": "MDRW2100036882.1.21",
          "form": "피아노를 같이 다니는데 마치고 늘 게임하는게 웃기네",
          "original_form": "피아노를 같이 다니는데 마치고 늘 게임하는게 웃기네",
          "speaker_id": "1",
          "time": "20210831 14:29"
        },
        {
          "id": "MDRW2100036882.1.22",
          "form": "들어오라고",
          "original_form": "들어오라고",
          "speaker_id": "2",
          "time": "20210831 14:29"
        },
        {
          "id": "MDRW2100036882.1.23",
          "form": "친구는 엄마가 안 부르냐고",
          "original_form": "친구는 엄마가 안 부르냐고",
          "speaker_id": "2",
          "time": "20210831 14:29"
        },
        {
          "id": "MDRW2100036882.1.24",
          "form": "물으니 자기 엄마만 전화한대요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "물으니 자기 엄마만 전화한대요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:29"
        },
        {
          "id": "MDRW2100036882.1.25",
          "form": "나만 나쁜 사람ㅋㅋㅋ",
          "original_form": "나만 나쁜 사람ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:29"
        },
        {
          "id": "MDRW2100036882.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:29"
        },
        {
          "id": "MDRW2100036882.1.27",
          "form": "아니 그엄마는 그시간까지",
          "original_form": "아니 그엄마는 그시간까지",
          "speaker_id": "1",
          "time": "20210831 14:30"
        },
        {
          "id": "MDRW2100036882.1.28",
          "form": "딸래미가 안들어오는데",
          "original_form": "딸래미가 안들어오는데",
          "speaker_id": "1",
          "time": "20210831 14:30"
        },
        {
          "id": "MDRW2100036882.1.29",
          "form": "왜 전화를 안하노",
          "original_form": "왜 전화를 안하노",
          "speaker_id": "1",
          "time": "20210831 14:30"
        },
        {
          "id": "MDRW2100036882.1.30",
          "form": "그러니까요 전화 안 온대요",
          "original_form": "그러니까요 전화 안 온대요",
          "speaker_id": "2",
          "time": "20210831 14:30"
        },
        {
          "id": "MDRW2100036882.1.31",
          "form": "희안하네 거기도",
          "original_form": "희안하네 거기도",
          "speaker_id": "1",
          "time": "20210831 14:30"
        },
        {
          "id": "MDRW2100036882.1.32",
          "form": "우리 원이랑 전에 놀이터에서 같이 놀았던 초등학교 친구도",
          "original_form": "우리 원이랑 전에 놀이터에서 같이 놀았던 초등학교 친구도",
          "speaker_id": "1",
          "time": "20210831 14:30"
        },
        {
          "id": "MDRW2100036882.1.33",
          "form": "하물며 외동 ㅋㅋㅋ",
          "original_form": "하물며 외동 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:30"
        },
        {
          "id": "MDRW2100036882.1.34",
          "form": "8시까지 엄마가 전화가 안 오는 거야",
          "original_form": "8시까지 엄마가 전화가 안 오는 거야",
          "speaker_id": "1",
          "time": "20210831 14:30"
        }
      ]
