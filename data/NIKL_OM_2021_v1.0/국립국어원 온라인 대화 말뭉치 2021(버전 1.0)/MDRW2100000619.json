      "utterance": [
        {
          "id": "MDRW2100000619.1.1",
          "form": "여행가고 싶다",
          "original_form": "여행가고 싶다",
          "speaker_id": "1",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000619.1.2",
          "form": "난 국내여행도 좋은데",
          "original_form": "난 국내여행도 좋은데",
          "speaker_id": "2",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000619.1.3",
          "form": "외국은 위험하다",
          "original_form": "외국은 위험하다",
          "speaker_id": "1",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000619.1.4",
          "form": "한동안 그렇겠지",
          "original_form": "한동안 그렇겠지",
          "speaker_id": "2",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000619.1.5",
          "form": "몇년은 그냥 국내여행으로만",
          "original_form": "몇년은 그냥 국내여행으로만",
          "speaker_id": "1",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000619.1.6",
          "form": "제주도도 좋지뭐",
          "original_form": "제주도도 좋지뭐",
          "speaker_id": "2",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000619.1.7",
          "form": "경주도 괜찮을듯하다",
          "original_form": "경주도 괜찮을듯하다",
          "speaker_id": "1",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000619.1.8",
          "form": "응 경주 한옥에 자면서",
          "original_form": "응 경주 한옥에 자면서",
          "speaker_id": "2",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000619.1.9",
          "form": "맞아 언제 한옥집에서 자보겠어",
          "original_form": "맞아 언제 한옥집에서 자보겠어",
          "speaker_id": "1",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000619.1.10",
          "form": "경주 문화재도 꼼꼼이 보고",
          "original_form": "경주 문화재도 꼼꼼이 보고",
          "speaker_id": "2",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000619.1.11",
          "form": "야경투어도 좋다고하더리",
          "original_form": "야경투어도 좋다고하더리",
          "speaker_id": "1",
          "time": "20210819 17:15"
        },
        {
          "id": "MDRW2100000619.1.12",
          "form": "운전면허 없는 우리한테는 최고일듯",
          "original_form": "운전면허 없는 우리한테는 최고일듯",
          "speaker_id": "2",
          "time": "20210819 17:15"
        },
        {
          "id": "MDRW2100000619.1.13",
          "form": "운전을 못하니 불편하다",
          "original_form": "운전을 못하니 불편하다",
          "speaker_id": "1",
          "time": "20210819 17:15"
        },
        {
          "id": "MDRW2100000619.1.14",
          "form": "올해 해맞이 갔어?",
          "original_form": "올해 해맞이 갔어?",
          "speaker_id": "1",
          "time": "20210820 17:58"
        }
