      "utterance": [
        {
          "id": "MDRW2100040682.1.1",
          "form": "정말 망해도 쌉니다",
          "original_form": "정말 망해도 쌉니다",
          "speaker_id": "1",
          "time": "20210905 00:05"
        },
        {
          "id": "MDRW2100040682.1.2",
          "form": "ㅋㅋㅋ 진짜요",
          "original_form": "ㅋㅋㅋ 진짜요",
          "speaker_id": "2",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040682.1.3",
          "form": "회사 다니고 계신가요?",
          "original_form": "회사 다니고 계신가요?",
          "speaker_id": "2",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040682.1.4",
          "form": "아뇨 회사 안 다녀요",
          "original_form": "아뇨 회사 안 다녀요",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040682.1.5",
          "form": "저도 그만둔지 꽤 됐답니다",
          "original_form": "저도 그만둔지 꽤 됐답니다",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040682.1.6",
          "form": "취업 어떻게 하는걸까요",
          "original_form": "취업 어떻게 하는걸까요",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040682.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040682.1.8",
          "form": "봤어요",
          "original_form": "봤어요",
          "speaker_id": "1",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040682.1.9",
          "form": "저 어제 면접 봤어요",
          "original_form": "저 어제 면접 봤어요",
          "speaker_id": "1",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040682.1.10",
          "form": "오우 느낌 좋으세요???",
          "original_form": "오우 느낌 좋으세요???",
          "speaker_id": "2",
          "time": "20210905 00:19"
        },
        {
          "id": "MDRW2100040682.1.11",
          "form": "모르겠어요 저는 별 잘못 없는 거 같은데",
          "original_form": "모르겠어요 저는 별 잘못 없는 거 같은데",
          "speaker_id": "1",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100040682.1.12",
          "form": "거기가 열심히 일할 사람을 뽑고 싶어 하는 줄 모르겠어요",
          "original_form": "거기가 열심히 일할 사람을 뽑고 싶어 하는 줄 모르겠어요",
          "speaker_id": "1",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100040682.1.13",
          "form": "연락오면 가실거에요??",
          "original_form": "연락오면 가실거에요??",
          "speaker_id": "2",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100040682.1.14",
          "form": "네 연락 오면 가서 일해야죠",
          "original_form": "네 연락 오면 가서 일해야죠",
          "speaker_id": "1",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100040682.1.15",
          "form": "느낌은 별로신거 같으면 안가시는게 나으려나여",
          "original_form": "느낌은 별로신거 같으면 안가시는게 나으려나여",
          "speaker_id": "2",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100040682.1.16",
          "form": "아 역싷ㅎㅎ",
          "original_form": "아 역싷ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100040682.1.17",
          "form": "일단 가서 일해 보고 그만두던지 해야죠",
          "original_form": "일단 가서 일해 보고 그만두던지 해야죠",
          "speaker_id": "1",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100040682.1.18",
          "form": "요즘 취업시장 진짜",
          "original_form": "요즘 취업시장 진짜",
          "speaker_id": "2",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100040682.1.19",
          "form": "연락오면 가야될거 같아요",
          "original_form": "연락오면 가야될거 같아요",
          "speaker_id": "2",
          "time": "20210905 00:22"
        },
        {
          "id": "MDRW2100040682.1.20",
          "form": "면접을 보는데 조금 높은 사람 있고 인사부 사람 있고",
          "original_form": "면접을 보는데 조금 높은 사람 있고 인사부 사람 있고",
          "speaker_id": "1",
          "time": "20210905 00:22"
        },
        {
          "id": "MDRW2100040682.1.21",
          "form": "ㅋㅋㅋ 그렇져",
          "original_form": "ㅋㅋㅋ 그렇져",
          "speaker_id": "2",
          "time": "20210905 00:22"
        },
        {
          "id": "MDRW2100040682.1.22",
          "form": "저랑 같이 일할 거 같은 사람 있더라고요",
          "original_form": "저랑 같이 일할 거 같은 사람 있더라고요",
          "speaker_id": "1",
          "time": "20210905 00:22"
        },
        {
          "id": "MDRW2100040682.1.23",
          "form": "근데 면접 질문하는데 저랑 일할 거 같은 사람이 엉뚱한 걸로 태클을 거는 거예요",
          "original_form": "근데 면접 질문하는데 저랑 일할 거 같은 사람이 엉뚱한 걸로 태클을 거는 거예요",
          "speaker_id": "1",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100040682.1.24",
          "form": "그거 보고 좀 아닌 거 같다고 생각했어요",
          "original_form": "그거 보고 좀 아닌 거 같다고 생각했어요",
          "speaker_id": "1",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100040682.1.25",
          "form": "일부러 그런걸까요?",
          "original_form": "일부러 그런걸까요?",
          "speaker_id": "2",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100040682.1.26",
          "form": "반응볼라고? 좀 별로네여",
          "original_form": "반응볼라고? 좀 별로네여",
          "speaker_id": "2",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100040682.1.27",
          "form": "지금까지 대충 일했는데 제가 들어오면 자기가 밀릴 거 같다고 생각하는 거 아니에요?",
          "original_form": "지금까지 대충 일했는데 제가 들어오면 자기가 밀릴 거 같다고 생각하는 거 아니에요?",
          "speaker_id": "1",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100040682.1.28",
          "form": "그래도 일해보면 또 다를수도 있긴하니까",
          "original_form": "그래도 일해보면 또 다를수도 있긴하니까",
          "speaker_id": "2",
          "time": "20210905 00:25"
        }
      ]
