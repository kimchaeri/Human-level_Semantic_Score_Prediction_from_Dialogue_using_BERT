      "utterance": [
        {
          "id": "MDRW2100042623.1.1",
          "form": "없어",
          "original_form": "없어",
          "speaker_id": "2",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100042623.1.2",
          "form": "그저께 다썼어",
          "original_form": "그저께 다썼어",
          "speaker_id": "2",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100042623.1.3",
          "form": "내일 사러가야대",
          "original_form": "내일 사러가야대",
          "speaker_id": "2",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100042623.1.4",
          "form": "ㅇㅎ....",
          "original_form": "ㅇㅎ....",
          "speaker_id": "1",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100042623.1.5",
          "form": "요리의 필수품",
          "original_form": "요리의 필수품",
          "speaker_id": "1",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100042623.1.6",
          "form": "매운건 잘 못먹지만",
          "original_form": "매운건 잘 못먹지만",
          "speaker_id": "1",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100042623.1.7",
          "form": "집에 라면도 없어서",
          "original_form": "집에 라면도 없어서",
          "speaker_id": "2",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100042623.1.8",
          "form": "이 기괴한 볶음우동을 먹어야해",
          "original_form": "이 기괴한 볶음우동을 먹어야해",
          "speaker_id": "2",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100042623.1.9",
          "form": "심지어 아까",
          "original_form": "심지어 아까",
          "speaker_id": "2",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100042623.1.10",
          "form": "애매한 잡탕을 만들엇다면 일단 땡초 반개를 썰어넣고 시작하지",
          "original_form": "애매한 잡탕을 만들엇다면 일단 땡초 반개를 썰어넣고 시작하지",
          "speaker_id": "1",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100042623.1.11",
          "form": "헷갈려서",
          "original_form": "헷갈려서",
          "speaker_id": "2",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042623.1.12",
          "form": "후추대신 큐민을 뿌렸어",
          "original_form": "후추대신 큐민을 뿌렸어",
          "speaker_id": "2",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042623.1.13",
          "form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042623.1.14",
          "form": "땡초가 없는데",
          "original_form": "땡초가 없는데",
          "speaker_id": "1",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042623.1.15",
          "form": "큐민은 왜잇는디 ㅋㅋㅋㅋ",
          "original_form": "큐민은 왜잇는디 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042623.1.16",
          "form": "짬뽕보다 더 짬뽕이란 수식이",
          "original_form": "짬뽕보다 더 짬뽕이란 수식이",
          "speaker_id": "2",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042623.1.17",
          "form": "잘 어울리지",
          "original_form": "잘 어울리지",
          "speaker_id": "2",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042623.1.18",
          "form": "아 큐민은",
          "original_form": "아 큐민은",
          "speaker_id": "2",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042623.1.19",
          "form": "동생놈이",
          "original_form": "동생놈이",
          "speaker_id": "2",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042623.1.20",
          "form": "혼자 살면서",
          "original_form": "혼자 살면서",
          "speaker_id": "2",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042623.1.21",
          "form": "요오리란걸",
          "original_form": "요오리란걸",
          "speaker_id": "2",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042623.1.22",
          "form": "하겠답시고 사서는",
          "original_form": "하겠답시고 사서는",
          "speaker_id": "2",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042623.1.23",
          "form": "지는 못쓰겠다고",
          "original_form": "지는 못쓰겠다고",
          "speaker_id": "2",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042623.1.24",
          "form": "집으로 들고온지",
          "original_form": "집으로 들고온지",
          "speaker_id": "2",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042623.1.25",
          "form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042623.1.26",
          "form": "1년차.....",
          "original_form": "1년차.....",
          "speaker_id": "2",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042623.1.27",
          "form": "역시...",
          "original_form": "역시...",
          "speaker_id": "1",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042623.1.28",
          "form": "가끔 카레해먹을때",
          "original_form": "가끔 카레해먹을때",
          "speaker_id": "2",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042623.1.29",
          "form": "조금씩 뿌려서",
          "original_form": "조금씩 뿌려서",
          "speaker_id": "2",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042623.1.30",
          "form": "섞어먹고있어",
          "original_form": "섞어먹고있어",
          "speaker_id": "2",
          "time": "20210906 23:53"
        },
        {
          "id": "MDRW2100042623.1.31",
          "form": "그 외엔 쓸일이 없지....",
          "original_form": "그 외엔 쓸일이 없지....",
          "speaker_id": "2",
          "time": "20210906 23:53"
        },
        {
          "id": "MDRW2100042623.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그치",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그치",
          "speaker_id": "1",
          "time": "20210906 23:53"
        },
        {
          "id": "MDRW2100042623.1.33",
          "form": "솔직히 큐민 향 너무 진해...",
          "original_form": "솔직히 큐민 향 너무 진해...",
          "speaker_id": "2",
          "time": "20210906 23:53"
        },
        {
          "id": "MDRW2100042623.1.34",
          "form": "아니면 양고기 찍어먹거나",
          "original_form": "아니면 양고기 찍어먹거나",
          "speaker_id": "1",
          "time": "20210906 23:53"
        },
        {
          "id": "MDRW2100042623.1.35",
          "form": "ㅇㅇ 그래서 보통",
          "original_form": "ㅇㅇ 그래서 보통",
          "speaker_id": "1",
          "time": "20210906 23:54"
        },
        {
          "id": "MDRW2100042623.1.36",
          "form": "고기요리에 잡내 제거용으로 쓰더라...",
          "original_form": "고기요리에 잡내 제거용으로 쓰더라...",
          "speaker_id": "1",
          "time": "20210906 23:54"
        }
      ]
    }
