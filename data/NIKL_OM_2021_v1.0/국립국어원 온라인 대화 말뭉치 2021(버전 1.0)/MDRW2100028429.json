      "utterance": [
        {
          "id": "MDRW2100028429.1.1",
          "form": "게임 하는거 있어?",
          "original_form": "게임 하는거 있어?",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028429.1.2",
          "form": "음 서든 피파 롤 데바데?",
          "original_form": "음 서든 피파 롤 데바데?",
          "speaker_id": "2",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028429.1.3",
          "form": "완전 많이 한다",
          "original_form": "완전 많이 한다",
          "speaker_id": "1",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028429.1.4",
          "form": "뭐가 제일 재밌어?",
          "original_form": "뭐가 제일 재밌어?",
          "speaker_id": "1",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028429.1.5",
          "form": "데바데!",
          "original_form": "데바데!",
          "speaker_id": "2",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100028429.1.6",
          "form": "그건 무슨 게임이야?",
          "original_form": "그건 무슨 게임이야?",
          "speaker_id": "1",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100028429.1.7",
          "form": "데바데 몰라?",
          "original_form": "데바데 몰라?",
          "speaker_id": "2",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028429.1.8",
          "form": "사실 나도 알아",
          "original_form": "사실 나도 알아",
          "speaker_id": "1",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028429.1.9",
          "form": "근데 왜물어봐 ㅡㅡ",
          "original_form": "근데 왜물어봐 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028429.1.10",
          "form": "ㅜㅠ 너 몇랭크야?",
          "original_form": "ㅜㅠ 너 몇랭크야?",
          "speaker_id": "1",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028429.1.11",
          "form": "나 1랭!!",
          "original_form": "나 1랭!!",
          "speaker_id": "2",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028429.1.12",
          "form": "거짓말",
          "original_form": "거짓말",
          "speaker_id": "1",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028429.1.13",
          "form": "진짜야!!!!",
          "original_form": "진짜야!!!!",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028429.1.14",
          "form": "얼마나 잘하는데",
          "original_form": "얼마나 잘하는데",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028429.1.15",
          "form": "생존자 살인마 둘다 1랭크야?",
          "original_form": "생존자 살인마 둘다 1랭크야?",
          "speaker_id": "1",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028429.1.16",
          "form": "응 개잘하지",
          "original_form": "응 개잘하지",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028429.1.17",
          "form": "나돈데",
          "original_form": "나돈데",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028429.1.18",
          "form": "살인마 주캐가 뭐야?",
          "original_form": "살인마 주캐가 뭐야?",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028429.1.19",
          "form": "너스! 토구!",
          "original_form": "너스! 토구!",
          "speaker_id": "2",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100028429.1.20",
          "form": "개자래",
          "original_form": "개자래",
          "speaker_id": "2",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100028429.1.21",
          "form": "오 멋있다",
          "original_form": "오 멋있다",
          "speaker_id": "1",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100028429.1.22",
          "form": "생존자는 주캐 뭐야?",
          "original_form": "생존자는 주캐 뭐야?",
          "speaker_id": "1",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100028429.1.23",
          "form": "생존자는 할배",
          "original_form": "생존자는 할배",
          "speaker_id": "2",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100028429.1.24",
          "form": "너는 현질 안해? 다 기본 캐릭터네",
          "original_form": "너는 현질 안해? 다 기본 캐릭터네",
          "speaker_id": "1",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100028429.1.25",
          "form": "데바데에 현질안해!",
          "original_form": "데바데에 현질안해!",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028429.1.26",
          "form": "다른 게임에는 해?",
          "original_form": "다른 게임에는 해?",
          "speaker_id": "1",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028429.1.27",
          "form": "아주가끔? 전에는 좀햇어",
          "original_form": "아주가끔? 전에는 좀햇어",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028429.1.28",
          "form": "얼마나 해봤어?",
          "original_form": "얼마나 해봤어?",
          "speaker_id": "1",
          "time": "20210903 10:29"
        }
      ]
    }
