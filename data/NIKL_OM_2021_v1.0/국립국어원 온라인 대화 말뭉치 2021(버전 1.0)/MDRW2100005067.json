        },
        {
          "id": "MDRW2100005067.1.3",
          "form": "마크는",
          "original_form": "마크는",
          "speaker_id": "2",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005067.1.4",
          "form": "해찬이랑 사귀는건가?",
          "original_form": "해찬이랑 사귀는건가?",
          "speaker_id": "2",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005067.1.5",
          "form": "셋이 아기때 사진도 걸어두고",
          "original_form": "셋이 아기때 사진도 걸어두고",
          "speaker_id": "1",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005067.1.6",
          "form": "귀엽다",
          "original_form": "귀엽다",
          "speaker_id": "1",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005067.1.7",
          "form": "그런것같기도",
          "original_form": "그런것같기도",
          "speaker_id": "1",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005067.1.8",
          "form": "둘이 보다보면 초딩들 연애하는 것 같아",
          "original_form": "둘이 보다보면 초딩들 연애하는 것 같아",
          "speaker_id": "1",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005067.1.9",
          "form": "아 맞아 초딩",
          "original_form": "아 맞아 초딩",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005067.1.10",
          "form": "그말이 딱맞지",
          "original_form": "그말이 딱맞지",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005067.1.11",
          "form": "특히 이해찬",
          "original_form": "특히 이해찬",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005067.1.12",
          "form": "초딩같아",
          "original_form": "초딩같아",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005067.1.13",
          "form": "아까 신문 거꾸로보던데",
          "original_form": "아까 신문 거꾸로보던데",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005067.1.14",
          "form": "웃기고 귀여워서 말 안했어",
          "original_form": "웃기고 귀여워서 말 안했어",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005067.1.15",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005067.1.16",
          "form": "넘귀엽다",
          "original_form": "넘귀엽다",
          "speaker_id": "2",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005067.1.17",
          "form": "쟈니선배랑 곰돌이 키링 맞췄던데",
          "original_form": "쟈니선배랑 곰돌이 키링 맞췄던데",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005067.1.18",
          "form": "기년으로 사진찍더라",
          "original_form": "기년으로 사진찍더라",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005067.1.19",
          "form": "기념",
          "original_form": "기념",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005067.1.20",
          "form": "그래서 쟈니선배한테 삥뜯으려 했는데 실패함ㅡㅡ",
          "original_form": "그래서 쟈니선배한테 삥뜯으려 했는데 실패함ㅡㅡ",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005067.1.21",
          "form": "그래서 그 키링 따라 사려고 중국배송 찾아봤는데 10월에 오더라",
          "original_form": "그래서 그 키링 따라 사려고 중국배송 찾아봤는데 10월에 오더라",
          "speaker_id": "1",
          "time": "20210823 18:06"
        },
        {
          "id": "MDRW2100005067.1.22",
          "form": "그냥 쟈니선배나 해찬이꺼 삥뜯는게 빠를듯",
          "original_form": "그냥 쟈니선배나 해찬이꺼 삥뜯는게 빠를듯",
          "speaker_id": "1",
          "time": "20210823 18:06"
        },
        {
          "id": "MDRW2100005067.1.23",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "2",
          "time": "20210823 20:47"
        },
        {
          "id": "MDRW2100005067.1.24",
          "form": "아니 그거 트위터에도 공구하길래",
          "original_form": "아니 그거 트위터에도 공구하길래",
          "speaker_id": "2",
          "time": "20210823 20:47"
        },
        {
          "id": "MDRW2100005067.1.25",
          "form": "들어갔는데",
          "original_form": "들어갔는데",
          "speaker_id": "2",
          "time": "20210823 20:47"
        },
        {
          "id": "MDRW2100005067.1.26",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210823 20:47"
        },
        {
          "id": "MDRW2100005067.1.27",
          "form": "품절ㅜㅠ",
          "original_form": "품절ㅜㅠ",
          "speaker_id": "2",
          "time": "20210823 20:47"
        },
        {
          "id": "MDRW2100005067.1.28",
          "form": "해찬이한테 삥뜯는게 빠를듯",
          "original_form": "해찬이한테 삥뜯는게 빠를듯",
          "speaker_id": "2",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100005067.1.29",
          "form": "그럼 남은방법은 그것 뿐이네",
          "original_form": "그럼 남은방법은 그것 뿐이네",
          "speaker_id": "1",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100005067.1.30",
          "form": "걔네 학교 언제오니",
          "original_form": "걔네 학교 언제오니",
          "speaker_id": "2",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100005067.1.31",
          "form": "넌 해찬이한테 뜯어",
          "original_form": "넌 해찬이한테 뜯어",
          "speaker_id": "1",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100005067.1.32",
          "form": "난 쟈니선배한테 뜯을게",
          "original_form": "난 쟈니선배한테 뜯을게",
          "speaker_id": "1",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100005067.1.33",
          "form": "저번주까진 자주 오던데",
          "original_form": "저번주까진 자주 오던데",
          "speaker_id": "1",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100005067.1.34",
          "form": "오늘 휴학계 냈다는 소리 있더라",
          "original_form": "오늘 휴학계 냈다는 소리 있더라",
          "speaker_id": "1",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100005067.1.35",
