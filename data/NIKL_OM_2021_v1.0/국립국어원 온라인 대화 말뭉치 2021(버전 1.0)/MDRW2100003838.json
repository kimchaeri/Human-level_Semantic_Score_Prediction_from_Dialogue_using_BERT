      "utterance": [
        {
          "id": "MDRW2100003838.1.1",
          "form": "차달남 재방송 해주는갑다 스폐셜인거 보니",
          "original_form": "차달남 재방송 해주는갑다 스폐셜인거 보니",
          "speaker_id": "2",
          "time": "20210821 12:45"
        },
        {
          "id": "MDRW2100003838.1.2",
          "form": "그거 재밌던데 뉴스 보시네ㅋ",
          "original_form": "그거 재밌던데 뉴스 보시네ㅋ",
          "speaker_id": "1",
          "time": "20210821 12:45"
        },
        {
          "id": "MDRW2100003838.1.3",
          "form": "뉴스도 좋지 뭐 새로운 뉴스거리 나왔나",
          "original_form": "뉴스도 좋지 뭐 새로운 뉴스거리 나왔나",
          "speaker_id": "2",
          "time": "20210821 12:46"
        },
        {
          "id": "MDRW2100003838.1.4",
          "form": "코로나 얘기한다",
          "original_form": "코로나 얘기한다",
          "speaker_id": "1",
          "time": "20210821 12:46"
        },
        {
          "id": "MDRW2100003838.1.5",
          "form": "이번 차달남은 재벌들 얘기인가보다",
          "original_form": "이번 차달남은 재벌들 얘기인가보다",
          "speaker_id": "2",
          "time": "20210821 12:46"
        },
        {
          "id": "MDRW2100003838.1.6",
          "form": "재벌순위?",
          "original_form": "재벌순위?",
          "speaker_id": "1",
          "time": "20210821 12:47"
        },
        {
          "id": "MDRW2100003838.1.7",
          "form": "아까 주제가 나왔었는데 기억이 안난다 재벌배고는 ㅜ.ㅜ",
          "original_form": "아까 주제가 나왔었는데 기억이 안난다 재벌배고는 ㅜ.ㅜ",
          "speaker_id": "2",
          "time": "20210821 12:47"
        },
        {
          "id": "MDRW2100003838.1.8",
          "form": "재벌들 얘기 많지 사고친 거 일수도 있고 ㅋ",
          "original_form": "재벌들 얘기 많지 사고친 거 일수도 있고 ㅋ",
          "speaker_id": "1",
          "time": "20210821 12:47"
        },
        {
          "id": "MDRW2100003838.1.9",
          "form": "재벌들의 음모론이다 그리고 스타의 리즈시절 2개 해줄려고 하나보다",
          "original_form": "재벌들의 음모론이다 그리고 스타의 리즈시절 2개 해줄려고 하나보다",
          "speaker_id": "2",
          "time": "20210821 12:48"
        },
        {
          "id": "MDRW2100003838.1.10",
          "form": "음모론 그런거 많겠지",
          "original_form": "음모론 그런거 많겠지",
          "speaker_id": "1",
          "time": "20210821 12:48"
        },
        {
          "id": "MDRW2100003838.1.11",
          "form": "엉 부를 쌓는데 조금 안좋은 일도 했겠지",
          "original_form": "엉 부를 쌓는데 조금 안좋은 일도 했겠지",
          "speaker_id": "2",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003838.1.12",
          "form": "광고가 뭐저리 길게 하지",
          "original_form": "광고가 뭐저리 길게 하지",
          "speaker_id": "2",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003838.1.13",
          "form": "원래 케이블은 광고가 길어",
          "original_form": "원래 케이블은 광고가 길어",
          "speaker_id": "1",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003838.1.14",
          "form": "길어도 너무 길다",
          "original_form": "길어도 너무 길다",
          "speaker_id": "2",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003838.1.15",
          "form": "스타의 리즈시절은 꼭 보고 싶네",
          "original_form": "스타의 리즈시절은 꼭 보고 싶네",
          "speaker_id": "2",
