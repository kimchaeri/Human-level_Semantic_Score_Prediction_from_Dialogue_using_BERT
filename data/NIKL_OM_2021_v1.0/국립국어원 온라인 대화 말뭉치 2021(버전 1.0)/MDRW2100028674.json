        },
        {
          "id": "MDRW2100028674.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028674.1.3",
          "form": "요즘 배달 시켜 드시나요?",
          "original_form": "요즘 배달 시켜 드시나요?",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028674.1.4",
          "form": "잘 안시켜요 넘 비싸요ㅠ",
          "original_form": "잘 안시켜요 넘 비싸요ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028674.1.5",
          "form": "그럼 집에서 직접 해드시는 건가요?",
          "original_form": "그럼 집에서 직접 해드시는 건가요?",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028674.1.6",
          "form": "넹... 걍 밀키트같은거나",
          "original_form": "넹... 걍 밀키트같은거나",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028674.1.7",
          "form": "반조리요 ㅋㅋ",
          "original_form": "반조리요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028674.1.8",
          "form": "맞아요 밀키트가 편하긴하죵",
          "original_form": "맞아요 밀키트가 편하긴하죵",
          "speaker_id": "1",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100028674.1.9",
          "form": "저도 얼마전에 담꾹에서 사먹었는데 맛있더라구요 ㅎㅎ",
          "original_form": "저도 얼마전에 담꾹에서 사먹었는데 맛있더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100028674.1.10",
          "form": "담꾹은 첨들어바요 브랜든가요",
          "original_form": "담꾹은 첨들어바요 브랜든가요",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028674.1.11",
          "form": "네 고퀄리티 밀키트같더라구요",
          "original_form": "네 고퀄리티 밀키트같더라구요",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028674.1.12",
          "form": "요리하기 워낙 귀찮아서요ㅠ",
          "original_form": "요리하기 워낙 귀찮아서요ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028674.1.13",
          "form": "오호? 담에시켜바야겟넹요 제가 먹어본거중엔 프레시지가 젤 괜차낫는뎅",
          "original_form": "오호? 담에시켜바야겟넹요 제가 먹어본거중엔 프레시지가 젤 괜차낫는뎅",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028674.1.14",
          "form": "오오옹그렇구낭",
          "original_form": "오오옹그렇구낭",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028674.1.15",
          "form": "맛집은 주롱 어디다니세요?",
          "original_form": "맛집은 주롱 어디다니세요?",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028674.1.16",
          "form": "ㅋㅋㅋ 흠 그냥 그때그때 찾아요 지도에서 별점 높은데로 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 흠 그냥 그때그때 찾아요 지도에서 별점 높은데로 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028674.1.17",
          "form": "맞아요 ㅋㅋㅋ네이버지도에서 별점 높은 데로 가는게 최고인 것  같아요",
          "original_form": "맞아요 ㅋㅋㅋ네이버지도에서 별점 높은 데로 가는게 최고인 것  같아요",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028674.1.18",
          "form": "ㅋㅋㅋ 무난무난 요즘 머가워낙 많아서요",
          "original_form": "ㅋㅋㅋ 무난무난 요즘 머가워낙 많아서요",
          "speaker_id": "2",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100028674.1.19",
          "form": "디저트ㅜ조아하시나요",
          "original_form": "디저트ㅜ조아하시나요",
