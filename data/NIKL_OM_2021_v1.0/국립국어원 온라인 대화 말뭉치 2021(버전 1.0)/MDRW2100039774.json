        {
          "id": "MDRW2100039774.1.1",
          "form": "데미소다 좋아하세요?",
          "original_form": "데미소다 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039774.1.2",
          "form": "네 아뇨",
          "original_form": "네 아뇨",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039774.1.3",
          "form": "탄산 안먹어요",
          "original_form": "탄산 안먹어요",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039774.1.4",
          "form": "방구뿡",
          "original_form": "방구뿡",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039774.1.5",
          "form": "오늘학교갔다가 자판기 있길래 데미소다 사먹었어요",
          "original_form": "오늘학교갔다가 자판기 있길래 데미소다 사먹었어요",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039774.1.6",
          "form": "네 아뇨는 뭐죠 ?",
          "original_form": "네 아뇨는 뭐죠 ?",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039774.1.7",
          "form": "내적자아의 분열인가요?",
          "original_form": "내적자아의 분열인가요?",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039774.1.8",
          "form": "저는 녹차마시고 있어요",
          "original_form": "저는 녹차마시고 있어요",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039774.1.9",
          "form": "녹차 좋아하시죠?",
          "original_form": "녹차 좋아하시죠?",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039774.1.10",
          "form": "id 와 ego 의 분열",
          "original_form": "id 와 ego 의 분열",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039774.1.11",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039774.1.12",
          "form": "근데 요즘엔 잘 안먹어요",
          "original_form": "근데 요즘엔 잘 안먹어요",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039774.1.13",
          "form": "id가 뭐예요",
          "original_form": "id가 뭐예요",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039774.1.14",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039774.1.15",
          "form": "id는 자아에요",
          "original_form": "id는 자아에요",
          "speaker_id": "2",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039774.1.16",
          "form": "id는 아에요",
          "original_form": "id는 아에요",
          "speaker_id": "2",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039774.1.17",
          "form": "왜 녹차를 안마시냐구요 ?",
          "original_form": "왜 녹차를 안마시냐구요 ?",
          "speaker_id": "2",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039774.1.18",
          "form": "아이디가 자아예요?",
          "original_form": "아이디가 자아예요?",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039774.1.19",
          "form": "속이 안좋아져서요",
          "original_form": "속이 안좋아져서요",
          "speaker_id": "2",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039774.1.20",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039774.1.21",
          "form": "카페인 때문에요?",
          "original_form": "카페인 때문에요?",
          "speaker_id": "1",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039774.1.22",
          "form": "눼.",
          "original_form": "눼.",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039774.1.23",
          "form": "저그래서 커피도 마시면",
          "original_form": "저그래서 커피도 마시면",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039774.1.24",
          "form": "어질어질하고",
          "original_form": "어질어질하고",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039774.1.25",
          "form": "막 심장이 쿵쾅거리고",
          "original_form": "막 심장이 쿵쾅거리고",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039774.1.26",
          "form": "그래요",
          "original_form": "그래요",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039774.1.27",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039774.1.28",
          "form": "님 커피 아예 안 드세요?",
          "original_form": "님 커피 아예 안 드세요?",
          "speaker_id": "1",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039774.1.29",
          "form": "네 안 먹어요",
          "original_form": "네 안 먹어요",
          "speaker_id": "2",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039774.1.30",
          "form": "님",
          "original_form": "님",
          "speaker_id": "1",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039774.1.31",
          "form": "당근케익 좋아하세요?",
          "original_form": "당근케익 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039774.1.32",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039774.1.33",
          "form": "왜욥",
          "original_form": "왜욥",
          "speaker_id": "1",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039774.1.34",
          "form": "전 고구맠0익이 더 졸아요",
          "original_form": "전 고구맠0익이 더 졸아요",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039774.1.35",
          "form": "오 님 저랑 취향 반절 비슷",
          "original_form": "오 님 저랑 취향 반절 비슷",
          "speaker_id": "1",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039774.1.36",
          "form": "근데 당케는 왜요?",
          "original_form": "근데 당케는 왜요?",
          "speaker_id": "1",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039774.1.37",
          "form": "반절은 뭐죠",
          "original_form": "반절은 뭐죠",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039774.1.38",
          "form": "당케는",
          "original_form": "당케는",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039774.1.39",
          "form": "그냥",
          "original_form": "그냥",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039774.1.40",
          "form": "싫어요",
          "original_form": "싫어요",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039774.1.41",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "1",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039774.1.42",
          "form": "케이크맛을 제대로 음미하고 생각해보세요",
          "original_form": "케이크맛을 제대로 음미하고 생각해보세요",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039774.1.43",
          "form": "내가 왜 당케을 미워하는지",
          "original_form": "내가 왜 당케을 미워하는지",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039774.1.44",
          "form": "싫어하는 데 이유가 있나요",
          "original_form": "싫어하는 데 이유가 있나요",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039774.1.45",
          "form": "나의 미각은 어딘가 꼬여있난게 아닌지",
          "original_form": "나의 미각은 어딘가 꼬여있난게 아닌지",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039774.1.46",
          "form": "아뇨 미워하지않아요",
          "original_form": "아뇨 미워하지않아요",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039774.1.47",
          "form": "싫어할 뷴",
          "original_form": "싫어할 뷴",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039774.1.48",
          "form": "제 미각은 매우 정상",
          "original_form": "제 미각은 매우 정상",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039774.1.49",
          "form": "왜 적당히 달달하고 담백하고 치즈크림과의 조화가 휼륭한 당케를 먹지 않는지",
          "original_form": "왜 적당히 달달하고 담백하고 치즈크림과의 조화가 휼륭한 당케를 먹지 않는지",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039774.1.50",
          "form": "궁금하군요",
          "original_form": "궁금하군요",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
