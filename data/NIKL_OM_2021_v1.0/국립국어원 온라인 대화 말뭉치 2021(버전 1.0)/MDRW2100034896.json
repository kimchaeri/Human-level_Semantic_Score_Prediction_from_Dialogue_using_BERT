      "utterance": [
        {
          "id": "MDRW2100034896.1.1",
          "form": "아무튼 지금 그",
          "original_form": "아무튼 지금 그",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100034896.1.2",
          "form": "소소떡볶이 거기 로제떡볶이 먹고 싶다",
          "original_form": "소소떡볶이 거기 로제떡볶이 먹고 싶다",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100034896.1.3",
          "form": "솔직히 리뷰는 별로 안 좋게 썼는데",
          "original_form": "솔직히 리뷰는 별로 안 좋게 썼는데",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100034896.1.4",
          "form": "김밥이 별로였어서",
          "original_form": "김밥이 별로였어서",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100034896.1.5",
          "form": "엉ㅋㅋㅋ",
          "original_form": "엉ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100034896.1.6",
          "form": "떡볶이가 너무 맛있어서 생각남...",
          "original_form": "떡볶이가 너무 맛있어서 생각남...",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100034896.1.7",
          "form": "4인분짜리 통으로 기본떡볶이 6900 로제떡볶이 9900에 파는데",
          "original_form": "4인분짜리 통으로 기본떡볶이 6900 로제떡볶이 9900에 파는데",
          "speaker_id": "1",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100034896.1.8",
          "form": "떡 양 진짜 많음... 4인분 아니더라도 3인분은 되는 듯",
          "original_form": "떡 양 진짜 많음... 4인분 아니더라도 3인분은 되는 듯",
          "speaker_id": "1",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100034896.1.9",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100034896.1.10",
          "form": "헐 엄청 싸네!",
          "original_form": "헐 엄청 싸네!",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100034896.1.11",
          "form": "가성비 미쳤다",
          "original_form": "가성비 미쳤다",
          "speaker_id": "2",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100034896.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그치",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그치",
          "speaker_id": "1",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100034896.1.13",
          "form": "아 못 참아~",
          "original_form": "아 못 참아~",
          "speaker_id": "1",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100034896.1.14",
          "form": "뭐라도 먹고 싶은 거 있을 때 먹어!",
          "original_form": "뭐라도 먹고 싶은 거 있을 때 먹어!",
          "speaker_id": "2",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100034896.1.15",
          "form": "그래서 시켯다",
          "original_form": "그래서 시켯다",
          "speaker_id": "1",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100034896.1.16",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "1",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100034896.1.17",
          "form": "잘했어ㅋㅋㅋ",
          "original_form": "잘했어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100034896.1.18",
          "form": "아플 때는 좀 관대해져도 돼",
          "original_form": "아플 때는 좀 관대해져도 돼",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100034896.1.19",
          "form": "맞지...",
          "original_form": "맞지...",
          "speaker_id": "1",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100034896.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100034896.1.21",
          "form": "떡볶이만 시켰어?",
          "original_form": "떡볶이만 시켰어?",
          "speaker_id": "2",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100034896.1.22",
          "form": "아니 최소주문 안 차길래 덮밥도",
          "original_form": "아니 최소주문 안 차길래 덮밥도",
          "speaker_id": "1",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100034896.1.23",
          "form": "여기 떡볶이가 싸서 뭘 시켜야 돼",
          "original_form": "여기 떡볶이가 싸서 뭘 시켜야 돼",
          "speaker_id": "1",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100034896.1.24",
          "form": "그럴 거 같았어",
          "original_form": "그럴 거 같았어",
          "speaker_id": "2",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100034896.1.25",
          "form": "덮밥도 맛있었으면 좋겠다",
          "original_form": "덮밥도 맛있었으면 좋겠다",
          "speaker_id": "2",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100034896.1.26",
          "form": "ㅋㅋㅋㅋㅋ근데 나머지는 좀 비싸ㅜ",
          "original_form": "ㅋㅋㅋㅋㅋ근데 나머지는 좀 비싸ㅜ",
          "speaker_id": "1",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100034896.1.27",
          "form": "저번엔 하와이안김밥 시켰는데 구렷으",
          "original_form": "저번엔 하와이안김밥 시켰는데 구렷으",
          "speaker_id": "1",
          "time": "20210828 22:24"
        }
      ]
    }
