      "utterance": [
        {
          "id": "MDRW2100039858.1.1",
          "form": "저는 사실...혼자 있을 때 탕수육 자주 시킵니다",
          "original_form": "저는 사실...혼자 있을 때 탕수육 자주 시킵니다",
          "speaker_id": "1",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039858.1.2",
          "form": "소자 시키면 짬뽕 2개보다 만족도 높고 양도 적당하쥬",
          "original_form": "소자 시키면 짬뽕 2개보다 만족도 높고 양도 적당하쥬",
          "speaker_id": "1",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039858.1.3",
          "form": "에엥",
          "original_form": "에엥",
          "speaker_id": "2",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039858.1.4",
          "form": "혼자 있을 땐",
          "original_form": "혼자 있을 땐",
          "speaker_id": "2",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039858.1.5",
          "form": "짜장면+탕수육 나홀로세트가 진리 아닌가여?!",
          "original_form": "짜장면+탕수육 나홀로세트가 진리 아닌가여?!",
          "speaker_id": "2",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039858.1.6",
          "form": "그건 탕수육이 넘 적어요",
          "original_form": "그건 탕수육이 넘 적어요",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039858.1.7",
          "form": "선택과 집중을 해야쥬(단호)",
          "original_form": "선택과 집중을 해야쥬(단호)",
          "speaker_id": "1",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039858.1.8",
          "form": "전 조금씩 다양하게 먹고 싶다그영",
          "original_form": "전 조금씩 다양하게 먹고 싶다그영",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039858.1.9",
          "form": "ㅉㅉㅉ 위도 작으면서 욕심이 많군욧!",
          "original_form": "ㅉㅉㅉ 위도 작으면서 욕심이 많군욧!",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039858.1.10",
          "form": "후후 진정한 미식가의 길이져",
          "original_form": "후후 진정한 미식가의 길이져",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100039858.1.11",
          "form": "짜탕을 말하면서 갑자기요?ㅋㅋㅋ",
          "original_form": "짜탕을 말하면서 갑자기요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100039858.1.12",
          "form": "짬뽕보다 짜장파인가요",
          "original_form": "짬뽕보다 짜장파인가요",
          "speaker_id": "1",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100039858.1.13",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039858.1.14",
          "form": "진정한 중식의 왕은",
          "original_form": "진정한 중식의 왕은",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039858.1.15",
          "form": "단연 짜장면이져",
          "original_form": "단연 짜장면이져",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039858.1.16",
          "form": "무척 확고하시네요",
          "original_form": "무척 확고하시네요",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039858.1.17",
          "form": "부먹인가요 찍먹인가요",
          "original_form": "부먹인가요 찍먹인가요",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039858.1.18",
          "form": "저는 기본 찍먹에",
          "original_form": "저는 기본 찍먹에",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039858.1.19",
          "form": "서너조각 담먹을 차용하고 있습니다",
          "original_form": "서너조각 담먹을 차용하고 있습니다",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039858.1.20",
          "form": "설마 부먹이신가여?",
          "original_form": "설마 부먹이신가여?",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039858.1.21",
          "form": "다같이 먹으면 부먹",
          "original_form": "다같이 먹으면 부먹",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039858.1.22",
          "form": "혼자 먹으면 담먹입니다",
          "original_form": "혼자 먹으면 담먹입니다",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039858.1.23",
          "form": "님과 저는 함께할 수 없네요ㅎㅎ",
          "original_form": "님과 저는 함께할 수 없네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100039858.1.24",
          "form": "에이",
          "original_form": "에이",
          "speaker_id": "2",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100039858.1.25",
          "form": "다같이먹으면 부먹 혼자먹으면 담먹 띠로링과먹으면 찍먹하시면 되져!",
          "original_form": "다같이먹으면 부먹 혼자먹으면 담먹 띠로링과먹으면 찍먹하시면 되져!",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039858.1.26",
          "form": "노노 name2님과 먹으면 부먹할 거예요",
          "original_form": "노노 &name2&님과 먹으면 부먹할 거예요",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039858.1.27",
          "form": "그래야 저 혼자 먹쥬ㅎㅎ",
          "original_form": "그래야 저 혼자 먹쥬ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039858.1.28",
          "form": "완숙인가요 반숙인가요",
          "original_form": "완숙인가요 반숙인가요",
