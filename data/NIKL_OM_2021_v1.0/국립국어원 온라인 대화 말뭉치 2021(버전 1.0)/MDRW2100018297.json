      "utterance": [
        {
          "id": "MDRW2100018297.1.1",
          "form": "자취하시나용",
          "original_form": "자취하시나용",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018297.1.2",
          "form": "아뇨 가족들과ㅜㅎㅎ",
          "original_form": "아뇨 가족들과ㅜㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018297.1.3",
          "form": "자취하세요?",
          "original_form": "자취하세요?",
          "speaker_id": "2",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018297.1.4",
          "form": "넹넹 ㅋㅋ 집안일 좀 도우시나요",
          "original_form": "넹넹 ㅋㅋ 집안일 좀 도우시나요",
          "speaker_id": "1",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018297.1.5",
          "form": "아 그럼요ㅋㅋ 주말엔 거의 제가.. 재택할때도 제가...",
          "original_form": "아 그럼요ㅋㅋ 주말엔 거의 제가.. 재택할때도 제가...",
          "speaker_id": "2",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018297.1.6",
          "form": "혼자사시면 완전 살림만랩이시겠어요",
          "original_form": "혼자사시면 완전 살림만랩이시겠어요",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018297.1.7",
          "form": "허걱 ... 아니요 전 게을러가지고",
          "original_form": "허걱 ... 아니요 전 게을러가지고",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018297.1.8",
          "form": "ㅜㅜ 요리도 가의걍 반도리 식품 해먹고 설거지 청소 다미루고",
          "original_form": "ㅜㅜ 요리도 가의걍 반도리 식품 해먹고 설거지 청소 다미루고",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018297.1.9",
          "form": "개판이요",
          "original_form": "개판이요",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018297.1.10",
          "form": "집안일 꿀템있나요",
          "original_form": "집안일 꿀템있나요",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018297.1.11",
          "form": "근데 저도 삼개월정도 혼자 살아봤는데 매끼 챙겨먹는것도 일이에요",
          "original_form": "근데 저도 삼개월정도 혼자 살아봤는데 매끼 챙겨먹는것도 일이에요",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018297.1.12",
          "form": "아 전 베이킹소다 이야기했거든요",
          "original_form": "아 전 베이킹소다 이야기했거든요",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018297.1.13",
          "form": "주부만랩친구들이 다른거 다 필요없고 락스가 최고라고",
          "original_form": "주부만랩친구들이 다른거 다 필요없고 락스가 최고라고",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018297.1.14",
          "form": "엌ㅋㅋㅋ 락슼ㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋ 락슼ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018297.1.15",
          "form": "화장실 청소할때만 쓰는뎅",
          "original_form": "화장실 청소할때만 쓰는뎅",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018297.1.16",
          "form": "자취생으로서 꿀템 있나요??",
          "original_form": "자취생으로서 꿀템 있나요??",
          "speaker_id": "2",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100018297.1.17",
          "form": "흠...물티슈? ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "흠...물티슈? ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100018297.1.18",
          "form": "청소 뭐로하세영?",
          "original_form": "청소 뭐로하세영?",
          "speaker_id": "1",
          "time": "20210826 23:56"
        },
        {
          "id": "MDRW2100018297.1.19",
          "form": "악ㅋㅋ 빵터졌어요 맞아요! 요즘은 다 물티슈쓰더라고요",
          "original_form": "악ㅋㅋ 빵터졌어요 맞아요! 요즘은 다 물티슈쓰더라고요",
          "speaker_id": "2",
          "time": "20210826 23:56"
        },
        {
          "id": "MDRW2100018297.1.20",
          "form": "전 길에서 행주로 나눠주는 그걸로 하곤해요",
          "original_form": "전 길에서 행주로 나눠주는 그걸로 하곤해요",
          "speaker_id": "2",
          "time": "20210826 23:56"
        },
        {
          "id": "MDRW2100018297.1.21",
          "form": "오잉 그게모지 ㅋㅋㅋㅋ",
          "original_form": "오잉 그게모지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018297.1.22",
          "form": "안받아봐서몰겟어요 ㅋㅋㅋㅋ",
          "original_form": "안받아봐서몰겟어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018297.1.23",
          "form": "자취하는 분들 너무 대단해요 혼자 살면 힘들거같아요",
          "original_form": "자취하는 분들 너무 대단해요 혼자 살면 힘들거같아요",
          "speaker_id": "2",
          "time": "20210827 08:12"
        },
        {
          "id": "MDRW2100018297.1.24",
          "form": "ㅋㅋㅋ 그죠 ㅠ엄마품에서 살고싶네요",
          "original_form": "ㅋㅋㅋ 그죠 ㅠ엄마품에서 살고싶네요",
          "speaker_id": "1",
          "time": "20210827 15:41"
        }
      ]
    }
  ]
