      "utterance": [
        {
          "id": "MDRW2100009280.1.1",
          "form": "설거지 무슨 요일에 하라고 했지?",
          "original_form": "설거지 무슨 요일에 하라고 했지?",
          "speaker_id": "1",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009280.1.2",
          "form": "주말빼고 다 하지",
          "original_form": "주말빼고 다 하지",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009280.1.3",
          "form": "나는 언제 해?",
          "original_form": "나는 언제 해?",
          "speaker_id": "1",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100009280.1.4",
          "form": "너는 언제하고?",
          "original_form": "너는 언제하고?",
          "speaker_id": "1",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100009280.1.5",
          "form": "설거지 월수 하면 되 난 화목 할게",
          "original_form": "설거지 월수 하면 되 난 화목 할게",
          "speaker_id": "2",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100009280.1.6",
          "form": "금요일은?",
          "original_form": "금요일은?",
          "speaker_id": "1",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100009280.1.7",
          "form": "금요일 아무나 하면 되",
          "original_form": "금요일 아무나 하면 되",
          "speaker_id": "2",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009280.1.8",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009280.1.9",
          "form": "다음주 계속 일찍 퇴근하지?",
          "original_form": "다음주 계속 일찍 퇴근하지?",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009280.1.10",
          "form": "응 일찍 퇴근 한다 빨래는 언제 해?",
          "original_form": "응 일찍 퇴근 한다 빨래는 언제 해?",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009280.1.11",
          "form": "그럼 저녁식사 당번 부탁해",
          "original_form": "그럼 저녁식사 당번 부탁해",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009280.1.12",
          "form": "빨래는 내일 할꺼야",
          "original_form": "빨래는 내일 할꺼야",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009280.1.13",
          "form": "검은색 옷",
          "original_form": "검은색 옷",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009280.1.14",
          "form": "너 검은색 빨래 있어?",
          "original_form": "너 검은색 빨래 있어?",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009280.1.15",
          "form": "낼 언제 할거야?",
          "original_form": "낼 언제 할거야?",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009280.1.16",
          "form": "너오면",
          "original_form": "너오면",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009280.1.17",
          "form": "검은색 빨래 없어",
          "original_form": "검은색 빨래 없어",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009280.1.18",
          "form": "내꺼만 해야겠다",
          "original_form": "내꺼만 해야겠다",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009280.1.19",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009280.1.20",
          "form": "낼 점심 뭐 먹을래?",
          "original_form": "낼 점심 뭐 먹을래?",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009280.1.21",
          "form": "치킨 남은거 먹을거야",
          "original_form": "치킨 남은거 먹을거야",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009280.1.22",
          "form": "아 맞다 치킨",
          "original_form": "아 맞다 치킨",
          "speaker_id": "1",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009280.1.23",
          "form": "저녁은?",
          "original_form": "저녁은?",
          "speaker_id": "1",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009280.1.24",
          "form": "생선조림 하라고 해서 그런거야",
          "original_form": "생선조림 하라고 해서 그런거야",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009280.1.25",
          "form": "기대할게",
          "original_form": "기대할게",
          "speaker_id": "1",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009280.1.26",
          "form": "잘꺼야?",
          "original_form": "잘꺼야?",
          "speaker_id": "1",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009280.1.27",
          "form": "지금 자야지 잘거야?",
          "original_form": "지금 자야지 잘거야?",
          "speaker_id": "2",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009280.1.28",
          "form": "좀 있다가",
          "original_form": "좀 있다가",
          "speaker_id": "1",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009280.1.29",
          "form": "잘자",
          "original_form": "잘자",
          "speaker_id": "1",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009280.1.30",
          "form": "응 잘자",
          "original_form": "응 잘자",
          "speaker_id": "2",
          "time": "20210822 22:43"
        }
      ]
    }
  ]
