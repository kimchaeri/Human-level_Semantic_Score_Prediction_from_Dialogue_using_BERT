      "utterance": [
        {
          "id": "MDRW2100019379.1.1",
          "form": "오늘 하루종일 흐리네요",
          "original_form": "오늘 하루종일 흐리네요",
          "speaker_id": "1",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100019379.1.2",
          "form": "네 여긴 비오네요",
          "original_form": "네 여긴 비오네요",
          "speaker_id": "2",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100019379.1.3",
          "form": "거기도 비오나요?",
          "original_form": "거기도 비오나요?",
          "speaker_id": "2",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100019379.1.4",
          "form": "물방울이 조금씩 떨어지긴 해요",
          "original_form": "물방울이 조금씩 떨어지긴 해요",
          "speaker_id": "1",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100019379.1.5",
          "form": "기온은 내려갔는데 비때문에 습해서 애매하게 더워요",
          "original_form": "기온은 내려갔는데 비때문에 습해서 애매하게 더워요",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019379.1.6",
          "form": "여기도 그래서 우산 들고다니고 있어요",
          "original_form": "여기도 그래서 우산 들고다니고 있어요",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019379.1.7",
          "form": "추워서 에어컨 틀기도 애매한거같아요",
          "original_form": "추워서 에어컨 틀기도 애매한거같아요",
          "speaker_id": "2",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100019379.1.8",
          "form": "그러게 말이에요",
          "original_form": "그러게 말이에요",
          "speaker_id": "1",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100019379.1.9",
          "form": "요즘같은 날씨엔 매일 우산 들고 다니는 게 좋은 것 같아요",
          "original_form": "요즘같은 날씨엔 매일 우산 들고 다니는 게 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100019379.1.10",
          "form": "맞아요 우산 필수인 날씨예요",
          "original_form": "맞아요 우산 필수인 날씨예요",
          "speaker_id": "2",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100019379.1.11",
          "form": "옷도 다시 긴팔 꺼내고 있어요",
          "original_form": "옷도 다시 긴팔 꺼내고 있어요",
          "speaker_id": "2",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100019379.1.12",
          "form": "맞아요 저도 오늘 긴팔입고 나왔어요",
          "original_form": "맞아요 저도 오늘 긴팔입고 나왔어요",
          "speaker_id": "1",
          "time": "20210827 18:39"
        },
        {
          "id": "MDRW2100019379.1.13",
          "form": "계절도 바꼈으니 옷도 사고 싶은데 무슨 옷을 사야할 지 모르겠어요 ㅋㅋ",
          "original_form": "계절도 바꼈으니 옷도 사고 싶은데 무슨 옷을 사야할 지 모르겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:39"
        },
        {
          "id": "MDRW2100019379.1.14",
          "form": "아직 니트는 좀 덥겠죠?",
          "original_form": "아직 니트는 좀 덥겠죠?",
          "speaker_id": "1",
          "time": "20210827 18:39"
        },
        {
          "id": "MDRW2100019379.1.15",
          "form": "헉 네 ㅋㅋㅋ 니트는 아니구 얇은 셔츠같은거 많이 입는거같아요",
          "original_form": "헉 네 ㅋㅋㅋ 니트는 아니구 얇은 셔츠같은거 많이 입는거같아요",
          "speaker_id": "2",
          "time": "20210827 18:40"
        },
        {
          "id": "MDRW2100019379.1.16",
          "form": "아하 ㅋㅋㄱ 그렇죠",
          "original_form": "아하 ㅋㅋㄱ 그렇죠",
          "speaker_id": "1",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100019379.1.17",
          "form": "바지도 다시 긴바지 입는 분위기더라구요",
          "original_form": "바지도 다시 긴바지 입는 분위기더라구요",
          "speaker_id": "2",
          "time": "20210827 18:43"
        },
        {
          "id": "MDRW2100019379.1.18",
          "form": "좋아하는 계절 있으세요?",
          "original_form": "좋아하는 계절 있으세요?",
          "speaker_id": "2",
          "time": "20210827 18:44"
        },
        {
          "id": "MDRW2100019379.1.19",
          "form": "맞아요 반바지 입기엔 너무 추워졌어요",
          "original_form": "맞아요 반바지 입기엔 너무 추워졌어요",
          "speaker_id": "1",
          "time": "20210827 18:44"
        },
        {
          "id": "MDRW2100019379.1.20",
          "form": "저는 여름을 좋아했는데",
          "original_form": "저는 여름을 좋아했는데",
          "speaker_id": "1",
          "time": "20210827 18:44"
        },
        {
          "id": "MDRW2100019379.1.21",
          "form": "이번 여름 더위를 너무 호되게 당했더니 다른 계절로 바뀔 것 같아요 ㅋㅋ",
          "original_form": "이번 여름 더위를 너무 호되게 당했더니 다른 계절로 바뀔 것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100019379.1.22",
          "form": "그쵸 ㅋㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100019379.1.23",
          "form": "저도 여름 맑은날씨가 좋았는대",
          "original_form": "저도 여름 맑은날씨가 좋았는대",
          "speaker_id": "2",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100019379.1.24",
          "form": "앞으로는 겨울이 좋아질거같아요",
          "original_form": "앞으로는 겨울이 좋아질거같아요",
          "speaker_id": "2",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100019379.1.25",
          "form": "맞아요 근데 또 겨울의 찬바람 맞으면 바뀔수도 ㅋㅋㅋ",
          "original_form": "맞아요 근데 또 겨울의 찬바람 맞으면 바뀔수도 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100019379.1.26",
          "form": "그쵸 ㅋㅋㅋ 사람 심리가...",
          "original_form": "그쵸 ㅋㅋㅋ 사람 심리가...",
          "speaker_id": "2",
          "time": "20210827 18:46"
        }
      ]
    }
