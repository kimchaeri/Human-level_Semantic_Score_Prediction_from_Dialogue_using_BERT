      "utterance": [
        {
          "id": "MDRW2100018930.1.1",
          "form": "name1님 동네도 비가 오나요??",
          "original_form": "&name1&님 동네도 비가 오나요??",
          "speaker_id": "2",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018930.1.2",
          "form": "저희는 지금 안 와요",
          "original_form": "저희는 지금 안 와요",
          "speaker_id": "1",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018930.1.3",
          "form": "그쪽은 비 오는 중인가요?",
          "original_form": "그쪽은 비 오는 중인가요?",
          "speaker_id": "1",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018930.1.4",
          "form": "넵 여기는 지금부터 7시까지 비온다고 하네요..",
          "original_form": "넵 여기는 지금부터 7시까지 비온다고 하네요..",
          "speaker_id": "2",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018930.1.5",
          "form": "저희도 비온다고 하긴 했었는데 아직 안 오네요",
          "original_form": "저희도 비온다고 하긴 했었는데 아직 안 오네요",
          "speaker_id": "1",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018930.1.6",
          "form": "어제도 오셨나요?",
          "original_form": "어제도 오셨나요?",
          "speaker_id": "1",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018930.1.7",
          "form": "어제는 비 안왔어요 ㅎㅎ",
          "original_form": "어제는 비 안왔어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018930.1.8",
          "form": "저희도요! 태풍 피해는 없으셨나요?",
          "original_form": "저희도요! 태풍 피해는 없으셨나요?",
          "speaker_id": "1",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018930.1.9",
          "form": "넵!여기는 바람이 그리 쎄게 불진 않았어요 ㅎㅎ",
          "original_form": "넵!여기는 바람이 그리 쎄게 불진 않았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100018930.1.10",
          "form": "거기도 괜찮죠??",
          "original_form": "거기도 괜찮죠??",
          "speaker_id": "2",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100018930.1.11",
          "form": "네 괜찮았어요",
          "original_form": "네 괜찮았어요",
          "speaker_id": "1",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100018930.1.12",
          "form": "날이 많이 쌀쌀해졌어요 이제 완전 가을이네요",
          "original_form": "날이 많이 쌀쌀해졌어요 이제 완전 가을이네요",
          "speaker_id": "1",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100018930.1.13",
          "form": "맞아요 가을옷 사셧나요??",
          "original_form": "맞아요 가을옷 사셧나요??",
          "speaker_id": "2",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100018930.1.14",
          "form": "봄에 샀던 거 대충 입으려고요...ㅎㅎ",
          "original_form": "봄에 샀던 거 대충 입으려고요...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:09"
        },
        {
          "id": "MDRW2100018930.1.15",
          "form": "코로나 때문에 자주 나갈 일도 없을 거 같아서",
          "original_form": "코로나 때문에 자주 나갈 일도 없을 거 같아서",
          "speaker_id": "1",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018930.1.16",
          "form": "ㅋㅋㅋㅋ맞네요..저도 그럴생각인데",
          "original_form": "ㅋㅋㅋㅋ맞네요..저도 그럴생각인데",
          "speaker_id": "2",
          "time": "20210827 14:13"
        },
        {
          "id": "MDRW2100018930.1.17",
          "form": "또 가을되면 옷을 살것같네요..정작 배송오면 겨울이 되겟죠..",
          "original_form": "또 가을되면 옷을 살것같네요..정작 배송오면 겨울이 되겟죠..",
          "speaker_id": "2",
          "time": "20210827 14:13"
        },
        {
          "id": "MDRW2100018930.1.18",
          "form": "가을이 워낙 짧아서...ㅠㅠ",
          "original_form": "가을이 워낙 짧아서...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018930.1.19",
          "form": "가을이 좀 길었으면 좋겧어요 ㅎㅎ",
          "original_form": "가을이 좀 길었으면 좋겧어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:16"
        },
        {
          "id": "MDRW2100018930.1.20",
          "form": "여름이랑 겨울은 좀 짧아지고",
          "original_form": "여름이랑 겨울은 좀 짧아지고",
          "speaker_id": "2",
          "time": "20210827 14:16"
        }
      ]
