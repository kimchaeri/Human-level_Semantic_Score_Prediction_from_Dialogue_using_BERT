      "utterance": [
        {
          "id": "MDRW2100015796.1.1",
          "form": "으 바로 눈에 띈 다이어트",
          "original_form": "으 바로 눈에 띈 다이어트",
          "speaker_id": "2",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015796.1.2",
          "form": "다이어트 하시나요?",
          "original_form": "다이어트 하시나요?",
          "speaker_id": "1",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015796.1.3",
          "form": "아니죠 매번 생각만 하죠ㅋㅋㅋㅋ",
          "original_form": "아니죠 매번 생각만 하죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015796.1.4",
          "form": "아픈적 있으세요?",
          "original_form": "아픈적 있으세요?",
          "speaker_id": "1",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015796.1.5",
          "form": "아니요ㅋㅋ 건강해요",
          "original_form": "아니요ㅋㅋ 건강해요",
          "speaker_id": "2",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015796.1.6",
          "form": "이번에 백신 맞으셨어요?",
          "original_form": "이번에 백신 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015796.1.7",
          "form": "아직이요!",
          "original_form": "아직이요!",
          "speaker_id": "1",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100015796.1.8",
          "form": "저두요ㅜㅜ",
          "original_form": "저두요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100015796.1.9",
          "form": "저는 내일부터 계단걷기 운동 시작하려구요ㅋㅋ",
          "original_form": "저는 내일부터 계단걷기 운동 시작하려구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100015796.1.10",
          "form": "화이팅 입니다",
          "original_form": "화이팅 입니다",
          "speaker_id": "1",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100015796.1.11",
          "form": "저는 요가해요",
          "original_form": "저는 요가해요",
          "speaker_id": "1",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100015796.1.12",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100015796.1.13",
          "form": "요가",
          "original_form": "요가",
          "speaker_id": "2",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100015796.1.14",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "2",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100015796.1.15",
          "form": "어디 다니면서 하시는거에요?",
          "original_form": "어디 다니면서 하시는거에요?",
          "speaker_id": "2",
          "time": "20210826 01:18"
        },
        {
          "id": "MDRW2100015796.1.16",
          "form": "집에서 해요",
          "original_form": "집에서 해요",
          "speaker_id": "1",
          "time": "20210826 01:18"
        },
        {
          "id": "MDRW2100015796.1.17",
          "form": "건강하고 싶어서 억지로합니다ㅎ",
          "original_form": "건강하고 싶어서 억지로합니다ㅎ",
          "speaker_id": "1",
          "time": "20210826 01:18"
        },
        {
          "id": "MDRW2100015796.1.18",
          "form": "저도 요가 영상 따라해본적 있는데",
          "original_form": "저도 요가 영상 따라해본적 있는데",
          "speaker_id": "2",
          "time": "20210826 01:18"
