        {
          "id": "MDRW2100034421.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100034421.1.2",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100034421.1.3",
          "form": "아침드셨나요??",
          "original_form": "아침드셨나요??",
          "speaker_id": "1",
          "time": "20210828 10:11"
        },
        {
          "id": "MDRW2100034421.1.4",
          "form": "아니요 name1님은요?",
          "original_form": "아니요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100034421.1.5",
          "form": "전 간단하게 베이글 먹었어요~ㅎㅎ",
          "original_form": "전 간단하게 베이글 먹었어요~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100034421.1.6",
          "form": "베이글 맛잇겟네요",
          "original_form": "베이글 맛잇겟네요",
          "speaker_id": "2",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100034421.1.7",
          "form": "뭐 발라드셧어요?",
          "original_form": "뭐 발라드셧어요?",
          "speaker_id": "2",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100034421.1.8",
          "form": "전 크림치즈랑 버터 반반씩 발라먹어요~",
          "original_form": "전 크림치즈랑 버터 반반씩 발라먹어요~",
          "speaker_id": "1",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100034421.1.9",
          "form": "맛잇겟다",
          "original_form": "맛잇겟다",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100034421.1.10",
          "form": "저도 급 배고파지네요",
          "original_form": "저도 급 배고파지네요",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100034421.1.11",
          "form": "점심은 뭐드실계획이세요?",
          "original_form": "점심은 뭐드실계획이세요?",
          "speaker_id": "2",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100034421.1.12",
          "form": "점심은 아직 정하진않았는데 피자 땡겨서 피자먹을것같아욮ㅋㅋㅋ",
          "original_form": "점심은 아직 정하진않았는데 피자 땡겨서 피자먹을것같아욮ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100034421.1.13",
          "form": "무슨 피자 좋아하세요?",
          "original_form": "무슨 피자 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100034421.1.14",
          "form": "전 파파존스 젤좋아해요~~",
          "original_form": "전 파파존스 젤좋아해요~~",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100034421.1.15",
          "form": "파파존스 맛잇죠 ㅋㅋㅋ",
          "original_form": "파파존스 맛잇죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100034421.1.16",
          "form": "파파존스에서 주로 뭐 시키세요?",
          "original_form": "파파존스에서 주로 뭐 시키세요?",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100034421.1.17",
          "form": "저는 존스페이버릿이나 스치랜 두개 좋아해요~",
          "original_form": "저는 존스페이버릿이나 스치랜 두개 좋아해요~",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100034421.1.18",
          "form": "스치랜은 안먹어봣어요",
          "original_form": "스치랜은 안먹어봣어요",
          "speaker_id": "2",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100034421.1.19",
          "form": "무슨 맛잇지 궁금해요",
          "original_form": "무슨 맛잇지 궁금해요",
          "speaker_id": "2",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100034421.1.20",
          "form": "치킨좋아하시면 추천이요! 토마토소스베이스가 아니긴한데 고소하고 맛있어요~",
          "original_form": "치킨좋아하시면 추천이요! 토마토소스베이스가 아니긴한데 고소하고 맛있어요~",
          "speaker_id": "1",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100034421.1.21",
          "form": "오 치킨좋아하는데 여태 안먹어봣네요",
          "original_form": "오 치킨좋아하는데 여태 안먹어봣네요",
          "speaker_id": "2",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100034421.1.22",
          "form": "한번 먹어봐야겟어요 추천감사해요",
          "original_form": "한번 먹어봐야겟어요 추천감사해요",
          "speaker_id": "2",
          "time": "20210828 10:38"
        }
      ]
