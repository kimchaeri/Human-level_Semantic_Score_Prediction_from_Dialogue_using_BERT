      "utterance": [
        {
          "id": "MDRW2100006449.1.1",
          "form": "미드나잇인파리요",
          "original_form": "미드나잇인파리요",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100006449.1.2",
          "form": "아 유명한 영화죠",
          "original_form": "아 유명한 영화죠",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006449.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100006449.1.4",
          "form": "기억에 남는영화 있으신가요?",
          "original_form": "기억에 남는영화 있으신가요?",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100006449.1.5",
          "form": "저는 인셉션 좋아합니다",
          "original_form": "저는 인셉션 좋아합니다",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100006449.1.6",
          "form": "추리극 좋아하시나요?",
          "original_form": "추리극 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100006449.1.7",
          "form": "놀란 감독을 좋아해요 ㅎㅎ",
          "original_form": "놀란 감독을 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100006449.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100006449.1.9",
          "form": "전 로맨스요 ㅎ",
          "original_form": "전 로맨스요 ㅎ",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100006449.1.10",
          "form": "노트북 아시나요?",
          "original_form": "노트북 아시나요?",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100006449.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100006449.1.12",
          "form": "근데 들어만 봤어요",
          "original_form": "근데 들어만 봤어요",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100006449.1.13",
          "form": "로맨스 명작이라 여쭤봤어요",
          "original_form": "로맨스 명작이라 여쭤봤어요",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100006449.1.14",
          "form": "그쵸ㅋㅋㅋㅋ 바이블이라고 들었는데 안봐지더라구요",
          "original_form": "그쵸ㅋㅋㅋㅋ 바이블이라고 들었는데 안봐지더라구요",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100006449.1.15",
          "form": "그렇군요 최근에 본 영화는요?",
          "original_form": "그렇군요 최근에 본 영화는요?",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100006449.1.16",
          "form": "마지막으로 본건 어벤져스네요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "마지막으로 본건 어벤져스네요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100006449.1.17",
          "form": "기다리는건있는데..",
          "original_form": "기다리는건있는데..",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100006449.1.18",
          "form": "어떤거요??",
          "original_form": "어떤거요??",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100006449.1.19",
          "form": "김남길 송가호 나오는 비상선언이요",
          "original_form": "김남길 송가호 나오는 비상선언이요",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100006449.1.20",
