      "utterance": [
        {
          "id": "MDRW2100018513.1.1",
          "form": "가족과 함께 사시나요?",
          "original_form": "가족과 함께 사시나요?",
          "speaker_id": "2",
          "time": "20210827 07:48"
        },
        {
          "id": "MDRW2100018513.1.2",
          "form": "음 원래 다같이 모여살다가",
          "original_form": "음 원래 다같이 모여살다가",
          "speaker_id": "1",
          "time": "20210827 07:48"
        },
        {
          "id": "MDRW2100018513.1.3",
          "form": "어머니랑 같이살아요",
          "original_form": "어머니랑 같이살아요",
          "speaker_id": "1",
          "time": "20210827 07:48"
        },
        {
          "id": "MDRW2100018513.1.4",
          "form": "name2은용?",
          "original_form": "&name2&은용?",
          "speaker_id": "1",
          "time": "20210827 07:48"
        },
        {
          "id": "MDRW2100018513.1.5",
          "form": "저는 부모님이랑요!",
          "original_form": "저는 부모님이랑요!",
          "speaker_id": "2",
          "time": "20210827 07:49"
        },
        {
          "id": "MDRW2100018513.1.6",
          "form": "같이살면 독립하고싶고 혼자면 그립고 그렇더라구요ㅋㅋ",
          "original_form": "같이살면 독립하고싶고 혼자면 그립고 그렇더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:49"
        },
        {
          "id": "MDRW2100018513.1.7",
          "form": "아 맞아요ㅋㅋㅋ",
          "original_form": "아 맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:50"
        },
        {
          "id": "MDRW2100018513.1.8",
          "form": "부모님이랑 같이사는게 심리적으로 안정은 되는것같아요",
          "original_form": "부모님이랑 같이사는게 심리적으로 안정은 되는것같아요",
          "speaker_id": "1",
          "time": "20210827 07:50"
        },
        {
          "id": "MDRW2100018513.1.9",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100018513.1.10",
          "form": "집이구나 하는 느낌이 다르더라구요",
          "original_form": "집이구나 하는 느낌이 다르더라구요",
          "speaker_id": "2",
          "time": "20210827 07:53"
        },
        {
          "id": "MDRW2100018513.1.11",
          "form": "그리고 제가 피곤해서 청소를 못하더라도 대신 해주시죠...ㅎㅎ",
          "original_form": "그리고 제가 피곤해서 청소를 못하더라도 대신 해주시죠...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 07:53"
        },
        {
          "id": "MDRW2100018513.1.12",
          "form": "아이고ㅋㅋ",
          "original_form": "아이고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:53"
        },
        {
          "id": "MDRW2100018513.1.13",
          "form": "ㅋㅋㅋ그쵸 이렇게 감사함을 느끼며",
          "original_form": "ㅋㅋㅋ그쵸 이렇게 감사함을 느끼며",
          "speaker_id": "2",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018513.1.14",
          "form": "죄송하고 감사하고ㅋㅋ",
          "original_form": "죄송하고 감사하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018513.1.15",
          "form": "오잉?",
          "original_form": "오잉?",
          "speaker_id": "1",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018513.1.16",
          "form": "독립심도 필요하겨",
          "original_form": "독립심도 필요하겨",
          "speaker_id": "1",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018513.1.17",
          "form": "책임감도 필요하고ㅠㅠ",
          "original_form": "책임감도 필요하고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018513.1.18",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018513.1.19",
          "form": "전구 혼자 가실 수 있으세요?",
          "original_form": "전구 혼자 가실 수 있으세요?",
          "speaker_id": "1",
          "time": "20210827 07:56"
        },
        {
          "id": "MDRW2100018513.1.20",
          "form": "아니요 name1은요?",
          "original_form": "아니요 &name1&은요?",
          "speaker_id": "2",
          "time": "20210827 07:57"
        },
        {
          "id": "MDRW2100018513.1.21",
          "form": "저는 매립형은 불가능하고 일반 전구는 갈 수 있어요!",
          "original_form": "저는 매립형은 불가능하고 일반 전구는 갈 수 있어요!",
          "speaker_id": "1",
          "time": "20210827 07:57"
        },
        {
          "id": "MDRW2100018513.1.22",
          "form": "오 멋지셔요!",
          "original_form": "오 멋지셔요!",
          "speaker_id": "2",
