      "utterance": [
        {
          "id": "MDRW2100027134.1.1",
          "form": "저는그래서 첨에말한 간단한거로는",
          "original_form": "저는그래서 첨에말한 간단한거로는",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027134.1.2",
          "form": "베라각 스벅각 출근길하고요",
          "original_form": "베라각 스벅각 출근길하고요",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027134.1.3",
          "form": "편의점각은 좀 시간걸려서 될때만해요",
          "original_form": "편의점각은 좀 시간걸려서 될때만해요",
          "speaker_id": "1",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100027134.1.4",
          "form": "요즘 심심이한다고 많이 못했어요ㅜ",
          "original_form": "요즘 심심이한다고 많이 못했어요ㅜ",
          "speaker_id": "1",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100027134.1.5",
          "form": "편의점각은 주말이나 한가할 때 가능하겠네요",
          "original_form": "편의점각은 주말이나 한가할 때 가능하겠네요",
          "speaker_id": "2",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100027134.1.6",
          "form": "베라각이랑 스벅각 얘들은 매일 하시는 편이예요??",
          "original_form": "베라각이랑 스벅각 얘들은 매일 하시는 편이예요??",
          "speaker_id": "2",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100027134.1.7",
          "form": "네네 매일 꾸준히해요 10분이면 다하니까요",
          "original_form": "네네 매일 꾸준히해요 10분이면 다하니까요",
          "speaker_id": "1",
          "time": "20210902 09:54"
        },
        {
          "id": "MDRW2100027134.1.8",
          "form": "지금 한 70일 됐어요 두개다 시작한지",
          "original_form": "지금 한 70일 됐어요 두개다 시작한지",
          "speaker_id": "1",
          "time": "20210902 09:54"
        },
        {
          "id": "MDRW2100027134.1.9",
          "form": "와 꾸준히 하셨네요 ㅇ0ㅇ 자투리시간 때우기 딱 좋은 거 같아요",
          "original_form": "와 꾸준히 하셨네요 ㅇ0ㅇ 자투리시간 때우기 딱 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100027134.1.10",
          "form": "미니게임이라 부담감이 없어서 좋네요 ㅋㅋㅋ",
          "original_form": "미니게임이라 부담감이 없어서 좋네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100027134.1.11",
          "form": "스벅각은 2개 스벅아아타먹었고 곧3개째 먹을거구요",
          "original_form": "스벅각은 2개 스벅아아타먹었고 곧3개째 먹을거구요",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100027134.1.12",
          "form": "베라각은 17000정도 모았어요",
          "original_form": "베라각은 17000정도 모았어요",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100027134.1.13",
          "form": "저 진짜 알바아니고",
          "original_form": "저 진짜 알바아니고",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100027134.1.14",
          "form": "제가해보니 좋아서",
          "original_form": "제가해보니 좋아서",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100027134.1.15",
          "form": "막하라고 추천하고당겨요 ㅋㅋ",
          "original_form": "막하라고 추천하고당겨요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100027134.1.16",
          "form": "와 진짜 쏠쏠하네요",
          "original_form": "와 진짜 쏠쏠하네요",
          "speaker_id": "2",
          "time": "20210902 10:07"
        },
        {
          "id": "MDRW2100027134.1.17",
          "form": "저 name1 덕분에 너무 좋은 거 알았어요 진짜 ㅋㅋㅋㅋ",
          "original_form": "저 &name1& 덕분에 너무 좋은 거 알았어요 진짜 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:07"
        },
        {
          "id": "MDRW2100027134.1.18",
          "form": "그거말고 나는게임을하고싶다 싶으시면",
          "original_form": "그거말고 나는게임을하고싶다 싶으시면",
          "speaker_id": "1",
          "time": "20210902 10:08"
        },
        {
          "id": "MDRW2100027134.1.19",
          "form": "치킨각이랑 캐시퍼요",
          "original_form": "치킨각이랑 캐시퍼요",
          "speaker_id": "1",
          "time": "20210902 10:08"
        },
        {
          "id": "MDRW2100027134.1.20",
          "form": "아니에요 저는 쫌쫌따리 돈을 벌고 싶은데",
          "original_form": "아니에요 저는 쫌쫌따리 돈을 벌고 싶은데",
          "speaker_id": "2",
          "time": "20210902 10:08"
        },
        {
          "id": "MDRW2100027134.1.21",
          "form": "다른 게임도 있군요 와",
          "original_form": "다른 게임도 있군요 와",
          "speaker_id": "2",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027134.1.22",
          "form": "전 좀 이런단순한 게임이취향인가봐요 ㅋㅋ",
          "original_form": "전 좀 이런단순한 게임이취향인가봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027134.1.23",
          "form": "이것도 치킨각은 알팔고 닭팔아서 돈벌고",
          "original_form": "이것도 치킨각은 알팔고 닭팔아서 돈벌고",
          "speaker_id": "1",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027134.1.24",
          "form": "캐시퍼는 땅팔아서돈벌고",
          "original_form": "캐시퍼는 땅팔아서돈벌고",
          "speaker_id": "1",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027134.1.25",
          "form": "저도 단순한 게 좋아요 미니미니한거요 ㅋㅋㅋ",
          "original_form": "저도 단순한 게 좋아요 미니미니한거요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027134.1.26",
          "form": "와 ㅋㅋㅋㅋㅋ",
          "original_form": "와 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027134.1.27",
          "form": "치킨각 귀여운데요 ㅋㅋㅋㅋㅋ",
          "original_form": "치킨각 귀여운데요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027134.1.28",
          "form": "치킨 기프티콘 주나여??",
          "original_form": "치킨 기프티콘 주나여??",
          "speaker_id": "2",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027134.1.29",
          "form": "네네 치킨이랑",
          "original_form": "네네 치킨이랑",
          "speaker_id": "1",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100027134.1.30",
          "form": "저는 맘스터치싸이버거 먹었어요",
          "original_form": "저는 맘스터치싸이버거 먹었어요",
          "speaker_id": "1",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100027134.1.31",
          "form": "치킨보다 버거 좋아해서",
          "original_form": "치킨보다 버거 좋아해서",
          "speaker_id": "1",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100027134.1.32",
          "form": "치킨각은 그거랑 비슷해요 그 농장겜",
          "original_form": "치킨각은 그거랑 비슷해요 그 농장겜",
          "speaker_id": "1",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100027134.1.33",
          "form": "헐 너무 좋은 게임이에요 버거도 선택이 된다니요 ㅋㅋㅋㅋ",
          "original_form": "헐 너무 좋은 게임이에요 버거도 선택이 된다니요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100027134.1.34",
          "form": "농장겜이면...",
          "original_form": "농장겜이면...",
          "speaker_id": "2",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100027134.1.35",
