      "utterance": [
        {
          "id": "MDRW2100018603.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018603.1.2",
          "form": "대학생이신가요?",
          "original_form": "대학생이신가요?",
          "speaker_id": "1",
          "time": "20210827 08:59"
        },
        {
          "id": "MDRW2100018603.1.3",
          "form": "아뇨 직장인입니다",
          "original_form": "아뇨 직장인입니다",
          "speaker_id": "2",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018603.1.4",
          "form": "저는 아직 대학생이에요...",
          "original_form": "저는 아직 대학생이에요...",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018603.1.5",
          "form": "4학년 휴학 중인데 아직 따놓은 자격증이 없어요...",
          "original_form": "4학년 휴학 중인데 아직 따놓은 자격증이 없어요...",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018603.1.6",
          "form": "컴활 2급도 괜찮나요?",
          "original_form": "컴활 2급도 괜찮나요?",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018603.1.7",
          "form": "1급은 너무 어려울 것 같아서요.",
          "original_form": "1급은 너무 어려울 것 같아서요.",
          "speaker_id": "1",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018603.1.8",
          "form": "헛 저도 컴활 2급 생각만 하고 따진 않았는데..기왕 딸거면 1급이 낫다고 들은것 같아요 ㅠㅠ",
          "original_form": "헛 저도 컴활 2급 생각만 하고 따진 않았는데..기왕 딸거면 1급이 낫다고 들은것 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100018603.1.9",
          "form": "아아 ㅠㅜ 역시 그런가 보네요...",
          "original_form": "아아 ㅠㅜ 역시 그런가 보네요...",
          "speaker_id": "1",
          "time": "20210827 09:05"
        },
        {
          "id": "MDRW2100018603.1.10",
          "form": "오늘 금요일 퇴근하시면 드디어 주말이네요!",
          "original_form": "오늘 금요일 퇴근하시면 드디어 주말이네요!",
          "speaker_id": "1",
          "time": "20210827 09:05"
        },
        {
          "id": "MDRW2100018603.1.11",
          "form": "네네..드디어요 ㅠㅠ 그러면 취업준비 하시는거예요?",
          "original_form": "네네..드디어요 ㅠㅠ 그러면 취업준비 하시는거예요?",
          "speaker_id": "2",
          "time": "20210827 09:11"
        },
        {
          "id": "MDRW2100018603.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210827 09:13"
        },
        {
          "id": "MDRW2100018603.1.13",
          "form": "사실 동시에 다른 대학으로 다시 입학할까 고민하고 있어요...",
          "original_form": "사실 동시에 다른 대학으로 다시 입학할까 고민하고 있어요...",
          "speaker_id": "1",
          "time": "20210827 09:13"
        },
        {
          "id": "MDRW2100018603.1.14",
          "form": "인문대학이라 정말 쉽지 않더라고요...",
          "original_form": "인문대학이라 정말 쉽지 않더라고요...",
          "speaker_id": "1",
          "time": "20210827 09:13"
        },
        {
          "id": "MDRW2100018603.1.15",
          "form": "아 고민이 많으시겠어요.. 진짜 진로에 대한 고민은 끝이없는것 같아요",
          "original_form": "아 고민이 많으시겠어요.. 진짜 진로에 대한 고민은 끝이없는것 같아요",
          "speaker_id": "2",
          "time": "20210827 09:22"
        },
        {
          "id": "MDRW2100018603.1.16",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "1",
          "time": "20210827 11:16"
        },
        {
          "id": "MDRW2100018603.1.17",
          "form": "이번에 지역아동센터 아르바이트 접수했는데",
          "original_form": "이번에 지역아동센터 아르바이트 접수했는데",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018603.1.18",
          "form": "꼭 되고 싶어요! ㅠ",
          "original_form": "꼭 되고 싶어요! ㅠ",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018603.1.19",
          "form": "오 되실거예요! 면접 보고 결과 나오는건가요?",
          "original_form": "오 되실거예요! 면접 보고 결과 나오는건가요?",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018603.1.20",
          "form": "예비 채용이라 기다리고 있어요 ㅜㅡㅠ",
          "original_form": "예비 채용이라 기다리고 있어요 ㅜㅡㅠ",
          "speaker_id": "1",
          "time": "20210827 12:46"
        },
        {
          "id": "MDRW2100018603.1.21",
          "form": "이때 좀 많이 떨리는 것 같아요",
          "original_form": "이때 좀 많이 떨리는 것 같아요",
          "speaker_id": "1",
          "time": "20210827 12:46"
        },
        {
          "id": "MDRW2100018603.1.22",
          "form": "잘되셨으면 좋겠네요 ㅠㅠ",
          "original_form": "잘되셨으면 좋겠네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018603.1.23",
          "form": "감사합니다 ㅜㅜ",
          "original_form": "감사합니다 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 15:50"
        }
      ]
    }
