      "utterance": [
        {
          "id": "MDRW2100004061.1.1",
          "form": "유명한 배우들 별로 없어서 그런거 아니야?",
          "original_form": "유명한 배우들 별로 없어서 그런거 아니야?",
          "speaker_id": "2",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004061.1.2",
          "form": "그것 보단 뭐랄까 내용 진행이 지루하네 ㅎㅎ",
          "original_form": "그것 보단 뭐랄까 내용 진행이 지루하네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004061.1.3",
          "form": "그래도 해리포터 후속작인데",
          "original_form": "그래도 해리포터 후속작인데",
          "speaker_id": "2",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004061.1.4",
          "form": "해리포터 기대하고 봤더니 완전 망이야",
          "original_form": "해리포터 기대하고 봤더니 완전 망이야",
          "speaker_id": "1",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004061.1.5",
          "form": "이런이런 ㅎㅎ",
          "original_form": "이런이런 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004061.1.6",
          "form": "졸음이 솔솔",
          "original_form": "졸음이 솔솔",
          "speaker_id": "1",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004061.1.7",
          "form": "끝까지 다 본거야?",
          "original_form": "끝까지 다 본거야?",
          "speaker_id": "2",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004061.1.8",
          "form": "이제 절반 정도 봤는데 너무 졸려",
          "original_form": "이제 절반 정도 봤는데 너무 졸려",
          "speaker_id": "1",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004061.1.9",
          "form": "ㅎㅎ 재미 없으면 그만 봐",
          "original_form": "ㅎㅎ 재미 없으면 그만 봐",
          "speaker_id": "2",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004061.1.10",
          "form": "애들이 꾸역꾸역 보고 있어서 어쩔 수 없이 보고 있심",
          "original_form": "애들이 꾸역꾸역 보고 있어서 어쩔 수 없이 보고 있심",
          "speaker_id": "1",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004061.1.11",
          "form": "힘들겠네",
          "original_form": "힘들겠네",
          "speaker_id": "2",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004061.1.12",
          "form": "마법도 뭔가 스케일이 ㅎㅎ",
          "original_form": "마법도 뭔가 스케일이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004061.1.13",
          "form": "해리포터가 눈을 너무 높여놨네",
          "original_form": "해리포터가 눈을 너무 높여놨네",
