      "utterance": [
        {
          "id": "MDRW2100045656.1.1",
          "form": "밥인데 볶은 것도 아니고 국물도 아니고",
          "original_form": "밥인데 볶은 것도 아니고 국물도 아니고",
          "speaker_id": "2",
          "time": "20210908 04:50"
        },
        {
          "id": "MDRW2100045656.1.2",
          "form": "그럼 혹시 구웠나요?",
          "original_form": "그럼 혹시 구웠나요?",
          "speaker_id": "2",
          "time": "20210908 04:50"
        },
        {
          "id": "MDRW2100045656.1.3",
          "form": "아니요 안구웟어요",
          "original_form": "아니요 안구웟어요",
          "speaker_id": "1",
          "time": "20210908 04:51"
        },
        {
          "id": "MDRW2100045656.1.4",
          "form": "이럴수가 그냥 공기밥만 드시고 싶으신 거 아니예요?",
          "original_form": "이럴수가 그냥 공기밥만 드시고 싶으신 거 아니예요?",
          "speaker_id": "2",
          "time": "20210908 04:51"
        },
        {
          "id": "MDRW2100045656.1.5",
          "form": "ㅋㅋㅋㅋㅋ아니에여",
          "original_form": "ㅋㅋㅋㅋㅋ아니에여",
          "speaker_id": "1",
          "time": "20210908 04:51"
        },
        {
          "id": "MDRW2100045656.1.6",
          "form": "ㅌㅋㅋㅋㅋㅋ뭘까요 그러믄 으으윽",
          "original_form": "ㅌㅋㅋㅋㅋㅋ뭘까요 그러믄 으으윽",
          "speaker_id": "2",
          "time": "20210908 04:52"
        },
        {
          "id": "MDRW2100045656.1.7",
          "form": "밥이랑 어떤 뭔가!!",
          "original_form": "밥이랑 어떤 뭔가!!",
          "speaker_id": "1",
          "time": "20210908 04:52"
        },
        {
          "id": "MDRW2100045656.1.8",
          "form": "그럼 뭉쳤나요?",
          "original_form": "그럼 뭉쳤나요?",
          "speaker_id": "2",
          "time": "20210908 04:52"
        },
        {
          "id": "MDRW2100045656.1.9",
          "form": "네 ㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:52"
        },
        {
          "id": "MDRW2100045656.1.10",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210908 04:52"
        },
        {
          "id": "MDRW2100045656.1.11",
          "form": "짭짤한가요?",
          "original_form": "짭짤한가요?",
          "speaker_id": "2",
          "time": "20210908 04:52"
        },
        {
          "id": "MDRW2100045656.1.12",
          "form": "아니요 짜지않아요",
          "original_form": "아니요 짜지않아요",
          "speaker_id": "1",
          "time": "20210908 04:53"
        },
        {
          "id": "MDRW2100045656.1.13",
          "form": "김이 들어가나요?",
          "original_form": "김이 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 04:54"
        },
        {
          "id": "MDRW2100045656.1.14",
          "form": "김 안들어가요",
          "original_form": "김 안들어가요",
          "speaker_id": "1",
          "time": "20210908 04:54"
        },
        {
          "id": "MDRW2100045656.1.15",
          "form": "이럴수가",
          "original_form": "이럴수가",
          "speaker_id": "2",
          "time": "20210908 04:54"
        },
        {
          "id": "MDRW2100045656.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ집에서 만들어먹는 음식이 아니에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ집에서 만들어먹는 음식이 아니에요",
          "speaker_id": "1",
          "time": "20210908 04:54"
        },
        {
          "id": "MDRW2100045656.1.17",
          "form": "보통은ㅋㅋㅋㅋㅋ",
          "original_form": "보통은ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:55"
        },
        {
          "id": "MDRW2100045656.1.18",
          "form": "아예 가정에서 못 만드나요?",
          "original_form": "아예 가정에서 못 만드나요?",
          "speaker_id": "2",
          "time": "20210908 04:55"
        },
        {
          "id": "MDRW2100045656.1.19",
          "form": "음 쉽지 않죠",
          "original_form": "음 쉽지 않죠",
          "speaker_id": "1",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100045656.1.20",
          "form": "단 맛이 나나요?",
          "original_form": "단 맛이 나나요?",
          "speaker_id": "2",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100045656.1.21",
          "form": "밥에서는 단맛이 약간 나요",
          "original_form": "밥에서는 단맛이 약간 나요",
          "speaker_id": "1",
          "time": "20210908 04:57"
        },
        {
          "id": "MDRW2100045656.1.22",
          "form": "아 혹시 일식인가요?",
          "original_form": "아 혹시 일식인가요?",
          "speaker_id": "2",
          "time": "20210908 04:57"
        },
        {
          "id": "MDRW2100045656.1.23",
          "form": "네 일식 맞아요",
          "original_form": "네 일식 맞아요",
