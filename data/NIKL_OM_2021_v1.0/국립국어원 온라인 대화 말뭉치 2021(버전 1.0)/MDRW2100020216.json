      },
      "utterance": [
        {
          "id": "MDRW2100020216.1.1",
          "form": "그체는 안됨 삶이 고단해져서",
          "original_form": "그체는 안됨 삶이 고단해져서",
          "speaker_id": "1",
          "time": "20210828 00:23"
        },
        {
          "id": "MDRW2100020216.1.2",
          "form": "삶이 고단한 게임이라",
          "original_form": "삶이 고단한 게임이라",
          "speaker_id": "1",
          "time": "20210828 00:23"
        },
        {
          "id": "MDRW2100020216.1.3",
          "form": "그체는 오늘말고",
          "original_form": "그체는 오늘말고",
          "speaker_id": "1",
          "time": "20210828 00:24"
        },
        {
          "id": "MDRW2100020216.1.4",
          "form": "다르날에 햇으면 좋겟어요",
          "original_form": "다르날에 햇으면 좋겟어요",
          "speaker_id": "1",
          "time": "20210828 00:24"
        },
        {
          "id": "MDRW2100020216.1.5",
          "form": "그체 짱나",
          "original_form": "그체 짱나",
          "speaker_id": "2",
          "time": "20210828 00:24"
        },
        {
          "id": "MDRW2100020216.1.6",
          "form": "왜그런겜인거야",
          "original_form": "왜그런겜인거야",
          "speaker_id": "2",
          "time": "20210828 00:24"
        },
        {
          "id": "MDRW2100020216.1.7",
          "form": "그니깐",
          "original_form": "그니깐",
          "speaker_id": "1",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020216.1.8",
          "form": "그럼 뭐하지",
          "original_form": "그럼 뭐하지",
          "speaker_id": "2",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020216.1.9",
          "form": "너무 ㅇ0ㅅ날게임이라그래",
          "original_form": "너무 ㅇ0ㅅ날게임이라그래",
          "speaker_id": "1",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020216.1.10",
          "form": "슈게임처럼",
          "original_form": "슈게임처럼",
          "speaker_id": "1",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020216.1.11",
          "form": "재밋다가도 힘들어서",
          "original_form": "재밋다가도 힘들어서",
          "speaker_id": "1",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020216.1.12",
          "form": "주저앉게됨",
          "original_form": "주저앉게됨",
          "speaker_id": "1",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020216.1.13",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "1",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020216.1.14",
          "form": "그래맞다",
          "original_form": "그래맞다",
          "speaker_id": "2",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020216.1.15",
          "form": "이스포츠판도 앵간 고인판 아닌가",
          "original_form": "이스포츠판도 앵간 고인판 아닌가",
          "speaker_id": "2",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100020216.1.16",
          "form": "좋아하는 사람만 좋아해",
          "original_form": "좋아하는 사람만 좋아해",
          "speaker_id": "2",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100020216.1.17",
          "form": "그런듯",
          "original_form": "그런듯",
          "speaker_id": "1",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100020216.1.18",
          "form": "저는 요즘도 옵치 프로판을 봐요",
          "original_form": "저는 요즘도 옵치 프로판을 봐요",
          "speaker_id": "2",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020216.1.19",
          "form": "오늘도 선수 인터뷰 봤다..",
          "original_form": "오늘도 선수 인터뷰 봤다..",
          "speaker_id": "2",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020216.1.20",
          "form": "오 대박이네요",
          "original_form": "오 대박이네요",
          "speaker_id": "1",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020216.1.21",
          "form": "재밌더라",
          "original_form": "재밌더라",
          "speaker_id": "2",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020216.1.22",
          "form": "요즘 코로나땜에 캠켜고 하는 인터뷰도 많은데",
          "original_form": "요즘 코로나땜에 캠켜고 하는 인터뷰도 많은데",
          "speaker_id": "2",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020216.1.23",
          "form": "막 자기 캠 없고 안 씻었다고 안 켜고 그러는 거 소소하게 웃김",
          "original_form": "막 자기 캠 없고 안 씻었다고 안 켜고 그러는 거 소소하게 웃김",
          "speaker_id": "2",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020216.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020216.1.25",
          "form": "안켜도돼요?",
          "original_form": "안켜도돼요?",
          "speaker_id": "1",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020216.1.26",
          "form": "그런가봄",
          "original_form": "그런가봄",
          "speaker_id": "2",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020216.1.27",
          "form": "프로판 얼굴까는게 완전 필수는 아닌거같아요",
          "original_form": "프로판 얼굴까는게 완전 필수는 아닌거같아요",
          "speaker_id": "2",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020216.1.28",
          "form": "할만하네",
          "original_form": "할만하네",
          "speaker_id": "1",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020216.1.29",
          "form": "안 까면 프로생활 하기 힘들어서 그렇지",
          "original_form": "안 까면 프로생활 하기 힘들어서 그렇지",
          "speaker_id": "2",
          "time": "20210828 00:29"
        }
      ]
    }
  ]
