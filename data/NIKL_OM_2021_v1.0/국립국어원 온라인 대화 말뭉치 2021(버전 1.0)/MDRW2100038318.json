      "utterance": [
        {
          "id": "MDRW2100038318.1.1",
          "form": "어느 축구 팀 응원하세요?",
          "original_form": "어느 축구 팀 응원하세요?",
          "speaker_id": "1",
          "time": "20210902 09:53"
        },
        {
          "id": "MDRW2100038318.1.2",
          "form": "저는 손흥민선수좋아해서 토트넘이요ㅎㅎ",
          "original_form": "저는 손흥민선수좋아해서 토트넘이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100038318.1.3",
          "form": "아하 손흥민 선수가 토트넘이었군요",
          "original_form": "아하 손흥민 선수가 토트넘이었군요",
          "speaker_id": "1",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100038318.1.4",
          "form": "주변에 토트넘 팬이 많은 이유를 이제 알았어요",
          "original_form": "주변에 토트넘 팬이 많은 이유를 이제 알았어요",
          "speaker_id": "1",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100038318.1.5",
          "form": "ㅋㅋㅋㅋ 네네 맞아요",
          "original_form": "ㅋㅋㅋㅋ 네네 맞아요",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100038318.1.6",
          "form": "영국가서 보고싶었는데 코로나가 터져서 아쉬워요..ㅎㅎ ㅠㅠ",
          "original_form": "영국가서 보고싶었는데 코로나가 터져서 아쉬워요..ㅎㅎ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100038318.1.7",
          "form": "영국은 지금 마스크도 안쓴다던데",
          "original_form": "영국은 지금 마스크도 안쓴다던데",
          "speaker_id": "1",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100038318.1.8",
          "form": "괜찮은가 모르겠어요",
          "original_form": "괜찮은가 모르겠어요",
          "speaker_id": "1",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100038318.1.9",
          "form": "안괜찮더라구요..ㅎㅎ 위드코로나라던데 솔직히 이해는 안가네요 ㅎㅎ ㅠㅠㅋ",
          "original_form": "안괜찮더라구요..ㅎㅎ 위드코로나라던데 솔직히 이해는 안가네요 ㅎㅎ ㅠㅠㅋ",
          "speaker_id": "2",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100038318.1.10",
          "form": "무슨 생각으로 위드코로나 하는지...",
          "original_form": "무슨 생각으로 위드코로나 하는지...",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100038318.1.11",
          "form": "ㅋㅋㅋ그냥 이미 늦었다생각하고 포기한거같아요",
          "original_form": "ㅋㅋㅋ그냥 이미 늦었다생각하고 포기한거같아요",
          "speaker_id": "2",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100038318.1.12",
          "form": "관중석도 거의 만원이더라구요..ㅎㅎ",
          "original_form": "관중석도 거의 만원이더라구요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100038318.1.13",
          "form": "심각하네요",
          "original_form": "심각하네요",
          "speaker_id": "1",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100038318.1.14",
          "form": "선수들은 괜찮겠죠ㅠ",
          "original_form": "선수들은 괜찮겠죠ㅠ",
          "speaker_id": "1",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100038318.1.15",
          "form": "안괜찮더라고요 ㅋㅋ 계속 나와요 매 라운드 마다 감염자가 흑흑 ㅠㅠㅋㅋ",
          "original_form": "안괜찮더라고요 ㅋㅋ 계속 나와요 매 라운드 마다 감염자가 흑흑 ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100038318.1.16",
          "form": "아이고야ㅠ 맘 아프시겠어요",
          "original_form": "아이고야ㅠ 맘 아프시겠어요",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100038318.1.17",
          "form": "불안불안해요 ㅠㅠㅋ 시합도 많아서 ㅎㅎ",
          "original_form": "불안불안해요 ㅠㅠㅋ 시합도 많아서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100038318.1.18",
          "form": "지금 시즌중인가요?",
          "original_form": "지금 시즌중인가요?",
          "speaker_id": "1",
