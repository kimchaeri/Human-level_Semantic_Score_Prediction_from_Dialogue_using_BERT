      "utterance": [
        {
          "id": "MDRW2100033590.1.1",
          "form": "좋아하는 공연 있으세요?",
          "original_form": "좋아하는 공연 있으세요?",
          "speaker_id": "1",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100033590.1.2",
          "form": "저는 가수나 아이돌 공연 좋아해오",
          "original_form": "저는 가수나 아이돌 공연 좋아해오",
          "speaker_id": "2",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100033590.1.3",
          "form": "오 누구 좋아하세요?",
          "original_form": "오 누구 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100033590.1.4",
          "form": "방탄소년단이랑 청하요!",
          "original_form": "방탄소년단이랑 청하요!",
          "speaker_id": "2",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100033590.1.5",
          "form": "오 그럼 콘서트도 가보셨나요?",
          "original_form": "오 그럼 콘서트도 가보셨나요?",
          "speaker_id": "1",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100033590.1.6",
          "form": "청하는 시상식에서 많이 봤고 방탄은 콘서트 가봤어요",
          "original_form": "청하는 시상식에서 많이 봤고 방탄은 콘서트 가봤어요",
          "speaker_id": "2",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100033590.1.7",
          "form": "방탄콘 가기 힘들다는데",
          "original_form": "방탄콘 가기 힘들다는데",
          "speaker_id": "1",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100033590.1.8",
          "form": "가보셨군요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "가보셨군요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100033590.1.9",
          "form": "티켓팅 정말 힘들었어요ㅠㅠ",
          "original_form": "티켓팅 정말 힘들었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100033590.1.10",
          "form": "성공하고 가신게 신기해요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "성공하고 가신게 신기해요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100033590.1.11",
          "form": "저 성공하고 친구들도 다 놀라더라구오",
          "original_form": "저 성공하고 친구들도 다 놀라더라구오",
          "speaker_id": "2",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100033590.1.12",
          "form": "그럴 거 같아요 ㅋㅋ ㅋㅋㅋ",
          "original_form": "그럴 거 같아요 ㅋㅋ ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:43"
        },
        {
          "id": "MDRW2100033590.1.13",
          "form": "뮤지컬도 좋아하세요?",
          "original_form": "뮤지컬도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 16:43"
        },
        {
          "id": "MDRW2100033590.1.14",
          "form": "네! 다만 자리별로 가격이 천차만별이라 많이 보진 못 했어요",
          "original_form": "네! 다만 자리별로 가격이 천차만별이라 많이 보진 못 했어요",
