      "utterance": [
        {
          "id": "MDRW2100001671.1.1",
          "form": "반려동물키우시나요?",
          "original_form": "반려동물키우시나요?",
          "speaker_id": "2",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100001671.1.2",
          "form": "고양이 집사로 살다가 무지개다리 건넌뒤로는 랜선집사로 살고있어요 ㅎㅎ",
          "original_form": "고양이 집사로 살다가 무지개다리 건넌뒤로는 랜선집사로 살고있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 18:02"
        },
        {
          "id": "MDRW2100001671.1.3",
          "form": "ㅜㅜㅜ듣기만해도 넘 슬프네요ㅜ",
          "original_form": "ㅜㅜㅜ듣기만해도 넘 슬프네요ㅜ",
          "speaker_id": "2",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001671.1.4",
          "form": "저도 집산데",
          "original_form": "저도 집산데",
          "speaker_id": "2",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001671.1.5",
          "form": "수명이 조금이라도 더 길었으면 좋겠어요ㅜㅜ",
          "original_form": "수명이 조금이라도 더 길었으면 좋겠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 18:11"
        },
        {
          "id": "MDRW2100001671.1.6",
          "form": "맞아요.... 정말 무엇과도 바꿀수 없는 존재인데 평생 함께할수 없다는게ㅠㅠ",
          "original_form": "맞아요.... 정말 무엇과도 바꿀수 없는 존재인데 평생 함께할수 없다는게ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001671.1.7",
          "form": "집사시군요! 뭔가 반갑네요",
          "original_form": "집사시군요! 뭔가 반갑네요",
          "speaker_id": "1",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001671.1.8",
          "form": "네 진짜 이렇게 볼때마다 사랑을배우게돼요 뭔가 몽글몽글 가만히있어도 뽀뽀 갈기러 가고",
          "original_form": "네 진짜 이렇게 볼때마다 사랑을배우게돼요 뭔가 몽글몽글 가만히있어도 뽀뽀 갈기러 가고",
          "speaker_id": "2",
          "time": "20210819 18:17"
        },
        {
          "id": "MDRW2100001671.1.9",
          "form": "다른 공간에서 쉬고 있는데 굳이 굳이 찾아가서 뽀뽀 갈기고.... 쓰다듬고 냄새 맡고ㅋㅋㅋ",
          "original_form": "다른 공간에서 쉬고 있는데 굳이 굳이 찾아가서 뽀뽀 갈기고.... 쓰다듬고 냄새 맡고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:18"
        },
        {
          "id": "MDRW2100001671.1.10",
          "form": "집사들은 다 똑같나봐요",
          "original_form": "집사들은 다 똑같나봐요",
          "speaker_id": "1",
          "time": "20210819 18:18"
        },
        {
          "id": "MDRW2100001671.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 갑자기 참을수없을때가있어요 너무귀여워서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 갑자기 참을수없을때가있어요 너무귀여워서",
          "speaker_id": "2",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100001671.1.12",
          "form": "랜선으로 이뻐하는 고양이들 있나여?",
          "original_form": "랜선으로 이뻐하는 고양이들 있나여?",
          "speaker_id": "2",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100001671.1.13",
          "form": "혹시...고양이 누리를 아시나요?",
          "original_form": "혹시...고양이 누리를 아시나요?",
          "speaker_id": "1",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001671.1.14",
          "form": "누리즈의 랜선집사입니다ㅋㅋㅋㅋ",
          "original_form": "누리즈의 랜선집사입니다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001671.1.15",
          "form": "네 알죠알죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 알죠알죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001671.1.16",
          "form": "저도 누리네랑 하삼네 알림뜨면뛰어갑니다",
          "original_form": "저도 누리네랑 하삼네 알림뜨면뛰어갑니다",
          "speaker_id": "2",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001671.1.17",
          "form": "하삼님네 영상들도 많이 봤어요!ㅋㅋ",
          "original_form": "하삼님네 영상들도 많이 봤어요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:34"
        },
        {
          "id": "MDRW2100001671.1.18",
          "form": "냥이들이 어쩜 그렇게 다 다르고 개성이 넘칠까요",
          "original_form": "냥이들이 어쩜 그렇게 다 다르고 개성이 넘칠까요",
          "speaker_id": "1",
          "time": "20210819 18:34"
        },
        {
          "id": "MDRW2100001671.1.19",
          "form": "그니까여 넘 신기하고 똑똑해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까여 넘 신기하고 똑똑해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:39"
        },
        {
          "id": "MDRW2100001671.1.20",
          "form": "귀엽기도 넘귀여워서 ㅋㅋㅋㅋㅋ",
          "original_form": "귀엽기도 넘귀여워서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:39"
        },
        {
          "id": "MDRW2100001671.1.21",
          "form": "볼때마다 최애가 바껴요",
          "original_form": "볼때마다 최애가 바껴요",
          "speaker_id": "2",
          "time": "20210819 18:39"
        },
        {
          "id": "MDRW2100001671.1.22",
          "form": "맞아요ㅋㅋㅋㅋ 그날 그날 하드캐리하는 아이가 다 달라요",
          "original_form": "맞아요ㅋㅋㅋㅋ 그날 그날 하드캐리하는 아이가 다 달라요",
          "speaker_id": "1",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100001671.1.23",
          "form": "저도 첨엔 특정 한 아이에게 감겨서 채널 들어갔었는데 매번 최애가 바뀌는 경험을..",
          "original_form": "저도 첨엔 특정 한 아이에게 감겨서 채널 들어갔었는데 매번 최애가 바뀌는 경험을..",
          "speaker_id": "1",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100001671.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ긍까요 어쩜 그렇게 다양하게 이쁜이들이있는지",
          "original_form": "ㅋㅋㅋㅋㅋㅋ긍까요 어쩜 그렇게 다양하게 이쁜이들이있는지",
          "speaker_id": "2",
          "time": "20210819 18:50"
        }
      ]
    }
