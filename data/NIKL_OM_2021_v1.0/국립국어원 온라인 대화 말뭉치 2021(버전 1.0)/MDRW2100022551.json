      "utterance": [
        {
          "id": "MDRW2100022551.1.1",
          "form": "여기 비 그치고 흐리기만 함\r\n근데 밝게 흐린거보니 비 안올거 같기도해",
          "original_form": "여기 비 그치고 흐리기만 함\r\n근데 밝게 흐린거보니 비 안올거 같기도해",
          "speaker_id": "2",
          "time": "20210829 15:41"
        },
        {
          "id": "MDRW2100022551.1.2",
          "form": "여기 밝게 흐리다 비 오더라",
          "original_form": "여기 밝게 흐리다 비 오더라",
          "speaker_id": "1",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022551.1.3",
          "form": "또 비 올 수 있음ㅜㅜ",
          "original_form": "또 비 올 수 있음ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022551.1.4",
          "form": "진짜 변덕이 심해",
          "original_form": "진짜 변덕이 심해",
          "speaker_id": "2",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022551.1.5",
          "form": "ㅇㅇ날씨가 이상혀~~",
          "original_form": "ㅇㅇ날씨가 이상혀~~",
          "speaker_id": "1",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100022551.1.6",
          "form": "지금 딱 좋아ㅋ\r\n밝고 선선하니ㅎㅎ창가로 보고 있으니 좋다",
          "original_form": "지금 딱 좋아ㅋ\r\n밝고 선선하니ㅎㅎ창가로 보고 있으니 좋다",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022551.1.7",
          "form": "이러다 가을되겠지ㅎ",
          "original_form": "이러다 가을되겠지ㅎ",
          "speaker_id": "2",
          "time": "20210829 16:16"
        },
        {
          "id": "MDRW2100022551.1.8",
          "form": "슬 에어컨이 추워져\r\n지금 커피숍이라ㅎㅎ",
          "original_form": "슬 에어컨이 추워져\r\n지금 커피숍이라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:16"
        },
        {
          "id": "MDRW2100022551.1.9",
          "form": "우와 여긴 해나서 갑자기 엄청밝다",
          "original_form": "우와 여긴 해나서 갑자기 엄청밝다",
          "speaker_id": "1",
          "time": "20210829 16:16"
        },
        {
          "id": "MDRW2100022551.1.10",
          "form": "ㅋㅋㅋ날씨 변덕 뭐여",
          "original_form": "ㅋㅋㅋ날씨 변덕 뭐여",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022551.1.11",
          "form": "여긴 오늘 주으로 해 안뜰거같어",
          "original_form": "여긴 오늘 주으로 해 안뜰거같어",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022551.1.12",
          "form": "하긴 시간이 해질때 다 되서",
          "original_form": "하긴 시간이 해질때 다 되서",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022551.1.13",
          "form": "응 밝긴밝은데 구름이 먹구름보다는 흰구름이 가득있어ㅎ",
          "original_form": "응 밝긴밝은데 구름이 먹구름보다는 흰구름이 가득있어ㅎ",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022551.1.14",
          "form": "아침부터 해났으면 놀이동산도 갔을텐데",
          "original_form": "아침부터 해났으면 놀이동산도 갔을텐데",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022551.1.15",
          "form": "아까비",
          "original_form": "아까비",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022551.1.16",
          "form": "그러게ㅎ 날씨가 진짜 변덕스러워",
          "original_form": "그러게ㅎ 날씨가 진짜 변덕스러워",
          "speaker_id": "2",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100022551.1.17",
          "form": "이제는 겉옷 챙겨야하려나\r\n나 카페서 나오니 거기 추웠던거 훅 체감돼고 감기걸릴거같아ㅜㅜㅋ",
          "original_form": "이제는 겉옷 챙겨야하려나\r\n나 카페서 나오니 거기 추웠던거 훅 체감돼고 감기걸릴거같아ㅜㅜㅋ",
          "speaker_id": "2",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022551.1.18",
          "form": "밖이 딱 좋네 선선하니ㅎ",
          "original_form": "밖이 딱 좋네 선선하니ㅎ",
          "speaker_id": "2",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022551.1.19",
          "form": "여기도 해질라하니까 날씨 괜찮은듯",
          "original_form": "여기도 해질라하니까 날씨 괜찮은듯",
          "speaker_id": "1",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100022551.1.20",
          "form": "ㅎㅎ해 질라하는데 더 밝아짐ㅋㅋ",
          "original_form": "ㅎㅎ해 질라하는데 더 밝아짐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022551.1.21",
