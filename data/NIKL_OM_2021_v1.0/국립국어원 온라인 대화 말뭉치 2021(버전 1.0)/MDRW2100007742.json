      "utterance": [
        {
          "id": "MDRW2100007742.1.1",
          "form": "아침은?먹었나",
          "original_form": "아침은?먹었나",
          "speaker_id": "1",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100007742.1.2",
          "form": "아니 이제 해야된다",
          "original_form": "아니 이제 해야된다",
          "speaker_id": "2",
          "time": "20210822 10:11"
        },
        {
          "id": "MDRW2100007742.1.3",
          "form": "너는 먹었어?",
          "original_form": "너는 먹었어?",
          "speaker_id": "2",
          "time": "20210822 10:11"
        },
        {
          "id": "MDRW2100007742.1.4",
          "form": "나도 아직 ㅠname3는 내려와서 돌아다니고ㅠㅠ",
          "original_form": "나도 아직 ㅠ&name3&는 내려와서 돌아다니고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 10:14"
        },
        {
          "id": "MDRW2100007742.1.5",
          "form": "ㅋㅋㅋ밥먹고 설거지하고 돌아서니 점심시간이네ㅋㅋ",
          "original_form": "ㅋㅋㅋ밥먹고 설거지하고 돌아서니 점심시간이네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100007742.1.6",
          "form": "이런ㅋㅋㄱ아침은 뭐먹었어?",
          "original_form": "이런ㅋㅋㄱ아침은 뭐먹었어?",
          "speaker_id": "1",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100007742.1.7",
          "form": "응 집에 카레, 짜장, 밑반찬있어서",
          "original_form": "응 집에 카레, 짜장, 밑반찬있어서",
          "speaker_id": "2",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007742.1.8",
          "form": "그거먹고ㅋㅋ",
          "original_form": "그거먹고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007742.1.9",
          "form": "점심은 뭐먹노?ㅜㅠ국수라도 삶아야되나싶다",
          "original_form": "점심은 뭐먹노?ㅜㅠ국수라도 삶아야되나싶다",
          "speaker_id": "2",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007742.1.10",
          "form": "ㅋㅋ난점심 엽떡",
          "original_form": "ㅋㅋ난점심 엽떡",
          "speaker_id": "1",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007742.1.11",
          "form": "시킬까 싶당ㅋㅋㅋ",
          "original_form": "시킬까 싶당ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007742.1.12",
          "form": "쉬고싶다",
          "original_form": "쉬고싶다",
          "speaker_id": "1",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007742.1.13",
          "form": "엽떡 맛있지ㅋㅋ",
          "original_form": "엽떡 맛있지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007742.1.14",
          "form": "ㅋㅋㅋ피곤하제?",
          "original_form": "ㅋㅋㅋ피곤하제?",
          "speaker_id": "2",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007742.1.15",
          "form": "나도 이제 쇼파에 누웠다.ㅋㅋ",
          "original_form": "나도 이제 쇼파에 누웠다.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007742.1.16",
          "form": "쉬고싶은데 또 국수해야되고ㅠㅜㅋㅋㅋ",
          "original_form": "쉬고싶은데 또 국수해야되고ㅠㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007742.1.17",
          "form": "점심 먹었나ㅋ",
          "original_form": "점심 먹었나ㅋ",
          "speaker_id": "1",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100007742.1.18",
          "form": "응 편의점에서 삼각김밥사먹고 급 캠핑왔다ㅋ",
          "original_form": "응 편의점에서 삼각김밥사먹고 급 캠핑왔다ㅋ",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100007742.1.19",
          "form": "대박 어디로 가써???",
          "original_form": "대박 어디로 가써???",
