      "utterance": [
        {
          "id": "MDRW2100010787.1.1",
          "form": "그러게요 선선해졌어요",
          "original_form": "그러게요 선선해졌어요",
          "speaker_id": "2",
          "time": "20210823 21:51"
        },
        {
          "id": "MDRW2100010787.1.2",
          "form": "비오나요?",
          "original_form": "비오나요?",
          "speaker_id": "1",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100010787.1.3",
          "form": "비가 하루종일 오네요 거긴 어때요?",
          "original_form": "비가 하루종일 오네요 거긴 어때요?",
          "speaker_id": "2",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100010787.1.4",
          "form": "오다안오다 하네요",
          "original_form": "오다안오다 하네요",
          "speaker_id": "1",
          "time": "20210824 12:06"
        },
        {
          "id": "MDRW2100010787.1.5",
          "form": "어떤 날좋아하시나요?",
          "original_form": "어떤 날좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 12:06"
        },
        {
          "id": "MDRW2100010787.1.6",
          "form": "저는 맑은날 좋아해요 지금도 비오나요?",
          "original_form": "저는 맑은날 좋아해요 지금도 비오나요?",
          "speaker_id": "2",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100010787.1.7",
          "form": "오다안오다해요",
          "original_form": "오다안오다해요",
          "speaker_id": "1",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100010787.1.8",
          "form": "거긴요?",
          "original_form": "거긴요?",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100010787.1.9",
          "form": "여기도 비가 오다안오다 하네요 눈 좋아하세요?",
          "original_form": "여기도 비가 오다안오다 하네요 눈 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 18:55"
        },
        {
          "id": "MDRW2100010787.1.10",
          "form": "추운건 안좋지만 눈은 좋아해요 ㅎ",
          "original_form": "추운건 안좋지만 눈은 좋아해요 ㅎ",
          "speaker_id": "1",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100010787.1.11",
          "form": "추운게 좋나요 더운게 좋나요?",
          "original_form": "추운게 좋나요 더운게 좋나요?",
          "speaker_id": "2",
          "time": "20210830 18:58"
        },
        {
