      "utterance": [
        {
          "id": "MDRW2100022532.1.1",
          "form": "혼자 가시면 무섭지 않아여?",
          "original_form": "혼자 가시면 무섭지 않아여?",
          "speaker_id": "1",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100022532.1.2",
          "form": "혼자여행 좋아해요ㅎ",
          "original_form": "혼자여행 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100022532.1.3",
          "form": "같이도 재밋지만 혼자도 편해서요",
          "original_form": "같이도 재밋지만 혼자도 편해서요",
          "speaker_id": "2",
          "time": "20210829 15:29"
        },
        {
          "id": "MDRW2100022532.1.4",
          "form": "혼자 여행은 멀리 가보셧어요?",
          "original_form": "혼자 여행은 멀리 가보셧어요?",
          "speaker_id": "1",
          "time": "20210829 15:29"
        },
        {
          "id": "MDRW2100022532.1.5",
          "form": "유럽다녀왓습니다",
          "original_form": "유럽다녀왓습니다",
          "speaker_id": "2",
          "time": "20210829 15:33"
        },
        {
          "id": "MDRW2100022532.1.6",
          "form": "그냥 성격이 여행자체를 좋아하시나보네요..",
          "original_form": "그냥 성격이 여행자체를 좋아하시나보네요..",
          "speaker_id": "1",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100022532.1.7",
          "form": "집순이긴한데.. 이때아님 언제가나해서 떠낫어요ㅎ",
          "original_form": "집순이긴한데.. 이때아님 언제가나해서 떠낫어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022532.1.8",
          "form": "몇일 일정이었어요?",
          "original_form": "몇일 일정이었어요?",
          "speaker_id": "1",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022532.1.9",
          "form": "2주요",
          "original_form": "2주요",
          "speaker_id": "2",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100022532.1.10",
          "form": "엄청 기네요.. 몇 개국 돌으신거에요?",
          "original_form": "엄청 기네요.. 몇 개국 돌으신거에요?",
          "speaker_id": "1",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100022532.1.11",
          "form": "4개정도? 다시가기 힘들꺼같아싀 막 돌아다녓어요ㅎ",
          "original_form": "4개정도? 다시가기 힘들꺼같아싀 막 돌아다녓어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022532.1.12",
          "form": "후랑스 독일 벨기에 스위스? 이런 코스? ㅋㅋㅋ",
          "original_form": "후랑스 독일 벨기에 스위스? 이런 코스? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022532.1.13",
          "form": "아낟ㅎ동유럽이라",
          "original_form": "아낟ㅎ동유럽이라",
          "speaker_id": "2",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022532.1.14",
          "form": "체코 오스트리아 부다페스트?",
          "original_form": "체코 오스트리아 부다페스트?",
          "speaker_id": "2",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022532.1.15",
          "form": "오\r\n. 뭔가 클래식 느낌.. ㅋㅋ",
          "original_form": "오\r\n. 뭔가 클래식 느낌.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022532.1.16",
          "form": "연주회 같은거도 보셧나요?",
          "original_form": "연주회 같은거도 보셧나요?",
          "speaker_id": "1",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022532.1.17",
          "form": "입석으로 잠깐요ㅎㅅㅅ",
          "original_form": "입석으로 잠깐요ㅎㅅㅅ",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022532.1.18",
          "form": "전 자연경관 쪽이 좋더라구요",
          "original_form": "전 자연경관 쪽이 좋더라구요",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022532.1.19",
          "form": "오스트리아는 워낙 유명하고..",
          "original_form": "오스트리아는 워낙 유명하고..",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022532.1.20",
          "form": "헝가리에도 산 있나여?",
          "original_form": "헝가리에도 산 있나여?",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022532.1.21",
          "form": "거긴 그냥  야경구경햇어요ㅎ",
          "original_form": "거긴 그냥  야경구경햇어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022532.1.22",
          "form": "동유럽 안무서웟어요?",
          "original_form": "동유럽 안무서웟어요?",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022532.1.23",
          "form": "막 음침하고.. 드랴쿨라 나올거 같고..",
          "original_form": "막 음침하고.. 드랴쿨라 나올거 같고..",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022532.1.24",
          "form": "음 밝을때만 다넛어요ㅎ",
          "original_form": "음 밝을때만 다넛어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 18:38"
        }
      ]
