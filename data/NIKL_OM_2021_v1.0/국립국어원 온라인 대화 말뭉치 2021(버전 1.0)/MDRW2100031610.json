      "utterance": [
        {
          "id": "MDRW2100031610.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100031610.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100031610.1.3",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100031610.1.4",
          "form": "운동 좋아하기는 하는데, 잘 안해요",
          "original_form": "운동 좋아하기는 하는데, 잘 안해요",
          "speaker_id": "1",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100031610.1.5",
          "form": "게을러서..",
          "original_form": "게을러서..",
          "speaker_id": "1",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100031610.1.6",
          "form": "name2님은 운동 좋아하세요?",
          "original_form": "&name2&님은 운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100031610.1.7",
          "form": "앗 저도 잘은 안해요",
          "original_form": "앗 저도 잘은 안해요",
          "speaker_id": "2",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100031610.1.8",
          "form": "하신다면 무슨 운동 하시나요??",
          "original_form": "하신다면 무슨 운동 하시나요??",
          "speaker_id": "2",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100031610.1.9",
          "form": "2018년에는 수영을 열심히 배웠습니다!",
          "original_form": "2018년에는 수영을 열심히 배웠습니다!",
          "speaker_id": "1",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100031610.1.10",
          "form": "물에서 노는 것을 좋아해서요!",
          "original_form": "물에서 노는 것을 좋아해서요!",
          "speaker_id": "1",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100031610.1.11",
          "form": "우와!!",
          "original_form": "우와!!",
          "speaker_id": "2",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100031610.1.12",
          "form": "수영 잘 하시나요???",
          "original_form": "수영 잘 하시나요???",
          "speaker_id": "2",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100031610.1.13",
          "form": "지금도 가끔 수영을 가기는 하는데, 잘 하지는 못 합니다..",
          "original_form": "지금도 가끔 수영을 가기는 하는데, 잘 하지는 못 합니다..",
          "speaker_id": "1",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100031610.1.14",
          "form": "name2님은 어떤 운동을 하시나요?",
          "original_form": "&name2&님은 어떤 운동을 하시나요?",
          "speaker_id": "1",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100031610.1.15",
          "form": "얼마나 하면 물에 뜰 수 있나요...?",
          "original_form": "얼마나 하면 물에 뜰 수 있나요...?",
          "speaker_id": "2",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100031610.1.16",
          "form": "저는 홈트해요 ㅋㅋㅋ",
          "original_form": "저는 홈트해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031610.1.17",
          "form": "음 한달 하면 물에 뜰 수 있어요ㅋㅋㅋ",
          "original_form": "음 한달 하면 물에 뜰 수 있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031610.1.18",
          "form": "근데 뜨기 위해서 끊임없이 움직여야합니다",
          "original_form": "근데 뜨기 위해서 끊임없이 움직여야합니다",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031610.1.19",
          "form": "오 저도 배우고 싶어요",
          "original_form": "오 저도 배우고 싶어요",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031610.1.20",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031610.1.21",
          "form": "오오 홈트!",
          "original_form": "오오 홈트!",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031610.1.22",
          "form": "홈트 준비물은 어떤게 있어요?",
          "original_form": "홈트 준비물은 어떤게 있어요?",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031610.1.23",
          "form": "홈트하려고..매트는 샀는데",
          "original_form": "홈트하려고..매트는 샀는데",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031610.1.24",
          "form": "요가매트만 있으면 되는 거 같아요",
          "original_form": "요가매트만 있으면 되는 거 같아요",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031610.1.25",
          "form": "대부분 맨몸 운동이더라구요",
          "original_form": "대부분 맨몸 운동이더라구요",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031610.1.26",
          "form": "아직 한번도 안했어요ㅜㅠ",
          "original_form": "아직 한번도 안했어요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031610.1.27",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031610.1.28",
          "form": "이제 시작하시면...!!!",
          "original_form": "이제 시작하시면...!!!",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031610.1.29",
          "form": "홈트도 종류가 다양하던데요",
          "original_form": "홈트도 종류가 다양하던데요",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031610.1.30",
          "form": "유산소쪽으로 관심 있으신가요?",
          "original_form": "유산소쪽으로 관심 있으신가요?",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031610.1.31",
          "form": "이제라도..운동을 하면 되겠죠?ㅎㅎ",
          "original_form": "이제라도..운동을 하면 되겠죠?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031610.1.32",
          "form": "유산소도 좋고 근력도 좋아욤",
          "original_form": "유산소도 좋고 근력도 좋아욤",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031610.1.33",
          "form": "뭐든지 몸을 움직이는게 필요한거 같은..",
          "original_form": "뭐든지 몸을 움직이는게 필요한거 같은..",
          "speaker_id": "1",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100031610.1.34",
          "form": "네 수영도 하셨는데요 뭐!!!",
          "original_form": "네 수영도 하셨는데요 뭐!!!",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100031610.1.35",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100031610.1.36",
          "form": "꾸준히만 하면 하루 10분만 해도 건강에 좋댔어요",
          "original_form": "꾸준히만 하면 하루 10분만 해도 건강에 좋댔어요",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100031610.1.37",
          "form": "홈트하실 때 비디오?보시면서 하시나요?",
          "original_form": "홈트하실 때 비디오?보시면서 하시나요?",
          "speaker_id": "1",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100031610.1.38",
          "form": "예전에 유튜브에서 타바타 보고 따라했었는데",
          "original_form": "예전에 유튜브에서 타바타 보고 따라했었는데",
          "speaker_id": "1",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100031610.1.39",
          "form": "원숭이?고릴라? 같이 보이는 타바타여서 동생이 놀린..기억이 나네요",
          "original_form": "원숭이?고릴라? 같이 보이는 타바타여서 동생이 놀린..기억이 나네요",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031610.1.40",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031610.1.41",
          "form": "네 저 보고 따라해요",
          "original_form": "네 저 보고 따라해요",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031610.1.42",
          "form": "비디오 안보면 못하겠어요",
          "original_form": "비디오 안보면 못하겠어요",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031610.1.43",
          "form": "고릴라처럼 보이는 타바타라닠ㅋㅋㅋㅋ동생분과 사이가 좋으시군요 ㅋㅋㅋㅋ",
          "original_form": "고릴라처럼 보이는 타바타라닠ㅋㅋㅋㅋ동생분과 사이가 좋으시군요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031610.1.44",
          "form": "다음에 한번..그 타바타 해보세요!",
          "original_form": "다음에 한번..그 타바타 해보세요!",
