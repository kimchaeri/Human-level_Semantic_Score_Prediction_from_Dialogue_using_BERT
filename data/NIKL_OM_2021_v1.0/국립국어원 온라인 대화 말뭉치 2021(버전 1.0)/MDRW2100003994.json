      "utterance": [
        {
          "id": "MDRW2100003994.1.1",
          "form": "안녕하세요   점심 뭐드셨나요?",
          "original_form": "안녕하세요   점심 뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003994.1.2",
          "form": "저는 황태국이랑 카레요",
          "original_form": "저는 황태국이랑 카레요",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003994.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003994.1.4",
          "form": "저는 떡볶이 먹었었어요!",
          "original_form": "저는 떡볶이 먹었었어요!",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003994.1.5",
          "form": "오 맛있얶겠다",
          "original_form": "오 맛있얶겠다",
          "speaker_id": "2",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003994.1.6",
          "form": "저녁은 뭐드실 예정?",
          "original_form": "저녁은 뭐드실 예정?",
          "speaker_id": "2",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003994.1.7",
          "form": "음  잘 모르겠는데  지금은 고기 구워먹고 싶어요",
          "original_form": "음  잘 모르겠는데  지금은 고기 구워먹고 싶어요",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003994.1.8",
          "form": "고기좋죠",
          "original_form": "고기좋죠",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003994.1.9",
          "form": "술도 좋아하시나요",
          "original_form": "술도 좋아하시나요",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003994.1.10",
          "form": "저 학생이라서 술 못마셔요ㅠㅠ",
          "original_form": "저 학생이라서 술 못마셔요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003994.1.11",
          "form": "아하 !",
          "original_form": "아하 !",
          "speaker_id": "2",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100003994.1.12",
          "form": "배달음식도 즐겨드시나요?",
          "original_form": "배달음식도 즐겨드시나요?",
          "speaker_id": "2",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100003994.1.13",
          "form": "넹  피자나 치킨 시켜먹어요!",
          "original_form": "넹  피자나 치킨 시켜먹어요!",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100003994.1.14",
          "form": "name1님도  배달음식 자주 드시나요?",
          "original_form": "&name1&님도  배달음식 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100003994.1.15",
          "form": "네 자주먹어요 ㅎㅎ 배달음식 넘 간편하고 좋죠?",
          "original_form": "네 자주먹어요 ㅎㅎ 배달음식 넘 간편하고 좋죠?",
          "speaker_id": "2",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100003994.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100003994.1.17",
