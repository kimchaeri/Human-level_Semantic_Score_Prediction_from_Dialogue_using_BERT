        },
        {
          "id": "MDRW2100008958.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 19:59"
        },
        {
          "id": "MDRW2100008958.1.5",
          "form": "name2 님은 디저트 좋아하세요?",
          "original_form": "&name2& 님은 디저트 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 19:59"
        },
        {
          "id": "MDRW2100008958.1.6",
          "form": "저두 좋아해요! 지금 먹을까말까 고민중이었어요",
          "original_form": "저두 좋아해요! 지금 먹을까말까 고민중이었어요",
          "speaker_id": "2",
          "time": "20210822 19:59"
        },
        {
          "id": "MDRW2100008958.1.7",
          "form": "과즐이 좀 있어서..ㅎㅎ",
          "original_form": "과즐이 좀 있어서..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:59"
        },
        {
          "id": "MDRW2100008958.1.8",
          "form": "과즐이 뭔가요??",
          "original_form": "과즐이 뭔가요??",
          "speaker_id": "1",
          "time": "20210822 20:00"
        },
        {
          "id": "MDRW2100008958.1.9",
          "form": "그 한과같은건데",
          "original_form": "그 한과같은건데",
          "speaker_id": "2",
          "time": "20210822 20:01"
        },
        {
          "id": "MDRW2100008958.1.10",
          "form": "그 쌀 튀긴거 겉에 붙이고 조청이랑 안에는 에이스같은거 있어요!",
          "original_form": "그 쌀 튀긴거 겉에 붙이고 조청이랑 안에는 에이스같은거 있어요!",
          "speaker_id": "2",
          "time": "20210822 20:01"
        },
        {
          "id": "MDRW2100008958.1.11",
          "form": "오 맛있을 거 같아요",
          "original_form": "오 맛있을 거 같아요",
          "speaker_id": "1",
          "time": "20210822 20:03"
        },
        {
          "id": "MDRW2100008958.1.12",
          "form": "저도 한과 좋아해요",
          "original_form": "저도 한과 좋아해요",
          "speaker_id": "1",
          "time": "20210822 20:03"
        },
        {
          "id": "MDRW2100008958.1.13",
          "form": "이시간만 되면 단게 땡기네요..",
          "original_form": "이시간만 되면 단게 땡기네요..",
          "speaker_id": "2",
          "time": "20210822 20:30"
        },
        {
          "id": "MDRW2100008958.1.14",
          "form": "저녁은 뭐 드셨어요?",
          "original_form": "저녁은 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210822 20:31"
        },
        {
          "id": "MDRW2100008958.1.15",
          "form": "전 무화과 먹고 있어요ㅋㅋ",
          "original_form": "전 무화과 먹고 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:31"
        },
        {
          "id": "MDRW2100008958.1.16",
          "form": "저녁은 닭볶음탕이요",
          "original_form": "저녁은 닭볶음탕이요",
          "speaker_id": "1",
          "time": "20210822 20:31"
        },
        {
          "id": "MDRW2100008958.1.17",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210822 20:31"
        },
        {
          "id": "MDRW2100008958.1.18",
          "form": "헐 무화과 대박 맛있겠어요..",
          "original_form": "헐 무화과 대박 맛있겠어요..",
          "speaker_id": "2",
          "time": "20210822 20:31"
        },
        {
          "id": "MDRW2100008958.1.19",
          "form": "부드러워서 너무 맛있어요 ㅠㅠ",
          "original_form": "부드러워서 너무 맛있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 20:31"
        },
        {
          "id": "MDRW2100008958.1.20",
          "form": "닭볶음탕에 고구마 파이신가요 감자 파이신가요",
          "original_form": "닭볶음탕에 고구마 파이신가요 감자 파이신가요",
          "speaker_id": "2",
          "time": "20210822 20:32"
        },
        {
          "id": "MDRW2100008958.1.21",
          "form": "무화과 완전 맛있죠!!",
          "original_form": "무화과 완전 맛있죠!!",
          "speaker_id": "1",
          "time": "20210822 20:32"
        },
        {
          "id": "MDRW2100008958.1.22",
          "form": "헐 저 고구마는 안 먹어봤어요",
          "original_form": "헐 저 고구마는 안 먹어봤어요",
          "speaker_id": "1",
          "time": "20210822 20:32"
        },
        {
          "id": "MDRW2100008958.1.23",
          "form": "하지만 어떻게 하나를 고르죠 둘 다 맛있어요",
          "original_form": "하지만 어떻게 하나를 고르죠 둘 다 맛있어요",
          "speaker_id": "1",
          "time": "20210822 20:33"
        },
        {
          "id": "MDRW2100008958.1.24",
          "form": "저도 탕평책입니다",
          "original_form": "저도 탕평책입니다",
          "speaker_id": "2",
          "time": "20210822 20:44"
        },
        {
          "id": "MDRW2100008958.1.25",
          "form": "둘 다 너무 맛있어요..",
          "original_form": "둘 다 너무 맛있어요..",
          "speaker_id": "2",
          "time": "20210822 20:44"
        },
        {
          "id": "MDRW2100008958.1.26",
          "form": "포실한 감자.. 달달한 고구마..",
          "original_form": "포실한 감자.. 달달한 고구마..",
          "speaker_id": "2",
          "time": "20210822 20:45"
        },
        {
          "id": "MDRW2100008958.1.27",
          "form": "ㅋㅋㅋㅌㅋㅋㅋㅌ",
          "original_form": "ㅋㅋㅋㅌㅋㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210822 20:45"
        },
        {
          "id": "MDRW2100008958.1.28",
          "form": "탄수화물은 뭐든 맛있죠",
          "original_form": "탄수화물은 뭐든 맛있죠",
          "speaker_id": "1",
          "time": "20210822 20:45"
        },
        {
          "id": "MDRW2100008958.1.29",
          "form": "인정입니다 탄수화물 짱..",
          "original_form": "인정입니다 탄수화물 짱..",
          "speaker_id": "2",
          "time": "20210822 20:46"
        },
        {
          "id": "MDRW2100008958.1.30",
          "form": "저는 저녁에 에어프라이어에 냉동돈까스 먹었는데",
          "original_form": "저는 저녁에 에어프라이어에 냉동돈까스 먹었는데",
          "speaker_id": "2",
          "time": "20210822 20:46"
        },
        {
          "id": "MDRW2100008958.1.31",
          "form": "바삭한 일식돈까스 먹구싶습니다",
          "original_form": "바삭한 일식돈까스 먹구싶습니다",
          "speaker_id": "2",
          "time": "20210822 20:46"
        },
        {
          "id": "MDRW2100008958.1.32",
          "form": "저도 일식 튀김요리 좋아해요",
          "original_form": "저도 일식 튀김요리 좋아해요",
          "speaker_id": "1",
          "time": "20210822 20:46"
        },
        {
          "id": "MDRW2100008958.1.33",
          "form": "야채튀김 먹고 싶네요",
          "original_form": "야채튀김 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210822 20:47"
        },
        {
          "id": "MDRW2100008958.1.34",
          "form": "야채튀김 헉! 진짜 맛있겠어요",
          "original_form": "야채튀김 헉! 진짜 맛있겠어요",
          "speaker_id": "2",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100008958.1.35",
          "form": "튀김에 맥주한잔..!",
          "original_form": "튀김에 맥주한잔..!",
