        {
          "id": "MDRW2100001030.1.2",
          "form": "저는 강아지요",
          "original_form": "저는 강아지요",
          "speaker_id": "2",
          "time": "20210720 16:35"
        },
        {
          "id": "MDRW2100001030.1.3",
          "form": "저도 강아지 키워요",
          "original_form": "저도 강아지 키워요",
          "speaker_id": "1",
          "time": "20210720 16:35"
        },
        {
          "id": "MDRW2100001030.1.4",
          "form": "종류가 뭐에요?",
          "original_form": "종류가 뭐에요?",
          "speaker_id": "1",
          "time": "20210720 16:35"
        },
        {
          "id": "MDRW2100001030.1.5",
          "form": "말티즈요",
          "original_form": "말티즈요",
          "speaker_id": "2",
          "time": "20210720 16:35"
        },
        {
          "id": "MDRW2100001030.1.6",
          "form": "저희강아지는 푸들이에요",
          "original_form": "저희강아지는 푸들이에요",
          "speaker_id": "1",
          "time": "20210720 16:35"
        },
        {
          "id": "MDRW2100001030.1.7",
          "form": "갈색!",
          "original_form": "갈색!",
          "speaker_id": "1",
          "time": "20210720 16:35"
        },
        {
          "id": "MDRW2100001030.1.8",
          "form": "갈색?",
          "original_form": "갈색?",
          "speaker_id": "2",
          "time": "20210720 16:35"
        },
        {
          "id": "MDRW2100001030.1.9",
