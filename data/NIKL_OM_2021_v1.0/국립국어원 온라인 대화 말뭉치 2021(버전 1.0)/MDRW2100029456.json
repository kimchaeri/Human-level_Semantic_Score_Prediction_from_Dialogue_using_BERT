      "utterance": [
        {
          "id": "MDRW2100029456.1.1",
          "form": "연애중이신가요",
          "original_form": "연애중이신가요",
          "speaker_id": "1",
          "time": "20210903 08:48"
        },
        {
          "id": "MDRW2100029456.1.2",
          "form": "ㅋ결혼했어요. 지금은 연애중이라고 할순없겠네요.",
          "original_form": "ㅋ결혼했어요. 지금은 연애중이라고 할순없겠네요.",
          "speaker_id": "2",
          "time": "20210903 08:48"
        },
        {
          "id": "MDRW2100029456.1.3",
          "form": "우와 결혼 생활은 어떠세요",
          "original_form": "우와 결혼 생활은 어떠세요",
          "speaker_id": "1",
          "time": "20210903 08:50"
        },
        {
          "id": "MDRW2100029456.1.4",
          "form": "좋긴한데 연애할때 데이트가 그립네요ㅠ",
          "original_form": "좋긴한데 연애할때 데이트가 그립네요ㅠ",
          "speaker_id": "2",
          "time": "20210903 08:51"
        },
        {
          "id": "MDRW2100029456.1.5",
          "form": "그런가요ㅋㅋ 오늘 그리운김에 데이트 다녀오세용",
          "original_form": "그런가요ㅋㅋ 오늘 그리운김에 데이트 다녀오세용",
          "speaker_id": "1",
          "time": "20210903 09:07"
        },
        {
          "id": "MDRW2100029456.1.6",
          "form": "저는 남친이 해외유학가버려서ㅠㅠ",
          "original_form": "저는 남친이 해외유학가버려서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 09:07"
        },
        {
          "id": "MDRW2100029456.1.7",
          "form": "결혼도 늦어지고 큰일이에요",
          "original_form": "결혼도 늦어지고 큰일이에요",
          "speaker_id": "1",
          "time": "20210903 09:07"
        },
        {
          "id": "MDRW2100029456.1.8",
          "form": "아이공..더 오래 떨어져야 하나요?",
          "original_form": "아이공..더 오래 떨어져야 하나요?",
          "speaker_id": "2",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100029456.1.9",
          "form": "이제 돌아오려면 1년 4개월 남았어요ㅠ",
          "original_form": "이제 돌아오려면 1년 4개월 남았어요ㅠ",
          "speaker_id": "1",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100029456.1.10",
          "form": "1년 8개월 지났구ㅠㅠ",
          "original_form": "1년 8개월 지났구ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100029456.1.11",
          "form": "총 3년 기간이거든요ㅠㅠ",
          "original_form": "총 3년 기간이거든요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100029456.1.12",
          "form": "와..절반은 넘겼네요. 에휴..힘들군요.",
          "original_form": "와..절반은 넘겼네요. 에휴..힘들군요.",
          "speaker_id": "2",
          "time": "20210903 09:09"
        },
        {
          "id": "MDRW2100029456.1.13",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 09:12"
        },
        {
          "id": "MDRW2100029456.1.14",
          "form": "각자 생활 하면서도 돌아오면 난관이많아서",
          "original_form": "각자 생활 하면서도 돌아오면 난관이많아서",
          "speaker_id": "1",
          "time": "20210903 09:12"
        },
        {
          "id": "MDRW2100029456.1.15",
          "form": "걱정이에요",
          "original_form": "걱정이에요",
          "speaker_id": "1",
          "time": "20210903 09:12"
        },
        {
          "id": "MDRW2100029456.1.16",
          "form": "나이도 작지않은데 혹시나 헤어지면 ㅜㅜ 노처녀 각이에요 ㅋㅋㅋ",
          "original_form": "나이도 작지않은데 혹시나 헤어지면 ㅜㅜ 노처녀 각이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:12"
        },
        {
          "id": "MDRW2100029456.1.17",
          "form": "지금 나이가 어떻게 되시죠?",
          "original_form": "지금 나이가 어떻게 되시죠?",
          "speaker_id": "2",
          "time": "20210903 09:13"
        },
        {
          "id": "MDRW2100029456.1.18",
          "form": "지금 33살이용",
          "original_form": "지금 33살이용",
          "speaker_id": "1",
          "time": "20210903 09:13"
        },
        {
          "id": "MDRW2100029456.1.19",
          "form": "전 20살에 cc로만나서 28살까지 만나던 첫사랑이랑 헤어지고",
          "original_form": "전 20살에 cc로만나서 28살까지 만나던 첫사랑이랑 헤어지고",
          "speaker_id": "2",
          "time": "20210903 09:13"
        },
        {
          "id": "MDRW2100029456.1.20",
          "form": "32에 결혼했어요",
          "original_form": "32에 결혼했어요",
          "speaker_id": "2",
          "time": "20210903 09:14"
        },
        {
          "id": "MDRW2100029456.1.21",
          "form": "헐 8년이나 만나셨는데",
          "original_form": "헐 8년이나 만나셨는데",
          "speaker_id": "1",
          "time": "20210903 09:14"
        },
        {
          "id": "MDRW2100029456.1.22",
          "form": "지금 결혼4년차됐으니까",
          "original_form": "지금 결혼4년차됐으니까",
          "speaker_id": "2",
          "time": "20210903 09:14"
        },
        {
          "id": "MDRW2100029456.1.23",
          "form": "그랬군요ㅠㅠ",
          "original_form": "그랬군요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 09:14"
        },
        {
          "id": "MDRW2100029456.1.24",
          "form": "연애한기간이랑 합쳐서 거의 8년되어가네요",
          "original_form": "연애한기간이랑 합쳐서 거의 8년되어가네요",
          "speaker_id": "2",
          "time": "20210903 09:14"
        }
      ]
    }
