      "utterance": [
        {
          "id": "MDRW2100014505.1.1",
          "form": "공연뭐좋아하세요",
          "original_form": "공연뭐좋아하세요",
          "speaker_id": "1",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014505.1.2",
          "form": "최근에 앙리할아버지와나 공연 재밌게 봤어여",
          "original_form": "최근에 앙리할아버지와나 공연 재밌게 봤어여",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014505.1.3",
          "form": "공연 따로 좋아하는거는 없어요 ㅋㅋ",
          "original_form": "공연 따로 좋아하는거는 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014505.1.4",
          "form": "공연 최근에 보러가신적 있으세요????",
          "original_form": "공연 최근에 보러가신적 있으세요????",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014505.1.5",
          "form": "아니요 ㅠㅡㅜ 제일 마지막이 지킬앤하이드요",
          "original_form": "아니요 ㅠㅡㅜ 제일 마지막이 지킬앤하이드요",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014505.1.6",
          "form": "오 지킬앤하이드",
          "original_form": "오 지킬앤하이드",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014505.1.7",
          "form": "지금이순간~",
          "original_form": "지금이순간~",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014505.1.8",
          "form": "2019년이네요",
          "original_form": "2019년이네요",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014505.1.9",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014505.1.10",
          "form": "전 올해봤어요...... ㅋㅋ",
          "original_form": "전 올해봤어요...... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014505.1.11",
          "form": "누구꺼요?",
          "original_form": "누구꺼요?",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014505.1.12",
          "form": "채수빈 나오는거요!",
          "original_form": "채수빈 나오는거요!",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014505.1.13",
          "form": "아 채수빈 연기못하지않아요?",
          "original_form": "아 채수빈 연기못하지않아요?",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014505.1.14",
          "form": "그래요????",
          "original_form": "그래요????",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014505.1.15",
          "form": "괜찮았어요?",
          "original_form": "괜찮았어요?",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014505.1.16",
          "form": "네 괜찮았어여 ㅋㅋ",
          "original_form": "네 괜찮았어여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014505.1.17",
          "form": "재밌게 보고옴",
          "original_form": "재밌게 보고옴",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014505.1.18",
          "form": "여주보단 할아버지들 연기짬밥이 엄청나니 ㅋㅋ",
          "original_form": "여주보단 할아버지들 연기짬밥이 엄청나니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014505.1.19",
          "form": "아하 할아버지들 그죠",
          "original_form": "아하 할아버지들 그죠",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014505.1.20",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014505.1.21",
          "form": "지킬앤하이드는 누구 나오는거 보셨어영??",
          "original_form": "지킬앤하이드는 누구 나오는거 보셨어영??",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014505.1.22",
          "form": "조승우요ㅋ",
          "original_form": "조승우요ㅋ",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014505.1.23",
          "form": "역시 지킬앤하이드는 조승우죠",
          "original_form": "역시 지킬앤하이드는 조승우죠",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014505.1.24",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014505.1.25",
          "form": "이거 하려고 두달전에 예매대기걸어놓은",
          "original_form": "이거 하려고 두달전에 예매대기걸어놓은",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014505.1.26",
          "form": "ㅋㅋㅋㅋ 조승우꺼 보려면 그 정도 노력은 인정이지",
          "original_form": "ㅋㅋㅋㅋ 조승우꺼 보려면 그 정도 노력은 인정이지",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014505.1.27",
          "form": "ㅋㅋㅋㅋ그라서 혼자 보러간",
          "original_form": "ㅋㅋㅋㅋ그라서 혼자 보러간",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014505.1.28",
          "form": "헐,,,  혼자서;",
          "original_form": "헐,,,  혼자서;",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014505.1.29",
          "form": "공연은 혼자죠",
          "original_form": "공연은 혼자죠",
          "speaker_id": "1",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014505.1.30",
          "form": "혼자가서 즐기는것도 좋죠ㅋㅋ",
          "original_form": "혼자가서 즐기는것도 좋죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014505.1.31",
          "form": "혼자가면 공연에만 집중할 수 있잖아여",
          "original_form": "혼자가면 공연에만 집중할 수 있잖아여",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014505.1.32",
          "form": "ㅋㅋㅋㅋ완죤 집중",
          "original_form": "ㅋㅋㅋㅋ완죤 집중",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014505.1.33",
          "form": "그렇죠!",
          "original_form": "그렇죠!",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014505.1.34",
          "form": "백신 맞고 휴유증 없으셨어요????",
          "original_form": "백신 맞고 휴유증 없으셨어요????",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014505.1.35",
          "form": "저 벌써 백신기운 올라오는듯;;;",
          "original_form": "저 벌써 백신기운 올라오는듯;;;",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014505.1.36",
          "form": "아스트라는 1차아파요ㅜ에효",
          "original_form": "아스트라는 1차아파요ㅜ에효",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014505.1.37",
          "form": "언능 자요",
          "original_form": "언능 자요",
          "speaker_id": "1",
          "time": "20210825 21:34"
        }
      ]
