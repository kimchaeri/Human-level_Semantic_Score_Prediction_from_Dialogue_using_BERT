      "utterance": [
        {
          "id": "MDRW2100020260.1.1",
          "form": "학교 졸업한지 오래됬어요. ㅋㅋ",
          "original_form": "학교 졸업한지 오래됬어요. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:10"
        },
        {
          "id": "MDRW2100020260.1.2",
          "form": "저도 졸업한지는 꽤 됐어요 ㅋㅋ",
          "original_form": "저도 졸업한지는 꽤 됐어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020260.1.3",
          "form": "졸업하니까 시험스트레스없는거 하난 좋은거같아요",
          "original_form": "졸업하니까 시험스트레스없는거 하난 좋은거같아요",
          "speaker_id": "2",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020260.1.4",
          "form": "맞아요. 그게 제일 싫었죠 ㅋㅋ",
          "original_form": "맞아요. 그게 제일 싫었죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020260.1.5",
          "form": "무슨전공이셨어요?? ㅎㅎ",
          "original_form": "무슨전공이셨어요?? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020260.1.6",
          "form": "전 국어국문학이요. name2은요?",
          "original_form": "전 국어국문학이요. &name2&은요?",
          "speaker_id": "1",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020260.1.7",
          "form": "전 심리학전공이였어요 ㅋㅋ",
          "original_form": "전 심리학전공이였어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020260.1.8",
          "form": "졸업하니 무쓸모 ㅜㅜㅜㅜㅜ",
          "original_form": "졸업하니 무쓸모 ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020260.1.9",
          "form": "전공 살리기가 진짜 어려운 것 같아요 ㅠ",
          "original_form": "전공 살리기가 진짜 어려운 것 같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020260.1.10",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020260.1.11",
          "form": "공대를갔어야했는데",
          "original_form": "공대를갔어야했는데",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020260.1.12",
          "form": "저는 학점따는것너무 어려워서",
          "original_form": "저는 학점따는것너무 어려워서",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020260.1.13",
          "form": "졸업도 겨우 했어요",
          "original_form": "졸업도 겨우 했어요",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020260.1.14",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020260.1.15",
          "form": "ㅋㅋ 저도 그 생각 해봤지만 전 이과쪽으로는 영 아니어서",
          "original_form": "ㅋㅋ 저도 그 생각 해봤지만 전 이과쪽으로는 영 아니어서",
          "speaker_id": "1",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020260.1.16",
          "form": "그건 인정... 수포자였어요",
          "original_form": "그건 인정... 수포자였어요",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020260.1.17",
          "form": "수학도 과학도 너무 어려웠어요ㅋㅋ",
          "original_form": "수학도 과학도 너무 어려웠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020260.1.18",
          "form": "국어도 어렵지않나요??ㅠㅋㅋ",
          "original_form": "국어도 어렵지않나요??ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020260.1.19",
          "form": "제일 쉬어보이지만 고득점받긴 어려웠던것같아요 언어영역.........",
          "original_form": "제일 쉬어보이지만 고득점받긴 어려웠던것같아요 언어영역.........",
