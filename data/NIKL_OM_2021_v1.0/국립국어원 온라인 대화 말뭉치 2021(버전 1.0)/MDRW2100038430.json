      "utterance": [
        {
          "id": "MDRW2100038430.1.1",
          "form": "직장 다니시나요",
          "original_form": "직장 다니시나요",
          "speaker_id": "2",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100038430.1.2",
          "form": "아니용:)\r\n지금은 주부입니다",
          "original_form": "아니용:)\r\n지금은 주부입니다",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038430.1.3",
          "form": "직장인이세요?",
          "original_form": "직장인이세요?",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038430.1.4",
          "form": "저는 대학생이에요",
          "original_form": "저는 대학생이에요",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100038430.1.5",
          "form": "아 그럼 알바는 해보셨어요?",
          "original_form": "아 그럼 알바는 해보셨어요?",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100038430.1.6",
          "form": "알바는 못해봤고 친구들 알바하는건 자주 봤어요",
          "original_form": "알바는 못해봤고 친구들 알바하는건 자주 봤어요",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100038430.1.7",
          "form": "요즘엔 키오스크 때문에 사람을 직접 상대 안하더라구요",
          "original_form": "요즘엔 키오스크 때문에 사람을 직접 상대 안하더라구요",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100038430.1.8",
          "form": "ㅇㅏ 그럼 주로 무슨 알바를 해요?",
          "original_form": "ㅇㅏ 그럼 주로 무슨 알바를 해요?",
          "speaker_id": "1",
          "time": "20210902 14:39"
        },
        {
          "id": "MDRW2100038430.1.9",
          "form": "여자애들은 프랜차이즈 음식점에서 많이 일하고",
          "original_form": "여자애들은 프랜차이즈 음식점에서 많이 일하고",
          "speaker_id": "2",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100038430.1.10",
          "form": "남자애들은 상하차 다니더라구요",
          "original_form": "남자애들은 상하차 다니더라구요",
          "speaker_id": "2",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100038430.1.11",
          "form": "코시국이라\r\n알바도 제약이 많겠네여ㅠ",
          "original_form": "코시국이라\r\n알바도 제약이 많겠네여ㅠ",
          "speaker_id": "1",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100038430.1.12",
          "form": "많이 안뽑는다고 하더라구요",
          "original_form": "많이 안뽑는다고 하더라구요",
          "speaker_id": "2",
          "time": "20210902 14:48"
        },
        {
          "id": "MDRW2100038430.1.13",
          "form": "그렇겠네요",
          "original_form": "그렇겠네요",
          "speaker_id": "1",
          "time": "20210902 14:59"
        },
        {
          "id": "MDRW2100038430.1.14",
          "form": "그래도 하는 애들은 꼭 합니다ㅋㅋ",
          "original_form": "그래도 하는 애들은 꼭 합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100038430.1.15",
          "form": "ㅋㅋ좋은 사장님들만\r\n만났으면 좋겠네용",
          "original_form": "ㅋㅋ좋은 사장님들만\r\n만났으면 좋겠네용",
          "speaker_id": "1",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100038430.1.16",
          "form": "그러게나 말이에요",
          "original_form": "그러게나 말이에요",
          "speaker_id": "2",
          "time": "20210902 15:34"
        }
