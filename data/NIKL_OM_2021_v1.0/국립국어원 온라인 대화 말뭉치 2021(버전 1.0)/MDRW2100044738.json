      "utterance": [
        {
          "id": "MDRW2100044738.1.1",
          "form": "요리는 잘 하시나요?",
          "original_form": "요리는 잘 하시나요?",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044738.1.2",
          "form": "저 요리 못해요ㅠㅠ",
          "original_form": "저 요리 못해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044738.1.3",
          "form": "요리 잘하시나요?",
          "original_form": "요리 잘하시나요?",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044738.1.4",
          "form": "저는 파스타만 해봤어요",
          "original_form": "저는 파스타만 해봤어요",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044738.1.5",
          "form": "소스가 잘 나와서 쉽더라구요",
          "original_form": "소스가 잘 나와서 쉽더라구요",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044738.1.6",
          "form": "오 토마토소스로 하셨나요?",
          "original_form": "오 토마토소스로 하셨나요?",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044738.1.7",
          "form": "토마토 소스가 가장 기본인 것 같아요!",
          "original_form": "토마토 소스가 가장 기본인 것 같아요!",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044738.1.8",
          "form": "크림은 가족들이 별로 안좋아해요ㅠㅠ",
          "original_form": "크림은 가족들이 별로 안좋아해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044738.1.9",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044738.1.10",
          "form": "저희도 별로 안좋아하더라구요",
          "original_form": "저희도 별로 안좋아하더라구요",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044738.1.11",
          "form": "투움바도 소스가 나왔길래",
          "original_form": "투움바도 소스가 나왔길래",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044738.1.12",
          "form": "시도해보려고 사다놨어요",
          "original_form": "시도해보려고 사다놨어요",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044738.1.13",
          "form": "헐 맛있을거같아요",
          "original_form": "헐 맛있을거같아요",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044738.1.14",
          "form": "투움바 좋아하세요?",
          "original_form": "투움바 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044738.1.15",
          "form": "네 아웃백 가면 무조건 투움바 먹습니다",
          "original_form": "네 아웃백 가면 무조건 투움바 먹습니다",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044738.1.16",
          "form": "좋아하는 다른 메뉴 있으신가요?",
          "original_form": "좋아하는 다른 메뉴 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044738.1.17",
          "form": "아웃백 오지치즈후라이 좋아해요",
          "original_form": "아웃백 오지치즈후라이 좋아해요",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044738.1.18",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044738.1.19",
          "form": "감자튀김에 치즈 올라간거 맞죠?",
          "original_form": "감자튀김에 치즈 올라간거 맞죠?",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044738.1.20",
          "form": "맥주 안주로 딱이에요!",
          "original_form": "맥주 안주로 딱이에요!",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044738.1.21",
          "form": "오 맥주랑 딱이겠네요",
          "original_form": "오 맥주랑 딱이겠네요",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044738.1.22",
          "form": "나중에 먹어봐야겠어요",
          "original_form": "나중에 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044738.1.23",
          "form": "마라탕 드셔보셨나요?",
          "original_form": "마라탕 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044738.1.24",
