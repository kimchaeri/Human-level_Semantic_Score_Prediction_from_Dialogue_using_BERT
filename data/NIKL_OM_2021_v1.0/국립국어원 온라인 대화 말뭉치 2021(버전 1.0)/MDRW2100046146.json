      "utterance": [
        {
          "id": "MDRW2100046146.1.1",
          "form": "좋아하는 음식 있으신가여?",
          "original_form": "좋아하는 음식 있으신가여?",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046146.1.2",
          "form": "네 오늘은 쌀국수가 먹고힢어요!!",
          "original_form": "네 오늘은 쌀국수가 먹고힢어요!!",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046146.1.3",
          "form": "오옹 저도 쌀국수 참 좋아해여",
          "original_form": "오옹 저도 쌀국수 참 좋아해여",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046146.1.4",
          "form": "그 에머이?? 거기가 유독 맛있더라구여 ㅎㅎ",
          "original_form": "그 에머이?? 거기가 유독 맛있더라구여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046146.1.5",
          "form": "아 에머이 맛있어요!",
          "original_form": "아 에머이 맛있어요!",
          "speaker_id": "2",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046146.1.6",
          "form": "오능 점심 어떤거 드시나요?",
          "original_form": "오능 점심 어떤거 드시나요?",
          "speaker_id": "2",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046146.1.7",
          "form": "오늘 점심 떡볶이 생각하구 잇어용",
          "original_form": "오늘 점심 떡볶이 생각하구 잇어용",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046146.1.8",
          "form": "헐 맛있겠어요!",
          "original_form": "헐 맛있겠어요!",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046146.1.9",
          "form": "저는 떡갈비요!",
          "original_form": "저는 떡갈비요!",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046146.1.10",
          "form": "떡갈비! 당연히 사서 드시는거겠죠??",
          "original_form": "떡갈비! 당연히 사서 드시는거겠죠??",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046146.1.11",
          "form": "떡갈비 맛있져",
          "original_form": "떡갈비 맛있져",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046146.1.12",
          "form": "네! 도시락 배달입니당..!",
          "original_form": "네! 도시락 배달입니당..!",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046146.1.13",
          "form": "오옹 도시락 배달 해서 드시는군여 매일 하는건가요?",
          "original_form": "오옹 도시락 배달 해서 드시는군여 매일 하는건가요?",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046146.1.14",
          "form": "아뇨! 오늘운 특별히 배달이요",
          "original_form": "아뇨! 오늘운 특별히 배달이요",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046146.1.15",
          "form": "사무싷에서 시켜주는 날만 이렇게 먹어요!",
          "original_form": "사무싷에서 시켜주는 날만 이렇게 먹어요!",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046146.1.16",
          "form": "오옹 좋은 날이네요!! 특별한날 ㅋㅋ",
          "original_form": "오옹 좋은 날이네요!! 특별한날 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046146.1.17",
          "form": "맞아요 식비도 아끼고 맛있고 좋아요!",
          "original_form": "맞아요 식비도 아끼고 맛있고 좋아요!",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046146.1.18",
          "form": "어디 떡볶이 드세요?",
          "original_form": "어디 떡볶이 드세요?",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046146.1.19",
          "form": "음 저 어제 먹다 남은 떡볶이가 있어요 ㅋㅋ",
          "original_form": "음 저 어제 먹다 남은 떡볶이가 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046146.1.20",
          "form": "그거 먹으려고요!",
          "original_form": "그거 먹으려고요!",
          "speaker_id": "1",
          "time": "20210908 11:14"
        }
