      "utterance": [
        {
          "id": "MDRW2100028196.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028196.1.2",
          "form": "자취하는지라",
          "original_form": "자취하는지라",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028196.1.3",
          "form": "필연적 숙명 같은거죠",
          "original_form": "필연적 숙명 같은거죠",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028196.1.4",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028196.1.5",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028196.1.6",
          "form": "아 그러면",
          "original_form": "아 그러면",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028196.1.7",
          "form": "여럿이서",
          "original_form": "여럿이서",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028196.1.8",
          "form": "업무 분담을 하시나 봐요",
          "original_form": "업무 분담을 하시나 봐요",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028196.1.9",
          "form": "네 가족끼리",
          "original_form": "네 가족끼리",
          "speaker_id": "1",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028196.1.10",
          "form": "로봇청소기를 마련해서",
          "original_form": "로봇청소기를 마련해서",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028196.1.11",
          "form": "업무를 넘겨주는건",
          "original_form": "업무를 넘겨주는건",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028196.1.12",
          "form": "어때요",
          "original_form": "어때요",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028196.1.13",
          "form": "4차 산업 혁명 시기잖아요",
          "original_form": "4차 산업 혁명 시기잖아요",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028196.1.14",
          "form": "화장실청소가 가장 힘들죠",
          "original_form": "화장실청소가 가장 힘들죠",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028196.1.15",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100028196.1.16",
          "form": "화장실 청소는",
          "original_form": "화장실 청소는",
          "speaker_id": "2",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100028196.1.17",
          "form": "로봇도 못하죠...",
          "original_form": "로봇도 못하죠...",
          "speaker_id": "2",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100028196.1.18",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100028196.1.19",
          "form": "확실히",
          "original_form": "확실히",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028196.1.20",
          "form": "건강에 안 좋을 거 같은",
          "original_form": "건강에 안 좋을 거 같은",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028196.1.21",
          "form": "냄새기는 하죠",
          "original_form": "냄새기는 하죠",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028196.1.22",
          "form": "머리도아프고",
          "original_form": "머리도아프고",
          "speaker_id": "1",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100028196.1.23",
          "form": "근데 또 락스를 쓸 때와 안 쓸 때가 차이가 나다보니",
          "original_form": "근데 또 락스를 쓸 때와 안 쓸 때가 차이가 나다보니",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100028196.1.24",
          "form": "안 쓸 수도 없구요...",
          "original_form": "안 쓸 수도 없구요...",
          "speaker_id": "2",
          "time": "20210902 18:51"
