        },
        {
          "id": "MDRW2100039333.1.3",
          "form": "점심 드겼나여",
          "original_form": "점심 드겼나여",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100039333.1.4",
          "form": "저 피자먹었숨돵ㅋㅋ",
          "original_form": "저 피자먹었숨돵ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100039333.1.5",
          "form": "와우 어디 피자 드셨어요?",
          "original_form": "와우 어디 피자 드셨어요?",
          "speaker_id": "1",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100039333.1.6",
          "form": "저는 지금 장트러블 ㅜㅜㅋㅋㅋㅋ",
          "original_form": "저는 지금 장트러블 ㅜㅜㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100039333.1.7",
          "form": "반올임피자욤",
          "original_form": "반올임피자욤",
          "speaker_id": "2",
          "time": "20210905 15:03"
        },
        {
          "id": "MDRW2100039333.1.8",
          "form": "갑자기 장트러블은 왜  ㅠ",
          "original_form": "갑자기 장트러블은 왜  ㅠ",
          "speaker_id": "2",
          "time": "20210905 15:04"
        },
        {
          "id": "MDRW2100039333.1.9",
          "form": "오 반올림",
          "original_form": "오 반올림",
          "speaker_id": "1",
          "time": "20210905 16:17"
        },
        {
          "id": "MDRW2100039333.1.10",
          "form": "맛있었음? ㅋㅋㅋㅋ",
          "original_form": "맛있었음? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100039333.1.11",
          "form": "장트러블은 왜 온지 모르겠음",
          "original_form": "장트러블은 왜 온지 모르겠음",
          "speaker_id": "1",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100039333.1.12",
          "form": "웅 나름 괜찮드라궁",
          "original_form": "웅 나름 괜찮드라궁",
          "speaker_id": "2",
          "time": "20210905 16:26"
        },
        {
          "id": "MDRW2100039333.1.13",
          "form": "음식이 뭐가 잘못됐낭..",
          "original_form": "음식이 뭐가 잘못됐낭..",
          "speaker_id": "2",
          "time": "20210905 16:26"
        },
        {
          "id": "MDRW2100039333.1.14",
          "form": "모르겠음 ㅜㅜ",
          "original_form": "모르겠음 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100039333.1.15",
          "form": "님 맥날\r\n창녕갈릭버거 드셔보셨어염?ㅋㅋ",
          "original_form": "님 맥날\r\n창녕갈릭버거 드셔보셨어염?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100039333.1.16",
          "form": "창녕??",
          "original_form": "창녕??",
          "speaker_id": "2",
          "time": "20210905 17:56"
        },
        {
          "id": "MDRW2100039333.1.17",
          "form": "갈릭버거?",
          "original_form": "갈릭버거?",
          "speaker_id": "2",
          "time": "20210905 17:56"
        },
        {
          "id": "MDRW2100039333.1.18",
          "form": "새로나온건가유??",
          "original_form": "새로나온건가유??",
          "speaker_id": "2",
          "time": "20210905 17:56"
        },
        {
          "id": "MDRW2100039333.1.19",
          "form": "넹ㅋㅋ\r\n한정기간 판매라\r\n먹어봤는데 괜찮네염",
          "original_form": "넹ㅋㅋ\r\n한정기간 판매라\r\n먹어봤는데 괜찮네염{emoji:♡}",
          "speaker_id": "1",
          "time": "20210905 19:05"
        },
        {
          "id": "MDRW2100039333.1.20",
          "form": "헝...영상봤는데",
          "original_form": "헝...영상봤는데",
          "speaker_id": "2",
          "time": "20210905 19:05"
        },
        {
          "id": "MDRW2100039333.1.21",
          "form": "먹어보고싶당 ㅠㅠㅠ",
          "original_form": "먹어보고싶당 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 19:05"
        },
        {
          "id": "MDRW2100039333.1.22",
          "form": "올마안남았네 기간",
          "original_form": "올마안남았네 기간",
          "speaker_id": "2",
          "time": "20210905 19:05"
        },
        {
          "id": "MDRW2100039333.1.23",
          "form": "웅ㅋㅋ 이제 곧 끝남",
          "original_form": "웅ㅋㅋ 이제 곧 끝남",
          "speaker_id": "1",
          "time": "20210905 19:05"
        },
        {
          "id": "MDRW2100039333.1.24",
          "form": "근데 입에 마늘 냄새 장난 아님ㅋㅋ",
          "original_form": "근데 입에 마늘 냄새 장난 아님ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:06"
        },
        {
          "id": "MDRW2100039333.1.25",
          "form": "점심 드셨어영?",
          "original_form": "점심 드셨어영?",
          "speaker_id": "1",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100039333.1.26",
          "form": "오잉;; 왜심심이 또 안떴딩 ㅜ",
          "original_form": "오잉;; 왜심심이 또 안떴딩 ㅜ",
          "speaker_id": "2",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100039333.1.27",
          "form": "방금 저녁 일찍먹었아여",
          "original_form": "방금 저녁 일찍먹었아여",
          "speaker_id": "2",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100039333.1.28",
          "form": "6시도 안되었는데 저녁을요? ㅋㅋㅋ",
          "original_form": "6시도 안되었는데 저녁을요? ㅋㅋㅋ",
