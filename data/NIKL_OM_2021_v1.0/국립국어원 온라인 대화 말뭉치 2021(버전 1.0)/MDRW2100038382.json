      "utterance": [
        {
          "id": "MDRW2100038382.1.1",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100038382.1.2",
          "form": "지금 먹고있는 중이랍니다 ㅎㅎㅎ",
          "original_form": "지금 먹고있는 중이랍니다 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100038382.1.3",
          "form": "잡곡밥에 김... 그리고 두부... 초고추장....",
          "original_form": "잡곡밥에 김... 그리고 두부... 초고추장....",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100038382.1.4",
          "form": "name2님은요?!",
          "original_form": "&name2&님은요?!",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100038382.1.5",
          "form": "저는 바빠서 컵라면 먹었어요",
          "original_form": "저는 바빠서 컵라면 먹었어요",
          "speaker_id": "2",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100038382.1.6",
          "form": "아이고 바쁘시구나",
          "original_form": "아이고 바쁘시구나",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100038382.1.7",
          "form": "건강하고 든든하게 드셨네요!",
          "original_form": "건강하고 든든하게 드셨네요!",
          "speaker_id": "2",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100038382.1.8",
          "form": "네네,, 어쩌다보니 건강식인데 사실",
          "original_form": "네네,, 어쩌다보니 건강식인데 사실",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100038382.1.9",
          "form": "이거 먹기 전에는",
          "original_form": "이거 먹기 전에는",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100038382.1.10",
          "form": "식빵에다가....",
          "original_form": "식빵에다가....",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100038382.1.11",
          "form": "로투스 잼을 발라먹었어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "로투스 잼을 발라먹었어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100038382.1.12",
          "form": "앜ㅋㄲㅋㅋㅋ",
          "original_form": "앜ㅋㄲㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100038382.1.13",
          "form": "맛있었겠어요",
          "original_form": "맛있었겠어요",
          "speaker_id": "2",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100038382.1.14",
          "form": "컵라면 드시는편이세요?",
          "original_form": "컵라면 드시는편이세요?",
          "speaker_id": "2",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100038382.1.15",
          "form": "네네 저 라면 진짜 좋아해요!!!!",
          "original_form": "네네 저 라면 진짜 좋아해요!!!!",
          "speaker_id": "1",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100038382.1.16",
          "form": "건강 생각해서 참는거지",
          "original_form": "건강 생각해서 참는거지",
          "speaker_id": "1",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100038382.1.17",
          "form": "만약 마음껏 먹을 수 있다면",
          "original_form": "만약 마음껏 먹을 수 있다면",
          "speaker_id": "1",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100038382.1.18",
          "form": "이틀에 한 번은 먹었을거예요 ㅋㅋㅋㅋ",
          "original_form": "이틀에 한 번은 먹었을거예요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100038382.1.19",
          "form": "앜ㅋㅋㄲㄱㄱ저두 라면 좋아해요",
          "original_form": "앜ㅋㅋㄲㄱㄱ저두 라면 좋아해요",
          "speaker_id": "2",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100038382.1.20",
          "form": "매일먹은 때도 있었네요",
          "original_form": "매일먹은 때도 있었네요",
          "speaker_id": "2",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100038382.1.21",
          "form": "컵라면 어떤거 좋아하세요?",
          "original_form": "컵라면 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100038382.1.22",
          "form": "일단 제가 뽑는 베스트는",
          "original_form": "일단 제가 뽑는 베스트는",
          "speaker_id": "1",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100038382.1.23",
          "form": "신라면 블랙....",
          "original_form": "신라면 블랙....",
          "speaker_id": "1",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100038382.1.24",
          "form": "name2님은요?!",
          "original_form": "&name2&님은요?!",
          "speaker_id": "1",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100038382.1.25",
          "form": "오....신라면 블랙 봉지로는 먹어봤는데",
          "original_form": "오....신라면 블랙 봉지로는 먹어봤는데",
          "speaker_id": "2",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100038382.1.26",
          "form": "든든한 느낌이라 좋았어요",
          "original_form": "든든한 느낌이라 좋았어요",
          "speaker_id": "2",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100038382.1.27",
          "form": "오오 그리고 신라면블랙 두부김치도 나왔죠!!!",
          "original_form": "오오 그리고 신라면블랙 두부김치도 나왔죠!!!",
          "speaker_id": "1",
          "time": "20210902 12:31"
        },
        {
          "id": "MDRW2100038382.1.28",
          "form": "뭔가 밥하고 먹기 좋은듯한",
          "original_form": "뭔가 밥하고 먹기 좋은듯한",
          "speaker_id": "2",
          "time": "20210902 12:31"
        },
        {
          "id": "MDRW2100038382.1.29",
          "form": "한식느낌가득한 라면이였어요",
          "original_form": "한식느낌가득한 라면이였어요",
          "speaker_id": "2",
          "time": "20210902 12:31"
        },
        {
          "id": "MDRW2100038382.1.30",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:32"
        },
        {
