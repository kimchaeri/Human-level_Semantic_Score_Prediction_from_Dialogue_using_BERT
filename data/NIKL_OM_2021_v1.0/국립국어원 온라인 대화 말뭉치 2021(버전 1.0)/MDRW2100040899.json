        },
        {
          "id": "MDRW2100040899.1.2",
          "form": "기억 조작 노래죠",
          "original_form": "기억 조작 노래죠",
          "speaker_id": "1",
          "time": "20210905 11:45"
        },
        {
          "id": "MDRW2100040899.1.3",
          "form": "그니까요 ㅠ ! 그거랑 다이너마이트 아세요??!!",
          "original_form": "그니까요 ㅠ ! 그거랑 다이너마이트 아세요??!!",
          "speaker_id": "2",
          "time": "20210905 12:23"
        },
        {
          "id": "MDRW2100040899.1.4",
          "form": "너무 ㅠㅠ 좋아요!",
          "original_form": "너무 ㅠㅠ 좋아요!",
          "speaker_id": "2",
          "time": "20210905 12:23"
        },
        {
          "id": "MDRW2100040899.1.5",
          "form": "ㄹㅇ 스엠 비트 그자체",
          "original_form": "ㄹㅇ 스엠 비트 그자체",
          "speaker_id": "2",
          "time": "20210905 12:23"
        },
        {
          "id": "MDRW2100040899.1.6",
          "form": "히 당연히 알죠~~~!!!!",
          "original_form": "히 당연히 알죠~~~!!!!",
          "speaker_id": "1",
          "time": "20210905 12:25"
        },
        {
          "id": "MDRW2100040899.1.7",
          "form": "그래서 저는",
          "original_form": "그래서 저는",
          "speaker_id": "1",
          "time": "20210905 12:25"
        },
        {
          "id": "MDRW2100040899.1.8",
          "form": "방탄 다이너마이트보다 샤이니 다이너마이트가",
          "original_form": "방탄 다이너마이트보다 샤이니 다이너마이트가",
          "speaker_id": "1",
          "time": "20210905 12:25"
        },
        {
          "id": "MDRW2100040899.1.9",
          "form": "제 온리원 다이너마이트랍니다 ㅎ_ㅎ",
          "original_form": "제 온리원 다이너마이트랍니다 ㅎ_ㅎ",
          "speaker_id": "1",
          "time": "20210905 12:25"
        },
        {
          "id": "MDRW2100040899.1.10",
          "form": "아 당근이죠!!! ㅎ.ㅎ !!!!!! 전 솔로곡들도 너무 좋아요 ㅠ!",
          "original_form": "아 당근이죠!!! ㅎ.ㅎ !!!!!! 전 솔로곡들도 너무 좋아요 ㅠ!",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040899.1.11",
          "form": "태민 괴도 ㅠㅠ 특히@!",
          "original_form": "태민 괴도 ㅠㅠ 특히@!",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040899.1.12",
          "form": "헉 맞아요!!!!",
          "original_form": "헉 맞아요!!!!",
          "speaker_id": "1",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040899.1.13",
          "form": "괴도 너무 좋고 그리고",
          "original_form": "괴도 너무 좋고 그리고",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040899.1.14",
          "form": "저는 drip drop이 최애예요 !!!",
          "original_form": "저는 drip drop이 최애예요 !!!",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040899.1.15",
          "form": "가사도 멋있고 안무도... 뮤비도...",
          "original_form": "가사도 멋있고 안무도... 뮤비도...",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040899.1.16",
          "form": "헐헐 ㅠ !!! 맞아요!!! 저 그거랑 프레스 유어 넘버?",
          "original_form": "헐헐 ㅠ !!! 맞아요!!! 저 그거랑 프레스 유어 넘버?",
          "speaker_id": "2",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100040899.1.17",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "1",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100040899.1.18",
          "form": "그것도 안무 너무 좋아해요",
          "original_form": "그것도 안무 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100040899.1.19",
          "form": "완전 왕자님 느낌",
          "original_form": "완전 왕자님 느낌",
          "speaker_id": "1",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100040899.1.20",
          "form": "앜ㅋㅋ 제가 너무 답장이 빠르죠;;; 하핫",
          "original_form": "앜ㅋㅋ 제가 너무 답장이 빠르죠;;; 하핫",
          "speaker_id": "1",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100040899.1.21",
          "form": "엇 아니에요 ㅎㅎㅎㅎ",
          "original_form": "엇 아니에요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:20"
        },
        {
          "id": "MDRW2100040899.1.22",
          "form": "전 약간 무브 이런 것도 좋은데 괴도 컨셉도 ㅠ!",
          "original_form": "전 약간 무브 이런 것도 좋은데 괴도 컨셉도 ㅠ!",
          "speaker_id": "2",
          "time": "20210905 13:20"
        },
        {
          "id": "MDRW2100040899.1.23",
          "form": "한 번 더 해줬음 좋겠어요",
          "original_form": "한 번 더 해줬음 좋겠어요",
          "speaker_id": "2",
          "time": "20210905 13:20"
        },
        {
          "id": "MDRW2100040899.1.24",
          "form": "앗 맞아요 무브도 진짜 대박이죠",
          "original_form": "앗 맞아요 무브도 진짜 대박이죠",
          "speaker_id": "1",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100040899.1.25",
          "form": "그런건 태민만 할 수 있는 느낌이에요!!!",
          "original_form": "그런건 태민만 할 수 있는 느낌이에요!!!",
          "speaker_id": "1",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100040899.1.26",
          "form": "그니까요! 전 원트도 너무 좋아요 ㅍ!",
          "original_form": "그니까요! 전 원트도 너무 좋아요 ㅍ!",
          "speaker_id": "2",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100040899.1.27",
          "form": "앗 맞아요",
          "original_form": "앗 맞아요",
          "speaker_id": "1",
          "time": "20210905 13:23"
        },
        {
          "id": "MDRW2100040899.1.28",
          "form": "샤이니가 정말",
          "original_form": "샤이니가 정말",
          "speaker_id": "1",
          "time": "20210905 13:23"
        },
        {
          "id": "MDRW2100040899.1.29",
          "form": "그 의상이랑 장갑도 진짜 ㅠ.^",
          "original_form": "그 의상이랑 장갑도 진짜 ㅠ.^",
          "speaker_id": "2",
          "time": "20210905 13:23"
        },
        {
          "id": "MDRW2100040899.1.30",
