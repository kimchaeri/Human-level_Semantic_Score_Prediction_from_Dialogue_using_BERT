      "utterance": [
        {
          "id": "MDRW2100019475.1.1",
          "form": "그쵸ㅋㅋ",
          "original_form": "그쵸ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019475.1.2",
          "form": "배고파요",
          "original_form": "배고파요",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019475.1.3",
          "form": "저녁드세요",
          "original_form": "저녁드세요",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019475.1.4",
          "form": "배달시킬까 고민중이에요",
          "original_form": "배달시킬까 고민중이에요",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019475.1.5",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019475.1.6",
          "form": "디저트류가 먹고싶네요",
          "original_form": "디저트류가 먹고싶네요",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019475.1.7",
          "form": "샌드위치?",
          "original_form": "샌드위치?",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019475.1.8",
          "form": "아뇨 뚱카롱이요ㅋㅋ",
          "original_form": "아뇨 뚱카롱이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019475.1.9",
          "form": "아 뚱카롱 이 아프지않나요ㅋㅋ",
          "original_form": "아 뚱카롱 이 아프지않나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019475.1.10",
          "form": "먹고싶어요",
          "original_form": "먹고싶어요",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019475.1.11",
          "form": "그럼 먹어야해요",
          "original_form": "그럼 먹어야해요",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019475.1.12",
          "form": "그럴까요",
          "original_form": "그럴까요",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019475.1.13",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019475.1.14",
          "form": "고민좀만해보고 시켜야겠어요",
          "original_form": "고민좀만해보고 시켜야겠어요",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019475.1.15",
          "form": "드시길 바랄게요",
          "original_form": "드시길 바랄게요",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019475.1.16",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019475.1.17",
          "form": "네ㅎㅎ 시리얼 먹었어요",
          "original_form": "네ㅎㅎ 시리얼 먹었어요",
          "speaker_id": "2",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019475.1.18",
          "form": "진짜 마카롱 먹고싶어요",
          "original_form": "진짜 마카롱 먹고싶어요",
