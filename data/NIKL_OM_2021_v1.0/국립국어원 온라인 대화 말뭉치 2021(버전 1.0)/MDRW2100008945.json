      "utterance": [
        {
          "id": "MDRW2100008945.1.1",
          "form": "아 왜 내일 월요일이지?",
          "original_form": "아 왜 내일 월요일이지?",
          "speaker_id": "2",
          "time": "20210822 19:52"
        },
        {
          "id": "MDRW2100008945.1.2",
          "form": "왜 출근이지?",
          "original_form": "왜 출근이지?",
          "speaker_id": "2",
          "time": "20210822 19:53"
        },
        {
          "id": "MDRW2100008945.1.3",
          "form": "주말 순삭",
          "original_form": "주말 순삭",
          "speaker_id": "1",
          "time": "20210822 19:53"
        },
        {
          "id": "MDRW2100008945.1.4",
          "form": "내일 수업이라 과제해야하는데",
          "original_form": "내일 수업이라 과제해야하는데",
          "speaker_id": "1",
          "time": "20210822 19:53"
        },
        {
          "id": "MDRW2100008945.1.5",
          "form": "토요일 일해서그런가 주말 금방..",
          "original_form": "토요일 일해서그런가 주말 금방..",
          "speaker_id": "2",
          "time": "20210822 19:53"
        },
        {
          "id": "MDRW2100008945.1.6",
          "form": "밤샐듯^^",
          "original_form": "밤샐듯^^",
          "speaker_id": "1",
          "time": "20210822 19:53"
        },
        {
          "id": "MDRW2100008945.1.7",
          "form": "너는 하루쉬는거잖아",
          "original_form": "너는 하루쉬는거잖아",
          "speaker_id": "1",
          "time": "20210822 19:53"
        },
        {
          "id": "MDRW2100008945.1.8",
          "form": "아 출근할생각에 짜증나는군^^",
          "original_form": "아 출근할생각에 짜증나는군^^",
          "speaker_id": "2",
          "time": "20210822 19:53"
        },
        {
          "id": "MDRW2100008945.1.9",
          "form": "아휴 힘들겠다 진짜",
          "original_form": "아휴 힘들겠다 진짜",
          "speaker_id": "1",
          "time": "20210822 19:53"
        },
        {
          "id": "MDRW2100008945.1.10",
          "form": "너는 과제가 많아??",
          "original_form": "너는 과제가 많아??",
          "speaker_id": "2",
          "time": "20210822 19:54"
        },
        {
          "id": "MDRW2100008945.1.11",
          "form": "많은건 아닌데 걍 내가 하기싫어서 미룸",
          "original_form": "많은건 아닌데 걍 내가 하기싫어서 미룸",
          "speaker_id": "1",
          "time": "20210822 19:54"
        },
        {
          "id": "MDRW2100008945.1.12",
          "form": "진짜 열심히 해야지 하는데 못해ㅋㅋㅋ",
          "original_form": "진짜 열심히 해야지 하는데 못해ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:54"
        },
        {
          "id": "MDRW2100008945.1.13",
          "form": "너는 상사가 짜증나게 하는건 없어?",
          "original_form": "너는 상사가 짜증나게 하는건 없어?",
          "speaker_id": "1",
          "time": "20210822 19:54"
        },
        {
          "id": "MDRW2100008945.1.14",
          "form": "웅ㅋㅋㅋ상사는 좋아",
          "original_form": "웅ㅋㅋㅋ상사는 좋아",
          "speaker_id": "2",
          "time": "20210822 19:54"
        },
        {
          "id": "MDRW2100008945.1.15",
          "form": "실수하거나 그래도 딱 그때만 단호하고 나중에 또 말한다던가 그런건없음",
          "original_form": "실수하거나 그래도 딱 그때만 단호하고 나중에 또 말한다던가 그런건없음",
          "speaker_id": "2",
          "time": "20210822 19:55"
        },
        {
          "id": "MDRW2100008945.1.16",
          "form": "와 진짜 다행이다 내친구는 상사 한명 때문에 힘들어하는중",
          "original_form": "와 진짜 다행이다 내친구는 상사 한명 때문에 힘들어하는중",
          "speaker_id": "1",
          "time": "20210822 19:56"
        },
        {
          "id": "MDRW2100008945.1.17",
          "form": "와 그러면 진짜 힘들지..",
          "original_form": "와 그러면 진짜 힘들지..",
          "speaker_id": "2",
          "time": "20210822 19:57"
        },
        {
          "id": "MDRW2100008945.1.18",
          "form": "상사가 힘들게하면 어휴",
          "original_form": "상사가 힘들게하면 어휴",
          "speaker_id": "2",
          "time": "20210822 19:57"
        },
        {
          "id": "MDRW2100008945.1.19",
          "form": "그 상사가 굳이 그렇게 할필요없는데 열정맨 스타일이라",
          "original_form": "그 상사가 굳이 그렇게 할필요없는데 열정맨 스타일이라",
          "speaker_id": "1",
          "time": "20210822 20:00"
        },
        {
          "id": "MDRW2100008945.1.20",
          "form": "새벽3시까지 막 야근한다는겨",
          "original_form": "새벽3시까지 막 야근한다는겨",
          "speaker_id": "1",
