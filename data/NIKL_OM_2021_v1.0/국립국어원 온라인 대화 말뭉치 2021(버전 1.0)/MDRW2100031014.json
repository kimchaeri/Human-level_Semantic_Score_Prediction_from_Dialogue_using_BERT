      "utterance": [
        {
          "id": "MDRW2100031014.1.1",
          "form": "업무힘드신가요",
          "original_form": "업무힘드신가요",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031014.1.2",
          "form": "안 힘들어요 집중이 안 됨",
          "original_form": "안 힘들어요 집중이 안 됨",
          "speaker_id": "1",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100031014.1.3",
          "form": "직장동료는 괜찮나요",
          "original_form": "직장동료는 괜찮나요",
          "speaker_id": "2",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100031014.1.4",
          "form": "넵 스트레스 없어요",
          "original_form": "넵 스트레스 없어요",
          "speaker_id": "1",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100031014.1.5",
          "form": "저두 괜찮아요",
          "original_form": "저두 괜찮아요",
          "speaker_id": "2",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100031014.1.6",
          "form": "맞다 최근에 재미잇는일이 있었어요\r\n이전 근무지에 있던 상사가 갑질로 인사이동을 했어요ㅋㅋ",
          "original_form": "맞다 최근에 재미잇는일이 있었어요\r\n이전 근무지에 있던 상사가 갑질로 인사이동을 했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100031014.1.7",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100031014.1.8",
          "form": "어떤 갑질이요",
          "original_form": "어떤 갑질이요",
          "speaker_id": "2",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100031014.1.9",
          "form": "음 글쎄요 언어폭력이겠죠",
          "original_form": "음 글쎄요 언어폭력이겠죠",
          "speaker_id": "1",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100031014.1.10",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100031014.1.11",
          "form": "조심해야해요",
          "original_form": "조심해야해요",
          "speaker_id": "2",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100031014.1.12",
          "form": "좀심하긴햇어요",
          "original_form": "좀심하긴햇어요",
          "speaker_id": "1",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100031014.1.13",
          "form": "두밤자면 출근이네요ㅜ",
          "original_form": "두밤자면 출근이네요ㅜ",
          "speaker_id": "2",
          "time": "20210904 19:59"
        },
        {
          "id": "MDRW2100031014.1.14",
          "form": "아직많이남았네요",
          "original_form": "아직많이남았네요",
          "speaker_id": "1",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031014.1.15",
          "form": "흑 출근ㅠㅠ",
          "original_form": "흑 출근ㅠㅠ",
