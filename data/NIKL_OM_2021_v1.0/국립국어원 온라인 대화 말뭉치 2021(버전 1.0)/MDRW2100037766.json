      "utterance": [
        {
          "id": "MDRW2100037766.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 18:04"
        },
        {
          "id": "MDRW2100037766.1.2",
          "form": "그 곳 날씨는 어떤가요?",
          "original_form": "그 곳 날씨는 어떤가요?",
          "speaker_id": "1",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100037766.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100037766.1.4",
          "form": "습하고 덥습니다! 비가 오려다 마네요",
          "original_form": "습하고 덥습니다! 비가 오려다 마네요",
          "speaker_id": "2",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100037766.1.5",
          "form": "날씨 괜찮나요?",
          "original_form": "날씨 괜찮나요?",
          "speaker_id": "2",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100037766.1.6",
          "form": "여기는 어제 저녁부터 계속 비가 쏟아지다 지금은 잠시 잦아들었어요",
          "original_form": "여기는 어제 저녁부터 계속 비가 쏟아지다 지금은 잠시 잦아들었어요",
          "speaker_id": "1",
          "time": "20210901 18:06"
        },
        {
          "id": "MDRW2100037766.1.7",
          "form": "어떤 날씨 좋아하세요?",
          "original_form": "어떤 날씨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 18:06"
        },
        {
          "id": "MDRW2100037766.1.8",
          "form": "저는 계절로 보면 가을 날씨요\r\n봄 날씨도 좋아하구요",
          "original_form": "저는 계절로 보면 가을 날씨요\r\n봄 날씨도 좋아하구요",
          "speaker_id": "1",
          "time": "20210901 18:06"
        },
        {
          "id": "MDRW2100037766.1.9",
          "form": "봄 가을이 짧아져서 아쉽겠어요!",
          "original_form": "봄 가을이 짧아져서 아쉽겠어요!",
          "speaker_id": "2",
          "time": "20210901 18:07"
        },
        {
          "id": "MDRW2100037766.1.10",
          "form": "네 너무 아쉬워요ㅠㅠ",
          "original_form": "네 너무 아쉬워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037766.1.11",
          "form": "name2님은 어떤 계절 좋아하세요?",
          "original_form": "&name2&님은 어떤 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037766.1.12",
          "form": "저는 겨울 좋아해요!",
          "original_form": "저는 겨울 좋아해요!",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037766.1.13",
          "form": "겨울을 기다리고 있습니다 ㅎㅎ",
          "original_form": "겨울을 기다리고 있습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:11"
        },
        {
          "id": "MDRW2100037766.1.14",
          "form": "겨울!! 눈오는걸 좋아하시는건가요?",
          "original_form": "겨울!! 눈오는걸 좋아하시는건가요?",
          "speaker_id": "1",
          "time": "20210901 18:13"
        },
        {
          "id": "MDRW2100037766.1.15",
          "form": "눈도 좋아하는데 차가운 공기를 좋아해요 ㅎㅎ",
          "original_form": "눈도 좋아하는데 차가운 공기를 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:13"
        },
        {
          "id": "MDRW2100037766.1.16",
          "form": "이불 말고있어도 안 혼나는 계절이라 좋아하기도 해요 ㅎㅎ",
          "original_form": "이불 말고있어도 안 혼나는 계절이라 좋아하기도 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:13"
        },
        {
          "id": "MDRW2100037766.1.17",
          "form": "아 공감가네요!  그쵸 맞아요",
          "original_form": "아 공감가네요!  그쵸 맞아요",
          "speaker_id": "1",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100037766.1.18",
          "form": "다들 이불을 돌돌 감고 있으니 혼나지 않죠ㅎㅎ",
          "original_form": "다들 이불을 돌돌 감고 있으니 혼나지 않죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100037766.1.19",
          "form": "그쵸! 에어컨 키고 이불 덮고있는 그 기분을 내내 느낄 수 있는 계절이라 좋아요 ㅎㅎ",
          "original_form": "그쵸! 에어컨 키고 이불 덮고있는 그 기분을 내내 느낄 수 있는 계절이라 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100037766.1.20",
          "form": "비유 정말 잘하시네요",
          "original_form": "비유 정말 잘하시네요",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100037766.1.21",
          "form": "여름에 에어컨틀고 이불 덮고있을 때 너무 좋죠",
          "original_form": "여름에 에어컨틀고 이불 덮고있을 때 너무 좋죠",
