      "utterance": [
        {
          "id": "MDRW2100002893.1.1",
          "form": "그럼 천상의 맛일거에요",
          "original_form": "그럼 천상의 맛일거에요",
          "speaker_id": "2",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002893.1.2",
          "form": "언젠간 꼭 먹어볼꺼예요!",
          "original_form": "언젠간 꼭 먹어볼꺼예요!",
          "speaker_id": "1",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002893.1.3",
          "form": "치즈 쭈우우우욱 늘어나는",
          "original_form": "치즈 쭈우우우욱 늘어나는",
          "speaker_id": "1",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002893.1.4",
          "form": "거기에 냉모밀 곁들여도 시원하겠어우",
          "original_form": "거기에 냉모밀 곁들여도 시원하겠어우",
          "speaker_id": "1",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002893.1.5",
          "form": "냉모밀 맛있어요 ㅠ",
          "original_form": "냉모밀 맛있어요 ㅠ",
          "speaker_id": "2",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002893.1.6",
          "form": "집에서 사다 해먹어도 밖에서먹는 맛은 덜 나요",
          "original_form": "집에서 사다 해먹어도 밖에서먹는 맛은 덜 나요",
          "speaker_id": "2",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002893.1.7",
          "form": "ㅋㅋㅋ맞아요... 밖에서 파는건 진짜 파는 이유가 있더라구요",
          "original_form": "ㅋㅋㅋ맞아요... 밖에서 파는건 진짜 파는 이유가 있더라구요",
          "speaker_id": "1",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002893.1.8",
          "form": "그런 의미로 오늘은 배달음식... 치킨 시킬까 해요",
          "original_form": "그런 의미로 오늘은 배달음식... 치킨 시킬까 해요",
          "speaker_id": "2",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002893.1.9",
          "form": "미미면가라고 서울에 한군데 있는데 나중에 되면 드셔보세요",
          "original_form": "미미면가라고 서울에 한군데 있는데 나중에 되면 드셔보세요",
          "speaker_id": "1",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002893.1.10",
          "form": "앗!!!!어디치킨이요?",
          "original_form": "앗!!!!어디치킨이요?",
          "speaker_id": "1",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002893.1.11",
          "form": "밖에서 파는거 다 맛있어요ㅠㅎ",
          "original_form": "밖에서 파는거 다 맛있어요ㅠㅎ",
          "speaker_id": "2",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002893.1.12",
          "form": "지금 후보는 애들때문에 교촌\r\n우리는 불로만이나 BhC 매운걸로?",
          "original_form": "지금 후보는 애들때문에 교촌\r\n우리는 불로만이나 BhC 매운걸로?",
          "speaker_id": "2",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002893.1.13",
          "form": "저희집은 두군데서 잘 시켜요",
          "original_form": "저희집은 두군데서 잘 시켜요",
          "speaker_id": "2",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002893.1.14",
          "form": "저도 교촌 좋아해요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 교촌 좋아해요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002893.1.15",
          "form": "저는 거의 교촌 많이 먹는거같아요 레드윙으로요!",
          "original_form": "저는 거의 교촌 많이 먹는거같아요 레드윙으로요!",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002893.1.16",
          "form": "어휴 레드윙 ㅠ 기분나빠요 매워매워 입술아파요",
          "original_form": "어휴 레드윙 ㅠ 기분나빠요 매워매워 입술아파요",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002893.1.17",
          "form": "우리동네가 못하는것일수도 있어요",
          "original_form": "우리동네가 못하는것일수도 있어요",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002893.1.18",
          "form": "ㅠㅠㅠ앗.. 동네마다 다르긴하더라구요",
          "original_form": "ㅠㅠㅠ앗.. 동네마다 다르긴하더라구요",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002893.1.19",
          "form": "이러면서 또 열심히 먹어요 쓰읍하아 하면소",
          "original_form": "이러면서 또 열심히 먹어요 쓰읍하아 하면소",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002893.1.20",
          "form": "어쩔땐 저희 동네두 엄청 매울때가 있어요",
          "original_form": "어쩔땐 저희 동네두 엄청 매울때가 있어요",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002893.1.21",
          "form": "늘 한곳에서 주문하는데도",
          "original_form": "늘 한곳에서 주문하는데도",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002893.1.22",
          "form": "맵기가 다르더라구요!",
          "original_form": "맵기가 다르더라구요!",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002893.1.23",
          "form": "아 그런거에요? 그럼 다시 시켜볼까봐요",
          "original_form": "아 그런거에요? 그럼 다시 시켜볼까봐요",
          "speaker_id": "2",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002893.1.24",
          "form": "까맣기도 해서 짰거든요",
          "original_form": "까맣기도 해서 짰거든요",
          "speaker_id": "2",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002893.1.25",
          "form": "허니콤보랑 같이 ㅎ",
          "original_form": "허니콤보랑 같이 ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002893.1.26",
          "form": "ㅋㅋ허니콤보 좋죠!!! 전 그 웨지감자도 좋아해요",
          "original_form": "ㅋㅋ허니콤보 좋죠!!! 전 그 웨지감자도 좋아해요",
          "speaker_id": "1",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002893.1.27",
          "form": "하나씩 집어먹음 좋더라구요!",
          "original_form": "하나씩 집어먹음 좋더라구요!",
          "speaker_id": "1",
          "time": "20210820 16:26"
        },
        {
          "id": "MDRW2100002893.1.28",
          "form": "ㅋㅋㅋㅋ감자는 남겼다가 다음날 일어나서 집어먹...ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ감자는 남겼다가 다음날 일어나서 집어먹...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:26"
        },
        {
          "id": "MDRW2100002893.1.29",
          "form": "그 퍽퍽함을 즐기며 허기달래는 거에요ㅋㅋㅋ",
          "original_form": "그 퍽퍽함을 즐기며 허기달래는 거에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:27"
        },
        {
          "id": "MDRW2100002893.1.30",
          "form": "아 또 다른맛이져!히히 남은거 하나씩 집어먹으면서",
          "original_form": "아 또 다른맛이져!히히 남은거 하나씩 집어먹으면서",
          "speaker_id": "1",
          "time": "20210820 16:27"
        },
        {
          "id": "MDRW2100002893.1.31",
          "form": "냉장고 봐요 저는",
          "original_form": "냉장고 봐요 저는",
          "speaker_id": "1",
          "time": "20210820 16:27"
        },
        {
          "id": "MDRW2100002893.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ뭘먹지~~",
          "original_form": "ㅋㅋㅋㅋㅋㅋ뭘먹지~~",
          "speaker_id": "1",
          "time": "20210820 16:27"
        },
        {
          "id": "MDRW2100002893.1.33",
          "form": "자취해요?",
          "original_form": "자취해요?",
          "speaker_id": "2",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002893.1.34",
          "form": "아니요! 저는 가족이랑 같이살아요!ㅎ.ㅎ",
          "original_form": "아니요! 저는 가족이랑 같이살아요!ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210820 16:29"
        }
      ]
