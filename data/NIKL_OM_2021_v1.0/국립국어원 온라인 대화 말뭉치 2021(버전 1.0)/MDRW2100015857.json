      "utterance": [
        {
          "id": "MDRW2100015857.1.1",
          "form": "쇼핑?지금 내 쿠팡엔 재입고된 칭따오와 자동벨트와 염색약 두개가 나를 기다리고있어",
          "original_form": "쇼핑?지금 내 쿠팡엔 재입고된 칭따오와 자동벨트와 염색약 두개가 나를 기다리고있어",
          "speaker_id": "2",
          "time": "20210826 01:55"
        },
        {
          "id": "MDRW2100015857.1.2",
          "form": "ㅋㅋㅋㅋㅋ나도 쿠팡에서 많이 사",
          "original_form": "ㅋㅋㅋㅋㅋ나도 쿠팡에서 많이 사",
          "speaker_id": "1",
          "time": "20210826 01:57"
        },
        {
          "id": "MDRW2100015857.1.3",
          "form": "가장 최근에는 헤어 식초를 샀지",
          "original_form": "가장 최근에는 헤어 식초를 샀지",
          "speaker_id": "1",
          "time": "20210826 01:57"
        },
        {
          "id": "MDRW2100015857.1.4",
          "form": "나도 염색약 사고싶다",
          "original_form": "나도 염색약 사고싶다",
          "speaker_id": "1",
          "time": "20210826 01:57"
        },
        {
          "id": "MDRW2100015857.1.5",
          "form": "아니 헤어식초가 너무웃김 헤어식초는 무슨 역할임?",
          "original_form": "아니 헤어식초가 너무웃김 헤어식초는 무슨 역할임?",
          "speaker_id": "2",
          "time": "20210826 02:10"
        },
        {
          "id": "MDRW2100015857.1.6",
          "form": "두피시원하게해줘서",
          "original_form": "두피시원하게해줘서",
          "speaker_id": "1",
          "time": "20210826 10:23"
        },
        {
          "id": "MDRW2100015857.1.7",
          "form": "이틀째에도 머리가 안간지러워",
          "original_form": "이틀째에도 머리가 안간지러워",
          "speaker_id": "1",
          "time": "20210826 10:23"
        },
        {
          "id": "MDRW2100015857.1.8",
          "form": "ㅇ..오우... 두피를 시원하게...",
          "original_form": "ㅇ..오우... 두피를 시원하게...{emoji:✏}{emoji:✏}",
          "speaker_id": "2",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100015857.1.9",
          "form": "킹치만 너무 번거로울것같아",
          "original_form": "킹치만 너무 번거로울것같아",
          "speaker_id": "2",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100015857.1.10",
          "form": "아 그건 그래ㅠㅠ",
          "original_form": "아 그건 그래ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100015857.1.11",
          "form": "하지만 이틀에 한번이니까 용서하는고지~",
          "original_form": "하지만 이틀에 한번이니까 용서하는고지~",
          "speaker_id": "1",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100015857.1.12",
          "form": "아니 머리는 원래 이틀에 한번 감는거잖아",
          "original_form": "아니 머리는 원래 이틀에 한번 감는거잖아",
          "speaker_id": "2",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100015857.1.13",
          "form": "그건 사람마다 다르지ㅋㅋㅋㅋ",
          "original_form": "그건 사람마다 다르지ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100015857.1.14",
          "form": "난 지금 두피도 두피지만 헤어오일도 사야해",
          "original_form": "난 지금 두피도 두피지만 헤어오일도 사야해",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100015857.1.15",
          "form": "너는 헤어오일 써?? 탈색모는 관리가 필요하잖아",
          "original_form": "너는 헤어오일 써?? 탈색모는 관리가 필요하잖아",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100015857.1.16",
          "form": "아니 난지금 단발이라 선호ㄴㄴ",
          "original_form": "아니 난지금 단발이라 선호ㄴㄴ",
          "speaker_id": "2",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100015857.1.17",
          "form": "응ㅠㅠ 칭따오 언제 살거야??",
          "original_form": "응ㅠㅠ 칭따오 언제 살거야??",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100015857.1.18",
          "form": "몰라 장바구니에서 대기중이야 어차피 2주비대면이니까..",
          "original_form": "몰라 장바구니에서 대기중이야 어차피 2주비대면이니까..",
          "speaker_id": "2",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100015857.1.19",
          "form": "내 쿠팡에는 불닭볶음면 한 박스(16개입)이 결제를 기다리고 있어ㅋㅋㅋㅋ",
          "original_form": "내 쿠팡에는 불닭볶음면 한 박스(16개입)이 결제를 기다리고 있어ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100015857.1.20",
          "form": "name2는 불닭안좋아해서 불닭 잘먹는 너가 신기해",
          "original_form": "&name2&는 불닭안좋아해서 불닭 잘먹는 너가 신기해",
          "speaker_id": "2",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100015857.1.21",
          "form": "ㅋㅋㅋㅋ근데 최근에 열라면 한 박스사서 불닭은 좀 시간이 지나고 살거야",
          "original_form": "ㅋㅋㅋㅋ근데 최근에 열라면 한 박스사서 불닭은 좀 시간이 지나고 살거야",
          "speaker_id": "1",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100015857.1.22",
          "form": "엥 라면을 그렇게 자주먹어?",
          "original_form": "엥 라면을 그렇게 자주먹어?",
          "speaker_id": "2",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100015857.1.23",
          "form": "자취란 그런거지ㅠㅠ",
          "original_form": "자취란 그런거지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100015857.1.24",
          "form": "파리바게뜨 포장이 나름 건강한거 먹는거야ㅠㅠ",
          "original_form": "파리바게뜨 포장이 나름 건강한거 먹는거야ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100015857.1.25",
          "form": "너 본가 다시 들어간거 아니야? 안들어갈거야??",
          "original_form": "너 본가 다시 들어간거 아니야? 안들어갈거야??",
          "speaker_id": "2",
