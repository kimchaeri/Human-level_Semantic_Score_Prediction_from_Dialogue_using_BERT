      "utterance": [
        {
          "id": "MDRW2100041214.1.1",
          "form": "그쵸ㅎㅎㅎ",
          "original_form": "그쵸ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100041214.1.2",
          "form": "점심은 되게 잘 먹어요",
          "original_form": "점심은 되게 잘 먹어요",
          "speaker_id": "2",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100041214.1.3",
          "form": "주로 육류에 샐러드로!!",
          "original_form": "주로 육류에 샐러드로!!",
          "speaker_id": "2",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100041214.1.4",
          "form": "그러니까요 영양을 생각하면 한끼라도 든든하게 잘 먹어야죠",
          "original_form": "그러니까요 영양을 생각하면 한끼라도 든든하게 잘 먹어야죠",
          "speaker_id": "1",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100041214.1.5",
          "form": "그럼 거의 다이어트식하는 거네요",
          "original_form": "그럼 거의 다이어트식하는 거네요",
          "speaker_id": "1",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100041214.1.6",
          "form": "간헐적 단식 하고 있는 건가요?",
          "original_form": "간헐적 단식 하고 있는 건가요?",
          "speaker_id": "1",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100041214.1.7",
          "form": "ㅋㅋㅋ단식은 아니구",
          "original_form": "ㅋㅋㅋ단식은 아니구",
          "speaker_id": "2",
          "time": "20210905 21:19"
        },
        {
          "id": "MDRW2100041214.1.8",
          "form": "음 제가 체구가 작아서",
          "original_form": "음 제가 체구가 작아서",
          "speaker_id": "2",
          "time": "20210905 21:19"
        },
        {
          "id": "MDRW2100041214.1.9",
          "form": "많이 안 먹어도 돼요!!",
          "original_form": "많이 안 먹어도 돼요!!",
          "speaker_id": "2",
          "time": "20210905 21:19"
        },
        {
          "id": "MDRW2100041214.1.10",
          "form": "먹고 싶은 건 다 먹어요ㅎㅎ",
          "original_form": "먹고 싶은 건 다 먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:19"
        },
        {
          "id": "MDRW2100041214.1.11",
          "form": "아항 그렇군요",
          "original_form": "아항 그렇군요",
          "speaker_id": "1",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100041214.1.12",
          "form": "그럼 먹는 거에서 가리는 건 없는거에요?",
          "original_form": "그럼 먹는 거에서 가리는 건 없는거에요?",
          "speaker_id": "1",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100041214.1.13",
          "form": "그쵸? 브로콜리가 싫긴하네요ㅎ",
          "original_form": "그쵸? 브로콜리가 싫긴하네요ㅎ",
          "speaker_id": "2",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100041214.1.14",
          "form": "name1님은 싫어하는거 있으세요?",
          "original_form": "&name1&님은 싫어하는거 있으세요?",
          "speaker_id": "2",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100041214.1.15",
          "form": "저는 정말 안가리고 다 먹어요",
          "original_form": "저는 정말 안가리고 다 먹어요",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100041214.1.16",
          "form": "알러지 음식도 없고",
          "original_form": "알러지 음식도 없고",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100041214.1.17",
          "form": "다만 최근에 매운걸 먹으면 속이 안좋기 시작했어요",
          "original_form": "다만 최근에 매운걸 먹으면 속이 안좋기 시작했어요",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100041214.1.18",
          "form": "그래서 덜 맵게 먹기 시작했어요",
          "original_form": "그래서 덜 맵게 먹기 시작했어요",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100041214.1.19",
          "form": "아 매운 거!!",
          "original_form": "아 매운 거!!",
          "speaker_id": "2",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100041214.1.20",
          "form": "사실 저는 불닭도 엄청 좋아하는데",
          "original_form": "사실 저는 불닭도 엄청 좋아하는데",
          "speaker_id": "2",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100041214.1.21",
          "form": "좀 고민이에요 위장 건강을 위해선 줄이는게좋겠죠?",
          "original_form": "좀 고민이에요 위장 건강을 위해선 줄이는게좋겠죠?",
          "speaker_id": "2",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100041214.1.22",
          "form": "찜닭수준도 안받으시나요",
          "original_form": "찜닭수준도 안받으시나요",
          "speaker_id": "2",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100041214.1.23",
          "form": "그정도는 아직 괜찮아요",
          "original_form": "그정도는 아직 괜찮아요",
          "speaker_id": "1",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100041214.1.24",
          "form": "근데 원래는 진짜 맵게 잘 먹었었거든요 \r\n최고맵기선택할 정도로",
          "original_form": "근데 원래는 진짜 맵게 잘 먹었었거든요 \r\n최고맵기선택할 정도로",
          "speaker_id": "1",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041214.1.25",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100041214.1.26",
          "form": "저랑 비슷한 취향이셨네요!!!",
          "original_form": "저랑 비슷한 취향이셨네요!!!",
          "speaker_id": "2",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100041214.1.27",
          "form": "매운거최고인데 언젠가 못먹게될걸생각하면 슬퍼요ㅠ",
          "original_form": "매운거최고인데 언젠가 못먹게될걸생각하면 슬퍼요ㅠ",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100041214.1.28",
          "form": "그래요",
          "original_form": "그래요",
          "speaker_id": "1",
          "time": "20210906 06:48"
        },
        {
          "id": "MDRW2100041214.1.29",
          "form": "매운건 통각이라 그런지",
          "original_form": "매운건 통각이라 그런지",
          "speaker_id": "1",
          "time": "20210906 06:48"
        },
        {
          "id": "MDRW2100041214.1.30",
          "form": "잘 먹다가 나이드니까 먹기 힘드네요",
          "original_form": "잘 먹다가 나이드니까 먹기 힘드네요",
          "speaker_id": "1",
          "time": "20210906 06:48"
        },
        {
          "id": "MDRW2100041214.1.31",
          "form": "매운 음식은 사실",
          "original_form": "매운 음식은 사실",
          "speaker_id": "2",
          "time": "20210906 10:11"
        },
        {
          "id": "MDRW2100041214.1.32",
          "form": "적당히 먹는 게 좋죠!!",
          "original_form": "적당히 먹는 게 좋죠!!",
          "speaker_id": "2",
          "time": "20210906 10:11"
        },
        {
          "id": "MDRW2100041214.1.33",
          "form": "혹시 나이가...?",
          "original_form": "혹시 나이가...?",
          "speaker_id": "2",
          "time": "20210906 10:11"
        },
        {
          "id": "MDRW2100041214.1.34",
          "form": "환갑지났답니다",
          "original_form": "환갑지났답니다",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041214.1.35",
          "form": "오늘 아침은 토스트 먹었네요",
          "original_form": "오늘 아침은 토스트 먹었네요",
          "speaker_id": "1",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100041214.1.36",
          "form": "아침은 드셧나요?",
          "original_form": "아침은 드셧나요?",
          "speaker_id": "1",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100041214.1.37",
          "form": "저는 오늘 밤을 홀딱 셌답니다!!!",
          "original_form": "저는 오늘 밤을 홀딱 셌답니다!!!",
          "speaker_id": "2",
          "time": "20210907 10:34"
        },
        {
          "id": "MDRW2100041214.1.38",
          "form": "자소서 쓰느라요ㅋㅋㅋ쫄쫄 굶었네요",
          "original_form": "자소서 쓰느라요ㅋㅋㅋ쫄쫄 굶었네요",
          "speaker_id": "2",
          "time": "20210907 10:35"
        },
        {
          "id": "MDRW2100041214.1.39",
          "form": "고구마 먹으려구요",
          "original_form": "고구마 먹으려구요",
          "speaker_id": "2",
          "time": "20210907 10:35"
        },
        {
          "id": "MDRW2100041214.1.40",
          "form": "아이고.. 자소서.. 고생했네요",
          "original_form": "아이고.. 자소서.. 고생했네요",
