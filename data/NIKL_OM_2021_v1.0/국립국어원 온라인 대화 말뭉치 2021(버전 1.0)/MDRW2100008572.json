        },
        {
          "id": "MDRW2100008572.1.2",
          "form": "제방 청소나 설거지 정도는요",
          "original_form": "제방 청소나 설거지 정도는요",
          "speaker_id": "2",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008572.1.3",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008572.1.4",
          "form": "뭐든 매일 하는 게 귀찮은 것 같아요...",
