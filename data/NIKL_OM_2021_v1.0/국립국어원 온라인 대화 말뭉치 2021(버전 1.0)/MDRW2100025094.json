      "utterance": [
        {
          "id": "MDRW2100025094.1.1",
          "form": "덥죠?",
          "original_form": "덥죠?",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025094.1.2",
          "form": "추운데요ㅜㅜ",
          "original_form": "추운데요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025094.1.3",
          "form": "비안오나요?",
          "original_form": "비안오나요?",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025094.1.4",
          "form": "여긴 더워요",
          "original_form": "여긴 더워요",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025094.1.5",
          "form": "한국아니세요?",
          "original_form": "한국아니세요?",
          "speaker_id": "2",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025094.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100025094.1.7",
          "form": "어느 지역이세요 전 서울인데 보일러넣었어요ㅜ",
          "original_form": "어느 지역이세요 전 서울인데 보일러넣었어요ㅜ",
          "speaker_id": "2",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025094.1.8",
          "form": "대구여",
          "original_form": "대구여",
          "speaker_id": "1",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100025094.1.9",
          "form": "비안와요?",
          "original_form": "비안와요?",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100025094.1.10",
          "form": "여긴 18도예요ㅜ",
          "original_form": "여긴 18도예요ㅜ",
          "speaker_id": "2",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100025094.1.11",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210831 23:25"
        },
        {
          "id": "MDRW2100025094.1.12",
          "form": "여긴 습해여",
          "original_form": "여긴 습해여",
          "speaker_id": "1",
          "time": "20210831 23:25"
        },
        {
          "id": "MDRW2100025094.1.13",
          "form": "아... 습하면 덥긴하죠 역시 대구 이름값을 하는군요!",
          "original_form": "아... 습하면 덥긴하죠 역시 대구 이름값을 하는군요!",
          "speaker_id": "2",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100025094.1.14",
          "form": "너무요",
          "original_form": "너무요",
          "speaker_id": "1",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100025094.1.15",
          "form": "ㅜㅜ 좀만 더 지나면 소중한 가을이 올거예요!",
          "original_form": "ㅜㅜ 좀만 더 지나면 소중한 가을이 올거예요!",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025094.1.16",
          "form": "금방 겨울되죠",
          "original_form": "금방 겨울되죠",
          "speaker_id": "1",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100025094.1.17",
          "form": "넵ㅋㅋ 그럼 또 봄이 그립겠죠ㅎㅎ",
          "original_form": "넵ㅋㅋ 그럼 또 봄이 그립겠죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100025094.1.18",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025094.1.19",
          "form": "ㅋㅋ 내일은 해를 보고싶네요",
