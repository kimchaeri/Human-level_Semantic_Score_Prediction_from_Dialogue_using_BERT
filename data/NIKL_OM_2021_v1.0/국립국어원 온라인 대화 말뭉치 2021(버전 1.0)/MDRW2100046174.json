          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100046174.1.2",
          "form": "돈까스나",
          "original_form": "돈까스나",
          "speaker_id": "2",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100046174.1.3",
          "form": "고기",
          "original_form": "고기",
          "speaker_id": "2",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100046174.1.4",
          "form": "와 저두요!!@@ 소고기 와사비 살짝 해서 먹어요 ㅋㅋ",
          "original_form": "와 저두요!!@@ 소고기 와사비 살짝 해서 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100046174.1.5",
          "form": "와 와사비 매니아",
          "original_form": "와 와사비 매니아",
          "speaker_id": "2",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100046174.1.6",
          "form": "좋군요 ㅎㅎ",
          "original_form": "좋군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100046174.1.7",
          "form": "식성이 굉장히 비슷하네요 ㅎㅎ",
          "original_form": "식성이 굉장히 비슷하네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100046174.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ 고기에 와사비 너무 좋아여 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 고기에 와사비 너무 좋아여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100046174.1.9",
          "form": "네 진짜 맛있죠 ㅎㅎ",
          "original_form": "네 진짜 맛있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046174.1.10",
          "form": "소고기 잘구우세용??",
          "original_form": "소고기 잘구우세용??",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046174.1.11",
          "form": "스테이크에 와사비도 환상의 궁합이예요",
          "original_form": "스테이크에 와사비도 환상의 궁합이예요",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046174.1.12",
          "form": "저는 제가 구우면 질기드라구요 ㅋㅋ",
          "original_form": "저는 제가 구우면 질기드라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046174.1.13",
          "form": "아니오 ㅎㅎㅎㅎ",
          "original_form": "아니오 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046174.1.14",
          "form": "전 다 신랑이해서 ㅎㅎ",
          "original_form": "전 다 신랑이해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046174.1.15",
          "form": "저도 고기는 잘 못구워요",
          "original_form": "저도 고기는 잘 못구워요",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046174.1.16",
          "form": "직접 구워주는곳으로만 가요 ㅋㅋ",
          "original_form": "직접 구워주는곳으로만 가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046174.1.17",
          "form": "ㅎㅎㅎㅎㅎㅎㅇ 요새는 직접 구워주는곳도 많으니",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅇ 요새는 직접 구워주는곳도 많으니",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046174.1.18",
          "form": "오 신랑분은 잘 구워주세용?",
          "original_form": "오 신랑분은 잘 구워주세용?",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046174.1.19",
          "form": "네 신랑은 고기를 잘 구워요ㅎㅎ",
          "original_form": "네 신랑은 고기를 잘 구워요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046174.1.20",
          "form": "요리를 곧 잘하는 편이라",
          "original_form": "요리를 곧 잘하는 편이라",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046174.1.21",
