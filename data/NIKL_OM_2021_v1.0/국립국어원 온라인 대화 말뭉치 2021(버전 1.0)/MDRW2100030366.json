      "utterance": [
        {
          "id": "MDRW2100030366.1.1",
          "form": "서면점 애도 북촌 피만두 있다고 알고 있어요",
          "original_form": "서면점 애도 북촌 피만두 있다고 알고 있어요",
          "speaker_id": "2",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100030366.1.2",
          "form": "엄청난 맛집이네요",
          "original_form": "엄청난 맛집이네요",
          "speaker_id": "1",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100030366.1.3",
          "form": "예전에 찾아 본 거라 있는지 모르겠어요",
          "original_form": "예전에 찾아 본 거라 있는지 모르겠어요",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030366.1.4",
          "form": "전국에 체인이 있고",
          "original_form": "전국에 체인이 있고",
          "speaker_id": "1",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030366.1.5",
          "form": "그냥 다음에 남친이랑 가세요",
          "original_form": "그냥 다음에 남친이랑 가세요",
          "speaker_id": "1",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030366.1.6",
          "form": "정말 정말 맛있어요 감동의 맛입니다",
          "original_form": "정말 정말 맛있어요 감동의 맛입니다",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030366.1.7",
          "form": "맛있는 지점으로 가세요",
          "original_form": "맛있는 지점으로 가세요",
          "speaker_id": "1",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030366.1.8",
          "form": "냉면도 맛이 있고 열무국수도 맛있었어요",
          "original_form": "냉면도 맛이 있고 열무국수도 맛있었어요",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030366.1.9",
          "form": "저는 언제나 먹어 볼까요",
          "original_form": "저는 언제나 먹어 볼까요",
          "speaker_id": "1",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030366.1.10",
          "form": "제주도에는 없나요",
          "original_form": "제주도에는 없나요",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030366.1.11",
          "form": "제주도에도 있을지 모르겠네요",
          "original_form": "제주도에도 있을지 모르겠네요",
          "speaker_id": "1",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030366.1.12",
          "form": "검색해 볼게요 없을 거예요",
          "original_form": "검색해 볼게요 없을 거예요",
          "speaker_id": "1",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030366.1.13",
          "form": "북촌 피만두 검색해 보세요",
          "original_form": "북촌 피만두 검색해 보세요",
