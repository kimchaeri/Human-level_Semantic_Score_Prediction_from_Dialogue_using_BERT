      "utterance": [
        {
          "id": "MDRW2100041745.1.1",
          "form": "배거프네요 점심을빨리먹으니..",
          "original_form": "배거프네요 점심을빨리먹으니..",
          "speaker_id": "2",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100041745.1.2",
          "form": "그러실만합니다...",
          "original_form": "그러실만합니다...",
          "speaker_id": "1",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100041745.1.3",
          "form": "저녁 뭐드시나요",
          "original_form": "저녁 뭐드시나요",
          "speaker_id": "1",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100041745.1.4",
          "form": "아직 정하지못하였어요...",
          "original_form": "아직 정하지못하였어요...",
          "speaker_id": "2",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041745.1.5",
          "form": "저는 요즘 넘 먹어서 굶을까 고민이에요 ",
          "original_form": "저는 요즘 넘 먹어서 굶을까 고민이에요 ",
          "speaker_id": "1",
          "time": "20210906 15:31"
        },
        {
          "id": "MDRW2100041745.1.6",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041745.1.7",
          "form": "굶어서빼는건",
          "original_form": "굶어서빼는건",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041745.1.8",
          "form": "않젛아요",
          "original_form": "않젛아요",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041745.1.9",
          "form": "차라리 반만 먹는걸로 추천드려요 ㅠㅠ",
          "original_form": "차라리 반만 먹는걸로 추천드려요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041745.1.10",
          "form": "앜ㅋㅋㅋ 또 차려야해서 그게 더 귀찮아요 며칠만 조이면..다시 돌아갈테니...",
          "original_form": "앜ㅋㅋㅋ 또 차려야해서 그게 더 귀찮아요 며칠만 조이면..다시 돌아갈테니...",
          "speaker_id": "1",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100041745.1.11",
          "form": "안뒈여옹",
          "original_form": "안뒈여옹",
          "speaker_id": "2",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100041745.1.12",
          "form": "간단하게라두 드세요 ㅠㅠ",
          "original_form": "간단하게라두 드세요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100041745.1.13",
          "form": "앜ㅋㅋㅋ이렇게나마 신경써주시다니 감사합니다",
          "original_form": "앜ㅋㅋㅋ이렇게나마 신경써주시다니 감사합니다",
          "speaker_id": "1",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041745.1.14",
          "form": "낼 도시락은 뭔가요",
          "original_form": "낼 도시락은 뭔가요",
          "speaker_id": "1",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041745.1.15",
          "form": "매일 냥동 볶음밥 데펴서 가요",
          "original_form": "매일 냥동 볶음밥 데펴서 가요",
          "speaker_id": "2",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100041745.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100041745.1.17",
          "form": "편해성...ㅎ",
          "original_form": "편해성...ㅎ",
          "speaker_id": "2",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100041745.1.18",
          "form": "앜ㅋㅋㅋ 저도 오늘은 냉동햐놨던 아구찜볶음밥입니다",
          "original_form": "앜ㅋㅋㅋ 저도 오늘은 냉동햐놨던 아구찜볶음밥입니다",
          "speaker_id": "1",
          "time": "20210907 08:14"
        },
        {
          "id": "MDRW2100041745.1.19",
          "form": "맛있는거 먹메요",
          "original_form": "맛있는거 먹메요",
          "speaker_id": "2",
          "time": "20210907 08:16"
        },
        {
          "id": "MDRW2100041745.1.20",
          "form": "저오눌 출근안했어요",
          "original_form": "저오눌 출근안했어요",
          "speaker_id": "2",
          "time": "20210907 08:16"
        },
        {
          "id": "MDRW2100041745.1.21",
          "form": "헉 출근 안하셨어요??",
          "original_form": "헉 출근 안하셨어요??",
          "speaker_id": "1",
          "time": "20210907 09:03"
        },
        {
          "id": "MDRW2100041745.1.22",
          "form": "넹넹!!!",
          "original_form": "넹넹!!!",
          "speaker_id": "2",
          "time": "20210907 09:22"
        },
        {
          "id": "MDRW2100041745.1.23",
          "form": "부렵죠!",
          "original_form": "부렵죠!",
          "speaker_id": "2",
          "time": "20210907 09:22"
        },
        {
          "id": "MDRW2100041745.1.24",
          "form": "완전 부러워요 ㅋㅋ 맛있는 거 드셔요",
          "original_form": "완전 부러워요 ㅋㅋ 맛있는 거 드셔요",
