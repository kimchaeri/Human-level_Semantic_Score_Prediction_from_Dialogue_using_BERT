      "utterance": [
        {
          "id": "MDRW2100011468.1.1",
          "form": "결혼에 대해 어떻게 생각하세요?",
          "original_form": "결혼에 대해 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100011468.1.2",
          "form": "음 반반이요?",
          "original_form": "음 반반이요?",
          "speaker_id": "2",
          "time": "20210824 13:15"
        },
        {
          "id": "MDRW2100011468.1.3",
          "form": "결혼은 참 어려운것같네요ㅋㅋㅋ",
          "original_form": "결혼은 참 어려운것같네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100011468.1.4",
          "form": "ㅋㅋ가족이 생기니 좋긴한데요",
          "original_form": "ㅋㅋ가족이 생기니 좋긴한데요",
          "speaker_id": "2",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100011468.1.5",
          "form": "여자는 아무래도 할일이 많으니..",
          "original_form": "여자는 아무래도 할일이 많으니..",
          "speaker_id": "2",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100011468.1.6",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011468.1.7",
          "form": "저는 집안일이 너무싫어요",
          "original_form": "저는 집안일이 너무싫어요",
          "speaker_id": "1",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011468.1.8",
          "form": "ㅋㅋ저는 육아요",
          "original_form": "ㅋㅋ저는 육아요",
          "speaker_id": "2",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100011468.1.9",
          "form": "집안일은 괜찮은데 육아가 안맞아요ㅋㅋ",
          "original_form": "집안일은 괜찮은데 육아가 안맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100011468.1.10",
          "form": "오우",
          "original_form": "오우",
          "speaker_id": "1",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100011468.1.11",
          "form": "육아는 너무힘들것같아요",
          "original_form": "육아는 너무힘들것같아요",
          "speaker_id": "1",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100011468.1.12",
          "form": "쉽지않네요ㅠ",
          "original_form": "쉽지않네요ㅠ",
          "speaker_id": "2",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100011468.1.13",
          "form": "부모님께 매일 절해야할기분",
          "original_form": "부모님께 매일 절해야할기분",
          "speaker_id": "2",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100011468.1.14",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100011468.1.15",
          "form": "사실 육아땜에",
          "original_form": "사실 육아땜에",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100011468.1.16",
          "form": "딩크도 많더라고요",
          "original_form": "딩크도 많더라고요",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100011468.1.17",
          "form": "그쳐 키우면 좋긴해여",
          "original_form": "그쳐 키우면 좋긴해여",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100011468.1.18",
          "form": "근데 그만큼 힘들기도ㅋㅋ",
          "original_form": "근데 그만큼 힘들기도ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100011468.1.19",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100011468.1.20",
          "form": "사실 저도",
          "original_form": "사실 저도",
          "speaker_id": "1",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100011468.1.21",
          "form": "안낳고싶어요",
          "original_form": "안낳고싶어요",
          "speaker_id": "1",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100011468.1.22",
          "form": "저도 그랬어여ㅋ",
          "original_form": "저도 그랬어여ㅋ",
          "speaker_id": "2",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100011468.1.23",
          "form": "근데 신랑과 부모님들이 넘 원하셔서",
          "original_form": "근데 신랑과 부모님들이 넘 원하셔서",
          "speaker_id": "2",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100011468.1.24",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100011468.1.25",
          "form": "아무래도",
          "original_form": "아무래도",
          "speaker_id": "1",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100011468.1.26",
          "form": "그게 현실이긴하죠",
          "original_form": "그게 현실이긴하죠",
          "speaker_id": "1",
