      "utterance": [
        {
          "id": "MDRW2100002801.1.1",
          "form": "스포츠 좋아하시나요",
          "original_form": "스포츠 좋아하시나요",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002801.1.2",
          "form": "네 좋아해여",
          "original_form": "네 좋아해여",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002801.1.3",
          "form": "어떤 종목이요?",
          "original_form": "어떤 종목이요?",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002801.1.4",
          "form": "야구 좋아해요",
          "original_form": "야구 좋아해요",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002801.1.5",
          "form": "응원하는 팀이 있나요",
          "original_form": "응원하는 팀이 있나요",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002801.1.6",
          "form": "네 있어요",
          "original_form": "네 있어요",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002801.1.7",
          "form": "무슨 팀인데요",
          "original_form": "무슨 팀인데요",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002801.1.8",
          "form": "기아 타이거즈요",
          "original_form": "기아 타이거즈요",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002801.1.9",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002801.1.10",
          "form": "저는 그냥 그래요",
          "original_form": "저는 그냥 그래요",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002801.1.11",
          "form": "챙겨보지는 않네요",
          "original_form": "챙겨보지는 않네요",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002801.1.12",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002801.1.13",
          "form": "야구는 재미있나요?",
          "original_form": "야구는 재미있나요?",
          "speaker_id": "1",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002801.1.14",
          "form": "할 줄은 모르는데 보는 건 재밌어요",
          "original_form": "할 줄은 모르는데 보는 건 재밌어요",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002801.1.15",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002801.1.16",
          "form": "저도 기회되면 한번 봐야겠네요",
          "original_form": "저도 기회되면 한번 봐야겠네요",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002801.1.17",
          "form": "그러세요",
          "original_form": "그러세요",
          "speaker_id": "2",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002801.1.18",
          "form": "스트레스가 풀리는 기분이에요",
          "original_form": "스트레스가 풀리는 기분이에요",
          "speaker_id": "2",
          "time": "20210820 15:42"
