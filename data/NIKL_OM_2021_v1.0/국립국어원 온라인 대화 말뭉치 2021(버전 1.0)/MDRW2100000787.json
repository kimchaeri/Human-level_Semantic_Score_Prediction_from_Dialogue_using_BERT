      "utterance": [
        {
          "id": "MDRW2100000787.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100000787.1.2",
          "form": "네 당근 자주이용합니다ㅎㅎ",
          "original_form": "네 당근 자주이용합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100000787.1.3",
          "form": "님은여",
          "original_form": "님은여",
          "speaker_id": "2",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100000787.1.4",
          "form": "저도 당근",
          "original_form": "저도 당근",
          "speaker_id": "1",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100000787.1.5",
          "form": "당근에서 뭐뭐 구입해보셨어요",
          "original_form": "당근에서 뭐뭐 구입해보셨어요",
          "speaker_id": "2",
          "time": "20210821 13:32"
        },
        {
          "id": "MDRW2100000787.1.6",
          "form": "최그에 공기청정기ㅇ",
          "original_form": "최그에 공기청정기ㅇ",
          "speaker_id": "1",
          "time": "20210821 13:32"
        },
        {
          "id": "MDRW2100000787.1.7",
          "form": "오 공청기 득템하셨네요",
          "original_form": "오 공청기 득템하셨네요",
          "speaker_id": "2",
          "time": "20210821 13:33"
        },
        {
          "id": "MDRW2100000787.1.8",
          "form": "저는 아직 구입은 안해보고 팔기만해봤어요",
          "original_form": "저는 아직 구입은 안해보고 팔기만해봤어요",
          "speaker_id": "2",
          "time": "20210821 13:33"
        },
        {
          "id": "MDRW2100000787.1.9",
          "form": "팔기맘 해요",
          "original_form": "팔기맘 해요",
          "speaker_id": "1",
          "time": "20210821 13:33"
        },
        {
          "id": "MDRW2100000787.1.10",
          "form": "아 공청기판매하신거구나",
          "original_form": "아 공청기판매하신거구나",
          "speaker_id": "2",
          "time": "20210821 13:34"
        },
        {
          "id": "MDRW2100000787.1.11",
          "form": "가전제품은 인기많더라구여",
          "original_form": "가전제품은 인기많더라구여",
          "speaker_id": "2",
          "time": "20210821 13:34"
        },
        {
          "id": "MDRW2100000787.1.12",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210821 13:35"
        },
        {
          "id": "MDRW2100000787.1.13",
          "form": "저도 청소기 판매해봤어요 ㅋㅋ 올리자마자 채팅오더라구여",
          "original_form": "저도 청소기 판매해봤어요 ㅋㅋ 올리자마자 채팅오더라구여",
          "speaker_id": "2",
          "time": "20210821 13:36"
        },
        {
          "id": "MDRW2100000787.1.14",
          "form": "거상이시네요",
          "original_form": "거상이시네요",
          "speaker_id": "1",
          "time": "20210821 13:37"
        },
        {
          "id": "MDRW2100000787.1.15",
          "form": "아직 당근온도는 낮지만 파는재미가 쏠쏠하더라구여",
          "original_form": "아직 당근온도는 낮지만 파는재미가 쏠쏠하더라구여",
          "speaker_id": "2",
          "time": "20210821 13:37"
        },
        {
          "id": "MDRW2100000787.1.16",
          "form": "금방올라가요",
          "original_form": "금방올라가요",
          "speaker_id": "1",
          "time": "20210821 13:38"
        },
        {
          "id": "MDRW2100000787.1.17",
          "form": "오 제법 파셨나봐여",
          "original_form": "오 제법 파셨나봐여",
