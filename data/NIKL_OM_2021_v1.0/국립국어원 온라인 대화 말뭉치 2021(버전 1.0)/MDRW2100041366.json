      "utterance": [
        {
          "id": "MDRW2100041366.1.1",
          "form": "미용을 위해 일찍 자야겠어",
          "original_form": "미용을 위해 일찍 자야겠어",
          "speaker_id": "2",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100041366.1.2",
          "form": "내일 또 출근이얌 ㅠㅠ",
          "original_form": "내일 또 출근이얌 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100041366.1.3",
          "form": "웅웅잘자",
          "original_form": "웅웅잘자",
          "speaker_id": "1",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100041366.1.4",
          "form": "굿밤",
          "original_form": "굿밤",
          "speaker_id": "1",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100041366.1.5",
          "form": "응응 내일봐 !!!",
          "original_form": "응응 내일봐 !!!",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041366.1.6",
          "form": "이제 몇개야???",
          "original_form": "이제 몇개야???",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041366.1.7",
          "form": "응응!",
          "original_form": "응응!",
          "speaker_id": "1",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041366.1.8",
          "form": "이거까지만 하자 !!",
          "original_form": "이거까지만 하자 !!",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041366.1.9",
          "form": "앜ㅋㅋㄱㄴ좋아!",
          "original_form": "앜ㅋㅋㄱㄴ좋아!",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041366.1.10",
          "form": "다이어트해?",
          "original_form": "다이어트해?",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041366.1.11",
          "form": "아니 여름에 입맛이없어서 6키로빠졌어",
          "original_form": "아니 여름에 입맛이없어서 6키로빠졌어",
          "speaker_id": "2",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100041366.1.12",
          "form": "이제 다시 채우고있어 ㅋㅋ",
          "original_form": "이제 다시 채우고있어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100041366.1.13",
          "form": "와6키로?ㅠㅜ",
          "original_form": "와6키로?ㅠㅜ",
          "speaker_id": "1",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100041366.1.14",
          "form": "잘먹어야겠다",
          "original_form": "잘먹어야겠다",
          "speaker_id": "1",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100041366.1.15",
          "form": "그냥 확 빠졌어ㅠㅠ",
          "original_form": "그냥 확 빠졌어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100041366.1.16",
          "form": "근데원래 여름엔 빠지고 가을겨울엔 빵빵해져 ㅋㅋㅋ",
          "original_form": "근데원래 여름엔 빠지고 가을겨울엔 빵빵해져 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100041366.1.17",
          "form": "나도나도ㅋㅋㅋ",
          "original_form": "나도나도ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100041366.1.18",
          "form": "나는한2-3키로왔다갔다해",
          "original_form": "나는한2-3키로왔다갔다해",
          "speaker_id": "1",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100041366.1.19",
          "form": "나도 원랜 그랬는데",
          "original_form": "나도 원랜 그랬는데",
          "speaker_id": "2",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100041366.1.20",
          "form": "이번엔 좀 범위가 컸네",
          "original_form": "이번엔 좀 범위가 컸네",
          "speaker_id": "2",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100041366.1.21",
          "form": "배달음식 마니먹어서 마이 쪄가지구",
          "original_form": "배달음식 마니먹어서 마이 쪄가지구",
          "speaker_id": "2",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100041366.1.22",
          "form": "마니 빠졌나봐 ㅋㅋ",
          "original_form": "마니 빠졌나봐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:49"
        },
        {
