        },
        {
          "id": "MDRW2100007440.1.2",
          "form": "하나 더 깔까요 말까요",
          "original_form": "하나 더 깔까요 말까요",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007440.1.3",
          "form": "저라면 멈출 것 같아요",
          "original_form": "저라면 멈출 것 같아요",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007440.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007440.1.5",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007440.1.6",
          "form": "안주 뭐였어요?",
          "original_form": "안주 뭐였어요?",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007440.1.7",
          "form": "안주...",
          "original_form": "안주...",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007440.1.8",
          "form": "빵..?",
          "original_form": "빵..?",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007440.1.9",
          "form": "무슨 빵?",
          "original_form": "무슨 빵?",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007440.1.10",
          "form": "오징어먹물크림치즈빵이요",
          "original_form": "오징어먹물크림치즈빵이요",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007440.1.11",
          "form": "아까 저녁에는",
          "original_form": "아까 저녁에는",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007440.1.12",
          "form": "우유랑 먹엇는데",
          "original_form": "우유랑 먹엇는데",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007440.1.13",
          "form": "아까 맥주 한캔 뜯은게 잇어서",
          "original_form": "아까 맥주 한캔 뜯은게 잇어서",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007440.1.14",
          "form": "방금 그거먹엇어요",
          "original_form": "방금 그거먹엇어요",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007440.1.15",
          "form": "이름부터 맛있네요",
          "original_form": "이름부터 맛있네요",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007440.1.16",
          "form": "우유랑 먹으면 진짜 맛있겠다",
          "original_form": "우유랑 먹으면 진짜 맛있겠다",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007440.1.17",
          "form": "밥면빵떡 중 빵인가요?",
          "original_form": "밥면빵떡 중 빵인가요?",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007440.1.18",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007440.1.19",
          "form": "밥면///빵//떡인데",
          "original_form": "밥면///빵//떡인데",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007440.1.20",
          "form": "그런날잇잖아요",
          "original_form": "그런날잇잖아요",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007440.1.21",
          "form": "음식물 쓰레기가 안나왓으면 하는날",
          "original_form": "음식물 쓰레기가 안나왓으면 하는날",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007440.1.22",
          "form": "그런날에 빵 사먹어요",
          "original_form": "그런날에 빵 사먹어요",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007440.1.23",
          "form": "육회좋아하세요?",
          "original_form": "육회좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 19:19"
        },
        {
          "id": "MDRW2100007440.1.24",
          "form": "육회랑 육회연어중에 뭐 더 좋아하세요",
          "original_form": "육회랑 육회연어중에 뭐 더 좋아하세요",
          "speaker_id": "1",
          "time": "20210827 19:19"
        },
        {
          "id": "MDRW2100007440.1.25",
          "form": "아니면 육회알탕",
          "original_form": "아니면 육회알탕",
          "speaker_id": "1",
          "time": "20210827 19:20"
        },
        {
          "id": "MDRW2100007440.1.26",
          "form": "육회연어",
          "original_form": "육회연어",
