        {
          "id": "MDRW2100007440.1.28",
          "form": "좋아여",
          "original_form": "좋아여",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100007440.1.29",
          "form": "육회연어는 정말 맛있죠!",
          "original_form": "육회연어는 정말 맛있죠!",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100007440.1.30",
          "form": "배부르면 안 되니까 소맥해요",
          "original_form": "배부르면 안 되니까 소맥해요",
          "speaker_id": "2",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100007440.1.31",
          "form": "ㅋㅋㅋㅋㅋ^^좋아유",
          "original_form": "ㅋㅋㅋㅋㅋ^^좋아유",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100007440.1.32",
          "form": "저 오늘 올리브영에서 크리스탈 라이트라는",
          "original_form": "저 오늘 올리브영에서 크리스탈 라이트라는",
          "speaker_id": "2",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100007440.1.33",
          "form": "트위터에서 유명한 아이스티를 하나 샀거든요",
          "original_form": "트위터에서 유명한 아이스티를 하나 샀거든요",
          "speaker_id": "2",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100007440.1.34",
          "form": "유명한 건 좋은 거니까",
          "original_form": "유명한 건 좋은 거니까",
          "speaker_id": "2",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100007440.1.35",
          "form": "소주에 타먹어요",
          "original_form": "소주에 타먹어요",
          "speaker_id": "2",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100007440.1.36",
          "form": "아아이스티",
          "original_form": "아아이스티",
          "speaker_id": "1",
          "time": "20210827 19:42"
        },
        {
