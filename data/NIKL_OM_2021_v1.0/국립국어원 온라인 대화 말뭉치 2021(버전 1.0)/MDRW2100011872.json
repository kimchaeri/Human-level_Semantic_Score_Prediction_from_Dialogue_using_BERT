      "utterance": [
        {
          "id": "MDRW2100011872.1.1",
          "form": "뭐 줄여 부르던데",
          "original_form": "뭐 줄여 부르던데",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100011872.1.2",
          "form": "하나하나 장난 아닌것처럼 보이던데요",
          "original_form": "하나하나 장난 아닌것처럼 보이던데요",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100011872.1.3",
          "form": "부담이 장난 아닐것같아요",
          "original_form": "부담이 장난 아닐것같아요",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100011872.1.4",
          "form": "웨딩자가 붙으면",
          "original_form": "웨딩자가 붙으면",
          "speaker_id": "1",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011872.1.5",
          "form": "값이 뛰어요 ㅋㅋㅋㅋ",
          "original_form": "값이 뛰어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011872.1.6",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011872.1.7",
          "form": "진짜 장난아니네요",
          "original_form": "진짜 장난아니네요",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011872.1.8",
          "form": "아 신부 혼자서는 잘 걷지도 못하는거",
          "original_form": "아 신부 혼자서는 잘 걷지도 못하는거",
          "speaker_id": "2",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100011872.1.9",
          "form": "아 그거 진짜",
          "original_form": "아 그거 진짜",
          "speaker_id": "1",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100011872.1.10",
          "form": "무슨신부 팔아넘기는것도아니고",
          "original_form": "무슨신부 팔아넘기는것도아니고",
          "speaker_id": "1",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100011872.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100011872.1.12",
          "form": "걷는것도 되게 불안불안해보이던데",
          "original_form": "걷는것도 되게 불안불안해보이던데",
          "speaker_id": "2",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100011872.1.13",
          "form": "넘어질까봐 걱정도되고",
          "original_form": "넘어질까봐 걱정도되고",
          "speaker_id": "2",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100011872.1.14",
          "form": "신부를 결혼식에 전시하고",
          "original_form": "신부를 결혼식에 전시하고",
          "speaker_id": "1",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100011872.1.15",
          "form": "주인공은 신랑같아요",
          "original_form": "주인공은 신랑같아요",
          "speaker_id": "1",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100011872.1.16",
          "form": "이상하기도하고",
          "original_form": "이상하기도하고",
          "speaker_id": "2",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100011872.1.17",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100011872.1.18",
          "form": "뭔가 신부는 눈요기고",
          "original_form": "뭔가 신부는 눈요기고",
          "speaker_id": "2",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100011872.1.19",
          "form": "주체적인 주인공은 신랑느낌",
          "original_form": "주체적인 주인공은 신랑느낌",
          "speaker_id": "2",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100011872.1.20",
          "form": "웨딩 영상도",
          "original_form": "웨딩 영상도",
          "speaker_id": "1",
          "time": "20210824 16:57"
        },
        {
          "id": "MDRW2100011872.1.21",
          "form": "별거없는데 돈 받고 그래요",
          "original_form": "별거없는데 돈 받고 그래요",
          "speaker_id": "1",
          "time": "20210824 16:57"
        },
        {
          "id": "MDRW2100011872.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:57"
        },
        {
          "id": "MDRW2100011872.1.23",
          "form": "웨딩영상",
          "original_form": "웨딩영상",
          "speaker_id": "2",
          "time": "20210824 16:57"
        },
        {
          "id": "MDRW2100011872.1.24",
          "form": "다 추억이다면서",
          "original_form": "다 추억이다면서",
          "speaker_id": "2",
          "time": "20210824 16:57"
        },
        {
          "id": "MDRW2100011872.1.25",
          "form": "하는거겠죠",
          "original_form": "하는거겠죠",
          "speaker_id": "2",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100011872.1.26",
          "form": "진짜 하나하나 다 돈이네요",
          "original_form": "진짜 하나하나 다 돈이네요",
          "speaker_id": "2",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100011872.1.27",
          "form": "그 쇼를 하는데 엄청난 공을들엿으니..",
          "original_form": "그 쇼를 하는데 엄청난 공을들엿으니..",
          "speaker_id": "1",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100011872.1.28",
          "form": "기록은 해야겟는데\r\n사실 다 허레허식이고..",
          "original_form": "기록은 해야겟는데\r\n사실 다 허레허식이고..",
          "speaker_id": "1",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100011872.1.29",
          "form": "아 그렇네요 공을 그만큼 들었으니",
          "original_form": "아 그렇네요 공을 그만큼 들었으니",
          "speaker_id": "2",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100011872.1.30",
          "form": "그래서 결혼 생각하는 지인들보면",
          "original_form": "그래서 결혼 생각하는 지인들보면",
          "speaker_id": "2",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100011872.1.31",
          "form": "좀 착찹해요",
          "original_form": "좀 착찹해요",
          "speaker_id": "2",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100011872.1.32",
          "form": "결혼 생각하는 지인들 많으세요?",
          "original_form": "결혼 생각하는 지인들 많으세요?",
          "speaker_id": "1",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100011872.1.33",
          "form": "그래도 뭐 다들 자기 인생이니까",
          "original_form": "그래도 뭐 다들 자기 인생이니까",
          "speaker_id": "2",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100011872.1.34",
          "form": "네 꽤 있던데요",
          "original_form": "네 꽤 있던데요",
          "speaker_id": "2",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100011872.1.35",
          "form": "좀 놀랍기도해요",
          "original_form": "좀 놀랍기도해요",
          "speaker_id": "2",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100011872.1.36",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100011872.1.37",
          "form": "아 정말 안타깝네요",
          "original_form": "아 정말 안타깝네요",
          "speaker_id": "1",
          "time": "20210824 17:00"
        }
      ]
