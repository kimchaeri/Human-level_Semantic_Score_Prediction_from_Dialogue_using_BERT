      "utterance": [
        {
          "id": "MDRW2100018708.1.1",
          "form": "오늘 점심은 뭐먹어?",
          "original_form": "오늘 점심은 뭐먹어?",
          "speaker_id": "2",
          "time": "20210827 11:09"
        },
        {
          "id": "MDRW2100018708.1.2",
          "form": "오늘은 떡만두국 먹으려고 ㅎㅎ",
          "original_form": "오늘은 떡만두국 먹으려고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:09"
        },
        {
          "id": "MDRW2100018708.1.3",
          "form": "ㅎㅎ 좋네~ 국고기 넣고 끓여먹어",
          "original_form": "ㅎㅎ 좋네~ 국고기 넣고 끓여먹어",
          "speaker_id": "2",
          "time": "20210827 11:10"
        },
        {
          "id": "MDRW2100018708.1.4",
          "form": "안그래도 국고기 해동시키는 중",
          "original_form": "안그래도 국고기 해동시키는 중",
          "speaker_id": "1",
          "time": "20210827 11:10"
        },
        {
          "id": "MDRW2100018708.1.5",
          "form": "굿 야채도 듬뿍 넣고 끓이면 영양만점이겠네",
          "original_form": "굿 야채도 듬뿍 넣고 끓이면 영양만점이겠네",
          "speaker_id": "2",
          "time": "20210827 11:10"
        },
        {
          "id": "MDRW2100018708.1.6",
          "form": "ㅎㅎ 당근이랑 버섯 양파 넣고 끓이게",
          "original_form": "ㅎㅎ 당근이랑 버섯 양파 넣고 끓이게",
          "speaker_id": "1",
          "time": "20210827 11:10"
        },
        {
          "id": "MDRW2100018708.1.7",
          "form": "국물 시원하겠네",
          "original_form": "국물 시원하겠네",
          "speaker_id": "2",
          "time": "20210827 11:10"
        },
        {
          "id": "MDRW2100018708.1.8",
          "form": "날씨도 찌뿌뚱하고 뜨끈한 국물 먹어야겠어",
          "original_form": "날씨도 찌뿌뚱하고 뜨끈한 국물 먹어야겠어",
          "speaker_id": "1",
          "time": "20210827 11:11"
        },
        {
          "id": "MDRW2100018708.1.9",
          "form": "난 뭐먹지?",
          "original_form": "난 뭐먹지?",
          "speaker_id": "2",
          "time": "20210827 11:11"
        },
        {
          "id": "MDRW2100018708.1.10",
          "form": "ㅎㅎ 아직 못골랐어?",
          "original_form": "ㅎㅎ 아직 못골랐어?",
          "speaker_id": "1",
          "time": "20210827 11:11"
        },
        {
          "id": "MDRW2100018708.1.11",
          "form": "ㅇㅇ 결정장애야 ㅋㅋ",
          "original_form": "ㅇㅇ 결정장애야 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:11"
        },
        {
          "id": "MDRW2100018708.1.12",
          "form": "신랑도 국물 음식으로 먹어 ㅎㅎ",
          "original_form": "신랑도 국물 음식으로 먹어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:11"
        },
        {
          "id": "MDRW2100018708.1.13",
          "form": "ㅇㅇ 그러던지",
          "original_form": "ㅇㅇ 그러던지",
          "speaker_id": "1",
          "time": "20210827 11:12"
        },
        {
          "id": "MDRW2100018708.1.14",
          "form": "칼국수 이런건 어떠심 ? ㅋ",
          "original_form": "칼국수 이런건 어떠심 ? ㅋ",
          "speaker_id": "1",
          "time": "20210827 11:13"
        },
        {
          "id": "MDRW2100018708.1.15",
          "form": "생각 좀 해보고 ㅎㅎ",
          "original_form": "생각 좀 해보고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 11:13"
        },
        {
          "id": "MDRW2100018708.1.16",
          "form": "든든한 걸로 먹고 오후에도 힘내요",
          "original_form": "든든한 걸로 먹고 오후에도 힘내요",
          "speaker_id": "1",
          "time": "20210827 11:13"
        }
      ]
