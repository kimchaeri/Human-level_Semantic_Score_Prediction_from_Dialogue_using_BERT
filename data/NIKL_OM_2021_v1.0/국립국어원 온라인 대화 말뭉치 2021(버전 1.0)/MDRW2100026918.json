      "utterance": [
        {
          "id": "MDRW2100026918.1.1",
          "form": "네 고기류 매우 좋아합니다",
          "original_form": "네 고기류 매우 좋아합니다",
          "speaker_id": "2",
          "time": "20210902 09:21"
        },
        {
          "id": "MDRW2100026918.1.2",
          "form": "요즘은 곱창이 땡기는군요",
          "original_form": "요즘은 곱창이 땡기는군요",
          "speaker_id": "2",
          "time": "20210902 09:22"
        },
        {
          "id": "MDRW2100026918.1.3",
          "form": "커뮤니티에서",
          "original_form": "커뮤니티에서",
          "speaker_id": "2",
          "time": "20210902 09:22"
        },
        {
          "id": "MDRW2100026918.1.4",
          "form": "맛있는 음식 레시피는",
          "original_form": "맛있는 음식 레시피는",
          "speaker_id": "2",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100026918.1.5",
          "form": "한번 만들거나 사면 다먹을때까지",
          "original_form": "한번 만들거나 사면 다먹을때까지",
          "speaker_id": "2",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100026918.1.6",
          "form": "계속 재탕하죠",
          "original_form": "계속 재탕하죠",
          "speaker_id": "2",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100026918.1.7",
          "form": "집근처에 있나요",
          "original_form": "집근처에 있나요",
          "speaker_id": "1",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100026918.1.8",
          "form": "닭발 좋아하세요?",
          "original_form": "닭발 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100026918.1.9",
          "form": "네 근처에있어요",
          "original_form": "네 근처에있어요",
          "speaker_id": "2",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100026918.1.10",
          "form": "제주도도 버거킹도 하나있어요",
          "original_form": "제주도도 버거킹도 하나있어요",
          "speaker_id": "1",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100026918.1.11",
          "form": "한번씩 포장으로 먹어요",
          "original_form": "한번씩 포장으로 먹어요",
          "speaker_id": "2",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100026918.1.12",
          "form": "닭발 택배는 내일옵니다",
          "original_form": "닭발 택배는 내일옵니다",
          "speaker_id": "1",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100026918.1.13",
          "form": "케케",
          "original_form": "케케",
          "speaker_id": "1",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100026918.1.14",
          "form": "닭발 전 배달로 많이먹구요",
          "original_form": "닭발 전 배달로 많이먹구요",
          "speaker_id": "2",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100026918.1.15",
          "form": "우체국택배는 음식 배송비 무료입니다",
          "original_form": "우체국택배는 음식 배송비 무료입니다",
          "speaker_id": "1",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100026918.1.16",
          "form": "탑마트에서 조리해서 파는 닭발",
          "original_form": "탑마트에서 조리해서 파는 닭발",
          "speaker_id": "2",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100026918.1.17",
          "form": "존맛입니다",
          "original_form": "존맛입니다",
          "speaker_id": "2",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100026918.1.18",
          "form": "탑마트가 없습니다",
          "original_form": "탑마트가 없습니다",
          "speaker_id": "1",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100026918.1.19",
          "form": "뼈있는 닭발 무뼈닭발",
          "original_form": "뼈있는 닭발 무뼈닭발",
          "speaker_id": "2",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100026918.1.20",
          "form": "둘다맛나요",
          "original_form": "둘다맛나요",
          "speaker_id": "2",
          "time": "20210902 09:38"
        },
        {
          "id": "MDRW2100026918.1.21",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "2",
          "time": "20210902 09:38"
        },
        {
          "id": "MDRW2100026918.1.22",
          "form": "맥주는 매장에서 시원하게 먹어야",
          "original_form": "맥주는 매장에서 시원하게 먹어야",
          "speaker_id": "2",
          "time": "20210902 09:40"
        },
        {
          "id": "MDRW2100026918.1.23",
          "form": "로제존맛입니다",
          "original_form": "로제존맛입니다",
          "speaker_id": "2",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100026918.1.24",
          "form": "엽떡에서 먹어봤어요",
          "original_form": "엽떡에서 먹어봤어요",
          "speaker_id": "2",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100026918.1.25",
          "form": "라면사리 끓여서 추가로 넣어서 먹었어요",
          "original_form": "라면사리 끓여서 추가로 넣어서 먹었어요",
          "speaker_id": "2",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100026918.1.26",
          "form": "근처에",
          "original_form": "근처에",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100026918.1.27",
          "form": "오뎅들어가는 김치우동과",
          "original_form": "오뎅들어가는 김치우동과",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100026918.1.28",
          "form": "삼겹살들어가는 김치우동",
          "original_form": "삼겹살들어가는 김치우동",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100026918.1.29",
          "form": "많이드시면",
          "original_form": "많이드시면",
          "speaker_id": "2",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100026918.1.30",
          "form": "혼자서도 쟁반짜장 드실수있어요",
          "original_form": "혼자서도 쟁반짜장 드실수있어요",
          "speaker_id": "2",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100026918.1.31",
          "form": "쟁반짜장엔 고추가루를 뿌려먹어요",
          "original_form": "쟁반짜장엔 고추가루를 뿌려먹어요",
          "speaker_id": "1",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100026918.1.32",
          "form": "그거 맛있죠",
          "original_form": "그거 맛있죠",
          "speaker_id": "2",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100026918.1.33",
          "form": "덜느끼하구요",
          "original_form": "덜느끼하구요",
          "speaker_id": "2",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100026918.1.34",
          "form": "짬뽕국물도 같이 드세요",
          "original_form": "짬뽕국물도 같이 드세요",
          "speaker_id": "2",
          "time": "20210902 10:23"
        }
      ]
