      "utterance": [
        {
          "id": "MDRW2100037536.1.1",
          "form": "비오네여 온니~",
          "original_form": "비오네여 온니~",
          "speaker_id": "1",
          "time": "20210901 08:53"
        },
        {
          "id": "MDRW2100037536.1.2",
          "form": "내 눈물 ㅋㅋㅋ",
          "original_form": "내 눈물 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 08:53"
        },
        {
          "id": "MDRW2100037536.1.3",
          "form": "ㅋㅋ 비온다ㅋㅋ",
          "original_form": "ㅋㅋ 비온다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:41"
        },
        {
          "id": "MDRW2100037536.1.4",
          "form": "니도 일진하지ㅜㅜ",
          "original_form": "니도 일진하지ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 09:41"
        },
        {
          "id": "MDRW2100037536.1.5",
          "form": "난 미래에셋 로그인이 안되요ㅠ",
          "original_form": "난 미래에셋 로그인이 안되요ㅠ",
          "speaker_id": "1",
          "time": "20210901 09:42"
        },
        {
          "id": "MDRW2100037536.1.6",
          "form": "아가씨때 만들었던게 뭐 휴면계좌되가ㅋㅋㅋ",
          "original_form": "아가씨때 만들었던게 뭐 휴면계좌되가ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 09:42"
        },
        {
          "id": "MDRW2100037536.1.7",
          "form": "영업점 가야됨요ㅠ",
          "original_form": "영업점 가야됨요ㅠ",
          "speaker_id": "1",
          "time": "20210901 09:42"
        },
        {
          "id": "MDRW2100037536.1.8",
          "form": "헐ㅡㅡ;;",
          "original_form": "헐ㅡㅡ;;",
          "speaker_id": "2",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100037536.1.9",
          "form": "아가씨때 뭐 이래부지런ㅋ",
          "original_form": "아가씨때 뭐 이래부지런{censored}ㅋ",
          "speaker_id": "2",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100037536.1.10",
          "form": "내 말이요ㅋㅋㅋㅋ",
          "original_form": "내 말이요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100037536.1.11",
          "form": "그런데 왜 돈은 없지??ㅋㅋㅋ",
          "original_form": "그런데 왜 돈은 없지??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100037536.1.12",
          "form": "지금 떨어지는 빗물은 내 눈물이라 요",
          "original_form": "지금 떨어지는 빗물은 내 눈물이라 요",
          "speaker_id": "1",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100037536.1.13",
          "form": "ㅋㅋ해났다ㅋㅋ",
          "original_form": "ㅋㅋ해났다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100037536.1.14",
          "form": "히안하네",
          "original_form": "히안하네",
          "speaker_id": "2",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100037536.1.15",
          "form": "지금부터",
          "original_form": "지금부터",
          "speaker_id": "1",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100037536.1.16",
          "form": "바닥이 있는 건 다 제 땀 입니다ㅋㅋㅋㅋㅋ",
          "original_form": "바닥이 있는 건 다 제 땀 입니다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100037536.1.17",
          "form": "듭네요 ㅋㅋㅋㅋ",
          "original_form": "듭네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100037536.1.18",
          "form": "아하 에어컨 빵빵하게  해라",
          "original_form": "아하 에어컨 빵빵하게  해라",
          "speaker_id": "2",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100037536.1.19",
          "form": "난 이제밥먹을려고",
          "original_form": "난 이제밥먹을려고",
          "speaker_id": "2",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100037536.1.20",
          "form": "난 오늘부터 여름옷 접는데",
          "original_form": "난 오늘부터 여름옷 접는데",
          "speaker_id": "1",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100037536.1.21",
          "form": "아따 겁나 많네요ㅠ",
          "original_form": "아따 겁나 많네요ㅠ",
          "speaker_id": "1",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100037536.1.22",
          "form": "밥 맛있게 먹어요 언니",
          "original_form": "밥 맛있게 먹어요 언니",
          "speaker_id": "1",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100037536.1.23",
          "form": "나도 습한거싫어",
          "original_form": "나도 습한거싫어",
          "speaker_id": "2",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100037536.1.24",
          "form": "습하다는 얘기한 적 없는데ㅋㅋㅋㅋㅋ",
          "original_form": "습하다는 얘기한 적 없는데ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100037536.1.25",
          "form": "누구예요ㅋㅋㅋㅋ",
          "original_form": "누구예요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100037536.1.26",
          "form": "니한테이야기한거다",
          "original_form": "니한테이야기한거다",
          "speaker_id": "2",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100037536.1.27",
          "form": "내가 습하다고ㅋㅈㅋ",
          "original_form": "내가 습하다고ㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100037536.1.28",
          "form": "척하면 딱이어야지ㅋㄱㅋ",
          "original_form": "척하면 딱이어야지ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100037536.1.29",
          "form": "아 언니가 습하다고??ㅋㅋㅋㅋ",
          "original_form": "아 언니가 습하다고??ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100037536.1.30",
          "form": "날씨 진짜 꿉꿉하고 덥네요ㅋㅋ",
          "original_form": "날씨 진짜 꿉꿉하고 덥네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:31"
        }
      ]
