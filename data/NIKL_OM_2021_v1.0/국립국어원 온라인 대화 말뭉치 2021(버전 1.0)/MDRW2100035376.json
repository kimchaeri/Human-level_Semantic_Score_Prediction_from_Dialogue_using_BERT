      "utterance": [
        {
          "id": "MDRW2100035376.1.1",
          "form": "히히 역시 식음료얘기 할거 너무많죠",
          "original_form": "히히 역시 식음료얘기 할거 너무많죠",
          "speaker_id": "2",
          "time": "20210830 08:04"
        },
        {
          "id": "MDRW2100035376.1.2",
          "form": "맞아요ㅋㅋ 점심 뭐드실 예정이신가요?",
          "original_form": "맞아요ㅋㅋ 점심 뭐드실 예정이신가요?",
          "speaker_id": "1",
          "time": "20210830 11:17"
        },
        {
          "id": "MDRW2100035376.1.3",
          "form": "저 고민중이에요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 고민중이에요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100035376.1.4",
          "form": "유부초밥을 만들어볼까....",
          "original_form": "유부초밥을 만들어볼까....",
          "speaker_id": "2",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100035376.1.5",
          "form": "오 유부초밥 좋죠!! 맛있어용 ..",
          "original_form": "오 유부초밥 좋죠!! 맛있어용 ..",
          "speaker_id": "1",
          "time": "20210830 11:23"
        },
        {
          "id": "MDRW2100035376.1.6",
          "form": "사실 만들기가 귀찮아서 고민중입니다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "사실 만들기가 귀찮아서 고민중입니다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:24"
        },
        {
          "id": "MDRW2100035376.1.7",
          "form": "아 맞죠ㅋㅋ 유부초밥이 참 안에 집어넣는게 귀찮은 일이더라구요",
          "original_form": "아 맞죠ㅋㅋ 유부초밥이 참 안에 집어넣는게 귀찮은 일이더라구요",
          "speaker_id": "1",
          "time": "20210830 11:25"
        },
        {
          "id": "MDRW2100035376.1.8",
          "form": "사실 저는 다 흘립니닼ㅋㅋㅋ",
          "original_form": "사실 저는 다 흘립니닼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:26"
        },
        {
          "id": "MDRW2100035376.1.9",
          "form": "가끔 화나서 그냥 밥위에 유부 얹어서 먹기도해욬ㅋㅋㅋㅋㅋ",
          "original_form": "가끔 화나서 그냥 밥위에 유부 얹어서 먹기도해욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:26"
        },
        {
          "id": "MDRW2100035376.1.10",
          "form": "그래도 씹으면 똑같은거죠~!",
          "original_form": "그래도 씹으면 똑같은거죠~!",
          "speaker_id": "1",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100035376.1.11",
          "form": "저는 그냥 유부 잘게잘라서 비벼먹을때도 있어요ㅋㅋ",
          "original_form": "저는 그냥 유부 잘게잘라서 비벼먹을때도 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100035376.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 와 그건 시도 안해봣는데 좋내요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 와 그건 시도 안해봣는데 좋내요",
          "speaker_id": "2",
          "time": "20210830 11:31"
        },
        {
          "id": "MDRW2100035376.1.13",
          "form": "비슷하더라구요!! ㅋㅋ",
          "original_form": "비슷하더라구요!! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100035376.1.14",
          "form": "유부초밥에 라면 먹으면 진짜 맛있는데 땡기네요..",
          "original_form": "유부초밥에 라면 먹으면 진짜 맛있는데 땡기네요..",
          "speaker_id": "1",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100035376.1.15",
          "form": "와 그거 진짜맛있죠",
          "original_form": "와 그거 진짜맛있죠",
          "speaker_id": "2",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100035376.1.16",
          "form": "전 ... 배달시켜먹었습니다 ^^",
          "original_form": "전 ... 배달시켜먹었습니다 ^^",
          "speaker_id": "2",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100035376.1.17",
          "form": "오...^^ 어떤 음식 시켜드셨나요",
          "original_form": "오...^^ 어떤 음식 시켜드셨나요",
          "speaker_id": "1",
          "time": "20210830 18:12"
        },
        {
          "id": "MDRW2100035376.1.18",
          "form": "꼬막비빔밥이요 깔깔깔",
          "original_form": "꼬막비빔밥이요 깔깔깔",
          "speaker_id": "2",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100035376.1.19",
          "form": "부럽쬬",
          "original_form": "부럽쬬",
          "speaker_id": "2",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100035376.1.20",
          "form": "나쁭 말 안했는데.. 왜 안보내졌을까요..",
          "original_form": "나쁭 말 안했는데.. 왜 안보내졌을까요..",
          "speaker_id": "1",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100035376.1.21",
          "form": "꼬막은 어떻게 먹어도 맛있죠 ㅠㅠ",
          "original_form": "꼬막은 어떻게 먹어도 맛있죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100035376.1.22",
          "form": "꼬막 먹고 싶네요",
          "original_form": "꼬막 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100035376.1.23",
          "form": "ㅋㅋㅋㅋㅋ 그니까유 요기 나쁜말 인식이 잘 안되나봐요 ㅋㅋ큐ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ 그니까유 요기 나쁜말 인식이 잘 안되나봐요 ㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100035376.1.24",
          "form": "꼬막 양념이 짱입니다요",
          "original_form": "꼬막 양념이 짱입니다요",
          "speaker_id": "2",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100035376.1.25",
          "form": "비빔.. 예술이에요 ..ㅠㅠ",
          "original_form": "비빔.. 예술이에요 ..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100035376.1.26",
          "form": "약간 비빔밥은 밥 가늠이 안되서 한 2인분은 먹는 것 같아요",
          "original_form": "약간 비빔밥은 밥 가늠이 안되서 한 2인분은 먹는 것 같아요",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100035376.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 헐 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 헐 맞아요",
          "speaker_id": "2",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100035376.1.28",
