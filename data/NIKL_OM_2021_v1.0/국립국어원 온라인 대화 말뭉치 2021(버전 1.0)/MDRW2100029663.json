      "utterance": [
        {
          "id": "MDRW2100029663.1.1",
          "form": "다행이에요",
          "original_form": "다행이에요",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029663.1.2",
          "form": "거지는안돼서",
          "original_form": "거지는안돼서",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029663.1.3",
          "form": "3명고스톱이랑",
          "original_form": "3명고스톱이랑",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029663.1.4",
          "form": "맞고중",
          "original_form": "맞고중",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029663.1.5",
          "form": "어떤걸 선호하시나요",
          "original_form": "어떤걸 선호하시나요",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029663.1.6",
          "form": "맞고를 선호합니다",
          "original_form": "맞고를 선호합니다",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100029663.1.7",
          "form": "진행이 빠르니까요",
          "original_form": "진행이 빠르니까요",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100029663.1.8",
          "form": "돈도 많이벌죠",
          "original_form": "돈도 많이벌죠",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100029663.1.9",
          "form": "어느사이트에서 하세요",
          "original_form": "어느사이트에서 하세요",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100029663.1.10",
          "form": "넷마블?한게임?",
          "original_form": "넷마블?한게임?",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100029663.1.11",
          "form": "넷마블이요",
          "original_form": "넷마블이요",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100029663.1.12",
          "form": "재밌죠",
          "original_form": "재밌죠",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100029663.1.13",
          "form": "그런데 안한지 좀 되었네요",
          "original_form": "그런데 안한지 좀 되었네요",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100029663.1.14",
          "form": "사이버머니라도많으면",
          "original_form": "사이버머니라도많으면",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100029663.1.15",
          "form": "마음이드든하요",
          "original_form": "마음이드든하요",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100029663.1.16",
          "form": "폰이 있으니 컴퓨터 안켜게돼요",
          "original_form": "폰이 있으니 컴퓨터 안켜게돼요",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100029663.1.17",
          "form": "든든",
          "original_form": "든든",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100029663.1.18",
          "form": "그 돈이 내돈이면 소원이  없죠",
          "original_form": "그 돈이 내돈이면 소원이  없죠",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100029663.1.19",
          "form": "폰에 어플로도 나와있어요",
          "original_form": "폰에 어플로도 나와있어요",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100029663.1.20",
          "form": "파산해도 공짜로주고",
          "original_form": "파산해도 공짜로주고",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100029663.1.21",
          "form": "화면이 작아서 치는 맛이 안나요",
          "original_form": "화면이 작아서 치는 맛이 안나요",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100029663.1.22",
          "form": "소리나진동이있잖아야",
          "original_form": "소리나진동이있잖아야",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100029663.1.23",
          "form": "크고 아름다운게 좋아요",
          "original_form": "크고 아름다운게 좋아요",
          "speaker_id": "1",
          "time": "20210903 22:50"
        }
