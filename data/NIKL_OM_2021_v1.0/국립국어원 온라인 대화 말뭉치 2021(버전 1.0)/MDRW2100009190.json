      "utterance": [
        {
          "id": "MDRW2100009190.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009190.1.2",
          "form": "공연 보러다니는 거 좋아하시나요?",
          "original_form": "공연 보러다니는 거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009190.1.3",
          "form": "네 뮤지컬 보는걸 좋아해요.",
          "original_form": "네 뮤지컬 보는걸 좋아해요.",
          "speaker_id": "2",
          "time": "20210822 22:05"
        },
        {
          "id": "MDRW2100009190.1.4",
          "form": "정말요? 추천해주실 수 있나요?",
          "original_form": "정말요? 추천해주실 수 있나요?",
          "speaker_id": "1",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009190.1.5",
          "form": "뮤지컬 캣츠 보러가고 싶었는데 또 하는 날이 오겠죠?",
          "original_form": "뮤지컬 캣츠 보러가고 싶었는데 또 하는 날이 오겠죠?",
          "speaker_id": "1",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009190.1.6",
          "form": "저는 박효신 나오는 뮤지컬은 다 봤어요",
          "original_form": "저는 박효신 나오는 뮤지컬은 다 봤어요",
          "speaker_id": "2",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009190.1.7",
          "form": "최근에는 안가서ㅠ코로나때문에 뮤지컬하나요?",
          "original_form": "최근에는 안가서ㅠ코로나때문에 뮤지컬하나요?",
          "speaker_id": "2",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009190.1.8",
          "form": "세상에 세상에",
          "original_form": "세상에 세상에",
          "speaker_id": "1",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009190.1.9",
          "form": "대장님 좋아하시는구나",
          "original_form": "대장님 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009190.1.10",
          "form": "혹시 님도 좋아하시나요?!",
          "original_form": "혹시 님도 좋아하시나요?!",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009190.1.11",
          "form": "그러니까요ㅠㅠ 코로나 전에는 취업만 하면 공연이고 콘서트도 다 다닐 줄 알았는데 무슨일이에요 이게. ..",
          "original_form": "그러니까요ㅠㅠ 코로나 전에는 취업만 하면 공연이고 콘서트도 다 다닐 줄 알았는데 무슨일이에요 이게. ..",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009190.1.12",
          "form": "아우 대장님 안좋아하는 사람이 있을까용ㅠㅠ",
          "original_form": "아우 대장님 안좋아하는 사람이 있을까용ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009190.1.13",
          "form": "저도 공연이랑 뮤지컬 못본지 한참이네요ㅠㅠ",
          "original_form": "저도 공연이랑 뮤지컬 못본지 한참이네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009190.1.14",
          "form": "그래도 전시회는 꾸준히 열리는 거같은데 공연은...",
          "original_form": "그래도 전시회는 꾸준히 열리는 거같은데 공연은...",
          "speaker_id": "1",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009190.1.15",
          "form": "내년이면 콘서트 갈 수 있겠죠ㅠㅠ?",
          "original_form": "내년이면 콘서트 갈 수 있겠죠ㅠㅠ?",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009190.1.16",
          "form": "공연 뭐가됐든 가고싶네요ㅠㅠ",
          "original_form": "공연 뭐가됐든 가고싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009190.1.17",
          "form": "부디 내년에는!",
          "original_form": "부디 내년에는!",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009190.1.18",
          "form": "부디  내년에는!",
          "original_form": "부디  내년에는!",
          "speaker_id": "1",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009190.1.19",
          "form": "좋은 공연 많이 생겼음 좋겠어요.",
          "original_form": "좋은 공연 많이 생겼음 좋겠어요.",
          "speaker_id": "2",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009190.1.20",
          "form": "저는 아이유 콘서트 너무 가고싶어요",
          "original_form": "저는 아이유 콘서트 너무 가고싶어요",
          "speaker_id": "1",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009190.1.21",
          "form": "아이유콘서트 재밌겠네요ㅠㅠ",
          "original_form": "아이유콘서트 재밌겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009190.1.22",
          "form": "저도 콘서트는 박효신이 첨이자 마지막..",
          "original_form": "저도 콘서트는 박효신이 첨이자 마지막..",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009190.1.23",
          "form": "아..예전에 컨츄리꼬꼬 콘서트 가본적있어요",
          "original_form": "아..예전에 컨츄리꼬꼬 콘서트 가본적있어요",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009190.1.24",
          "form": "김미김미ㅋㅋㅋ",
          "original_form": "김미김미ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009190.1.25",
          "form": "엄청 추억돋네요ㅋㅋ",
          "original_form": "엄청 추억돋네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009190.1.26",
          "form": "다행히 동년배이시네요",
          "original_form": "다행히 동년배이시네요",
          "speaker_id": "2",
          "time": "20210822 22:17"
        }
      ]
    }
