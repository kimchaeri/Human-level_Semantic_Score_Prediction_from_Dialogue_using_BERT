      "utterance": [
        {
          "id": "MDRW2100008380.1.1",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100008380.1.2",
          "form": "헉 아뇨 취준생입니다ㅜㅜㅜㅜㅜ헣급",
          "original_form": "헉 아뇨 취준생입니다ㅜㅜㅜㅜㅜ헣급",
          "speaker_id": "1",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100008380.1.3",
          "form": "흑흑... 취준생이시군요 ㅠㅠ.ㅠ",
          "original_form": "흑흑... 취준생이시군요 ㅠㅠ.ㅠ",
          "speaker_id": "2",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100008380.1.4",
          "form": "저는 졸업 예정자 대학생이에요..",
          "original_form": "저는 졸업 예정자 대학생이에요..",
          "speaker_id": "2",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100008380.1.5",
          "form": "이번이 막학기.. 또르르..",
          "original_form": "이번이 막학기.. 또르르..",
          "speaker_id": "2",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100008380.1.6",
          "form": "허헙",
          "original_form": "허헙",
          "speaker_id": "1",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100008380.1.7",
          "form": "내년 2월 졸업생이시군여...",
          "original_form": "내년 2월 졸업생이시군여...",
          "speaker_id": "1",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100008380.1.8",
          "form": "학과는 어떤과세요?",
          "original_form": "학과는 어떤과세요?",
          "speaker_id": "1",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100008380.1.9",
          "form": "저는 굶는 문콘.",
          "original_form": "저는 굶는 문콘.",
          "speaker_id": "1",
          "time": "20210822 21:58"
        },
        {
          "id": "MDRW2100008380.1.10",
          "form": "아 저는 공대다녀요 ㅋㅋ ㅠㅠ",
          "original_form": "아 저는 공대다녀요 ㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100008380.1.11",
          "form": "근데 배운게 없어서 멍하니 있어요 미치겠어요",
          "original_form": "근데 배운게 없어서 멍하니 있어요 미치겠어요",
          "speaker_id": "2",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100008380.1.12",
          "form": "헐 공..대",
          "original_form": "헐 공..대",
          "speaker_id": "1",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100008380.1.13",
          "form": "너무 멋있다",
          "original_form": "너무 멋있다",
          "speaker_id": "1",
          "time": "20210822 22:01"
        },
        {
          "id": "MDRW2100008380.1.14",
          "form": "전기쪽인가요",
          "original_form": "전기쪽인가요",
          "speaker_id": "1",
          "time": "20210822 22:01"
        },
        {
          "id": "MDRW2100008380.1.15",
          "form": "아니 어케..수학..잘하시나봐요",
          "original_form": "아니 어케..수학..잘하시나봐요",
          "speaker_id": "1",
          "time": "20210822 22:01"
        },
        {
          "id": "MDRW2100008380.1.16",
          "form": "전자 쪽이요 ㅎㅎ..",
          "original_form": "전자 쪽이요 ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210822 22:01"
        },
        {
          "id": "MDRW2100008380.1.17",
          "form": "아니요...",
          "original_form": "아니요...",
          "speaker_id": "2",
          "time": "20210822 22:01"
        },
        {
          "id": "MDRW2100008380.1.18",
          "form": "수학 못하는데....",
          "original_form": "수학 못하는데....",
          "speaker_id": "2",
          "time": "20210822 22:01"
        },
        {
          "id": "MDRW2100008380.1.19",
          "form": "좋아는했어요....",
          "original_form": "좋아는했어요....",
          "speaker_id": "2",
          "time": "20210822 22:01"
        },
        {
          "id": "MDRW2100008380.1.20",
          "form": "눈물",
          "original_form": "눈물",
          "speaker_id": "2",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100008380.1.21",
          "form": "외..외사랑인가요",
          "original_form": "외..외사랑인가요",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100008380.1.22",
          "form": "수학은 아무나 좋아하지 않던데",
          "original_form": "수학은 아무나 좋아하지 않던데",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100008380.1.23",
          "form": "어케...떡잎부터 공대셨군요",
          "original_form": "어케...떡잎부터 공대셨군요",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100008380.1.24",
          "form": "저는 진짜 어지러워요",
          "original_form": "저는 진짜 어지러워요",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100008380.1.25",
          "form": "수학",
          "original_form": "수학",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100008380.1.26",
          "form": "수학 진짜 어려운데..",
          "original_form": "수학 진짜 어려운데..",
          "speaker_id": "2",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100008380.1.27",
          "form": "오래된 짝사랑이죠..",
          "original_form": "오래된 짝사랑이죠..",
          "speaker_id": "2",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100008380.1.28",
          "form": "물리도 그랬어요",
          "original_form": "물리도 그랬어요",
          "speaker_id": "2",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100008380.1.29",
          "form": "name1님은 전공이?",
          "original_form": "&name1&님은 전공이?",
          "speaker_id": "2",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100008380.1.30",
          "form": "아 저는 문콘이요!",
          "original_form": "아 저는 문콘이요!",
          "speaker_id": "1",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100008380.1.31",
          "form": "완전..휴..",
          "original_form": "완전..휴..",
          "speaker_id": "1",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100008380.1.32",
          "form": "저희는 팀플 진짜 많았는데 공대도 많나요?",
          "original_form": "저희는 팀플 진짜 많았는데 공대도 많나요?",
          "speaker_id": "1",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100008380.1.33",
          "form": "공대 과제가 지옥인데 그걸 표현 못해서 많이 안 알려진거라 들었어요",
          "original_form": "공대 과제가 지옥인데 그걸 표현 못해서 많이 안 알려진거라 들었어요",
          "speaker_id": "1",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100008380.1.34",
          "form": "아 문콘이뭔가했어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 문콘이뭔가했어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100008380.1.35",
          "form": "저희는 팀플은 그렇ㄱ 많진 않아서 죽을것같진않았어요",
          "original_form": "저희는 팀플은 그렇ㄱ 많진 않아서 죽을것같진않았어요",
          "speaker_id": "2",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100008380.1.36",
          "form": "대신.. 매주 새과제가 나오긴 했죠 ㅠㅠㅠ",
          "original_form": "대신.. 매주 새과제가 나오긴 했죠 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100008380.1.37",
          "form": "그걸 매 과목....",
          "original_form": "그걸 매 과목....",
          "speaker_id": "2",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100008380.1.38",
          "form": "헙..허헙",
          "original_form": "헙..허헙",
          "speaker_id": "1",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100008380.1.39",
          "form": "살아계..세요?",
          "original_form": "살아계..세요?",
          "speaker_id": "1",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100008380.1.40",
          "form": "아니 교수님들 본인것만 듣는 줄 아시는 거 아녜요?",
          "original_form": "아니 교수님들 본인것만 듣는 줄 아시는 거 아녜요?",
          "speaker_id": "1",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100008380.1.41",
          "form": "ㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100008380.1.42",
          "form": "가끔 봐주시는 교수님도 계셨어요 ㅋㅋㅋㅋㅋ",
          "original_form": "가끔 봐주시는 교수님도 계셨어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100008380.1.43",
          "form": "교슈님 저희.. 과제가 넘 많아요 하면서 울면 한주 미뤄주시거나 ㅋㅋㅋㅋㅋ",
          "original_form": "교슈님 저희.. 과제가 넘 많아요 하면서 울면 한주 미뤄주시거나 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100008380.1.44",
          "form": "쪽지시험 많았던것도 ..진짜 지옥이었네요",
          "original_form": "쪽지시험 많았던것도 ..진짜 지옥이었네요",
          "speaker_id": "2",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100008380.1.45",
          "form": "서윗하시네요",
          "original_form": "서윗하시네요",
          "speaker_id": "1",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100008380.1.46",
          "form": "으윽",
          "original_form": "으윽",
          "speaker_id": "1",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100008380.1.47",
          "form": "근데 그걸 다 해내신거잖어요",
          "original_form": "근데 그걸 다 해내신거잖어요",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100008380.1.48",
          "form": "근데 팀플도 장난 아니잖아요",
          "original_form": "근데 팀플도 장난 아니잖아요",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100008380.1.49",
          "form": "근데 뭐랄까 그건 커버 가능한것같어요",
          "original_form": "근데 뭐랄까 그건 커버 가능한것같어요",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100008380.1.50",
          "form": "ㅎㅎ.. 팀플ㅇ비하면.. 과제많은세 나은것같기도..",
          "original_form": "ㅎㅎ.. 팀플ㅇ비하면.. 과제많은세 나은것같기도..",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100008380.1.51",
          "form": "열받지만",
          "original_form": "열받지만",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100008380.1.52",
          "form": "와.. 대단하새요...",
          "original_form": "와.. 대단하새요...",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100008380.1.53",
          "form": "요즘 프리라이더는 없는 것 같아요",
          "original_form": "요즘 프리라이더는 없는 것 같아요",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100008380.1.54",
          "form": "다들 예민해서",
          "original_form": "다들 예민해서",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100008380.1.55",
          "form": "그건 참 다향이내요",
          "original_form": "그건 참 다향이내요",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100008380.1.56",
          "form": "근데 이 과는 좀",
          "original_form": "근데 이 과는 좀",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100008380.1.57",
          "form": "저는 한번 잘못만나서..",
          "original_form": "저는 한번 잘못만나서..",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100008380.1.58",
          "form": "자기가 길을 개척해야하는것같아요",
          "original_form": "자기가 길을 개척해야하는것같아요",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100008380.1.59",
          "form": "콘텐츠가 워낙 광범위하다보니까",
          "original_form": "콘텐츠가 워낙 광범위하다보니까",
          "speaker_id": "1",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100008380.1.60",
          "form": "그냥 자기가 좋아하는 콘텐츠쪽으로 개척해서 가더라고요",
          "original_form": "그냥 자기가 좋아하는 콘텐츠쪽으로 개척해서 가더라고요",
          "speaker_id": "1",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100008380.1.61",
          "form": "진짜 자기힘으로 해야하는개 많갰내요",
          "original_form": "진짜 자기힘으로 해야하는개 많갰내요",
          "speaker_id": "2",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100008380.1.62",
          "form": "저는 좀 메뉴얼대로 쑥쑥?",
          "original_form": "저는 좀 메뉴얼대로 쑥쑥?",
