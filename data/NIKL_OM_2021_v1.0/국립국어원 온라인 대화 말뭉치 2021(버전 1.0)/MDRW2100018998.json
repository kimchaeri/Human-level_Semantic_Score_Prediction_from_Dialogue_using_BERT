      "utterance": [
        {
          "id": "MDRW2100018998.1.1",
          "form": "아직이요! 9월 안으로 스케일링 하러 가려구요",
          "original_form": "아직이요! 9월 안으로 스케일링 하러 가려구요",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100018998.1.2",
          "form": "스케일링 받으러 가시는구나",
          "original_form": "스케일링 받으러 가시는구나",
          "speaker_id": "1",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100018998.1.3",
          "form": "좀 치과는 언제가도 긴장되지않나요",
          "original_form": "좀 치과는 언제가도 긴장되지않나요",
          "speaker_id": "1",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100018998.1.4",
          "form": "사랑니에 대한 소견이 없기를 바라요ㅠㅠ 너무 무서워요ㅠㅠ",
          "original_form": "사랑니에 대한 소견이 없기를 바라요ㅠㅠ 너무 무서워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100018998.1.5",
          "form": "사랑니 나셨구나",
          "original_form": "사랑니 나셨구나",
          "speaker_id": "1",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100018998.1.6",
          "form": "저 매복 뽑다가 죽을번했잖아요 너무 아파서",
          "original_form": "저 매복 뽑다가 죽을번했잖아요 너무 아파서",
          "speaker_id": "1",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100018998.1.7",
          "form": "아직 안 났는데...혹시나 사진상 보일까봐요ㅠ",
          "original_form": "아직 안 났는데...혹시나 사진상 보일까봐요ㅠ",
          "speaker_id": "2",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100018998.1.8",
          "form": "아.. 공포..",
          "original_form": "아.. 공포..",
          "speaker_id": "1",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100018998.1.9",
          "form": "저도 안보이다 사진에서 사랑니가 있늕것을 알았거든요",
          "original_form": "저도 안보이다 사진에서 사랑니가 있늕것을 알았거든요",
          "speaker_id": "1",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100018998.1.10",
          "form": "그걸 보는 제 마음이란..",
          "original_form": "그걸 보는 제 마음이란..",
          "speaker_id": "1",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100018998.1.11",
          "form": "아아ㅜㅜㅠㅠㅠㅠㅠ 엄청 아프셨죠ㅠㅠ",
          "original_form": "아아ㅜㅜㅠㅠㅠㅠㅠ 엄청 아프셨죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100018998.1.12",
          "form": "부디 name2님의 잇몸속엔..",
          "original_form": "부디 &name2&님의 잇몸속엔..",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100018998.1.13",
          "form": "그 사악한 아이들이 없기를",
          "original_form": "그 사악한 아이들이 없기를",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100018998.1.14",
          "form": "제발요ㅠㅠ 잇몸마취 상상하기도 싫어요",
          "original_form": "제발요ㅠㅠ 잇몸마취 상상하기도 싫어요",
          "speaker_id": "2",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100018998.1.15",
          "form": "겁나 큰 주사 들고오거든요",
          "original_form": "겁나 큰 주사 들고오거든요",
          "speaker_id": "1",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100018998.1.16",
          "form": "저 태어나서 잇몸에 그런주사 처음맞아봤잖아요",
          "original_form": "저 태어나서 잇몸에 그런주사 처음맞아봤잖아요",
          "speaker_id": "1",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100018998.1.17",
          "form": "ㅋㅋㅋㅋ 와 그 주사를 제 눈으로 봤을 때의 공포감이란..",
          "original_form": "ㅋㅋㅋㅋ 와 그 주사를 제 눈으로 봤을 때의 공포감이란..",
          "speaker_id": "1",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100018998.1.18",
          "form": "스케일링은 참을만 한가요",
          "original_form": "스케일링은 참을만 한가요",
          "speaker_id": "1",
          "time": "20210827 15:38"
        },
        {
          "id": "MDRW2100018998.1.19",
          "form": "저는 할만 했어요! 주사...눈으로 보면ㅜㅜㅜ",
          "original_form": "저는 할만 했어요! 주사...눈으로 보면ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100018998.1.20",
          "form": "스케일링은 참을만 하셨구나",
          "original_form": "스케일링은 참을만 하셨구나",
          "speaker_id": "1",
          "time": "20210827 16:10"
        },
        {
          "id": "MDRW2100018998.1.21",
          "form": "저도 그랬어요",
          "original_form": "저도 그랬어요",
          "speaker_id": "1",
          "time": "20210827 16:10"
        },
        {
          "id": "MDRW2100018998.1.22",
          "form": "근데 주사는..",
          "original_form": "근데 주사는..",
          "speaker_id": "1",
          "time": "20210827 16:10"
        },
        {
          "id": "MDRW2100018998.1.23",
          "form": "무섭죠 아무래도 그쵸",
          "original_form": "무섭죠 아무래도 그쵸",
          "speaker_id": "1",
          "time": "20210827 16:10"
        },
        {
          "id": "MDRW2100018998.1.24",
          "form": "진짜 치과는 돈도 고통도 너무",
          "original_form": "진짜 치과는 돈도 고통도 너무",
          "speaker_id": "1",
          "time": "20210827 16:11"
        },
        {
          "id": "MDRW2100018998.1.25",
          "form": "무서운곳이에요",
          "original_form": "무서운곳이에요",
          "speaker_id": "1",
          "time": "20210827 16:11"
        },
        {
          "id": "MDRW2100018998.1.26",
          "form": "돈...정말 무섭죠ㅠㅠ",
          "original_form": "돈...정말 무섭죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100018998.1.27",
          "form": "저 그 교정때문에",
          "original_form": "저 그 교정때문에",
          "speaker_id": "1",
          "time": "20210827 16:13"
        },
        {
          "id": "MDRW2100018998.1.28",
          "form": "뺀 상태에요",
          "original_form": "뺀 상태에요",
          "speaker_id": "1",
          "time": "20210827 16:13"
        }
      ]
    }
