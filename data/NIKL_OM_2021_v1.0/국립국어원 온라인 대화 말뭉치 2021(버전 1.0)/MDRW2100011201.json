      "utterance": [
        {
          "id": "MDRW2100011201.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 03:52"
        },
        {
          "id": "MDRW2100011201.1.2",
          "form": "닉이 너무 웃겨서 왔어요 ㅋㅋㅋㅋㅋ",
          "original_form": "닉이 너무 웃겨서 왔어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 03:52"
        },
        {
          "id": "MDRW2100011201.1.3",
          "form": "앗 안녕하세요ㅋㅋㅋ",
          "original_form": "앗 안녕하세요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011201.1.4",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011201.1.5",
          "form": "앗 아니요 작년에 졸업했습니다 ㅎㅎ",
          "original_form": "앗 아니요 작년에 졸업했습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011201.1.6",
          "form": "앗 그럼 축제같은거 즐기셨겠네요!",
          "original_form": "앗 그럼 축제같은거 즐기셨겠네요!",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011201.1.7",
          "form": "네 다행히 코로나 전이어서 한 번정도 즐겼네요ㅎㅎ",
          "original_form": "네 다행히 코로나 전이어서 한 번정도 즐겼네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:57"
        },
        {
          "id": "MDRW2100011201.1.8",
          "form": "퐁퐁님은 학생이신가요?",
          "original_form": "퐁퐁님은 학생이신가요?",
          "speaker_id": "2",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100011201.1.9",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 19:43"
        },
        {
          "id": "MDRW2100011201.1.10",
          "form": "곧 개강이라 슬퍼요 ..",
          "original_form": "곧 개강이라 슬퍼요 ..",
          "speaker_id": "1",
          "time": "20210824 19:44"
        },
        {
          "id": "MDRW2100011201.1.11",
          "form": "아아아...ㅠ",
          "original_form": "아아아...ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100011201.1.12",
          "form": "수강신청은 잘 하셨나요?ㅠ",
          "original_form": "수강신청은 잘 하셨나요?ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100011201.1.13",
          "form": "어제 했답니다 ㅠㅠㅠ",
          "original_form": "어제 했답니다 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100011201.1.14",
          "form": "서버가 펑~~!",
          "original_form": "서버가 펑~~!",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100011201.1.15",
          "form": "기대를 저버리지않더라고요",
          "original_form": "기대를 저버리지않더라고요",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100011201.1.16",
          "form": "ㅎㅎㅎㅠ 학교서버는 진짜 한결같이 유리서버인 것 같아요ㅋㅋ",
          "original_form": "ㅎㅎㅎㅠ 학교서버는 진짜 한결같이 유리서버인 것 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100011201.1.17",
          "form": "이번에는 오프라인 수업 많이 할거라던데 화이팅하세요ㅠ",
          "original_form": "이번에는 오프라인 수업 많이 할거라던데 화이팅하세요ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100011201.1.18",
          "form": "저는 저번학기 전부 학교가서 ㅋㅋㅋ",
          "original_form": "저는 저번학기 전부 학교가서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100011201.1.19",
          "form": "오프라인 수업하려나...?",
          "original_form": "오프라인 수업하려나...?",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100011201.1.20",
          "form": "학교를 믿을 숲엊ㅅ어요",
          "original_form": "학교를 믿을 숲엊ㅅ어요",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100011201.1.21",
          "form": "아앗.. 저번학기부터 오프라인이셨으면 왠지 이번에도..",
          "original_form": "아앗.. 저번학기부터 오프라인이셨으면 왠지 이번에도..",
          "speaker_id": "2",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100011201.1.22",
          "form": "교수님이 과제라도 적게 내주셨으면 좋겠네요ㅠ",
          "original_form": "교수님이 과제라도 적게 내주셨으면 좋겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100011201.1.23",
          "form": "공대라 ㅠㅠ",
          "original_form": "공대라 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100011201.1.24",
          "form": "기대도 안하고 있어요..",
          "original_form": "기대도 안하고 있어요..",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100011201.1.25",
          "form": "그냥 등록금뽕을 열심히 뽑아야지..",
          "original_form": "그냥 등록금뽕을 열심히 뽑아야지..",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100011201.1.26",
          "form": "허규 공대면 수업 엄청 빡세지 않나요ㅠ",
          "original_form": "허규 공대면 수업 엄청 빡세지 않나요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:20"
        }
      ]
    }
  ]
