      "utterance": [
        {
          "id": "MDRW2100042581.1.1",
          "form": "언니 뭐보고 있어요?",
          "original_form": "언니 뭐보고 있어요?",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100042581.1.2",
          "form": "나 그냥 뉴스틀어놨다",
          "original_form": "나 그냥 뉴스틀어놨다",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100042581.1.3",
          "form": "뉴스 볼거 없지 않아요?",
          "original_form": "뉴스 볼거 없지 않아요?",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100042581.1.4",
          "form": "없지 그냥 무의식적으로 틀어놓앗다",
          "original_form": "없지 그냥 무의식적으로 틀어놓앗다",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100042581.1.5",
          "form": "ㅋㅋ 드라마 안해요?",
          "original_form": "ㅋㅋ 드라마 안해요?",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100042581.1.6",
          "form": "일일저녁드라마 할껀데 그냥 안보게 되네",
          "original_form": "일일저녁드라마 할껀데 그냥 안보게 되네",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100042581.1.7",
          "form": "그쵸 저도 본적이 없어요",
          "original_form": "그쵸 저도 본적이 없어요",
          "speaker_id": "1",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100042581.1.8",
          "form": "한번보게되면 계속 봐야할거 같아서",
          "original_form": "한번보게되면 계속 봐야할거 같아서",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100042581.1.9",
          "form": "그렇긴 하죠 워낙 길어서ㅋ",
          "original_form": "그렇긴 하죠 워낙 길어서ㅋ",
          "speaker_id": "1",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100042581.1.10",
          "form": "엉 일일드라마는 100회가 넘으니 힘들다",
          "original_form": "엉 일일드라마는 100회가 넘으니 힘들다",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100042581.1.11",
          "form": "그리고 중간에 봐도 내용 다 알아요",
          "original_form": "그리고 중간에 봐도 내용 다 알아요",
          "speaker_id": "1",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100042581.1.12",
          "form": "응 채림동생 박윤재는 뭘로 데뷔했지",
          "original_form": "응 채림동생 박윤재는 뭘로 데뷔했지",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100042581.1.13",
          "form": "박윤재 글쎄요.  검색해 보께요",
          "original_form": "박윤재 글쎄요.  검색해 보께요",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100042581.1.14",
          "form": "영화 해안선으로 데뷔했네 검색해보니",
          "original_form": "영화 해안선으로 데뷔했네 검색해보니",
          "speaker_id": "2",
