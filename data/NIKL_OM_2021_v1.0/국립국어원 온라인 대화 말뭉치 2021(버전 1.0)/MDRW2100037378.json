      "utterance": [
        {
          "id": "MDRW2100037378.1.1",
          "form": "아침부터 뭘샀어??",
          "original_form": "아침부터 뭘샀어??",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037378.1.2",
          "form": "구글은 대기업인데 처리가 굉장히 느리더라",
          "original_form": "구글은 대기업인데 처리가 굉장히 느리더라",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037378.1.3",
          "form": "나는 공부할때 쓰는 어플을 구입했어",
          "original_form": "나는 공부할때 쓰는 어플을 구입했어",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037378.1.4",
          "form": "왜그렇지 외국이라그런가",
          "original_form": "왜그렇지 외국이라그런가",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037378.1.5",
          "form": "어떤 어플이야???",
          "original_form": "어떤 어플이야???",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037378.1.6",
          "form": "영구권은 50퍼 할인해서 무려 만오천원이더라",
          "original_form": "영구권은 50퍼 할인해서 무려 만오천원이더라",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037378.1.7",
          "form": "시간관리를 효율적으오 할 수 있도록 도와주고 데일리레포트도 제공해줘",
          "original_form": "시간관리를 효율적으오 할 수 있도록 도와주고 데일리레포트도 제공해줘",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037378.1.8",
          "form": "그정도면 적당한 가격이긴하네",
          "original_form": "그정도면 적당한 가격이긴하네",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037378.1.9",
          "form": "자기관리를 더 철저히 항 수 있겠어",
          "original_form": "자기관리를 더 철저히 항 수 있겠어",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037378.1.10",
          "form": "근데 왜 환불한거야???",
          "original_form": "근데 왜 환불한거야???",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037378.1.11",
          "form": "나는 4900원정도가 좋아",
          "original_form": "나는 4900원정도가 좋아",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037378.1.12",
          "form": "페이백을 받어야했어",
          "original_form": "페이백을 받어야했어",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037378.1.13",
          "form": "어플이 너무 비싸잖아",
          "original_form": "어플이 너무 비싸잖아",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037378.1.14",
          "form": "어휴 그런건 처음부터 알아서 해줘야할텐데",
          "original_form": "어휴 그런건 처음부터 알아서 해줘야할텐데",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037378.1.15",
          "form": "나는 최적의 결제방법을 생각해야했어.",
          "original_form": "나는 최적의 결제방법을 생각해야했어.",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037378.1.16",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037378.1.17",
          "form": "왜 소비자가 직접 신청해야하는거야?",
          "original_form": "왜 소비자가 직접 신청해야하는거야?",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037378.1.18",
          "form": "신청하고 구매하는 시스템이라니 불편해",
          "original_form": "신청하고 구매하는 시스템이라니 불편해",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037378.1.19",
          "form": "그건 카카오페이 잘못이야",
          "original_form": "그건 카카오페이 잘못이야",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037378.1.20",
          "form": "모두가 그런걸 누릴 수 잌ㅅ어야한다고 생각해",
          "original_form": "모두가 그런걸 누릴 수 잌ㅅ어야한다고 생각해",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037378.1.21",
          "form": "혜택을 너무 숨겨뒀어.",
          "original_form": "혜택을 너무 숨겨뒀어.",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037378.1.22",
          "form": "그러게말이야 다음부터는 구매할때 신중해야겠다",
          "original_form": "그러게말이야 다음부터는 구매할때 신중해야겠다",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037378.1.23",
          "form": "똑똑한 쇼핑에 방해가 돼",
          "original_form": "똑똑한 쇼핑에 방해가 돼",
