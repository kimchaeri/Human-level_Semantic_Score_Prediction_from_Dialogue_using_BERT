      "utterance": [
        {
          "id": "MDRW2100027158.1.1",
          "form": "안녕하세요 최근에 보신 책 있으신가요?",
          "original_form": "안녕하세요 최근에 보신 책 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100027158.1.2",
          "form": "아공 요즘에 책을 잘 안읽어서 ㅠㅠ 시간이 길게 나지않으면 손이 잘 안가더라구요",
          "original_form": "아공 요즘에 책을 잘 안읽어서 ㅠㅠ 시간이 길게 나지않으면 손이 잘 안가더라구요",
          "speaker_id": "2",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100027158.1.3",
          "form": "name1님은 최근에 보신 책 있으세요?",
          "original_form": "&name1&님은 최근에 보신 책 있으세요?",
          "speaker_id": "2",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100027158.1.4",
          "form": "내게 무해한 사람 이라는 책을 봤는데",
          "original_form": "내게 무해한 사람 이라는 책을 봤는데",
          "speaker_id": "1",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100027158.1.5",
          "form": "단편집이라 짧고 재밌더라고요!",
          "original_form": "단편집이라 짧고 재밌더라고요!",
          "speaker_id": "1",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100027158.1.6",
          "form": "단편집이면 좋네요 장편소설은 한번딱 각잡고 ㅋㅋㅋ 읽어야하는데 단편이면 가볍게 보기 좋을고같아요",
          "original_form": "단편집이면 좋네요 장편소설은 한번딱 각잡고 ㅋㅋㅋ 읽어야하는데 단편이면 가볍게 보기 좋을고같아요",
          "speaker_id": "2",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027158.1.7",
          "form": "맞아요 요새는 짧은 책도 많긴 해요",
          "original_form": "맞아요 요새는 짧은 책도 많긴 해요",
          "speaker_id": "1",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100027158.1.8",
          "form": "김초엽작가 아시나요?",
          "original_form": "김초엽작가 아시나요?",
          "speaker_id": "1",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100027158.1.9",
          "form": "알죠!",
          "original_form": "알죠!",
          "speaker_id": "2",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100027158.1.10",
          "form": "우리가 빛의 속도로 갈 수 업ㄱ다면",
          "original_form": "우리가 빛의 속도로 갈 수 업ㄱ다면",
          "speaker_id": "2",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027158.1.11",
          "form": "우리가 빛의 속도로 갈 수 없다면",
          "original_form": "우리가 빛의 속도로 갈 수 없다면",
          "speaker_id": "1",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027158.1.12",
          "form": "오 맞아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 맞아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027158.1.13",
          "form": "저벚에 보려고 샀는데 ! ㅋㅋㅋㅋ",
          "original_form": "저벚에 보려고 샀는데 ! ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027158.1.14",
          "form": "아직 못읽구있어요....",
          "original_form": "아직 못읽구있어요....",
          "speaker_id": "2",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027158.1.15",
          "form": "이 sf단편집도 재밌어요!",
          "original_form": "이 sf단편집도 재밌어요!",
          "speaker_id": "1",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027158.1.16",
          "form": "근데 개인적으로 한번 더 읽고 싶진 않은 그런...",
          "original_form": "근데 개인적으로 한번 더 읽고 싶진 않은 그런...",
          "speaker_id": "1",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027158.1.17",
          "form": "ㅋㅋㅋㅋㅋ 아 그런가요 ㅠㅠㅋㅋㅋㅋㅋㅋㅋㅋㅋ  책 사명서 지구 끝의 온실이라는 책",
          "original_form": "ㅋㅋㅋㅋㅋ 아 그런가요 ㅠㅠㅋㅋㅋㅋㅋㅋㅋㅋㅋ  책 사명서 지구 끝의 온실이라는 책",
          "speaker_id": "2",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027158.1.18",
          "form": "샘플북도와서",
          "original_form": "샘플북도와서",
          "speaker_id": "2",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027158.1.19",
          "form": "이것두 읽어보려는데 혹시 봐 보셨나요?",
          "original_form": "이것두 읽어보려는데 혹시 봐 보셨나요?",
          "speaker_id": "2",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027158.1.20",
          "form": "엇 그거...정유정 작가 거였나??",
          "original_form": "엇 그거...정유정 작가 거였나??",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027158.1.21",
          "form": "아 김초엽...",
          "original_form": "아 김초엽...",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027158.1.22",
          "form": "아뇨!! 저도 나온 소식은 들었은데",
          "original_form": "아뇨!! 저도 나온 소식은 들었은데",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027158.1.23",
          "form": "아직 못 읽었어요 ㅋㅋㅋㅋ",
          "original_form": "아직 못 읽었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027158.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 일단 샘플북부터 봐바야겠네요 ㅋㅋㅋㅋ 담엔 단편집을 사봐야겠어요ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 일단 샘플북부터 봐바야겠네요 ㅋㅋㅋㅋ 담엔 단편집을 사봐야겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027158.1.25",
          "form": "왜 맨날 못읽는거 알면서 장편소설 사는지 ㅠㅠ",
          "original_form": "왜 맨날 못읽는거 알면서 장편소설 사는지 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027158.1.26",
          "form": "ㅋㅋㅋㅋ샘플북이라는 것도 있군요!",
          "original_form": "ㅋㅋㅋㅋ샘플북이라는 것도 있군요!",
          "speaker_id": "1",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027158.1.27",
          "form": "독서하기 좋은 계절인데",
          "original_form": "독서하기 좋은 계절인데",
          "speaker_id": "1",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027158.1.28",
          "form": "설렁설렁 읽어보세요!",
          "original_form": "설렁설렁 읽어보세요!",
          "speaker_id": "1",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027158.1.29",
          "form": "넵! 알라딘에서 책 사면서 이벤트? 하더라구용 !",
          "original_form": "넵! 알라딘에서 책 사면서 이벤트? 하더라구용 !",
          "speaker_id": "2",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027158.1.30",
          "form": "넵넵! 그래야죠 ㅋㅋㅋ 주말에 날 잡구 쌓아둔 책 한번 읽으려구요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "넵넵! 그래야죠 ㅋㅋㅋ 주말에 날 잡구 쌓아둔 책 한번 읽으려구요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
