      "utterance": [
        {
          "id": "MDRW2100023248.1.1",
          "form": "다이어트 함께할분!",
          "original_form": "다이어트 함께할분!",
          "speaker_id": "1",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023248.1.2",
          "form": "전 다이어트 안해용",
          "original_form": "전 다이어트 안해용",
          "speaker_id": "2",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023248.1.3",
          "form": "운동두요?",
          "original_form": "운동두요?",
          "speaker_id": "1",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023248.1.4",
          "form": "운동도 안해요ㅋㅋ",
          "original_form": "운동도 안해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023248.1.5",
          "form": "마른편인가요",
          "original_form": "마른편인가요",
          "speaker_id": "1",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023248.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023248.1.7",
          "form": "비실이?",
          "original_form": "비실이?",
          "speaker_id": "1",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023248.1.8",
          "form": "그렇죠ㅋㅋ 종이인형이죠",
          "original_form": "그렇죠ㅋㅋ 종이인형이죠",
          "speaker_id": "2",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023248.1.9",
          "form": "이번기회에 같이해봐요",
          "original_form": "이번기회에 같이해봐요",
          "speaker_id": "1",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100023248.1.10",
          "form": "근육있는 종이인형밖에 더 되겠어요?",
          "original_form": "근육있는 종이인형밖에 더 되겠어요?",
          "speaker_id": "2",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100023248.1.11",
          "form": "아니에요 엄청 활기있으실걸요",
          "original_form": "아니에요 엄청 활기있으실걸요",
          "speaker_id": "1",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100023248.1.12",
          "form": "그러려나요",
          "original_form": "그러려나요",
          "speaker_id": "2",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100023248.1.13",
          "form": "전 코첼라직캠보고 로제 몸매에 반했어요",
          "original_form": "전 코첼라직캠보고 로제 몸매에 반했어요",
          "speaker_id": "1",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023248.1.14",
          "form": "아  레전드죠",
          "original_form": "아  레전드죠",
          "speaker_id": "2",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023248.1.15",
          "form": "그쵸ㅜㅜ",
          "original_form": "그쵸ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023248.1.16",
          "form": "엄청 말랐다던데",
          "original_form": "엄청 말랐다던데",
          "speaker_id": "2",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023248.1.17",
          "form": "근데 근력이 있어보여요",
          "original_form": "근데 근력이 있어보여요",
          "speaker_id": "1",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023248.1.18",
          "form": "아무래도 춤을 추니까요",
          "original_form": "아무래도 춤을 추니까요",
          "speaker_id": "2",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023248.1.19",
          "form": "부러우면 나도 해야죠",
          "original_form": "부러우면 나도 해야죠",
          "speaker_id": "1",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023248.1.20",
          "form": "힝내세용",
          "original_form": "힝내세용",
          "speaker_id": "2",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023248.1.21",
          "form": "감사합니다ㅋㅋ함께해요",
          "original_form": "감사합니다ㅋㅋ함께해요",
          "speaker_id": "1",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023248.1.22",
          "form": "저는 고민좀...",
          "original_form": "저는 고민좀...",
          "speaker_id": "2",
          "time": "20210829 23:22"
        }
