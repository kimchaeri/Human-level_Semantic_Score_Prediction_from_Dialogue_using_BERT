      "utterance": [
        {
          "id": "MDRW2100014038.1.1",
          "form": "쇼핑 자주하시나요?",
          "original_form": "쇼핑 자주하시나요?",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014038.1.2",
          "form": "완전 자주해여 ㅋㅋㅋㅋ",
          "original_form": "완전 자주해여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100014038.1.3",
          "form": "저는 별로 안해서요ㅋㅋ큐ㅠ",
          "original_form": "저는 별로 안해서요ㅋㅋ큐ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100014038.1.4",
          "form": "마지막으로 사신거 뭐였어요?",
          "original_form": "마지막으로 사신거 뭐였어요?",
          "speaker_id": "1",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100014038.1.5",
          "form": "볼펜이여 ㅋㅋㅋㅋ",
          "original_form": "볼펜이여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100014038.1.6",
          "form": "저는 약간 짜잘한걸 많이사서 ㅋㅋㅋㅋ",
          "original_form": "저는 약간 짜잘한걸 많이사서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100014038.1.7",
          "form": "저는 손소독제....ㅋㅋㅋㅋㅋ",
          "original_form": "저는 손소독제....ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100014038.1.8",
          "form": "손소독제 받은것도 안써서 버렸는데",
          "original_form": "손소독제 받은것도 안써서 버렸는데",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100014038.1.9",
          "form": "티파니 왓츠인마이백에서 소개해준 탬버린즈 너무 써보고 싶어서 어제샀어요",
          "original_form": "티파니 왓츠인마이백에서 소개해준 탬버린즈 너무 써보고 싶어서 어제샀어요",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100014038.1.10",
          "form": "오 좋아여???",
          "original_form": "오 좋아여???",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100014038.1.11",
          "form": "아직안왔오요ㅠㅍ",
          "original_form": "아직안왔오요ㅠㅍ",
          "speaker_id": "1",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100014038.1.12",
          "form": "아 어제 삿다고 하셧져",
          "original_form": "아 어제 삿다고 하셧져",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014038.1.13",
          "form": "택배는 진짜 오기 전이 제일 설레져",
          "original_form": "택배는 진짜 오기 전이 제일 설레져",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014038.1.14",
          "form": "기다리시는거있으세요?!",
          "original_form": "기다리시는거있으세요?!",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014038.1.15",
          "form": "냉동식품 시켰는데 아직 안와서 기다리고 있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "냉동식품 시켰는데 아직 안와서 기다리고 있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014038.1.16",
          "form": "구매할때 어느사이트 잘 쓰세요?",
          "original_form": "구매할때 어느사이트 잘 쓰세요?",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014038.1.17",
          "form": "완전 여러군데 써요",
          "original_form": "완전 여러군데 써요",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014038.1.18",
          "form": "티몬 11번가",
          "original_form": "티몬 11번가",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014038.1.19",
          "form": "주로 많이 쓰는서 같아여",
          "original_form": "주로 많이 쓰는서 같아여",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014038.1.20",
          "form": "아 저는 네이버에서 많이사는것같아요",
          "original_form": "아 저는 네이버에서 많이사는것같아요",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014038.1.21",
          "form": "페이 적립이 되어서요",
          "original_form": "페이 적립이 되어서요",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014038.1.22",
          "form": "네이버도 좋져",
          "original_form": "네이버도 좋져",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014038.1.23",
          "form": "페이 모으는 재미 ㅋㅋㅋ",
          "original_form": "페이 모으는 재미 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014038.1.24",
          "form": "근데 후기쓰는 게 좀 긔찮은것같아요",
          "original_form": "근데 후기쓰는 게 좀 긔찮은것같아요",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014038.1.25",
          "form": "진짜 연초에만해도 배송받을때마다 다 썼는데",
          "original_form": "진짜 연초에만해도 배송받을때마다 다 썼는데",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014038.1.26",
          "form": "그럼 그 카드 있으세여? 신한카드.네이버페이 적립되는거",
          "original_form": "그럼 그 카드 있으세여? 신한카드.네이버페이 적립되는거",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014038.1.27",
          "form": "아 아니요!",
          "original_form": "아 아니요!",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014038.1.28",
          "form": "그거 참 좋은뎁 단종됏더라고요 ㅠㅠㅠ",
          "original_form": "그거 참 좋은뎁 단종됏더라고요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014038.1.29",
          "form": "혜택이 정말 쎘나봐요",
          "original_form": "혜택이 정말 쎘나봐요",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014038.1.30",
          "form": "사고싶은 물건은 있으세요?",
          "original_form": "사고싶은 물건은 있으세요?",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014038.1.31",
          "form": "네일이여",
          "original_form": "네일이여",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014038.1.32",
          "form": "오 오호라요?",
          "original_form": "오 오호라요?",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014038.1.33",
          "form": "손톱이 짧아서 붙이는거 말고 그냥 제가 바르는게 좋아여 ㅋㅋㅋ",
          "original_form": "손톱이 짧아서 붙이는거 말고 그냥 제가 바르는게 좋아여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014038.1.34",
          "form": "그러시군요 저는 냄새때문에 늘 스티커 류로 아 네 사요ㅎㅎ",
          "original_form": "그러시군요 저는 냄새때문에 늘 스티커 류로 아 네 사요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014038.1.35",
          "form": "스티커가 예쁘긴한데 오래 가여??",
          "original_form": "스티커가 예쁘긴한데 오래 가여??",
          "speaker_id": "2",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014038.1.36",
          "form": "아 일~이주 가는것 같아요 근데 잘 떨어지는건 이 삼일이면 떨어져요ㅠ",
          "original_form": "아 일~이주 가는것 같아요 근데 잘 떨어지는건 이 삼일이면 떨어져요ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014038.1.37",
          "form": "중고거래는 잘하시나요?",
          "original_form": "중고거래는 잘하시나요?",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014038.1.38",
          "form": "중고거래는 거의 안해여",
          "original_form": "중고거래는 거의 안해여",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014038.1.39",
          "form": "저는 당근 가끔 써요",
          "original_form": "저는 당근 가끔 써요",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014038.1.40",
          "form": "저도 딱한번 해봤는데",
          "original_form": "저도 딱한번 해봤는데",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014038.1.41",
          "form": "만나러 나가는게 너무 귀찮더라고요",
          "original_form": "만나러 나가는게 너무 귀찮더라고요",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014038.1.42",
          "form": "아 그렇긴해요ㅠㅠ!",
          "original_form": "아 그렇긴해요ㅠㅠ!",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014038.1.43",
          "form": "그리고 약간 만나기 전에 그 뻘쭘한 눈치싸움이 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그리고 약간 만나기 전에 그 뻘쭘한 눈치싸움이 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014038.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅠㅠ마자요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅠㅠ마자요",
          "speaker_id": "1",
          "time": "20210825 17:46"
        }
