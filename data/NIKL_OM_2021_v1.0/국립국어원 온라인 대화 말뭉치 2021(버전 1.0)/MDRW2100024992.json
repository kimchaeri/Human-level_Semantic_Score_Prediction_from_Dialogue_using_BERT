      "utterance": [
        {
          "id": "MDRW2100024992.1.1",
          "form": "최애 배달음식이머야",
          "original_form": "최애 배달음식이머야",
          "speaker_id": "1",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024992.1.2",
          "form": "정답 찜닭",
          "original_form": "정답 찜닭",
          "speaker_id": "1",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024992.1.3",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ 그건 이제 질렸다",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ 그건 이제 질렸다",
          "speaker_id": "2",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024992.1.4",
          "form": "요즘은 치사떡임",
          "original_form": "요즘은 치사떡임",
          "speaker_id": "2",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024992.1.5",
          "form": "곱창올려서",
          "original_form": "곱창올려서",
          "speaker_id": "2",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024992.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ아 새로운메뉴가 올라갓군",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 새로운메뉴가 올라갓군",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024992.1.7",
          "form": "곱창...",
          "original_form": "곱창...",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024992.1.8",
          "form": "님 최애는...치킨?",
          "original_form": "님 최애는...치킨?",
          "speaker_id": "2",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024992.1.9",
          "form": "네 저는 치킨입니다",
          "original_form": "네 저는 치킨입니다",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024992.1.10",
          "form": "어제도 치킨을 먹엇어요",
          "original_form": "어제도 치킨을 먹엇어요",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024992.1.11",
          "form": "그 치즈볼이 굽네였나",
          "original_form": "그 치즈볼이 굽네였나",
          "speaker_id": "2",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100024992.1.12",
          "form": "bhc였나",
          "original_form": "bhc였나",
          "speaker_id": "2",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100024992.1.13",
          "form": "네네?...",
          "original_form": "네네?...",
          "speaker_id": "2",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100024992.1.14",
          "form": "치즈볼맛집은 비에이치시!",
          "original_form": "치즈볼맛집은 비에이치시!",
          "speaker_id": "1",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100024992.1.15",
          "form": "난여즘 바른치킨 조아하는데",
          "original_form": "난여즘 바른치킨 조아하는데",
          "speaker_id": "1",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100024992.1.16",
          "form": "아하 어제는 어디먹었어",
          "original_form": "아하 어제는 어디먹었어",
          "speaker_id": "2",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100024992.1.17",
          "form": "새우랑 매운양념 미쳐따",
          "original_form": "새우랑 매운양념 미쳐따",
          "speaker_id": "1",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100024992.1.18",
          "form": "어제도 바른치킨 먹고팟는데",
          "original_form": "어제도 바른치킨 먹고팟는데",
          "speaker_id": "1",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100024992.1.19",
          "form": "ㅋㅋㅋ먹고보고 싶긴 한데",
          "original_form": "ㅋㅋㅋ먹고보고 싶긴 한데",
          "speaker_id": "2",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100024992.1.20",
          "form": "휴무날이라 그냥 푸라닭..",
          "original_form": "휴무날이라 그냥 푸라닭..",
          "speaker_id": "1",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024992.1.21",
          "form": "아..푸라닭도 괜찮지",
          "original_form": "아..푸라닭도 괜찮지",
          "speaker_id": "2",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024992.1.22",
          "form": "마자 몬가 튀김은 부담스러울때 딱이여",
          "original_form": "마자 몬가 튀김은 부담스러울때 딱이여",
          "speaker_id": "1",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024992.1.23",
          "form": "근데 칼로리는 부담인거같여",
          "original_form": "근데 칼로리는 부담인거같여",
          "speaker_id": "1",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024992.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024992.1.25",
          "form": "맞엉ㅇㅇㅋㅋ",
          "original_form": "맞엉ㅇㅇㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024992.1.26",
          "form": "회사앞에 땅땅치킨 생겼는데",
          "original_form": "회사앞에 땅땅치킨 생겼는데",
          "speaker_id": "2",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024992.1.27",
          "form": "내일부터 4일동안 4천원 할인...",
          "original_form": "내일부터 4일동안 4천원 할인...",
          "speaker_id": "2",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024992.1.28",
          "form": "3번세트각인데",
          "original_form": "3번세트각인데",
          "speaker_id": "2",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024992.1.29",
          "form": "와 땅땅 미쳐따",
          "original_form": "와 땅땅 미쳐따",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024992.1.30",
          "form": "와 그러면 한번은 사먹어야되는데?",
          "original_form": "와 그러면 한번은 사먹어야되는데?",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024992.1.31",
          "form": "그러니까",
