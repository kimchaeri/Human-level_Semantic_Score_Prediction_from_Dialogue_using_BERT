        },
        {
          "id": "MDRW2100005303.1.2",
          "form": "전 요즘 프랜치토스트 만드는데",
          "original_form": "전 요즘 프랜치토스트 만드는데",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005303.1.3",
          "form": "이렇개요?",
          "original_form": "이렇개요?",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005303.1.4",
          "form": "재미를 들렸습니다",
          "original_form": "재미를 들렸습니다",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005303.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005303.1.6",
          "form": "저도 만들어주새요",
          "original_form": "저도 만들어주새요",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005303.1.7",
          "form": "래시피를 공개하시죠",
          "original_form": "래시피를 공개하시죠",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005303.1.8",
          "form": "반나절이나 재워야해요",
          "original_form": "반나절이나 재워야해요",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005303.1.9",
          "form": "순순히 레시피를 내놓으시지",
          "original_form": "순순히 레시피를 내놓으시지",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005303.1.10",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005303.1.11",
          "form": "그렇게 오래자요?",
          "original_form": "그렇게 오래자요?",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005303.1.12",
          "form": "사실 밤새 넣어두면 빵이 다 녹아버리는거아닌가 싶었지만",
          "original_form": "사실 밤새 넣어두면 빵이 다 녹아버리는거아닌가 싶었지만",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005303.1.13",
          "form": "의외로 형체를 유지하더라구요",
          "original_form": "의외로 형체를 유지하더라구요",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005303.1.14",
          "form": "전자레인지쓰는 방법도 있다는데",
          "original_form": "전자레인지쓰는 방법도 있다는데",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005303.1.15",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005303.1.16",
          "form": "식빵계란찜 만들어먹음",
          "original_form": "식빵계란찜 만들어먹음",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005303.1.17",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005303.1.18",
          "form": "잘자면 더 부드럽나요",
          "original_form": "잘자면 더 부드럽나요",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005303.1.19",
          "form": "아 녹는구나",
          "original_form": "아 녹는구나",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005303.1.20",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005303.1.21",
          "form": "네 촉촉해요",
          "original_form": "네 촉촉해요",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005303.1.22",
          "form": "촉촉~ 겉바속촉",
          "original_form": "촉촉~ 겉바속촉",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005303.1.23",
          "form": "맛있으면 장땡이죠",
          "original_form": "맛있으면 장땡이죠",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005303.1.24",
          "form": "겉바는 굽기실력이라",
          "original_form": "겉바는 굽기실력이라",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005303.1.25",
          "form": "저도 식빵먹고싶어요",
          "original_form": "저도 식빵먹고싶어요",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005303.1.26",
          "form": "제건 겉...속촉",
          "original_form": "제건 겉...속촉",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005303.1.27",
          "form": "바삭바삭..",
          "original_form": "바삭바삭..",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005303.1.28",
          "form": "식빵종류 좋아하시나요",
          "original_form": "식빵종류 좋아하시나요",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005303.1.29",
          "form": "촉촉한 사람이시군요",
          "original_form": "촉촉한 사람이시군요",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005303.1.30",
          "form": "전 요런거좋아해서",
          "original_form": "전 요런거좋아해서",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005303.1.31",
          "form": "흠..",
          "original_form": "흠..",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005303.1.32",
          "form": "모닌빵",
          "original_form": "모닌빵",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005303.1.33",
          "form": "빵은 잘 안먹는데",
          "original_form": "빵은 잘 안먹는데",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005303.1.34",
          "form": "시 ㄱ 빵이 금지어라니",
          "original_form": "시 ㄱ 빵이 금지어라니",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005303.1.35",
          "form": "토스트는 러브",
          "original_form": "토스트는 러브",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005303.1.36",
          "form": "에?",
          "original_form": "에?",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005303.1.37",
          "form": "안보내져요ㅠ",
          "original_form": "안보내져요ㅠ",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005303.1.38",
          "form": "충격적",
          "original_form": "충격적",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005303.1.39",
          "form": "토스트... 전 샌드위치파",
          "original_form": "토스트... 전 샌드위치파",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005303.1.40",
          "form": "깐깐하네요..",
          "original_form": "깐깐하네요..",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005303.1.41",
          "form": "섭웨 vs 이삭",
          "original_form": "섭웨 vs 이삭",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005303.1.42",
          "form": "샌드위치도 굳..",
          "original_form": "샌드위치도 굳..",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005303.1.43",
          "form": "하지만 전 기름이",
          "original_form": "하지만 전 기름이",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005303.1.44",
          "form": "저 이삭",
          "original_form": "저 이삭",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005303.1.45",
          "form": "안먹어봄",
          "original_form": "안먹어봄",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005303.1.46",
          "form": "두둥",
          "original_form": "두둥",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005303.1.47",
          "form": "충격이죠",
          "original_form": "충격이죠",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005303.1.48",
          "form": "글케 멋잇다는데",
          "original_form": "글케 멋잇다는데",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005303.1.49",
          "form": "나만 못먹어봤어",
          "original_form": "나만 못먹어봤어",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005303.1.50",
          "form": "이케 이삭상~!",
          "original_form": "이케 이삭상~!",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005303.1.51",
          "form": "헉 이상한말했다",
          "original_form": "헉 이상한말했다",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005303.1.52",
          "form": "모른척",
          "original_form": "모른척",
          "speaker_id": "1",
          "time": "20210821 21:47"
        }
      ]
    }
