        {
          "id": "MDRW2100006857.1.1",
          "form": "어릴적에 엄마가 마시던걸 기억행",
          "original_form": "어릴적에 엄마가 마시던걸 기억행",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006857.1.2",
          "form": "배합이 훌륭했죠",
          "original_form": "배합이 훌륭했죠",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006857.1.3",
          "form": "커피, 프림, 설탕이 취햐에 따라 달랐지요",
          "original_form": "커피, 프림, 설탕이 취햐에 따라 달랐지요",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006857.1.4",
          "form": "기억나나요?",
          "original_form": "기억나나요?",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006857.1.5",
          "form": "엄마는 2, 2, 2이었어요 ㅋㅋ",
          "original_form": "엄마는 2, 2, 2이었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100006857.1.6",
          "form": "저는 그 프리마가 넘 맛있었어요",
          "original_form": "저는 그 프리마가 넘 맛있었어요",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100006857.1.7",
          "form": "탈지분유 같은 건데 고소하고 달았지요",
          "original_form": "탈지분유 같은 건데 고소하고 달았지요",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100006857.1.8",
          "form": "그거 한스푼 어찌나 먹고싶던지",
          "original_form": "그거 한스푼 어찌나 먹고싶던지",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100006857.1.9",
          "form": "저도요 그래서 아메리카노는 썼어요",
          "original_form": "저도요 그래서 아메리카노는 썼어요",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100006857.1.10",
          "form": "네 첨엔요",
          "original_form": "네 첨엔요",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100006857.1.11",
          "form": "에스프레소는 아직 못먹어요",
          "original_form": "에스프레소는 아직 못먹어요",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100006857.1.12",
          "form": "라테도 좋고요",
          "original_form": "라테도 좋고요",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100006857.1.13",
          "form": "마끼아또 카라멜 예전에 매일 마셨어요",
          "original_form": "마끼아또 카라멜 예전에 매일 마셨어요",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100006857.1.14",
          "form": "부산 돼지국밥 맛집은 어디지요?",
          "original_form": "부산 돼지국밥 맛집은 어디지요?",
