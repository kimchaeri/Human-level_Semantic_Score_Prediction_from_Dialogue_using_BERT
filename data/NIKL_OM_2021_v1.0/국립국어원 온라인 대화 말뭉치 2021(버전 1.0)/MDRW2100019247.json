      "utterance": [
        {
          "id": "MDRW2100019247.1.1",
          "form": "혹시 요즘 위시템 있으신가요??",
          "original_form": "혹시 요즘 위시템 있으신가요??",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019247.1.2",
          "form": "음...저는",
          "original_form": "음...저는",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019247.1.3",
          "form": "필카 가지고 싶어여!!!",
          "original_form": "필카 가지고 싶어여!!!",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019247.1.4",
          "form": "오늘 이렇게 대화했는데",
          "original_form": "오늘 이렇게 대화했는데",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019247.1.5",
          "form": "벌써 뭔가 친해진느낌,,,,ㅌㅋㅋㅋㅋ",
          "original_form": "벌써 뭔가 친해진느낌,,,,ㅌㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019247.1.6",
          "form": "필카! 저두 갖고싶어요 ㅋㅋ 근데 막상 사면 별로안쓸거같아서 위시템까지는 아닌아이템 ㅋㅋ",
          "original_form": "필카! 저두 갖고싶어요 ㅋㅋ 근데 막상 사면 별로안쓸거같아서 위시템까지는 아닌아이템 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019247.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ 그러게요 !! 이렇게 잘맞는분걸려서 기분좋아요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그러게요 !! 이렇게 잘맞는분걸려서 기분좋아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019247.1.8",
          "form": "아잌ㅋㅋㅋㅋ 그니까여",
          "original_form": "아잌ㅋㅋㅋㅋ 그니까여",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019247.1.9",
          "form": "저두 친구가 도와달라고래서",
          "original_form": "저두 친구가 도와달라고래서",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019247.1.10",
          "form": "앱깔았다가",
          "original_form": "앱깔았다가",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019247.1.11",
          "form": "이렇게 좋은분만나서너무 좋아여!",
          "original_form": "이렇게 좋은분만나서너무 좋아여!",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019247.1.12",
          "form": "사실 다른 톡오는거 다 차단하고있거든여,,, ㅋㅋㅋㅌㅋ",
          "original_form": "사실 다른 톡오는거 다 차단하고있거든여,,, ㅋㅋㅋㅌㅋ",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019247.1.13",
          "form": "앜ㅋㅋㅋㅋ 저두 요새 친구들하고 연락하는거에 회의감?같은거느껴서 톡 자제중이에요 ㅜㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ 저두 요새 친구들하고 연락하는거에 회의감?같은거느껴서 톡 자제중이에요 ㅜㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019247.1.14",
          "form": "친구분 부러워요! 전 동생이 도와달래서 깔았어요 ㅋㅋ",
          "original_form": "친구분 부러워요! 전 동생이 도와달래서 깔았어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019247.1.15",
          "form": "아아아",
          "original_form": "아아아",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019247.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 이러다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 이러다",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019247.1.17",
          "form": "저희둘이서",
          "original_form": "저희둘이서",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019247.1.18",
          "form": "채워보는것도,,,(?)ㅋㅋㅋㅋ",
          "original_form": "채워보는것도,,,(?)ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019247.1.19",
          "form": "좋아요! 해보자구요!! ㅋㅋㅋ",
          "original_form": "좋아요! 해보자구요!! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100019247.1.20",
          "form": "참 쇼핑에관해서 얘기해야하네요",
          "original_form": "참 쇼핑에관해서 얘기해야하네요",
          "speaker_id": "1",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100019247.1.21",
          "form": "조아여!!!",
          "original_form": "조아여!!!",
          "speaker_id": "2",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100019247.1.22",
          "form": "필카,,,저는 사진찍는게취미라",
          "original_form": "필카,,,저는 사진찍는게취미라",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019247.1.23",
          "form": "꼭살거에여",
          "original_form": "꼭살거에여",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019247.1.24",
          "form": "아하! 어디꺼 노리고계신가요?",
          "original_form": "아하! 어디꺼 노리고계신가요?",
          "speaker_id": "1",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019247.1.25",
          "form": "일회용 필카는있는데",
          "original_form": "일회용 필카는있는데",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019247.1.26",
          "form": "그거말구 좋은거,,,코닥!!",
          "original_form": "그거말구 좋은거,,,코닥!!",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019247.1.27",
          "form": "역시 코닥 ㅋㅋ",
          "original_form": "역시 코닥 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019247.1.28",
          "form": "얼마전에 의류도 낸거같던데",
          "original_form": "얼마전에 의류도 낸거같던데",
          "speaker_id": "1",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019247.1.29",
          "form": "우엉 ....멋지다",
          "original_form": "우엉 ....멋지다",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019247.1.30",
          "form": "소비자들에게 사라고하는 그런느낌",
          "original_form": "소비자들에게 사라고하는 그런느낌",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019247.1.31",
          "form": "맞아요 ㅋㅋㅋ 그 코닥만의 노란색이 특징이더라구요",
          "original_form": "맞아요 ㅋㅋㅋ 그 코닥만의 노란색이 특징이더라구요",
