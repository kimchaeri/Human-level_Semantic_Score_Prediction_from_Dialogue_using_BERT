      "utterance": [
        {
          "id": "MDRW2100010108.1.1",
          "form": "달리기 할 때 핸드폰은 어떻게 해",
          "original_form": "달리기 할 때 핸드폰은 어떻게 해",
          "speaker_id": "2",
          "time": "20210823 14:27"
        },
        {
          "id": "MDRW2100010108.1.2",
          "form": "들고 달려?",
          "original_form": "들고 달려?",
          "speaker_id": "2",
          "time": "20210823 14:27"
        },
        {
          "id": "MDRW2100010108.1.3",
          "form": "ㅇㅇㅇ바지에",
          "original_form": "ㅇㅇㅇ바지에",
          "speaker_id": "1",
          "time": "20210823 14:27"
        },
        {
          "id": "MDRW2100010108.1.4",
          "form": "주머니없으면들고달리고",
          "original_form": "주머니없으면들고달리고",
          "speaker_id": "1",
          "time": "20210823 14:27"
        },
        {
          "id": "MDRW2100010108.1.5",
          "form": "있으면주머니에넣어서달림",
          "original_form": "있으면주머니에넣어서달림",
          "speaker_id": "1",
          "time": "20210823 14:27"
        },
        {
          "id": "MDRW2100010108.1.6",
          "form": "귀찮긴함",
          "original_form": "귀찮긴함",
          "speaker_id": "1",
          "time": "20210823 14:27"
        },
        {
          "id": "MDRW2100010108.1.7",
          "form": "아 그럼 덜렁거리지 않아?",
          "original_form": "아 그럼 덜렁거리지 않아?",
          "speaker_id": "2",
          "time": "20210823 14:28"
        },
        {
          "id": "MDRW2100010108.1.8",
          "form": "그래도 그냥 넣고 달려?",
          "original_form": "그래도 그냥 넣고 달려?",
          "speaker_id": "2",
          "time": "20210823 14:28"
        },
        {
          "id": "MDRW2100010108.1.9",
          "form": "초반에는거슬리는데",
          "original_form": "초반에는거슬리는데",
          "speaker_id": "1",
          "time": "20210823 14:28"
        },
        {
          "id": "MDRW2100010108.1.10",
          "form": "달리다보면",
          "original_form": "달리다보면",
          "speaker_id": "1",
          "time": "20210823 14:28"
        },
        {
          "id": "MDRW2100010108.1.11",
          "form": "힘들어서그런거생각할",
          "original_form": "힘들어서그런거생각할",
          "speaker_id": "1",
          "time": "20210823 14:28"
        },
        {
          "id": "MDRW2100010108.1.12",
          "form": "체력이없음",
          "original_form": "체력이없음",
          "speaker_id": "1",
          "time": "20210823 14:28"
        },
        {
          "id": "MDRW2100010108.1.13",
          "form": "그리고주머니마다약간다를듯",
          "original_form": "그리고주머니마다약간다를듯",
          "speaker_id": "1",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010108.1.14",
          "form": "앜ㅌㅋㅋㅌㅌㅋ 아 그거에 신경쓸 여력이 없어지는 구나",
          "original_form": "앜ㅌㅋㅋㅌㅌㅋ 아 그거에 신경쓸 여력이 없어지는 구나",
          "speaker_id": "2",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010108.1.15",
          "form": "그렇겠네 달라붙는 주머니도 있으니까",
          "original_form": "그렇겠네 달라붙는 주머니도 있으니까",
          "speaker_id": "2",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010108.1.16",
          "form": "ㅇㅇㅇ그리고주머니에넣을거면",
          "original_form": "ㅇㅇㅇ그리고주머니에넣을거면",
          "speaker_id": "1",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010108.1.17",
          "form": "좀깊은주머니가좋아",
          "original_form": "좀깊은주머니가좋아",
          "speaker_id": "1",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010108.1.18",
          "form": "그래야안빠지고좋음",
          "original_form": "그래야안빠지고좋음",
          "speaker_id": "1",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100010108.1.19",
          "form": "근데너백신언제맞음?",
          "original_form": "근데너백신언제맞음?",
          "speaker_id": "1",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100010108.1.20",
          "form": "1차",
          "original_form": "1차",
          "speaker_id": "1",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100010108.1.21",
          "form": "아 나 백신 9월 3일에",
          "original_form": "아 나 백신 9월 3일에",
          "speaker_id": "2",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100010108.1.22",
          "form": "학기 시작하고 이틀있다가 맞아",
          "original_form": "학기 시작하고 이틀있다가 맞아",
          "speaker_id": "2",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100010108.1.23",
          "form": "그러면2차는10월쯤에맞겠네",
          "original_form": "그러면2차는10월쯤에맞겠네",
          "speaker_id": "1",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100010108.1.24",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100010108.1.25",
          "form": "아 그럴 걸 아마",
          "original_form": "아 그럴 걸 아마",
          "speaker_id": "2",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010108.1.26",
          "form": "화이자어니면모더나,",
          "original_form": "화이자어니면모더나,",
          "speaker_id": "1",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010108.1.27",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010108.1.28",
          "form": "아마 모더나 아닐까",
          "original_form": "아마 모더나 아닐까",
          "speaker_id": "2",
          "time": "20210823 14:33"
        },
        {
          "id": "MDRW2100010108.1.29",
          "form": "아 아직 안 나왔어?",
          "original_form": "아 아직 안 나왔어?",
          "speaker_id": "1",
          "time": "20210823 14:33"
        },
        {
          "id": "MDRW2100010108.1.30",
          "form": "응 아직 안나와ㅛ더라고",
          "original_form": "응 아직 안나와ㅛ더라고",
          "speaker_id": "2",
          "time": "20210823 14:34"
        },
        {
          "id": "MDRW2100010108.1.31",
          "form": "둘 중에 하나인 것 밖에 몰라",
          "original_form": "둘 중에 하나인 것 밖에 몰라",
          "speaker_id": "2",
          "time": "20210823 14:34"
        },
        {
          "id": "MDRW2100010108.1.32",
          "form": "그건언제알려줘?",
          "original_form": "그건언제알려줘?",
          "speaker_id": "1",
          "time": "20210823 14:34"
        },
        {
          "id": "MDRW2100010108.1.33",
          "form": "맞기직전에알려주려나",
          "original_form": "맞기직전에알려주려나",
          "speaker_id": "1",
          "time": "20210823 14:34"
        },
        {
          "id": "MDRW2100010108.1.34",
          "form": "오 모르겠는데",
          "original_form": "오 모르겠는데",
          "speaker_id": "2",
          "time": "20210823 14:34"
        },
        {
          "id": "MDRW2100010108.1.35",
          "form": "그렇지 않을까",
          "original_form": "그렇지 않을까",
          "speaker_id": "2",
          "time": "20210823 14:34"
        }
      ]
    }
