      "utterance": [
        {
          "id": "MDRW2100003208.1.1",
          "form": "요새 운동하는거 있으세요?",
          "original_form": "요새 운동하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210820 20:33"
        },
        {
          "id": "MDRW2100003208.1.2",
          "form": "워킹패드로 걷기하고잇습니다ㅎ name1님은요?",
          "original_form": "워킹패드로 걷기하고잇습니다ㅎ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210820 20:33"
        },
        {
          "id": "MDRW2100003208.1.3",
          "form": "우와 신기해요 워킹패드",
          "original_form": "우와 신기해요 워킹패드",
          "speaker_id": "1",
          "time": "20210820 20:34"
        },
        {
          "id": "MDRW2100003208.1.4",
          "form": "저도 날씨 좋아지면 슬슬 런데이 하려구요!",
          "original_form": "저도 날씨 좋아지면 슬슬 런데이 하려구요!",
          "speaker_id": "1",
          "time": "20210820 20:34"
        },
        {
          "id": "MDRW2100003208.1.5",
          "form": "집에서 런닝머신보다 자리를 덜 차지해요ㅎㅎ\r\n달리기로 다이어트하시나요?",
          "original_form": "집에서 런닝머신보다 자리를 덜 차지해요ㅎㅎ\r\n달리기로 다이어트하시나요?",
          "speaker_id": "2",
          "time": "20210820 20:34"
        },
        {
          "id": "MDRW2100003208.1.6",
          "form": "네 다이어트 겸 체력을 위해서...",
          "original_form": "네 다이어트 겸 체력을 위해서...",
          "speaker_id": "1",
          "time": "20210820 20:35"
        },
        {
          "id": "MDRW2100003208.1.7",
          "form": "날씨 너무 더우면 나가기가 싫더라구요",
          "original_form": "날씨 너무 더우면 나가기가 싫더라구요",
          "speaker_id": "1",
          "time": "20210820 20:35"
        },
        {
          "id": "MDRW2100003208.1.8",
          "form": "다이어트로 젤마니뺀건 몇키로에요?",
          "original_form": "다이어트로 젤마니뺀건 몇키로에요?",
          "speaker_id": "2",
          "time": "20210820 20:35"
        },
        {
          "id": "MDRW2100003208.1.9",
          "form": "저는 5키로 정도 빼봤어요!",
          "original_form": "저는 5키로 정도 빼봤어요!",
          "speaker_id": "1",
          "time": "20210820 20:36"
        },
        {
          "id": "MDRW2100003208.1.10",
          "form": "진짜 힘들었어요ㅠㅠ",
          "original_form": "진짜 힘들었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 20:36"
        },
        {
          "id": "MDRW2100003208.1.11",
          "form": "맞아요 찌는건 빠른데 빼는건힘들어요\r\n유지는하고계신가요?",
          "original_form": "맞아요 찌는건 빠른데 빼는건힘들어요\r\n유지는하고계신가요?",
          "speaker_id": "2",
          "time": "20210820 20:37"
        },
        {
          "id": "MDRW2100003208.1.12",
          "form": "조금 쪘어요",
          "original_form": "조금 쪘어요",
          "speaker_id": "1",
          "time": "20210820 20:37"
        },
        {
          "id": "MDRW2100003208.1.13",
          "form": "요새 입맛이 너무 좋아가지구 잘 먹어요",
          "original_form": "요새 입맛이 너무 좋아가지구 잘 먹어요",
          "speaker_id": "1",
          "time": "20210820 20:37"
        },
        {
          "id": "MDRW2100003208.1.14",
          "form": "혹시 해보고 싶은 운동 있으세요?",
          "original_form": "혹시 해보고 싶은 운동 있으세요?",
          "speaker_id": "1",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003208.1.15",
          "form": "저 테니스해보고싶어요ㅎㅎ\r\n테니스칠줄아세요?",
          "original_form": "저 테니스해보고싶어요ㅎㅎ\r\n테니스칠줄아세요?",
          "speaker_id": "2",
          "time": "20210820 20:41"
        },
        {
          "id": "MDRW2100003208.1.16",
          "form": "아뇨 한 번도 안해봤어요",
          "original_form": "아뇨 한 번도 안해봤어요",
          "speaker_id": "1",
          "time": "20210820 20:43"
        },
        {
          "id": "MDRW2100003208.1.17",
          "form": "뭔가 힘이 빡 생길 거 같아요 테니스하면ㅎㅎ",
          "original_form": "뭔가 힘이 빡 생길 거 같아요 테니스하면ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 20:43"
        },
        {
          "id": "MDRW2100003208.1.18",
          "form": "꼭한번해보고싶은 운동이랍니다ㅎㅎ",
          "original_form": "꼭한번해보고싶은 운동이랍니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 20:50"
        },
        {
          "id": "MDRW2100003208.1.19",
          "form": "저는 클라이밍 배워보고 싶어요",
          "original_form": "저는 클라이밍 배워보고 싶어요",
          "speaker_id": "1",
          "time": "20210820 20:52"
        },
        {
          "id": "MDRW2100003208.1.20",
          "form": "올림픽에서 보는데 진짜 멋있더라구요",
          "original_form": "올림픽에서 보는데 진짜 멋있더라구요",
