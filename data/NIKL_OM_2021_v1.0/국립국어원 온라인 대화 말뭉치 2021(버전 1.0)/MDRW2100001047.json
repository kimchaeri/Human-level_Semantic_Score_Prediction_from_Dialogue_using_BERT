        {
          "id": "MDRW2100001047.1.1",
          "form": "배달 자주 시켜 먹어??",
          "original_form": "배달 자주 시켜 먹어??",
          "speaker_id": "2",
          "time": "20210722 16:33"
        },
        {
          "id": "MDRW2100001047.1.2",
          "form": "거의맨날 시켜먹어 ㅋㅋ",
          "original_form": "거의맨날 시켜먹어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210722 16:34"
        },
        {
          "id": "MDRW2100001047.1.3",
          "form": "아 ㅌㅌㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅌㅌㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210722 16:34"
        },
        {
          "id": "MDRW2100001047.1.4",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210722 16:35"
        },
        {
          "id": "MDRW2100001047.1.5",
          "form": "뭐를 가장 많이시켜먹오??",
          "original_form": "뭐를 가장 많이시켜먹오??",
          "speaker_id": "2",
          "time": "20210722 16:35"
        },
        {
          "id": "MDRW2100001047.1.6",
          "form": "나는 한달에 3~4번 가족들이랑 먹어",
          "original_form": "나는 한달에 3~4번 가족들이랑 먹어",
          "speaker_id": "2",
          "time": "20210722 16:35"
        },
        {
          "id": "MDRW2100001047.1.7",
          "form": "도시락같은거!",
          "original_form": "도시락같은거!",
          "speaker_id": "1",
          "time": "20210722 16:35"
        },
        {
          "id": "MDRW2100001047.1.8",
          "form": "맛있겠다!",
          "original_form": "맛있겠다!",
          "speaker_id": "2",
          "time": "20210722 16:36"
        },
        {
          "id": "MDRW2100001047.1.9",
          "form": "너는 뭐많이시켜먹어?",
          "original_form": "너는 뭐많이시켜먹어?",
          "speaker_id": "1",
          "time": "20210722 16:36"
        },
        {
          "id": "MDRW2100001047.1.10",
          "form": "난 치킨이나 피자 많이 시켜먹어",
          "original_form": "난 치킨이나 피자 많이 시켜먹어",
          "speaker_id": "2",
          "time": "20210722 16:36"
        },
        {
          "id": "MDRW2100001047.1.11",
          "form": "아 맛있겠다",
          "original_form": "아 맛있겠다",
          "speaker_id": "1",
          "time": "20210722 16:37"
        },
        {
          "id": "MDRW2100001047.1.12",
          "form": "어디치킨 좋아해?",
          "original_form": "어디치킨 좋아해?",
          "speaker_id": "1",
          "time": "20210722 16:37"
        },
        {
          "id": "MDRW2100001047.1.13",
          "form": "교촌!!",
          "original_form": "교촌!!",
          "speaker_id": "2",
          "time": "20210722 16:37"
        },
        {
          "id": "MDRW2100001047.1.14",
          "form": "교촌에 허니콤보?",
          "original_form": "교촌에 허니콤보?",
          "speaker_id": "1",
          "time": "20210722 16:38"
        },
        {
          "id": "MDRW2100001047.1.15",
          "form": "어!!!",
          "original_form": "어!!!",
          "speaker_id": "2",
          "time": "20210722 16:38"
        },
        {
          "id": "MDRW2100001047.1.16",
          "form": "난 네네치킨 좋아해",
          "original_form": "난 네네치킨 좋아해",
          "speaker_id": "1",
          "time": "20210722 16:39"
        },
        {
          "id": "MDRW2100001047.1.17",
          "form": "네네도 맛있지!!",
          "original_form": "네네도 맛있지!!",
          "speaker_id": "2",
          "time": "20210722 16:39"
        },
        {
          "id": "MDRW2100001047.1.18",
          "form": "치킨은 좀비싸서 자주시켜 먹진 않아!ㅜㅜ",
