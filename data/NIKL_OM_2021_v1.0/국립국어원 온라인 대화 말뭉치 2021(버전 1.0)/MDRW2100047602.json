      "utterance": [
        {
          "id": "MDRW2100047602.1.1",
          "form": "취업 과정은 수월하셨나요",
          "original_form": "취업 과정은 수월하셨나요",
          "speaker_id": "2",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100047602.1.2",
          "form": "나름 어려웠어요",
          "original_form": "나름 어려웠어요",
          "speaker_id": "1",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100047602.1.3",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100047602.1.4",
          "form": "스터디..인강.. 이렇게 하셨겠죠?",
          "original_form": "스터디..인강.. 이렇게 하셨겠죠?",
          "speaker_id": "2",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100047602.1.5",
          "form": "네 시험 너무 싫어요ㅠ",
          "original_form": "네 시험 너무 싫어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100047602.1.6",
          "form": "저두요 그당시 인강 강사 기억나나요",
          "original_form": "저두요 그당시 인강 강사 기억나나요",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100047602.1.7",
          "form": "하...  다시하라고하면 못할듯요",
          "original_form": "하...  다시하라고하면 못할듯요",
          "speaker_id": "1",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100047602.1.8",
          "form": "저는 취업공부하며 친구들과 먹는 낙으로 버텼네요",
          "original_form": "저는 취업공부하며 친구들과 먹는 낙으로 버텼네요",
          "speaker_id": "2",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100047602.1.9",
          "form": "마져요 그낙이 젤커요",
          "original_form": "마져요 그낙이 젤커요",
          "speaker_id": "1",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100047602.1.10",
          "form": "지금도요ㅋ",
          "original_form": "지금도요ㅋ",
          "speaker_id": "1",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100047602.1.11",
          "form": "저녁메뉴 먹으며 내일 메뉴 고민하는 그런 기쁨?으로.. 그 시기를 버틴것 같아요",
          "original_form": "저녁메뉴 먹으며 내일 메뉴 고민하는 그런 기쁨?으로.. 그 시기를 버틴것 같아요",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100047602.1.12",
          "form": "아르바이트도 하셨나요?",
          "original_form": "아르바이트도 하셨나요?",
          "speaker_id": "1",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100047602.1.13",
          "form": "과외 알바만 해봤어요",
          "original_form": "과외 알바만 해봤어요",
          "speaker_id": "2",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100047602.1.14",
          "form": "전 피자집이요",
          "original_form": "전 피자집이요",
          "speaker_id": "1",
