      "utterance": [
        {
          "id": "MDRW2100041868.1.1",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100041868.1.2",
          "form": "포비피엠 멤버들 보면",
          "original_form": "포비피엠 멤버들 보면",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100041868.1.3",
          "form": "월수입이 얼마나 될지",
          "original_form": "월수입이 얼마나 될지",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100041868.1.4",
          "form": "너무 궁금해요ㅋㅋㅋㅋㅋㅋㅋㅌ",
          "original_form": "너무 궁금해요ㅋㅋㅋㅋㅋㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100041868.1.5",
          "form": "그니까요 ㅋㅋㅋ 진짜 궁금함",
          "original_form": "그니까요 ㅋㅋㅋ 진짜 궁금함",
          "speaker_id": "2",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100041868.1.6",
          "form": "자영업은 정말 빠지는게 많아서 예상도 못하겠디만",
          "original_form": "자영업은 정말 빠지는게 많아서 예상도 못하겠디만",
          "speaker_id": "2",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100041868.1.7",
          "form": "처음 적자로 시작했어도",
          "original_form": "처음 적자로 시작했어도",
          "speaker_id": "2",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100041868.1.8",
          "form": "지금은 이미 흑자로 전환되고도 남았을거같아요",
          "original_form": "지금은 이미 흑자로 전환되고도 남았을거같아요",
          "speaker_id": "2",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100041868.1.9",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041868.1.10",
          "form": "그리고 그",
          "original_form": "그리고 그",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041868.1.11",
          "form": "아니 ㅠ",
          "original_form": "아니 ㅠ",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041868.1.12",
          "form": "칵테일바도 그러는데",
          "original_form": "칵테일바도 그러는데",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041868.1.13",
          "form": "영상수익이랑...",
          "original_form": "영상수익이랑...",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041868.1.14",
          "form": "근데 옷피엠은 별로 마진 안남았을 것 같지 않나요",
          "original_form": "근데 옷피엠은 별로 마진 안남았을 것 같지 않나요",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041868.1.15",
          "form": "그치만... 그래도... 그가격대면...꽤나 남을듯한",
          "original_form": "그치만... 그래도... 그가격대면...꽤나 남을듯한",
          "speaker_id": "2",
          "time": "20210906 17:01"
        },
        {
          "id": "MDRW2100041868.1.16",
          "form": "앗....그건 그렇네요....흠.....",
          "original_form": "앗....그건 그렇네요....흠.....",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041868.1.17",
          "form": "정말...",
          "original_form": "정말...",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041868.1.18",
          "form": "아니 심지어...",
          "original_form": "아니 심지어...",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041868.1.19",
          "form": "아 약간 뒷담화같은데",
          "original_form": "아 약간 뒷담화같은데",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041868.1.20",
          "form": "아니죠 저희는 소비자로서",
          "original_form": "아니죠 저희는 소비자로서",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041868.1.21",
          "form": "할 말을 하는거쥬",
          "original_form": "할 말을 하는거쥬",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041868.1.22",
          "form": "(정당화",
          "original_form": "(정당화",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041868.1.23",
          "form": "아니 심지어",
          "original_form": "아니 심지어",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041868.1.24",
          "form": "ㅋㅋㅋㅋ아니 이게 왜 뒷담입니까",
          "original_form": "ㅋㅋㅋㅋ아니 이게 왜 뒷담입니까",
          "speaker_id": "2",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041868.1.25",
          "form": "볼캡도 모양이 별로더라구요",
          "original_form": "볼캡도 모양이 별로더라구요",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041868.1.26",
          "form": "저희는 소비잡니다",
          "original_form": "저희는 소비잡니다",
          "speaker_id": "2",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041868.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그춐ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그춐ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041868.1.28",
          "form": "맞아요.",
          "original_form": "맞아요.",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041868.1.29",
          "form": "아주 동의합니다.",
          "original_form": "아주 동의합니다.",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041868.1.30",
          "form": "당당",
          "original_form": "당당",
          "speaker_id": "2",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041868.1.31",
          "form": "당.당.",
          "original_form": "당.당.",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041868.1.32",
          "form": "제가 모자 사려고",
          "original_form": "제가 모자 사려고",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041868.1.33",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041868.1.34",
          "form": "전시회 가서 써봤는데",
          "original_form": "전시회 가서 써봤는데",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041868.1.35",
          "form": "음....",
          "original_form": "음....",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041868.1.36",
          "form": "ㅋㅋㅋㅋㅋ음..",
          "original_form": "ㅋㅋㅋㅋㅋ음..",
          "speaker_id": "2",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041868.1.37",
          "form": "그냥 저랑 안어울리는건지...",
          "original_form": "그냥 저랑 안어울리는건지...{emoji:☆}",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041868.1.38",
          "form": "그래도 티셔츠는",
          "original_form": "그래도 티셔츠는",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041868.1.39",
          "form": "진짜 잘 입고있긴 하거든요",
          "original_form": "진짜 잘 입고있긴 하거든요",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041868.1.40",
          "form": "늘어나긴 했어도ㅠ",
          "original_form": "늘어나긴 했어도ㅠ",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041868.1.41",
          "form": "근데 맨투맨은 진짜",
          "original_form": "근데 맨투맨은 진짜",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041868.1.42",
          "form": "그 좀 그 뭐지 앞에 챙이 말발굽처럼 휜 너낌인가",
          "original_form": "그 좀 그 뭐지 앞에 챙이 말발굽처럼 휜 너낌인가",
          "speaker_id": "2",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041868.1.43",
          "form": "오 맞아요맞아요",
          "original_form": "오 맞아요맞아요",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041868.1.44",
          "form": "맨투맨 팔아버리세요..",
          "original_form": "맨투맨 팔아버리세요..",
          "speaker_id": "2",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041868.1.45",
          "form": "맨투맨은 진짜,, 당근해야하나",
          "original_form": "맨투맨은 진짜,, 당근해야하나",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041868.1.46",
          "form": "누가 사갈까요...?",
          "original_form": "누가 사갈까요...?",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041868.1.47",
          "form": "하ㅡ....",
          "original_form": "하ㅡ....",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041868.1.48",
          "form": "소그노 좋아하는 사람이 봐야할텐데...",
          "original_form": "소그노 좋아하는 사람이 봐야할텐데...",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041868.1.49",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100041868.1.50",
          "form": "소그노 아는사람이면 바로 팔릴거같은데",
          "original_form": "소그노 아는사람이면 바로 팔릴거같은데",
          "speaker_id": "2",
          "time": "20210906 17:08"
        }
      ]
