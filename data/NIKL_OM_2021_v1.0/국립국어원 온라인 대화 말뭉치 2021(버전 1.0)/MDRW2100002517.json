      "utterance": [
        {
          "id": "MDRW2100002517.1.1",
          "form": "즐겨보는 컨텐츠있으세요?",
          "original_form": "즐겨보는 컨텐츠있으세요?",
          "speaker_id": "1",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100002517.1.2",
          "form": "저 유튜브에서",
          "original_form": "저 유튜브에서",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002517.1.3",
          "form": "고양이보는거 좋아해요",
          "original_form": "고양이보는거 좋아해요",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002517.1.4",
          "form": "어떤채널이요?",
          "original_form": "어떤채널이요?",
          "speaker_id": "1",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002517.1.5",
          "form": "하하하님이랑 메탈남? 유명하던데",
          "original_form": "하하하님이랑 메탈남? 유명하던데",
          "speaker_id": "1",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002517.1.6",
          "form": "22똥괭이아세요?",
          "original_form": "22똥괭이아세요?",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002517.1.7",
          "form": "엇 그것도 봐요",
          "original_form": "엇 그것도 봐요",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002517.1.8",
          "form": "네 22마리 키우는 집 맞죠?",
          "original_form": "네 22마리 키우는 집 맞죠?",
          "speaker_id": "1",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002517.1.9",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "1",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002517.1.10",
          "form": "넹넹 고양이들 다 너무 귀여워요",
          "original_form": "넹넹 고양이들 다 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002517.1.11",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002517.1.12",
          "form": "여자분이 엄청많이 키우시는거",
          "original_form": "여자분이 엄청많이 키우시는거",
          "speaker_id": "1",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002517.1.13",
          "form": "그거 본거같아요",
          "original_form": "그거 본거같아요",
          "speaker_id": "1",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002517.1.14",
          "form": "name1님은 어떤거 보세요?",
          "original_form": "&name1&님은 어떤거 보세요?",
          "speaker_id": "2",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002517.1.15",
          "form": "저는 카페브이로그 같은거 봐요",
          "original_form": "저는 카페브이로그 같은거 봐요",
          "speaker_id": "1",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002517.1.16",
          "form": "음료만드는거 멍하니 보고있어요ㅋㅋ",
          "original_form": "음료만드는거 멍하니 보고있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002517.1.17",
          "form": "보는 asmr같더라구요ㅋㅋㅋ",
          "original_form": "보는 asmr같더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002517.1.18",
          "form": "먹방같은 건 잘 못보겠더라구요",
          "original_form": "먹방같은 건 잘 못보겠더라구요",
          "speaker_id": "1",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002517.1.19",
          "form": "그냥 음료수만드는거",
          "original_form": "그냥 음료수만드는거",
          "speaker_id": "2",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002517.1.20",
          "form": "보고있으면 마음편해져서 저도 가끔봐요",
          "original_form": "보고있으면 마음편해져서 저도 가끔봐요",
          "speaker_id": "2",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002517.1.21",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002517.1.22",
          "form": "그리고 요즘엔 또",
          "original_form": "그리고 요즘엔 또",
          "speaker_id": "2",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002517.1.23",
          "form": "근데 그 때 드는 생각중 하나가",
          "original_form": "근데 그 때 드는 생각중 하나가",
          "speaker_id": "1",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002517.1.24",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002517.1.25",
          "form": "농사짓는거 봐여ㅋㅋㅋ",
          "original_form": "농사짓는거 봐여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002517.1.26",
          "form": "농촌",
          "original_form": "농촌",
          "speaker_id": "1",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002517.1.27",
          "form": "힐링 재질",
          "original_form": "힐링 재질",
          "speaker_id": "1",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002517.1.28",
          "form": "텃밭가꾸기 이런거",
          "original_form": "텃밭가꾸기 이런거",
          "speaker_id": "2",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002517.1.29",
          "form": "그런거 재밌죠",
          "original_form": "그런거 재밌죠",
          "speaker_id": "1",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002517.1.30",
          "form": "그냥 별거아닌데 시간순삭",
          "original_form": "그냥 별거아닌데 시간순삭",
          "speaker_id": "2",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002517.1.31",
          "form": "수확의 즐거움도 있고",
