      "utterance": [
        {
          "id": "MDRW2100030205.1.1",
          "form": "요즘 주로 어떤 컨텐츠 관심있으세요?",
          "original_form": "요즘 주로 어떤 컨텐츠 관심있으세요?",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030205.1.2",
          "form": "미드요",
          "original_form": "미드요",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030205.1.3",
          "form": "빅뱅이론 아세요?",
          "original_form": "빅뱅이론 아세요?",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030205.1.4",
          "form": "알죠 쉘든이던가요?",
          "original_form": "알죠 쉘든이던가요?",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030205.1.5",
          "form": "오피스도 보시나요?",
          "original_form": "오피스도 보시나요?",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030205.1.6",
          "form": "쉘든 맞아여 ㅋㅋㅋ",
          "original_form": "쉘든 맞아여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030205.1.7",
          "form": "오피스는 아니요...",
          "original_form": "오피스는 아니요...",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030205.1.8",
          "form": "재밋나요",
          "original_form": "재밋나요",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030205.1.9",
          "form": "ㅋㅋ저 모팸이랑 오피스봤어요 무한상사가 오피스 따라한 부분이 많아요",
          "original_form": "ㅋㅋ저 모팸이랑 오피스봤어요 무한상사가 오피스 따라한 부분이 많아요",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030205.1.10",
          "form": "모팸도 진짜 재밌죠",
          "original_form": "모팸도 진짜 재밌죠",
          "speaker_id": "2",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100030205.1.11",
          "form": "오피스 봐야겠어요",
          "original_form": "오피스 봐야겠어요",
          "speaker_id": "2",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100030205.1.12",
          "form": "한 회에 몇 분 정도 걸리나요",
          "original_form": "한 회에 몇 분 정도 걸리나요",
          "speaker_id": "2",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100030205.1.13",
          "form": "이삼십분이었나... 오래전이라...",
          "original_form": "이삼십분이었나... 오래전이라...",
          "speaker_id": "1",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100030205.1.14",
          "form": "조각영상으로 많이 돌아다녀요",
          "original_form": "조각영상으로 많이 돌아다녀요",
          "speaker_id": "1",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100030205.1.15",
          "form": "모팸에서 최애캐는 누군가요",
          "original_form": "모팸에서 최애캐는 누군가요",
          "speaker_id": "2",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100030205.1.16",
          "form": "미첼ㅋㅋ",
          "original_form": "미첼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100030205.1.17",
          "form": "누구 좋아하세요?",
          "original_form": "누구 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100030205.1.18",
          "form": "앜ㅋㅋㅋ 미첼도 너무 젛죠 ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ 미첼도 너무 젛죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:58"
        },
        {
          "id": "MDRW2100030205.1.19",
          "form": "전 콜롬비아에서 오신 여자분이여 ㅋㅋㅌ",
          "original_form": "전 콜롬비아에서 오신 여자분이여 ㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210904 22:58"
        },
        {
          "id": "MDRW2100030205.1.20",
          "form": "아 똑뚜미 여사ㅎㅎ",
          "original_form": "아 똑뚜미 여사ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100030205.1.21",
          "form": "캠 너무 싫지않나요?",
          "original_form": "캠 너무 싫지않나요?",
          "speaker_id": "1",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100030205.1.22",
          "form": "똑뚜미 ㅋㅋㅋㅋ",
          "original_form": "똑뚜미 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:04"
        },
        {
          "id": "MDRW2100030205.1.23",
          "form": "좀 그런 편이죠ㅋㅋㅋ",
          "original_form": "좀 그런 편이죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:05"
        },
        {
          "id": "MDRW2100030205.1.24",
          "form": "시즌 다 챙겨보셨니요",
          "original_form": "시즌 다 챙겨보셨니요",
          "speaker_id": "2",
          "time": "20210905 00:05"
        },
        {
          "id": "MDRW2100030205.1.25",
          "form": "시즌5까진가 챙기다가ㅋㅋ",
          "original_form": "시즌5까진가 챙기다가ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:06"
        },
        {
          "id": "MDRW2100030205.1.26",
          "form": "헤일리 결혼하고 이럴때부터 하차ㅋㅋ",
          "original_form": "헤일리 결혼하고 이럴때부터 하차ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:06"
        }
      ]
    }
  ]
