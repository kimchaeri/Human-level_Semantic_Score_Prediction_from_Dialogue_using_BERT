      "utterance": [
        {
          "id": "MDRW2100000613.1.1",
          "form": "요즘에 읽고 계시는 책 있으신가요?",
          "original_form": "요즘에 읽고 계시는 책 있으신가요?",
          "speaker_id": "1",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000613.1.2",
          "form": "저는 책을 잘 안읽는데 혹시 추천해주실만한 책 있을까요?",
          "original_form": "저는 책을 잘 안읽는데 혹시 추천해주실만한 책 있을까요?",
          "speaker_id": "2",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000613.1.3",
          "form": "요즘 다들 주식/금융 얘기를 많이 해서",
          "original_form": "요즘 다들 주식/금융 얘기를 많이 해서",
          "speaker_id": "1",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000613.1.4",
          "form": "근간에 관련된 책 찾다가 '돈의 심리학'이라는 책을 읽어봣는데",
          "original_form": "근간에 관련된 책 찾다가 '돈의 심리학'이라는 책을 읽어봣는데",
          "speaker_id": "1",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000613.1.5",
          "form": "좋더라고요~",
          "original_form": "좋더라고요~",
          "speaker_id": "1",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000613.1.6",
          "form": "돈의 심리학!",
          "original_form": "돈의 심리학!",
          "speaker_id": "2",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000613.1.7",
          "form": "저도 한번 찾아봐야겠어요",
          "original_form": "저도 한번 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000613.1.8",
          "form": "지인이 예스24에서 근무해서 상품권을 투척했는데 묵혀두고만 있었거든요",
          "original_form": "지인이 예스24에서 근무해서 상품권을 투척했는데 묵혀두고만 있었거든요",
          "speaker_id": "2",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000613.1.9",
          "form": "와~ 좋은 지인분이시네요~",
          "original_form": "와~ 좋은 지인분이시네요~",
          "speaker_id": "1",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000613.1.10",
          "form": "돈에 관심 있으시다면 추천이요~",
          "original_form": "돈에 관심 있으시다면 추천이요~",
          "speaker_id": "1",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000613.1.11",
          "form": "한번 구매해봐야겠어요 좋은책 추천 감사해요",
          "original_form": "한번 구매해봐야겠어요 좋은책 추천 감사해요",
          "speaker_id": "2",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000613.1.12",
          "form": "네~ 책이 좀 두꺼워서 약간 부담이 되실 수도,,",
          "original_form": "네~ 책이 좀 두꺼워서 약간 부담이 되실 수도,,",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000613.1.13",
          "form": "요즘 '달러구트의 꿈의 백화점' 소설 읽고 있는데,",
          "original_form": "요즘 '달러구트의 꿈의 백화점' 소설 읽고 있는데,",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000613.1.14",
          "form": "이건 내용이 가벼워서 읽기 좋더라고요~",
          "original_form": "이건 내용이 가벼워서 읽기 좋더라고요~",
          "speaker_id": "1",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000613.1.15",
          "form": "요건 무슨내용인가요? 저는 슬픈거는 잘 못읽어서",
          "original_form": "요건 무슨내용인가요? 저는 슬픈거는 잘 못읽어서",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000613.1.16",
          "form": "이거는 밝은 내용이에요~",
          "original_form": "이거는 밝은 내용이에요~",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000613.1.17",
          "form": "사람들이 꾸는 꿈에 관련된 이야기~ㅎㅎ",
          "original_form": "사람들이 꾸는 꿈에 관련된 이야기~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000613.1.18",
          "form": "아 다행이네요",
          "original_form": "아 다행이네요",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000613.1.19",
          "form": "추천해주신 책 두개다 봐야겠어요",
          "original_form": "추천해주신 책 두개다 봐야겠어요",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000613.1.20",
          "form": "부끄럽지만 전 평소에 만화만봐서 ㅋ큐ㅠ",
          "original_form": "부끄럽지만 전 평소에 만화만봐서 ㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000613.1.21",
          "form": "만화~ 만화도 좋아해요~ㅎㅎ",
          "original_form": "만화~ 만화도 좋아해요~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000613.1.22",
          "form": "만화는 재밌지만 어디가서 말하기 조금 쑥스럽죠 ㅋㅋ",
          "original_form": "만화는 재밌지만 어디가서 말하기 조금 쑥스럽죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000613.1.23",
          "form": "왜요~ 만화도 엄연한 문학장르인데ㅎㅎ",
          "original_form": "왜요~ 만화도 엄연한 문학장르인데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000613.1.24",
          "form": "전 만화도 좋아하고, 웹툰도 좋아해요~",
          "original_form": "전 만화도 좋아하고, 웹툰도 좋아해요~",
          "speaker_id": "1",
          "time": "20210819 17:44"
        }
      ]
    }
  ]
