      "utterance": [
        {
          "id": "MDRW2100038687.1.1",
          "form": "좋아하는",
          "original_form": "좋아하는",
          "speaker_id": "2",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100038687.1.2",
          "form": "과자 있으세요?",
          "original_form": "과자 있으세요?",
          "speaker_id": "2",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100038687.1.3",
          "form": "저는 꼬북칩 초코요!",
          "original_form": "저는 꼬북칩 초코요!",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100038687.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100038687.1.5",
          "form": "오 맛있나요?",
          "original_form": "오 맛있나요?",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100038687.1.6",
          "form": "전 썬칩이요",
          "original_form": "전 썬칩이요",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100038687.1.7",
          "form": "네 당떨어질때 먹으면 딱 좋답니다",
          "original_form": "네 당떨어질때 먹으면 딱 좋답니다",
          "speaker_id": "1",
          "time": "20210902 19:33"
        },
        {
          "id": "MDRW2100038687.1.8",
          "form": "썬칩도 맛있죠ㅠㅠ",
          "original_form": "썬칩도 맛있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:33"
        },
        {
          "id": "MDRW2100038687.1.9",
          "form": "네 ㅠ 맛있죠",
          "original_form": "네 ㅠ 맛있죠",
          "speaker_id": "2",
          "time": "20210902 19:34"
        },
        {
          "id": "MDRW2100038687.1.10",
          "form": "전 꼬북칩 식감이 제 스타일이 아니에요",
          "original_form": "전 꼬북칩 식감이 제 스타일이 아니에요",
          "speaker_id": "2",
          "time": "20210902 19:34"
        },
        {
          "id": "MDRW2100038687.1.11",
          "form": "ㅠㅠ아쉽네여 참 맛있는데!!",
          "original_form": "ㅠㅠ아쉽네여 참 맛있는데!!",
          "speaker_id": "1",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100038687.1.12",
          "form": "약간 인디언밥 느낌 아닌가요??",
          "original_form": "약간 인디언밥 느낌 아닌가요??",
          "speaker_id": "2",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100038687.1.13",
          "form": "인디언밥은 좀 부드럽게 녹는 느낌아니었나여??기억이 잘..",
          "original_form": "인디언밥은 좀 부드럽게 녹는 느낌아니었나여??기억이 잘..",
          "speaker_id": "1",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100038687.1.14",
          "form": "이건 와작와작",
          "original_form": "이건 와작와작",
          "speaker_id": "1",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100038687.1.15",
          "form": "아 잘 녹았나요ㅋㅋㅋㅋ!?",
          "original_form": "아 잘 녹았나요ㅋㅋㅋㅋ!?",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100038687.1.16",
          "form": "사또밥이랑 헷갈려요ㅋㅋㅋㅋ인디언밥도 우유 말아먹으면 맛났던거같은데",
          "original_form": "사또밥이랑 헷갈려요ㅋㅋㅋㅋ인디언밥도 우유 말아먹으면 맛났던거같은데",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038687.1.17",
          "form": "아 사또밥 맛있는데",
          "original_form": "아 사또밥 맛있는데",
          "speaker_id": "2",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100038687.1.18",
          "form": "뭔가 맹맹해요 알죠?",
          "original_form": "뭔가 맹맹해요 알죠?",
          "speaker_id": "2",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100038687.1.19",
          "form": "네네 알죠ㅋㅋㅋ첨엔 맛난데 먹다보면 아무맛도 안나는 느낌..",
          "original_form": "네네 알죠ㅋㅋㅋ첨엔 맛난데 먹다보면 아무맛도 안나는 느낌..",
          "speaker_id": "1",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100038687.1.20",
          "form": "ㅋㅋㅋ 맛있는맛이 좀 오래가면 좋겠어요 ㅠ",
          "original_form": "ㅋㅋㅋ 맛있는맛이 좀 오래가면 좋겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100038687.1.21",
          "form": "글게요 좀 더 자극적인맛을 원해욧",
          "original_form": "글게요 좀 더 자극적인맛을 원해욧",
          "speaker_id": "1",
          "time": "20210902 21:49"
        }
