      "utterance": [
        {
          "id": "MDRW2100010578.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010578.1.2",
          "form": "온라인 커뮤니티가 SNS 같은거겠죠?",
          "original_form": "온라인 커뮤니티가 SNS 같은거겠죠?",
          "speaker_id": "1",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010578.1.3",
          "form": "안녕하세요, 인스티즈나 카페 생각했었어요!",
          "original_form": "안녕하세요, 인스티즈나 카페 생각했었어요!",
          "speaker_id": "2",
          "time": "20210823 20:22"
        },
        {
          "id": "MDRW2100010578.1.4",
          "form": "아 그런거겠네요",
          "original_form": "아 그런거겠네요",
          "speaker_id": "1",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010578.1.5",
          "form": "카페 가입하신거 있으신가용?",
          "original_form": "카페 가입하신거 있으신가용?",
          "speaker_id": "1",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010578.1.6",
          "form": "다음 카페랑 인스티즈 가입했어요",
          "original_form": "다음 카페랑 인스티즈 가입했어요",
          "speaker_id": "2",
          "time": "20210823 22:01"
        },
        {
          "id": "MDRW2100010578.1.7",
          "form": "인스티즈는 가입창이 잠깐 열렸다 닫혀서 가입하기 힘든 것같아요",
          "original_form": "인스티즈는 가입창이 잠깐 열렸다 닫혀서 가입하기 힘든 것같아요",
          "speaker_id": "1",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100010578.1.8",
          "form": "저도 회원인 친구한테 부탁해서 그냥 유료로 회원했어요ㅠ",
          "original_form": "저도 회원인 친구한테 부탁해서 그냥 유료로 회원했어요ㅠ",
          "speaker_id": "2",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100010578.1.9",
          "form": "헉 맞아요 친구 추천도 있더라구요",
          "original_form": "헉 맞아요 친구 추천도 있더라구요",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100010578.1.10",
          "form": "최근에 인스티즈 가입창 열렸던거 봤는데",
          "original_form": "최근에 인스티즈 가입창 열렸던거 봤는데",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100010578.1.11",
          "form": "10분만 열린거 보고ㅋㅋㅋㅋ대단하다고 생각했네요",
          "original_form": "10분만 열린거 보고ㅋㅋㅋㅋ대단하다고 생각했네요",
          "speaker_id": "1",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100010578.1.12",
          "form": "많이 비싸지 않아서 했는데 요즘은 별로 안 하게 되더라구요",
          "original_form": "많이 비싸지 않아서 했는데 요즘은 별로 안 하게 되더라구요",
          "speaker_id": "2",
          "time": "20210824 19:44"
        },
        {
          "id": "MDRW2100010578.1.13",
          "form": "근데 시간 때우기에는 좋은 것같아요",
          "original_form": "근데 시간 때우기에는 좋은 것같아요",
          "speaker_id": "1",
          "time": "20210825 12:09"
        },
        {
          "id": "MDRW2100010578.1.14",
          "form": "가입 안하면 하고 싶고...ㅋㅋㅋㅋ",
          "original_form": "가입 안하면 하고 싶고...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100010578.1.15",
          "form": "비회원도 읽을 수 있는 글도 있으니 굳이 가입하지 않아도 상관 없긴 하죠ㅋㅋ",
          "original_form": "비회원도 읽을 수 있는 글도 있으니 굳이 가입하지 않아도 상관 없긴 하죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:41"
        },
        {
          "id": "MDRW2100010578.1.16",
          "form": "글킨 하죠ㅋㅋ",
          "original_form": "글킨 하죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:52"
        },
        {
          "id": "MDRW2100010578.1.17",
          "form": "말씀대로 시간 때우기는 좋지만요ㅋㅋ",
          "original_form": "말씀대로 시간 때우기는 좋지만요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:59"
        },
        {
          "id": "MDRW2100010578.1.18",
          "form": "인스티즈도 정모같은거 하나요?",
          "original_form": "인스티즈도 정모같은거 하나요?",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100010578.1.19",
          "form": "정모가 딱히 있진 않지만 회원간에 만나고 싶으면 만날 수 있게 연락할 수단은 있어요",
          "original_form": "정모가 딱히 있진 않지만 회원간에 만나고 싶으면 만날 수 있게 연락할 수단은 있어요",
          "speaker_id": "2",
          "time": "20210825 14:04"
        }
      ]
