      "utterance": [
        {
          "id": "MDRW2100002117.1.1",
          "form": "책 읽어요?",
          "original_form": "책 읽어요?",
          "speaker_id": "1",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002117.1.2",
          "form": "시간이 핑계요",
          "original_form": "시간이 핑계요",
          "speaker_id": "2",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002117.1.3",
          "form": "그럴수 있죠ㅋㅋ",
          "original_form": "그럴수 있죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002117.1.4",
          "form": "요즘은 폰북도 잘되있던뎅",
          "original_form": "요즘은 폰북도 잘되있던뎅",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002117.1.5",
          "form": "김혜수 광고하는 리딩북도 있대요",
          "original_form": "김혜수 광고하는 리딩북도 있대요",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002117.1.6",
          "form": "맞아여 저도 읽어야하는데",
          "original_form": "맞아여 저도 읽어야하는데",
          "speaker_id": "1",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002117.1.7",
          "form": "책은 사놓고 잘안보게되네요 ㅋㅋ",
          "original_form": "책은 사놓고 잘안보게되네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002117.1.8",
          "form": "그래도 사긴 하시네요",
          "original_form": "그래도 사긴 하시네요",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002117.1.9",
          "form": "해리포터 시리즈 읽어보셨나요",
          "original_form": "해리포터 시리즈 읽어보셨나요",
          "speaker_id": "1",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002117.1.10",
          "form": "영화만 봤어요",
          "original_form": "영화만 봤어요",
          "speaker_id": "2",
          "time": "20210819 23:51"
        },
        {
          "id": "MDRW2100002117.1.11",
          "form": "진짜 잼나요 추천",
