      "utterance": [
        {
          "id": "MDRW2100011587.1.1",
          "form": "야 만약에 너 미대 가고 나 한국에 있어도 연락하기는 괜찮겠다",
          "original_form": "야 만약에 너 미대 가고 나 한국에 있어도 연락하기는 괜찮겠다",
          "speaker_id": "2",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011587.1.2",
          "form": "아마 잠을 못잘테니까 시차가 맞을 지도 몰라",
          "original_form": "아마 잠을 못잘테니까 시차가 맞을 지도 몰라",
          "speaker_id": "2",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011587.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ재밌네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ재밌네",
          "speaker_id": "1",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011587.1.4",
          "form": "어차피너는",
          "original_form": "어차피너는",
          "speaker_id": "1",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011587.1.5",
          "form": "밤낮바뀔테니까",
          "original_form": "밤낮바뀔테니까",
          "speaker_id": "1",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011587.1.6",
          "form": "그니까 지금은 정상이지만",
          "original_form": "그니까 지금은 정상이지만",
          "speaker_id": "2",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011587.1.7",
          "form": "2028년에 la에서 올림픽하잖아",
          "original_form": "2028년에 la에서 올림픽하잖아",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011587.1.8",
          "form": "개강하거나 일을 한다 그럼 뭐 바뀌는거 시간문제임",
          "original_form": "개강하거나 일을 한다 그럼 뭐 바뀌는거 시간문제임",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011587.1.9",
          "form": "오 진짜?",
          "original_form": "오 진짜?",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011587.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ개강하면잠을",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ개강하면잠을",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011587.1.11",
          "form": "못자잖아",
          "original_form": "못자잖아",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011587.1.12",
          "form": "과제하느라",
          "original_form": "과제하느라",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011587.1.13",
          "form": "맞지",
          "original_form": "맞지",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011587.1.14",
          "form": "그래도대면하면",
          "original_form": "그래도대면하면",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011587.1.15",
          "form": "과제좀줄지않음?",
          "original_form": "과제좀줄지않음?",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011587.1.16",
          "form": "비대면보다",
          "original_form": "비대면보다",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011587.1.17",
          "form": "우리 과는 똑같대",
          "original_form": "우리 과는 똑같대",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011587.1.18",
          "form": "하나 안하나 그게 그거래",
          "original_form": "하나 안하나 그게 그거래",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011587.1.19",
          "form": "대면하면 공연 연출까지 해야 해서 더 바빠질걸",
          "original_form": "대면하면 공연 연출까지 해야 해서 더 바빠질걸",
          "speaker_id": "2",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011587.1.20",
          "form": "아 원래 그 학과는 그렇게 바빠?",
          "original_form": "아 원래 그 학과는 그렇게 바빠?",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011587.1.21",
          "form": "너네학교가특히많은거임?",
          "original_form": "너네학교가특히많은거임?",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011587.1.22",
          "form": "우리 학과가 특히 그럴 걸",
          "original_form": "우리 학과가 특히 그럴 걸",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011587.1.23",
          "form": "글쓰고 작품보고 글 쓴 거가지고 돌아가며 평가하고 공연올리고 공연 무대 만들고 책 디자인도 하고 또 글쓰고",
          "original_form": "글쓰고 작품보고 글 쓴 거가지고 돌아가며 평가하고 공연올리고 공연 무대 만들고 책 디자인도 하고 또 글쓰고",
          "speaker_id": "2",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011587.1.24",
          "form": "글쓰고 글쓰고 평가받고 울고 화내고 글쓰고",
          "original_form": "글쓰고 글쓰고 평가받고 울고 화내고 글쓰고",
          "speaker_id": "2",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011587.1.25",
          "form": "시나리온데왜공연무대는",
          "original_form": "시나리온데왜공연무대는",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011587.1.26",
          "form": "만들어?",
          "original_form": "만들어?",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011587.1.27",
          "form": "희곡 연출 덕분에",
          "original_form": "희곡 연출 덕분에",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011587.1.28",
          "form": "근데 그거 우리과만 그런게 아니라 연기과도 무대 만들고 그런대",
          "original_form": "근데 그거 우리과만 그런게 아니라 연기과도 무대 만들고 그런대",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011587.1.29",
          "form": "무대만드는건",
          "original_form": "무대만드는건",
          "speaker_id": "1",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011587.1.30",
          "form": "완전다른학과인줄",
          "original_form": "완전다른학과인줄",
          "speaker_id": "1",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011587.1.31",
          "form": "알았는데",
          "original_form": "알았는데",
          "speaker_id": "1",
          "time": "20210824 14:25"
        }
      ]
