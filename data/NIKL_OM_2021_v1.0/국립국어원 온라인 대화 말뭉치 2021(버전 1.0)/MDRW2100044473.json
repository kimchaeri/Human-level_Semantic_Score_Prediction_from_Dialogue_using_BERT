      "utterance": [
        {
          "id": "MDRW2100044473.1.1",
          "form": "간짜장이 그냥 짜장이랑 면 따로주는거 아니에요??",
          "original_form": "간짜장이 그냥 짜장이랑 면 따로주는거 아니에요??",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044473.1.2",
          "form": "맛이 다른가요?",
          "original_form": "맛이 다른가요?",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044473.1.3",
          "form": "다르죠 따로볶아서",
          "original_form": "다르죠 따로볶아서",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044473.1.4",
          "form": "재료고 더많고 맛잇는대",
          "original_form": "재료고 더많고 맛잇는대",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044473.1.5",
          "form": "더비싸잔아요",
          "original_form": "더비싸잔아요",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044473.1.6",
          "form": "그런가요 저는 걍 별 차이 모르겠어서 ㅋㅋㅋㅋ",
          "original_form": "그런가요 저는 걍 별 차이 모르겠어서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044473.1.7",
          "form": "아이 멀 모르시네",
          "original_form": "아이 멀 모르시네",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044473.1.8",
          "form": "님 야끼짬뽕 조아해요?",
          "original_form": "님 야끼짬뽕 조아해요?",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044473.1.9",
          "form": "야끼짬뽕이 뭐죠?",
          "original_form": "야끼짬뽕이 뭐죠?",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044473.1.10",
          "form": "볶음 짬뽕",
          "original_form": "볶음 짬뽕",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044473.1.11",
          "form": "아 먹어본적 있는것도 같고 ㅋㅋㅋㅋ",
          "original_form": "아 먹어본적 있는것도 같고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044473.1.12",
          "form": "함 드시러가시죠",
          "original_form": "함 드시러가시죠",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044473.1.13",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044473.1.14",
          "form": "좋아요 ㅋㅋㅋㅋ 대전에 있는 맛집인가요?",
          "original_form": "좋아요 ㅋㅋㅋㅋ 대전에 있는 맛집인가요?",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044473.1.15",
          "form": "네네 어은동 이라고",
          "original_form": "네네 어은동 이라고",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044473.1.16",
          "form": "거기에 중국집 잇어요",
          "original_form": "거기에 중국집 잇어요",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044473.1.17",
          "form": "이름이 뭐예요? 지도에 찍어둘게여",
          "original_form": "이름이 뭐예요? 지도에 찍어둘게여",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044473.1.18",
          "form": "왕비성이요",
          "original_form": "왕비성이요",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044473.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044473.1.20",
          "form": "아 정말 중국집스러운 이름이네요 ㅎㅎ",
          "original_form": "아 정말 중국집스러운 이름이네요 ㅎㅎ",
