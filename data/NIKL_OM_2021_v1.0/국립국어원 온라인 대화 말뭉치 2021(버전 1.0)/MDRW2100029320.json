      "utterance": [
        {
          "id": "MDRW2100029320.1.1",
          "form": "안사귀고 그냥 만나면 되겠네요",
          "original_form": "안사귀고 그냥 만나면 되겠네요",
          "speaker_id": "2",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100029320.1.2",
          "form": "이러고 나중에 제가 아흔아홉살에 첫사랑이 생김 또 모르죠?ㅋㅋ",
          "original_form": "이러고 나중에 제가 아흔아홉살에 첫사랑이 생김 또 모르죠?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100029320.1.3",
          "form": "ㅋㅋㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100029320.1.4",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "2",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100029320.1.5",
          "form": "그쵸 딱 그러면 좋죠ㅎㅎ",
          "original_form": "그쵸 딱 그러면 좋죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100029320.1.6",
          "form": "아직 첫사랑이 없어요?",
          "original_form": "아직 첫사랑이 없어요?",
          "speaker_id": "2",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100029320.1.7",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100029320.1.8",
          "form": "있음 무 성 애 자겠어요ㅜ?",
          "original_form": "있음 무 성 애 자겠어요ㅜ?",
          "speaker_id": "1",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100029320.1.9",
          "form": "좋아하는 연예인들은 많아요ㅎ",
          "original_form": "좋아하는 연예인들은 많아요ㅎ",
          "speaker_id": "1",
          "time": "20210903 01:31"
        },
        {
          "id": "MDRW2100029320.1.10",
          "form": "전 후천적으로 남자가 싫어진줄 알앗서요",
          "original_form": "전 후천적으로 남자가 싫어진줄 알앗서요",
          "speaker_id": "2",
          "time": "20210903 01:31"
        },
        {
          "id": "MDRW2100029320.1.11",
          "form": "남자가 싫은게 아니라",
          "original_form": "남자가 싫은게 아니라",
          "speaker_id": "2",
          "time": "20210903 01:31"
        },
        {
          "id": "MDRW2100029320.1.12",
          "form": "아니ㅋㅋ",
          "original_form": "아니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:31"
        },
        {
          "id": "MDRW2100029320.1.13",
          "form": "연애 자체가 싫은거네요?",
          "original_form": "연애 자체가 싫은거네요?",
          "speaker_id": "2",
          "time": "20210903 01:31"
        },
        {
          "id": "MDRW2100029320.1.14",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210903 01:31"
        },
        {
          "id": "MDRW2100029320.1.15",
          "form": "음 좋습니다 ㅎㅎ",
          "original_form": "음 좋습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:31"
        },
        {
          "id": "MDRW2100029320.1.16",
          "form": "요즘",
          "original_form": "요즘",
          "speaker_id": "2",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029320.1.17",
          "form": "선호?",
          "original_form": "선호?",
          "speaker_id": "2",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029320.1.18",
          "form": "진짜 참하던데",
          "original_form": "진짜 참하던데",
          "speaker_id": "2",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029320.1.19",
          "form": "후천적으로 남자에게 트라우마 생기기엔..제가 너무 돌같아요ㅎ",
          "original_form": "후천적으로 남자에게 트라우마 생기기엔..제가 너무 돌같아요ㅎ",
          "speaker_id": "1",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029320.1.20",
          "form": "남자가 봐도",
          "original_form": "남자가 봐도",
          "speaker_id": "2",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029320.1.21",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029320.1.22",
          "form": "돌?",
          "original_form": "돌?",
          "speaker_id": "2",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029320.1.23",
          "form": "아ㅋㅋ그 남자배우요?",
          "original_form": "아ㅋㅋ그 남자배우요?",
          "speaker_id": "1",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029320.1.24",
          "form": "김선호?",
          "original_form": "김선호?",
          "speaker_id": "2",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029320.1.25",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029320.1.26",
          "form": "1박2일나오는사람",
          "original_form": "1박2일나오는사람",
          "speaker_id": "2",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029320.1.27",
          "form": "누구 좋아하세요?",
          "original_form": "누구 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029320.1.28",
          "form": "ㅋㅋ남자가 봐도 참하더이까ㅎ",
          "original_form": "ㅋㅋ남자가 봐도 참하더이까ㅎ",
          "speaker_id": "1",
          "time": "20210903 01:33"
        },
        {
          "id": "MDRW2100029320.1.29",
          "form": "전.. 김세정 옹성우 김종국 유재석 하휘동이요!",
          "original_form": "전.. 김세정 옹성우 김종국 유재석 하휘동이요!",
          "speaker_id": "1",
          "time": "20210903 01:33"
        },
        {
          "id": "MDRW2100029320.1.30",
          "form": "중구난방이고 유명 안유명 섞여 있어 반은 모르실듯ㅎ",
          "original_form": "중구난방이고 유명 안유명 섞여 있어 반은 모르실듯ㅎ",
