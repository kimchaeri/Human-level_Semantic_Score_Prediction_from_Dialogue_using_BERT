      "utterance": [
        {
          "id": "MDRW2100009481.1.1",
          "form": "앱테크하신다고요. 저는 뽐뿌해요ㅋ 뽐거지인듯ㅜ",
          "original_form": "앱테크하신다고요. 저는 뽐뿌해요ㅋ 뽐거지인듯ㅜ",
          "speaker_id": "1",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009481.1.2",
          "form": "ㅇㅅㅇ 웃대 같은 커뮤니티인가요?",
          "original_form": "ㅇㅅㅇ 웃대 같은 커뮤니티인가요?",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009481.1.3",
          "form": "엇 앱테크 하신다면서 뽐뿌를 모르시다니ㅋ",
          "original_form": "엇 앱테크 하신다면서 뽐뿌를 모르시다니ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009481.1.4",
          "form": "앱테크나 앱테크 커뮤니티 활동 뭐하시나요?",
          "original_form": "앱테크나 앱테크 커뮤니티 활동 뭐하시나요?",
          "speaker_id": "1",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009481.1.5",
          "form": "전 각시리즈 파고있어요",
          "original_form": "전 각시리즈 파고있어요",
          "speaker_id": "2",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009481.1.6",
          "form": "각시리즈는 뭐에요?",
          "original_form": "각시리즈는 뭐에요?",
          "speaker_id": "1",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009481.1.7",
          "form": "베라각 스벅각 편의점각 치킨각 캐시퍼 ㅋㅋ",
          "original_form": "베라각 스벅각 편의점각 치킨각 캐시퍼 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009481.1.8",
          "form": "전부매일 하면서 티끌을 모으고있어요?!",
          "original_form": "전부매일 하면서 티끌을 모으고있어요?!",
          "speaker_id": "2",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009481.1.9",
          "form": "제법 쏠쏠해요 ㅋㅋ",
          "original_form": "제법 쏠쏠해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009481.1.10",
          "form": "헉 그게뭐에요? 베라각?",
          "original_form": "헉 그게뭐에요? 베라각?",
          "speaker_id": "1",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009481.1.11",
          "form": "말 그대로 베라각이란앱인뎅",
          "original_form": "말 그대로 베라각이란앱인뎅",
          "speaker_id": "2",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009481.1.12",
          "form": "매일 200포인트줘요",
          "original_form": "매일 200포인트줘요",
          "speaker_id": "2",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009481.1.13",
          "form": "전 매일해서 14000원 넘게모았어요",
          "original_form": "전 매일해서 14000원 넘게모았어요",
          "speaker_id": "2",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009481.1.14",
          "form": "스벅각은  150 콩",
          "original_form": "스벅각은  150 콩",
          "speaker_id": "2",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009481.1.15",
          "form": "아! 음 신한쏠 리브메이트 엔에이치등 많은데",
          "original_form": "아! 음 신한쏠 리브메이트 엔에이치등 많은데",
          "speaker_id": "1",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009481.1.16",
          "form": "커뮤니티들에 정보 많아요ㅋ",
          "original_form": "커뮤니티들에 정보 많아요ㅋ",
          "speaker_id": "1",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009481.1.17",
          "form": "약간 그런거죠 ㅋㅋ",
          "original_form": "약간 그런거죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009481.1.18",
          "form": "그거보다 간단해서요",
          "original_form": "그거보다 간단해서요",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009481.1.19",
          "form": "스벅각은 매일 150콩인데 4500콩이면 스벅한잔이니까요",
          "original_form": "스벅각은 매일 150콩인데 4500콩이면 스벅한잔이니까요",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009481.1.20",
          "form": "오호 저도 해봐야겠네요. 정모 같은 건 해보셨어요?",
          "original_form": "오호 저도 해봐야겠네요. 정모 같은 건 해보셨어요?",
          "speaker_id": "1",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009481.1.21",
          "form": "아뇨아뇨 요샌 음 사람만나는거 겁도나요 ㅋㅋ",
          "original_form": "아뇨아뇨 요샌 음 사람만나는거 겁도나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009481.1.22",
          "form": "왜요? 코로나때문에요?",
          "original_form": "왜요? 코로나때문에요?",
          "speaker_id": "1",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009481.1.23",
          "form": "아무래도? ㅋㅋ4인이상 모이지 말라고도 하고 ㅋㅋㅋ",
          "original_form": "아무래도? ㅋㅋ4인이상 모이지 말라고도 하고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009481.1.24",
          "form": "잉.. 그러면 4인모이면 되죠!",
          "original_form": "잉.. 그러면 4인모이면 되죠!",
          "speaker_id": "1",
          "time": "20210823 00:10"
        }
      ]
