      "utterance": [
        {
          "id": "MDRW2100005317.1.1",
          "form": "너는 반려동물 키운 경험 있어?",
          "original_form": "너는 반려동물 키운 경험 있어?",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005317.1.2",
          "form": "마당에서 진돗개 키웠었어",
          "original_form": "마당에서 진돗개 키웠었어",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005317.1.3",
          "form": "진돗개? 우와",
          "original_form": "진돗개? 우와",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005317.1.4",
          "form": "진짜 오래됐네",
          "original_form": "진짜 오래됐네",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005317.1.5",
          "form": "키우는 거 쉽지 않지",
          "original_form": "키우는 거 쉽지 않지",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005317.1.6",
          "form": "맞아 쉽지 않아",
          "original_form": "맞아 쉽지 않아",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005317.1.7",
          "form": "사료값 병원비도 많이 들고",
          "original_form": "사료값 병원비도 많이 들고",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005317.1.8",
          "form": "예방접종도 맞춰야하잖아",
          "original_form": "예방접종도 맞춰야하잖아",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005317.1.9",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005317.1.10",
          "form": "고양이 키우고 싶다",
          "original_form": "고양이 키우고 싶다",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005317.1.11",
          "form": "그래도 반려동물이 좋지",
          "original_form": "그래도 반려동물이 좋지",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005317.1.12",
          "form": "애니멀 테라피도 있잖아",
          "original_form": "애니멀 테라피도 있잖아",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005317.1.13",
          "form": "어떨땐 동물이 사람보다 나은 것 같기도 하고",
          "original_form": "어떨땐 동물이 사람보다 나은 것 같기도 하고",
          "speaker_id": "1",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005317.1.14",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005317.1.15",
          "form": "은혜 값은 강아지 막 그런 이야기도 있지",
          "original_form": "은혜 값은 강아지 막 그런 이야기도 있지",
