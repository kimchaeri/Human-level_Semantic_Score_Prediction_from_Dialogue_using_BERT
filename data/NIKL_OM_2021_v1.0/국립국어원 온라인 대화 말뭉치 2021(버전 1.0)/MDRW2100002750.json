        },
        {
          "id": "MDRW2100002750.1.2",
          "form": "점심은 드셨나요?",
          "original_form": "점심은 드셨나요?",
          "speaker_id": "1",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002750.1.3",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002750.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002750.1.5",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "2",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002750.1.6",
          "form": "네:) 저도 식사했어요^^",
          "original_form": "네:) 저도 식사했어요^^",
          "speaker_id": "1",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002750.1.7",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002750.1.8",
          "form": "ㅎㅎ 불닭볶음면이요",
          "original_form": "ㅎㅎ 불닭볶음면이요",
          "speaker_id": "2",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002750.1.9",
          "form": "뭐드신지 여쭤도 될까요",
          "original_form": "뭐드신지 여쭤도 될까요",
          "speaker_id": "2",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002750.1.10",
          "form": "오 맛있었겠어요:)",
          "original_form": "오 맛있었겠어요:)",
          "speaker_id": "1",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002750.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002750.1.12",
          "form": "맵찔이긴 하지만\r\n가끔씩 먹어요ㅋㅋㅋ",
          "original_form": "맵찔이긴 하지만\r\n가끔씩 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002750.1.13",
          "form": "저는 비빔면 먹었어용ㅋㅋ",
          "original_form": "저는 비빔면 먹었어용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002750.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002750.1.15",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "2",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002750.1.16",
          "form": "비빔면",
          "original_form": "비빔면",
          "speaker_id": "2",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002750.1.17",
          "form": "요샌 비빔면도 많아서ㅋㅋ",
          "original_form": "요샌 비빔면도 많아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002750.1.18",
          "form": "자주 먹진 못하지만",
          "original_form": "자주 먹진 못하지만",
          "speaker_id": "2",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002750.1.19",
          "form": "어디 비빔면 젤 좋아하세영?",
          "original_form": "어디 비빔면 젤 좋아하세영?",
          "speaker_id": "1",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002750.1.20",
          "form": "진비빔면이요 ㅎㅎ",
          "original_form": "진비빔면이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002750.1.21",
          "form": "어떤비빔면 드셨나요",
          "original_form": "어떤비빔면 드셨나요",
          "speaker_id": "2",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002750.1.22",
          "form": "저는 배홍동 먹었어요:)",
          "original_form": "저는 배홍동 먹었어요:)",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002750.1.23",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002750.1.24",
          "form": "진비빔면도 맛있쥬",
          "original_form": "진비빔면도 맛있쥬{emoji:♡}",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002750.1.25",
          "form": "엄청맛있다던데",
          "original_form": "엄청맛있다던데",
          "speaker_id": "2",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002750.1.26",
          "form": "호불호가 좀 있긴하던데",
          "original_form": "호불호가 좀 있긴하던데",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002750.1.27",
          "form": "한 번도 못먹어봤네요 ㅎㅎ",
          "original_form": "한 번도 못먹어봤네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002750.1.28",
          "form": "저는 괜찮더라구용ㅋㅋ",
          "original_form": "저는 괜찮더라구용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002750.1.29",
          "form": "치즈불닭볶음면도 드셔보셨어영?",
          "original_form": "치즈불닭볶음면도 드셔보셨어영?",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002750.1.30",
          "form": "이것도 맵찔이에겐\r\n맵긴한데 맛있더라구영ㅋㅋ",
          "original_form": "이것도 맵찔이에겐\r\n맵긴한데 맛있더라구영ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002750.1.31",
          "form": "아니요 아직이요 ㅎㅎ",
          "original_form": "아니요 아직이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002750.1.32",
          "form": "꼭 먹어봐야겠어뇨",
          "original_form": "꼭 먹어봐야겠어뇨",
          "speaker_id": "2",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002750.1.33",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "2",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002750.1.34",
          "form": "네ㅋㅋ 꼭 드셔보세요ㅋㅋ",
          "original_form": "네ㅋㅋ 꼭 드셔보세요ㅋㅋ",
