        },
        {
          "id": "MDRW2100019017.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019017.1.3",
          "form": "점심드셨나요?",
          "original_form": "점심드셨나요?",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019017.1.4",
          "form": "네 식사하셨어요?",
          "original_form": "네 식사하셨어요?",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019017.1.5",
          "form": "네! 육개장국밥 먹었어용",
          "original_form": "네! 육개장국밥 먹었어용",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019017.1.6",
          "form": "맛난거 드셨네용",
          "original_form": "맛난거 드셨네용",
          "speaker_id": "2",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100019017.1.7",
          "form": "네 ㅋㅋ 국밥좋아하시나요?",
          "original_form": "네 ㅋㅋ 국밥좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019017.1.8",
          "form": "네 설렁탕  갈비탕 추어탕",
          "original_form": "네 설렁탕  갈비탕 추어탕",
          "speaker_id": "2",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019017.1.9",
          "form": "탕 다 좋아요ㅋㅋ",
          "original_form": "탕 다 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019017.1.10",
          "form": "아하 탕하니까 낙지탕탕이 먹고싶네요 ㅋㅋ",
          "original_form": "아하 탕하니까 낙지탕탕이 먹고싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019017.1.11",
          "form": "아 진짜",
          "original_form": "아 진짜",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019017.1.12",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019017.1.13",
          "form": "ㅜㅜㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019017.1.14",
          "form": "낙지완전좋아요",
          "original_form": "낙지완전좋아요",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019017.1.15",
          "form": "저두요! 낙지볶음도 좋아해요",
          "original_form": "저두요! 낙지볶음도 좋아해요",
          "speaker_id": "1",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019017.1.16",
          "form": "진짜 맛있죠",
          "original_form": "진짜 맛있죠",
          "speaker_id": "2",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019017.1.17",
          "form": "입에도 좋고 몸에도 좋고",
          "original_form": "입에도 좋고 몸에도 좋고",
          "speaker_id": "2",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019017.1.18",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "1",
          "time": "20210827 15:17"
        }
      ]
