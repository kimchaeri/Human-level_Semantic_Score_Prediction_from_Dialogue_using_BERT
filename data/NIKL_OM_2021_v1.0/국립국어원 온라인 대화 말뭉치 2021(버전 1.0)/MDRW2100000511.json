      "utterance": [
        {
          "id": "MDRW2100000511.1.1",
          "form": "영화 뿐만 아니라 여행도 좋아하시나요?",
          "original_form": "영화 뿐만 아니라 여행도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000511.1.2",
          "form": "네 여행도 좋아하는데",
          "original_form": "네 여행도 좋아하는데",
          "speaker_id": "2",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000511.1.3",
          "form": "못 간지 너무 오래됐네요",
          "original_form": "못 간지 너무 오래됐네요",
          "speaker_id": "2",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000511.1.4",
          "form": "맞아요. 지금 제주도는 난리라네요.",
          "original_form": "맞아요. 지금 제주도는 난리라네요.",
          "speaker_id": "1",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000511.1.5",
          "form": "신혼여행으로도 많이 가고",
          "original_form": "신혼여행으로도 많이 가고",
          "speaker_id": "2",
          "time": "20210819 16:39"
        },
        {
          "id": "MDRW2100000511.1.6",
          "form": "여름휴가로도 많이 가겠지요?",
          "original_form": "여름휴가로도 많이 가겠지요?",
          "speaker_id": "2",
          "time": "20210819 16:39"
        },
        {
          "id": "MDRW2100000511.1.7",
          "form": "네 그런데 거리두기 격상되어서",
          "original_form": "네 그런데 거리두기 격상되어서",
          "speaker_id": "1",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000511.1.8",
          "form": "다 취소하고",
          "original_form": "다 취소하고",
          "speaker_id": "1",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000511.1.9",
          "form": "아..그렇구나",
          "original_form": "아..그렇구나",
          "speaker_id": "2",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000511.1.10",
          "form": "제주도 3단계인가요?",
          "original_form": "제주도 3단계인가요?",
          "speaker_id": "2",
          "time": "20210819 16:44"
        },
        {
          "id": "MDRW2100000511.1.11",
          "form": "4단계 되었대요.",
          "original_form": "4단계 되었대요.",
          "speaker_id": "1",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000511.1.12",
          "form": "어머나",
          "original_form": "어머나",
          "speaker_id": "2",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000511.1.13",
          "form": "4단계 됐구나",
          "original_form": "4단계 됐구나",
          "speaker_id": "2",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000511.1.14",
          "form": "다들 취소하느라 난리겠어요",
          "original_form": "다들 취소하느라 난리겠어요",
          "speaker_id": "2",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000511.1.15",
          "form": "네 렌터카도 2인만 가능하대요.",
          "original_form": "네 렌터카도 2인만 가능하대요.",
          "speaker_id": "1",
          "time": "20210819 16:56"
        },
        {
          "id": "MDRW2100000511.1.16",
          "form": "제주도 너무 좋은데!",
          "original_form": "제주도 너무 좋은데!",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000511.1.17",
          "form": "맛있는 것도 많고 바다도 예쁘고요",
          "original_form": "맛있는 것도 많고 바다도 예쁘고요",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000511.1.18",
          "form": "제주만의 갬성이 있지요ㅎㅎ",
          "original_form": "제주만의 갬성이 있지요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000511.1.19",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000511.1.20",
          "form": "재작년에 제주도 갔을 때 서핑도 했는데",
          "original_form": "재작년에 제주도 갔을 때 서핑도 했는데",
          "speaker_id": "2",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000511.1.21",
          "form": "너무 재밌었어요",
          "original_form": "너무 재밌었어요",
          "speaker_id": "2",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000511.1.22",
          "form": "와 서핑 해보고 싶어요. 요즘엔 서핑스팟이 명소더라고요.",
          "original_form": "와 서핑 해보고 싶어요. 요즘엔 서핑스팟이 명소더라고요.",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000511.1.23",
          "form": "원래 물을 좋아해서 서핑은 진짜 크으",
          "original_form": "원래 물을 좋아해서 서핑은 진짜 크으",
          "speaker_id": "2",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000511.1.24",
          "form": "근데 양양은 아직 못 가 봤어요",
          "original_form": "근데 양양은 아직 못 가 봤어요",
          "speaker_id": "2",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000511.1.25",
          "form": "양양은 위험할 지경이라는데ㅎㅎ",
          "original_form": "양양은 위험할 지경이라는데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000511.1.26",
          "form": "오 그래요?",
          "original_form": "오 그래요?",
          "speaker_id": "2",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000511.1.27",
          "form": "사람이 몰려서요?",
          "original_form": "사람이 몰려서요?",
          "speaker_id": "2",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000511.1.28",
          "form": "네 안쓰고 다니는 사람도 많대요. 여름 휴가 다녀오셨어요?",
          "original_form": "네 안쓰고 다니는 사람도 많대요. 여름 휴가 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000511.1.29",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210819 17:26"
        },
        {
          "id": "MDRW2100000511.1.30",
          "form": "겁쟁이라서 집콕이요",
          "original_form": "겁쟁이라서 집콕이요",
          "speaker_id": "2",
          "time": "20210819 17:26"
        },
        {
          "id": "MDRW2100000511.1.31",
          "form": "영화보고 드라마보면서 대리만족하고 있어요",
          "original_form": "영화보고 드라마보면서 대리만족하고 있어요",
          "speaker_id": "2",
          "time": "20210819 17:26"
        }
      ]
    }
