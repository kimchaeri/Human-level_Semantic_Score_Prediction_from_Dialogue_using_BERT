        {
          "id": "MDRW2100014092.1.2",
          "form": "자고있을듯",
          "original_form": "자고있을듯",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014092.1.3",
          "form": "강아지는 잠이 많잖아요",
          "original_form": "강아지는 잠이 많잖아요",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014092.1.4",
          "form": "ㅡ저 따라하시나욬ㅋ ㅋ ㅋ ㅋㅋ",
          "original_form": "ㅡ저 따라하시나욬ㅋ ㅋ ㅋ ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014092.1.5",
          "form": "전시회얘기 재미없어요",
          "original_form": "전시회얘기 재미없어요",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014092.1.6",
          "form": "아뮤튼 name3는 귀엽더라고요",
          "original_form": "아뮤튼 &name3&는 귀엽더라고요",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014092.1.7",
          "form": "귀엽죠",
          "original_form": "귀엽죠",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014092.1.8",
          "form": "저번에 홈씨씨티비봒을때",
          "original_form": "저번에 홈씨씨티비봒을때",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014092.1.9",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014092.1.10",
          "form": "또 name3야 하고 불러보세요",
          "original_form": "또 &name3&야 하고 불러보세요",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014092.1.11",
          "form": "궁금하니까",
          "original_form": "궁금하니까",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014092.1.12",
          "form": "강아지같이생김",
          "original_form": "강아지같이생김",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014092.1.13",
          "form": "지금요?",
          "original_form": "지금요?",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014092.1.14",
          "form": "호다닥 뛰어올듯",
          "original_form": "호다닥 뛰어올듯",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014092.1.15",
          "form": "0궁금",
          "original_form": "0궁금",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014092.1.16",
          "form": "우리name4는 뭐하고 있으려나",
          "original_form": "우리&name4&는 뭐하고 있으려나",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014092.1.17",
          "form": "빈둥빈둥하겠죠",
          "original_form": "빈둥빈둥하겠죠",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014092.1.18",
          "form": "오늘 산책담당 누구죠",
          "original_form": "오늘 산책담당 누구죠",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014092.1.19",
          "form": "오늘은 동생입니다",
          "original_form": "오늘은 동생입니다",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014092.1.20",
          "form": "여!동생",
          "original_form": "여!동생",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014092.1.21",
          "form": "ㅋㅋㅋ난 왜",
          "original_form": "ㅋㅋㅋ난 왜",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014092.1.22",
          "form": "남동생으로 알았지;;",
          "original_form": "남동생으로 알았지;;",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014092.1.23",
          "form": "진짜 넘하시네요!",
          "original_form": "진짜 넘하시네요!",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014092.1.24",
          "form": "이름은 name4입니다",
          "original_form": "이름은 &name4&입니다",
          "speaker_id": "1",
          "time": "20210825 17:23"
