        {
          "id": "MDRW2100003757.1.1",
          "form": "name1님 요새 구입한 물건 있으세요?",
          "original_form": "&name1&님 요새 구입한 물건 있으세요?",
          "speaker_id": "2",
          "time": "20210821 11:50"
        },
        {
          "id": "MDRW2100003757.1.2",
          "form": "최근에 테이프클리너 구입했어요",
          "original_form": "최근에 테이프클리너 구입했어요",
          "speaker_id": "1",
          "time": "20210821 11:50"
        },
        {
          "id": "MDRW2100003757.1.3",
          "form": "오 테이프클리너면 그 청소할때 쓰는",
          "original_form": "오 테이프클리너면 그 청소할때 쓰는",
          "speaker_id": "2",
          "time": "20210821 11:50"
        },
        {
          "id": "MDRW2100003757.1.4",
          "form": "돌돌이 같은건가요?",
          "original_form": "돌돌이 같은건가요?",
          "speaker_id": "2",
          "time": "20210821 11:50"
        },
        {
          "id": "MDRW2100003757.1.5",
          "form": "맞아요 ㅎ 뭔가 청소기나 빗자루만으로는 안되는게 있더라구요",
          "original_form": "맞아요 ㅎ 뭔가 청소기나 빗자루만으로는 안되는게 있더라구요",
          "speaker_id": "1",
          "time": "20210821 11:50"
        },
        {
          "id": "MDRW2100003757.1.6",
          "form": "name2님은 최근에 구매한거 있나요?",
          "original_form": "&name2&님은 최근에 구매한거 있나요?",
          "speaker_id": "1",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003757.1.7",
          "form": "저는 갤럭시워치 구매했어요!",
          "original_form": "저는 갤럭시워치 구매했어요!",
          "speaker_id": "2",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003757.1.8",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003757.1.9",
          "form": "그리고 아이패드 케이스도 샀어요",
          "original_form": "그리고 아이패드 케이스도 샀어요",
          "speaker_id": "2",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003757.1.10",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "1",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003757.1.11",
          "form": "둘 다 아직 오기 전이랍니다..ㅠㅠ",
          "original_form": "둘 다 아직 오기 전이랍니다..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003757.1.12",
          "form": "9월 넘어 올 거 같아요",
          "original_form": "9월 넘어 올 거 같아요",
          "speaker_id": "2",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003757.1.13",
          "form": "앗 엄청 설레시겠어요",
          "original_form": "앗 엄청 설레시겠어요",
          "speaker_id": "1",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003757.1.14",
          "form": "헉 9월 왤케 늦어요?",
          "original_form": "헉 9월 왤케 늦어요?",
          "speaker_id": "1",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003757.1.15",
          "form": "맞아요 엄청 찾아보게 되더라구요",
          "original_form": "맞아요 엄청 찾아보게 되더라구요",
          "speaker_id": "2",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003757.1.16",
          "form": "이번에 주문이 진짜 많았나봐요",
          "original_form": "이번에 주문이 진짜 많았나봐요",
          "speaker_id": "2",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003757.1.17",
          "form": "아 주문폭주",
          "original_form": "아 주문폭주",
          "speaker_id": "1",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003757.1.18",
          "form": "그 기다림이 너무 설렐거같아요",
          "original_form": "그 기다림이 너무 설렐거같아요",
