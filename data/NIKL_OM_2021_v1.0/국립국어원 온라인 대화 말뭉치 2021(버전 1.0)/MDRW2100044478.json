      "utterance": [
        {
          "id": "MDRW2100044478.1.1",
          "form": "네 저 라떼",
          "original_form": "네 저 라떼",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044478.1.2",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044478.1.3",
          "form": "요즘은 막 엄청 대용량도 잇더라고요",
          "original_form": "요즘은 막 엄청 대용량도 잇더라고요",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044478.1.4",
          "form": "오 일리터 이런데요? ㅋㅋㅋ",
          "original_form": "오 일리터 이런데요? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044478.1.5",
          "form": "네ㅋㅋ보기만 해도 배불러요",
          "original_form": "네ㅋㅋ보기만 해도 배불러요",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044478.1.6",
          "form": "ㅋㅋㅋㅋ 진짜 넘 많아요 ㅋㅋ큐",
          "original_form": "ㅋㅋㅋㅋ 진짜 넘 많아요 ㅋㅋ큐",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044478.1.7",
          "form": "어떤커피 조아하세요",
          "original_form": "어떤커피 조아하세요",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044478.1.8",
          "form": "전 커피보단 과일음료요ㅋㅋ",
          "original_form": "전 커피보단 과일음료요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044478.1.9",
          "form": "오 예를들면?",
          "original_form": "오 예를들면?",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044478.1.10",
          "form": "자몽도 좋아하고\r\n여름엔 수박많이 먹엇어여ㅋ",
          "original_form": "자몽도 좋아하고\r\n여름엔 수박많이 먹엇어여ㅋ",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044478.1.11",
          "form": "우와수박",
          "original_form": "우와수박",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044478.1.12",
          "form": "수박쥬스요?",
          "original_form": "수박쥬스요?",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044478.1.13",
          "form": "네 은근 맛나요",
          "original_form": "네 은근 맛나요",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044478.1.14",
          "form": "수박쥬스 엄청맛있겟다ㅠ",
          "original_form": "수박쥬스 엄청맛있겟다ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044478.1.15",
          "form": "지금먹고싶네요",
          "original_form": "지금먹고싶네요",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044478.1.16",
          "form": "네 요즘 날이 추은듯 더워서 시원하게 먹음 좋아요",
          "original_form": "네 요즘 날이 추은듯 더워서 시원하게 먹음 좋아요",
          "speaker_id": "1",
          "time": "20210907 23:02"
        }
