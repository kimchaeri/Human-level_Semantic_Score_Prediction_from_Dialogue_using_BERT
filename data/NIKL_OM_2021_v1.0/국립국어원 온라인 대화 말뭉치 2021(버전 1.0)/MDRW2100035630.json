      "utterance": [
        {
          "id": "MDRW2100035630.1.1",
          "form": "우리손님이 라면을 대령하랍신다!!!는 느낌",
          "original_form": "우리손님이 라면을 대령하랍신다!!!는 느낌",
          "speaker_id": "2",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100035630.1.2",
          "form": "오사카 사람들 약간 그 번화가 뒷길에",
          "original_form": "오사카 사람들 약간 그 번화가 뒷길에",
          "speaker_id": "1",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100035630.1.3",
          "form": "유흥주점에서 종사하시는분들 같았는데",
          "original_form": "유흥주점에서 종사하시는분들 같았는데",
          "speaker_id": "1",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100035630.1.4",
          "form": "저는 편의점에서 특이해보이는 거 이것저것 사먹어봤어요ㅋㅋ",
          "original_form": "저는 편의점에서 특이해보이는 거 이것저것 사먹어봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100035630.1.5",
          "form": "한국인인거 알아보더니 친절하게 길 알려주셨어요!",
          "original_form": "한국인인거 알아보더니 친절하게 길 알려주셨어요!",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100035630.1.6",
          "form": "오 친절하셔라",
          "original_form": "오 친절하셔라",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100035630.1.7",
          "form": "편의점 음식이 의외로 너무 맛있더라구욬ㅋㅋㅋㅋ",
          "original_form": "편의점 음식이 의외로 너무 맛있더라구욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100035630.1.8",
          "form": "ㅋㅋ그죠",
          "original_form": "ㅋㅋ그죠",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100035630.1.9",
          "form": "에그 샌드위치 진짜 맛있고",
          "original_form": "에그 샌드위치 진짜 맛있고",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100035630.1.10",
          "form": "근데 제가 일본어를 하나도 못읽어서",
          "original_form": "근데 제가 일본어를 하나도 못읽어서",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100035630.1.11",
          "form": "컵라면도 맛있고ㅋㅋㅋㅋ",
          "original_form": "컵라면도 맛있고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100035630.1.12",
          "form": "그림만 보고 샀어요",
          "original_form": "그림만 보고 샀어요",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100035630.1.13",
          "form": "아 그 파파고 어플에",
          "original_form": "아 그 파파고 어플에",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100035630.1.14",
          "form": "사진으로 찍으면 바로 한글로 번역되는 기능 있어용",
          "original_form": "사진으로 찍으면 바로 한글로 번역되는 기능 있어용",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100035630.1.15",
          "form": "여행 다니실때 참고하시면 좋아용",
          "original_form": "여행 다니실때 참고하시면 좋아용",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100035630.1.16",
          "form": "네 그거쎃어요ㅋㅋ",
          "original_form": "네 그거쎃어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100035630.1.17",
          "form": "근데 이것도",
          "original_form": "근데 이것도",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100035630.1.18",
          "form": "배터리닳고",
          "original_form": "배터리닳고",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100035630.1.19",
          "form": "그리고 검은건 김 분홍색은 명란 초록색은 와사비더라구요",
          "original_form": "그리고 검은건 김 분홍색은 명란 초록색은 와사비더라구요",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100035630.1.20",
          "form": "귀찮아서 그냥 사게되는거 있져ㅋㅋㅋ",
          "original_form": "귀찮아서 그냥 사게되는거 있져ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100035630.1.21",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100035630.1.22",
          "form": "초록색 와사비 진짜 많았어요",
          "original_form": "초록색 와사비 진짜 많았어요",
          "speaker_id": "1",
          "time": "20210829 21:36"
        }
      ]
    }
