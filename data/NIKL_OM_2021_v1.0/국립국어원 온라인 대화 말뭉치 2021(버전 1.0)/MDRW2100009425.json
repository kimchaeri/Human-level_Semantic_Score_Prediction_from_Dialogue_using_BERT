      "utterance": [
        {
          "id": "MDRW2100009425.1.1",
          "form": "일해요?",
          "original_form": "일해요?",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009425.1.2",
          "form": "전업이에요~",
          "original_form": "전업이에요~",
          "speaker_id": "2",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009425.1.3",
          "form": "주부도 힘들죠",
          "original_form": "주부도 힘들죠",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009425.1.4",
          "form": "네ㅠㅠ 쉬운것 없는것 같아요. 돈버는게 차라리 나은듯 합니다ㅜ",
          "original_form": "네ㅠㅠ 쉬운것 없는것 같아요. 돈버는게 차라리 나은듯 합니다ㅜ",
          "speaker_id": "2",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009425.1.5",
          "form": "그런가요?",
          "original_form": "그런가요?",
          "speaker_id": "1",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009425.1.6",
          "form": "ㅋㅋ남자분이신거죠?ㅋ",
          "original_form": "ㅋㅋ남자분이신거죠?ㅋ",
          "speaker_id": "2",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009425.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009425.1.8",
          "form": "대답에서 느껴집니다ㅋㅋㅋ 보통 일하는게 나은것 같다고 하면ㅋㄱ",
          "original_form": "대답에서 느껴집니다ㅋㅋㅋ 보통 일하는게 나은것 같다고 하면ㅋㄱ",
          "speaker_id": "2",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009425.1.9",
          "form": "대게 여자들은 맞다고 ㅋㅋ",
          "original_form": "대게 여자들은 맞다고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009425.1.10",
          "form": "옛말에 일할래 애볼래 하면 일한다고 하잖아요ㅋㅋ",
          "original_form": "옛말에 일할래 애볼래 하면 일한다고 하잖아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009425.1.11",
          "form": "육아가 젤 힘들죠",
          "original_form": "육아가 젤 힘들죠",
          "speaker_id": "1",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009425.1.12",
          "form": "크게공감대를 형성 못하시는거보니 그럴것 같았습니다ㅎ 저희 신랑도ㅋㄱ집에서 저 논다고 생각하니ㅋㅋㅋ",
          "original_form": "크게공감대를 형성 못하시는거보니 그럴것 같았습니다ㅎ 저희 신랑도ㅋㄱ집에서 저 논다고 생각하니ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009425.1.13",
          "form": "일하는것보다ㅋㅋ애보는게 낮다고 하더라구요ㅋㅈㅋ",
          "original_form": "일하는것보다ㅋㅋ애보는게 낮다고 하더라구요ㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009425.1.14",
          "form": "전 주말에 얘랑 노니 녹초가되어",
          "original_form": "전 주말에 얘랑 노니 녹초가되어",
          "speaker_id": "1",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009425.1.15",
          "form": "ㅋㅋㅋ아들인가요오?",
          "original_form": "ㅋㅋㅋ아들인가요오?",
          "speaker_id": "2",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009425.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009425.1.17",
          "form": "초딩이요",
          "original_form": "초딩이요",
