      "utterance": [
        {
          "id": "MDRW2100042967.1.1",
          "form": "날이춥죠",
          "original_form": "날이춥죠",
          "speaker_id": "1",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100042967.1.2",
          "form": "저는 땀쟁이라서아직 여름옷차림임다",
          "original_form": "저는 땀쟁이라서아직 여름옷차림임다",
          "speaker_id": "2",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100042967.1.3",
          "form": "ㅎㅎ건강하네요",
          "original_form": "ㅎㅎ건강하네요",
          "speaker_id": "1",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100042967.1.4",
          "form": "저는가디건꺼냈어요",
          "original_form": "저는가디건꺼냈어요",
          "speaker_id": "1",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100042967.1.5",
          "form": "일할때는 추워서 가디건입어요ㅋㅋ",
          "original_form": "일할때는 추워서 가디건입어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100042967.1.6",
          "form": "가을옷 장만하셧나요",
          "original_form": "가을옷 장만하셧나요",
          "speaker_id": "2",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100042967.1.7",
          "form": "가디건하나샀어요",
          "original_form": "가디건하나샀어요",
          "speaker_id": "1",
          "time": "20210907 12:17"
        },
        {
          "id": "MDRW2100042967.1.8",
          "form": "ㅎㅅㅎ거기도비오나요?",
          "original_form": "ㅎㅅㅎ거기도비오나요?",
          "speaker_id": "1",
          "time": "20210907 12:17"
        },
        {
          "id": "MDRW2100042967.1.9",
          "form": "올거같아서 우산챙겻어요",
          "original_form": "올거같아서 우산챙겻어요",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042967.1.10",
          "form": "가을장마 그만햇음좋겟네요",
          "original_form": "가을장마 그만햇음좋겟네요",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042967.1.11",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042967.1.12",
          "form": "비오니까습하네요",
          "original_form": "비오니까습하네요",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042967.1.13",
          "form": "좋아하는계절잇으신가요",
          "original_form": "좋아하는계절잇으신가요",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042967.1.14",
          "form": "저는봄이요",
          "original_form": "저는봄이요",
          "speaker_id": "1",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042967.1.15",
          "form": "저는 여름빼고 싹다 좋슴다",
          "original_form": "저는 여름빼고 싹다 좋슴다",
          "speaker_id": "2",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042967.1.16",
          "form": "더운거싫죠ㅎㅎ",
          "original_form": "더운거싫죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100042967.1.17",
          "form": "네ㅋ 지금 여름끝나서 무지행복함다",
          "original_form": "네ㅋ 지금 여름끝나서 무지행복함다",
