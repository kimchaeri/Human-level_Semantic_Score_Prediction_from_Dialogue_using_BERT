      "utterance": [
        {
          "id": "MDRW2100028219.1.1",
          "form": "저도 집꾸미기인줄알았는데 뭔퍼즐게임이라면서요?",
          "original_form": "저도 집꾸미기인줄알았는데 뭔퍼즐게임이라면서요?",
          "speaker_id": "1",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100028219.1.2",
          "form": "앗 이거 계속 해도 되는 거였군요!!!!!",
          "original_form": "앗 이거 계속 해도 되는 거였군요!!!!!",
          "speaker_id": "2",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100028219.1.3",
          "form": "맞아요 그냥 캔디크러쉬사가랑 똑같더라고요",
          "original_form": "맞아요 그냥 캔디크러쉬사가랑 똑같더라고요",
          "speaker_id": "2",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100028219.1.4",
          "form": "저도 그런거 진짜 질려서...",
          "original_form": "저도 그런거 진짜 질려서...",
          "speaker_id": "2",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028219.1.5",
          "form": "네네 ㅋㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028219.1.6",
          "form": "전안해봤는데 여기서하시는분이 설명해주셨어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전안해봤는데 여기서하시는분이 설명해주셨어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028219.1.7",
          "form": "와 엄청 잘 만나셨다!!! ㅋㅋㅋ",
          "original_form": "와 엄청 잘 만나셨다!!! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028219.1.8",
          "form": "전에 온라인 겜은 뭐하셨었어요?",
          "original_form": "전에 온라인 겜은 뭐하셨었어요?",
          "speaker_id": "2",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100028219.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ저 온라인게임은 오버워치 아주잠깐해봤었고",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저 온라인게임은 오버워치 아주잠깐해봤었고",
          "speaker_id": "1",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100028219.1.10",
          "form": "메이플이랑 마비노기 마영전 이런류",
          "original_form": "메이플이랑 마비노기 마영전 이런류",
          "speaker_id": "1",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100028219.1.11",
          "form": "했었어요",
          "original_form": "했었어요",
          "speaker_id": "1",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100028219.1.12",
          "form": "아!!! 전 파판도 잠깐 해봤어요",
          "original_form": "아!!! 전 파판도 잠깐 해봤어요",
          "speaker_id": "2",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100028219.1.13",
          "form": "rpg류 해보셨구나",
          "original_form": "rpg류 해보셨구나",
          "speaker_id": "2",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100028219.1.14",
          "form": "네 파판은안해보고",
          "original_form": "네 파판은안해보고",
          "speaker_id": "1",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100028219.1.15",
          "form": "마비노기 학창시절에 미쳤었죠..",
          "original_form": "마비노기 학창시절에 미쳤었죠..",
          "speaker_id": "1",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100028219.1.16",
          "form": "진짜 마비노기 제 친구들도",
          "original_form": "진짜 마비노기 제 친구들도",
          "speaker_id": "2",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100028219.1.17",
          "form": "많이 했었어요",
          "original_form": "많이 했었어요",
          "speaker_id": "2",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100028219.1.18",
          "form": "룩 꾸미고",
          "original_form": "룩 꾸미고",
          "speaker_id": "2",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100028219.1.19",
          "form": "네 넥슨이 돈슨이라 불리기시작한..",
          "original_form": "네 넥슨이 돈슨이라 불리기시작한..",
          "speaker_id": "1",
          "time": "20210902 18:27"
        },
        {
          "id": "MDRW2100028219.1.20",
          "form": "확률에 돈맛본게 거기서시작이었을거에요..",
          "original_form": "확률에 돈맛본게 거기서시작이었을거에요..",
          "speaker_id": "1",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100028219.1.21",
          "form": "휴..생각하니 화난다",
          "original_form": "휴..생각하니 화난다",
          "speaker_id": "2",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100028219.1.22",
          "form": "자꾸 뽑기 시키고",
          "original_form": "자꾸 뽑기 시키고",
          "speaker_id": "2",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100028219.1.23",
          "form": "전 펫에돈많이썼었죠..",
          "original_form": "전 펫에돈많이썼었죠..",
          "speaker_id": "1",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100028219.1.24",
          "form": "거의 강원랜드인줄 알았잖아요",
          "original_form": "거의 강원랜드인줄 알았잖아요",
          "speaker_id": "2",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100028219.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100028219.1.26",
          "form": "온갖군데에서 뽑기 나오고........",
          "original_form": "온갖군데에서 뽑기 나오고........",
          "speaker_id": "2",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100028219.1.27",
          "form": "뽑기나오면 재미없어지기시작..",
          "original_form": "뽑기나오면 재미없어지기시작..",
