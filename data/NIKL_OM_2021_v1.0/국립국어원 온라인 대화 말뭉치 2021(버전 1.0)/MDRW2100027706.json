      "utterance": [
        {
          "id": "MDRW2100027706.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027706.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027706.1.3",
          "form": "요즘 챙겨보는 프로그램 있나요?",
          "original_form": "요즘 챙겨보는 프로그램 있나요?",
          "speaker_id": "2",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100027706.1.4",
          "form": "저는 유퀴즈 챙겨봐요",
          "original_form": "저는 유퀴즈 챙겨봐요",
          "speaker_id": "1",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100027706.1.5",
          "form": "어제는 바빠서 못봤지만요ㅠ",
          "original_form": "어제는 바빠서 못봤지만요ㅠ",
          "speaker_id": "1",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100027706.1.6",
          "form": "유퀴즈 재밌죠 ㅎㅎ",
          "original_form": "유퀴즈 재밌죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100027706.1.7",
          "form": "저는 어제 스트릿우먼파이터 봤어요",
          "original_form": "저는 어제 스트릿우먼파이터 봤어요",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100027706.1.8",
          "form": "헐 저도 봤어요",
          "original_form": "헐 저도 봤어요",
          "speaker_id": "1",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100027706.1.9",
          "form": "완전 재밌지 않나요? 흥미진진해요",
          "original_form": "완전 재밌지 않나요? 흥미진진해요",
          "speaker_id": "2",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100027706.1.10",
          "form": "춤 추는거 너무 멋있어요",
          "original_form": "춤 추는거 너무 멋있어요",
          "speaker_id": "1",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100027706.1.11",
          "form": "어느 분 좋아하세요?",
          "original_form": "어느 분 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100027706.1.12",
          "form": "저는 허니제이랑 리정이 멋있더라구요 ㅎ",
          "original_form": "저는 허니제이랑 리정이 멋있더라구요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100027706.1.13",
          "form": "저도 리정님 좋아해요",
          "original_form": "저도 리정님 좋아해요",
          "speaker_id": "1",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027706.1.14",
          "form": "어제 인스타도 바로 팔로우했어요",
          "original_form": "어제 인스타도 바로 팔로우했어요",
          "speaker_id": "1",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027706.1.15",
          "form": "빠르시네요!",
          "original_form": "빠르시네요!",
          "speaker_id": "2",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027706.1.16",
          "form": "전 유튜브로 찾아봣어요",
          "original_form": "전 유튜브로 찾아봣어요",
          "speaker_id": "2",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027706.1.17",
          "form": "저는 전편이 궁금해서 재방송 봤어요",
          "original_form": "저는 전편이 궁금해서 재방송 봤어요",
          "speaker_id": "1",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100027706.1.18",
          "form": "아 2회 먼저 봣나봐요!",
          "original_form": "아 2회 먼저 봣나봐요!",
          "speaker_id": "2",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027706.1.19",
          "form": "아하 그러셨군요",
          "original_form": "아하 그러셨군요",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027706.1.20",
          "form": "요즘 재밌는 프로그램이 별로 없었는데",
          "original_form": "요즘 재밌는 프로그램이 별로 없었는데",
          "speaker_id": "2",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027706.1.21",
          "form": "이런 것도 좋은 것 같아요",
          "original_form": "이런 것도 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210902 15:00"
        }
      ]
