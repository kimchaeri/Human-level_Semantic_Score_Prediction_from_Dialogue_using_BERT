        {
          "id": "MDRW2100032189.1.1",
          "form": "어떤거드세요?",
          "original_form": "어떤거드세요?",
          "speaker_id": "2",
          "time": "20210905 14:56"
        },
        {
          "id": "MDRW2100032189.1.2",
          "form": "치킨과 칵테일을 좋아해요",
          "original_form": "치킨과 칵테일을 좋아해요",
          "speaker_id": "1",
          "time": "20210906 06:07"
        },
        {
          "id": "MDRW2100032189.1.3",
          "form": "저도 치킨과 칵테일을 좋아해요",
          "original_form": "저도 치킨과 칵테일을 좋아해요",
          "speaker_id": "2",
          "time": "20210906 07:08"
        },
        {
          "id": "MDRW2100032189.1.4",
          "form": "칵테일의 유래를 아세요?",
          "original_form": "칵테일의 유래를 아세요?",
          "speaker_id": "2",
          "time": "20210906 07:08"
        },
        {
          "id": "MDRW2100032189.1.5",
          "form": "아니요 역사는 잘모르겠어요",
          "original_form": "아니요 역사는 잘모르겠어요",
          "speaker_id": "1",
          "time": "20210906 16:02"
        },
        {
          "id": "MDRW2100032189.1.6",
          "form": "술을 수탉의 꼬리깃으로 저어 마시던게 유래가 되어 칵테일이 되었다고 알고있어요",
          "original_form": "술을 수탉의 꼬리깃으로 저어 마시던게 유래가 되어 칵테일이 되었다고 알고있어요",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100032189.1.7",
          "form": "그런설이 있다고해요",
          "original_form": "그런설이 있다고해요",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100032189.1.8",
          "form": "잉??",
          "original_form": "잉??",
          "speaker_id": "1",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100032189.1.9",
          "form": "수탉의꼬리깃이요???",
          "original_form": "수탉의꼬리깃이요???",
          "speaker_id": "1",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100032189.1.10",
          "form": "칵테일 되게 맛있는데",
          "original_form": "칵테일 되게 맛있는데",
          "speaker_id": "1",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100032189.1.11",
          "form": "그런설이 있구낭",
          "original_form": "그런설이 있구낭",
          "speaker_id": "1",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100032189.1.12",
          "form": "네네 칵테일 어떤거 좋아하세요?",
          "original_form": "네네 칵테일 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100032189.1.13",
          "form": "저는 많이 마셔보진않아서",
          "original_form": "저는 많이 마셔보진않아서",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100032189.1.14",
          "form": "칵테일종류는잘모르겠어요",
          "original_form": "칵테일종류는잘모르겠어요",
          "speaker_id": "1",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100032189.1.15",
          "form": "도수있는거 먹어봤어요",
          "original_form": "도수있는거 먹어봤어요",
          "speaker_id": "1",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100032189.1.16",
          "form": "술 좋아하세요?",
          "original_form": "술 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100032189.1.17",
          "form": "술은 안좋아하고 칵테일류 좋아해요",
          "original_form": "술은 안좋아하고 칵테일류 좋아해요",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100032189.1.18",
          "form": "칵테일은 도수가 있어도 괜찮거든요",
          "original_form": "칵테일은 도수가 있어도 괜찮거든요",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100032189.1.19",
          "form": "그러시구나 저는 와인이좋아요",
          "original_form": "그러시구나 저는 와인이좋아요",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100032189.1.20",
          "form": "와인도맛있죠!!",
          "original_form": "와인도맛있죠!!",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100032189.1.21",
          "form": "와인먹어봤어요",
          "original_form": "와인먹어봤어요",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100032189.1.22",
          "form": "진짜맛있더라구요",
          "original_form": "진짜맛있더라구요",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100032189.1.23",
          "form": "칵테일만큼요!!",
          "original_form": "칵테일만큼요!!",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100032189.1.24",
          "form": "네 와이프가  와인을 좋아해서 종종 먹어요",
          "original_form": "네 와이프가  와인을 좋아해서 종종 먹어요",
