      "utterance": [
        {
          "id": "MDRW2100033743.1.1",
          "form": "발명걸 알지",
          "original_form": "발명걸 알지",
          "speaker_id": "2",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100033743.1.2",
          "form": "너도 발명걸 알아?",
          "original_form": "너도 발명걸 알아?",
          "speaker_id": "2",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100033743.1.3",
          "form": "응응 그 유투버 너무 재미있어",
          "original_form": "응응 그 유투버 너무 재미있어",
          "speaker_id": "1",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100033743.1.4",
          "form": "폐자원으로 신기한 걸 많이 만들어내는 유튜버야.",
          "original_form": "폐자원으로 신기한 걸 많이 만들어내는 유튜버야.",
          "speaker_id": "2",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100033743.1.5",
          "form": "아 아는구나!",
          "original_form": "아 아는구나!",
          "speaker_id": "2",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033743.1.6",
          "form": "광고영상인데도 웃기게 만들어",
          "original_form": "광고영상인데도 웃기게 만들어",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033743.1.7",
          "form": "무슨 과를 다니는 걸까",
          "original_form": "무슨 과를 다니는 걸까",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033743.1.8",
          "form": "맞아. 최근 광고영상에 푹 빠져버렸어",
          "original_form": "맞아. 최근 광고영상에 푹 빠져버렸어",
          "speaker_id": "2",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033743.1.9",
          "form": "아이디어가 너무 신박해",
          "original_form": "아이디어가 너무 신박해",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033743.1.10",
          "form": "너도 봤구나",
          "original_form": "너도 봤구나",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033743.1.11",
          "form": "컴퓨터도 자유자재로 만지더라",
          "original_form": "컴퓨터도 자유자재로 만지더라",
          "speaker_id": "2",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033743.1.12",
          "form": "역시 나랑 취향이 비슷해",
          "original_form": "역시 나랑 취향이 비슷해",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033743.1.13",
          "form": "나는 그들을 실제로 만나보고싶어",
          "original_form": "나는 그들을 실제로 만나보고싶어",
          "speaker_id": "2",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033743.1.14",
          "form": "맞아 친구 놀리는데 진심이더라",
          "original_form": "맞아 친구 놀리는데 진심이더라",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033743.1.15",
          "form": "그들은 매우 재치있어.",
          "original_form": "그들은 매우 재치있어.",
          "speaker_id": "2",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033743.1.16",
          "form": "나두 얼굴이 너무 궁금해",
          "original_form": "나두 얼굴이 너무 궁금해",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033743.1.17",
          "form": "그들은 나와 비슷해",
          "original_form": "그들은 나와 비슷해",
          "speaker_id": "2",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033743.1.18",
          "form": "나도 친구를 놀리고싶어",
          "original_form": "나도 친구를 놀리고싶어",
          "speaker_id": "2",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033743.1.19",
          "form": "어떤?",
          "original_form": "어떤?",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033743.1.20",
          "form": "아아 그건 나도 그래",
          "original_form": "아아 그건 나도 그래",
          "speaker_id": "1",
          "time": "20210824 23:59"
        }
      ]
