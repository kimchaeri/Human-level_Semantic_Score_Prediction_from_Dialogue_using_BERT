      "utterance": [
        {
          "id": "MDRW2100047245.1.1",
          "form": "아르바이트 경험이 있으신가요?",
          "original_form": "아르바이트 경험이 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100047245.1.2",
          "form": "네 있어요",
          "original_form": "네 있어요",
          "speaker_id": "1",
          "time": "20210826 12:29"
        },
        {
          "id": "MDRW2100047245.1.3",
          "form": "어떤 아르바이트를 해보셨나요?",
          "original_form": "어떤 아르바이트를 해보셨나요?",
          "speaker_id": "2",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100047245.1.4",
          "form": "사무보조 알바해봤어요",
          "original_form": "사무보조 알바해봤어요",
          "speaker_id": "1",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100047245.1.5",
          "form": "알바 구직 앱에서 구하셨나요?",
          "original_form": "알바 구직 앱에서 구하셨나요?",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100047245.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100047245.1.7",
          "form": "업무는 괜찮으셨나요?",
          "original_form": "업무는 괜찮으셨나요?",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100047245.1.8",
          "form": "단순업무여서 쉬웠어요",
          "original_form": "단순업무여서 쉬웠어요",
          "speaker_id": "1",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100047245.1.9",
          "form": "어느 정도 기간동안 하셨나요?",
          "original_form": "어느 정도 기간동안 하셨나요?",
          "speaker_id": "2",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100047245.1.10",
          "form": "6개월이요",
          "original_form": "6개월이요",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100047245.1.11",
          "form": "길게 하셨네요! 하루에 몇 시간 하셨나요?",
          "original_form": "길게 하셨네요! 하루에 몇 시간 하셨나요?",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100047245.1.12",
          "form": "풀타임이요!",
          "original_form": "풀타임이요!",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100047245.1.13",
          "form": "단순 사무여도 피곤하셨겠네요ㅠ",
          "original_form": "단순 사무여도 피곤하셨겠네요ㅠ",
