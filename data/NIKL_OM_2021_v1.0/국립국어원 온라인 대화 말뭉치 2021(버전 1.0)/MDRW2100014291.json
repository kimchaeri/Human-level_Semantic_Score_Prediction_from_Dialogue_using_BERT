      "utterance": [
        {
          "id": "MDRW2100014291.1.1",
          "form": "맞아 거북이 귀여워",
          "original_form": "맞아 거북이 귀여워",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014291.1.2",
          "form": "근데 거북이 냄새 엄청 심해",
          "original_form": "근데 거북이 냄새 엄청 심해",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014291.1.3",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014291.1.4",
          "form": "응 내 친구가 전에 거북이 키웠었어",
          "original_form": "응 내 친구가 전에 거북이 키웠었어",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014291.1.5",
          "form": "어땠어?",
          "original_form": "어땠어?",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014291.1.6",
          "form": "엄청 귀여워 되게 천천히 움직여",
          "original_form": "엄청 귀여워 되게 천천히 움직여",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014291.1.7",
          "form": "귀엽긴 하더라",
          "original_form": "귀엽긴 하더라",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014291.1.8",
          "form": "모양도 되게 특이했어",
          "original_form": "모양도 되게 특이했어",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014291.1.9",
          "form": "어땠는데?",
          "original_form": "어땠는데?",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014291.1.10",
          "form": "무늬 같은 게 되게 신기했어",
          "original_form": "무늬 같은 게 되게 신기했어",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014291.1.11",
          "form": "맞아 무늬 특이하지",
          "original_form": "맞아 무늬 특이하지",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014291.1.12",
          "form": "그리고 엄청 천천히 움직이더라",
          "original_form": "그리고 엄청 천천히 움직이더라",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014291.1.13",
          "form": "많이  느려?",
          "original_form": "많이  느려?",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014291.1.14",
          "form": "응 약간 달팽이 느낌?",
          "original_form": "응 약간 달팽이 느낌?",
          "speaker_id": "1",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014291.1.15",
          "form": "키워보고 싶은 동물 있어?",
          "original_form": "키워보고 싶은 동물 있어?",
