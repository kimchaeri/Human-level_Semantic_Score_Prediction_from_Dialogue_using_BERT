      "utterance": [
        {
          "id": "MDRW2100027261.1.1",
          "form": "저 이제 밥먹으러가요",
          "original_form": "저 이제 밥먹으러가요",
          "speaker_id": "1",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027261.1.2",
          "form": "점심요",
          "original_form": "점심요",
          "speaker_id": "1",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027261.1.3",
          "form": "벌써요ㅎㅎ 식사 맛있게 하세요",
          "original_form": "벌써요ㅎㅎ 식사 맛있게 하세요",
          "speaker_id": "2",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027261.1.4",
          "form": "밥다먹었어요ㅎㅎ",
          "original_form": "밥다먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 11:27"
        },
        {
          "id": "MDRW2100027261.1.5",
          "form": "아침안먹어서 배고팠는데",
          "original_form": "아침안먹어서 배고팠는데",
          "speaker_id": "1",
          "time": "20210902 11:27"
        },
        {
          "id": "MDRW2100027261.1.6",
          "form": "점심먹고나니깐 피곤때문에 잠이올려구하네요",
          "original_form": "점심먹고나니깐 피곤때문에 잠이올려구하네요",
          "speaker_id": "1",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100027261.1.7",
          "form": "곧 퇴근시간 아닌가요?",
          "original_form": "곧 퇴근시간 아닌가요?",
          "speaker_id": "2",
          "time": "20210902 16:51"
        },
        {
          "id": "MDRW2100027261.1.8",
          "form": "오늘 아침에 물마셔야하는데 못마셨어요ㅜㅜ",
          "original_form": "오늘 아침에 물마셔야하는데 못마셨어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 07:03"
        },
        {
          "id": "MDRW2100027261.1.9",
          "form": "출근 진짜 빠르시네요",
          "original_form": "출근 진짜 빠르시네요",
          "speaker_id": "2",
          "time": "20210903 07:49"
        },
        {
          "id": "MDRW2100027261.1.10",
          "form": "회사가서 마시면 되죠",
          "original_form": "회사가서 마시면 되죠",
          "speaker_id": "2",
          "time": "20210903 07:50"
        },
        {
          "id": "MDRW2100027261.1.11",
          "form": "네 도착해서 물마셨어요",
          "original_form": "네 도착해서 물마셨어요",
          "speaker_id": "1",
          "time": "20210903 08:38"
        },
        {
          "id": "MDRW2100027261.1.12",
          "form": "밥을안먹었더니 되게 배고프네요",
          "original_form": "밥을안먹었더니 되게 배고프네요",
          "speaker_id": "1",
          "time": "20210903 08:38"
        },
        {
          "id": "MDRW2100027261.1.13",
          "form": "비스켓이나 초코바같은거 챙겨다니세요",
          "original_form": "비스켓이나 초코바같은거 챙겨다니세요",
          "speaker_id": "2",
          "time": "20210903 09:07"
        },
        {
          "id": "MDRW2100027261.1.14",
          "form": "무슨일하시는거에요?",
          "original_form": "무슨일하시는거에요?",
          "speaker_id": "2",
          "time": "20210903 09:07"
        },
        {
          "id": "MDRW2100027261.1.15",
          "form": "유치원에서 일해요",
          "original_form": "유치원에서 일해요",
          "speaker_id": "1",
          "time": "20210903 09:16"
        },
        {
          "id": "MDRW2100027261.1.16",
          "form": "들어오기전에 편의점 들러야겠다ㅜㅜ",
          "original_form": "들어오기전에 편의점 들러야겠다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 09:17"
        },
        {
          "id": "MDRW2100027261.1.17",
          "form": "네네 편의점 들러서 먹기편한 스낵 몇개 사가세요",
          "original_form": "네네 편의점 들러서 먹기편한 스낵 몇개 사가세요",
          "speaker_id": "2",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100027261.1.18",
          "form": "간식으로 명랑핫도그먹었머요",
          "original_form": "간식으로 명랑핫도그먹었머요",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100027261.1.19",
          "form": "저도 그거 좋아해요",
          "original_form": "저도 그거 좋아해요",
          "speaker_id": "2",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100027261.1.20",
          "form": "새로나온 고구마 맛있더라구요",
          "original_form": "새로나온 고구마 맛있더라구요",
          "speaker_id": "2",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100027261.1.21",
          "form": "달달하니",
          "original_form": "달달하니",
          "speaker_id": "2",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100027261.1.22",
          "form": "예전에 명랑 핫도그 맛있게먹었어요",
          "original_form": "예전에 명랑 핫도그 맛있게먹었어요",
