        {
          "id": "MDRW2100018882.1.1",
          "form": "name1님은 연애중이신가요?",
          "original_form": "&name1&님은 연애중이신가요?",
          "speaker_id": "2",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100018882.1.2",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100018882.1.3",
          "form": "내년엔 결혼예정이에요",
          "original_form": "내년엔 결혼예정이에요",
          "speaker_id": "1",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100018882.1.4",
          "form": "와 축하드려요!!!",
          "original_form": "와 축하드려요!!!",
          "speaker_id": "2",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100018882.1.5",
          "form": "연애중이세요?",
          "original_form": "연애중이세요?",
          "speaker_id": "1",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100018882.1.6",
          "form": "name1님 결혼하실땐 코로나 잠잠해지길",
          "original_form": "&name1&님 결혼하실땐 코로나 잠잠해지길",
          "speaker_id": "2",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100018882.1.7",
          "form": "아뇨 ㅎㅎ",
          "original_form": "아뇨 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100018882.1.8",
          "form": "결혼식은상관없는데",
          "original_form": "결혼식은상관없는데",
          "speaker_id": "1",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100018882.1.9",
          "form": "신행은 미룰꺼에요",
          "original_form": "신행은 미룰꺼에요",
          "speaker_id": "1",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100018882.1.10",
          "form": "안전할때 멀리 오~래 갈려구욤",
          "original_form": "안전할때 멀리 오~래 갈려구욤",
          "speaker_id": "1",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100018882.1.11",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100018882.1.12",
          "form": "맞아요! 요즘 결혼하는 분들 국내여행 2박3일 정도로 다녀오시더라구요 ㅠㅠ",
          "original_form": "맞아요! 요즘 결혼하는 분들 국내여행 2박3일 정도로 다녀오시더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 15:25"
        },
        {
          "id": "MDRW2100018882.1.13",
          "form": "이상형이 궁금해요 ㅎㅎ",
          "original_form": "이상형이 궁금해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100018882.1.14",
          "form": "음 외적으론 키크고 덩치 좋고! 배려심있고 배울 점 있는 사람요! ㅎㅎ",
          "original_form": "음 외적으론 키크고 덩치 좋고! 배려심있고 배울 점 있는 사람요! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100018882.1.15",
          "form": "name1님 예비신랑님은 이상형과 일치하시나요? ㅎㅎ",
          "original_form": "&name1&님 예비신랑님은 이상형과 일치하시나요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100018882.1.16",
          "form": "오 name2님 이상형과 흡사해요",
          "original_form": "오 &name2&님 이상형과 흡사해요",
          "speaker_id": "1",
          "time": "20210827 16:06"
        },
        {
          "id": "MDRW2100018882.1.17",
          "form": "비혼주의에 뾰족한 저를",
          "original_form": "비혼주의에 뾰족한 저를",
          "speaker_id": "1",
          "time": "20210827 16:06"
        },
        {
          "id": "MDRW2100018882.1.18",
          "form": "잘 보듬어주어서 변하게 됐어요",
          "original_form": "잘 보듬어주어서 변하게 됐어요",
          "speaker_id": "1",
          "time": "20210827 16:06"
        },
        {
          "id": "MDRW2100018882.1.19",
          "form": "우와 정말 부러워요!! 두 분 넘 잘어울리시네요 ㅎㅎ",
          "original_form": "우와 정말 부러워요!! 두 분 넘 잘어울리시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 16:13"
        },
        {
          "id": "MDRW2100018882.1.20",
          "form": "name2님은 더 좋은짝이 기다리고있을꺼에욤",
          "original_form": "&name2&님은 더 좋은짝이 기다리고있을꺼에욤",
          "speaker_id": "1",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100018882.1.21",
          "form": "몸과 정신이 건강한 사람이 드문 세상이라서",
          "original_form": "몸과 정신이 건강한 사람이 드문 세상이라서",
          "speaker_id": "1",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100018882.1.22",
          "form": "맞아요 ㅠㅠ 정말 유니콘이죠...",
          "original_form": "맞아요 ㅠㅠ 정말 유니콘이죠...",
          "speaker_id": "2",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100018882.1.23",
          "form": "인성이 정말 중요한거같아요",
          "original_form": "인성이 정말 중요한거같아요",
          "speaker_id": "1",
