      "utterance": [
        {
          "id": "MDRW2100004247.1.1",
          "form": "이따 시킬 족발이 너무 기대된다",
          "original_form": "이따 시킬 족발이 너무 기대된다",
          "speaker_id": "1",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004247.1.2",
          "form": "인정쓰 내가",
          "original_form": "인정쓰 내가",
          "speaker_id": "2",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004247.1.3",
          "form": "보쌈도 시킬게",
          "original_form": "보쌈도 시킬게",
          "speaker_id": "2",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004247.1.4",
          "form": "완전 맛있겠다",
          "original_form": "완전 맛있겠다",
          "speaker_id": "1",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004247.1.5",
          "form": "입에서 침흘리는중",
          "original_form": "입에서 침흘리는중",
          "speaker_id": "1",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004247.1.6",
          "form": "막국수는 필수~",
          "original_form": "막국수는 필수~",
          "speaker_id": "2",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004247.1.7",
          "form": "와 막국수 진짜 맛있다",
          "original_form": "와 막국수 진짜 맛있다",
          "speaker_id": "2",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004247.1.8",
          "form": "보쌈은 실패~",
          "original_form": "보쌈은 실패~",
          "speaker_id": "2",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004247.1.9",
          "form": "보삼은 실패ㅜㅠ 아쉽구먼",
          "original_form": "보삼은 실패ㅜㅠ 아쉽구먼",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004247.1.10",
          "form": "막국수는 진짜 마시따",
          "original_form": "막국수는 진짜 마시따",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004247.1.11",
          "form": "리뷰써야지 막국수 맛있다고",
          "original_form": "리뷰써야지 막국수 맛있다고",
          "speaker_id": "2",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004247.1.12",
          "form": "막국수집으로 바꿔야겠다 여기",
          "original_form": "막국수집으로 바꿔야겠다 여기",
          "speaker_id": "2",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004247.1.13",
          "form": "ㅋㅋㅋ'막국수 잘하는집'으로 바꿔!",
          "original_form": "ㅋㅋㅋ'막국수 잘하는집'으로 바꿔!",
          "speaker_id": "1",
          "time": "20210821 18:57"
        },
        {
          "id": "MDRW2100004247.1.14",
          "form": "막타쿠나!",
          "original_form": "막타쿠나!",
          "speaker_id": "2",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004247.1.15",
          "form": "하쿠나 막타타",
          "original_form": "하쿠나 막타타",
          "speaker_id": "2",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004247.1.16",
          "form": "개웃기닼ㅋㅋㅋㅋㅋ",
          "original_form": "개웃기닼ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004247.1.17",
          "form": "히히ㅋㅋㅋㅋㄱㄲㅋㅋ",
          "original_form": "히히ㅋㅋㅋㅋㄱㄲㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004247.1.18",
          "form": "족타쿠나 경쟁사 막타쿠나로 가게차려!",
          "original_form": "족타쿠나 경쟁사 막타쿠나로 가게차려!",
          "speaker_id": "1",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004247.1.19",
          "form": "사이다랑 먹었으면",
          "original_form": "사이다랑 먹었으면",
          "speaker_id": "2",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004247.1.20",
          "form": "더 맛있었을거",
          "original_form": "더 맛있었을거",
          "speaker_id": "2",
          "time": "20210821 19:08"
        },
