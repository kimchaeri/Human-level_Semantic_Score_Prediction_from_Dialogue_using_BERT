      "utterance": [
        {
          "id": "MDRW2100039627.1.1",
          "form": "안녕하세요! 저녁 드셨나요?",
          "original_form": "안녕하세요! 저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039627.1.2",
          "form": "아직요ㅠ",
          "original_form": "아직요ㅠ",
          "speaker_id": "2",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039627.1.3",
          "form": "드셨나요??",
          "original_form": "드셨나요??",
          "speaker_id": "2",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039627.1.4",
          "form": "네ㅎㅎ 오징어볶음먹었어요! 왜 안드셨나요?",
          "original_form": "네ㅎㅎ 오징어볶음먹었어요! 왜 안드셨나요?",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039627.1.5",
          "form": "저 9시에 먹기로했어요",
          "original_form": "저 9시에 먹기로했어요",
          "speaker_id": "2",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039627.1.6",
          "form": "뭐 드셔요?",
          "original_form": "뭐 드셔요?",
          "speaker_id": "1",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039627.1.7",
          "form": "꿔바로우요!",
          "original_form": "꿔바로우요!",
          "speaker_id": "2",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039627.1.8",
          "form": "꿔바로우좋아하세요?",
          "original_form": "꿔바로우좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039627.1.9",
          "form": "앗 꿔바로우 못먹어요...",
          "original_form": "앗 꿔바로우 못먹어요...",
          "speaker_id": "1",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039627.1.10",
          "form": "없어서 못먹어요....",
          "original_form": "없어서 못먹어요....{emoji:☆}",
          "speaker_id": "1",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039627.1.11",
          "form": "ㅋㄱㄱㄱㄱㅋㄱㅋㄱㄱㅋㄱㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㄱㄱㄱㄱㅋㄱㅋㄱㄱㅋㄱㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039627.1.12",
          "form": "순간 헉 무슨사연있는줄",
          "original_form": "순간 헉 무슨사연있는줄",
          "speaker_id": "2",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039627.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 중국집 가시는거세요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 중국집 가시는거세요?",
          "speaker_id": "1",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100039627.1.14",
          "form": "아뇨 포장해서 집에서 먹으려고요!",
          "original_form": "아뇨 포장해서 집에서 먹으려고요!",
          "speaker_id": "2",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100039627.1.15",
          "form": "오징어볶음은 해드신건가요?!",
          "original_form": "오징어볶음은 해드신건가요?!",
          "speaker_id": "2",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100039627.1.16",
          "form": "어머니께서 해주셨어요ㅎㅎ 혼밥하시게요?",
          "original_form": "어머니께서 해주셨어요ㅎㅎ 혼밥하시게요?",
          "speaker_id": "1",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100039627.1.17",
          "form": "넵! 혼밥최고!",
          "original_form": "넵! 혼밥최고!",
          "speaker_id": "2",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100039627.1.18",
          "form": "아 어머니가 해주시는 오징어볶음 진짜",
          "original_form": "아 어머니가 해주시는 오징어볶음 진짜",
          "speaker_id": "2",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100039627.1.19",
          "form": "밥도둑",
          "original_form": "밥도둑",
          "speaker_id": "2",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100039627.1.20",
          "form": "맞아요 밥 순삭- 콜라도 같이 드실건가요?",
          "original_form": "맞아요 밥 순삭- 콜라도 같이 드실건가요?",
          "speaker_id": "1",
          "time": "20210903 20:01"
        },
        {
          "id": "MDRW2100039627.1.21",
          "form": "아 당연해요! 맛잘알이시네요",
          "original_form": "아 당연해요! 맛잘알이시네요",
