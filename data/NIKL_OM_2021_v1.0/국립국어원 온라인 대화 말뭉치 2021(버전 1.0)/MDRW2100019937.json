        {
          "id": "MDRW2100019937.1.1",
          "form": "맞벌이신가요 ㅎㅎ",
          "original_form": "맞벌이신가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100019937.1.2",
          "form": "네ㅠㅠ 극한 상황",
          "original_form": "네ㅠㅠ 극한 상황",
          "speaker_id": "1",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100019937.1.3",
          "form": "어휴 힘드시죠 ㅠㅠ",
          "original_form": "어휴 힘드시죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100019937.1.4",
          "form": "일하시나요?",
          "original_form": "일하시나요?",
          "speaker_id": "1",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100019937.1.5",
          "form": "네 ㅎㅎ 저도 맞벌이에요",
          "original_form": "네 ㅎㅎ 저도 맞벌이에요",
          "speaker_id": "2",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100019937.1.6",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
          "time": "20210828 01:39"
        },
        {
          "id": "MDRW2100019937.1.7",
          "form": "아르바이트 해보셨나요?",
          "original_form": "아르바이트 해보셨나요?",
          "speaker_id": "1",
          "time": "20210828 01:39"
        },
        {
          "id": "MDRW2100019937.1.8",
          "form": "네 몇개 해봣죠 서너개?",
          "original_form": "네 몇개 해봣죠 서너개?",
          "speaker_id": "2",
          "time": "20210828 01:43"
        },
        {
          "id": "MDRW2100019937.1.9",
          "form": "저도요 피자집에서요",
          "original_form": "저도요 피자집에서요",
          "speaker_id": "1",
          "time": "20210828 01:44"
        },
        {
          "id": "MDRW2100019937.1.10",
          "form": "재밌으셨나요",
          "original_form": "재밌으셨나요",
          "speaker_id": "2",
          "time": "20210828 01:59"
        },
        {
          "id": "MDRW2100019937.1.11",
          "form": "나름 재밌었어요",
          "original_form": "나름 재밌었어요",
