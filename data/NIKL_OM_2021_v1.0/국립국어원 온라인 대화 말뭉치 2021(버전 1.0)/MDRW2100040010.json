      "utterance": [
        {
          "id": "MDRW2100040010.1.1",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100040010.1.2",
          "form": "아 뭐 그 정도면 그냥 백신 동기져~~~!",
          "original_form": "아 뭐 그 정도면 그냥 백신 동기져~~~!",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100040010.1.3",
          "form": "전 모더나 맞고 싶어욧",
          "original_form": "전 모더나 맞고 싶어욧",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100040010.1.4",
          "form": "님  맞을때쯤은 가능할 것 같아요",
          "original_form": "님  맞을때쯤은 가능할 것 같아요",
          "speaker_id": "1",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100040010.1.5",
          "form": "저 맞을 땐 화이자뿐이었지만ㅜ",
          "original_form": "저 맞을 땐 화이자뿐이었지만ㅜ",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100040010.1.6",
          "form": "델타에 모더나가 강하다던데",
          "original_form": "델타에 모더나가 강하다던데",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100040010.1.7",
          "form": "그",
          "original_form": "그",
          "speaker_id": "2",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100040010.1.8",
          "form": "뮤 변이도 나온 거 아시나요..",
          "original_form": "뮤 변이도 나온 거 아시나요..",
          "speaker_id": "2",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100040010.1.9",
          "form": "쟨 백신 회피래여... 이제 큰일남",
          "original_form": "쟨 백신 회피래여... 이제 큰일남",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040010.1.10",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040010.1.11",
          "form": "어차피 지금 백신 목적이",
          "original_form": "어차피 지금 백신 목적이",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040010.1.12",
          "form": "걸려도 중증으로 가는걸 막는게 강하니까요",
          "original_form": "걸려도 중증으로 가는걸 막는게 강하니까요",
          "speaker_id": "1",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100040010.1.13",
          "form": "그냥 코로나가 감기같아지기만을 빌 뿐입니다ㅜ우",
          "original_form": "그냥 코로나가 감기같아지기만을 빌 뿐입니다ㅜ우",
          "speaker_id": "1",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100040010.1.14",
          "form": "위드코로나라는데,,",
          "original_form": "위드코로나라는데,,",
          "speaker_id": "2",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100040010.1.15",
          "form": "감기는 갈수록 전염력은 높아지고 치명률은 낮아지는데ㅠ 이건 치명률도 같이 높아지니까 큰일이에여",
          "original_form": "감기는 갈수록 전염력은 높아지고 치명률은 낮아지는데ㅠ 이건 치명률도 같이 높아지니까 큰일이에여",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100040010.1.16",
          "form": "백신이 치명률을 낮춰주는 역할을 하니까요",
          "original_form": "백신이 치명률을 낮춰주는 역할을 하니까요",
          "speaker_id": "1",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100040010.1.17",
          "form": "그랬는데 새로나온 변이는 백신 회피라니까 큰일이라구 한 거져",
          "original_form": "그랬는데 새로나온 변이는 백신 회피라니까 큰일이라구 한 거져",
          "speaker_id": "2",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100040010.1.18",
          "form": "부스터샷도 있고",
          "original_form": "부스터샷도 있고",
          "speaker_id": "1",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100040010.1.19",
          "form": "그나마 델타보다 전염성은 낮아보이니ㅜ",
          "original_form": "그나마 델타보다 전염성은 낮아보이니ㅜ",
          "speaker_id": "1",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100040010.1.20",
          "form": "변이가 워낙 많았는데 그중 델타가 확 우세종 된거잖아요",
          "original_form": "변이가 워낙 많았는데 그중 델타가 확 우세종 된거잖아요",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100040010.1.21",
          "form": "뮤가 수많은 변이 중 하나로 남거나",
          "original_form": "뮤가 수많은 변이 중 하나로 남거나",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100040010.1.22",
          "form": "백신이 레벨업하기만을ㅜ",
          "original_form": "백신이 레벨업하기만을ㅜ",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100040010.1.23",
          "form": "백신도 계속 연구중이니까요",
          "original_form": "백신도 계속 연구중이니까요",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100040010.1.24",
          "form": "당연히 희망론적인 사고는 누구나 하져",
          "original_form": "당연히 희망론적인 사고는 누구나 하져",
          "speaker_id": "2",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100040010.1.25",
          "form": "이미 뮤 사망자도 있고 국내에서도 감염이 3건 발생했는데",
          "original_form": "이미 뮤 사망자도 있고 국내에서도 감염이 3건 발생했는데",
          "speaker_id": "2",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100040010.1.26",
          "form": "걱정스러운 것도 이상한 게 아니잖아여?",
          "original_form": "걱정스러운 것도 이상한 게 아니잖아여?",
          "speaker_id": "2",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100040010.1.27",
          "form": "아니 왜 갑자기 저에겢?",
          "original_form": "아니 왜 갑자기 저에겢?",
          "speaker_id": "1",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100040010.1.28",
          "form": "ㅋㅋ아니 제가 계속 뮤 걱정된다는 식으로 말하니까 계속해서 걱정할 필요없단 식으로 피력하신 건 님 아닙니까",
          "original_form": "ㅋㅋ아니 제가 계속 뮤 걱정된다는 식으로 말하니까 계속해서 걱정할 필요없단 식으로 피력하신 건 님 아닙니까",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100040010.1.29",
          "form": "접종자도 완전 죽지 않는게 아녜요;",
          "original_form": "접종자도 완전 죽지 않는게 아녜요;",
          "speaker_id": "1",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100040010.1.30",
          "form": ";; 그걸 누가 모르냐구요",
          "original_form": ";; 그걸 누가 모르냐구요",
          "speaker_id": "2",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100040010.1.31",
          "form": "전 처음부터 백신 회피라서 걱정된다고 말했고 지금은 백신 회피가 없었으니까 한 말인데여",
          "original_form": "전 처음부터 백신 회피라서 걱정된다고 말했고 지금은 백신 회피가 없었으니까 한 말인데여",
          "speaker_id": "2",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100040010.1.32",
          "form": "걱정할 필요없다고 누가 그랬나요",
          "original_form": "걱정할 필요없다고 누가 그랬나요",
          "speaker_id": "1",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100040010.1.33",
          "form": "감기같아지길 백신 레벨업하길 빈다",
          "original_form": "감기같아지길 백신 레벨업하길 빈다",
          "speaker_id": "1",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100040010.1.34",
          "form": "이게 포인트 아니었나요",
          "original_form": "이게 포인트 아니었나요",
          "speaker_id": "1",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100040010.1.35",
          "form": "그 말씀만 하셨어야지 그건 마지막즈음에 한 번 말씀하시고 계속 제가 한 말에 본인 주장 부연설명하는 게 훨씬 더 길었는데요?",
          "original_form": "그 말씀만 하셨어야지 그건 마지막즈음에 한 번 말씀하시고 계속 제가 한 말에 본인 주장 부연설명하는 게 훨씬 더 길었는데요?",
          "speaker_id": "2",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100040010.1.36",
          "form": "그리고 백신회피 변이가 없던게 아녜요",
          "original_form": "그리고 백신회피 변이가 없던게 아녜요",
          "speaker_id": "1",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100040010.1.37",
          "form": "하.......",
          "original_form": "하.......",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100040010.1.38",
          "form": "뮤가 속한 카테고리가 회피변이인데 전파력으로 또 카테고리가 갈렸던 거고요",
          "original_form": "뮤가 속한 카테고리가 회피변이인데 전파력으로 또 카테고리가 갈렸던 거고요",
          "speaker_id": "1",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100040010.1.39",
          "form": "누가 없었다고 했냐구요 백신회피가 0이었으면 지금까지 사망자가 나오지도 않았겠죠",
          "original_form": "누가 없었다고 했냐구요 백신회피가 0이었으면 지금까지 사망자가 나오지도 않았겠죠",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100040010.1.40",
          "form": "걱정은 같아요",
          "original_form": "걱정은 같아요",
          "speaker_id": "1",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100040010.1.41",
          "form": "전파력이 어떻든 처음부터 제가 말한 포인트는 회피였고 잘 아시네요 회피변이에 속한다는 거",
          "original_form": "전파력이 어떻든 처음부터 제가 말한 포인트는 회피였고 잘 아시네요 회피변이에 속한다는 거",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100040010.1.42",
          "form": "회피도 백퍼가 아니라고요",
          "original_form": "회피도 백퍼가 아니라고요",
          "speaker_id": "1",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100040010.1.43",
          "form": "지금까지와는 다른 양상을 보이는 변이니까 걱정된다는 말에 회피를 보이는 백신 얘기만 하시면서 괜찮을 거다 라고 말하니까 납득이 안 됐던 거고요",
          "original_form": "지금까지와는 다른 양상을 보이는 변이니까 걱정된다는 말에 회피를 보이는 백신 얘기만 하시면서 괜찮을 거다 라고 말하니까 납득이 안 됐던 거고요",
          "speaker_id": "2",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100040010.1.44",
          "form": "죄송하지만 이걸로 님과 논쟁할 이유를 못 찾겠습니다",
          "original_form": "죄송하지만 이걸로 님과 논쟁할 이유를 못 찾겠습니다",
          "speaker_id": "1",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100040010.1.45",
          "form": "다른 분들과 즐거운 대화 나누시길 바랍니다",
          "original_form": "다른 분들과 즐거운 대화 나누시길 바랍니다",
          "speaker_id": "1",
          "time": "20210904 00:33"
        }
      ]
