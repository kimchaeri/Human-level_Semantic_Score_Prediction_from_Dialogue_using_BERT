      "utterance": [
        {
          "id": "MDRW2100004744.1.1",
          "form": "저녁 뭐 드실건가요?",
          "original_form": "저녁 뭐 드실건가요?",
          "speaker_id": "2",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004744.1.2",
          "form": "저는 수제햄버거 시켜먹었네요!",
          "original_form": "저는 수제햄버거 시켜먹었네요!",
          "speaker_id": "1",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004744.1.3",
          "form": "저녁 뭐 드셨나요?",
          "original_form": "저녁 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004744.1.4",
          "form": "오 저도 햄버거요",
          "original_form": "오 저도 햄버거요",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004744.1.5",
          "form": "젘 버거킹이요 ㅎㅎ",
          "original_form": "젘 버거킹이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004744.1.6",
          "form": "오 버거킹 맛이죠",
          "original_form": "오 버거킹 맛이죠",
          "speaker_id": "1",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004744.1.7",
          "form": "햄버거에 감튀는 진리죠",
          "original_form": "햄버거에 감튀는 진리죠",
          "speaker_id": "1",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004744.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100004744.1.9",
          "form": "너뮤 먓있어요",
          "original_form": "너뮤 먓있어요",
          "speaker_id": "2",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100004744.1.10",
          "form": "저는 쉑쉑 맛있더라구요",
          "original_form": "저는 쉑쉑 맛있더라구요",
          "speaker_id": "1",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100004744.1.11",
          "form": "밀크쉐이크도 존맛탱...",
          "original_form": "밀크쉐이크도 존맛탱...",
          "speaker_id": "1",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100004744.1.12",
          "form": "진짜 밀쉐에 감튀를",
          "original_form": "진짜 밀쉐에 감튀를",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100004744.1.13",
          "form": "찍어먹어요??!",
          "original_form": "찍어먹어요??!",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100004744.1.14",
          "form": "그렇게 먹어두 맛잇어요!!",
          "original_form": "그렇게 먹어두 맛잇어요!!",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100004744.1.15",
          "form": "저도 반신반의 했는데 또다른 새로운 맛이더라구요",
          "original_form": "저도 반신반의 했는데 또다른 새로운 맛이더라구요",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100004744.1.16",
          "form": "와 먹고싶어요!!!!!",
          "original_form": "와 먹고싶어요!!!!!",
          "speaker_id": "2",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100004744.1.17",
          "form": "유명하잖아유",
          "original_form": "유명하잖아유",
          "speaker_id": "2",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100004744.1.18",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100004744.1.19",
          "form": "꼭 한번 먹어보세요",
          "original_form": "꼭 한번 먹어보세요",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100004744.1.20",
          "form": "좀 비싸긴하지만... 한번쯤은 먹을만 한 것 같아요",
          "original_form": "좀 비싸긴하지만... 한번쯤은 먹을만 한 것 같아요",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100004744.1.21",
          "form": "서울가면 먹어야겠어요 ㅎ",
          "original_form": "서울가면 먹어야겠어요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100004744.1.22",
          "form": "네 꼭 드셔보세용!!",
          "original_form": "네 꼭 드셔보세용!!",
          "speaker_id": "1",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100004744.1.23",
          "form": "요새 뜨는 노티드 도넛 그것두 드셔보세용",
          "original_form": "요새 뜨는 노티드 도넛 그것두 드셔보세용",
          "speaker_id": "1",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100004744.1.24",
          "form": "그것두 맛나요",
          "original_form": "그것두 맛나요",
          "speaker_id": "1",
          "time": "20210821 22:48"
        }
      ]
