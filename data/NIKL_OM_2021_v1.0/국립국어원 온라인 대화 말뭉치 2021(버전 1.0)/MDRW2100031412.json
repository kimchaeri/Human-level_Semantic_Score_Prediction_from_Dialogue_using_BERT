      "utterance": [
        {
          "id": "MDRW2100031412.1.1",
          "form": "와 근데 이번에 꽃게 진짜 잘샀다",
          "original_form": "와 근데 이번에 꽃게 진짜 잘샀다",
          "speaker_id": "2",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100031412.1.2",
          "form": "맞아 진짜 싸게 삼",
          "original_form": "맞아 진짜 싸게 삼",
          "speaker_id": "1",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031412.1.3",
          "form": "그리고 엄청 싱싱하잖아",
          "original_form": "그리고 엄청 싱싱하잖아",
          "speaker_id": "2",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031412.1.4",
          "form": "안죽고 살아있고",
          "original_form": "안죽고 살아있고",
          "speaker_id": "2",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031412.1.5",
          "form": "그래서 음식할때 좀 그렇더라ㅠㅜ",
          "original_form": "그래서 음식할때 좀 그렇더라ㅠㅜ",
          "speaker_id": "1",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031412.1.6",
          "form": "지는 소리만 질렀으면서",
          "original_form": "지는 소리만 질렀으면서",
          "speaker_id": "2",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031412.1.7",
          "form": "맛은 있었어..",
          "original_form": "맛은 있었어..",
          "speaker_id": "1",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031412.1.8",
          "form": "꽃게찜 처음 먹는데 완전 맛있더라",
          "original_form": "꽃게찜 처음 먹는데 완전 맛있더라",
          "speaker_id": "1",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100031412.1.9",
          "form": "그지? 나도 그렇게는 처음임",
          "original_form": "그지? 나도 그렇게는 처음임",
          "speaker_id": "2",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100031412.1.10",
          "form": "확실히 암컷이 수컷보다 맛있긴하더라",
          "original_form": "확실히 암컷이 수컷보다 맛있긴하더라",
          "speaker_id": "1",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100031412.1.11",
          "form": "암놈이 최고지",
          "original_form": "암놈이 최고지",
          "speaker_id": "2",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031412.1.12",
          "form": "암컷 먹으면 안되는데",
          "original_form": "암컷 먹으면 안되는데",
          "speaker_id": "1",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031412.1.13",
          "form": "너무 맛있어ㅠㅜ",
          "original_form": "너무 맛있어ㅠㅜ",
          "speaker_id": "1",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031412.1.14",
          "form": "오늘 찜이랑 탕해먹고",
          "original_form": "오늘 찜이랑 탕해먹고",
          "speaker_id": "2",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031412.1.15",
          "form": "내일 또 찜해먹을거 남았다",
          "original_form": "내일 또 찜해먹을거 남았다",
          "speaker_id": "2",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031412.1.16",
          "form": "진짜 대박인듯",
          "original_form": "진짜 대박인듯",
          "speaker_id": "1",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031412.1.17",
          "form": "완전 잘샀어 진짜",
          "original_form": "완전 잘샀어 진짜",
          "speaker_id": "1",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100031412.1.18",
          "form": "원없이 먹는다 꽃게",
          "original_form": "원없이 먹는다 꽃게",
          "speaker_id": "1",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100031412.1.19",
          "form": "name3 이모도 전화해서 알려주니까",
          "original_form": "&name3& 이모도 전화해서 알려주니까",
          "speaker_id": "2",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100031412.1.20",
          "form": "사러가는 거 갔더랔ㅋ",
          "original_form": "사러가는 거 갔더랔ㅋ",
          "speaker_id": "2",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100031412.1.21",
          "form": "역시 행동력이 빨라야 맛있는 걸 먹어",
          "original_form": "역시 행동력이 빨라야 맛있는 걸 먹어",
