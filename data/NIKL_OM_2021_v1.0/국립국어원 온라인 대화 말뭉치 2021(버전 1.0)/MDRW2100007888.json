      "utterance": [
        {
          "id": "MDRW2100007888.1.1",
          "form": "최근 사신 물건 중 좋았던 상품 있으세요",
          "original_form": "최근 사신 물건 중 좋았던 상품 있으세요",
          "speaker_id": "1",
          "time": "20210822 11:36"
        },
        {
          "id": "MDRW2100007888.1.2",
          "form": "고무장갑..?",
          "original_form": "고무장갑..?",
          "speaker_id": "2",
          "time": "20210822 11:37"
        },
        {
          "id": "MDRW2100007888.1.3",
          "form": "어디서 사셨어요?",
          "original_form": "어디서 사셨어요?",
          "speaker_id": "1",
          "time": "20210822 11:37"
        },
        {
          "id": "MDRW2100007888.1.4",
          "form": "잘 벗겨지는 고무장갑이 있는데요 정말 잘 벗겨지더라구요!",
          "original_form": "잘 벗겨지는 고무장갑이 있는데요 정말 잘 벗겨지더라구요!",
          "speaker_id": "2",
          "time": "20210822 11:37"
        },
        {
          "id": "MDRW2100007888.1.5",
          "form": "쿠팡에서 샀어요",
          "original_form": "쿠팡에서 샀어요",
          "speaker_id": "2",
          "time": "20210822 11:37"
        },
        {
          "id": "MDRW2100007888.1.6",
          "form": "잘벗겨지는 고무장갑이란것도 있군요",
          "original_form": "잘벗겨지는 고무장갑이란것도 있군요",
          "speaker_id": "1",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007888.1.7",
          "form": "일반 고무장갑보다는 비싸지만..",
          "original_form": "일반 고무장갑보다는 비싸지만..",
          "speaker_id": "2",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007888.1.8",
          "form": "첨들어봐요 ㅋㅋ",
          "original_form": "첨들어봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007888.1.9",
          "form": "색깔은 똑같이 빨간색인가요",
          "original_form": "색깔은 똑같이 빨간색인가요",
          "speaker_id": "1",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007888.1.10",
          "form": "여러가지 색이 있어요",
          "original_form": "여러가지 색이 있어요",
          "speaker_id": "2",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007888.1.11",
          "form": "촌스럽지 않아요",
          "original_form": "촌스럽지 않아요",
          "speaker_id": "2",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007888.1.12",
          "form": "근데 제가 생각해도 정말 소박하네요 최근 쇼핑 상품이 고무장갑이라니",
          "original_form": "근데 제가 생각해도 정말 소박하네요 최근 쇼핑 상품이 고무장갑이라니",
          "speaker_id": "2",
          "time": "20210822 11:39"
        },
        {
          "id": "MDRW2100007888.1.13",
          "form": "필수품이죠",
          "original_form": "필수품이죠",
          "speaker_id": "1",
          "time": "20210822 11:39"
        },
        {
          "id": "MDRW2100007888.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 11:39"
        },
        {
          "id": "MDRW2100007888.1.15",
          "form": "전 아쿠아슈즈 샀어요",
          "original_form": "전 아쿠아슈즈 샀어요",
          "speaker_id": "1",
          "time": "20210822 11:39"
        },
        {
          "id": "MDRW2100007888.1.16",
          "form": "오 어디 놀러가시나봐요",
          "original_form": "오 어디 놀러가시나봐요",
          "speaker_id": "2",
          "time": "20210822 11:39"
        },
        {
          "id": "MDRW2100007888.1.17",
          "form": "비가 많이와서ㅜ 장마용 신발로샀어요 ㅋㅋㅋㅋㅋ",
          "original_form": "비가 많이와서ㅜ 장마용 신발로샀어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007888.1.18",
          "form": "레저용이 아니라 일상용 흑흑",
          "original_form": "레저용이 아니라 일상용 흑흑",
          "speaker_id": "1",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007888.1.19",
          "form": "아 슬프네요",
          "original_form": "아 슬프네요",
          "speaker_id": "2",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007888.1.20",
          "form": "레저용이었어야 하는데",
          "original_form": "레저용이었어야 하는데",
          "speaker_id": "2",
          "time": "20210822 11:41"
        }
      ]
