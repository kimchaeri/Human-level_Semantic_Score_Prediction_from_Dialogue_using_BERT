      "utterance": [
        {
          "id": "MDRW2100028699.1.1",
          "form": "지금 하고 계시는 게임이 있나요?",
          "original_form": "지금 하고 계시는 게임이 있나요?",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028699.1.2",
          "form": "저는 요즘 쿠킹덤 해요!",
          "original_form": "저는 요즘 쿠킹덤 해요!",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028699.1.3",
          "form": "오 저도 예전에 했었는데",
          "original_form": "오 저도 예전에 했었는데",
          "speaker_id": "1",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028699.1.4",
          "form": "모바일 게임만 하시나요?",
          "original_form": "모바일 게임만 하시나요?",
          "speaker_id": "1",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028699.1.5",
          "form": "주로 모바일 게임만 하는 것 같아요ㅋㅋㅋ",
          "original_form": "주로 모바일 게임만 하는 것 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028699.1.6",
          "form": "저는 요즘 스팀게임을 자주 하고 있어요",
          "original_form": "저는 요즘 스팀게임을 자주 하고 있어요",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028699.1.7",
          "form": "오 어떤거 하시나요?",
          "original_form": "오 어떤거 하시나요?",
          "speaker_id": "2",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028699.1.8",
          "form": "레포데라고 하는 좀비게임이요~",
          "original_form": "레포데라고 하는 좀비게임이요~",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028699.1.9",
          "form": "아 저도 알아요~",
          "original_form": "아 저도 알아요~",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028699.1.10",
          "form": "제가 약간 이런류 게임을 좋아해서ㅋㅋㅋㅋ",
          "original_form": "제가 약간 이런류 게임을 좋아해서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028699.1.11",
          "form": "그럼 데바데도 하시나요?",
          "original_form": "그럼 데바데도 하시나요?",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028699.1.12",
          "form": "했었는데",
          "original_form": "했었는데",
          "speaker_id": "1",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028699.1.13",
          "form": "요즘은 안해요",
          "original_form": "요즘은 안해요",
          "speaker_id": "1",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028699.1.14",
          "form": "그렇군요..",
          "original_form": "그렇군요..",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028699.1.15",
          "form": "오랜만에 쿠킹덤 깔아봐야겠어요",
