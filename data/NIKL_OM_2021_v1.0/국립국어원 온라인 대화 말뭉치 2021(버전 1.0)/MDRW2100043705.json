      "utterance": [
        {
          "id": "MDRW2100043705.1.1",
          "form": "네가 먹고 싶은 걸로",
          "original_form": "네가 먹고 싶은 걸로",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043705.1.2",
          "form": "파인애플피자 어떄용",
          "original_form": "파인애플피자 어떄용",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043705.1.3",
          "form": "맛있겠네",
          "original_form": "맛있겠네",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043705.1.4",
          "form": "그치? ㅋㅋ 파인애플 안좋아하는 사람들이 많더라",
          "original_form": "그치? ㅋㅋ 파인애플 안좋아하는 사람들이 많더라",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043705.1.5",
          "form": "나는 좋아한다",
          "original_form": "나는 좋아한다",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043705.1.6",
          "form": "나도 ㅋㅋ",
          "original_form": "나도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043705.1.7",
          "form": "파인애플피자랑 불고기피자 반반 시켜야겠다",
          "original_form": "파인애플피자랑 불고기피자 반반 시켜야겠다",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043705.1.8",
          "form": "아빠도 같이 먹게 그렇게 시켜라",
          "original_form": "아빠도 같이 먹게 그렇게 시켜라",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043705.1.9",
          "form": "알았어용",
          "original_form": "알았어용",
          "speaker_id": "1",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043705.1.10",
          "form": "너무 비싼 거 말고",
          "original_form": "너무 비싼 거 말고",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043705.1.11",
          "form": "웅웅ㅋㅋ 적당한사이즈로~",
          "original_form": "웅웅ㅋㅋ 적당한사이즈로~",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043705.1.12",
          "form": "저녁도 먹고 또 먹는데 살찐다",
          "original_form": "저녁도 먹고 또 먹는데 살찐다",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
