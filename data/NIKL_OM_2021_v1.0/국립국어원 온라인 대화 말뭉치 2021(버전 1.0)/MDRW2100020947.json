      "utterance": [
        {
          "id": "MDRW2100020947.1.1",
          "form": "아니요 같이 살아요!",
          "original_form": "아니요 같이 살아요!",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020947.1.2",
          "form": "name1님은 따로사세요?",
          "original_form": "&name1&님은 따로사세요?",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020947.1.3",
          "form": "저도 같이 살아요!",
          "original_form": "저도 같이 살아요!",
          "speaker_id": "1",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100020947.1.4",
          "form": "나가고 싶지만 ㅋㅋ",
          "original_form": "나가고 싶지만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100020947.1.5",
          "form": "ㅋㅋㅋㅋㅋ저두요 혼자 자취하고싶어요 ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ저두요 혼자 자취하고싶어요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020947.1.6",
          "form": "안하시는 이유 있나요?",
          "original_form": "안하시는 이유 있나요?",
          "speaker_id": "1",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020947.1.7",
          "form": "경제적인게 크죠! 저는 아직 취준생이라서요 ㅠㅠ",
          "original_form": "경제적인게 크죠! 저는 아직 취준생이라서요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020947.1.8",
          "form": "아아 ㅜㅜ 맞아요",
          "original_form": "아아 ㅜㅜ 맞아요",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020947.1.9",
          "form": "저도 경제적인게 젤 크네요 ㅎㅎ",
          "original_form": "저도 경제적인게 젤 크네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100020947.1.10",
          "form": "네네 고정비용이 만만치 않을거같더라구요",
          "original_form": "네네 고정비용이 만만치 않을거같더라구요",
          "speaker_id": "2",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100020947.1.11",
          "form": "몬가 친구랑 같이 살고싶기도하고 ㅋㅋㅋㅋ",
          "original_form": "몬가 친구랑 같이 살고싶기도하고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100020947.1.12",
          "form": "맞아요 ㅋㅋ 근데 또 잘맞는 친구랑 살아야되니까",
          "original_form": "맞아요 ㅋㅋ 근데 또 잘맞는 친구랑 살아야되니까",
          "speaker_id": "1",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100020947.1.13",
          "form": "찾기 힘들죠 ㅜㅜ",
          "original_form": "찾기 힘들죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100020947.1.14",
          "form": "맞아요 ㅋㅋㅋㅋ괜히 같이살다가 안맞으면 사이멀어지구요",
          "original_form": "맞아요 ㅋㅋㅋㅋ괜히 같이살다가 안맞으면 사이멀어지구요",
          "speaker_id": "2",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100020947.1.15",
          "form": "그래서 실행으로 옮기긴 힘들거같아요",
          "original_form": "그래서 실행으로 옮기긴 힘들거같아요",
          "speaker_id": "2",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100020947.1.16",
          "form": "맞아요 저는 친구랑 한달 산적 있는데",
          "original_form": "맞아요 저는 친구랑 한달 산적 있는데",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020947.1.17",
          "form": "그 친구랑 처음으로 싸웠어요 .. ㅎㅎ",
          "original_form": "그 친구랑 처음으로 싸웠어요 .. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020947.1.18",
          "form": "아...경험이잇으시군요 ㅠㅠ",
          "original_form": "아...경험이잇으시군요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100020947.1.19",
          "form": "어떤거 때문에 싸우셨어요?",
          "original_form": "어떤거 때문에 싸우셨어요?",
          "speaker_id": "2",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100020947.1.20",
          "form": "일단 서로 안맞는게 젤 컸어요 ㅜㅜ",
          "original_form": "일단 서로 안맞는게 젤 컸어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020947.1.21",
          "form": "저는 바로바로 치우는 스탈인데 친구는 안치우는 스탈이여서 ..",
          "original_form": "저는 바로바로 치우는 스탈인데 친구는 안치우는 스탈이여서 ..",
          "speaker_id": "1",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020947.1.22",
          "form": "아아 청소 문제군요 ㅠㅠ",
          "original_form": "아아 청소 문제군요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020947.1.23",
          "form": "맞아요 깔끔한성향도 잘 맞아야된다하더러구요",
          "original_form": "맞아요 깔끔한성향도 잘 맞아야된다하더러구요",
