      "utterance": [
        {
          "id": "MDRW2100031662.1.1",
          "form": "돈이 많다면 샀을것 같애",
          "original_form": "돈이 많다면 샀을것 같애",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031662.1.2",
          "form": "이뻐서ㅋㅋㅋ",
          "original_form": "이뻐서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031662.1.3",
          "form": "내가 진짜 부자였잖아?",
          "original_form": "내가 진짜 부자였잖아?",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031662.1.4",
          "form": "한 백개 사서 뿌렸어",
          "original_form": "한 백개 사서 뿌렸어",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031662.1.5",
          "form": "우리 삼성이 좀 보라고",
          "original_form": "우리 삼성이 좀 보라고",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031662.1.6",
          "form": "이렇게 예쁘다고",
          "original_form": "이렇게 예쁘다고",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031662.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031662.1.8",
          "form": "누구한테ㅋㅋㅋㅋㅋㅋ",
          "original_form": "누구한테ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031662.1.9",
          "form": "그냥 주변 사람들 다 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그냥 주변 사람들 다 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031662.1.10",
          "form": "뭐야~",
          "original_form": "뭐야~",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031662.1.11",
          "form": "너가",
          "original_form": "너가",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031662.1.12",
          "form": "만든줄",
          "original_form": "만든줄",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031662.1.13",
          "form": "내가 또 삼성 좋아하잖아",
          "original_form": "내가 또 삼성 좋아하잖아",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100031662.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ삼성입사해야겠네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ삼성입사해야겠네",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100031662.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ하면 행복하겠지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ하면 행복하겠지",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100031662.1.16",
          "form": "나름 삼성덕후인데",
          "original_form": "나름 삼성덕후인데",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100031662.1.17",
          "form": "이번 플립 덮었을 때 화면이 진짜 예쁘더라",
          "original_form": "이번 플립 덮었을 때 화면이 진짜 예쁘더라",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100031662.1.18",
          "form": "폴더?는",
          "original_form": "폴더?는",
          "speaker_id": "1",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031662.1.19",
          "form": "가로로 접는거?!",
          "original_form": "가로로 접는거?!",
          "speaker_id": "1",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031662.1.20",
          "form": "맞아 폴드가 화면 엄청 큰거고",
          "original_form": "맞아 폴드가 화면 엄청 큰거고",
          "speaker_id": "2",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100031662.1.21",
          "form": "플립이 세로로 접는거",
          "original_form": "플립이 세로로 접는거",
          "speaker_id": "2",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100031662.1.22",
          "form": "응응 그건 어떻게 생각해?",
          "original_form": "응응 그건 어떻게 생각해?",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031662.1.23",
          "form": "폴드도 예쁘지",
          "original_form": "폴드도 예쁘지",
          "speaker_id": "2",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100031662.1.24",
          "form": "근데 플립이 압도적이어서..",
          "original_form": "근데 플립이 압도적이어서..",
          "speaker_id": "2",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100031662.1.25",
          "form": "글쿠낭",
          "original_form": "글쿠낭",
          "speaker_id": "1",
          "time": "20210904 23:50"
        }
      ]
