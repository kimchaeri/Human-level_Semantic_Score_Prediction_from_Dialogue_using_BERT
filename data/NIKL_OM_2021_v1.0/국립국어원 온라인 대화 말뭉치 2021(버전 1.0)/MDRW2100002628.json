      "utterance": [
        {
          "id": "MDRW2100002628.1.1",
          "form": "우울할땐 쇼핑이죠",
          "original_form": "우울할땐 쇼핑이죠",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002628.1.2",
          "form": "쇼핑좋아하세요?ㅎㅎ",
          "original_form": "쇼핑좋아하세요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002628.1.3",
          "form": "ㅎㅎ 돈버는 의미를 찾고 싶을 때! 꼭 하죠ㅋ",
          "original_form": "ㅎㅎ 돈버는 의미를 찾고 싶을 때! 꼭 하죠ㅋ",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002628.1.4",
          "form": "주로 어떤거에 쇼핑하시나요? 옷? 가방?",
          "original_form": "주로 어떤거에 쇼핑하시나요? 옷? 가방?",
          "speaker_id": "2",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002628.1.5",
          "form": "둘 다요ㅋㅋㅋ",
          "original_form": "둘 다요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002628.1.6",
          "form": "마카롱님은요?",
          "original_form": "마카롱님은요?",
          "speaker_id": "1",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002628.1.7",
          "form": "전 나이먹으니 알아보는게 귀찮더라구요 ㅎ 옷산지 오래됐어요ㅋ 주로 먹을거쇼핑ㅋ",
          "original_form": "전 나이먹으니 알아보는게 귀찮더라구요 ㅎ 옷산지 오래됐어요ㅋ 주로 먹을거쇼핑ㅋ",
          "speaker_id": "2",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002628.1.8",
          "form": "오ㅋ 핫딜 만두 최고죠ㅋㅋ",
          "original_form": "오ㅋ 핫딜 만두 최고죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002628.1.9",
          "form": "만두좋아하시는군요ㅎ 풀무원 납작만두 드셔보셨어요?",
          "original_form": "만두좋아하시는군요ㅎ 풀무원 납작만두 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002628.1.10",
          "form": "앗 아뇨. 비비고만 많이 먹어봤어요. 맛있나요?",
          "original_form": "앗 아뇨. 비비고만 많이 먹어봤어요. 맛있나요?",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002628.1.11",
          "form": "맛있더라구요 지에스프레시몰에서 원플러스 하길래 사봤거든요",
          "original_form": "맛있더라구요 지에스프레시몰에서 원플러스 하길래 사봤거든요",
          "speaker_id": "2",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002628.1.12",
          "form": "납작만두는 구워야겠어요?",
          "original_form": "납작만두는 구워야겠어요?",
          "speaker_id": "1",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002628.1.13",
          "form": "네 군만두로요ㅎ 요즘 신제품인지 한개붙여서 파는거같아요",
          "original_form": "네 군만두로요ㅎ 요즘 신제품인지 한개붙여서 파는거같아요",
          "speaker_id": "2",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002628.1.14",
          "form": "아 붙여줘야 사지요ㅎㅎ",
          "original_form": "아 붙여줘야 사지요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 16:19"
        }
      ]
