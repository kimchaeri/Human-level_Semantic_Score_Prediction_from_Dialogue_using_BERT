      "utterance": [
        {
          "id": "MDRW2100003378.1.1",
          "form": "요즘에 코로나때문에 외식을 잘 못하잖아",
          "original_form": "요즘에 코로나때문에 외식을 잘 못하잖아",
          "speaker_id": "2",
          "time": "20210820 22:20"
        },
        {
          "id": "MDRW2100003378.1.2",
          "form": "그렇지 못하지",
          "original_form": "그렇지 못하지",
          "speaker_id": "1",
          "time": "20210820 22:21"
        },
        {
          "id": "MDRW2100003378.1.3",
          "form": "인터넷으로 유명한 돈까스 시켰는데 진짜 맛있더라",
          "original_form": "인터넷으로 유명한 돈까스 시켰는데 진짜 맛있더라",
          "speaker_id": "2",
          "time": "20210820 22:21"
        },
        {
          "id": "MDRW2100003378.1.4",
          "form": "어디 건데",
          "original_form": "어디 건데",
          "speaker_id": "1",
          "time": "20210820 22:21"
        },
        {
          "id": "MDRW2100003378.1.5",
          "form": "유혜광 돈까스라고",
          "original_form": "유혜광 돈까스라고",
          "speaker_id": "2",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100003378.1.6",
          "form": "오 나도 나중에 먹어봐야겠다",
          "original_form": "오 나도 나중에 먹어봐야겠다",
          "speaker_id": "1",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100003378.1.7",
          "form": "냉동 돈까스 중에는 최고인 것 같아",
          "original_form": "냉동 돈까스 중에는 최고인 것 같아",
          "speaker_id": "2",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100003378.1.8",
          "form": "냉동인데 맛있다고?",
          "original_form": "냉동인데 맛있다고?",
          "speaker_id": "1",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100003378.1.9",
          "form": "응 진짜 맛있어",
          "original_form": "응 진짜 맛있어",
          "speaker_id": "2",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100003378.1.10",
          "form": "진짜 먹어보고 싶다",
          "original_form": "진짜 먹어보고 싶다",
          "speaker_id": "1",
          "time": "20210820 22:23"
        },
        {
          "id": "MDRW2100003378.1.11",
          "form": "근데 조리가 좀 귀찮아",
          "original_form": "근데 조리가 좀 귀찮아",
          "speaker_id": "2",
          "time": "20210820 22:23"
        },
        {
          "id": "MDRW2100003378.1.12",
          "form": "그건 그렇지",
          "original_form": "그건 그렇지",
          "speaker_id": "1",
          "time": "20210820 22:23"
        },
        {
          "id": "MDRW2100003378.1.13",
          "form": "에어프라이어는 좀 맛이 덜한 듯",
          "original_form": "에어프라이어는 좀 맛이 덜한 듯",
          "speaker_id": "2",
          "time": "20210820 22:23"
        },
        {
          "id": "MDRW2100003378.1.14",
          "form": "오 그래? 역시",
          "original_form": "오 그래? 역시",
          "speaker_id": "1",
          "time": "20210820 22:23"
        },
        {
          "id": "MDRW2100003378.1.15",
          "form": "기름에 튀겨야 맛나",
          "original_form": "기름에 튀겨야 맛나",
          "speaker_id": "2",
          "time": "20210820 22:24"
        },
        {
          "id": "MDRW2100003378.1.16",
          "form": "기름맛이지",
          "original_form": "기름맛이지",
          "speaker_id": "1",
          "time": "20210820 22:24"
        },
        {
          "id": "MDRW2100003378.1.17",
          "form": "튀기면 정말 맛있지",
          "original_form": "튀기면 정말 맛있지",
          "speaker_id": "1",
          "time": "20210820 22:24"
        }
      ]
