        },
        {
          "id": "MDRW2100045486.1.2",
          "form": "그럤던거같아요 ㅋㅋㅋㅋ",
          "original_form": "그럤던거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045486.1.3",
          "form": "ㅋㅋㅋㅋ 기숙사 얘기 더 해주세요",
          "original_form": "ㅋㅋㅋㅋ 기숙사 얘기 더 해주세요",
          "speaker_id": "1",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045486.1.4",
          "form": "엄청 추억 많을것같아요",
          "original_form": "엄청 추억 많을것같아요",
          "speaker_id": "1",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045486.1.5",
          "form": "근데 거의",
          "original_form": "근데 거의",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045486.1.6",
          "form": "사고친 얘기들이에요 ㅋㅋㅋ",
          "original_form": "사고친 얘기들이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045486.1.7",
          "form": "그런것만 기억에 남나봐요 ㅋㅋㅋㅋ",
          "original_form": "그런것만 기억에 남나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045486.1.8",
          "form": "원래 그런게 재밌는거 아닌가요ㅋㅋㅋㅋ",
          "original_form": "원래 그런게 재밌는거 아닌가요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045486.1.9",
          "form": "ㅋㅋㅋㅋ 그치만 그때는 진짜 심각했거든요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 그치만 그때는 진짜 심각했거든요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100045486.1.10",
          "form": "사감쌤 숙소에 있는 전자레인지 빌려다 핫팩 데우다가",
