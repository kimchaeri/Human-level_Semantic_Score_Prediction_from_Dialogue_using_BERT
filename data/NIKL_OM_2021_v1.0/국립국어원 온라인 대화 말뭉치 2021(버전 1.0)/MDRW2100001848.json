      "utterance": [
        {
          "id": "MDRW2100001848.1.1",
          "form": "중고거래 해보셨어요?",
          "original_form": "중고거래 해보셨어요?",
          "speaker_id": "1",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100001848.1.2",
          "form": "네 한때는 물품 파느라",
          "original_form": "네 한때는 물품 파느라",
          "speaker_id": "2",
          "time": "20210819 21:44"
        },
        {
          "id": "MDRW2100001848.1.3",
          "form": "우체국의 단골이 되었어요 ㅎ",
          "original_form": "우체국의 단골이 되었어요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 21:44"
        },
        {
          "id": "MDRW2100001848.1.4",
          "form": "아 정말요?? 중고나라요?",
          "original_form": "아 정말요?? 중고나라요?",
          "speaker_id": "1",
          "time": "20210819 21:44"
        },
        {
          "id": "MDRW2100001848.1.5",
          "form": "요즘은 당근이대세던데",
          "original_form": "요즘은 당근이대세던데",
          "speaker_id": "1",
          "time": "20210819 21:44"
        },
        {
          "id": "MDRW2100001848.1.6",
          "form": "한번도 안해봄용 ..",
          "original_form": "한번도 안해봄용 ..",
          "speaker_id": "1",
          "time": "20210819 21:44"
        },
        {
          "id": "MDRW2100001848.1.7",
          "form": "중고나라는 아니고 그냥 커뮤니티 같은 곳 비슷하네요!",
          "original_form": "중고나라는 아니고 그냥 커뮤니티 같은 곳 비슷하네요!",
          "speaker_id": "2",
          "time": "20210819 21:44"
        },
        {
          "id": "MDRW2100001848.1.8",
          "form": "맞아요 당근 정말 유용햐요",
          "original_form": "맞아요 당근 정말 유용햐요",
          "speaker_id": "2",
          "time": "20210819 21:44"
        },
        {
          "id": "MDRW2100001848.1.9",
          "form": "아아 옷같은거는 브랜드 명품아님 잘안팔리죵?",
          "original_form": "아아 옷같은거는 브랜드 명품아님 잘안팔리죵?",
          "speaker_id": "1",
          "time": "20210819 21:45"
        },
        {
          "id": "MDRW2100001848.1.10",
          "form": "아녀 디자이너브랜드 요쪽라인도 팔리긴 꽤 팔려요!!!",
          "original_form": "아녀 디자이너브랜드 요쪽라인도 팔리긴 꽤 팔려요!!!",
          "speaker_id": "2",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001848.1.11",
          "form": "오호?? 저도 함 도전해봐야겠네요 ㅎㅎ",
          "original_form": "오호?? 저도 함 도전해봐야겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:49"
        },
        {
          "id": "MDRW2100001848.1.12",
          "form": "최근엔 뭐파셨어여?",
          "original_form": "최근엔 뭐파셨어여?",
          "speaker_id": "1",
          "time": "20210819 21:49"
        },
        {
          "id": "MDRW2100001848.1.13",
          "form": "최근에는 시간이 없어서 물품은 못 팔고",
          "original_form": "최근에는 시간이 없어서 물품은 못 팔고",
          "speaker_id": "2",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001848.1.14",
          "form": "그냥 기프티콘 이런 거 팔았어요!!",
          "original_form": "그냥 기프티콘 이런 거 팔았어요!!",
          "speaker_id": "2",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001848.1.15",
          "form": "기프티콘도 쏠쏠하져 !!",
          "original_form": "기프티콘도 쏠쏠하져 !!",
          "speaker_id": "1",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001848.1.16",
          "form": "직장동료한테 추석선물 뭐가괜찮을까여?",
          "original_form": "직장동료한테 추석선물 뭐가괜찮을까여?",
          "speaker_id": "1",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001848.1.17",
          "form": "선물 하는게 은근 어려운 거 같아요",
          "original_form": "선물 하는게 은근 어려운 거 같아요",
          "speaker_id": "2",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100001848.1.18",
          "form": "마음은 센스있게 선물하고 싶은데요!",
          "original_form": "마음은 센스있게 선물하고 싶은데요!",
