          "id": "MDRW2100033064.1.36",
          "form": "플립근데",
          "original_form": "플립근데",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100033064.1.37",
          "form": "나무비싸지않나요",
          "original_form": "나무비싸지않나요",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100033064.1.38",
          "form": "199만원이라면서요...",
          "original_form": "199만원이라면서요...",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100033064.1.39",
          "form": "삼성이..",
          "original_form": "삼성이..",
          "speaker_id": "2",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100033064.1.40",
          "form": "왜..",
          "original_form": "왜..",
          "speaker_id": "2",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100033064.1.41",
          "form": "그런가격으로..",
          "original_form": "그런가격으로..",
          "speaker_id": "2",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100033064.1.42",
          "form": "내놨는지 .",
          "original_form": "내놨는지 .",
          "speaker_id": "2",
          "time": "20210906 17:47"
        },
