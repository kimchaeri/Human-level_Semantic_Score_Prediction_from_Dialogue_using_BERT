      "utterance": [
        {
          "id": "MDRW2100033064.1.1",
          "form": "핸드폰 뭐쓰세요?",
          "original_form": "핸드폰 뭐쓰세요?",
          "speaker_id": "1",
          "time": "20210906 15:09"
        },
        {
          "id": "MDRW2100033064.1.2",
          "form": "허걱 저 노트10이용",
          "original_form": "허걱 저 노트10이용",
          "speaker_id": "2",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100033064.1.3",
          "form": "노트10도 나왔을때는 막 완전 대박이다 이런 분위기였는데",
          "original_form": "노트10도 나왔을때는 막 완전 대박이다 이런 분위기였는데",
          "speaker_id": "2",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100033064.1.4",
          "form": "시대가참빨리바뀌네요",
          "original_form": "시대가참빨리바뀌네요",
          "speaker_id": "2",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100033064.1.5",
          "form": "저도 노트10쓰는데",
          "original_form": "저도 노트10쓰는데",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100033064.1.6",
          "form": "노트10  디자인 예쁘고 좋은데",
          "original_form": "노트10  디자인 예쁘고 좋은데",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100033064.1.7",
          "form": "그런데 이번에 플립3 디자인은 진짜 너무 예뻐요",
          "original_form": "그런데 이번에 플립3 디자인은 진짜 너무 예뻐요",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100033064.1.8",
          "form": "보셨어요?",
          "original_form": "보셨어요?",
          "speaker_id": "1",
          "time": "20210906 15:18"
        },
        {
          "id": "MDRW2100033064.1.9",
          "form": "크 봤죠",
          "original_form": "크 봤죠",
          "speaker_id": "2",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100033064.1.10",
          "form": "크림색...",
          "original_form": "크림색...",
          "speaker_id": "2",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100033064.1.11",
          "form": "20년된 에어컨 색",
          "original_form": "20년된 에어컨 색",
          "speaker_id": "2",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100033064.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100033064.1.13",
          "form": "제가 그컬러는 모르겠네요",
          "original_form": "제가 그컬러는 모르겠네요",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100033064.1.14",
          "form": "저는 잇섭 리뷰 영상을 봤거든요",
          "original_form": "저는 잇섭 리뷰 영상을 봤거든요",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100033064.1.15",
          "form": "실물은 안보고",
          "original_form": "실물은 안보고",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100033064.1.16",
          "form": "저 삼성전자 주식을 갖고 있어서 이번에 폴드랑 플립 꼭 대박나야함니다",
          "original_form": "저 삼성전자 주식을 갖고 있어서 이번에 폴드랑 플립 꼭 대박나야함니다",
          "speaker_id": "1",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100033064.1.17",
          "form": "이얔ㅋㅋㅋㅋ",
          "original_form": "이얔ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100033064.1.18",
          "form": "헐 저도 잇섭님알아요",
          "original_form": "헐 저도 잇섭님알아요",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100033064.1.19",
          "form": "설명넘잘해주시죠",
          "original_form": "설명넘잘해주시죠",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100033064.1.20",
          "form": "플립3근데 너무 이뻐서..",
          "original_form": "플립3근데 너무 이뻐서..",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100033064.1.21",
          "form": "많이들사지않을까싶네요",
          "original_form": "많이들사지않을까싶네요",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100033064.1.22",
          "form": "네 제발 삼성전자 주식 탈출하고 싶어요",
          "original_form": "네 제발 삼성전자 주식 탈출하고 싶어요",
          "speaker_id": "1",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100033064.1.23",
          "form": "많이 팔려서 주가 올라라",
          "original_form": "많이 팔려서 주가 올라라",
          "speaker_id": "1",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100033064.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:53"
        },
        {
          "id": "MDRW2100033064.1.25",
          "form": "근데 이번에 플립3가",
          "original_form": "근데 이번에 플립3가",
          "speaker_id": "2",
          "time": "20210906 15:53"
        },
        {
          "id": "MDRW2100033064.1.26",
          "form": "접히는부분이 많이 개선이됐을까요",
          "original_form": "접히는부분이 많이 개선이됐을까요",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100033064.1.27",
          "form": "뭔가 예전에는 모래가루 들어가고",
          "original_form": "뭔가 예전에는 모래가루 들어가고",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100033064.1.28",
          "form": "그럴수도있을법한",
          "original_form": "그럴수도있을법한",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100033064.1.29",
          "form": "틈이생겼었다는데",
          "original_form": "틈이생겼었다는데",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100033064.1.30",
          "form": "안돼요 ㅠㅠ 개선됐을거에요ㅠㅠ",
          "original_form": "안돼요 ㅠㅠ 개선됐을거에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100033064.1.31",
          "form": "개선됐겠죠!!!",
          "original_form": "개선됐겠죠!!!",
          "speaker_id": "2",
          "time": "20210906 16:00"
        },
        {
          "id": "MDRW2100033064.1.32",
          "form": "핸드폰 바꿀 계획은 갖고계세요?",
          "original_form": "핸드폰 바꿀 계획은 갖고계세요?",
          "speaker_id": "1",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033064.1.33",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100033064.1.34",
          "form": "돈만있다면",
          "original_form": "돈만있다면",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
