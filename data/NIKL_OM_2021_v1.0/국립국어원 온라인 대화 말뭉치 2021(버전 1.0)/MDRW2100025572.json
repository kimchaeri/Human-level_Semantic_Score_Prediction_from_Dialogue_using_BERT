        },
        {
          "id": "MDRW2100025572.1.2",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "1",
          "time": "20210901 13:35"
        },
        {
          "id": "MDRW2100025572.1.3",
          "form": "안녕하세요~ 네네",
          "original_form": "안녕하세요~ 네네",
          "speaker_id": "2",
          "time": "20210901 13:38"
        },
        {
          "id": "MDRW2100025572.1.4",
          "form": "회사 식당에서 먹었습니다",
          "original_form": "회사 식당에서 먹었습니다",
          "speaker_id": "2",
          "time": "20210901 13:38"
        },
        {
          "id": "MDRW2100025572.1.5",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210901 13:38"
        },
        {
          "id": "MDRW2100025572.1.6",
          "form": "저는 간단하게 빵 먹엇어요",
          "original_form": "저는 간단하게 빵 먹엇어요",
          "speaker_id": "1",
          "time": "20210901 13:39"
        },
        {
          "id": "MDRW2100025572.1.7",
          "form": "제드님은 메뉴가 뭐엿나요?",
          "original_form": "제드님은 메뉴가 뭐엿나요?",
          "speaker_id": "1",
          "time": "20210901 13:39"
        },
        {
          "id": "MDRW2100025572.1.8",
          "form": "오늘 치킨 나왔는데",
          "original_form": "오늘 치킨 나왔는데",
          "speaker_id": "2",
          "time": "20210901 13:40"
        },
        {
          "id": "MDRW2100025572.1.9",
          "form": "치킨이라하긴 좀 그런",
          "original_form": "치킨이라하긴 좀 그런",
          "speaker_id": "2",
          "time": "20210901 13:40"
        },
        {
          "id": "MDRW2100025572.1.10",
          "form": "닭튀김?",
          "original_form": "닭튀김?",
          "speaker_id": "2",
          "time": "20210901 13:40"
        },
        {
          "id": "MDRW2100025572.1.11",
          "form": "이랑 먹었어요",
          "original_form": "이랑 먹었어요",
          "speaker_id": "2",
          "time": "20210901 13:40"
        },
        {
          "id": "MDRW2100025572.1.12",
          "form": "빵 좋아하세요?",
          "original_form": "빵 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 13:40"
        },
        {
          "id": "MDRW2100025572.1.13",
          "form": "닭튀김도 맛잇죠",
          "original_form": "닭튀김도 맛잇죠",
          "speaker_id": "1",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100025572.1.14",
          "form": "네 빵 좋아해요",
          "original_form": "네 빵 좋아해요",
          "speaker_id": "1",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100025572.1.15",
          "form": "제드님은 빵 좋아하세요?",
          "original_form": "제드님은 빵 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100025572.1.16",
          "form": "아뇨 전 떡을 더 좋아해요ㅋㅋㅋ",
          "original_form": "아뇨 전 떡을 더 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025572.1.17",
          "form": "빵 맛이 없진 않는데",
          "original_form": "빵 맛이 없진 않는데",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025572.1.18",
          "form": "가끔? 아주 가끔 사먹는정도?",
          "original_form": "가끔? 아주 가끔 사먹는정도?",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025572.1.19",
          "form": "오 저도 떡도 좋아해요",
          "original_form": "오 저도 떡도 좋아해요",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100025572.1.20",
          "form": "무슨 떡 좋아하세요?",
          "original_form": "무슨 떡 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100025572.1.21",
          "form": "백설기 좋아요",
          "original_form": "백설기 좋아요",
          "speaker_id": "2",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100025572.1.22",
          "form": "바람떡도 괜찮고",
          "original_form": "바람떡도 괜찮고",
          "speaker_id": "2",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100025572.1.23",
          "form": "술빵인지 술떡인지 그거만 빼고 다 좋습니다ㅋㅋ",
          "original_form": "술빵인지 술떡인지 그거만 빼고 다 좋습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100025572.1.24",
          "form": "전 쑥인절미를 그렇게 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 쑥인절미를 그렇게 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025572.1.25",
          "form": "콩시루떡도 ㅋㅋㅋㅋ",
          "original_form": "콩시루떡도 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025572.1.26",
          "form": "오 쑥떡 맛있죠",
          "original_form": "오 쑥떡 맛있죠",
          "speaker_id": "2",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100025572.1.27",
          "form": "콩 시루떡의 단점은 빨리 쉰다",
          "original_form": "콩 시루떡의 단점은 빨리 쉰다",
          "speaker_id": "2",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100025572.1.28",
          "form": "맞아요 빨리 쉬는게 단점 ㅠㅠ",
          "original_form": "맞아요 빨리 쉬는게 단점 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 14:35"
        },
        {
          "id": "MDRW2100025572.1.29",
          "form": "백설기도 좋아해요",
          "original_form": "백설기도 좋아해요",
          "speaker_id": "1",
          "time": "20210901 14:35"
        },
        {
          "id": "MDRW2100025572.1.30",
          "form": "아 오랜만에 쑥떡 먹고싶네요",
          "original_form": "아 오랜만에 쑥떡 먹고싶네요",
          "speaker_id": "2",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100025572.1.31",
          "form": "빵은 뭐 좋아하세요?",
          "original_form": "빵은 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100025572.1.32",
          "form": "학교다닐때 파바 꿀카스테라인가 이거 자주 먹었었는데",
          "original_form": "학교다닐때 파바 꿀카스테라인가 이거 자주 먹었었는데",
          "speaker_id": "2",
          "time": "20210901 14:39"
        },
        {
          "id": "MDRW2100025572.1.33",
          "form": "저 빵은 다 좋아해요",
          "original_form": "저 빵은 다 좋아해요",
          "speaker_id": "1",
          "time": "20210901 15:12"
        },
        {
          "id": "MDRW2100025572.1.34",
          "form": "전 학교다닐때 피자빵을 그렇게 먹엇어요 ㅋㅋㅋㅋ",
          "original_form": "전 학교다닐때 피자빵을 그렇게 먹엇어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:12"
        },
        {
          "id": "MDRW2100025572.1.35",
          "form": "ㅋㅋㅋㅋㅋ피자빵 제 동생이 환장하는데",
          "original_form": "ㅋㅋㅋㅋㅋ피자빵 제 동생이 환장하는데",
          "speaker_id": "2",
          "time": "20210901 15:18"
        },
        {
          "id": "MDRW2100025572.1.36",
          "form": "고로케랑",
          "original_form": "고로케랑",
          "speaker_id": "2",
          "time": "20210901 15:18"
        },
        {
          "id": "MDRW2100025572.1.37",
          "form": "저는 페스츄리가 맛있는듯요",
          "original_form": "저는 페스츄리가 맛있는듯요",
