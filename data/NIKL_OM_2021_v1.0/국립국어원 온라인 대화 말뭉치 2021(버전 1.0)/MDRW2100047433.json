      "utterance": [
        {
          "id": "MDRW2100047433.1.1",
          "form": "점심 드셨나요!",
          "original_form": "점심 드셨나요!",
          "speaker_id": "2",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100047433.1.2",
          "form": "네 된장찌개 먹었어요",
          "original_form": "네 된장찌개 먹었어요",
          "speaker_id": "1",
          "time": "20210826 16:08"
        },
        {
          "id": "MDRW2100047433.1.3",
          "form": "name2님도 점심 드셨나요!",
          "original_form": "&name2&님도 점심 드셨나요!",
          "speaker_id": "1",
          "time": "20210826 16:08"
        },
        {
          "id": "MDRW2100047433.1.4",
          "form": "헐 된찌 맛나죠",
          "original_form": "헐 된찌 맛나죠",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100047433.1.5",
          "form": "저는 단호박먹었어요 ㅎ",
          "original_form": "저는 단호박먹었어요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100047433.1.6",
          "form": "단호박 요즘 달달하니 맛있어요",
          "original_form": "단호박 요즘 달달하니 맛있어요",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100047433.1.7",
          "form": "밤호박이 유행이라는데 드셔보셨나요??",
          "original_form": "밤호박이 유행이라는데 드셔보셨나요??",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100047433.1.8",
          "form": "오 밤호박이란것도 있어요?",
          "original_form": "오 밤호박이란것도 있어요?",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100047433.1.9",
          "form": "다른게 있나요?",
          "original_form": "다른게 있나요?",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100047433.1.10",
          "form": "밤고구마맛이 난대요!",
          "original_form": "밤고구마맛이 난대요!",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100047433.1.11",
          "form": "저도 먹어봤는데 단호박보다 담백하더라구용",
          "original_form": "저도 먹어봤는데 단호박보다 담백하더라구용",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100047433.1.12",
          "form": "오 맛없을 수가 없내요",
          "original_form": "오 맛없을 수가 없내요",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100047433.1.13",
          "form": "다음에 장보러 갈때 한번 담아봐야갰어요",
          "original_form": "다음에 장보러 갈때 한번 담아봐야갰어요",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100047433.1.14",
          "form": "아 복숭하는 드셨나요! 제철인데",
          "original_form": "아 복숭하는 드셨나요! 제철인데",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100047433.1.15",
          "form": "네네.. 오늘도 먹었어요 딱복 물복 어떤 파이신가요!",
          "original_form": "네네.. 오늘도 먹었어요 딱복 물복 어떤 파이신가요!",
          "speaker_id": "1",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100047433.1.16",
          "form": "아무래도... 처먹인.. 편이죠 ㅎㅎ",
          "original_form": "아무래도... 처먹인.. 편이죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100047433.1.17",
          "form": "둘다 너무 좋아해요",
          "original_form": "둘다 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100047433.1.18",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100047433.1.19",
          "form": "저는 완전 물복파였는데 요즘 딱복의 맛도 조금 알아버렸어요",
          "original_form": "저는 완전 물복파였는데 요즘 딱복의 맛도 조금 알아버렸어요",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100047433.1.20",
          "form": "복숭아 넘 맛있어요 ㅠㅠ",
          "original_form": "복숭아 넘 맛있어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100047433.1.21",
          "form": "달달해가지고 ㅠㅠㅠ",
          "original_form": "달달해가지고 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100047433.1.22",
          "form": "그러고보니 요거트랑 얼려먹는게 그렇개 맛있다던대!",
          "original_form": "그러고보니 요거트랑 얼려먹는게 그렇개 맛있다던대!",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100047433.1.23",
          "form": "오오 맞아요! 저도 궁금해서 어제 해먹어봤어요",
          "original_form": "오오 맞아요! 저도 궁금해서 어제 해먹어봤어요",
          "speaker_id": "1",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100047433.1.24",
          "form": "맛있더라구요 ..역시 맛있는 거에 맛있는거 조합은..  굿이에요",
          "original_form": "맛있더라구요 ..역시 맛있는 거에 맛있는거 조합은..  굿이에요",
          "speaker_id": "1",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100047433.1.25",
          "form": "사실 맛없을 수가 없는 조헙이기는해요",
          "original_form": "사실 맛없을 수가 없는 조헙이기는해요",
          "speaker_id": "2",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100047433.1.26",
          "form": "저도 얼른 먹고싶은  ㅠㅠㅠ",
          "original_form": "저도 얼른 먹고싶은  ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100047433.1.27",
          "form": "세상애는 맛난개 넘 많아요 ㅠㅠ",
          "original_form": "세상애는 맛난개 넘 많아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100047433.1.28",
          "form": "네네 꼭 드셔보세용 강추예요!",
          "original_form": "네네 꼭 드셔보세용 강추예요!",
          "speaker_id": "1",
          "time": "20210826 22:14"
        }
      ]
    }
  ]
