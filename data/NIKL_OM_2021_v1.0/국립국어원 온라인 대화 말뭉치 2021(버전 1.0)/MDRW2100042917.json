        },
        {
          "id": "MDRW2100042917.1.2",
          "form": "근데 확실히",
          "original_form": "근데 확실히",
          "speaker_id": "2",
          "time": "20210907 11:21"
        },
        {
          "id": "MDRW2100042917.1.3",
          "form": "도감모으기 컨텐츠가",
          "original_form": "도감모으기 컨텐츠가",
          "speaker_id": "2",
          "time": "20210907 11:21"
        },
        {
          "id": "MDRW2100042917.1.4",
          "form": "생각없이 하고있기",
          "original_form": "생각없이 하고있기",
          "speaker_id": "2",
          "time": "20210907 11:21"
        },
        {
          "id": "MDRW2100042917.1.5",
          "form": "좋긴 하다",
          "original_form": "좋긴 하다",
          "speaker_id": "2",
          "time": "20210907 11:21"
        },
        {
          "id": "MDRW2100042917.1.6",
          "form": "정 할게 없으면 수집이나 해라",
          "original_form": "정 할게 없으면 수집이나 해라",
          "speaker_id": "1",
          "time": "20210907 11:21"
        },
        {
          "id": "MDRW2100042917.1.7",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:21"
        },
        {
          "id": "MDRW2100042917.1.8",
          "form": "일단 다른 포인트에서 낚시는",
          "original_form": "일단 다른 포인트에서 낚시는",
          "speaker_id": "2",
          "time": "20210907 11:21"
        },
        {
          "id": "MDRW2100042917.1.9",
          "form": "빨간구피 12마리",
          "original_form": "빨간구피 12마리",
          "speaker_id": "2",
          "time": "20210907 11:22"
        },
        {
          "id": "MDRW2100042917.1.10",
          "form": "잡을때까지로 미뤄야겠어",
          "original_form": "잡을때까지로 미뤄야겠어",
          "speaker_id": "2",
          "time": "20210907 11:22"
        },
        {
          "id": "MDRW2100042917.1.11",
          "form": "미끼부터 쭉 충당해놔야지",
          "original_form": "미끼부터 쭉 충당해놔야지",
          "speaker_id": "2",
          "time": "20210907 11:22"
        },
        {
          "id": "MDRW2100042917.1.12",
          "form": "그 어획?은 도감보상이야?",
          "original_form": "그 어획?은 도감보상이야?",
          "speaker_id": "2",
          "time": "20210907 11:22"
        },
        {
          "id": "MDRW2100042917.1.13",
          "form": "모르겟어",
          "original_form": "모르겟어",
          "speaker_id": "1",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042917.1.14",
          "form": "교환 아냐?",
          "original_form": "교환 아냐?",
          "speaker_id": "1",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042917.1.15",
          "form": "몰라....",
          "original_form": "몰라....",
          "speaker_id": "2",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042917.1.16",
          "form": "이나즈마 낚시 협회에 가면 있으려나",
          "original_form": "이나즈마 낚시 협회에 가면 있으려나",
          "speaker_id": "2",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042917.1.17",
          "form": "그 몬드성 낚시관리인?한테 가면 있지 않아?",
          "original_form": "그 몬드성 낚시관리인?한테 가면 있지 않아?",
          "speaker_id": "1",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042917.1.18",
          "form": "없어",
          "original_form": "없어",
          "speaker_id": "2",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042917.1.19",
          "form": "거기는 고급 낚싯대랑",
          "original_form": "거기는 고급 낚싯대랑",
          "speaker_id": "2",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042917.1.20",
          "form": "미끼 레시피밖에",
          "original_form": "미끼 레시피밖에",
          "speaker_id": "2",
          "time": "20210907 11:26"
        },
        {
          "id": "MDRW2100042917.1.21",
          "form": "없던데?",
          "original_form": "없던데?",
          "speaker_id": "2",
          "time": "20210907 11:26"
        },
        {
          "id": "MDRW2100042917.1.22",
          "form": "엥 그래?",
          "original_form": "엥 그래?",
          "speaker_id": "1",
          "time": "20210907 11:30"
        },
        {
          "id": "MDRW2100042917.1.23",
          "form": "그럼 내가 본 게 고급낚시대엿나",
          "original_form": "그럼 내가 본 게 고급낚시대엿나",
          "speaker_id": "1",
          "time": "20210907 11:30"
        },
        {
          "id": "MDRW2100042917.1.24",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100042917.1.25",
          "form": "쪽지 뭐야 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "쪽지 뭐야 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100042917.1.26",
          "form": "나 한참 헤맸는데 겨우 하나 찾음 ㅋㅋㅋㅋ",
          "original_form": "나 한참 헤맸는데 겨우 하나 찾음 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100042917.1.27",
          "form": "아 그 울림풀?",
          "original_form": "아 그 울림풀?",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100042917.1.28",
          "form": "진짜 그냥 찾기 절대 못해......",
          "original_form": "진짜 그냥 찾기 절대 못해......",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100042917.1.29",
          "form": "길안내 하는 표시가",
          "original_form": "길안내 하는 표시가",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042917.1.30",
          "form": "벽으로 돌진하라고 가리키는데",
          "original_form": "벽으로 돌진하라고 가리키는데",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042917.1.31",
          "form": "뭘 어쩌라는거죠 ㅋㅋㅋㅋㅋ",
          "original_form": "뭘 어쩌라는거죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042917.1.32",
          "form": "애꿎은 근처 몹들만 조지는중 ㅋㅋㅋㅋ",
          "original_form": "애꿎은 근처 몹들만 조지는중 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042917.1.33",
          "form": "걍 공략 보고",
          "original_form": "걍 공략 보고",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042917.1.34",
          "form": "그 주변 원소시야로 뒤지는게",
          "original_form": "그 주변 원소시야로 뒤지는게",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042917.1.35",
          "form": "속 편해....",
          "original_form": "속 편해....",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042917.1.36",
          "form": "이거 총 몇개여?",
          "original_form": "이거 총 몇개여?",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042917.1.37",
          "form": "어차피 쪽지 찾는거 자체는",
          "original_form": "어차피 쪽지 찾는거 자체는",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042917.1.38",
          "form": "별 의미 없어서",
          "original_form": "별 의미 없어서",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042917.1.39",
          "form": "세개",
          "original_form": "세개",
          "speaker_id": "2",
          "time": "20210907 13:19"
        }
      ]
    }
