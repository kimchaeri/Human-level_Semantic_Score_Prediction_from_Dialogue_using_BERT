      "utterance": [
        {
          "id": "MDRW2100020135.1.1",
          "form": "기억에 남는 책 있으신가요?",
          "original_form": "기억에 남는 책 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020135.1.2",
          "form": "저늗 추리소설좋아해서 좋아하는작가책들 다 좋아히니요",
          "original_form": "저늗 추리소설좋아해서 좋아하는작가책들 다 좋아히니요",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020135.1.3",
          "form": "히가시노게이고 아세요?",
          "original_form": "히가시노게이고 아세요?",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020135.1.4",
          "form": "오 유명한 사람이죠",
          "original_form": "오 유명한 사람이죠",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100020135.1.5",
          "form": "한때 그분책 많이봤는데 책마다 호불호가 갈리다보니 이젠 미나토가나에 좋아해요",
          "original_form": "한때 그분책 많이봤는데 책마다 호불호가 갈리다보니 이젠 미나토가나에 좋아해요",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100020135.1.6",
          "form": "네 유명하죠",
          "original_form": "네 유명하죠",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020135.1.7",
          "form": "미나토가나에는 첨들어요",
          "original_form": "미나토가나에는 첨들어요",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020135.1.8",
          "form": "추리소설 썼나요,",
          "original_form": "추리소설 썼나요,",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020135.1.9",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020135.1.10",
          "form": "음 스릴러라고 해야할까요",
          "original_form": "음 스릴러라고 해야할까요",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020135.1.11",
          "form": "오 그래요?",
          "original_form": "오 그래요?",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020135.1.12",
          "form": "고백을 재미나게 읽었어요",
          "original_form": "고백을 재미나게 읽었어요",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020135.1.13",
          "form": "제목이 머예요?",
          "original_form": "제목이 머예요?",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020135.1.14",
          "form": "고백 이 제목이예요",
          "original_form": "고백 이 제목이예요",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020135.1.15",
          "form": "그외에도 2권 더 읽었는데 모두 재밌었어요 제목은 기억이 안나네요",
          "original_form": "그외에도 2권 더 읽었는데 모두 재밌었어요 제목은 기억이 안나네요",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020135.1.16",
          "form": "아 찾아볼께요",
          "original_form": "아 찾아볼께요",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020135.1.17",
          "form": "책 편식이 심해서 히가시노것만 읽었거든요",
          "original_form": "책 편식이 심해서 히가시노것만 읽었거든요",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020135.1.18",
          "form": "김진명이랑요",
          "original_form": "김진명이랑요",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020135.1.19",
          "form": "김진명아시죠?",
          "original_form": "김진명아시죠?",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020135.1.20",
          "form": "아니요 어떤책을 쓰셨나요?",
          "original_form": "아니요 어떤책을 쓰셨나요?",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020135.1.21",
          "form": "앗 그래요?",
          "original_form": "앗 그래요?",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020135.1.22",
          "form": "많이 쓰셨는데 바이러스X",
          "original_form": "많이 쓰셨는데 바이러스X",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020135.1.23",
          "form": "글자전쟁 미중전쟁",
          "original_form": "글자전쟁 미중전쟁",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020135.1.24",
          "form": "사드",
          "original_form": "사드",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020135.1.25",
          "form": "등등 많아요",
          "original_form": "등등 많아요",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020135.1.26",
          "form": "하나도 못들어보신책이신가요?",
          "original_form": "하나도 못들어보신책이신가요?",
          "speaker_id": "2",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100020135.1.27",
          "form": "최근 도서관이나 책방을 못가서 못들어봤네요",
          "original_form": "최근 도서관이나 책방을 못가서 못들어봤네요",
          "speaker_id": "1",
          "time": "20210828 18:17"
        }
      ]
    }
