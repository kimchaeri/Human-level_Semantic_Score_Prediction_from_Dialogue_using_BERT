      "utterance": [
        {
          "id": "MDRW2100037598.1.1",
          "form": "캐릭터에맞게 술과 안주도 찰떡이지 않나요 ㅋㅋㅋ",
          "original_form": "캐릭터에맞게 술과 안주도 찰떡이지 않나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:41"
        },
        {
          "id": "MDRW2100037598.1.2",
          "form": "박하선이 과자로 안주 대신하는거ㅋㅋㅋㅋ",
          "original_form": "박하선이 과자로 안주 대신하는거ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:48"
        },
        {
          "id": "MDRW2100037598.1.3",
          "form": "짱구과자 이어놓고 곱창인척 먹는거ㅎㅎㅎㅎ",
          "original_form": "짱구과자 이어놓고 곱창인척 먹는거ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100037598.1.4",
          "form": "안쓰러운데 귀엽고 예쁘고 다하죠 ㅋㅋㅋ",
          "original_form": "안쓰러운데 귀엽고 예쁘고 다하죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100037598.1.5",
          "form": "박하선 이런 캐릭터 많이 하는거같아요",
          "original_form": "박하선 이런 캐릭터 많이 하는거같아요",
          "speaker_id": "1",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100037598.1.6",
          "form": "벌써 5화까지 다 봤어요ㅋㅋㅋㅋ",
          "original_form": "벌써 5화까지 다 봤어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100037598.1.7",
          "form": "그거 한번잡으면 쭉봐야되요 ㅋㅋㅋ 전 그거보고 그 민진웅 배우가 좋더라구요 거기서 처음봤어요",
          "original_form": "그거 한번잡으면 쭉봐야되요 ㅋㅋㅋ 전 그거보고 그 민진웅 배우가 좋더라구요 거기서 처음봤어요",
          "speaker_id": "2",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100037598.1.8",
          "form": "지금까지는",
          "original_form": "지금까지는",
          "speaker_id": "1",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100037598.1.9",
          "form": "연기구멍 1도 없어요",
          "original_form": "연기구멍 1도 없어요",
          "speaker_id": "1",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100037598.1.10",
          "form": "정채연도 생각보다 괜찮죠??",
          "original_form": "정채연도 생각보다 괜찮죠??",
          "speaker_id": "2",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100037598.1.11",
          "form": "네ㅋㅋㅋ처음에 연기못한다고 생각했는데 왜그랬는지...",
          "original_form": "네ㅋㅋㅋ처음에 연기못한다고 생각했는데 왜그랬는지...",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100037598.1.12",
          "form": "민진웅씨는 얼굴이 몇개예요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "민진웅씨는 얼굴이 몇개예요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100037598.1.13",
          "form": "성대모사가 다 잘어울려",
          "original_form": "성대모사가 다 잘어울려",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100037598.1.14",
          "form": "그니까 다 잘해요 ㅋㅋㅋㅋ",
          "original_form": "그니까 다 잘해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100037598.1.15",
          "form": "황우슬혜도 거기서 캐릭터 매력적이었던거같아요 ㅋㅋㅋ",
          "original_form": "황우슬혜도 거기서 캐릭터 매력적이었던거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100037598.1.16",
          "form": "이선균도 따라했네요ㅋㅋㅋㅋㅋ",
          "original_form": "이선균도 따라했네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100037598.1.17",
          "form": "이정도면 출연료 더 받으셔야했을듯",
          "original_form": "이정도면 출연료 더 받으셔야했을듯",
          "speaker_id": "1",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100037598.1.18",
          "form": "ㅋㅋㅋ 정채연나온것중에 투제니 보셨어요?",
          "original_form": "ㅋㅋㅋ 정채연나온것중에 투제니 보셨어요?",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100037598.1.19",
          "form": "아뇨 첨들어봐요",
          "original_form": "아뇨 첨들어봐요",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100037598.1.20",
          "form": "또 누구나와요?",
          "original_form": "또 누구나와요?",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100037598.1.21",
          "form": "김성철? 이성철?",
          "original_form": "김성철? 이성철?",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100037598.1.22",
          "form": "이름을 정확히 모르겠네요 그분이랑 그 한다다 나왔던...",
          "original_form": "이름을 정확히 모르겠네요 그분이랑 그 한다다 나왔던...",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100037598.1.23",
          "form": "이상이 요! ㅋㅋ",
          "original_form": "이상이 요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100037598.1.24",
          "form": "김성철 정채연 둘이 주인공이고 2편짜리에요 ㅋㅋ",
          "original_form": "김성철 정채연 둘이 주인공이고 2편짜리에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100037598.1.25",
          "form": "오 두배우다 좋게 보고있는데!",
          "original_form": "오 두배우다 좋게 보고있는데!",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100037598.1.26",
          "form": "이거 다보고 찾아봐야겠어요ㅋㅋ",
          "original_form": "이거 다보고 찾아봐야겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:23"
        }
      ]
    }
