      "utterance": [
        {
          "id": "MDRW2100021893.1.1",
          "form": "여행 계획 있나요?",
          "original_form": "여행 계획 있나요?",
          "speaker_id": "2",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100021893.1.2",
          "form": "여행 가고싶은데 코로나때문에 너무 무서워서 엄두가안나요ㅠ",
          "original_form": "여행 가고싶은데 코로나때문에 너무 무서워서 엄두가안나요ㅠ",
          "speaker_id": "1",
          "time": "20210829 09:56"
        },
        {
          "id": "MDRW2100021893.1.3",
          "form": "해외는 당분간 불가능일거같고",
          "original_form": "해외는 당분간 불가능일거같고",
          "speaker_id": "1",
          "time": "20210829 09:56"
        },
        {
          "id": "MDRW2100021893.1.4",
          "form": "좀 잠잠해지면 국내라도 가고싶네요",
          "original_form": "좀 잠잠해지면 국내라도 가고싶네요",
          "speaker_id": "1",
          "time": "20210829 09:56"
        },
        {
          "id": "MDRW2100021893.1.5",
          "form": "국내 어디 가고 싶으세요?",
          "original_form": "국내 어디 가고 싶으세요?",
          "speaker_id": "2",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100021893.1.6",
          "form": "전 제주도 가고파요^^",
          "original_form": "전 제주도 가고파요^^",
          "speaker_id": "1",
          "time": "20210829 15:22"
        },
        {
          "id": "MDRW2100021893.1.7",
          "form": "가족여행으루요~",
          "original_form": "가족여행으루요~",
          "speaker_id": "1",
          "time": "20210829 15:23"
        },
        {
          "id": "MDRW2100021893.1.8",
          "form": "저도 제주도나 여수 쪽 가보고 싶어요~",
          "original_form": "저도 제주도나 여수 쪽 가보고 싶어요~",
          "speaker_id": "2",
          "time": "20210829 15:23"
        },
        {
          "id": "MDRW2100021893.1.9",
          "form": "제주도 어디 가고 싶으세요?",
          "original_form": "제주도 어디 가고 싶으세요?",
          "speaker_id": "2",
          "time": "20210829 15:23"
        },
        {
          "id": "MDRW2100021893.1.10",
          "form": "딱히 계획을 세워놓진 않아서 ㅠㅠ 당분간은 위험하지싶네요",
          "original_form": "딱히 계획을 세워놓진 않아서 ㅠㅠ 당분간은 위험하지싶네요",
          "speaker_id": "1",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100021893.1.11",
          "form": "통영거제는 가보셨나요 ㅎㅎ",
          "original_form": "통영거제는 가보셨나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100021893.1.12",
          "form": "아니요. 아랫지역은 거의 안 가봤어요",
          "original_form": "아니요. 아랫지역은 거의 안 가봤어요",
          "speaker_id": "2",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100021893.1.13",
          "form": "기회가된다면 언젠가~ 여행하기 참 좋답니다~~",
          "original_form": "기회가된다면 언젠가~ 여행하기 참 좋답니다~~",
          "speaker_id": "1",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100021893.1.14",
          "form": "여수 스카이워크 인가 줄하나 메고 하늘을 걷는데가 있다고 해서 거기 가보고 싶어요",
          "original_form": "여수 스카이워크 인가 줄하나 메고 하늘을 걷는데가 있다고 해서 거기 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100021893.1.15",
          "form": "통영도 경치가 좋은거 같아서 함 가보고 싶네요",
          "original_form": "통영도 경치가 좋은거 같아서 함 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100021893.1.16",
          "form": "오~ 여수에 그런게 있었나요??",
          "original_form": "오~ 여수에 그런게 있었나요??",
          "speaker_id": "1",
          "time": "20210829 15:29"
        },
        {
          "id": "MDRW2100021893.1.17",
          "form": "관광명소겠군요!ㅎ",
          "original_form": "관광명소겠군요!ㅎ",
          "speaker_id": "1",
          "time": "20210829 15:29"
        },
        {
          "id": "MDRW2100021893.1.18",
          "form": "네 TV에 나오더라구요\n무섭지만 재미있을 거 같아요^^",
          "original_form": "네 TV에 나오더라구요\n무섭지만 재미있을 거 같아요^^",
          "speaker_id": "2",
          "time": "20210829 15:30"
        },
        {
          "id": "MDRW2100021893.1.19",
          "form": "그러네요^^  레일바이크도 타러가고싶네요~ㅋㅋ",
          "original_form": "그러네요^^  레일바이크도 타러가고싶네요~ㅋㅋ",
          "speaker_id": "1",
