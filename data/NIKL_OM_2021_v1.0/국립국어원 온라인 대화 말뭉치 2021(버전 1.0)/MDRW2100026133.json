      "utterance": [
        {
          "id": "MDRW2100026133.1.1",
          "form": "안녕~ 너는 봉사하는거 있어?",
          "original_form": "안녕~ 너는 봉사하는거 있어?",
          "speaker_id": "1",
          "time": "20210901 19:59"
        },
        {
          "id": "MDRW2100026133.1.2",
          "form": "아니! 나는 지금 봉사활동은 안해!",
          "original_form": "아니! 나는 지금 봉사활동은 안해!",
          "speaker_id": "2",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100026133.1.3",
          "form": "너는 어때?? 봉사활동 하는것 있어??",
          "original_form": "너는 어때?? 봉사활동 하는것 있어??",
          "speaker_id": "2",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026133.1.4",
          "form": "나는 유기견 센터에서 봉사하는 거 해! 가끔?",
          "original_form": "나는 유기견 센터에서 봉사하는 거 해! 가끔?",
          "speaker_id": "1",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026133.1.5",
          "form": "오~ 동물 좋아하나 보구나!",
          "original_form": "오~ 동물 좋아하나 보구나!",
          "speaker_id": "2",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026133.1.6",
          "form": "힘들진 않아??",
          "original_form": "힘들진 않아??",
          "speaker_id": "2",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026133.1.7",
          "form": "응 강아지 키우거든!",
          "original_form": "응 강아지 키우거든!",
          "speaker_id": "1",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026133.1.8",
          "form": "엄청 힘들진 않아~ 가끔만 가니까 괜찮은 거 같아!",
          "original_form": "엄청 힘들진 않아~ 가끔만 가니까 괜찮은 거 같아!",
          "speaker_id": "1",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026133.1.9",
          "form": "가끔이라도 봉사한다니 너무 멋지다!",
          "original_form": "가끔이라도 봉사한다니 너무 멋지다!",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026133.1.10",
          "form": "혼자가서 하는거야??",
          "original_form": "혼자가서 하는거야??",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026133.1.11",
          "form": "응 혼자 가는데 나 혼자서 다 하진 않고",
          "original_form": "응 혼자 가는데 나 혼자서 다 하진 않고",
          "speaker_id": "1",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026133.1.12",
          "form": "스케줄 표가 따로 있거든 거기 사람들이 적어놔",
          "original_form": "스케줄 표가 따로 있거든 거기 사람들이 적어놔",
          "speaker_id": "1",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026133.1.13",
          "form": "그래서 보통 한 두세명이 같이 해!",
          "original_form": "그래서 보통 한 두세명이 같이 해!",
          "speaker_id": "1",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026133.1.14",
          "form": "그렇구나! 다들 너무 대단하다",
          "original_form": "그렇구나! 다들 너무 대단하다",
          "speaker_id": "2",
          "time": "20210901 20:17"
        },
        {
          "id": "MDRW2100026133.1.15",
          "form": "봉사말고는 모임이나 다른거 하는게 있어??",
          "original_form": "봉사말고는 모임이나 다른거 하는게 있어??",
          "speaker_id": "2",
          "time": "20210901 20:17"
        },
        {
          "id": "MDRW2100026133.1.16",
          "form": "나 동호회도 있어 문학 동호회!",
          "original_form": "나 동호회도 있어 문학 동호회!",
          "speaker_id": "1",
          "time": "20210901 20:17"
        },
        {
          "id": "MDRW2100026133.1.17",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210901 20:17"
        },
        {
          "id": "MDRW2100026133.1.18",
          "form": "오 여러가지 많이 하는구나!",
          "original_form": "오 여러가지 많이 하는구나!",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100026133.1.19",
          "form": "나는 동호회는 아니고 학생이라 학교에서 동아리해!",
          "original_form": "나는 동호회는 아니고 학생이라 학교에서 동아리해!",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100026133.1.20",
          "form": "댄스동아리 하고있어~",
          "original_form": "댄스동아리 하고있어~",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100026133.1.21",
          "form": "우와 댄스동아리라니",
          "original_form": "우와 댄스동아리라니",
          "speaker_id": "1",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100026133.1.22",
          "form": "진짜 멋지다",
          "original_form": "진짜 멋지다",
          "speaker_id": "1",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100026133.1.23",
          "form": "어떤 댄스해?",
          "original_form": "어떤 댄스해?",
          "speaker_id": "1",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100026133.1.24",
          "form": "방송댄스 하고있어~",
          "original_form": "방송댄스 하고있어~",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026133.1.25",
          "form": "요즘엔 코로나때매 어디서 공연하고 그러지는 못하지만ㅠㅠ",
          "original_form": "요즘엔 코로나때매 어디서 공연하고 그러지는 못하지만ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026133.1.26",
          "form": "우와 방송댄스! 공연도 하는 구나 대박이다",
          "original_form": "우와 방송댄스! 공연도 하는 구나 대박이다",
          "speaker_id": "1",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100026133.1.27",
          "form": "원래는 공연 계획 있었어?",
          "original_form": "원래는 공연 계획 있었어?",
          "speaker_id": "1",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100026133.1.28",
          "form": "응 원래 학교 축제할때 공연하는데 대학 축제를 안하니까 안하고 있어ㅜㅜ",
          "original_form": "응 원래 학교 축제할때 공연하는데 대학 축제를 안하니까 안하고 있어ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 21:05"
        }
      ]
    }
