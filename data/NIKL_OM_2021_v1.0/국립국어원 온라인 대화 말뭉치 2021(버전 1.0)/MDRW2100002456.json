      "utterance": [
        {
          "id": "MDRW2100002456.1.1",
          "form": "곰표맥주 드셔보셨나요",
          "original_form": "곰표맥주 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002456.1.2",
          "form": "아니요 술을 좋아하는편이 아니라서 먹어보진 않았어요",
          "original_form": "아니요 술을 좋아하는편이 아니라서 먹어보진 않았어요",
          "speaker_id": "2",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002456.1.3",
          "form": "근데 맛있다그러더라구요",
          "original_form": "근데 맛있다그러더라구요",
          "speaker_id": "2",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002456.1.4",
          "form": "드셔보셨나요?",
          "original_form": "드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002456.1.5",
          "form": "아뇨 저도 아직ㅎㅎ",
          "original_form": "아뇨 저도 아직ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002456.1.6",
          "form": "소문만 들었어요",
          "original_form": "소문만 들었어요",
          "speaker_id": "1",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002456.1.7",
          "form": "요새 이런 콜라보제품이 많이 나오더라구요",
          "original_form": "요새 이런 콜라보제품이 많이 나오더라구요",
          "speaker_id": "1",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002456.1.8",
          "form": "마자요 구두약도 콜라보하고",
          "original_form": "마자요 구두약도 콜라보하고",
          "speaker_id": "2",
          "time": "20210820 12:36"
        },
        {
          "id": "MDRW2100002456.1.9",
          "form": "정말 별걸",
          "original_form": "정말 별걸",
          "speaker_id": "1",
          "time": "20210820 12:36"
        },
        {
          "id": "MDRW2100002456.1.10",
          "form": "다하는거같아요",
          "original_form": "다하는거같아요",
          "speaker_id": "1",
          "time": "20210820 12:36"
        },
        {
          "id": "MDRW2100002456.1.11",
          "form": "그니까요 몇개 히트치니까 다 따라서 이것저것 하나봐요",
          "original_form": "그니까요 몇개 히트치니까 다 따라서 이것저것 하나봐요",
          "speaker_id": "2",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002456.1.12",
          "form": "딱 곰표까지가",
          "original_form": "딱 곰표까지가",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002456.1.13",
          "form": "적절했던거같아요",
          "original_form": "적절했던거같아요",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002456.1.14",
          "form": "마자요 곰표가 제일 위트있게 잘한거같아요",
          "original_form": "마자요 곰표가 제일 위트있게 잘한거같아요",
          "speaker_id": "2",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002456.1.15",
          "form": "패딩도 그렇고 팝콘도 그렇고",
          "original_form": "패딩도 그렇고 팝콘도 그렇고",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002456.1.16",
          "form": "맞아요 신박한거 많더라구요",
          "original_form": "맞아요 신박한거 많더라구요",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002456.1.17",
          "form": "전 곰표가방 있어요ㅋㅋ",
          "original_form": "전 곰표가방 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002456.1.18",
          "form": "에코백",
          "original_form": "에코백",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002456.1.19",
          "form": "아 진짜요?\n실제로 착용하고 다녀보셨나요?",
          "original_form": "아 진짜요?\n실제로 착용하고 다녀보셨나요?",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002456.1.20",
          "form": "넵...좀 힙해보여서....",
          "original_form": "넵...좀 힙해보여서....",
          "speaker_id": "1",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002456.1.21",
          "form": "꽤튼튼해여",
          "original_form": "꽤튼튼해여",
          "speaker_id": "1",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002456.1.22",
          "form": "근데 조금 부끄럽긴..했어욬ㅋㅋ",
          "original_form": "근데 조금 부끄럽긴..했어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002456.1.23",
          "form": "와 대단하시네요!",
          "original_form": "와 대단하시네요!",
          "speaker_id": "2",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002456.1.24",
          "form": "근데 진짜 힙해보이긴할거같아요 !",
          "original_form": "근데 진짜 힙해보이긴할거같아요 !",
          "speaker_id": "2",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002456.1.25",
          "form": "ㅋㅋㅋ 나중에 용기가 생기면 또 매고 나가야겠어요",
          "original_form": "ㅋㅋㅋ 나중에 용기가 생기면 또 매고 나가야겠어요",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002456.1.26",
          "form": "용기가 멋있어요!",
          "original_form": "용기가 멋있어요!",
          "speaker_id": "2",
          "time": "20210820 12:43"
        }
      ]
    }
  ]
