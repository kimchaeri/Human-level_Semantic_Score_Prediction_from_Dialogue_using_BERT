        },
        {
          "id": "MDRW2100010236.1.2",
          "form": "안녕하세요 ~ 좋아하는 음료 있으신가요",
          "original_form": "안녕하세요 ~ 좋아하는 음료 있으신가요",
          "speaker_id": "2",
          "time": "20210823 16:42"
        },
        {
          "id": "MDRW2100010236.1.3",
          "form": "전 단거좋아해요ㅋㅋㅋ",
          "original_form": "전 단거좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:45"
        },
        {
          "id": "MDRW2100010236.1.4",
          "form": "자바칩프랍이요",
          "original_form": "자바칩프랍이요",
          "speaker_id": "1",
          "time": "20210823 16:46"
        },
        {
          "id": "MDRW2100010236.1.5",
          "form": "오 ㅎ 그거 맛있죠",
          "original_form": "오 ㅎ 그거 맛있죠",
          "speaker_id": "2",
          "time": "20210823 17:34"
        },
        {
          "id": "MDRW2100010236.1.6",
          "form": "민초도 좋아하시나용",
          "original_form": "민초도 좋아하시나용",
          "speaker_id": "2",
          "time": "20210823 17:34"
        },
        {
          "id": "MDRW2100010236.1.7",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010236.1.8",
          "form": "민초를 전혀 이해못하는 1인이랍니다",
          "original_form": "민초를 전혀 이해못하는 1인이랍니다",
          "speaker_id": "1",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010236.1.9",
          "form": "헐 왜죠.....",
          "original_form": "헐 왜죠.....",
          "speaker_id": "2",
          "time": "20210823 17:48"
        },
        {
          "id": "MDRW2100010236.1.10",
          "form": "민초 정말 맛있는데",
          "original_form": "민초 정말 맛있는데",
          "speaker_id": "2",
          "time": "20210823 17:48"
        },
        {
          "id": "MDRW2100010236.1.11",
          "form": "치약맛",
          "original_form": "치약맛",
          "speaker_id": "1",
          "time": "20210823 17:49"
        },
        {
          "id": "MDRW2100010236.1.12",
          "form": "아닌가요?!",
          "original_form": "아닌가요?!",
          "speaker_id": "1",
          "time": "20210823 17:49"
        },
        {
          "id": "MDRW2100010236.1.13",
          "form": "저한테는 치약맛이 아니더라구요..",
          "original_form": "저한테는 치약맛이 아니더라구요..",
          "speaker_id": "2",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100010236.1.14",
          "form": "배라꺼도 안먹어보셨어요?",
          "original_form": "배라꺼도 안먹어보셨어요?",
          "speaker_id": "2",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100010236.1.15",
          "form": "ㅋㅋㅋ 안먹어본게아니고",
          "original_form": "ㅋㅋㅋ 안먹어본게아니고",
          "speaker_id": "1",
          "time": "20210824 10:43"
        },
        {
          "id": "MDRW2100010236.1.16",
          "form": "안먹는답니다ㅠㅠ",
          "original_form": "안먹는답니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 10:43"
        },
        {
          "id": "MDRW2100010236.1.17",
          "form": "돈줘도 싫은정도?",
          "original_form": "돈줘도 싫은정도?",
          "speaker_id": "1",
          "time": "20210824 10:43"
        },
        {
          "id": "MDRW2100010236.1.18",
          "form": "ㅋㅋㅋㅋ 헉 정말 싫어하시는군요",
          "original_form": "ㅋㅋㅋㅋ 헉 정말 싫어하시는군요",
          "speaker_id": "2",
          "time": "20210824 11:17"
        },
        {
          "id": "MDRW2100010236.1.19",
          "form": "돈줘도 싫은정도라니 ",
          "original_form": "돈줘도 싫은정도라니 ",
          "speaker_id": "2",
          "time": "20210824 11:17"
        },
        {
          "id": "MDRW2100010236.1.20",
          "form": "맛이 없잖아요ㅠㅠ",
          "original_form": "맛이 없잖아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100010236.1.21",
          "form": "맛이 없다니 충격적이요!!!!!",
          "original_form": "맛이 없다니 충격적이요!!!!!",
          "speaker_id": "2",
          "time": "20210824 11:45"
        },
        {
          "id": "MDRW2100010236.1.22",
          "form": "민초파말고는",
          "original_form": "민초파말고는",
          "speaker_id": "1",
          "time": "20210824 11:53"
        },
        {
          "id": "MDRW2100010236.1.23",
          "form": "전부그렇게생각할거예요!",
          "original_form": "전부그렇게생각할거예요!",
          "speaker_id": "1",
          "time": "20210824 11:53"
        },
