      "utterance": [
        {
          "id": "MDRW2100045556.1.1",
          "form": "음 한국에서 자랄수 있나요?",
          "original_form": "음 한국에서 자랄수 있나요?",
          "speaker_id": "2",
          "time": "20210908 03:26"
        },
        {
          "id": "MDRW2100045556.1.2",
          "form": "네 한국에서 재배하는 과일이예요",
          "original_form": "네 한국에서 재배하는 과일이예요",
          "speaker_id": "1",
          "time": "20210908 03:27"
        },
        {
          "id": "MDRW2100045556.1.3",
          "form": "혹시 가을 제철 과일 인가요'?",
          "original_form": "혹시 가을 제철 과일 인가요'?",
          "speaker_id": "2",
          "time": "20210908 03:28"
        },
        {
          "id": "MDRW2100045556.1.4",
          "form": "어..아니요! 가을철 과일이 아니예요",
          "original_form": "어..아니요! 가을철 과일이 아니예요",
          "speaker_id": "1",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100045556.1.5",
          "form": "여름과일입니다",
          "original_form": "여름과일입니다",
          "speaker_id": "1",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100045556.1.6",
          "form": "껍질이 얇은 과일인가요?",
          "original_form": "껍질이 얇은 과일인가요?",
          "speaker_id": "2",
          "time": "20210908 03:30"
        },
        {
          "id": "MDRW2100045556.1.7",
          "form": "네 껍질이 얇은 과일이예요ㅎㅎ",
          "original_form": "네 껍질이 얇은 과일이예요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 03:30"
        },
        {
          "id": "MDRW2100045556.1.8",
          "form": "색상이 핑크색인가염??",
          "original_form": "색상이 핑크색인가염??",
          "speaker_id": "2",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100045556.1.9",
          "form": "헉 네 예리하신데요?!",
          "original_form": "헉 네 예리하신데요?!",
          "speaker_id": "1",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100045556.1.10",
          "form": "그렇다면 복숭아!!!",
          "original_form": "그렇다면 복숭아!!!",
          "speaker_id": "2",
          "time": "20210908 03:33"
        },
        {
          "id": "MDRW2100045556.1.11",
          "form": "맞나여~?",
          "original_form": "맞나여~?",
          "speaker_id": "2",
          "time": "20210908 03:33"
        },
        {
          "id": "MDRW2100045556.1.12",
          "form": "네 맞아요! 복숭아 정답입니다~!!!",
          "original_form": "네 맞아요! 복숭아 정답입니다~!!!",
          "speaker_id": "1",
          "time": "20210908 03:34"
        },
        {
          "id": "MDRW2100045556.1.13",
          "form": "이번엔 엄청 빨리 맞추셨어요!",
          "original_form": "이번엔 엄청 빨리 맞추셨어요!",
          "speaker_id": "1",
          "time": "20210908 03:34"
        },
        {
          "id": "MDRW2100045556.1.14",
          "form": "어떻게 감 잡으신거지",
          "original_form": "어떻게 감 잡으신거지",
          "speaker_id": "1",
          "time": "20210908 03:34"
        },
        {
          "id": "MDRW2100045556.1.15",
          "form": "크크",
          "original_form": "크크",
          "speaker_id": "2",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045556.1.16",
          "form": "복숭아 아니먼 틀렷을거에여",
          "original_form": "복숭아 아니먼 틀렷을거에여",
          "speaker_id": "2",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045556.1.17",
          "form": "지식의한계로",
          "original_form": "지식의한계로",
          "speaker_id": "2",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045556.1.18",
          "form": "여름은 복숭안데 ㅋㅋㅋㅠ햇어요",
          "original_form": "여름은 복숭안데 ㅋㅋㅋㅠ햇어요",
          "speaker_id": "2",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045556.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ신깋해요 저같았으면 여름은 수박밖에 몰랐을텐데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ신깋해요 저같았으면 여름은 수박밖에 몰랐을텐데",
          "speaker_id": "1",
          "time": "20210908 03:35"
        },
        {
