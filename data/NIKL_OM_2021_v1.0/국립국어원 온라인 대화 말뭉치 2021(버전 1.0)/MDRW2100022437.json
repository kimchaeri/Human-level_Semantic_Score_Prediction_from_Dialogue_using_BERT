        },
        {
          "id": "MDRW2100022437.1.2",
          "form": "시간이 핑계죠",
          "original_form": "시간이 핑계죠",
          "speaker_id": "2",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100022437.1.3",
          "form": "인생작있어요?",
          "original_form": "인생작있어요?",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022437.1.4",
          "form": "가시고기요",
          "original_form": "가시고기요",
          "speaker_id": "2",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022437.1.5",
          "form": "뭔내용이에여?",
          "original_form": "뭔내용이에여?",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022437.1.6",
          "form": "아버지늬 사랑이요",
          "original_form": "아버지늬 사랑이요",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022437.1.7",
          "form": "오 감동적일것같네요",
          "original_form": "오 감동적일것같네요",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022437.1.8",
          "form": "슬퍼요?",
          "original_form": "슬퍼요?",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022437.1.9",
          "form": "네 너무",
          "original_form": "네 너무",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022437.1.10",
          "form": "울지도 모르겠네요",
          "original_form": "울지도 모르겠네요",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
