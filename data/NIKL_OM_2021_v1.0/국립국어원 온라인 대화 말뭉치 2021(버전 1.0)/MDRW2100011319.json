      "utterance": [
        {
          "id": "MDRW2100011319.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 08:36"
        },
        {
          "id": "MDRW2100011319.1.2",
          "form": "네 안녕하세요~",
          "original_form": "네 안녕하세요~",
          "speaker_id": "2",
          "time": "20210903 08:37"
        },
        {
          "id": "MDRW2100011319.1.3",
          "form": "게임 좋아하시나요",
          "original_form": "게임 좋아하시나요",
          "speaker_id": "1",
          "time": "20210903 08:39"
        },
        {
          "id": "MDRW2100011319.1.4",
          "form": "잘은 못하지만 좋아하는편이에요 ㅎㅎ",
          "original_form": "잘은 못하지만 좋아하는편이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 09:52"
        },
        {
          "id": "MDRW2100011319.1.5",
          "form": "어떤 게임 하세요?",
          "original_form": "어떤 게임 하세요?",
          "speaker_id": "1",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100011319.1.6",
          "form": "요즘엔 폰게임 많이하는편이에요 ㅎ..",
          "original_form": "요즘엔 폰게임 많이하는편이에요 ㅎ..",
          "speaker_id": "2",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100011319.1.7",
          "form": "약간 퍼즐같은거..",
          "original_form": "약간 퍼즐같은거..",
          "speaker_id": "2",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100011319.1.8",
          "form": "저도 퍼즐겜 좋아해요",
          "original_form": "저도 퍼즐겜 좋아해요",
          "speaker_id": "1",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100011319.1.9",
          "form": "어떤거 제일좋아하세요~? z",
          "original_form": "어떤거 제일좋아하세요~? z",
          "speaker_id": "2",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100011319.1.10",
          "form": "퍼즐앤드래곤 좋아해요ㅎㅎ",
          "original_form": "퍼즐앤드래곤 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100011319.1.11",
          "form": "오 첨들어보는겜이긴한데",
          "original_form": "오 첨들어보는겜이긴한데",
          "speaker_id": "2",
          "time": "20210903 10:57"
        },
        {
          "id": "MDRW2100011319.1.12",
          "form": "재밌어보이네요 ㅎㅎ",
          "original_form": "재밌어보이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:57"
        },
        {
          "id": "MDRW2100011319.1.13",
          "form": "퍼즐보다는 캐릭터빨이 우선되긴 하지만",
          "original_form": "퍼즐보다는 캐릭터빨이 우선되긴 하지만",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100011319.1.14",
          "form": "재밌어요",
          "original_form": "재밌어요",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100011319.1.15",
          "form": "헣ㅎ...저는 그런 가차게임",
          "original_form": "헣ㅎ...저는 그런 가차게임",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100011319.1.16",
          "form": "만 하면",
          "original_form": "만 하면",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100011319.1.17",
          "form": "지갑전사가되가지고",
          "original_form": "지갑전사가되가지고",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100011319.1.18",
          "form": "..자제중이에여...",
          "original_form": "..자제중이에여...",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100011319.1.19",
          "form": "저도 지갑전사 될뻔했어요",
          "original_form": "저도 지갑전사 될뻔했어요",
          "speaker_id": "1",
          "time": "20210903 11:59"
        },
        {
          "id": "MDRW2100011319.1.20",
          "form": "ㅋㅋㅋㅋ정말...참기힘들어요...",
          "original_form": "ㅋㅋㅋㅋ정말...참기힘들어요...",
          "speaker_id": "2",
          "time": "20210903 12:09"
        },
        {
          "id": "MDRW2100011319.1.21",
          "form": "안할라고는하는데...",
          "original_form": "안할라고는하는데...",
          "speaker_id": "2",
          "time": "20210903 12:09"
        },
        {
          "id": "MDRW2100011319.1.22",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 12:10"
        },
        {
          "id": "MDRW2100011319.1.23",
          "form": "가챠의 짜릿함은 도박인가봐요ㅋㅋ",
          "original_form": "가챠의 짜릿함은 도박인가봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:37"
        },
        {
          "id": "MDRW2100011319.1.24",
          "form": "그춐ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그춐ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100011319.1.25",
          "form": "진짜 사람이",
          "original_form": "진짜 사람이",
          "speaker_id": "2",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100011319.1.26",
          "form": "도박에빠진사람을..",
          "original_form": "도박에빠진사람을..",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100011319.1.27",
          "form": "이해하게된순간이랄까..ㅎ",
          "original_form": "이해하게된순간이랄까..ㅎ",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100011319.1.28",
          "form": "그래서 저는 그런 순간에 게임을 지워요ㅠ",
          "original_form": "그래서 저는 그런 순간에 게임을 지워요ㅠ",
          "speaker_id": "1",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100011319.1.29",
          "form": "어헛...저도 막 지우는데",
          "original_form": "어헛...저도 막 지우는데",
          "speaker_id": "2",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100011319.1.30",
          "form": "저번에",
          "original_form": "저번에",
          "speaker_id": "2",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100011319.1.31",
          "form": "귀여운...쿠키런..",
          "original_form": "귀여운...쿠키런..",
          "speaker_id": "2",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100011319.1.32",
          "form": "돈지르고",
          "original_form": "돈지르고",
          "speaker_id": "2",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100011319.1.33",
          "form": "현타와서",
          "original_form": "현타와서",
          "speaker_id": "2",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100011319.1.34",
          "form": "지웠어요..ㅎ",
          "original_form": "지웠어요..ㅎ",
          "speaker_id": "2",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100011319.1.35",
          "form": "쿠키런하셨었군요",
          "original_form": "쿠키런하셨었군요",
          "speaker_id": "1",
          "time": "20210903 12:48"
        },
        {
          "id": "MDRW2100011319.1.36",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100011319.1.37",
          "form": "쿠키런 킹덤이요",
          "original_form": "쿠키런 킹덤이요",
          "speaker_id": "2",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100011319.1.38",
          "form": "워낙 귀여운 게임도 좋아해가지고 ..ㅎㅎ",
          "original_form": "워낙 귀여운 게임도 좋아해가지고 ..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100011319.1.39",
          "form": "ㅋㅋ 쿠키들이 귀엽긴하죠",
          "original_form": "ㅋㅋ 쿠키들이 귀엽긴하죠",
          "speaker_id": "1",
          "time": "20210903 14:59"
        },
        {
          "id": "MDRW2100011319.1.40",
          "form": "최애 누구였어요?",
          "original_form": "최애 누구였어요?",
          "speaker_id": "1",
          "time": "20210903 14:59"
        },
        {
          "id": "MDRW2100011319.1.41",
          "form": "아무래도 아메리카노였나",
          "original_form": "아무래도 아메리카노였나",
          "speaker_id": "2",
          "time": "20210903 15:03"
        },
        {
          "id": "MDRW2100011319.1.42",
          "form": "커피맛쿠키요 ㅋㅋㅋ",
          "original_form": "커피맛쿠키요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:03"
        },
        {
          "id": "MDRW2100011319.1.43",
          "form": "이름이 생각이안나넹..ㅎ",
          "original_form": "이름이 생각이안나넹..ㅎ",
          "speaker_id": "2",
          "time": "20210903 15:03"
        },
        {
          "id": "MDRW2100011319.1.44",
          "form": "에스프레소 아닌가요",
          "original_form": "에스프레소 아닌가요",
          "speaker_id": "1",
          "time": "20210903 15:07"
        },
        {
          "id": "MDRW2100011319.1.45",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "1",
          "time": "20210903 15:07"
        },
        {
          "id": "MDRW2100011319.1.46",
          "form": "아맞다",
          "original_form": "아맞다",
          "speaker_id": "2",
          "time": "20210903 15:17"
        },
        {
          "id": "MDRW2100011319.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:17"
        },
        {
          "id": "MDRW2100011319.1.48",
          "form": "기억력이 안좋아서ㅋㅋㅋ",
          "original_form": "기억력이 안좋아서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:18"
        },
        {
          "id": "MDRW2100011319.1.49",
          "form": "ㅋㅋ 잊어먹으실 수 있죠 뭐",
          "original_form": "ㅋㅋ 잊어먹으실 수 있죠 뭐",
          "speaker_id": "1",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100011319.1.50",
          "form": "접은지 좀 되가지고 ㅋㅋㅋㅋ",
          "original_form": "접은지 좀 되가지고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:24"
        },
        {
          "id": "MDRW2100011319.1.51",
          "form": "진짜.많이 걀제했는데...ㅎ.ㅎ.",
          "original_form": "진짜.많이 걀제했는데...ㅎ.ㅎ.",
