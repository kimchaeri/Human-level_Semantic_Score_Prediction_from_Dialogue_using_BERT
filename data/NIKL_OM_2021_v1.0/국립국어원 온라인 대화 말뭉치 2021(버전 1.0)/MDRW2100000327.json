        },
        {
          "id": "MDRW2100000327.1.3",
          "form": "일은 할만하신가요",
          "original_form": "일은 할만하신가요",
          "speaker_id": "1",
          "time": "20210818 15:33"
        },
        {
          "id": "MDRW2100000327.1.4",
          "form": "네 할만합니다~!",
          "original_form": "네 할만합니다~!",
          "speaker_id": "2",
          "time": "20210818 15:33"
        },
        {
          "id": "MDRW2100000327.1.5",
          "form": "어떤일하시나요?",
          "original_form": "어떤일하시나요?",
          "speaker_id": "1",
          "time": "20210818 15:33"
        },
        {
          "id": "MDRW2100000327.1.6",
          "form": "대화 내용 식별화 하는 일을 하고있습니다~!",
          "original_form": "대화 내용 식별화 하는 일을 하고있습니다~!",
          "speaker_id": "2",
          "time": "20210818 15:33"
        },
        {
          "id": "MDRW2100000327.1.7",
          "form": "아 특이한 일을 하시네요",
          "original_form": "아 특이한 일을 하시네요",
          "speaker_id": "1",
          "time": "20210818 15:34"
        },
        {
          "id": "MDRW2100000327.1.8",
          "form": "재택은 아니죠?",
          "original_form": "재택은 아니죠?",
          "speaker_id": "1",
          "time": "20210818 15:34"
        },
        {
          "id": "MDRW2100000327.1.9",
          "form": "네 회사에 나와서 일하고 있어요",
          "original_form": "네 회사에 나와서 일하고 있어요",
          "speaker_id": "2",
          "time": "20210818 15:34"
        },
        {
          "id": "MDRW2100000327.1.10",
          "form": "아하ㅋㅋ",
          "original_form": "아하ㅋㅋ",
          "speaker_id": "1",
          "time": "20210818 15:35"
        },
        {
          "id": "MDRW2100000327.1.11",
          "form": "출근길은 머시나요?",
          "original_form": "출근길은 머시나요?",
          "speaker_id": "1",
          "time": "20210818 15:35"
        },
        {
          "id": "MDRW2100000327.1.12",
          "form": "집에서 50분 정도 걸리는거 같아요",
          "original_form": "집에서 50분 정도 걸리는거 같아요",
          "speaker_id": "2",
          "time": "20210818 15:35"
        },
        {
          "id": "MDRW2100000327.1.13",
          "form": "아 지하철로 다니시나요?",
          "original_form": "아 지하철로 다니시나요?",
          "speaker_id": "1",
          "time": "20210818 15:36"
        },
        {
          "id": "MDRW2100000327.1.14",
          "form": "네 지하철로 다녀요",
          "original_form": "네 지하철로 다녀요",
          "speaker_id": "2",
          "time": "20210818 15:36"
        },
        {
          "id": "MDRW2100000327.1.15",
          "form": "사람 많지않아요?",
          "original_form": "사람 많지않아요?",
          "speaker_id": "1",
          "time": "20210818 15:36"
        },
        {
          "id": "MDRW2100000327.1.16",
          "form": "요즘 코로나가 심해서;;",
          "original_form": "요즘 코로나가 심해서;;",
          "speaker_id": "1",
          "time": "20210818 15:36"
        },
        {
          "id": "MDRW2100000327.1.17",
          "form": "출근시간이라 사람이 엄청 많네요 ㅠ",
