      "utterance": [
        {
          "id": "MDRW2100033916.1.1",
          "form": "4시간 동안 어떤 운동하셨어요?",
          "original_form": "4시간 동안 어떤 운동하셨어요?",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100033916.1.2",
          "form": "네 제가 봐도 참",
          "original_form": "네 제가 봐도 참",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100033916.1.3",
          "form": "자전거",
          "original_form": "자전거",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100033916.1.4",
          "form": "훌라후프",
          "original_form": "훌라후프",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100033916.1.5",
          "form": "홈트요",
          "original_form": "홈트요",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100033916.1.6",
          "form": "미친듯이 춤추며 운동요 ㅋㅋ",
          "original_form": "미친듯이 춤추며 운동요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100033916.1.7",
          "form": "홈트.. 엄청 어렵던데.. 진짜 대단해요",
          "original_form": "홈트.. 엄청 어렵던데.. 진짜 대단해요",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100033916.1.8",
          "form": "근데 건강하게 다이어트  하면 그렇게 한번에 뺄수가 없잖아요",
          "original_form": "근데 건강하게 다이어트  하면 그렇게 한번에 뺄수가 없잖아요",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100033916.1.9",
          "form": "한번에 많이 빼면 건강이 나빠지고.. 참 딜레마에요..",
          "original_form": "한번에 많이 빼면 건강이 나빠지고.. 참 딜레마에요..",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100033916.1.10",
          "form": "살이 갑자기 빠지면 피부는 괜찮나요?",
          "original_form": "살이 갑자기 빠지면 피부는 괜찮나요?",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100033916.1.11",
          "form": "피부 늘어지고 주름이 팍요",
          "original_form": "피부 늘어지고 주름이 팍요",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100033916.1.12",
          "form": "홈트 하다 막 짜증난데 그거 넘기면 하는거고 안되면 포기예요",
          "original_form": "홈트 하다 막 짜증난데 그거 넘기면 하는거고 안되면 포기예요",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100033916.1.13",
          "form": "인내심의 싸움이예요",
          "original_form": "인내심의 싸움이예요",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100033916.1.14",
          "form": "그나마 엄마 tv가 쉬워요 ㅋㅋ",
          "original_form": "그나마 엄마 tv가 쉬워요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100033916.1.15",
          "form": "네 안뺄수도 없고",
          "original_form": "네 안뺄수도 없고",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100033916.1.16",
          "form": "진짜 다이어트는 평생 해야 하나봐요 쉽지가 않아요",
          "original_form": "진짜 다이어트는 평생 해야 하나봐요 쉽지가 않아요",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100033916.1.17",
          "form": "그럼요 아안하고 싶어요",
          "original_form": "그럼요 아안하고 싶어요",
          "speaker_id": "2",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100033916.1.18",
          "form": "저도 한번에 몇십키로는 아니여도 한달에 5키로 정도 빼본적 있는데 유지가 어려우요",
          "original_form": "저도 한번에 몇십키로는 아니여도 한달에 5키로 정도 빼본적 있는데 유지가 어려우요",
          "speaker_id": "1",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100033916.1.19",
          "form": "네 500그람도 힘들어요 ㅡㅡ",
          "original_form": "네 500그람도 힘들어요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 19:28"
        },
        {
          "id": "MDRW2100033916.1.20",
          "form": "아 저도 먹고 안찌는 윤택한삶 살고 싶어요 ㅋ",
          "original_form": "아 저도 먹고 안찌는 윤택한삶 살고 싶어요 ㅋ",
          "speaker_id": "2",
          "time": "20210825 19:28"
        },
        {
          "id": "MDRW2100033916.1.21",
          "form": "생각만해도 행복하네요 ㅎㅎ 먹고 안찌다니 ㅋㅋ",
          "original_form": "생각만해도 행복하네요 ㅎㅎ 먹고 안찌다니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:28"
        },
        {
          "id": "MDRW2100033916.1.22",
          "form": "근데 저희 아들이 딱밥만 먹는데 40킬로대예요",
          "original_form": "근데 저희 아들이 딱밥만 먹는데 40킬로대예요",
          "speaker_id": "2",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100033916.1.23",
          "form": "역시 군것질 안하면 ㅋㅋ",
          "original_form": "역시 군것질 안하면 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100033916.1.24",
          "form": "안쪄요",
          "original_form": "안쪄요",
          "speaker_id": "2",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100033916.1.25",
          "form": "군것질 참는거 어렵던데",
          "original_form": "군것질 참는거 어렵던데",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100033916.1.26",
          "form": "저도 지금 딱 밥만 먹으려고 하는데",
          "original_form": "저도 지금 딱 밥만 먹으려고 하는데",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100033916.1.27",
          "form": "네 디저트 배가 떠로 있어요 ㅋㅋ",
          "original_form": "네 디저트 배가 떠로 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100033916.1.28",
          "form": "그 밥이 넘 고칼로리인가봐요ㅜ",
          "original_form": "그 밥이 넘 고칼로리인가봐요ㅜ",
          "speaker_id": "1",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100033916.1.29",
          "form": "밥먹고 간식먹고 ㅜㅜ",
          "original_form": "밥먹고 간식먹고 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100033916.1.30",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100033916.1.31",
          "form": "고기에 등등",
          "original_form": "고기에 등등",
          "speaker_id": "2",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100033916.1.32",
          "form": "밥도 조금 먹어야 해요",
          "original_form": "밥도 조금 먹어야 해요",
          "speaker_id": "1",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100033916.1.33",
          "form": "그래도 모니모니 해도 건강하게 다이어트하는 건 운동이 짱인거 같아요",
