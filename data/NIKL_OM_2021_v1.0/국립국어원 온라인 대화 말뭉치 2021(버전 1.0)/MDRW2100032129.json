      "utterance": [
        {
          "id": "MDRW2100032129.1.1",
          "form": "안녕하세요 여행 좋아하세요?",
          "original_form": "안녕하세요 여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 12:52"
        },
        {
          "id": "MDRW2100032129.1.2",
          "form": "누가 짜 준 여행가는 거 좋아해요",
          "original_form": "누가 짜 준 여행가는 거 좋아해요",
          "speaker_id": "2",
          "time": "20210905 12:57"
        },
        {
          "id": "MDRW2100032129.1.3",
          "form": "계획없는 여행은 안 좋아하시나용",
          "original_form": "계획없는 여행은 안 좋아하시나용",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032129.1.4",
          "form": "아 그것도 좋죠. 시간 여유가 좀 있다면요",
          "original_form": "아 그것도 좋죠. 시간 여유가 좀 있다면요",
          "speaker_id": "2",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100032129.1.5",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100032129.1.6",
          "form": "네 좋아하는.편이에요",
          "original_form": "네 좋아하는.편이에요",
          "speaker_id": "1",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032129.1.7",
          "form": "요즘은 못 가지만요...ㅋㅋ",
          "original_form": "요즘은 못 가지만요...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032129.1.8",
          "form": "저는.제가 계획을 주도적으로 짜는.편이에요",
          "original_form": "저는.제가 계획을 주도적으로 짜는.편이에요",
          "speaker_id": "1",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100032129.1.9",
          "form": "우와 멋지네요. 계획 짜는 거 어렵지 않아요?",
          "original_form": "우와 멋지네요. 계획 짜는 거 어렵지 않아요?",
          "speaker_id": "2",
          "time": "20210905 14:38"
        },
        {
          "id": "MDRW2100032129.1.10",
          "form": "재미있어요!",
          "original_form": "재미있어요!",
          "speaker_id": "1",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032129.1.11",
          "form": "그리고 계획없이 가면 좀 불안해하는 스타일이에요 ㅋㅋ",
          "original_form": "그리고 계획없이 가면 좀 불안해하는 스타일이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032129.1.12",
          "form": "특히.해외에서는요ㅠ",
          "original_form": "특히.해외에서는요ㅠ",
          "speaker_id": "1",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032129.1.13",
          "form": "아 해외에서 불안하죠. 전 계획 짜서 갔는데 가보니 상황이 바뀌어 있고 그래서 식은땀 난 적 있어요 ㅠ",
          "original_form": "아 해외에서 불안하죠. 전 계획 짜서 갔는데 가보니 상황이 바뀌어 있고 그래서 식은땀 난 적 있어요 ㅠ",
          "speaker_id": "2",
          "time": "20210905 14:54"
        },
        {
          "id": "MDRW2100032129.1.14",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100032129.1.15",
          "form": "그런거 너무 싫어해서 ㅋㅋㅋ",
          "original_form": "그런거 너무 싫어해서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100032129.1.16",
          "form": "플램 b까지 짜서 가요 그래도 돌발상황은 생기더라구요 ㅋㅋㅋ",
          "original_form": "플램 b까지 짜서 가요 그래도 돌발상황은 생기더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100032129.1.17",
          "form": "오오오 배운분... 짱 멋지십미다",
          "original_form": "오오오 배운분... 짱 멋지십미다",
          "speaker_id": "2",
          "time": "20210905 16:01"
        },
        {
          "id": "MDRW2100032129.1.18",
          "form": "그래도 요샌 급할 때 검색이라도 할 수 있어서 좋아요",
          "original_form": "그래도 요샌 급할 때 검색이라도 할 수 있어서 좋아요",
          "speaker_id": "2",
          "time": "20210905 16:02"
        },
        {
          "id": "MDRW2100032129.1.19",
          "form": "맞아요 ㅋㅋㅋ 요즘은 근데 구글맵만 있으면...",
          "original_form": "맞아요 ㅋㅋㅋ 요즘은 근데 구글맵만 있으면...",
          "speaker_id": "1",
          "time": "20210905 16:06"
        },
        {
          "id": "MDRW2100032129.1.20",
          "form": "그래도 기본적으로는 가이드를 다 짜가요 ㅋㅋㅋ",
          "original_form": "그래도 기본적으로는 가이드를 다 짜가요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:07"
        },
        {
          "id": "MDRW2100032129.1.21",
          "form": "근데 친구들은 잘 안 보더라고요 ㅋㅋㅋ",
          "original_form": "근데 친구들은 잘 안 보더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:07"
        },
        {
          "id": "MDRW2100032129.1.22",
          "form": "같이 가는 친구들이요?",
          "original_form": "같이 가는 친구들이요?",
          "speaker_id": "2",
          "time": "20210905 16:17"
        },
        {
          "id": "MDRW2100032129.1.23",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032129.1.24",
          "form": "주로 제가 여행 계획 가이드 만들어서 뿌려요 ㅋㅋ",
          "original_form": "주로 제가 여행 계획 가이드 만들어서 뿌려요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032129.1.25",
          "form": "키야 가이드까지. 친구들 복 받았네요",
          "original_form": "키야 가이드까지. 친구들 복 받았네요",
          "speaker_id": "2",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100032129.1.26",
          "form": "ㅋㅋㅋ 경비 계산같은 것도 제거 해요 주로",
          "original_form": "ㅋㅋㅋ 경비 계산같은 것도 제거 해요 주로",
          "speaker_id": "1",
          "time": "20210905 21:48"
        },
        {
          "id": "MDRW2100032129.1.27",
          "form": "그게 맘 편하더라구요",
          "original_form": "그게 맘 편하더라구요",
