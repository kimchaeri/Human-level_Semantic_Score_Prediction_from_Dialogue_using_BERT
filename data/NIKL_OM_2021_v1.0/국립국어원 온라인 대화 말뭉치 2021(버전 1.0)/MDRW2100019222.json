          "speaker_id": "2",
          "time": "20210827 16:24"
        },
        {
          "id": "MDRW2100019222.1.2",
          "form": "언니 하우징 좀 해?",
          "original_form": "언니 하우징 좀 해?",
          "speaker_id": "2",
          "time": "20210827 16:24"
        },
        {
          "id": "MDRW2100019222.1.3",
          "form": "나 완전 손 놓고있어.....",
          "original_form": "나 완전 손 놓고있어.....",
          "speaker_id": "2",
          "time": "20210827 16:24"
        },
        {
          "id": "MDRW2100019222.1.4",
          "form": "가구들이 생각보다 내취향이 아녀...",
          "original_form": "가구들이 생각보다 내취향이 아녀...",
          "speaker_id": "2",
          "time": "20210827 16:24"
        },
        {
          "id": "MDRW2100019222.1.5",
          "form": "나도 안들어간지 한참됐어 ㅋㅋㅋㅋ",
          "original_form": "나도 안들어간지 한참됐어 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019222.1.6",
          "form": "생각보다 칙칙하지?",
          "original_form": "생각보다 칙칙하지?",
          "speaker_id": "1",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019222.1.7",
          "form": "게임 컨텐츠 중에 젤 기대했는데",
          "original_form": "게임 컨텐츠 중에 젤 기대했는데",
          "speaker_id": "1",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019222.1.8",
          "form": "ㅇㅇ......안예뻐",
          "original_form": "ㅇㅇ......안예뻐",
          "speaker_id": "2",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019222.1.9",
          "form": "별로인듯....",
          "original_form": "별로인듯....",
          "speaker_id": "1",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019222.1.10",
          "form": "그리고 뭔가 컨텐츠랄게 없ㅇ니",
          "original_form": "그리고 뭔가 컨텐츠랄게 없ㅇ니",
          "speaker_id": "2",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019222.1.11",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019222.1.12",
          "form": "그거는 진짜",
          "original_form": "그거는 진짜",
          "speaker_id": "1",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019222.1.13",
          "form": "걍 시간차면 코인 얻어서",
          "original_form": "걍 시간차면 코인 얻어서",
          "speaker_id": "2",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019222.1.14",
          "form": "새 도면사기...?",
          "original_form": "새 도면사기...?",
          "speaker_id": "2",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019222.1.15",
          "form": "캐릭터 세워놓고 인형놀이 하는거에 가까워서",
          "original_form": "캐릭터 세워놓고 인형놀이 하는거에 가까워서",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019222.1.16",
          "form": "난 솔직히 영주민이나",
          "original_form": "난 솔직히 영주민이나",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019222.1.17",
          "form": "ㅇㅇ......걍",
          "original_form": "ㅇㅇ......걍",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019222.1.18",
          "form": "npc 세워놓고",
          "original_form": "npc 세워놓고",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019222.1.19",
          "form": "미연시하라고 만든듯",
          "original_form": "미연시하라고 만든듯",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019222.1.20",
          "form": "북적북적한거 즐기고싶음",
          "original_form": "북적북적한거 즐기고싶음",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019222.1.21",
          "form": "ㅇㅇ.....수달주민",
          "original_form": "ㅇㅇ.....수달주민",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019222.1.22",
          "form": "검은사막에서 제공한 영주성이",
          "original_form": "검은사막에서 제공한 영주성이",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019222.1.23",
          "form": "ㅇㅏ니",
          "original_form": "ㅇㅏ니",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019222.1.24",
          "form": "기능만 좀 더 있었어도 취향인데",
          "original_form": "기능만 좀 더 있었어도 취향인데",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019222.1.25",
          "form": "넣어달라는게",
          "original_form": "넣어달라는게",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019222.1.26",
          "form": "왜 나쁜말이냐고",
          "original_form": "왜 나쁜말이냐고",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019222.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019222.1.28",
          "form": "여튼 수달주민",
          "original_form": "여튼 수달주민",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019222.1.29",
          "form": "여기도 해줘라",
          "original_form": "여기도 해줘라",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019222.1.30",
          "form": "아니면 동물",
          "original_form": "아니면 동물",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019222.1.31",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019222.1.32",
          "form": "상호작용이랑 종류",
          "original_form": "상호작용이랑 종류",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019222.1.33",
          "form": "좀 다양하게 해줬으면 좋겠어",
          "original_form": "좀 다양하게 해줬으면 좋겠어",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019222.1.34",
          "form": "육성 컨텐츠도 포함시켜라",
          "original_form": "육성 컨텐츠도 포함시켜라",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019222.1.35",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019222.1.36",
          "form": "근데 낚시랑 관상어 시스템 들어온거 보면",
          "original_form": "근데 낚시랑 관상어 시스템 들어온거 보면",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019222.1.37",
          "form": "한 3.0버전쯤 되면",
          "original_form": "한 3.0버전쯤 되면",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019222.1.38",
          "form": "동물 키우기 생길듯 ㅋㅋㅋㅋ",
          "original_form": "동물 키우기 생길듯 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019222.1.39",
          "form": "집안에도 동물배치 가능하게 해줘라",
          "original_form": "집안에도 동물배치 가능하게 해줘라",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019222.1.40",
          "form": "아니 고영",
          "original_form": "아니 고영",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019222.1.41",
          "form": "집에 넣어주려햇는데",
          "original_form": "집에 넣어주려햇는데",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019222.1.42",
          "form": "안된대",
          "original_form": "안된대",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019222.1.43",
          "form": "그래서 마당에서 굴러다니는중이야",
          "original_form": "그래서 마당에서 굴러다니는중이야",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019222.1.44",
          "form": "동물 상호작용이랑 호감도 잇엇으면",
          "original_form": "동물 상호작용이랑 호감도 잇엇으면",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019222.1.45",
          "form": "각 동물별 테마방",
          "original_form": "각 동물별 테마방",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019222.1.46",
          "form": "하나씩 해줬다",
          "original_form": "하나씩 해줬다",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019222.1.47",
          "form": "어류는",
          "original_form": "어류는",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019222.1.48",
          "form": "안귀엽단말야",
          "original_form": "안귀엽단말야",
          "speaker_id": "2",
          "time": "20210827 16:29"
        },
        {
          "id": "MDRW2100019222.1.49",
          "form": "걔넨 애교도 안부리잔아",
          "original_form": "걔넨 애교도 안부리잔아",
          "speaker_id": "2",
          "time": "20210827 16:29"
        },
        {
          "id": "MDRW2100019222.1.50",
          "form": "그니까 ㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:29"
        },
        {
          "id": "MDRW2100019222.1.51",
          "form": "내가 첨벙 한번만 해도",
          "original_form": "내가 첨벙 한번만 해도",
          "speaker_id": "2",
          "time": "20210827 16:29"
        },
        {
          "id": "MDRW2100019222.1.52",
          "form": "데려와서 밥주고 키울 수 있게 해줘라",
          "original_form": "데려와서 밥주고 키울 수 있게 해줘라",
          "speaker_id": "1",
          "time": "20210827 16:29"
        },
        {
          "id": "MDRW2100019222.1.53",
          "form": "부리나케 달아나는데",
          "original_form": "부리나케 달아나는데",
          "speaker_id": "2",
          "time": "20210827 16:29"
        },
        {
          "id": "MDRW2100019222.1.54",
          "form": "무슨맛으로 키워",
          "original_form": "무슨맛으로 키워",
          "speaker_id": "2",
          "time": "20210827 16:29"
        },
        {
          "id": "MDRW2100019222.1.55",
          "form": "그니까 ㅋㅋㅋㅋ 얘네는 게임에서 유저들이 뭘 하는지 모르나봄",
          "original_form": "그니까 ㅋㅋㅋㅋ 얘네는 게임에서 유저들이 뭘 하는지 모르나봄",
          "speaker_id": "1",
          "time": "20210827 16:30"
        }
