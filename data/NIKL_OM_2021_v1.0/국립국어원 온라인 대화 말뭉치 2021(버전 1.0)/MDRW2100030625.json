      "utterance": [
        {
          "id": "MDRW2100030625.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210904 10:34"
        },
        {
          "id": "MDRW2100030625.1.2",
          "form": "네 해야죠 ㅎㅎ",
          "original_form": "네 해야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 10:34"
        },
        {
          "id": "MDRW2100030625.1.3",
          "form": "와이프 혼자할수 있나요",
          "original_form": "와이프 혼자할수 있나요",
          "speaker_id": "2",
          "time": "20210904 10:34"
        },
        {
          "id": "MDRW2100030625.1.4",
          "form": "거의 모든 일을 다합니다",
          "original_form": "거의 모든 일을 다합니다",
          "speaker_id": "2",
          "time": "20210904 10:34"
        },
        {
          "id": "MDRW2100030625.1.5",
          "form": "요리 빼고",
          "original_form": "요리 빼고",
          "speaker_id": "2",
          "time": "20210904 10:34"
        },
        {
          "id": "MDRW2100030625.1.6",
          "form": "수채구멍",
          "original_form": "수채구멍",
          "speaker_id": "2",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100030625.1.7",
          "form": "청소",
          "original_form": "청소",
          "speaker_id": "2",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100030625.1.8",
          "form": "음식물",
          "original_form": "음식물",
          "speaker_id": "2",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100030625.1.9",
          "form": "통",
          "original_form": "통",
          "speaker_id": "2",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100030625.1.10",
          "form": "더러운건 제담당",
          "original_form": "더러운건 제담당",
          "speaker_id": "2",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100030625.1.11",
          "form": "거실화장실도 제담당",
          "original_form": "거실화장실도 제담당",
          "speaker_id": "2",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100030625.1.12",
          "form": "청소기는 로봇",
          "original_form": "청소기는 로봇",
          "speaker_id": "2",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100030625.1.13",
          "form": "방닦는것도 제담당",
          "original_form": "방닦는것도 제담당",
          "speaker_id": "2",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100030625.1.14",
          "form": "시간이 너무 빨리 지나갑니다",
          "original_form": "시간이 너무 빨리 지나갑니다",
          "speaker_id": "2",
          "time": "20210904 10:36"
        },
        {
          "id": "MDRW2100030625.1.15",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210904 10:36"
        },
        {
          "id": "MDRW2100030625.1.16",
          "form": "분담해야죠",
          "original_form": "분담해야죠",
          "speaker_id": "1",
          "time": "20210904 10:36"
        },
        {
          "id": "MDRW2100030625.1.17",
          "form": "네 같이 합니다",
          "original_form": "네 같이 합니다",
          "speaker_id": "2",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100030625.1.18",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100030625.1.19",
          "form": "남편은유?",
          "original_form": "남편은유?",
          "speaker_id": "2",
          "time": "20210904 12:28"
        },
        {
          "id": "MDRW2100030625.1.20",
          "form": "전데여",
          "original_form": "전데여",
          "speaker_id": "1",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100030625.1.21",
          "form": "남편이세요?",
          "original_form": "남편이세요?",
          "speaker_id": "2",
          "time": "20210905 06:43"
        },
        {
          "id": "MDRW2100030625.1.22",
          "form": "오 너무 반갑습니다 남자랑 대화해보고 싶었습니다",
          "original_form": "오 너무 반갑습니다 남자랑 대화해보고 싶었습니다",
          "speaker_id": "2",
          "time": "20210905 06:44"
        },
        {
          "id": "MDRW2100030625.1.23",
          "form": "결혼 괜히햇나 싶습니다",
          "original_form": "결혼 괜히햇나 싶습니다",
          "speaker_id": "2",
          "time": "20210905 06:44"
        },
        {
          "id": "MDRW2100030625.1.24",
          "form": "너무 힘들어요 형님",
          "original_form": "너무 힘들어요 형님",
          "speaker_id": "2",
          "time": "20210905 06:44"
        },
        {
          "id": "MDRW2100030625.1.25",
          "form": "첨엔 싸우다가 포기하게 됩니다",
          "original_form": "첨엔 싸우다가 포기하게 됩니다",
          "speaker_id": "1",
          "time": "20210905 09:02"
        },
        {
          "id": "MDRW2100030625.1.26",
          "form": "합의점을 찾아야 됩니다",
          "original_form": "합의점을 찾아야 됩니다",
          "speaker_id": "2",
          "time": "20210905 09:11"
        },
        {
          "id": "MDRW2100030625.1.27",
          "form": "아마 못 찾고 지면서 포기하게 됩니다",
          "original_form": "아마 못 찾고 지면서 포기하게 됩니다",
          "speaker_id": "1",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100030625.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:13"
        },
        {
          "id": "MDRW2100030625.1.29",
          "form": "형님 힘내세요",
          "original_form": "형님 힘내세요",
          "speaker_id": "2",
          "time": "20210905 09:13"
        },
        {
          "id": "MDRW2100030625.1.30",
          "form": "익숙해지면괜찮아 지는데 어렵네오",
