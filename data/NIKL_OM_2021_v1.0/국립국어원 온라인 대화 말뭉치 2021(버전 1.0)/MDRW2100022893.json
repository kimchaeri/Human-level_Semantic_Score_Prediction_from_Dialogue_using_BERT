          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022893.1.13",
          "form": "그게좀걸림",
          "original_form": "그게좀걸림",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022893.1.14",
          "form": "오타가많이나더라고",
          "original_form": "오타가많이나더라고",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022893.1.15",
          "form": "동그란 자판 말하냐",
          "original_form": "동그란 자판 말하냐",
