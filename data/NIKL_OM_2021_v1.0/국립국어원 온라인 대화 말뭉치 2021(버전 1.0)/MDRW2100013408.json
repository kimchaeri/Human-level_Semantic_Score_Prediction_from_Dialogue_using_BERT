      "utterance": [
        {
          "id": "MDRW2100013408.1.1",
          "form": "나중에 계대카페가자",
          "original_form": "나중에 계대카페가자",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013408.1.2",
          "form": "내가 아는 카페잇어",
          "original_form": "내가 아는 카페잇어",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013408.1.3",
          "form": "전에 친구랑 간곳??",
          "original_form": "전에 친구랑 간곳??",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013408.1.4",
          "form": "어디야?",
          "original_form": "어디야?",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013408.1.5",
          "form": "카페앨고",
          "original_form": "카페앨고",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013408.1.6",
          "form": "여기 븡루레몬에이드 맛잇어",
          "original_form": "여기 븡루레몬에이드 맛잇어",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013408.1.7",
          "form": "오옹 궁금해",
          "original_form": "오옹 궁금해",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013408.1.8",
          "form": "글고 무슨 펠리컨?",
          "original_form": "글고 무슨 펠리컨?",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013408.1.9",
          "form": "플라멩고도 줘",
          "original_form": "플라멩고도 줘",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013408.1.10",
          "form": "인향이야?",
          "original_form": "인향이야?",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013408.1.11",
          "form": "인형",
          "original_form": "인형",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013408.1.12",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013408.1.13",
          "form": "아니 그건아니고",
          "original_form": "아니 그건아니고",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013408.1.14",
          "form": "무슨 막대기야",
          "original_form": "무슨 막대기야",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013408.1.15",
          "form": "그걸로 저으라고",
          "original_form": "그걸로 저으라고",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013408.1.16",
          "form": "아~",
          "original_form": "아~",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013408.1.17",
          "form": "글고 오마이데이즈",
          "original_form": "글고 오마이데이즈",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013408.1.18",
          "form": "여기는 너도알거야",
          "original_form": "여기는 너도알거야",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013408.1.19",
          "form": "어디지??",
          "original_form": "어디지??",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013408.1.20",
          "form": "그 무인프린트히는곳 알아?",
          "original_form": "그 무인프린트히는곳 알아?",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013408.1.21",
          "form": "아니....",
          "original_form": "아니....",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013408.1.22",
          "form": "암튼 거기바로옆이야",
          "original_form": "암튼 거기바로옆이야",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013408.1.23",
          "form": "거기도 건물이더라구",
          "original_form": "거기도 건물이더라구",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013408.1.24",
          "form": "나도 전엔",
          "original_form": "나도 전엔",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013408.1.25",
          "form": "초코만 먹었어",
          "original_form": "초코만 먹었어",
          "speaker_id": "1",
          "time": "20210825 14:29"
        }
      ]
    }
