      "utterance": [
        {
          "id": "MDRW2100003351.1.1",
          "form": "건강하시죠",
          "original_form": "건강하시죠",
          "speaker_id": "2",
          "time": "20210820 22:05"
        },
        {
          "id": "MDRW2100003351.1.2",
          "form": "네네 건강한편인데 살이 쪄서 문제네요",
          "original_form": "네네 건강한편인데 살이 쪄서 문제네요",
          "speaker_id": "1",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003351.1.3",
          "form": "운동 하시는거 있나요?",
          "original_form": "운동 하시는거 있나요?",
          "speaker_id": "1",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003351.1.4",
          "form": "네네 집엣서 홈트합니다",
          "original_form": "네네 집엣서 홈트합니다",
          "speaker_id": "2",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003351.1.5",
          "form": "홈트하면 주로 무슨운동해요? 스쿼트?",
          "original_form": "홈트하면 주로 무슨운동해요? 스쿼트?",
          "speaker_id": "1",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003351.1.6",
          "form": "아니요 기구삿어요",
          "original_form": "아니요 기구삿어요",
          "speaker_id": "2",
          "time": "20210820 22:12"
        },
        {
          "id": "MDRW2100003351.1.7",
          "form": "오 운동좋아하시나보네요 기구도 살정도면",
          "original_form": "오 운동좋아하시나보네요 기구도 살정도면",
          "speaker_id": "1",
          "time": "20210820 22:14"
        },
        {
          "id": "MDRW2100003351.1.8",
          "form": "한운동합니다ㅎㅎㅎ",
          "original_form": "한운동합니다ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 22:14"
        },
        {
          "id": "MDRW2100003351.1.9",
          "form": "건강도잡고 일석이조죠",
          "original_form": "건강도잡고 일석이조죠",
          "speaker_id": "2",
          "time": "20210820 22:14"
        },
        {
          "id": "MDRW2100003351.1.10",
          "form": "무슨운동하세요",
          "original_form": "무슨운동하세요",
          "speaker_id": "2",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003351.1.11",
          "form": "코로나전엔 필라테스도 하고 줌바도하고",
          "original_form": "코로나전엔 필라테스도 하고 줌바도하고",
          "speaker_id": "1",
          "time": "20210820 22:16"
        },
        {
          "id": "MDRW2100003351.1.12",
          "form": "요즘은 숨쉬기운동만해요ㅎ",
          "original_form": "요즘은 숨쉬기운동만해요ㅎ",
          "speaker_id": "1",
          "time": "20210820 22:16"
        },
        {
          "id": "MDRW2100003351.1.13",
          "form": "오오필라테스 줌바",
          "original_form": "오오필라테스 줌바",
          "speaker_id": "2",
          "time": "20210820 22:25"
        },
        {
          "id": "MDRW2100003351.1.14",
          "form": "줌바는뭐에요",
          "original_form": "줌바는뭐에요",
          "speaker_id": "2",
          "time": "20210820 22:25"
        },
        {
          "id": "MDRW2100003351.1.15",
          "form": "줌바댄스요ㅎ 라틴 레게 이런음악들으면서 춤이라해야되나",
          "original_form": "줌바댄스요ㅎ 라틴 레게 이런음악들으면서 춤이라해야되나",
          "speaker_id": "1",
          "time": "20210820 22:26"
        },
        {
          "id": "MDRW2100003351.1.16",
          "form": "율동하는거있어요",
          "original_form": "율동하는거있어요",
          "speaker_id": "1",
          "time": "20210820 22:26"
        },
        {
          "id": "MDRW2100003351.1.17",
          "form": "에어로빅이랑은 또달라요",
          "original_form": "에어로빅이랑은 또달라요",
          "speaker_id": "1",
          "time": "20210820 22:26"
        },
        {
          "id": "MDRW2100003351.1.18",
          "form": "우와라틴좋은데",
          "original_form": "우와라틴좋은데",
          "speaker_id": "2",
          "time": "20210820 22:28"
        },
        {
          "id": "MDRW2100003351.1.19",
          "form": "열정의라틴",
          "original_form": "열정의라틴",
          "speaker_id": "2",
          "time": "20210820 22:28"
        },
        {
          "id": "MDRW2100003351.1.20",
          "form": "함도전해보세요ㅎ 주로 여자분들이 배우긴하는데",
          "original_form": "함도전해보세요ㅎ 주로 여자분들이 배우긴하는데",
          "speaker_id": "1",
          "time": "20210820 22:30"
        },
        {
          "id": "MDRW2100003351.1.21",
          "form": "남자분이신가요?",
          "original_form": "남자분이신가요?",
          "speaker_id": "1",
          "time": "20210820 22:30"
        },
        {
          "id": "MDRW2100003351.1.22",
          "form": "네네 남자에요",
          "original_form": "네네 남자에요",
          "speaker_id": "2",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003351.1.23",
          "form": "재밌겠다ㅎㅎㅎ",
          "original_form": "재밌겠다ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003351.1.24",
          "form": "스피닝도 재밌어요",
          "original_form": "스피닝도 재밌어요",
          "speaker_id": "1",
          "time": "20210820 22:32"
        }
      ]
    }
