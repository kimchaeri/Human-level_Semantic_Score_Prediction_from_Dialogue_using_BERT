      "utterance": [
        {
          "id": "MDRW2100003595.1.1",
          "form": "새벽3시넘었는데 안주무시나요?",
          "original_form": "새벽3시넘었는데 안주무시나요?",
          "speaker_id": "1",
          "time": "20210821 03:21"
        },
        {
          "id": "MDRW2100003595.1.2",
          "form": "앗 저 어제 늦게 일어나서 밤샐거 같아요ㅋㅋㅋ",
          "original_form": "앗 저 어제 늦게 일어나서 밤샐거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 04:55"
        },
        {
          "id": "MDRW2100003595.1.3",
          "form": "밤새신거예요? 전 톡보내고 잠들었네요",
          "original_form": "밤새신거예요? 전 톡보내고 잠들었네요",
          "speaker_id": "1",
          "time": "20210821 12:20"
        },
        {
          "id": "MDRW2100003595.1.4",
          "form": "네 밤새고 아침에 잠들었어요!",
          "original_form": "네 밤새고 아침에 잠들었어요!",
          "speaker_id": "2",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100003595.1.5",
          "form": "늦게 주무셨나봐요  전 인나서 티비시청중이요",
          "original_form": "늦게 주무셨나봐요  전 인나서 티비시청중이요",
          "speaker_id": "1",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100003595.1.6",
          "form": "오 어떤거 보셨어요?",
          "original_form": "오 어떤거 보셨어요?",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100003595.1.7",
          "form": "지금 알고있지만 최종회래서 보고있어요",
          "original_form": "지금 알고있지만 최종회래서 보고있어요",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100003595.1.8",
          "form": "앗 오늘 최종회인가요?",
          "original_form": "앗 오늘 최종회인가요?",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100003595.1.9",
          "form": "비주얼합 되게 좋더라구요",
          "original_form": "비주얼합 되게 좋더라구요",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100003595.1.10",
          "form": "송강 첨보는데 매력있네요",
          "original_form": "송강 첨보는데 매력있네요",
          "speaker_id": "1",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100003595.1.11",
          "form": "즐겨보는 프로그램있어요?",
          "original_form": "즐겨보는 프로그램있어요?",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100003595.1.12",
          "form": "맞아요 송강 잘생겼죠",
          "original_form": "맞아요 송강 잘생겼죠",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100003595.1.13",
          "form": "저는 최근에 경찰수업 봐요!",
          "original_form": "저는 최근에 경찰수업 봐요!",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100003595.1.14",
          "form": "kbs에서 하는거죠?",
          "original_form": "kbs에서 하는거죠?",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100003595.1.15",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100003595.1.16",
          "form": "시작한지 얼마 안됐더라구요",
          "original_form": "시작한지 얼마 안됐더라구요",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100003595.1.17",
          "form": "누가 주인공이예요?",
          "original_form": "누가 주인공이예요?",
          "speaker_id": "1",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100003595.1.18",
          "form": "얼굴은 아는데 이름이 생각안나가지고ㅎ",
          "original_form": "얼굴은 아는데 이름이 생각안나가지고ㅎ",
          "speaker_id": "1",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100003595.1.19",
          "form": "진영이 주인공인 것 같아요!",
          "original_form": "진영이 주인공인 것 같아요!",
          "speaker_id": "2",
          "time": "20210821 23:21"
        }
      ]
