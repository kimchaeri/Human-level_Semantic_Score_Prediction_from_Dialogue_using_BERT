      "utterance": [
        {
          "id": "MDRW2100001417.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211015 16:09"
        },
        {
          "id": "MDRW2100001417.1.2",
          "form": "안녕하세요~ 동물 좋아하시나요",
          "original_form": "안녕하세요~ 동물 좋아하시나요",
          "speaker_id": "2",
          "time": "20211015 16:10"
        },
        {
          "id": "MDRW2100001417.1.3",
          "form": "동물 좋아하긴 좋아하는데",
          "original_form": "동물 좋아하긴 좋아하는데",
          "speaker_id": "1",
          "time": "20211015 16:16"
        },
        {
          "id": "MDRW2100001417.1.4",
          "form": "어케아셨죠?",
          "original_form": "어케아셨죠?",
          "speaker_id": "1",
          "time": "20211015 16:16"
        },
        {
          "id": "MDRW2100001417.1.5",
          "form": "어떤 동물 좋아하세요?",
          "original_form": "어떤 동물 좋아하세요?",
          "speaker_id": "2",
          "time": "20211015 16:17"
        },
        {
          "id": "MDRW2100001417.1.6",
          "form": "저는 네발동물 다 좋아해요",
          "original_form": "저는 네발동물 다 좋아해요",
          "speaker_id": "2",
          "time": "20211015 16:17"
        },
        {
          "id": "MDRW2100001417.1.7",
          "form": "강아지 호랑이 같은거",
          "original_form": "강아지 호랑이 같은거",
          "speaker_id": "1",
          "time": "20211015 16:18"
        },
        {
          "id": "MDRW2100001417.1.8",
          "form": "좋아합니다",
          "original_form": "좋아합니다",
          "speaker_id": "1",
          "time": "20211015 16:18"
        },
        {
          "id": "MDRW2100001417.1.9",
          "form": "ㅋㅋ고양이는 어떠세요?",
          "original_form": "ㅋㅋ고양이는 어떠세요?",
          "speaker_id": "2",
          "time": "20211015 16:19"
        },
        {
          "id": "MDRW2100001417.1.10",
          "form": "고양이는 케바케에요",
          "original_form": "고양이는 케바케에요",
          "speaker_id": "1",
          "time": "20211015 16:20"
        },
        {
          "id": "MDRW2100001417.1.11",
          "form": "제 맘에드는 애들은 괜찮은데",
          "original_form": "제 맘에드는 애들은 괜찮은데",
          "speaker_id": "1",
          "time": "20211015 16:20"
        },
        {
          "id": "MDRW2100001417.1.12",
          "form": "아닌애들은 좀...",
          "original_form": "아닌애들은 좀...",
          "speaker_id": "1",
          "time": "20211015 16:20"
        },
        {
          "id": "MDRW2100001417.1.13",
          "form": "저는 알러지가 있어서 직접 키우지는 못하고",
          "original_form": "저는 알러지가 있어서 직접 키우지는 못하고",
          "speaker_id": "2",
          "time": "20211015 16:21"
        },
        {
          "id": "MDRW2100001417.1.14",
          "form": "고양이는 다들 생긴게 귀엽더라구요",
          "original_form": "고양이는 다들 생긴게 귀엽더라구요",
          "speaker_id": "2",
          "time": "20211015 16:21"
        },
        {
          "id": "MDRW2100001417.1.15",
          "form": "저도 키우지는 않습니다",
          "original_form": "저도 키우지는 않습니다",
          "speaker_id": "1",
          "time": "20211015 16:22"
        },
        {
          "id": "MDRW2100001417.1.16",
          "form": "유튜브로 볼 뿐이죠",
          "original_form": "유튜브로 볼 뿐이죠",
          "speaker_id": "1",
          "time": "20211015 16:22"
        },
        {
          "id": "MDRW2100001417.1.17",
          "form": "요즘 동물 영상 올리는 유튜버가 많아서 좋아요",
          "original_form": "요즘 동물 영상 올리는 유튜버가 많아서 좋아요",
          "speaker_id": "2",
          "time": "20211015 16:23"
        },
        {
          "id": "MDRW2100001417.1.18",
          "form": "다양한 종을 볼 수도 잇고",
          "original_form": "다양한 종을 볼 수도 잇고",
          "speaker_id": "2",
          "time": "20211015 16:23"
        },
        {
          "id": "MDRW2100001417.1.19",
          "form": "네 그죠",
          "original_form": "네 그죠",
          "speaker_id": "1",
          "time": "20211015 16:25"
        },
        {
          "id": "MDRW2100001417.1.20",
          "form": "시대가 바뀌었죠",
          "original_form": "시대가 바뀌었죠",
          "speaker_id": "1",
          "time": "20211015 16:25"
        },
        {
          "id": "MDRW2100001417.1.21",
          "form": "어쨌든 네발달린 모든 동물들은 다 귀여워요",
          "original_form": "어쨌든 네발달린 모든 동물들은 다 귀여워요",
          "speaker_id": "2",
          "time": "20211015 16:26"
        }
      ]
