      "utterance": [
        {
          "id": "MDRW2100043797.1.1",
          "form": "세상에 그 학교도 노인컴 학대 하는군요",
          "original_form": "세상에 그 학교도 노인컴 학대 하는군요",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043797.1.2",
          "form": "윈도우 업뎃도 안해요. 애초에 인터넷 연결이 안됐지만",
          "original_form": "윈도우 업뎃도 안해요. 애초에 인터넷 연결이 안됐지만",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043797.1.3",
          "form": "아마 아직도 xp일듯",
          "original_form": "아마 아직도 xp일듯",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043797.1.4",
          "form": "와....작동된다는게 신기할 지경이에요",
          "original_form": "와....작동된다는게 신기할 지경이에요",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043797.1.5",
          "form": "ㅎㅎㅎ 작동은 됩디다",
          "original_form": "ㅎㅎㅎ 작동은 됩디다",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043797.1.6",
          "form": "그러니까 안 바꿔주는 걸까요....",
          "original_form": "그러니까 안 바꿔주는 걸까요....",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043797.1.7",
          "form": "아 그르네. 이거 내가 고이 보내줘야하는건가....",
          "original_form": "아 그르네. 이거 내가 고이 보내줘야하는건가....",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043797.1.8",
          "form": "컴퓨터 과실치사 사건이 벌어지나요ㅋㅋㅋ",
          "original_form": "컴퓨터 과실치사 사건이 벌어지나요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043797.1.9",
          "form": "ㅋㅋㅋㅋ R.I.P 컴퓨터....",
          "original_form": "ㅋㅋㅋㅋ R.I.P 컴퓨터....",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043797.1.10",
          "form": "과실치사?가 일어나기전에 좀 멀쩡한걸로 바꿔주면 좋을텐데요",
          "original_form": "과실치사?가 일어나기전에 좀 멀쩡한걸로 바꿔주면 좋을텐데요",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043797.1.11",
          "form": "바이러스 안먹은걸로....",
          "original_form": "바이러스 안먹은걸로....",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043797.1.12",
          "form": "그러게요 등록금이 다 어디로 가는지...",
          "original_form": "그러게요 등록금이 다 어디로 가는지...",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043797.1.13",
          "form": "하 진짜 등록금",
          "original_form": "하 진짜 등록금",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043797.1.14",
          "form": "저야 이제 안낸다지만 진심 내가 생각해도 아까워요",
          "original_form": "저야 이제 안낸다지만 진심 내가 생각해도 아까워요",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043797.1.15",
          "form": "최소한 좀 덜받으란 말이야ㅠㅠ",
          "original_form": "최소한 좀 덜받으란 말이야ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043797.1.16",
          "form": "그니까요ㅠㅠ 4년 다녔지만 그 동안 비커 한 번 교체 됐네요",
          "original_form": "그니까요ㅠㅠ 4년 다녔지만 그 동안 비커 한 번 교체 됐네요",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100043797.1.17",
          "form": "코로나로 실험수업도 비대면인데 등록금 변화 거의 없는거 걍 사깁니다. 사기 ㅠㅠ",
          "original_form": "코로나로 실험수업도 비대면인데 등록금 변화 거의 없는거 걍 사깁니다. 사기 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043797.1.18",
          "form": "덜 받네 어쩌네 하더니 다 받았나봐요ㅜㅜ 진짜 너무하다",
          "original_form": "덜 받네 어쩌네 하더니 다 받았나봐요ㅜㅜ 진짜 너무하다",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100043797.1.19",
          "form": "후배들한테 물어보니 딱히 적게 받진 않았더라구요ㅠ",
