      "utterance": [
        {
          "id": "MDRW2100040260.1.1",
          "form": "아진짜요? ㅋㅋㅋㅋㅋ",
          "original_form": "아진짜요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040260.1.2",
          "form": "넵 다 성공하지는 못하더라구요ㅠㅠ",
          "original_form": "넵 다 성공하지는 못하더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040260.1.3",
          "form": "하긴,,,,",
          "original_form": "하긴,,,,",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040260.1.4",
          "form": "가족이랑 같이사세요?",
          "original_form": "가족이랑 같이사세요?",
          "speaker_id": "2",
          "time": "20210904 15:13"
        },
        {
          "id": "MDRW2100040260.1.5",
          "form": "네네! 저는 부모님과 함께 지내요 ㅎㅎ",
          "original_form": "네네! 저는 부모님과 함께 지내요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:13"
        },
        {
          "id": "MDRW2100040260.1.6",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210904 15:13"
        },
        {
          "id": "MDRW2100040260.1.7",
          "form": "저도 같이살아요ㅋㅋㅋ 근데 슬슬 독립할까싶기도 하구요",
          "original_form": "저도 같이살아요ㅋㅋㅋ 근데 슬슬 독립할까싶기도 하구요",
          "speaker_id": "2",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100040260.1.8",
          "form": "그쵸... 독립을 해야 비로소",
          "original_form": "그쵸... 독립을 해야 비로소",
          "speaker_id": "1",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100040260.1.9",
          "form": "좀 어려운 자식이 되는 느낌이에요",
          "original_form": "좀 어려운 자식이 되는 느낌이에요",
          "speaker_id": "1",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100040260.1.10",
          "form": "근데 독립하면 돈이 안모일 것 같아서 그게 좀 걱정이에요...",
          "original_form": "근데 독립하면 돈이 안모일 것 같아서 그게 좀 걱정이에요...",
          "speaker_id": "1",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100040260.1.11",
          "form": "맞아요ㅋㅋ 목돈 모아서 전세로 가는게그나마 괜찮을거같아요",
          "original_form": "맞아요ㅋㅋ 목돈 모아서 전세로 가는게그나마 괜찮을거같아요",
          "speaker_id": "2",
          "time": "20210904 15:15"
        },
        {
          "id": "MDRW2100040260.1.12",
          "form": "맞아요... 하 근데 저 어제",
          "original_form": "맞아요... 하 근데 저 어제",
          "speaker_id": "1",
          "time": "20210904 15:15"
        },
        {
          "id": "MDRW2100040260.1.13",
          "form": "전세 사기 당한 사람 이야기 들어가지고 놀랐어요",
          "original_form": "전세 사기 당한 사람 이야기 들어가지고 놀랐어요",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040260.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ 아니 세상이 너무 지뢰밭이에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 아니 세상이 너무 지뢰밭이에요",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040260.1.15",
          "form": "만약 집주인이 탈세자면",
          "original_form": "만약 집주인이 탈세자면",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040260.1.16",
          "form": "앗 진짜요.,",
          "original_form": "앗 진짜요.,",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040260.1.17",
          "form": "전세금",
          "original_form": "전세금",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040260.1.18",
          "form": "무섭네요 이런",
          "original_form": "무섭네요 이런",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040260.1.19",
          "form": "못돌려받을 수도 있대요",
          "original_form": "못돌려받을 수도 있대요",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040260.1.20",
          "form": "목돈인데 머선일이고",
          "original_form": "목돈인데 머선일이고",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040260.1.21",
          "form": "법적으로 그렇대요...",
          "original_form": "법적으로 그렇대요...",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040260.1.22",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040260.1.23",
          "form": "주변에 그런분이있어요?",
          "original_form": "주변에 그런분이있어요?",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040260.1.24",
          "form": "그게 나라에서도",
          "original_form": "그게 나라에서도",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040260.1.25",
          "form": "아 제 주변인은 아니고",
          "original_form": "아 제 주변인은 아니고",
          "speaker_id": "1",
          "time": "20210904 15:16"
        }
      ]
    }
  ]
