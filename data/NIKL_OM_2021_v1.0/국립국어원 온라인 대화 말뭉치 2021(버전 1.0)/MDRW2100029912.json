          "speaker_id": "1",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100029912.1.4",
          "form": "징그러워서못봄",
          "original_form": "징그러워서못봄",
          "speaker_id": "1",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100029912.1.5",
          "form": "보다보면 재밌는데",
          "original_form": "보다보면 재밌는데",
          "speaker_id": "2",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029912.1.6",
          "form": "자기들만의 생활방식이 있어가지구",
          "original_form": "자기들만의 생활방식이 있어가지구",
          "speaker_id": "2",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029912.1.7",
          "form": "젤재밋는애가 누구?",
          "original_form": "젤재밋는애가 누구?",
          "speaker_id": "1",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029912.1.8",
          "form": "그런게 어딨어요",
          "original_form": "그런게 어딨어요",
          "speaker_id": "2",
          "time": "20210903 16:29"
        },
        {
          "id": "MDRW2100029912.1.9",
          "form": "그냥 아 얜 이렇게 사는구나",
          "original_form": "그냥 아 얜 이렇게 사는구나",
          "speaker_id": "2",
          "time": "20210903 16:29"
        },
        {
          "id": "MDRW2100029912.1.10",
          "form": "이래서 저러는구나",
          "original_form": "이래서 저러는구나",
          "speaker_id": "2",
          "time": "20210903 16:29"
        },
        {
          "id": "MDRW2100029912.1.11",
          "form": "하는거지",
          "original_form": "하는거지",
          "speaker_id": "2",
          "time": "20210903 16:29"
        },
        {
          "id": "MDRW2100029912.1.12",
          "form": "요샌 벌이요",
          "original_form": "요샌 벌이요",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100029912.1.13",
          "form": "꿀벌은 사회집단이더라구요?",
          "original_form": "꿀벌은 사회집단이더라구요?",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100029912.1.14",
          "form": "여왕도 하나의 직업임",
          "original_form": "여왕도 하나의 직업임",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100029912.1.15",
          "form": "왕이 아님",
          "original_form": "왕이 아님",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100029912.1.16",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100029912.1.17",
          "form": "하긴 숨어지내려면그것도힘들듯",
          "original_form": "하긴 숨어지내려면그것도힘들듯",
          "speaker_id": "1",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100029912.1.18",
          "form": "유튜브 봐요?",
          "original_form": "유튜브 봐요?",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100029912.1.19",
          "form": "잘안봐요",
          "original_form": "잘안봐요",
          "speaker_id": "1",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100029912.1.20",
          "form": "저도 잘 안보는데",
          "original_form": "저도 잘 안보는데",
          "speaker_id": "2",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100029912.1.21",
          "form": "프응 이라는 양봉업자 영상 자꾸봄ㅋㅋㅋ",
          "original_form": "프응 이라는 양봉업자 영상 자꾸봄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100029912.1.22",
          "form": "재밌게 설명 잘해서ㅋㅋ",
          "original_form": "재밌게 설명 잘해서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100029912.1.23",
          "form": "지네 이런건 싫긴합니다",
          "original_form": "지네 이런건 싫긴합니다",
          "speaker_id": "2",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100029912.1.24",
          "form": "양봉업자 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "양봉업자 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029912.1.25",
          "form": "별걸다보시네여",
          "original_form": "별걸다보시네여",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029912.1.26",
          "form": "갑자기 떠서 하나 봤는데",
          "original_form": "갑자기 떠서 하나 봤는데",
          "speaker_id": "2",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029912.1.27",
          "form": "재밌드라구요?",
          "original_form": "재밌드라구요?",
          "speaker_id": "2",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029912.1.28",
          "form": "벌 원래 좋아하지 않았는데",
          "original_form": "벌 원래 좋아하지 않았는데",
          "speaker_id": "2",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029912.1.29",
          "form": "커여움",
          "original_form": "커여움",
          "speaker_id": "2",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029912.1.30",
          "form": "ㅠㅠ아님",
          "original_form": "ㅠㅠ아님",
          "speaker_id": "1",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100029912.1.31",
          "form": "곤충들중에 귀여운거 1도없음",
          "original_form": "곤충들중에 귀여운거 1도없음",
          "speaker_id": "1",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100029912.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100029912.1.33",
          "form": "강아지풀로 놀려주고싶다",
          "original_form": "강아지풀로 놀려주고싶다",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100029912.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100029912.1.35",
          "form": "왜요 거기 벌레천국이에여?",
          "original_form": "왜요 거기 벌레천국이에여?",
          "speaker_id": "1",
          "time": "20210903 16:46"
        },
        {
          "id": "MDRW2100029912.1.36",
          "form": "강아지풀..?",
          "original_form": "강아지풀..?",
          "speaker_id": "1",
          "time": "20210903 16:46"
        },
        {
          "id": "MDRW2100029912.1.37",
          "form": "아뇨 어릴때 강아지풀 뜯어다가",
          "original_form": "아뇨 어릴때 강아지풀 뜯어다가",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029912.1.38",
          "form": "꺅 벌레! 이러면서 애들한테 주지않나요",
          "original_form": "꺅 벌레! 이러면서 애들한테 주지않나요",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029912.1.39",
          "form": "아닌데요",
          "original_form": "아닌데요",
          "speaker_id": "1",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100029912.1.40",
          "form": "모야 노잼",
          "original_form": "모야 노잼",
          "speaker_id": "1",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100029912.1.41",
          "form": "슬프네 세대차이",
          "original_form": "슬프네 세대차이",
          "speaker_id": "2",
          "time": "20210903 16:49"
