      "utterance": [
        {
          "id": "MDRW2100008917.1.1",
          "form": "애들 내일 개학이네...",
          "original_form": "애들 내일 개학이네...",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100008917.1.2",
          "form": "시간 참 빠르다. 한 달 금방가네 ㅎㅎ",
          "original_form": "시간 참 빠르다. 한 달 금방가네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100008917.1.3",
          "form": "개학 준비는 다 했지?",
          "original_form": "개학 준비는 다 했지?",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100008917.1.4",
          "form": "준비랄게 뭐 있나 ㅎㅎ 그냥 내일 수업할 거 책 준비하는 거지",
          "original_form": "준비랄게 뭐 있나 ㅎㅎ 그냥 내일 수업할 거 책 준비하는 거지",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100008917.1.5",
          "form": "방학 숙제는 다 했고?",
          "original_form": "방학 숙제는 다 했고?",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100008917.1.6",
          "form": "ㅇㅇ 그냥 기본만 딱 했어",
          "original_form": "ㅇㅇ 그냥 기본만 딱 했어",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100008917.1.7",
          "form": "독서기록장도 주 1회 ㅎㅎ",
          "original_form": "독서기록장도 주 1회 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100008917.1.8",
          "form": "ㅎㅎ 아주 간소하게 했네",
          "original_form": "ㅎㅎ 아주 간소하게 했네",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100008917.1.9",
          "form": "2학기 예습하느라 은근 시간이 없어서",
          "original_form": "2학기 예습하느라 은근 시간이 없어서",
          "speaker_id": "1",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100008917.1.10",
          "form": "그래도 계획표대로 아주 알차게 보냈네",
          "original_form": "그래도 계획표대로 아주 알차게 보냈네",
          "speaker_id": "2",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100008917.1.11",
          "form": "ㅇㅇ 애들 진짜 열심히 했어. 미안할 정도로",
          "original_form": "ㅇㅇ 애들 진짜 열심히 했어. 미안할 정도로",
          "speaker_id": "1",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100008917.1.12",
          "form": "지금 열심히 해야 나중에 고생 덜하지",
          "original_form": "지금 열심히 해야 나중에 고생 덜하지",
          "speaker_id": "2",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100008917.1.13",
          "form": "ㅎㅎ 애들은 아직 그런 거 잘 모르니까",
          "original_form": "ㅎㅎ 애들은 아직 그런 거 잘 모르니까",
          "speaker_id": "1",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100008917.1.14",
          "form": "새학기 책 받아오려면 힘들겠네",
          "original_form": "새학기 책 받아오려면 힘들겠네",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100008917.1.15",
          "form": "금요일에나 준다는 거 같아. 이번주까지는 1학기니",
          "original_form": "금요일에나 준다는 거 같아. 이번주까지는 1학기니",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100008917.1.16",
          "form": "학기가 이상하게 끊겼네 ㅎㅎ",
          "original_form": "학기가 이상하게 끊겼네 ㅎㅎ",
