      "utterance": [
        {
          "id": "MDRW2100006361.1.1",
          "form": "동물 키우시나요",
          "original_form": "동물 키우시나요",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100006361.1.2",
          "form": "아니요..name1님은요",
          "original_form": "아니요..&name1&님은요",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100006361.1.3",
          "form": "아니요...키우고싶은건요?",
          "original_form": "아니요...키우고싶은건요?",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100006361.1.4",
          "form": "없습니다..name1님은요",
          "original_form": "없습니다..&name1&님은요",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100006361.1.5",
          "form": "저 강아지요",
          "original_form": "저 강아지요",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006361.1.6",
          "form": "동물 안조아하세요,?",
          "original_form": "동물 안조아하세요,?",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006361.1.7",
          "form": "네..안좋아합니다..",
          "original_form": "네..안좋아합니다..",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100006361.1.8",
          "form": "ㅠㅠㅠ왜용",
          "original_form": "ㅠㅠㅠ왜용",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006361.1.9",
          "form": "안귀여워요,?",
          "original_form": "안귀여워요,?",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006361.1.10",
          "form": "글쎄요 경험이.없어서 그런지.눈으론 예쁜데.키우고 싶다는 생각은 안드네요 비염도 있구요",
          "original_form": "글쎄요 경험이.없어서 그런지.눈으론 예쁜데.키우고 싶다는 생각은 안드네요 비염도 있구요",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100006361.1.11",
          "form": "아항 ㅜㅜ그건 어렵죠",
          "original_form": "아항 ㅜㅜ그건 어렵죠",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006361.1.12",
          "form": "전 맘만 잇어요",
          "original_form": "전 맘만 잇어요",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006361.1.13",
          "form": "그렇죠 책임질일이 많죠",
          "original_form": "그렇죠 책임질일이 많죠",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100006361.1.14",
          "form": "ㅋㅋ 가족중 키우고싶어하는 구성원은 없나여",
          "original_form": "ㅋㅋ 가족중 키우고싶어하는 구성원은 없나여",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006361.1.15",
          "form": "3살 둘째요?한두번 이야기했어요..ㅎㅎ 지금은 안하네요",
          "original_form": "3살 둘째요?한두번 이야기했어요..ㅎㅎ 지금은 안하네요",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006361.1.16",
          "form": "ㅋㅋㅋ 관심 없어진건가요",
          "original_form": "ㅋㅋㅋ 관심 없어진건가요",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006361.1.17",
          "form": "네 책에 강아지가 나오니 비슷한 말로 키우자고 하더라구요 ㅎㅎ",
          "original_form": "네 책에 강아지가 나오니 비슷한 말로 키우자고 하더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100006361.1.18",
          "form": "아항 키우고싶엇겟네요",
          "original_form": "아항 키우고싶엇겟네요",
