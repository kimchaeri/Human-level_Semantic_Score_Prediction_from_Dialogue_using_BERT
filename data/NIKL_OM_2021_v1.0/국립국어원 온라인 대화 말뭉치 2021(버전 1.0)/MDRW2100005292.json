      "utterance": [
        {
          "id": "MDRW2100005292.1.1",
          "form": "저녁 뭐 드셨어요?",
          "original_form": "저녁 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005292.1.2",
          "form": "저는 미역국 먹었어요",
          "original_form": "저는 미역국 먹었어요",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005292.1.3",
          "form": "저는 김치랑 닭가슴살을 반찬으로 먹었어요",
          "original_form": "저는 김치랑 닭가슴살을 반찬으로 먹었어요",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005292.1.4",
          "form": "닭가슴살은 따로 조리해서 먹나요?",
          "original_form": "닭가슴살은 따로 조리해서 먹나요?",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005292.1.5",
          "form": "아니면 그냥 삶기만 해서 먹는건가요?",
          "original_form": "아니면 그냥 삶기만 해서 먹는건가요?",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005292.1.6",
          "form": "요즘 잘 나와있어서 냉동된거 개별 포장으로 나와요",
          "original_form": "요즘 잘 나와있어서 냉동된거 개별 포장으로 나와요",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005292.1.7",
          "form": "그래서 해동해서 데워먹어요",
          "original_form": "그래서 해동해서 데워먹어요",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005292.1.8",
          "form": "그런 건 맛이 어때요? 파는 건 봤는데 먹어본 적은 없어서",
          "original_form": "그런 건 맛이 어때요? 파는 건 봤는데 먹어본 적은 없어서",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005292.1.9",
          "form": "맛도 다양하던데 기본만 드시나요?",
          "original_form": "맛도 다양하던데 기본만 드시나요?",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005292.1.10",
          "form": "넹 저는 기본만 먹었어요",
          "original_form": "넹 저는 기본만 먹었어요",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005292.1.11",
          "form": "진짜 딱 고기만 먹는 느낌으로 먹어서",
          "original_form": "진짜 딱 고기만 먹는 느낌으로 먹어서",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005292.1.12",
          "form": "오히려 저는 맛 첨가 된게 부담스럽거든요",
          "original_form": "오히려 저는 맛 첨가 된게 부담스럽거든요",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005292.1.13",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005292.1.14",
          "form": "저도 다음에 한 번 먹어봐야겠어요",
          "original_form": "저도 다음에 한 번 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005292.1.15",
          "form": "추천해주실 브랜드 혹시 있으신가요?",
          "original_form": "추천해주실 브랜드 혹시 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005292.1.16",
          "form": "종류가 무지 많더라구요",
          "original_form": "종류가 무지 많더라구요",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005292.1.17",
          "form": "처음엔 쿠팡 자체 브랜드를 샀는데 쿠팡 갑질논란 뜨고",
          "original_form": "처음엔 쿠팡 자체 브랜드를 샀는데 쿠팡 갑질논란 뜨고",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005292.1.18",
          "form": "쿠팡도 자체브랜드가 있구나..",
          "original_form": "쿠팡도 자체브랜드가 있구나..",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005292.1.19",
          "form": "어디로 갈아탔나요?",
          "original_form": "어디로 갈아탔나요?",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005292.1.20",
          "form": "모든닭이라는 브랜드로 갈아탔어요",
          "original_form": "모든닭이라는 브랜드로 갈아탔어요",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005292.1.21",
          "form": "스팀 오리지널 어쩌구  닭으로요",
          "original_form": "스팀 오리지널 어쩌구  닭으로요",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005292.1.22",
          "form": "모든닭? 처음 들어보는데 기회가 되면 구매해서 먹어봐야겠어요",
          "original_form": "모든닭? 처음 들어보는데 기회가 되면 구매해서 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005292.1.23",
          "form": "추천 고맙습니다",
          "original_form": "추천 고맙습니다",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005292.1.24",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005292.1.25",
          "form": "미역국은 직접 해드셨어요??",
          "original_form": "미역국은 직접 해드셨어요??",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005292.1.26",
          "form": "아니요 아빠가 끓여주셨어요",
          "original_form": "아니요 아빠가 끓여주셨어요",
