      "utterance": [
        {
          "id": "MDRW2100032004.1.1",
          "form": "오늘 뭐드실거에요~?",
          "original_form": "오늘 뭐드실거에요~?",
          "speaker_id": "1",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100032004.1.2",
          "form": "저 아직 안정했어요!",
          "original_form": "저 아직 안정했어요!",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100032004.1.3",
          "form": "있다 나가서 점심먹을껀데 메뉴추천 해주실수 있을까요?",
          "original_form": "있다 나가서 점심먹을껀데 메뉴추천 해주실수 있을까요?",
          "speaker_id": "2",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100032004.1.4",
          "form": "ㅋㅋㅋㅋ음",
          "original_form": "ㅋㅋㅋㅋ음",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100032004.1.5",
          "form": "오늘은 날이 좋으니",
          "original_form": "오늘은 날이 좋으니",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100032004.1.6",
          "form": "브런치 ?",
          "original_form": "브런치 ?",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100032004.1.7",
          "form": "샌드위치나",
          "original_form": "샌드위치나",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100032004.1.8",
          "form": "오!좋져!",
          "original_form": "오!좋져!",
          "speaker_id": "2",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100032004.1.9",
          "form": "케이크요 ㅎㅎ",
          "original_form": "케이크요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100032004.1.10",
          "form": "아메리카노랑!",
          "original_form": "아메리카노랑!",
          "speaker_id": "2",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100032004.1.11",
          "form": "커피 드세요?",
          "original_form": "커피 드세요?",
          "speaker_id": "2",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100032004.1.12",
          "form": "식사는아니지만 이런날엔 케익이 ㅎㅎ",
          "original_form": "식사는아니지만 이런날엔 케익이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100032004.1.13",
          "form": "아물론 비와도 케익을 시키지만...",
          "original_form": "아물론 비와도 케익을 시키지만...",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100032004.1.14",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100032004.1.15",
          "form": "네 ~~",
          "original_form": "네 ~~",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100032004.1.16",
          "form": "케이크 좋아요",
          "original_form": "케이크 좋아요",
          "speaker_id": "2",
          "time": "20210905 10:42"
        },
        {
          "id": "MDRW2100032004.1.17",
          "form": "단거랑 아메리카노랑 먹음",
          "original_form": "단거랑 아메리카노랑 먹음",
          "speaker_id": "2",
          "time": "20210905 10:42"
        },
        {
          "id": "MDRW2100032004.1.18",
          "form": "최고!!",
          "original_form": "최고!!",
          "speaker_id": "2",
          "time": "20210905 10:42"
        },
        {
          "id": "MDRW2100032004.1.19",
          "form": "ㅋㅋㅋ어디쪽으로 나가세요?",
          "original_form": "ㅋㅋㅋ어디쪽으로 나가세요?",
          "speaker_id": "1",
          "time": "20210905 10:44"
        },
        {
          "id": "MDRW2100032004.1.20",
          "form": "저 아마 그냥 집앞으로 나갈꺼같아요!ㅎ.ㅎ",
          "original_form": "저 아마 그냥 집앞으로 나갈꺼같아요!ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100032004.1.21",
          "form": "파주 예쁜카페 나가고싶은데",
          "original_form": "파주 예쁜카페 나가고싶은데",
          "speaker_id": "2",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100032004.1.22",
          "form": "차밀릴꺼같아서요!",
          "original_form": "차밀릴꺼같아서요!",
          "speaker_id": "2",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100032004.1.23",
          "form": "와 파주 진짜 가고싶어요",
          "original_form": "와 파주 진짜 가고싶어요",
          "speaker_id": "1",
          "time": "20210905 10:47"
        },
        {
          "id": "MDRW2100032004.1.24",
          "form": "ㅠㅠ드라이브가고싶네요",
          "original_form": "ㅠㅠ드라이브가고싶네요",
          "speaker_id": "1",
          "time": "20210905 10:47"
        },
        {
          "id": "MDRW2100032004.1.25",
          "form": "파주에 아늑이라는 카페가 있는데",
          "original_form": "파주에 아늑이라는 카페가 있는데",
          "speaker_id": "2",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100032004.1.26",
          "form": "거기 가보고싶거든요! 강이 보이는 뷰라서",
          "original_form": "거기 가보고싶거든요! 강이 보이는 뷰라서",
          "speaker_id": "2",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100032004.1.27",
