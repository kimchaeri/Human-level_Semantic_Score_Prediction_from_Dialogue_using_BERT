      "utterance": [
        {
          "id": "MDRW2100035116.1.1",
          "form": "필리핀 호주 대만가봤어요!",
          "original_form": "필리핀 호주 대만가봤어요!",
          "speaker_id": "2",
          "time": "20210829 03:05"
        },
        {
          "id": "MDRW2100035116.1.2",
          "form": "앗 대만!!",
          "original_form": "앗 대만!!",
          "speaker_id": "1",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100035116.1.3",
          "form": "전 안당해봣지만",
          "original_form": "전 안당해봣지만",
          "speaker_id": "2",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100035116.1.4",
          "form": "진짜 많은가봐요ㅠㅠ",
          "original_form": "진짜 많은가봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100035116.1.5",
          "form": "와 여행 많이 다니셨네요",
          "original_form": "와 여행 많이 다니셨네요",
          "speaker_id": "1",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100035116.1.6",
          "form": "어마 ㅋㅋㅋㅋㅋ나",
          "original_form": "어마 ㅋㅋㅋㅋㅋ나",
          "speaker_id": "2",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100035116.1.7",
          "form": "부러워요ㅠ",
          "original_form": "부러워요ㅠ",
          "speaker_id": "1",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100035116.1.8",
          "form": "대만아니고 태국이에요ㅠㅠㅋㅋㅋ",
          "original_form": "대만아니고 태국이에요ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100035116.1.9",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100035116.1.10",
          "form": "대만은 가보고싶은곳인데",
          "original_form": "대만은 가보고싶은곳인데",
          "speaker_id": "2",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100035116.1.11",
          "form": "튀어나왓네요 ㅋㅋㅋ",
          "original_form": "튀어나왓네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100035116.1.12",
          "form": "저희 엄마 태국 가셨을때 말린망고 잔뜻 사오셨는데 너무 맛있더라구요",
          "original_form": "저희 엄마 태국 가셨을때 말린망고 잔뜻 사오셨는데 너무 맛있더라구요",
          "speaker_id": "1",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100035116.1.13",
          "form": "저도 대만 가고싶어용ㅋㅋㅋ",
          "original_form": "저도 대만 가고싶어용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100035116.1.14",
          "form": "맞아여ㅠㅠ 필리핀망고보다 ㅋㅋㅋ태국망고가",
          "original_form": "맞아여ㅠㅠ 필리핀망고보다 ㅋㅋㅋ태국망고가",
          "speaker_id": "2",
          "time": "20210829 03:07"
        },
        {
          "id": "MDRW2100035116.1.15",
          "form": "더더 맛잇어요ㅠㅠ",
          "original_form": "더더 맛잇어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:07"
        },
        {
          "id": "MDRW2100035116.1.16",
          "form": "대만 음식도 잘맞데용 !!",
          "original_form": "대만 음식도 잘맞데용 !!",
          "speaker_id": "2",
          "time": "20210829 03:07"
        },
        {
          "id": "MDRW2100035116.1.17",
          "form": "대만 유명한 음식 보니까",
          "original_form": "대만 유명한 음식 보니까",
          "speaker_id": "1",
          "time": "20210829 03:07"
        },
        {
          "id": "MDRW2100035116.1.18",
          "form": "전원래 아시아음식도 좋아해서 고수도 잘먹어요 ㅋㅋ",
          "original_form": "전원래 아시아음식도 좋아해서 고수도 잘먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:07"
        },
        {
          "id": "MDRW2100035116.1.19",
          "form": "다 맛있어보이더라구요",
          "original_form": "다 맛있어보이더라구요",
          "speaker_id": "1",
          "time": "20210829 03:07"
        },
        {
          "id": "MDRW2100035116.1.20",
          "form": "엇 저두요!ㅋㅋㅋ",
          "original_form": "엇 저두요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:07"
        },
        {
          "id": "MDRW2100035116.1.21",
          "form": "쌀국수엔 고수ㅋㅋ",
          "original_form": "쌀국수엔 고수ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:07"
        },
        {
          "id": "MDRW2100035116.1.22",
          "form": "와!!!!역쉬",
          "original_form": "와!!!!역쉬",
          "speaker_id": "2",
          "time": "20210829 03:07"
        },
        {
          "id": "MDRW2100035116.1.23",
          "form": "아시는자!!먹고싶네요ㅠㅠ",
          "original_form": "아시는자!!먹고싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:07"
        },
        {
          "id": "MDRW2100035116.1.24",
          "form": "그러게요ㅠㅠ",
          "original_form": "그러게요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 03:08"
        },
        {
          "id": "MDRW2100035116.1.25",
          "form": "꼼양꿍도 드세요!?",
          "original_form": "꼼양꿍도 드세요!?",
          "speaker_id": "2",
          "time": "20210829 03:08"
        },
        {
          "id": "MDRW2100035116.1.26",
          "form": "고수 안들어가면 전 살짝 느끼하더라구용",
          "original_form": "고수 안들어가면 전 살짝 느끼하더라구용",
          "speaker_id": "1",
          "time": "20210829 03:08"
        },
        {
          "id": "MDRW2100035116.1.27",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210829 03:08"
        },
        {
          "id": "MDRW2100035116.1.28",
          "form": "한국에는 라면이 있듯이",
          "original_form": "한국에는 라면이 있듯이",
          "speaker_id": "1",
          "time": "20210829 03:08"
        },
        {
          "id": "MDRW2100035116.1.29",
          "form": "태국에도 똠양꿍식 라면이 있나봐요",
          "original_form": "태국에도 똠양꿍식 라면이 있나봐요",
          "speaker_id": "1",
          "time": "20210829 03:08"
        }
      ]
    }
