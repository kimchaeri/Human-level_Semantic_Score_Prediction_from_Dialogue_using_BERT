      "utterance": [
        {
          "id": "MDRW2100020650.1.1",
          "form": "안녕하세요 반려동물 키우시나요?",
          "original_form": "안녕하세요 반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020650.1.2",
          "form": "네 키우고있습니다",
          "original_form": "네 키우고있습니다",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020650.1.3",
          "form": "고양이랑 같이 살고있어요",
          "original_form": "고양이랑 같이 살고있어요",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020650.1.4",
          "form": "name1님은 반려동물 키우시나요?",
          "original_form": "&name1&님은 반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020650.1.5",
          "form": "저기요.. 드디어 만났네요. 저도 고양이랑 살아요~!~!~!~~!~!~!~!~!~!~!~!~!~!~!",
          "original_form": "저기요.. 드디어 만났네요. 저도 고양이랑 살아요~!~!~!~~!~!~!~!~!~!~!~!~!~!~!",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020650.1.6",
          "form": "헤헤 우리 집사네요",
          "original_form": "헤헤 우리 집사네요",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020650.1.7",
          "form": "오!!",
          "original_form": "오!!",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020650.1.8",
          "form": "고양이 집사!!",
          "original_form": "고양이 집사!!",
          "speaker_id": "2",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020650.1.9",
          "form": "반가워요",
          "original_form": "반가워요",
          "speaker_id": "2",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020650.1.10",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020650.1.11",
          "form": "지금 제 옆에서 그루밍하고 있어요ㅋㅋㅋ",
          "original_form": "지금 제 옆에서 그루밍하고 있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020650.1.12",
          "form": "ㅋㅋㅋㅋ 간식은 어떤걸 좋아하나요? 고양이님께서",
          "original_form": "ㅋㅋㅋㅋ 간식은 어떤걸 좋아하나요? 고양이님께서",
          "speaker_id": "1",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020650.1.13",
          "form": "우와.. 뭐죠? 우리 고양이는 어딨는지 모르겠네요.. 방 밖으로 나갔거든요",
          "original_form": "우와.. 뭐죠? 우리 고양이는 어딨는지 모르겠네요.. 방 밖으로 나갔거든요",
          "speaker_id": "1",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020650.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020650.1.15",
          "form": "간식은 아무거나 잘 먹는 편인데",
          "original_form": "간식은 아무거나 잘 먹는 편인데",
          "speaker_id": "2",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020650.1.16",
          "form": "이상하게 연어는 잘 안먹더라구요",
          "original_form": "이상하게 연어는 잘 안먹더라구요",
          "speaker_id": "2",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020650.1.17",
          "form": "추르보다",
          "original_form": "추르보다",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020650.1.18",
          "form": "육포느낌의 간식을 더 좋아해요",
          "original_form": "육포느낌의 간식을 더 좋아해요",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020650.1.19",
          "form": "다른 고양이들은 잘먹던데 입맛따라 다른가봐요",
          "original_form": "다른 고양이들은 잘먹던데 입맛따라 다른가봐요",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020650.1.20",
          "form": "오 정말요? 네 맞아요 고양이들 마다 입맛이 진짜 다르더라구요",
          "original_form": "오 정말요? 네 맞아요 고양이들 마다 입맛이 진짜 다르더라구요",
          "speaker_id": "1",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020650.1.21",
          "form": "우리 고양이는 닭가슴살을 제일 좋아해요!",
          "original_form": "우리 고양이는 닭가슴살을 제일 좋아해요!",
          "speaker_id": "1",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020650.1.22",
          "form": "아 혹시 고양이 턱드름 있나요?",
          "original_form": "아 혹시 고양이 턱드름 있나요?",
          "speaker_id": "1",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020650.1.23",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020650.1.24",
          "form": "예전에 살짝 있는것 같았는데 없더라구요",
          "original_form": "예전에 살짝 있는것 같았는데 없더라구요",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020650.1.25",
          "form": "닭가슴살 단백질 제대로 챙겨먹을줄 아는 고양이네요",
          "original_form": "닭가슴살 단백질 제대로 챙겨먹을줄 아는 고양이네요",
          "speaker_id": "2",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020650.1.26",
          "form": "ㅋㅋㅋㅋㅋ너무 웃기네요",
          "original_form": "ㅋㅋㅋㅋㅋ너무 웃기네요",
          "speaker_id": "1",
          "time": "20210828 13:22"
        },
        {
          "id": "MDRW2100020650.1.27",
          "form": "아 요새 턱드름 생겨서 식기 먼저 바꿔봤어요",
          "original_form": "아 요새 턱드름 생겨서 식기 먼저 바꿔봤어요",
          "speaker_id": "1",
          "time": "20210828 13:22"
        },
        {
          "id": "MDRW2100020650.1.28",
          "form": "사료도 얼마 남지 않아서 그냥 원래 먹이던거로 바꿀까 고민중이에요 ㅜㅠ",
          "original_form": "사료도 얼마 남지 않아서 그냥 원래 먹이던거로 바꿀까 고민중이에요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210828 13:23"
        },
        {
          "id": "MDRW2100020650.1.29",
          "form": "그렇군요 턱드름용 사료 먹는것도 좋죠",
          "original_form": "그렇군요 턱드름용 사료 먹는것도 좋죠",
          "speaker_id": "2",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100020650.1.30",
          "form": "관리를 직접 해주시는 분들도 있더라구요",
          "original_form": "관리를 직접 해주시는 분들도 있더라구요",
          "speaker_id": "2",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100020650.1.31",
          "form": "그쵸? 그렇게 해 보려고요",
          "original_form": "그쵸? 그렇게 해 보려고요",
          "speaker_id": "1",
          "time": "20210829 10:41"
        },
        {
          "id": "MDRW2100020650.1.32",
          "form": "오늘은 일어나보니 고양이가 옆에서 자고 있더군요 하하",
          "original_form": "오늘은 일어나보니 고양이가 옆에서 자고 있더군요 하하",
          "speaker_id": "1",
          "time": "20210829 10:41"
        },
        {
          "id": "MDRW2100020650.1.33",
          "form": "우리 고양이는 항상 발 이나 다리 사이에서 자요..\r\n건들지 말라고 그러나 ㅋㅋ",
          "original_form": "우리 고양이는 항상 발 이나 다리 사이에서 자요..\r\n건들지 말라고 그러나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:42"
        },
        {
          "id": "MDRW2100020650.1.34",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:43"
        },
        {
          "id": "MDRW2100020650.1.35",
          "form": "저희 고양이도요",
          "original_form": "저희 고양이도요",
          "speaker_id": "2",
          "time": "20210829 10:43"
        },
        {
          "id": "MDRW2100020650.1.36",
          "form": "자다가 뒤척이면 놀라고",
          "original_form": "자다가 뒤척이면 놀라고",
          "speaker_id": "2",
          "time": "20210829 10:44"
        },
        {
          "id": "MDRW2100020650.1.37",
          "form": "내가 미안해할게 아닌데 뻘쭘해요ㅋㅋㅋ",
          "original_form": "내가 미안해할게 아닌데 뻘쭘해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:44"
        },
        {
          "id": "MDRW2100020650.1.38",
          "form": "ㅋㅋㅋㅋㅋ그쵸",
          "original_form": "ㅋㅋㅋㅋㅋ그쵸",
          "speaker_id": "1",
          "time": "20210829 10:47"
        },
        {
          "id": "MDRW2100020650.1.39",
          "form": "캣휠 쓰시나요? 살까 말까 고민 중이거든요",
          "original_form": "캣휠 쓰시나요? 살까 말까 고민 중이거든요",
          "speaker_id": "1",
          "time": "20210829 10:47"
        },
        {
          "id": "MDRW2100020650.1.40",
          "form": "아뇨 캣휠 살까말까 고민하다가",
          "original_form": "아뇨 캣휠 살까말까 고민하다가",
          "speaker_id": "2",
          "time": "20210829 10:49"
        },
        {
          "id": "MDRW2100020650.1.41",
          "form": "결국 안샀어요",
          "original_form": "결국 안샀어요",
          "speaker_id": "2",
          "time": "20210829 10:49"
        },
        {
          "id": "MDRW2100020650.1.42",
          "form": "냥이가 나이가 좀 있는 편이라",
          "original_form": "냥이가 나이가 좀 있는 편이라",
          "speaker_id": "2",
          "time": "20210829 10:49"
        },
        {
          "id": "MDRW2100020650.1.43",
          "form": "캣휠도 쓰는 고양이는 잘 쓰는데",
          "original_form": "캣휠도 쓰는 고양이는 잘 쓰는데",
          "speaker_id": "2",
          "time": "20210829 10:49"
        },
        {
          "id": "MDRW2100020650.1.44",
          "form": "안쓰는 고양이는 안써서",
          "original_form": "안쓰는 고양이는 안써서",
          "speaker_id": "2",
