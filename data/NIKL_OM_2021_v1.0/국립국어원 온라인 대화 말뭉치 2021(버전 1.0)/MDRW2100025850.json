      "utterance": [
        {
          "id": "MDRW2100025850.1.1",
          "form": "군대 좋아요 군대 웹툰 즐겨봅니디",
          "original_form": "군대 좋아요 군대 웹툰 즐겨봅니디",
          "speaker_id": "1",
          "time": "20210901 17:41"
        },
        {
          "id": "MDRW2100025850.1.2",
          "form": "군대 좋아하십니까?",
          "original_form": "군대 좋아하십니까?",
          "speaker_id": "1",
          "time": "20210901 17:41"
        },
        {
          "id": "MDRW2100025850.1.3",
          "form": "군대 가보질 않았는데 힘들 것 같네요",
          "original_form": "군대 가보질 않았는데 힘들 것 같네요",
          "speaker_id": "2",
          "time": "20210901 17:41"
        },
        {
          "id": "MDRW2100025850.1.4",
          "form": "힘들겠죠 가고싶지는 않네요",
          "original_form": "힘들겠죠 가고싶지는 않네요",
          "speaker_id": "1",
          "time": "20210901 17:42"
        },
        {
          "id": "MDRW2100025850.1.5",
          "form": "군 생활이 남자등은 기억에 많이 남겠어요",
          "original_form": "군 생활이 남자등은 기억에 많이 남겠어요",
          "speaker_id": "2",
          "time": "20210901 17:42"
        },
        {
          "id": "MDRW2100025850.1.6",
          "form": "네 다시 돌아가고 싶은 추억을 만들고 올수도 있고",
          "original_form": "네 다시 돌아가고 싶은 추억을 만들고 올수도 있고",
          "speaker_id": "1",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025850.1.7",
          "form": "끔찍한 시간일 수도 있겠네요",
          "original_form": "끔찍한 시간일 수도 있겠네요",
          "speaker_id": "1",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025850.1.8",
          "form": "선임을 잘 만나야할 것 같네요",
          "original_form": "선임을 잘 만나야할 것 같네요",
          "speaker_id": "2",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025850.1.9",
          "form": "디피라는 군대영화 넷플 개봉했어요",
          "original_form": "디피라는 군대영화 넷플 개봉했어요",
          "speaker_id": "1",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025850.1.10",
          "form": "네 저같은 선임",
          "original_form": "네 저같은 선임",
          "speaker_id": "1",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025850.1.11",
          "form": "어제 다 봣답니다",
          "original_form": "어제 다 봣답니다",
          "speaker_id": "2",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025850.1.12",
          "form": "군대 다녀온것 같나요?",
          "original_form": "군대 다녀온것 같나요?",
          "speaker_id": "1",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025850.1.13",
          "form": "군대드라마 감상평 주세요",
          "original_form": "군대드라마 감상평 주세요",
          "speaker_id": "1",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100025850.1.14",
          "form": "궁금 합니다",
          "original_form": "궁금 합니다",
          "speaker_id": "1",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100025850.1.15",
          "form": "음 뭔가 처음에는 보기어려웠는데 갈수록 재밌더라두요",
          "original_form": "음 뭔가 처음에는 보기어려웠는데 갈수록 재밌더라두요",
          "speaker_id": "2",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100025850.1.16",
          "form": "선임한테 맞는 내용이 많아서 슬펐어요",
          "original_form": "선임한테 맞는 내용이 많아서 슬펐어요",
          "speaker_id": "2",
          "time": "20210901 17:45"
        },
        {
          "id": "MDRW2100025850.1.17",
          "form": "네 저도 오늘 감상해볼게요",
          "original_form": "네 저도 오늘 감상해볼게요",
          "speaker_id": "1",
          "time": "20210901 17:46"
        },
        {
          "id": "MDRW2100025850.1.18",
          "form": "너무 사실적이라더라고요",
          "original_form": "너무 사실적이라더라고요",
          "speaker_id": "1",
          "time": "20210901 17:46"
        },
        {
          "id": "MDRW2100025850.1.19",
          "form": "군대 가보고싶으신가요?",
          "original_form": "군대 가보고싶으신가요?",
          "speaker_id": "2",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100025850.1.20",
          "form": "군대 동기랑은 끈끈해지겠네요",
          "original_form": "군대 동기랑은 끈끈해지겠네요",
          "speaker_id": "1",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100025850.1.21",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100025850.1.22",
          "form": "근데 관심은 있어요",
          "original_form": "근데 관심은 있어요",
          "speaker_id": "1",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100025850.1.23",
          "form": "뽀글이는 먹어보고싶네요",
          "original_form": "뽀글이는 먹어보고싶네요",
          "speaker_id": "2",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100025850.1.24",
          "form": "군데리아두요",
          "original_form": "군데리아두요",
          "speaker_id": "2",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100025850.1.25",
          "form": "그쵸 저도요",
          "original_form": "그쵸 저도요",
          "speaker_id": "1",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100025850.1.26",
          "form": "군대는 피하고 싶긴 하네요",
          "original_form": "군대는 피하고 싶긴 하네요",
          "speaker_id": "2",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100025850.1.27",
          "form": "사회에서 2년간 격리되는것도 나쁘진않을것같아요",
          "original_form": "사회에서 2년간 격리되는것도 나쁘진않을것같아요",
          "speaker_id": "1",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100025850.1.28",
          "form": "선임을 잘 만나야할 것ㅍ같아요",
          "original_form": "선임을 잘 만나야할 것ㅍ같아요",
          "speaker_id": "2",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025850.1.29",
          "form": "당근이죠",
          "original_form": "당근이죠",
          "speaker_id": "1",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025850.1.30",
          "form": "군대가면 불면증 같은건 사라질둣해요",
          "original_form": "군대가면 불면증 같은건 사라질둣해요",
          "speaker_id": "1",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025850.1.31",
          "form": "저는 불면증 없지만",
          "original_form": "저는 불면증 없지만",
          "speaker_id": "1",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025850.1.32",
          "form": "그래도 요새는 휴대폰도 사용가능하대요",
          "original_form": "그래도 요새는 휴대폰도 사용가능하대요",
          "speaker_id": "2",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025850.1.33",
          "form": "점점 좋아지네요",
          "original_form": "점점 좋아지네요",
          "speaker_id": "1",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025850.1.34",
          "form": "코로나때문에 군대애들이 힘들겠어요",
          "original_form": "코로나때문에 군대애들이 힘들겠어요",
          "speaker_id": "2",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100025850.1.35",
          "form": "휴가 못나가서요?",
          "original_form": "휴가 못나가서요?",
          "speaker_id": "1",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100025850.1.36",
          "form": "휴가도 못가고 뭔가 제한이 많을 것 같네요",
          "original_form": "휴가도 못가고 뭔가 제한이 많을 것 같네요",
          "speaker_id": "2",
          "time": "20210901 17:49"
        }
      ]
