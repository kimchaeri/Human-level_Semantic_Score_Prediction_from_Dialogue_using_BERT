      "utterance": [
        {
          "id": "MDRW2100028837.1.1",
          "form": "어떤치킨 좋아하세요?",
          "original_form": "어떤치킨 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028837.1.2",
          "form": "푸라닭요..지금하나 시켰네요",
          "original_form": "푸라닭요..지금하나 시켰네요",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028837.1.3",
          "form": "전 교촌 허니콤보 좋아해요ㅋ",
          "original_form": "전 교촌 허니콤보 좋아해요ㅋ",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028837.1.4",
          "form": "푸라닭은 안먹어봤어요",
          "original_form": "푸라닭은 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028837.1.5",
          "form": "치즈볼도있고 아이들이 좋아하더라구요",
          "original_form": "치즈볼도있고 아이들이 좋아하더라구요",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028837.1.6",
          "form": "뭐 맛없는 치킨은 없죠",
          "original_form": "뭐 맛없는 치킨은 없죠",
          "speaker_id": "1",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100028837.1.7",
          "form": "정답입니다!!!!",
          "original_form": "정답입니다!!!!",
          "speaker_id": "2",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100028837.1.8",
          "form": "네ㅋㅋ 매운거 잘드시나요",
          "original_form": "네ㅋㅋ 매운거 잘드시나요",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028837.1.9",
          "form": "아니요..매운거보다는 달달한거 좋아해요",
          "original_form": "아니요..매운거보다는 달달한거 좋아해요",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028837.1.10",
          "form": "전 매운거도 좋아해서요",
          "original_form": "전 매운거도 좋아해서요",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028837.1.11",
          "form": "매운건 물먹다가 배가 차서~ㅎㅎ",
          "original_form": "매운건 물먹다가 배가 차서~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028837.1.12",
          "form": "그렇긴하죠ㅋ\r\n전 불족발도 잘시켜먹어요",
          "original_form": "그렇긴하죠ㅋ\r\n전 불족발도 잘시켜먹어요",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028837.1.13",
          "form": "전 그건 못먹어요",
          "original_form": "전 그건 못먹어요",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028837.1.14",
          "form": "매워서 손이 떨려요ㅋㅋ",
          "original_form": "매워서 손이 떨려요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028837.1.15",
          "form": "그정도예요?",
          "original_form": "그정도예요?",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028837.1.16",
          "form": "네 근데도 계속먹게되요ㅋ",
          "original_form": "네 근데도 계속먹게되요ㅋ",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028837.1.17",
          "form": "한번도 못먹어봤어요",
          "original_form": "한번도 못먹어봤어요",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028837.1.18",
          "form": "그냥족발도 좋아해요 쫀득쫀득",
          "original_form": "그냥족발도 좋아해요 쫀득쫀득",
