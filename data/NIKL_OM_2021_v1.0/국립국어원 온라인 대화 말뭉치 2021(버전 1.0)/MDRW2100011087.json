      "utterance": [
        {
          "id": "MDRW2100011087.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100011087.1.2",
          "form": "안녕하세요~ 커뮤니티 활동 하시나용?",
          "original_form": "안녕하세요~ 커뮤니티 활동 하시나용?",
          "speaker_id": "2",
          "time": "20210824 00:45"
        },
        {
          "id": "MDRW2100011087.1.3",
          "form": "코로나라 힘드네요ㅠㅠ",
          "original_form": "코로나라 힘드네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 00:45"
        },
        {
          "id": "MDRW2100011087.1.4",
          "form": "어떤활동 하시나요?",
          "original_form": "어떤활동 하시나요?",
          "speaker_id": "1",
          "time": "20210824 00:45"
        },
        {
          "id": "MDRW2100011087.1.5",
          "form": "옹 코로나라 온라인으로 더 많이 활동하지 않나요?",
          "original_form": "옹 코로나라 온라인으로 더 많이 활동하지 않나요?",
          "speaker_id": "2",
          "time": "20210824 00:45"
        },
        {
          "id": "MDRW2100011087.1.6",
          "form": "아 그런가요 추천해주실만한",
          "original_form": "아 그런가요 추천해주실만한",
          "speaker_id": "1",
          "time": "20210824 00:46"
        },
        {
          "id": "MDRW2100011087.1.7",
          "form": "활동 있을까요",
          "original_form": "활동 있을까요",
          "speaker_id": "1",
          "time": "20210824 00:46"
        },
        {
          "id": "MDRW2100011087.1.8",
          "form": "옹 사실 전 온라인커뮤니티를 많이 하진 않게되더라구요",
          "original_form": "옹 사실 전 온라인커뮤니티를 많이 하진 않게되더라구요",
          "speaker_id": "2",
          "time": "20210824 00:51"
        },
        {
          "id": "MDRW2100011087.1.9",
          "form": "가끔 카페에 정보얻으러 가는정도?",
          "original_form": "가끔 카페에 정보얻으러 가는정도?",
          "speaker_id": "2",
          "time": "20210824 00:51"
        },
        {
          "id": "MDRW2100011087.1.10",
          "form": "카페에 검색하면 다 나오더라구요ㅎㅎ",
          "original_form": "카페에 검색하면 다 나오더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 00:52"
        },
        {
          "id": "MDRW2100011087.1.11",
          "form": "맞아요ㅎㅎ 뭐에 관심생기면 카페먼저 둘러보면 정보얻기도 좋고 그래요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ 뭐에 관심생기면 카페먼저 둘러보면 정보얻기도 좋고 그래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 00:53"
        },
        {
          "id": "MDRW2100011087.1.12",
          "form": "마자요 네이버 검색보다 오히려",
          "original_form": "마자요 네이버 검색보다 오히려",
          "speaker_id": "1",
          "time": "20210824 00:53"
        },
        {
          "id": "MDRW2100011087.1.13",
          "form": "좋을때도 있더라구요ㅎㅎ",
          "original_form": "좋을때도 있더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 00:53"
        },
        {
          "id": "MDRW2100011087.1.14",
          "form": "ㅋㅋㅋ맞아요 근데 온라인으로 친해져서 정모까지 하는 분들 신기한것같아요",
          "original_form": "ㅋㅋㅋ맞아요 근데 온라인으로 친해져서 정모까지 하는 분들 신기한것같아요",
          "speaker_id": "2",
          "time": "20210824 00:54"
        },
        {
          "id": "MDRW2100011087.1.15",
          "form": "전 내성적인편이어서 그런지 커뮤활동을 하게되도 정모까진  안나갈것같아요",
          "original_form": "전 내성적인편이어서 그런지 커뮤활동을 하게되도 정모까진  안나갈것같아요",
          "speaker_id": "2",
          "time": "20210824 00:55"
        },
        {
          "id": "MDRW2100011087.1.16",
          "form": "오 온라인정모... 신기하네요",
          "original_form": "오 온라인정모... 신기하네요",
          "speaker_id": "1",
          "time": "20210824 01:05"
        },
        {
          "id": "MDRW2100011087.1.17",
          "form": "ㅋㅋㄱㅋ그쵸 그런거 나가실 것 같으세요?",
          "original_form": "ㅋㅋㄱㅋ그쵸 그런거 나가실 것 같으세요?",
          "speaker_id": "2",
          "time": "20210824 01:09"
        },
        {
          "id": "MDRW2100011087.1.18",
          "form": "정모는 좀 꺼려지네요ㅎㅎ",
          "original_form": "정모는 좀 꺼려지네요ㅎㅎ",
