        {
          "id": "MDRW2100009655.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄴㅋㅋㅋㅌㅋㅋㅂzzzzzzz",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄴㅋㅋㅋㅌㅋㅋㅂzzzzzzz",
          "speaker_id": "2",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009655.1.2",
          "form": "질문 실화냐고욬ㅋㅌㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "질문 실화냐고욬ㅋㅌㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009655.1.3",
          "form": "없습니다.",
          "original_form": "없습니다.",
          "speaker_id": "2",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009655.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009655.1.5",
          "form": "저희집은 유전자에도 탈모유전자는 없습니다.",
          "original_form": "저희집은 유전자에도 탈모유전자는 없습니다.",
          "speaker_id": "2",
          "time": "20210823 02:32"
        },
        {
          "id": "MDRW2100009655.1.6",
          "form": "대뱍",
          "original_form": "대뱍",
          "speaker_id": "1",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009655.1.7",
          "form": "저희집도 유전자에는 없는데",
          "original_form": "저희집도 유전자에는 없는데",
          "speaker_id": "1",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009655.1.8",
          "form": "원래 탈모라는게",
          "original_form": "원래 탈모라는게",
          "speaker_id": "1",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009655.1.9",
          "form": "없다가도 있고 그러니까",
          "original_form": "없다가도 있고 그러니까",
          "speaker_id": "1",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009655.1.10",
          "form": "저는 요즘 탈모인거같기도 하고",
          "original_form": "저는 요즘 탈모인거같기도 하고",
          "speaker_id": "1",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009655.1.11",
          "form": "하 씁...",
          "original_form": "하 씁...",
          "speaker_id": "2",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009655.1.12",
          "form": "그 가르마 점점 넓어져요",
          "original_form": "그 가르마 점점 넓어져요",
          "speaker_id": "1",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009655.1.13",
          "form": "그거 초기에 잘 잡아줘야한대요",
          "original_form": "그거 초기에 잘 잡아줘야한대요",
          "speaker_id": "2",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009655.1.14",
          "form": "속좁은만큼 가르마도 좁아지길 제발.",
          "original_form": "속좁은만큼 가르마도 좁아지길 제발.",
          "speaker_id": "1",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009655.1.15",
          "form": "잘 먹고... 잘자고.....해야한다는데...",
          "original_form": "잘 먹고... 잘자고.....해야한다는데...",
          "speaker_id": "2",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009655.1.16",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009655.1.17",
          "form": "ㅋㅌㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅌㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009655.1.18",
          "form": "아진짜!!!ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아진짜!!!ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009655.1.19",
          "form": "저희 아부지 뒷머리 휑하시거든요",
          "original_form": "저희 아부지 뒷머리 휑하시거든요",
          "speaker_id": "1",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009655.1.20",
          "form": "근데 앞은 풍성해서",
          "original_form": "근데 앞은 풍성해서",
          "speaker_id": "1",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009655.1.21",
          "form": "님...진짜 웃긴사람이에요...하..",
          "original_form": "님...진짜 웃긴사람이에요...하..",
          "speaker_id": "2",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009655.1.22",
          "form": "거울볼 때 머리 쓸어넘기면서 꾸며요",
          "original_form": "거울볼 때 머리 쓸어넘기면서 꾸며요",
          "speaker_id": "1",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009655.1.23",
          "form": "얼마나 귀여운지",
          "original_form": "얼마나 귀여운지",
          "speaker_id": "1",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009655.1.24",
          "form": "보통 앞으로 오지...않나여? 잘 몰라서 묻는겁니다.... 하 씁 귀여우셔",
          "original_form": "보통 앞으로 오지...않나여? 잘 몰라서 묻는겁니다.... 하 씁 귀여우셔",
          "speaker_id": "2",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009655.1.25",
          "form": "뒷머리에 뽀뽀하고싶답니다",
          "original_form": "뒷머리에 뽀뽀하고싶답니다",
          "speaker_id": "1",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009655.1.26",
          "form": "집안내력이네요 귀여움이",
          "original_form": "집안내력이네요 귀여움이",
          "speaker_id": "2",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009655.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009655.1.28",
          "form": "근데 진짜 탈모는 조금만 스트레스받아도 훅 오는 거 같아요",
          "original_form": "근데 진짜 탈모는 조금만 스트레스받아도 훅 오는 거 같아요",
          "speaker_id": "2",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009655.1.29",
          "form": "저 자가격리 기간동안 머리가 한움큼씩 빠졌거든요",
          "original_form": "저 자가격리 기간동안 머리가 한움큼씩 빠졌거든요",
          "speaker_id": "2",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009655.1.30",
          "form": "지금은 복귀됐지만",
          "original_form": "지금은 복귀됐지만",
          "speaker_id": "2",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009655.1.31",
          "form": "하 진짜 그때 너무 놀랐어요 나듀 탈모의 길...? #가보자고",
          "original_form": "하 진짜 그때 너무 놀랐어요 나듀 탈모의 길...? #가보자고",
          "speaker_id": "2",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009655.1.32",
          "form": "안돼안돼 돌아와요",
          "original_form": "안돼안돼 돌아와요",
          "speaker_id": "1",
          "time": "20210823 02:39"
        },
        {
          "id": "MDRW2100009655.1.33",
          "form": "30대부터는 머리숱싸움이라고요",
          "original_form": "30대부터는 머리숱싸움이라고요",
          "speaker_id": "1",
          "time": "20210823 02:39"
        },
        {
          "id": "MDRW2100009655.1.34",
          "form": "지고 들어 #가보자고",
          "original_form": "지고 들어 #가보자고",
          "speaker_id": "1",
          "time": "20210823 02:39"
        },
        {
          "id": "MDRW2100009655.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
