      "utterance": [
        {
          "id": "MDRW2100022183.1.1",
          "form": "김치볶음밥이요 ㅎㅎ name1님은?",
          "original_form": "김치볶음밥이요 ㅎㅎ &name1&님은?",
          "speaker_id": "2",
          "time": "20210829 11:44"
        },
        {
          "id": "MDRW2100022183.1.2",
          "form": "저는 오늘 패스하고 점심먹으려구요!ㅋㅋ",
          "original_form": "저는 오늘 패스하고 점심먹으려구요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022183.1.3",
          "form": "점심 뭐드셔요?",
          "original_form": "점심 뭐드셔요?",
          "speaker_id": "1",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022183.1.4",
          "form": "오늘 정신이 없어서 ㅎㅎ",
          "original_form": "오늘 정신이 없어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100022183.1.5",
          "form": "바빴네요 ㅠ",
          "original_form": "바빴네요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100022183.1.6",
          "form": "식사는 잘 챙겨드시고 계신거죠?ㅠ",
          "original_form": "식사는 잘 챙겨드시고 계신거죠?ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100022183.1.7",
          "form": "네 ㅎㅎ 먹고는 있어요 ㅎㅎ",
          "original_form": "네 ㅎㅎ 먹고는 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100022183.1.8",
          "form": "지금 상 중이라서 ㅠ",
          "original_form": "지금 상 중이라서 ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100022183.1.9",
          "form": "답이 좀 늦었네요 ㅜㅠ",
          "original_form": "답이 좀 늦었네요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100022183.1.10",
          "form": "앗..ㅠ 아니예요",
          "original_form": "앗..ㅠ 아니예요",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100022183.1.11",
          "form": "정신없으시겠어요..ㅠ",
          "original_form": "정신없으시겠어요..ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100022183.1.12",
          "form": "뭔가 훅훅 가네요 ㅎㅎ",
          "original_form": "뭔가 훅훅 가네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100022183.1.13",
          "form": "그쵸ㅠㅠ",
          "original_form": "그쵸ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 07:42"
        },
        {
          "id": "MDRW2100022183.1.14",
          "form": "지치기도 엄청 지치죠",
          "original_form": "지치기도 엄청 지치죠",
          "speaker_id": "1",
          "time": "20210830 07:43"
        },
        {
          "id": "MDRW2100022183.1.15",
          "form": "ㅋㅋ이제 복귀입니다",
          "original_form": "ㅋㅋ이제 복귀입니다",
          "speaker_id": "2",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100022183.1.16",
          "form": "저녁 뭐드시나요",
          "original_form": "저녁 뭐드시나요",
          "speaker_id": "2",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100022183.1.17",
          "form": "고생하셨습니닷!",
          "original_form": "고생하셨습니닷!",
          "speaker_id": "1",
          "time": "20210830 19:22"
        },
        {
          "id": "MDRW2100022183.1.18",
          "form": "저는 피자먹었어요",
          "original_form": "저는 피자먹었어요",
          "speaker_id": "1",
          "time": "20210830 19:22"
        },
        {
          "id": "MDRW2100022183.1.19",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 19:22"
        },
        {
          "id": "MDRW2100022183.1.20",
          "form": "저는 불닭이요 ㅎㅎ",
          "original_form": "저는 불닭이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:05"
        },
        {
          "id": "MDRW2100022183.1.21",
          "form": "크으... 매운거 잘드시나봐요!",
          "original_form": "크으... 매운거 잘드시나봐요!",
          "speaker_id": "1",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100022183.1.22",
          "form": "아침 드셨어오?",
          "original_form": "아침 드셨어오?",
          "speaker_id": "1",
          "time": "20210831 06:52"
        },
        {
          "id": "MDRW2100022183.1.23",
          "form": "아 ㅎㅎ 늦잠자서요",
          "original_form": "아 ㅎㅎ 늦잠자서요",
          "speaker_id": "2",
          "time": "20210831 09:58"
        },
        {
          "id": "MDRW2100022183.1.24",
          "form": "식사 하셨나요?",
          "original_form": "식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210831 09:58"
        },
        {
          "id": "MDRW2100022183.1.25",
          "form": "넹! 주스 갈아 마셨어오~",
          "original_form": "넹! 주스 갈아 마셨어오~",
          "speaker_id": "1",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100022183.1.26",
          "form": "부지런하시네요 ㅎㅎ",
          "original_form": "부지런하시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 10:02"
        },
        {
          "id": "MDRW2100022183.1.27",
          "form": "저는 보통 아침은 걸러서 ㅋ ㅋ",
          "original_form": "저는 보통 아침은 걸러서 ㅋ ㅋ",
          "speaker_id": "2",
          "time": "20210831 10:02"
        },
        {
          "id": "MDRW2100022183.1.28",
          "form": "엄마의 도움으로..ㅋㅋㅋ",
          "original_form": "엄마의 도움으로..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:05"
        },
        {
          "id": "MDRW2100022183.1.29",
          "form": "저도 거르다가 최근에 챙겨먹고 있어용!",
          "original_form": "저도 거르다가 최근에 챙겨먹고 있어용!",
          "speaker_id": "1",
          "time": "20210831 10:05"
        },
        {
          "id": "MDRW2100022183.1.30",
          "form": "엄마랑 같이 살때가 좋죠ㅎㅎ",
          "original_form": "엄마랑 같이 살때가 좋죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 10:07"
        },
        {
          "id": "MDRW2100022183.1.31",
          "form": "좋네용ㅋㅋ",
          "original_form": "좋네용ㅋㅋ",
          "speaker_id": "1",
