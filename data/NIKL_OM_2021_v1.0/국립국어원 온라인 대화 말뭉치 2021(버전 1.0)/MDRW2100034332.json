      "utterance": [
        {
          "id": "MDRW2100034332.1.1",
          "form": "할인 많이되는 쇼핑몰 있을까요?",
          "original_form": "할인 많이되는 쇼핑몰 있을까요?",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100034332.1.2",
          "form": "음..글쎄요! 저는 매년 자라에서 일년에 두번 빅세일할때를 노려요~",
          "original_form": "음..글쎄요! 저는 매년 자라에서 일년에 두번 빅세일할때를 노려요~",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100034332.1.3",
          "form": "오 어디서요?",
          "original_form": "오 어디서요?",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100034332.1.4",
          "form": "자라요 ㅋㅋ 스페인 스파브랜드요~",
          "original_form": "자라요 ㅋㅋ 스페인 스파브랜드요~",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100034332.1.5",
          "form": "우와 그거 어렵지 않나요?",
          "original_form": "우와 그거 어렵지 않나요?",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100034332.1.6",
          "form": "아 세일되면 물건 너무 빨리나가서 난감해요ㅠ",
          "original_form": "아 세일되면 물건 너무 빨리나가서 난감해요ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100034332.1.7",
          "form": "어렵진않은데 손이 빨라야해요 ㅋㅋ",
          "original_form": "어렵진않은데 손이 빨라야해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100034332.1.8",
          "form": "완전 순식간이라고 하더라구요",
          "original_form": "완전 순식간이라고 하더라구요",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100034332.1.9",
          "form": "복잡해보여 도전해보지 않았네요ㅠㅎㅠ",
          "original_form": "복잡해보여 도전해보지 않았네요ㅠㅎㅠ",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100034332.1.10",
          "form": "맞아여 ㅋㅌㅌㅋ",
          "original_form": "맞아여 ㅋㅌㅌㅋ",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100034332.1.11",
          "form": "담번에 도전해보셔요!",
          "original_form": "담번에 도전해보셔요!",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100034332.1.12",
          "form": "지금부터 미리미리 자라가셔서 사이즈 체크해두시면 편해요",
          "original_form": "지금부터 미리미리 자라가셔서 사이즈 체크해두시면 편해요",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100034332.1.13",
          "form": "오옹 name1님 덕분에 지식이 넓어진 느낌이예용",
          "original_form": "오옹 &name1&님 덕분에 지식이 넓어진 느낌이예용",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100034332.1.14",
          "form": "앜ㅋㅋㅋㅋ 아녀요~ 저도 다년간 시도하면서 그냥 깨달은거 공유한거에용 ㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ 아녀요~ 저도 다년간 시도하면서 그냥 깨달은거 공유한거에용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100034332.1.15",
          "form": "크아 다년을 뛰어넘을순 없지만 가까워진 기분이!ㅋㅋㅋ",
          "original_form": "크아 다년을 뛰어넘을순 없지만 가까워진 기분이!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100034332.1.16",
          "form": "전 네이버스토어에서 자주 이용하다보니 그것만 찾게되네요ㅜ",
          "original_form": "전 네이버스토어에서 자주 이용하다보니 그것만 찾게되네요ㅜ",
