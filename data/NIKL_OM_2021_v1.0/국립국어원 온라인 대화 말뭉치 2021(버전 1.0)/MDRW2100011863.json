      "utterance": [
        {
          "id": "MDRW2100011863.1.1",
          "form": "저도 처음 만들어보는데 잘되겠죠..?! ㅋㅋㅋ",
          "original_form": "저도 처음 만들어보는데 잘되겠죠..?! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011863.1.2",
          "form": "재료는 사왔어요",
          "original_form": "재료는 사왔어요",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011863.1.3",
          "form": "우유랑 비피더스같은거!",
          "original_form": "우유랑 비피더스같은거!",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011863.1.4",
          "form": "12시간이나요;;;? 아이고 생각보다 엄청 오래 걸리네요",
          "original_form": "12시간이나요;;;? 아이고 생각보다 엄청 오래 걸리네요",
          "speaker_id": "2",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011863.1.5",
          "form": "그릭요거트를 집에서 만들다니 신세계",
          "original_form": "그릭요거트를 집에서 만들다니 신세계",
          "speaker_id": "2",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011863.1.6",
          "form": "맨날 돈 주고 사먹었는데",
          "original_form": "맨날 돈 주고 사먹었는데",
          "speaker_id": "1",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011863.1.7",
          "form": "너무 비싸요",
          "original_form": "너무 비싸요",
          "speaker_id": "1",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011863.1.8",
          "form": "꿀이랑 그래놀라도 사려구요 ㅋㅋㅋ",
          "original_form": "꿀이랑 그래놀라도 사려구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011863.1.9",
          "form": "오오 그릭요거트에 꿀과 그래놀라 조합이라니 넘 좋네요",
          "original_form": "오오 그릭요거트에 꿀과 그래놀라 조합이라니 넘 좋네요",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100011863.1.10",
          "form": "아침이나 간식 메뉴로 완전 좋을듯",
          "original_form": "아침이나 간식 메뉴로 완전 좋을듯",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100011863.1.11",
          "form": "그쵸,, 꼭 도전 성공할수있길",
          "original_form": "그쵸,, 꼭 도전 성공할수있길",
          "speaker_id": "1",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100011863.1.12",
          "form": "망하면 그냥 버려야된다더라구요ㅠㅋㅋㅋㅋ",
          "original_form": "망하면 그냥 버려야된다더라구요ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100011863.1.13",
          "form": "과발효되거나 하면,,",
          "original_form": "과발효되거나 하면,,",
          "speaker_id": "1",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100011863.1.14",
          "form": "성공할 수 있을 거예요!",
          "original_form": "성공할 수 있을 거예요!",
          "speaker_id": "2",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100011863.1.15",
          "form": "발효는 실온에서 하나요?",
          "original_form": "발효는 실온에서 하나요?",
          "speaker_id": "2",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100011863.1.16",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100011863.1.17",
          "form": "전 전기밥솥이 없어서",
          "original_form": "전 전기밥솥이 없어서",
          "speaker_id": "1",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100011863.1.18",
          "form": "전자레인지에 돌리고 그 안에서 발효시킨다고 하더라구요",
          "original_form": "전자레인지에 돌리고 그 안에서 발효시킨다고 하더라구요",
          "speaker_id": "1",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100011863.1.19",
          "form": "전자레인지를 사용해 발효라니 신기하네요",
          "original_form": "전자레인지를 사용해 발효라니 신기하네요",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011863.1.20",
          "form": "원래는 전기밥솥이군요",
          "original_form": "원래는 전기밥솥이군요",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011863.1.21",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100011863.1.22",
          "form": "그뒤에 방식에 따라서 리코타 치즈도되고",
          "original_form": "그뒤에 방식에 따라서 리코타 치즈도되고",
          "speaker_id": "1",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100011863.1.23",
          "form": "그릭요거트도 되고",
          "original_form": "그릭요거트도 되고",
          "speaker_id": "1",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100011863.1.24",
          "form": "하는것같아요 ㅎㅎ",
          "original_form": "하는것같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100011863.1.25",
          "form": "헉 리코타치즈도 같은 방식이에요?",
          "original_form": "헉 리코타치즈도 같은 방식이에요?",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011863.1.26",
          "form": "반은 요거트 만들고 반은 치즈 만들어도 좋겠어요",
          "original_form": "반은 요거트 만들고 반은 치즈 만들어도 좋겠어요",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011863.1.27",
          "form": "리코타치즈 샐러드도 집에서 해먹을 수 있겠네요",
          "original_form": "리코타치즈 샐러드도 집에서 해먹을 수 있겠네요",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011863.1.28",
          "form": "매번 12시간씩 걸리나요?",
          "original_form": "매번 12시간씩 걸리나요?",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011863.1.29",
          "form": "아마도 그런 것 같아요",
          "original_form": "아마도 그런 것 같아요",
          "speaker_id": "1",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100011863.1.30",
          "form": "어제 하고 중간중간 체크해줬는데",
          "original_form": "어제 하고 중간중간 체크해줬는데",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100011863.1.31",
          "form": "엄청 꾸덕한 그릭요거트 완성 했어요 ㅎㅎ 성골",
          "original_form": "엄청 꾸덕한 그릭요거트 완성 했어요 ㅎㅎ 성골",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100011863.1.32",
          "form": "성공",
          "original_form": "성공",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100011863.1.33",
          "form": "확실히 직접 해먹는 건  정성이 필요하네요",
          "original_form": "확실히 직접 해먹는 건  정성이 필요하네요",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100011863.1.34",
          "form": "오오 축하드려요",
          "original_form": "오오 축하드려요",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100011863.1.35",
          "form": "아침에 그래놀라와 꿀 타드셨나요?",
          "original_form": "아침에 그래놀라와 꿀 타드셨나요?",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100011863.1.36",
          "form": "아쉽게도 집에 아직 그래놀라가 없었습니다,,",
          "original_form": "아쉽게도 집에 아직 그래놀라가 없었습니다,,",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100011863.1.37",
          "form": "그래도 블루베리랑 먹었어요",
          "original_form": "그래도 블루베리랑 먹었어요",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
