      "utterance": [
        {
          "id": "MDRW2100040246.1.1",
          "form": "머드세용",
          "original_form": "머드세용",
          "speaker_id": "2",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100040246.1.2",
          "form": "저 아이스아메리카노랑 물이요",
          "original_form": "저 아이스아메리카노랑 물이요",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100040246.1.3",
          "form": "오 디저트는요",
          "original_form": "오 디저트는요",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040246.1.4",
          "form": "디저트는 말차파운드가 있는데 팍팍해용",
          "original_form": "디저트는 말차파운드가 있는데 팍팍해용",
          "speaker_id": "1",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040246.1.5",
          "form": "이런 촉촉하지않구요?",
          "original_form": "이런 촉촉하지않구요?",
          "speaker_id": "2",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100040246.1.6",
          "form": "넹 드셔보실래요?",
          "original_form": "넹 드셔보실래요?",
          "speaker_id": "1",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040246.1.7",
          "form": "네...어디로갈까요",
          "original_form": "네...어디로갈까요",
          "speaker_id": "2",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100040246.1.8",
          "form": "히히 다음에 드릴게요",
          "original_form": "히히 다음에 드릴게요",
          "speaker_id": "1",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100040246.1.9",
          "form": "저 커피마셨더니 너무 설레네요",
          "original_form": "저 커피마셨더니 너무 설레네요",
          "speaker_id": "1",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100040246.1.10",
          "form": "두근거리나요",
          "original_form": "두근거리나요",
          "speaker_id": "2",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100040246.1.11",
          "form": "네 이런이런 ㅜ 두근거려요 ㅋㅋ 힝 카페인",
          "original_form": "네 이런이런 ㅜ 두근거려요 ㅋㅋ 힝 카페인",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100040246.1.12",
          "form": "먹고시픈건마는데 몸이 말을 안듣네유",
          "original_form": "먹고시픈건마는데 몸이 말을 안듣네유",
          "speaker_id": "2",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100040246.1.13",
          "form": "맞아요 ㅠㅠㅠㅠㅠ 힝 ㅠㅠㅠname2님은 괜찮으신가요?",
          "original_form": "맞아요 ㅠㅠㅠㅠㅠ 힝 ㅠㅠㅠ&name2&님은 괜찮으신가요?",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100040246.1.14",
          "form": "아직은요..",
          "original_form": "아직은요..",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100040246.1.15",
          "form": "저녁 머드세요",
          "original_form": "저녁 머드세요",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100040246.1.16",
          "form": "그러게요 ㅋㅋㅋ뭐먹을지 고민되네용",
          "original_form": "그러게요 ㅋㅋㅋ뭐먹을지 고민되네용",
          "speaker_id": "1",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100040246.1.17",
          "form": "엄마가 주는대로 먹어야해요",
          "original_form": "엄마가 주는대로 먹어야해요",
          "speaker_id": "1",
