      "utterance": [
        {
          "id": "MDRW2100037544.1.1",
          "form": "오늘은 뭐하니",
          "original_form": "오늘은 뭐하니",
          "speaker_id": "2",
          "time": "20210901 09:57"
        },
        {
          "id": "MDRW2100037544.1.2",
          "form": "애들은 다 잘 갔나??",
          "original_form": "애들은 다 잘 갔나??",
          "speaker_id": "2",
          "time": "20210901 09:57"
        },
        {
          "id": "MDRW2100037544.1.3",
          "form": "응 잘갔어ㅋ",
          "original_form": "응 잘갔어ㅋ",
          "speaker_id": "1",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100037544.1.4",
          "form": "조금이따가 온누리사러가게ㅋㅋ",
          "original_form": "조금이따가 온누리사러가게ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100037544.1.5",
          "form": "집청소 대충하고ㅋ",
          "original_form": "집청소 대충하고ㅋ",
          "speaker_id": "1",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100037544.1.6",
          "form": "아하 바쁘다이",
          "original_form": "아하 바쁘다이",
          "speaker_id": "2",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100037544.1.7",
          "form": "알뜰살뜰 잘 살고 있구먼",
          "original_form": "알뜰살뜰 잘 살고 있구먼",
          "speaker_id": "2",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100037544.1.8",
          "form": "잘한다 정말",
          "original_form": "잘한다 정말",
          "speaker_id": "2",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100037544.1.9",
          "form": "언니 ^^ 나 사기당했잖아ㅋ  이렇게라도 해야지^^ 매꿀라면",
          "original_form": "언니 ^^ 나 사기당했잖아ㅋ  이렇게라도 해야지^^ 매꿀라면",
          "speaker_id": "1",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100037544.1.10",
          "form": "울신랑은 아무것도하지말래 머지로 인해 신뢰를  잃음ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "울신랑은 아무것도하지말래 머지로 인해 신뢰를  잃음ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:36"
        },
        {
          "id": "MDRW2100037544.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ 그건 돈을 못받는다고 했나??",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그건 돈을 못받는다고 했나??",
          "speaker_id": "2",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100037544.1.12",
          "form": "주식이가 그것도??",
          "original_form": "주식이가 그것도??",
          "speaker_id": "2",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100037544.1.13",
          "form": "응 쪼매씩받고있긴해 ㅋㅋ아주 쪼금씩ㅋ",
          "original_form": "응 쪼매씩받고있긴해 ㅋㅋ아주 쪼금씩ㅋ",
          "speaker_id": "1",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100037544.1.14",
          "form": "그건주식아니야ㅋ",
          "original_form": "그건주식아니야ㅋ",
          "speaker_id": "1",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100037544.1.15",
          "form": "폰지사기래 ㅋㄲ첨 알게됐음",
          "original_form": "폰지사기래 ㅋㄲ첨 알게됐음",
          "speaker_id": "1",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100037544.1.16",
          "form": "아 뭔지 모르겠다",
          "original_form": "아 뭔지 모르겠다",
          "speaker_id": "2",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100037544.1.17",
          "form": "힘들고 만",
          "original_form": "힘들고 만",
          "speaker_id": "2",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100037544.1.18",
          "form": "우리 신랑은 뭐 돈을 한참 잃어 가지고",
          "original_form": "우리 신랑은 뭐 돈을 한참 잃어 가지고",
          "speaker_id": "2",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100037544.1.19",
          "form": "신뢰가 없다 없어",
          "original_form": "신뢰가 없다 없어",
          "speaker_id": "2",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100037544.1.20",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㅋㅋ웃겨",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㅋㅋ웃겨",
          "speaker_id": "1",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100037544.1.21",
          "form": "맞다 나두그려ㅋㅋ",
          "original_form": "맞다 나두그려ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100037544.1.22",
          "form": "이제 밥먹는다",
          "original_form": "이제 밥먹는다",
          "speaker_id": "1",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100037544.1.23",
          "form": "언니도 밥먹으러가겟네ㅋ",
          "original_form": "언니도 밥먹으러가겟네ㅋ",
          "speaker_id": "1",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100037544.1.24",
          "form": "어 나 밥 먹고 왔어",
          "original_form": "어 나 밥 먹고 왔어",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100037544.1.25",
          "form": "오늘은 짬뽕밥",
          "original_form": "오늘은 짬뽕밥",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100037544.1.26",
          "form": "밥 먹었나??",
          "original_form": "밥 먹었나??",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100037544.1.27",
          "form": "응 진미김밥먹었당ㅋ",
          "original_form": "응 진미김밥먹었당ㅋ",
          "speaker_id": "1",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100037544.1.28",
          "form": "집에 재료 대충 넣어서ㅋㅋ",
          "original_form": "집에 재료 대충 넣어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100037544.1.29",
          "form": "언니는 맛있는거 많이먹네ㅋㅋ",
          "original_form": "언니는 맛있는거 많이먹네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100037544.1.30",
          "form": "맨날 밖에서 사먹으니",
          "original_form": "맨날 밖에서 사먹으니",
          "speaker_id": "2",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100037544.1.31",
          "form": "집에서는 잘 안시켜먹게됨",
          "original_form": "집에서는 잘 안시켜먹게됨",
          "speaker_id": "2",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100037544.1.32",
          "form": "딱히 애들한테 맛있는거 해주는것도 못하면서",
          "original_form": "딱히 애들한테 맛있는거 해주는것도 못하면서",
          "speaker_id": "2",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100037544.1.33",
          "form": "내가 사 먹었으니 왠지 시켜 먹기 싫은 기분",
          "original_form": "내가 사 먹었으니 왠지 시켜 먹기 싫은 기분",
          "speaker_id": "2",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100037544.1.34",
          "form": "응응  맞어ㅋㅋ",
          "original_form": "응응  맞어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100037544.1.35",
          "form": "너무 사먹으니ㅋ",
          "original_form": "너무 사먹으니ㅋ",
          "speaker_id": "1",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100037544.1.36",
          "form": "신랑도 언니도 점심은 각자 돈으로 사먹나",
          "original_form": "신랑도 언니도 점심은 각자 돈으로 사먹나",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100037544.1.37",
          "form": "어 맨날 사먹으니 딱히 먹고 싶은 것도 없고",
          "original_form": "어 맨날 사먹으니 딱히 먹고 싶은 것도 없고",
          "speaker_id": "2",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100037544.1.38",
          "form": "신랑은 저녁은 사무실에서 카드가 나오고",
          "original_form": "신랑은 저녁은 사무실에서 카드가 나오고",
          "speaker_id": "2",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100037544.1.39",
          "form": "점심은 각자 돈으로",
          "original_form": "점심은 각자 돈으로",
          "speaker_id": "2",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100037544.1.40",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100037544.1.41",
          "form": "점심값도 장난아니겠다",
          "original_form": "점심값도 장난아니겠다",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100037544.1.42",
          "form": "한 달에 사 먹는 밥값이 꽤 나옴",
          "original_form": "한 달에 사 먹는 밥값이 꽤 나옴",
          "speaker_id": "2",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100037544.1.43",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "1",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100037544.1.44",
          "form": "어 밥도 사먹고 가끔 커피도 사먹고",
          "original_form": "어 밥도 사먹고 가끔 커피도 사먹고",
          "speaker_id": "2",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100037544.1.45",
          "form": "그럴듯",
          "original_form": "그럴듯",
          "speaker_id": "1",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100037544.1.46",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "1",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100037544.1.47",
          "form": "커피값을 줄여야지",
          "original_form": "커피값을 줄여야지",
          "speaker_id": "2",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100037544.1.48",
          "form": "또 누구사주고 이런것도있지않나?",
          "original_form": "또 누구사주고 이런것도있지않나?",
          "speaker_id": "1",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100037544.1.49",
          "form": "밥은 굶을 수 없으니",
          "original_form": "밥은 굶을 수 없으니",
          "speaker_id": "2",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100037544.1.50",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100037544.1.51",
          "form": "어 또 누가 사주기도 하고 우리가 사주기도 하고",
          "original_form": "어 또 누가 사주기도 하고 우리가 사주기도 하고",
          "speaker_id": "2",
          "time": "20210901 12:30"
        }
      ]
    }
