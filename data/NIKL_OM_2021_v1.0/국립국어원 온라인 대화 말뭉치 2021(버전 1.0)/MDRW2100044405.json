        },
        {
          "id": "MDRW2100044405.1.3",
          "form": "진짜 박서준이랑 백진희가",
          "original_form": "진짜 박서준이랑 백진희가",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044405.1.4",
          "form": "지금은 아니지만 그때 당시요",
          "original_form": "지금은 아니지만 그때 당시요",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044405.1.5",
          "form": "미래의 선택 드라마",
