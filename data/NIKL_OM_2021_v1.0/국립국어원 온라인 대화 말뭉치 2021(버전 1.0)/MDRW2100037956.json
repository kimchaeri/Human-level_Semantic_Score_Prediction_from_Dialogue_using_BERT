      "utterance": [
        {
          "id": "MDRW2100037956.1.1",
          "form": "휴...ㅠㅠ 눕혀놓고 나오는건 안되제??",
          "original_form": "휴...ㅠㅠ 눕혀놓고 나오는건 안되제??",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100037956.1.2",
          "form": "첫찌는 잠들었나??",
          "original_form": "첫찌는 잠들었나??",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100037956.1.3",
          "form": "응 첫찌는 눕자마자",
          "original_form": "응 첫찌는 눕자마자",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100037956.1.4",
          "form": "10분안으로자고",
          "original_form": "10분안으로자고",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100037956.1.5",
          "form": "지금은 거실에 일단 누워서울고잇음",
          "original_form": "지금은 거실에 일단 누워서울고잇음",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100037956.1.6",
          "form": "아하 둘찌가 항상 문제네",
          "original_form": "아하 둘찌가 항상 문제네",
          "speaker_id": "2",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100037956.1.7",
          "form": "그러니깐ㅜㅜ돌겠ㅜㅜ",
          "original_form": "그러니깐ㅜㅜ돌겠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100037956.1.8",
          "form": "아 ㅠ 울고있나 ㅠ",
          "original_form": "아 ㅠ 울고있나 ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100037956.1.9",
          "form": "신랑은 아직이고??",
          "original_form": "신랑은 아직이고??",
          "speaker_id": "2",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100037956.1.10",
          "form": "응 신랑은 아직",
          "original_form": "응 신랑은 아직",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100037956.1.11",
          "form": "아빠찾는다",
          "original_form": "아빠찾는다",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100037956.1.12",
          "form": "ㅜㅠ 일찍 육퇴할라했는디",
          "original_form": "ㅜㅠ 일찍 육퇴할라했는디",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100037956.1.13",
          "form": "아이고 둘찌야 ㅠ",
          "original_form": "아이고 둘찌야 ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100037956.1.14",
          "form": "속터진다",
          "original_form": "속터진다",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037956.1.15",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037956.1.16",
          "form": "이제 그쳤네",
          "original_form": "이제 그쳤네",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037956.1.17",
          "form": "눈물",
          "original_form": "눈물",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037956.1.18",
          "form": "어후 ㅠㅠ",
          "original_form": "어후 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100037956.1.19",
          "form": "한번 더 노력해봐라",
          "original_form": "한번 더 노력해봐라",
          "speaker_id": "2",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100037956.1.20",
          "form": "힘내라 ㅠ",
          "original_form": "힘내라 ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100037956.1.21",
          "form": "뒹굴다가자것지.  후ㅋㅋ",
          "original_form": "뒹굴다가자것지.  후ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100037956.1.22",
          "form": "신랑지금 오는중이래",
          "original_form": "신랑지금 오는중이래",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100037956.1.23",
          "form": "재워라고해야겠다",
          "original_form": "재워라고해야겠다",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100037956.1.24",
          "form": "그래 포기하자...",
          "original_form": "그래 포기하자...",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100037956.1.25",
          "form": "오늘 애썼다",
          "original_form": "오늘 애썼다",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100037956.1.26",
          "form": "응응ㅋㅋㅋ아오",
          "original_form": "응응ㅋㅋㅋ아오",
          "speaker_id": "1",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100037956.1.27",
          "form": "오늘은 참 재우기힘드네",
          "original_form": "오늘은 참 재우기힘드네",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100037956.1.28",
          "form": "쉬자 ㅠ",
          "original_form": "쉬자 ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100037956.1.29",
          "form": "우리신랑도 지금 왔음",
          "original_form": "우리신랑도 지금 왔음",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100037956.1.30",
          "form": "저녁안먹었다고 뭐 시켜먹자해서",
          "original_form": "저녁안먹었다고 뭐 시켜먹자해서",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100037956.1.31",
          "form": "고르고 있다 ㅋㅋ",
          "original_form": "고르고 있다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:43"
        }
      ]
    }
