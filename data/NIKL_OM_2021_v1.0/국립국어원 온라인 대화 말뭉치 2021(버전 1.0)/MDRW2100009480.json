        {
          "id": "MDRW2100009480.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100009480.1.2",
          "form": "당근 깔아두긴했어요",
          "original_form": "당근 깔아두긴했어요",
          "speaker_id": "2",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009480.1.3",
          "form": "전 해요",
          "original_form": "전 해요",
          "speaker_id": "1",
          "time": "20210823 07:18"
        },
        {
          "id": "MDRW2100009480.1.4",
          "form": "음 요즘 네이버페이나 카카오페이 많이 써요",
          "original_form": "음 요즘 네이버페이나 카카오페이 많이 써요",
          "speaker_id": "2",
          "time": "20210823 09:43"
        },
        {
          "id": "MDRW2100009480.1.5",
          "form": "네이버페이도 좋죠",
          "original_form": "네이버페이도 좋죠",
          "speaker_id": "1",
          "time": "20210823 09:54"
        },
        {
          "id": "MDRW2100009480.1.6",
          "form": "쓰고나면 보너스로 적립해주는게 좋더라고요",
          "original_form": "쓰고나면 보너스로 적립해주는게 좋더라고요",
          "speaker_id": "2",
          "time": "20210823 09:55"
        },
        {
          "id": "MDRW2100009480.1.7",
          "form": "적립 쏠쏠하죠",
          "original_form": "적립 쏠쏠하죠",
          "speaker_id": "1",
          "time": "20210823 09:56"
        },
        {
          "id": "MDRW2100009480.1.8",
          "form": "공짜돈 얻는 기분이죠",
          "original_form": "공짜돈 얻는 기분이죠",
          "speaker_id": "2",
          "time": "20210823 09:57"
        },
        {
          "id": "MDRW2100009480.1.9",
          "form": "지역화폐도 좋아요",
          "original_form": "지역화폐도 좋아요",
          "speaker_id": "2",
          "time": "20210823 09:57"
        },
        {
          "id": "MDRW2100009480.1.10",
          "form": "할인이 대박이죠",
          "original_form": "할인이 대박이죠",
          "speaker_id": "1",
          "time": "20210823 09:58"
        },
        {
          "id": "MDRW2100009480.1.11",
          "form": "최근 잘산 것 같다 싶은 물건 있으신가요?",
          "original_form": "최근 잘산 것 같다 싶은 물건 있으신가요?",
          "speaker_id": "2",
          "time": "20210823 10:20"
        },
        {
          "id": "MDRW2100009480.1.12",
          "form": "제습기요",
          "original_form": "제습기요",
          "speaker_id": "1",
          "time": "20210823 10:27"
        },
        {
          "id": "MDRW2100009480.1.13",
          "form": "용량이 얼마짜리인가요 요즘껀",
          "original_form": "용량이 얼마짜리인가요 요즘껀",
