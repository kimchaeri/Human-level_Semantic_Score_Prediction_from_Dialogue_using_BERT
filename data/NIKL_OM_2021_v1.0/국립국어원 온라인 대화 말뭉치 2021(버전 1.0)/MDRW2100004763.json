          "time": "20210821 18:54"
        },
        {
          "id": "MDRW2100004763.1.12",
          "form": "동의해요.",
          "original_form": "동의해요.",
          "speaker_id": "2",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004763.1.13",
          "form": "그쵸",
