      "utterance": [
        {
          "id": "MDRW2100029006.1.1",
          "form": "아 그러게요",
          "original_form": "아 그러게요",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029006.1.2",
          "form": "확실히 추워지면 붕어빵은 진짜 참기 힘들어요ㅋㅋㅋ",
          "original_form": "확실히 추워지면 붕어빵은 진짜 참기 힘들어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029006.1.3",
          "form": "팥붕vs슈붕",
          "original_form": "팥붕vs슈붕",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029006.1.4",
          "form": "전 슈붕이요!",
          "original_form": "전 슈붕이요!",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029006.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029006.1.6",
          "form": "앗 저는 팥붕을 더 좋아해요 ㅎ 슈붕도 잘 먹지만",
          "original_form": "앗 저는 팥붕을 더 좋아해요 ㅎ 슈붕도 잘 먹지만",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029006.1.7",
          "form": "머리부터vs꼬리부터",
          "original_form": "머리부터vs꼬리부터",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029006.1.8",
          "form": "저는 잡히는것부터 먹긴하지만 머리부터 먹는 것같아요",
          "original_form": "저는 잡히는것부터 먹긴하지만 머리부터 먹는 것같아요",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029006.1.9",
          "form": "님은요?!",
          "original_form": "님은요?!",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029006.1.10",
          "form": "오 저는 무조건 머리부터요 ㅋㅋㅋㅋ",
          "original_form": "오 저는 무조건 머리부터요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029006.1.11",
          "form": "겨울간식 좋아하는거 또 있으신가요",
          "original_form": "겨울간식 좋아하는거 또 있으신가요",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029006.1.12",
          "form": "호떡이나 찐빵이나",
          "original_form": "호떡이나 찐빵이나",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029006.1.13",
          "form": "호떡!! 호떡 맛있어요ㅋㅋㅋ",
          "original_form": "호떡!! 호떡 맛있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029006.1.14",
          "form": "저희 동네에 고르곤졸라호떡 파는 아저씨 오는데 진짜 맛있더라구요",
          "original_form": "저희 동네에 고르곤졸라호떡 파는 아저씨 오는데 진짜 맛있더라구요",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029006.1.15",
          "form": "고르곤졸라랑 호떡이라니",
          "original_form": "고르곤졸라랑 호떡이라니",
          "speaker_id": "2",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029006.1.16",
          "form": "꿀 같이 들어가있는데 아주 환상입니다",
          "original_form": "꿀 같이 들어가있는데 아주 환상입니다",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029006.1.17",
          "form": "캬 또 어떤 맛잘알이 그런걸 만들어냈는지",
          "original_form": "캬 또 어떤 맛잘알이 그런걸 만들어냈는지",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029006.1.18",
          "form": "그러게요.. 요리 잘하세요?",
          "original_form": "그러게요.. 요리 잘하세요?",
          "speaker_id": "1",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029006.1.19",
          "form": "음 일단 먹고 살정도는 하는데",
          "original_form": "음 일단 먹고 살정도는 하는데",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029006.1.20",
          "form": "메뉴갯수가 몇개 안되는...ㅋㅋㅋ",
          "original_form": "메뉴갯수가 몇개 안되는...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029006.1.21",
          "form": "ㅋㅋㅋㅋㅋ먹고 살면 된거죠",
          "original_form": "ㅋㅋㅋㅋㅋ먹고 살면 된거죠",
          "speaker_id": "1",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100029006.1.22",
          "form": "그거 해보셨나요 박막례 할머니 간장국수",
          "original_form": "그거 해보셨나요 박막례 할머니 간장국수",
