      "utterance": [
        {
          "id": "MDRW2100024642.1.1",
          "form": "어제 남편 로션 인터넷으로 구입했구만요",
          "original_form": "어제 남편 로션 인터넷으로 구입했구만요",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024642.1.2",
          "form": "난. 오늘 가방하나 사기로햇어",
          "original_form": "난. 오늘 가방하나 사기로햇어",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024642.1.3",
          "form": "자매들끼리 사기로했지",
          "original_form": "자매들끼리 사기로했지",
          "speaker_id": "1",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024642.1.4",
          "form": "캘빈클라인에서 화장품도 나온다\r\n\r\n가방? 어떤거?",
          "original_form": "캘빈클라인에서 화장품도 나온다\r\n\r\n가방? 어떤거?",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024642.1.5",
          "form": "언니가 사오는거야?",
          "original_form": "언니가 사오는거야?",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024642.1.6",
          "form": "아니 여기 자매들끼리 가방공동구매",
          "original_form": "아니 여기 자매들끼리 가방공동구매",
          "speaker_id": "1",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024642.1.7",
          "form": "카바스라는 가방",
          "original_form": "카바스라는 가방",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024642.1.8",
          "form": "공동구매 진행은?",
          "original_form": "공동구매 진행은?",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024642.1.9",
          "form": "lf몰에서 세일하는거",
          "original_form": "lf몰에서 세일하는거",
          "speaker_id": "1",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100024642.1.10",
          "form": "오호~ 좋구만",
          "original_form": "오호~ 좋구만",
          "speaker_id": "2",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100024642.1.11",
          "form": "가방 몇개는 중고로 내놔야겠어",
          "original_form": "가방 몇개는 중고로 내놔야겠어",
          "speaker_id": "1",
          "time": "20210831 17:47"
        },
        {
          "id": "MDRW2100024642.1.12",
          "form": "내놓기전에 구경시켜줘",
          "original_form": "내놓기전에 구경시켜줘",
          "speaker_id": "2",
          "time": "20210831 19:14"
        },
        {
          "id": "MDRW2100024642.1.13",
          "form": "당근거래 하실껀가요?",
          "original_form": "당근거래 하실껀가요?",
          "speaker_id": "2",
          "time": "20210831 19:14"
        },
        {
          "id": "MDRW2100024642.1.14",
          "form": "당근에서 팔아야겠지?",
          "original_form": "당근에서 팔아야겠지?",
          "speaker_id": "1",
          "time": "20210831 19:27"
        },
        {
          "id": "MDRW2100024642.1.15",
          "form": "중고거래 사이트중 당근이 젤 좋은거 같아",
          "original_form": "중고거래 사이트중 당근이 젤 좋은거 같아",
          "speaker_id": "2",
          "time": "20210831 19:28"
        },
        {
          "id": "MDRW2100024642.1.16",
          "form": "눅스오일 사고싶어",
          "original_form": "눅스오일 사고싶어",
          "speaker_id": "2",
          "time": "20210831 19:28"
        },
        {
          "id": "MDRW2100024642.1.17",
          "form": "가격 많이 내렸던데",
          "original_form": "가격 많이 내렸던데",
          "speaker_id": "2",
          "time": "20210831 19:28"
        },
        {
          "id": "MDRW2100024642.1.18",
          "form": "보는중",
          "original_form": "보는중",
          "speaker_id": "2",
          "time": "20210831 19:28"
        },
        {
          "id": "MDRW2100024642.1.19",
          "form": "면세점이 싸긴한데",
          "original_form": "면세점이 싸긴한데",
          "speaker_id": "1",
          "time": "20210831 19:29"
        },
        {
          "id": "MDRW2100024642.1.20",
          "form": "눅스오일은 안써봤네",
          "original_form": "눅스오일은 안써봤네",
          "speaker_id": "1",
          "time": "20210831 19:29"
        },
        {
          "id": "MDRW2100024642.1.21",
          "form": "난 운동화사고 싶어",
          "original_form": "난 운동화사고 싶어",
          "speaker_id": "1",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024642.1.22",
          "form": "향이 진짜 좋아",
          "original_form": "향이 진짜 좋아",
          "speaker_id": "2",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024642.1.23",
          "form": "운동화? 골구?",
          "original_form": "운동화? 골구?",
          "speaker_id": "2",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024642.1.24",
          "form": "골든구스 사고싶어?",
          "original_form": "골든구스 사고싶어?",
          "speaker_id": "2",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024642.1.25",
          "form": "이모티콘보니 사고 싶긴 하더라",
          "original_form": "이모티콘보니 사고 싶긴 하더라",
          "speaker_id": "2",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024642.1.26",
          "form": "골든구스는 아직 묻어두고 편한것부터 사야지",
          "original_form": "골든구스는 아직 묻어두고 편한것부터 사야지",
          "speaker_id": "1",
          "time": "20210831 19:38"
        }
      ]
    }
