      "utterance": [
        {
          "id": "MDRW2100015225.1.1",
          "form": "단백질 섭취는 어떻게 하세요? 평소에",
          "original_form": "단백질 섭취는 어떻게 하세요? 평소에",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015225.1.2",
          "form": "딱히 안챙겨요",
          "original_form": "딱히 안챙겨요",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015225.1.3",
          "form": "연어 좋아하세요?",
          "original_form": "연어 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015225.1.4",
          "form": "네네 ㅋㅋㅋ 좋아해요 연어 부드럽고",
          "original_form": "네네 ㅋㅋㅋ 좋아해요 연어 부드럽고",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015225.1.5",
          "form": "느끼해서 안좋아하는 사람도 많지만요",
          "original_form": "느끼해서 안좋아하는 사람도 많지만요",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015225.1.6",
          "form": "전 회를 못먹어요  ㅜㅁ ㅜ",
          "original_form": "전 회를 못먹어요  ㅜㅁ ㅜ",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015225.1.7",
          "form": "아앗 그러면 새우는 드세요?",
          "original_form": "아앗 그러면 새우는 드세요?",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015225.1.8",
          "form": "회는 못먹어도 오징어물회나 새우는 먹는 사람 있더라고요",
          "original_form": "회는 못먹어도 오징어물회나 새우는 먹는 사람 있더라고요",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015225.1.9",
          "form": "어 제가 그렀습니다.ㅋ",
          "original_form": "어 제가 그렀습니다.ㅋ",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015225.1.10",
          "form": "오징어회. 산낙진 먹습니다 쿠쿠쿠",
          "original_form": "오징어회. 산낙진 먹습니다 쿠쿠쿠",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015225.1.11",
          "form": "우와 ㅋㅋㅋㅋ name1님도 그럴거라도 생각한건 아닌데 신기해요",
          "original_form": "우와 ㅋㅋㅋㅋ &name1&님도 그럴거라도 생각한건 아닌데 신기해요",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015225.1.12",
          "form": "산낙지도!!",
          "original_form": "산낙지도!!",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015225.1.13",
          "form": "그럼 새우는 아닌가요?ㅋㅋㅋ",
          "original_form": "그럼 새우는 아닌가요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015225.1.14",
          "form": "새우먹어요",
          "original_form": "새우먹어요",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015225.1.15",
          "form": "그냥 생선(회)  육(회) 만 못먹어요 ㅎㅎ",
          "original_form": "그냥 생선(회)  육(회) 만 못먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015225.1.16",
          "form": "나머진 먹어요 ㅋ",
          "original_form": "나머진 먹어요 ㅋ",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015225.1.17",
          "form": "아!! 육회도요 약간 날로 먹는걸 안좋아하시는거군요 ㅎㅎ",
          "original_form": "아!! 육회도요 약간 날로 먹는걸 안좋아하시는거군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015225.1.18",
          "form": "이중적 의미 ㅋㅋㅋㅋ",
          "original_form": "이중적 의미 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015225.1.19",
          "form": "ㅋㅋ 근데  산낙지는 먹는 ㅋㅋ",
          "original_form": "ㅋㅋ 근데  산낙지는 먹는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015225.1.20",
          "form": "희안하죠",
          "original_form": "희안하죠",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015225.1.21",
          "form": "저희 외할머니가  낙지 잡는딜 하시거든요",
