      "utterance": [
        {
          "id": "MDRW2100035464.1.1",
          "form": "ㅎㅎ 마지막만찬...",
          "original_form": "ㅎㅎ 마지막만찬...",
          "speaker_id": "2",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100035464.1.2",
          "form": "저녁인거예요?",
          "original_form": "저녁인거예요?",
          "speaker_id": "2",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100035464.1.3",
          "form": "저녁대신? 뭔가 이걸 먹고있으니 더이상 뭔가 먹음 안될거같아요 ㅋㅋ",
          "original_form": "저녁대신? 뭔가 이걸 먹고있으니 더이상 뭔가 먹음 안될거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100035464.1.4",
          "form": "ㅎㅎ 그래도 저녁되면배고프실듯해요",
          "original_form": "ㅎㅎ 그래도 저녁되면배고프실듯해요",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100035464.1.5",
          "form": "저는 백순대볶음 밀키트사서 먹었는데",
          "original_form": "저는 백순대볶음 밀키트사서 먹었는데",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100035464.1.6",
          "form": "생각보다 괜찮네요",
          "original_form": "생각보다 괜찮네요",
          "speaker_id": "2",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100035464.1.7",
          "form": "오 백순대볶음은 처음 들어봤어요 ㅋㅋ",
          "original_form": "오 백순대볶음은 처음 들어봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100035464.1.8",
          "form": "마트같은 곳에서 파나요?",
          "original_form": "마트같은 곳에서 파나요?",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100035464.1.9",
          "form": "아뇨 ㅎ",
          "original_form": "아뇨 ㅎ",
          "speaker_id": "2",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100035464.1.10",
          "form": "윙잇인가?",
          "original_form": "윙잇인가?",
          "speaker_id": "2",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100035464.1.11",
          "form": "거기서 샀어요 밀키트",
          "original_form": "거기서 샀어요 밀키트",
          "speaker_id": "2",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100035464.1.12",
          "form": "순대볶음은 매운것만 있던데",
          "original_form": "순대볶음은 매운것만 있던데",
          "speaker_id": "2",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100035464.1.13",
          "form": "여긴백순대로 있어서 주문해봤거든요",
          "original_form": "여긴백순대로 있어서 주문해봤거든요",
          "speaker_id": "2",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100035464.1.14",
          "form": "오 백순대는 안매운거군요?",
          "original_form": "오 백순대는 안매운거군요?",
          "speaker_id": "1",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100035464.1.15",
          "form": "1인분시켰는데 둘이서 먹고도남네요",
          "original_form": "1인분시켰는데 둘이서 먹고도남네요",
          "speaker_id": "2",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100035464.1.16",
          "form": "밀키트라 재료 딱맞게 있으니 남는재료도없고",
          "original_form": "밀키트라 재료 딱맞게 있으니 남는재료도없고",
          "speaker_id": "2",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100035464.1.17",
          "form": "편하네요",
          "original_form": "편하네요",
          "speaker_id": "2",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100035464.1.18",
          "form": "약간 그럴때 희열 느껴져요",
          "original_form": "약간 그럴때 희열 느껴져요",
          "speaker_id": "1",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100035464.1.19",
          "form": "추어탕도 1인분사왔는데 ㅋㅋ둘이먹기충분했을때 ㅋㅋㅋ 같은",
          "original_form": "추어탕도 1인분사왔는데 ㅋㅋ둘이먹기충분했을때 ㅋㅋㅋ 같은",
          "speaker_id": "1",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100035464.1.20",
          "form": "재료는 별거없는데\r\n양배추 쫄면 순대 들깨",
          "original_form": "재료는 별거없는데\r\n양배추 쫄면 순대 들깨",
          "speaker_id": "2",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100035464.1.21",
          "form": "막상 재료 다 준비해서 먹으면 양배추나 쫄면 남아서",
          "original_form": "막상 재료 다 준비해서 먹으면 양배추나 쫄면 남아서",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100035464.1.22",
          "form": "어떻게 소진할지 고민될텐데",
          "original_form": "어떻게 소진할지 고민될텐데",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100035464.1.23",
          "form": "그런고민없으니 좋더라구요",
          "original_form": "그런고민없으니 좋더라구요",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100035464.1.24",
          "form": "맞아요 1인분이라 양 작을줄알았는데 넉넉하고",
          "original_form": "맞아요 1인분이라 양 작을줄알았는데 넉넉하고",
          "speaker_id": "2",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100035464.1.25",
          "form": "맘에들어요 담엔 다른거로 시켜봐야겠어요",
          "original_form": "맘에들어요 담엔 다른거로 시켜봐야겠어요",
          "speaker_id": "2",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100035464.1.26",
          "form": "저는 음 그러고보니 떡좋아하세요?",
          "original_form": "저는 음 그러고보니 떡좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100035464.1.27",
          "form": "떡볶이?떡국?",
          "original_form": "떡볶이?떡국?",
          "speaker_id": "2",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100035464.1.28",
          "form": "좋아하죠",
          "original_form": "좋아하죠",
          "speaker_id": "2",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100035464.1.29",
          "form": "그냥 먹는 인절미같은 떡도좋아해요",
          "original_form": "그냥 먹는 인절미같은 떡도좋아해요",
          "speaker_id": "2",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100035464.1.30",
          "form": "그냥 떡이요 인절미 백설기 같은 ㅋㅋ",
          "original_form": "그냥 떡이요 인절미 백설기 같은 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100035464.1.31",
          "form": "최근 택배맛집 찾아서",
          "original_form": "최근 택배맛집 찾아서",
          "speaker_id": "1",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100035464.1.32",
          "form": "오~~어디요?",
          "original_form": "오~~어디요?",
          "speaker_id": "2",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100035464.1.33",
          "form": "배달시켰는데 만족스러웠어요",
          "original_form": "배달시켰는데 만족스러웠어요",
          "speaker_id": "1",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100035464.1.34",
          "form": "대구 빛깔좋은 떡이요",
          "original_form": "대구 빛깔좋은 떡이요",
          "speaker_id": "1",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100035464.1.35",
          "form": "무조건다천원이에요",
          "original_form": "무조건다천원이에요",
          "speaker_id": "1",
          "time": "20210829 19:18"
        }
      ]
    }
