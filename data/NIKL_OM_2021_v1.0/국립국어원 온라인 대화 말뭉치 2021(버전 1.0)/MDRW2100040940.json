      "utterance": [
        {
          "id": "MDRW2100040940.1.1",
          "form": "저는 쭈꾸미 땡겨요 ㅠㅠ",
          "original_form": "저는 쭈꾸미 땡겨요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100040940.1.2",
          "form": "우왓 쭈꾸미 너무 맛있죠",
          "original_form": "우왓 쭈꾸미 너무 맛있죠",
          "speaker_id": "1",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100040940.1.3",
          "form": "가을에 더 많이 먹는 음식 맞나요?!",
          "original_form": "가을에 더 많이 먹는 음식 맞나요?!",
          "speaker_id": "1",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100040940.1.4",
          "form": "잘모르겠어요!!그냥 쭈삼겹먹구싶어서 ㅋㅋㅋ",
          "original_form": "잘모르겠어요!!그냥 쭈삼겹먹구싶어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100040940.1.5",
          "form": "쭈삼겹에 들어가는 삼겹살은",
          "original_form": "쭈삼겹에 들어가는 삼겹살은",
          "speaker_id": "1",
          "time": "20210905 13:25"
        },
        {
          "id": "MDRW2100040940.1.6",
          "form": "대패삼겹살인가요?",
          "original_form": "대패삼겹살인가요?",
          "speaker_id": "1",
          "time": "20210905 13:25"
        },
        {
          "id": "MDRW2100040940.1.7",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "2",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100040940.1.8",
          "form": "오오 역시!!!",
          "original_form": "오오 역시!!!",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100040940.1.9",
          "form": "좀 얇은 삼겹살이어야",
          "original_form": "좀 얇은 삼겹살이어야",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100040940.1.10",
          "form": "자그마한 쭈꾸미랑 잘 어울리겠어요 ㅎㅎ",
          "original_form": "자그마한 쭈꾸미랑 잘 어울리겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100040940.1.11",
          "form": "맞아요 맞아요 맛있더라구요!!",
          "original_form": "맞아요 맞아요 맛있더라구요!!",
          "speaker_id": "2",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100040940.1.12",
          "form": "조개에도 잘어울리던뎅",
          "original_form": "조개에도 잘어울리던뎅",
          "speaker_id": "2",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100040940.1.13",
          "form": "name2님 정말 드실 줄 아는 분이시군요 ><",
          "original_form": "&name2&님 정말 드실 줄 아는 분이시군요 ><",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100040940.1.14",
          "form": "name2님 어떤 햄버거 가장 좋아하세요?",
          "original_form": "&name2&님 어떤 햄버거 가장 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100040940.1.15",
          "form": "저는 맥날 좋아랍니당",
          "original_form": "저는 맥날 좋아랍니당",
          "speaker_id": "2",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100040940.1.16",
          "form": "애그 불고기가 외케맛있던지요 ㅠㅠ",
          "original_form": "애그 불고기가 외케맛있던지요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040940.1.17",
          "form": "우와 저 안먹어봤는데",
          "original_form": "우와 저 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210905 14:05"
        },
        {
          "id": "MDRW2100040940.1.18",
          "form": "시도해봐야겠어요!",
          "original_form": "시도해봐야겠어요!",
          "speaker_id": "1",
          "time": "20210905 14:05"
        },
        {
          "id": "MDRW2100040940.1.19",
          "form": "저는 주로",
          "original_form": "저는 주로",
          "speaker_id": "1",
          "time": "20210905 14:05"
        },
        {
          "id": "MDRW2100040940.1.20",
          "form": "치킨버거를 먹거든요!",
          "original_form": "치킨버거를 먹거든요!",
          "speaker_id": "1",
          "time": "20210905 14:05"
        },
        {
          "id": "MDRW2100040940.1.21",
          "form": "맘스터치 먹겠군요",
          "original_form": "맘스터치 먹겠군요",
          "speaker_id": "2",
          "time": "20210905 14:09"
        },
        {
          "id": "MDRW2100040940.1.22",
          "form": "맘스터치 아주 사랑하죠",
          "original_form": "맘스터치 아주 사랑하죠",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100040940.1.23",
          "form": "저 원래 최애 버거 브랜드가",
          "original_form": "저 원래 최애 버거 브랜드가",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100040940.1.24",
          "form": "그...어디지 그... 으아아아 이름이 생각안나요",
          "original_form": "그...어디지 그... 으아아아 이름이 생각안나요",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100040940.1.25",
          "form": "버거킹!!!",
          "original_form": "버거킹!!!",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100040940.1.26",
          "form": "버거킹이었는데 버거킹이 요즘 잘 안보이더라구요ㅠ",
          "original_form": "버거킹이었는데 버거킹이 요즘 잘 안보이더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100040940.1.27",
          "form": "아진짜요??",
          "original_form": "아진짜요??",
