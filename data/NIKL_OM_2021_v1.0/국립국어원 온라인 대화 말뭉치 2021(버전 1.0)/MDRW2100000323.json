      "utterance": [
        {
          "id": "MDRW2100000323.1.1",
          "form": "요새도 재택근무해?",
          "original_form": "요새도 재택근무해?",
          "speaker_id": "2",
          "time": "20210818 15:21"
        },
        {
          "id": "MDRW2100000323.1.2",
          "form": "재택으로",
          "original_form": "재택으로",
          "speaker_id": "1",
          "time": "20210818 15:21"
        },
        {
          "id": "MDRW2100000323.1.3",
          "form": "아니 요즘은 안해",
          "original_form": "아니 요즘은 안해",
          "speaker_id": "1",
          "time": "20210818 15:22"
        },
        {
          "id": "MDRW2100000323.1.4",
          "form": "확진자는 더 나오는데",
          "original_form": "확진자는 더 나오는데",
          "speaker_id": "1",
          "time": "20210818 15:22"
        },
        {
          "id": "MDRW2100000323.1.5",
          "form": "재택은 안한다",
          "original_form": "재택은 안한다",
          "speaker_id": "1",
          "time": "20210818 15:22"
        },
        {
          "id": "MDRW2100000323.1.6",
          "form": "우리도야",
          "original_form": "우리도야",
          "speaker_id": "2",
          "time": "20210818 15:22"
        },
        {
          "id": "MDRW2100000323.1.7",
          "form": "재택할 수가 없어 ㅋㅋㅋ",
          "original_form": "재택할 수가 없어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210818 15:22"
        },
        {
          "id": "MDRW2100000323.1.8",
          "form": "이상해",
          "original_form": "이상해",
          "speaker_id": "1",
          "time": "20210818 15:22"
        },
        {
          "id": "MDRW2100000323.1.9",
          "form": "그런데 나 하는일은 재택근무하기 좀 애매한 직업임",
          "original_form": "그런데 나 하는일은 재택근무하기 좀 애매한 직업임",
          "speaker_id": "1",
          "time": "20210818 15:22"
        },
        {
          "id": "MDRW2100000323.1.10",
          "form": "일주일에 하루이틀 정도는 할 만 할거같은데",
          "original_form": "일주일에 하루이틀 정도는 할 만 할거같은데",
          "speaker_id": "2",
          "time": "20210818 15:23"
        },
        {
          "id": "MDRW2100000323.1.11",
          "form": "나도 그래",
          "original_form": "나도 그래",
          "speaker_id": "2",
          "time": "20210818 15:23"
        },
        {
          "id": "MDRW2100000323.1.12",
          "form": "나 팀 바뀐거 얘기했나?",
          "original_form": "나 팀 바뀐거 얘기했나?",
          "speaker_id": "2",
          "time": "20210818 15:23"
        },
        {
          "id": "MDRW2100000323.1.13",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210818 15:23"
        },
        {
          "id": "MDRW2100000323.1.14",
          "form": "무슨팀인데",
          "original_form": "무슨팀인데",
          "speaker_id": "1",
          "time": "20210818 15:23"
        },
        {
          "id": "MDRW2100000323.1.15",
          "form": "인사팀있을땐 재택 할만했는데",
          "original_form": "인사팀있을땐 재택 할만했는데",
          "speaker_id": "2",
          "time": "20210818 15:23"
        },
        {
          "id": "MDRW2100000323.1.16",
          "form": "총무팀으로 자리옮겼어",
          "original_form": "총무팀으로 자리옮겼어",
          "speaker_id": "2",
          "time": "20210818 15:23"
        },
        {
          "id": "MDRW2100000323.1.17",
          "form": "거기도 재택",
          "original_form": "거기도 재택",
          "speaker_id": "1",
          "time": "20210818 15:23"
        },
        {
          "id": "MDRW2100000323.1.18",
          "form": "본부장 부장 맨날봐야되서",
          "original_form": "본부장 부장 맨날봐야되서",
          "speaker_id": "2",
          "time": "20210818 15:23"
        },
        {
          "id": "MDRW2100000323.1.19",
          "form": "가능할 것 같은데",
          "original_form": "가능할 것 같은데",
          "speaker_id": "1",
          "time": "20210818 15:23"
        },
        {
          "id": "MDRW2100000323.1.20",
          "form": "재택할수가없어 ㅋㅋㅋㅋㅋ",
          "original_form": "재택할수가없어 ㅋㅋㅋㅋㅋ",
