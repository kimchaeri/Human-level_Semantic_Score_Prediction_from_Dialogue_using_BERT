      "utterance": [
        {
          "id": "MDRW2100041595.1.1",
          "form": "팥을 엄청 좋아해서",
          "original_form": "팥을 엄청 좋아해서",
          "speaker_id": "2",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041595.1.2",
          "form": "그럼 당연히 좋아하시겠네요!",
          "original_form": "그럼 당연히 좋아하시겠네요!",
          "speaker_id": "1",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041595.1.3",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041595.1.4",
          "form": "아 저도 엄청 좋아해요 겨울엔 꼭 사먹어요",
          "original_form": "아 저도 엄청 좋아해요 겨울엔 꼭 사먹어요",
          "speaker_id": "1",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041595.1.5",
          "form": "갓구워 나온 바삭한거도 맛있지만",
          "original_form": "갓구워 나온 바삭한거도 맛있지만",
          "speaker_id": "2",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041595.1.6",
          "form": "맞아요 거기서서 먹으면 바삭하고 너무 맛있어요ㅠㅠ",
          "original_form": "맞아요 거기서서 먹으면 바삭하고 너무 맛있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041595.1.7",
          "form": "저는 포장해오는동안 살짝 눅눅해진건 그거대로 맛있더라고요 ㅎㅎ",
          "original_form": "저는 포장해오는동안 살짝 눅눅해진건 그거대로 맛있더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041595.1.8",
          "form": "슈크림 붕어빵은 안좋아하세요?",
          "original_form": "슈크림 붕어빵은 안좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041595.1.9",
          "form": "슈크림도 좋아하긴 하는데.. 팥을 더",
          "original_form": "슈크림도 좋아하긴 하는데.. 팥을 더",
          "speaker_id": "2",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041595.1.10",
          "form": "아 팥충청충성이시구나 ㅋㅋㅋ",
          "original_form": "아 팥충청충성이시구나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041595.1.11",
          "form": "네 팥이 클래식하고 더 맛있어요",
          "original_form": "네 팥이 클래식하고 더 맛있어요",
          "speaker_id": "2",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041595.1.12",
          "form": "저는 둘 다 좋아해서 반반씩 사요 늘ㅋㅋㅋ",
          "original_form": "저는 둘 다 좋아해서 반반씩 사요 늘ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:03"
        },
        {
          "id": "MDRW2100041595.1.13",
          "form": "두배로 먹고 좋네요 ㅎㅎ",
          "original_form": "두배로 먹고 좋네요 ㅎㅎ",
          "speaker_id": "2",
