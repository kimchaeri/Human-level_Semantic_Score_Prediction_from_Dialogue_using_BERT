        {
          "id": "MDRW2100021538.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210828 20:10"
        },
        {
          "id": "MDRW2100021538.1.2",
          "form": "안녕하세요~ 네 좋아해요",
          "original_form": "안녕하세요~ 네 좋아해요",
          "speaker_id": "2",
          "time": "20210828 20:11"
        },
        {
          "id": "MDRW2100021538.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210828 20:13"
        },
        {
          "id": "MDRW2100021538.1.4",
          "form": "어디 여행 가보셨어요?",
          "original_form": "어디 여행 가보셨어요?",
          "speaker_id": "2",
          "time": "20210828 20:13"
        },
        {
          "id": "MDRW2100021538.1.5",
          "form": "호주요",
          "original_form": "호주요",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100021538.1.6",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021538.1.7",
          "form": "전 해외 딱 한번밖에 못가봤어요",
          "original_form": "전 해외 딱 한번밖에 못가봤어요",
          "speaker_id": "2",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021538.1.8",
          "form": "호주 여행 얘기 들려주실 수 있을까요?",
          "original_form": "호주 여행 얘기 들려주실 수 있을까요?",
          "speaker_id": "2",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021538.1.9",
          "form": "청정지역이에요",
          "original_form": "청정지역이에요",
          "speaker_id": "1",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100021538.1.10",
          "form": "맑고요",
          "original_form": "맑고요",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021538.1.11",
          "form": "자연경관 구경하는것만으로도 너무 행복할거같아요",
          "original_form": "자연경관 구경하는것만으로도 너무 행복할거같아요",
          "speaker_id": "2",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100021538.1.12",
          "form": "네 사람들도 좋구요",
          "original_form": "네 사람들도 좋구요",
          "speaker_id": "1",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100021538.1.13",
          "form": "음식도 맛있었을까요?",
          "original_form": "음식도 맛있었을까요?",
          "speaker_id": "2",
          "time": "20210828 21:13"
        },
