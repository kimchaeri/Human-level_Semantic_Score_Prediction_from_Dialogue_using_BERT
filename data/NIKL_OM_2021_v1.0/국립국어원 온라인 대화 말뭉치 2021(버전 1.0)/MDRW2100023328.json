      "utterance": [
        {
          "id": "MDRW2100023328.1.1",
          "form": "저번에 샀던 책들 중에 아몬드",
          "original_form": "저번에 샀던 책들 중에 아몬드",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100023328.1.2",
          "form": "중반까진 걍 그랬는데",
          "original_form": "중반까진 걍 그랬는데",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100023328.1.3",
          "form": "다 읽으니까 감동적이군요",
          "original_form": "다 읽으니까 감동적이군요",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100023328.1.4",
          "form": "ㅠ.ㅠ",
          "original_form": "ㅠ.ㅠ",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100023328.1.5",
          "form": "오 다 보셨습니까요??",
          "original_form": "오 다 보셨습니까요??",
          "speaker_id": "2",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100023328.1.6",
          "form": "대단하시네요",
          "original_form": "대단하시네요",
          "speaker_id": "2",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100023328.1.7",
          "form": "저는 아직 읽어보지도 않앗습니다",
          "original_form": "저는 아직 읽어보지도 않앗습니다",
          "speaker_id": "2",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100023328.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100023328.1.9",
          "form": "ㅋㅋㅋㅌㅋㅋㅋㅋㅋ아몬드?",
          "original_form": "ㅋㅋㅋㅌㅋㅋㅋㅋㅋ아몬드?",
          "speaker_id": "1",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100023328.1.10",
          "form": "뭔가 술술 읽힘 그책은",
          "original_form": "뭔가 술술 읽힘 그책은",
          "speaker_id": "1",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100023328.1.11",
          "form": "가끔 소설 중에 진짜 안읽히는 것들도 있다고",
          "original_form": "가끔 소설 중에 진짜 안읽히는 것들도 있다고",
          "speaker_id": "1",
          "time": "20210831 11:18"
        },
        {
          "id": "MDRW2100023328.1.12",
          "form": "지금 모리와 함께하는 화요일 절반까지 읽었다",
          "original_form": "지금 모리와 함께하는 화요일 절반까지 읽었다",
          "speaker_id": "1",
          "time": "20210831 11:18"
        },
        {
          "id": "MDRW2100023328.1.13",
          "form": "엌ㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:42"
        },
        {
          "id": "MDRW2100023328.1.14",
          "form": "나도그거읽는중ㅋㅋㅋ",
          "original_form": "나도그거읽는중ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:42"
        },
        {
          "id": "MDRW2100023328.1.15",
          "form": "오늘공부하고저녁에좀읽어야겟다",
          "original_form": "오늘공부하고저녁에좀읽어야겟다",
          "speaker_id": "2",
          "time": "20210831 11:42"
        },
        {
          "id": "MDRW2100023328.1.16",
          "form": "그게서론만해도30장은되는듯ㅋㅋㅋㅌ",
          "original_form": "그게서론만해도30장은되는듯ㅋㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210831 11:42"
        },
        {
          "id": "MDRW2100023328.1.17",
          "form": "ㅋㅋ사실 읽으면서",
          "original_form": "ㅋㅋ사실 읽으면서",
          "speaker_id": "1",
          "time": "20210831 11:47"
        },
        {
          "id": "MDRW2100023328.1.18",
          "form": "거기 제자가 약간 나랑 비슷한거같애",
          "original_form": "거기 제자가 약간 나랑 비슷한거같애",
          "speaker_id": "1",
          "time": "20210831 11:47"
        },
        {
          "id": "MDRW2100023328.1.19",
          "form": "긍까 뭔가 모리 교수 말을 듣고 바뀌어야하는 상태인 느낌이야",
          "original_form": "긍까 뭔가 모리 교수 말을 듣고 바뀌어야하는 상태인 느낌이야",
          "speaker_id": "1",
          "time": "20210831 11:48"
        },
        {
          "id": "MDRW2100023328.1.20",
          "form": "ㅋㅋㅋㅋ근데 아직은 교수 말에 완전 공감이 가진 않음",
          "original_form": "ㅋㅋㅋㅋ근데 아직은 교수 말에 완전 공감이 가진 않음",
          "speaker_id": "1",
          "time": "20210831 11:48"
        },
        {
          "id": "MDRW2100023328.1.21",
          "form": "다행히도 제자분도 그러셔서(?) ㅋㅋㅋ 계속 읽는중",
          "original_form": "다행히도 제자분도 그러셔서(?) ㅋㅋㅋ 계속 읽는중",
          "speaker_id": "1",
          "time": "20210831 11:48"
        },
        {
          "id": "MDRW2100023328.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ난서론만읽어서모르겟닼ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ난서론만읽어서모르겟닼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:51"
        },
        {
          "id": "MDRW2100023328.1.23",
          "form": "서론이라니 난 이분 때문에 읽기 시작했는뎈ㅋㅋㅋㅋㅋㅋ",
          "original_form": "서론이라니 난 이분 때문에 읽기 시작했는뎈ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:57"
        },
        {
          "id": "MDRW2100023328.1.24",
          "form": "어서 읽자!!!",
          "original_form": "어서 읽자!!!",
          "speaker_id": "1",
          "time": "20210831 11:57"
        },
        {
          "id": "MDRW2100023328.1.25",
          "form": "모리교수가 너무 감성적이야",
          "original_form": "모리교수가 너무 감성적이야",
          "speaker_id": "1",
          "time": "20210831 12:00"
        },
        {
          "id": "MDRW2100023328.1.26",
          "form": "너가 읽어봐야 안다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "너가 읽어봐야 안다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:00"
        },
        {
          "id": "MDRW2100023328.1.27",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210831 12:12"
        },
        {
          "id": "MDRW2100023328.1.28",
          "form": "오늘부터다시읽을게여",
          "original_form": "오늘부터다시읽을게여",
          "speaker_id": "2",
          "time": "20210831 12:12"
        },
        {
          "id": "MDRW2100023328.1.29",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:12"
        },
        {
          "id": "MDRW2100023328.1.30",
          "form": "일주일안에다읽어보겟습니다",
          "original_form": "일주일안에다읽어보겟습니다",
          "speaker_id": "2",
          "time": "20210831 12:13"
        },
        {
          "id": "MDRW2100023328.1.31",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:13"
        },
        {
          "id": "MDRW2100023328.1.32",
          "form": "흠 기대하고 잇겠습니다",
          "original_form": "흠 기대하고 잇겠습니다",
          "speaker_id": "1",
          "time": "20210831 12:15"
        },
        {
          "id": "MDRW2100023328.1.33",
          "form": "아몬드가 재밌어",
          "original_form": "아몬드가 재밌어",
          "speaker_id": "1",
          "time": "20210831 12:15"
        },
        {
          "id": "MDRW2100023328.1.34",
          "form": "그거도 읽으라구 ㅎㅎ",
          "original_form": "그거도 읽으라구 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:15"
        },
        {
          "id": "MDRW2100023328.1.35",
          "form": "당근거래로 위대한 개츠비 삼",
          "original_form": "당근거래로 위대한 개츠비 삼",
          "speaker_id": "1",
          "time": "20210831 12:15"
        },
        {
          "id": "MDRW2100023328.1.36",
          "form": "동생이 영화 재밌다길래 난 책으로 읽어봐야지",
          "original_form": "동생이 영화 재밌다길래 난 책으로 읽어봐야지",
          "speaker_id": "1",
          "time": "20210831 12:15"
        },
        {
          "id": "MDRW2100023328.1.37",
