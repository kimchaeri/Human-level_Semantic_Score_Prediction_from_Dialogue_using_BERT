        {
          "id": "MDRW2100018713.1.1",
          "form": "초코푸들 키우고싶어",
          "original_form": "초코푸들 키우고싶어",
          "speaker_id": "2",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018713.1.2",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "1",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018713.1.3",
          "form": "나는 귀여운 고양이",
          "original_form": "나는 귀여운 고양이",
          "speaker_id": "1",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018713.1.4",
          "form": "꺅",
          "original_form": "꺅",
          "speaker_id": "2",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018713.1.5",
          "form": "우리전에 구출한 name3...",
          "original_form": "우리전에 구출한 &name3&...",
          "speaker_id": "2",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018713.1.6",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018713.1.7",
          "form": "이름이 name3이였지",
          "original_form": "이름이 &name3&이였지",
          "speaker_id": "1",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018713.1.8",
          "form": "너무슬프다",
          "original_form": "너무슬프다",
          "speaker_id": "2",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018713.1.9",
          "form": "유기묘들이 아직도 그렇게 많아",
          "original_form": "유기묘들이 아직도 그렇게 많아",
          "speaker_id": "1",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018713.1.10",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018713.1.11",
          "form": "근데 우린 돈이없다",
          "original_form": "근데 우린 돈이없다",
          "speaker_id": "2",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018713.1.12",
          "form": "키울때 돈없음 안돼",
          "original_form": "키울때 돈없음 안돼",
          "speaker_id": "1",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018713.1.13",
          "form": "당연",
          "original_form": "당연",
          "speaker_id": "2",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018713.1.14",
          "form": "사랑만 있다고 키울수없어",
          "original_form": "사랑만 있다고 키울수없어",
          "speaker_id": "1",
          "time": "20210827 11:16"
        },
        {
          "id": "MDRW2100018713.1.15",
          "form": "진짜마저",
          "original_form": "진짜마저",
          "speaker_id": "2",
          "time": "20210827 11:16"
        },
        {
          "id": "MDRW2100018713.1.16",
          "form": "우리 돈벌면 꼭 키우자",
          "original_form": "우리 돈벌면 꼭 키우자",
          "speaker_id": "1",
          "time": "20210827 11:16"
        },
        {
          "id": "MDRW2100018713.1.17",
          "form": "오키좋아",
          "original_form": "오키좋아",
          "speaker_id": "2",
          "time": "20210827 11:16"
        },
        {
