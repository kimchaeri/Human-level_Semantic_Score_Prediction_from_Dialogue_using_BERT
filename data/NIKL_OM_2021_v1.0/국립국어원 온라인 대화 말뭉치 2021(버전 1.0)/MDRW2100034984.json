      "utterance": [
        {
          "id": "MDRW2100034984.1.1",
          "form": "점심 드셧어요?",
          "original_form": "점심 드셧어요?",
          "speaker_id": "2",
          "time": "20210901 13:33"
        },
        {
          "id": "MDRW2100034984.1.2",
          "form": "배달시켜먹으려구요 점심 드셨나요?",
          "original_form": "배달시켜먹으려구요 점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 13:34"
        },
        {
          "id": "MDRW2100034984.1.3",
          "form": "네 저는 먹엇어요",
          "original_form": "네 저는 먹엇어요",
          "speaker_id": "2",
          "time": "20210901 13:34"
        },
        {
          "id": "MDRW2100034984.1.4",
          "form": "배달음식 뭘 드실 계획이세요?",
          "original_form": "배달음식 뭘 드실 계획이세요?",
          "speaker_id": "2",
          "time": "20210901 13:34"
        },
        {
          "id": "MDRW2100034984.1.5",
          "form": "초밥먹었어요! 뭐드셨나요?",
          "original_form": "초밥먹었어요! 뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100034984.1.6",
          "form": "저는 마늘바게트 먹엇어요 ㅋㅋㅋㅋ",
          "original_form": "저는 마늘바게트 먹엇어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100034984.1.7",
          "form": "무슨초밥 드셧어요??",
          "original_form": "무슨초밥 드셧어요??",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100034984.1.8",
          "form": "오 마늘바게트 맛있죠 ㅋㅋ",
          "original_form": "오 마늘바게트 맛있죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100034984.1.9",
          "form": "연어랑 타다끼 초밥 먹었어요!",
          "original_form": "연어랑 타다끼 초밥 먹었어요!",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100034984.1.10",
          "form": "맛잇겟네요 ㅠㅠ",
          "original_form": "맛잇겟네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100034984.1.11",
          "form": "저녁은 뭐드실거예요?",
          "original_form": "저녁은 뭐드실거예요?",
          "speaker_id": "2",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100034984.1.12",
          "form": "아직 고민중이에여 ㅠㅠ",
          "original_form": "아직 고민중이에여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 18:58"
        },
        {
          "id": "MDRW2100034984.1.13",
          "form": "뭐드실예정이신가요",
          "original_form": "뭐드실예정이신가요",
          "speaker_id": "1",
          "time": "20210901 18:58"
        },
        {
          "id": "MDRW2100034984.1.14",
          "form": "전 이미 먹엇어요ㅋㅋㅋㅋ",
          "original_form": "전 이미 먹엇어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100034984.1.15",
          "form": "생선조림이랑 김치부침개 먹엇어요",
          "original_form": "생선조림이랑 김치부침개 먹엇어요",
          "speaker_id": "2",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100034984.1.16",
          "form": "오 완전 알차게 드셨네요!!",
          "original_form": "오 완전 알차게 드셨네요!!",
          "speaker_id": "1",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100034984.1.17",
          "form": "직접 만들어드셨어요?",
          "original_form": "직접 만들어드셨어요?",
          "speaker_id": "1",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100034984.1.18",
          "form": "네 ㅋㅋㅋㅋ조미료의 힘을 빌려서 ㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ조미료의 힘을 빌려서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100034984.1.19",
          "form": "역시 조미료가 최고죠...!",
          "original_form": "역시 조미료가 최고죠...!",
          "speaker_id": "1",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100034984.1.20",
          "form": "생선조림드실때 무도 드시나요? 전 생선보다 무를 더 많이 먹는 것 같아요ㅋㅋ",
          "original_form": "생선조림드실때 무도 드시나요? 전 생선보다 무를 더 많이 먹는 것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100034984.1.21",
          "form": "양념뱅 무 너무 맛잇죠",
          "original_form": "양념뱅 무 너무 맛잇죠",
          "speaker_id": "2",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100034984.1.22",
          "form": "근데 오늘은 무가 없어서 양파랑 두부를 넣엇어요 ㅠㅠ",
          "original_form": "근데 오늘은 무가 없어서 양파랑 두부를 넣엇어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100034984.1.23",
          "form": "오 두부 넣어도 맛있나봐요 !! 양파도 달달하니 맛있죠",
          "original_form": "오 두부 넣어도 맛있나봐요 !! 양파도 달달하니 맛있죠",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100034984.1.24",
          "form": "국물이 살짝 텁텁해지긴 하지만 두부부침이랑은 또다른 맛이라서 좋아합니다",
          "original_form": "국물이 살짝 텁텁해지긴 하지만 두부부침이랑은 또다른 맛이라서 좋아합니다",
          "speaker_id": "2",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100034984.1.25",
          "form": "담에 두부 넣어서 먹어봐야겠어요 ㅋㅋ 좋네요",
          "original_form": "담에 두부 넣어서 먹어봐야겠어요 ㅋㅋ 좋네요",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100034984.1.26",
          "form": "추천합니다 맛잇어요",
          "original_form": "추천합니다 맛잇어요",
          "speaker_id": "2",
          "time": "20210901 21:42"
        }
      ]
