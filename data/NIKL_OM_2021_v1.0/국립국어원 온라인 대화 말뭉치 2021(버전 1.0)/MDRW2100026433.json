        {
          "id": "MDRW2100026433.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026433.1.2",
          "form": "아까 그 꽃게를 쪄먹었어요",
          "original_form": "아까 그 꽃게를 쪄먹었어요",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026433.1.3",
          "form": "살아있어서 손질이 ㅎㄷㄷ",
          "original_form": "살아있어서 손질이 ㅎㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026433.1.4",
          "form": "대단해요",
          "original_form": "대단해요",
          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026433.1.5",
          "form": "저녁은 뭐드셨나요?",
          "original_form": "저녁은 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026433.1.6",
          "form": "어묵국이요",
          "original_form": "어묵국이요",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026433.1.7",
          "form": "시원하게 어묵국 좋지요",
          "original_form": "시원하게 어묵국 좋지요",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026433.1.8",
          "form": "배달 자주 시켜드시나요?",
          "original_form": "배달 자주 시켜드시나요?",
          "speaker_id": "2",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026433.1.9",
          "form": "네 자주요",
          "original_form": "네 자주요",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026433.1.10",
          "form": "주로 뭐 시켜드시나요?",
          "original_form": "주로 뭐 시켜드시나요?",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026433.1.11",
          "form": "메뉴 고르는게 일이에요",
          "original_form": "메뉴 고르는게 일이에요",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026433.1.12",
          "form": "치킨요",
          "original_form": "치킨요",
          "speaker_id": "1",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026433.1.13",
          "form": "오 어디 치킨 젤 좋아하세요",
          "original_form": "오 어디 치킨 젤 좋아하세요",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026433.1.14",
          "form": "네네요",
          "original_form": "네네요",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026433.1.15",
          "form": "오 저는 네네는 파닭 좋더라구요",
          "original_form": "오 저는 네네는 파닭 좋더라구요",
