      "utterance": [
        {
          "id": "MDRW2100016156.1.1",
          "form": "근데 세상에 무소음이란 말자체가 ㅋ모순같아요",
          "original_form": "근데 세상에 무소음이란 말자체가 ㅋ모순같아요",
          "speaker_id": "1",
          "time": "20210826 10:43"
        },
        {
          "id": "MDRW2100016156.1.2",
          "form": "무소음말고 저소음인걸로",
          "original_form": "무소음말고 저소음인걸로",
          "speaker_id": "1",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100016156.1.3",
          "form": "여긴 제법 햇빛나네요",
          "original_form": "여긴 제법 햇빛나네요",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016156.1.4",
          "form": "양산 필수 흐",
          "original_form": "양산 필수 흐",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016156.1.5",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016156.1.6",
          "form": "어제 그렇게 쏟아붓더니",
          "original_form": "어제 그렇게 쏟아붓더니",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016156.1.7",
          "form": "오늘은 해가 떴더라구요ㅎ",
          "original_form": "오늘은 해가 떴더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016156.1.8",
          "form": "그래도 아직 비기운이 남아있나 습한거같아요",
          "original_form": "그래도 아직 비기운이 남아있나 습한거같아요",
          "speaker_id": "2",
          "time": "20210826 11:12"
        },
        {
          "id": "MDRW2100016156.1.9",
          "form": "어떠세요?",
          "original_form": "어떠세요?",
          "speaker_id": "2",
          "time": "20210826 11:12"
        },
        {
          "id": "MDRW2100016156.1.10",
          "form": "습한데 햇빛은 따가워요",
          "original_form": "습한데 햇빛은 따가워요",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016156.1.11",
          "form": "으어 신발 젖었네요",
          "original_form": "으어 신발 젖었네요",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016156.1.12",
          "form": "물 웅덩이 같은 거 밟으셨나요",
          "original_form": "물 웅덩이 같은 거 밟으셨나요",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016156.1.13",
          "form": "네 ㅜ 그리고 덥네요",
          "original_form": "네 ㅜ 그리고 덥네요",
          "speaker_id": "1",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100016156.1.14",
          "form": "한참 햇빛이 ㅋㅋ",
          "original_form": "한참 햇빛이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100016156.1.15",
          "form": "썬크림 바르셨나요",
          "original_form": "썬크림 바르셨나요",
          "speaker_id": "2",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100016156.1.16",
          "form": "오늘 같은 날은 필수라더라구요",
          "original_form": "오늘 같은 날은 필수라더라구요",
          "speaker_id": "2",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100016156.1.17",
          "form": "아뇨 그냥 포기했어요 올여름",
          "original_form": "아뇨 그냥 포기했어요 올여름",
          "speaker_id": "1",
          "time": "20210826 11:21"
        },
        {
          "id": "MDRW2100016156.1.18",
          "form": "마스크때매",
          "original_form": "마스크때매",
          "speaker_id": "1",
          "time": "20210826 11:21"
        },
        {
          "id": "MDRW2100016156.1.19",
          "form": "아ㅠㅠ 하긴 마스크 쓰고 선크림바르기엔 많이 답답하더라구요",
          "original_form": "아ㅠㅠ 하긴 마스크 쓰고 선크림바르기엔 많이 답답하더라구요",
          "speaker_id": "2",
          "time": "20210826 11:22"
        },
        {
          "id": "MDRW2100016156.1.20",
          "form": "그냥 이미탔어요 ㅋㅋ",
          "original_form": "그냥 이미탔어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016156.1.21",
          "form": "자연의 미를 추구하자며 ㅋㅋㅋ",
          "original_form": "자연의 미를 추구하자며 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016156.1.22",
          "form": "포기하셨구나..",
          "original_form": "포기하셨구나..",
          "speaker_id": "2",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016156.1.23",
          "form": "ㅋㅋㅋㅋ  그럴 수 있어요",
          "original_form": "ㅋㅋㅋㅋ  그럴 수 있어요",
          "speaker_id": "2",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016156.1.24",
          "form": "오늘 습하고 더워서 더 불쾌지수가 높아지는거같아요",
          "original_form": "오늘 습하고 더워서 더 불쾌지수가 높아지는거같아요",
          "speaker_id": "2",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100016156.1.25",
          "form": "여긴 동남아에요 ㅋㅋ",
          "original_form": "여긴 동남아에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016156.1.26",
          "form": "저건 소나기가 아닌스콜이라며",
          "original_form": "저건 소나기가 아닌스콜이라며",
          "speaker_id": "1",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016156.1.27",
          "form": "아 맞아요ㅠ",
          "original_form": "아 맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100016156.1.28",
          "form": "요즘 너무 이상기후 심해요",
          "original_form": "요즘 너무 이상기후 심해요",
          "speaker_id": "2",
          "time": "20210826 11:37"
        },
        {
          "id": "MDRW2100016156.1.29",
          "form": "운동화 빨고 싶은데 또 언제비가올지",
          "original_form": "운동화 빨고 싶은데 또 언제비가올지",
          "speaker_id": "1",
          "time": "20210826 13:30"
        },
        {
          "id": "MDRW2100016156.1.30",
          "form": "일기예보도 못믿겠네요",
          "original_form": "일기예보도 못믿겠네요",
          "speaker_id": "1",
          "time": "20210826 13:30"
        }
      ]
    }
  ]
