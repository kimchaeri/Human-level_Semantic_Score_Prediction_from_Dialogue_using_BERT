        },
        {
          "id": "MDRW2100018663.1.2",
          "form": "주룩주룩 주르륵",
          "original_form": "주룩주룩 주르륵",
          "speaker_id": "2",
          "time": "20210827 10:41"
        },
        {
          "id": "MDRW2100018663.1.3",
          "form": "비와서ㅡㅡ",
          "original_form": "비와서ㅡㅡ",
          "speaker_id": "1",
          "time": "20210827 11:14"
        },
        {
          "id": "MDRW2100018663.1.4",
          "form": "뭐지?",
          "original_form": "뭐지?",
          "speaker_id": "1",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018663.1.5",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018663.1.6",
          "form": "ㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018663.1.7",
          "form": "일기예보 바껴서",
          "original_form": "일기예보 바껴서",
          "speaker_id": "2",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018663.1.8",
          "form": "캠핑다시 잡았는데",
          "original_form": "캠핑다시 잡았는데",
          "speaker_id": "2",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018663.1.9",
          "form": "비와서 화나네",
          "original_form": "비와서 화나네",
          "speaker_id": "2",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018663.1.10",
          "form": "새텐트",
          "original_form": "새텐트",
          "speaker_id": "2",
          "time": "20210827 11:16"
        },
        {
          "id": "MDRW2100018663.1.11",
          "form": "쳐야되는디",
          "original_form": "쳐야되는디",
          "speaker_id": "2",
          "time": "20210827 11:16"
        },
        {
          "id": "MDRW2100018663.1.12",
          "form": "그릫네 이버주잖아",
          "original_form": "그릫네 이버주잖아",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018663.1.13",
          "form": "이번주 해가 없나?",
          "original_form": "이번주 해가 없나?",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018663.1.14",
          "form": "엉 오늘간다 언닠ㅋㅋㅋㄱㅋㅋㅋ",
          "original_form": "엉 오늘간다 언닠ㅋㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018663.1.15",
          "form": "해 있었는데",
          "original_form": "해 있었는데",
          "speaker_id": "2",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018663.1.16",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018663.1.17",
          "form": "계속 쭈 륵 쭈르고 오는 건 아니고",
          "original_form": "계속 쭈 륵 쭈르고 오는 건 아니고",
          "speaker_id": "2",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018663.1.18",
          "form": "그렇구만",
          "original_form": "그렇구만",
          "speaker_id": "1",
          "time": "20210827 11:18"
        },
        {
          "id": "MDRW2100018663.1.19",
          "form": "휴...일기예보...",
          "original_form": "휴...일기예보...",
          "speaker_id": "2",
          "time": "20210827 11:23"
        },
        {
          "id": "MDRW2100018663.1.20",
          "form": "계속 바뀌네",
          "original_form": "계속 바뀌네",
          "speaker_id": "2",
          "time": "20210827 11:23"
        },
        {
          "id": "MDRW2100018663.1.21",
          "form": "애들 우산 챙겨 보냈는교??",
          "original_form": "애들 우산 챙겨 보냈는교??",
          "speaker_id": "2",
          "time": "20210827 11:23"
        },
        {
          "id": "MDRW2100018663.1.22",
          "form": "아니ㅜ",
          "original_form": "아니ㅜ",
          "speaker_id": "1",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100018663.1.23",
          "form": "우산 없다둘다",
          "original_form": "우산 없다둘다",
          "speaker_id": "1",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100018663.1.24",
          "form": "ㅋㅋㄱㄱ 예비우산도 없어요??",
          "original_form": "ㅋㅋㄱㄱ 예비우산도 없어요??",
          "speaker_id": "2",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018663.1.25",
          "form": "어 지금안오네ㅋ",
          "original_form": "어 지금안오네ㅋ",
          "speaker_id": "1",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100018663.1.26",
          "form": "옹 다행이구먼 ㅎㅎ",
          "original_form": "옹 다행이구먼 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018663.1.27",
          "form": "매미 운다",
          "original_form": "매미 운다",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018663.1.28",
          "form": "아 언니 오늘 아파서 쉬는거여??",
          "original_form": "아 언니 오늘 아파서 쉬는거여??",
          "speaker_id": "2",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018663.1.29",
          "form": "엉 그렇지",
          "original_form": "엉 그렇지",
          "speaker_id": "1",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018663.1.30",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018663.1.31",
          "form": "온몸이 쑤신다",
          "original_form": "온몸이 쑤신다",
          "speaker_id": "1",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018663.1.32",
          "form": "언니 몸 좀 괜찮아요??",
          "original_form": "언니 몸 좀 괜찮아요??",
          "speaker_id": "2",
          "time": "20210829 00:52"
        },
        {
          "id": "MDRW2100018663.1.33",
          "form": "어제는 비와서 텐트치기 힘들었는데",
          "original_form": "어제는 비와서 텐트치기 힘들었는데",
          "speaker_id": "2",
          "time": "20210829 00:52"
        },
        {
          "id": "MDRW2100018663.1.34",
          "form": "오늘은 날씨가 좋아서 물놀이하고 잘 놀았구만요",
          "original_form": "오늘은 날씨가 좋아서 물놀이하고 잘 놀았구만요",
          "speaker_id": "2",
          "time": "20210829 00:52"
        },
        {
          "id": "MDRW2100018663.1.35",
          "form": "어제날씨좋았는듸ㅣ",
          "original_form": "어제날씨좋았는듸ㅣ",
          "speaker_id": "1",
          "time": "20210829 09:38"
        },
        {
          "id": "MDRW2100018663.1.36",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "2",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100018663.1.37",
          "form": "텐트 칠때 비오고",
          "original_form": "텐트 칠때 비오고",
          "speaker_id": "2",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100018663.1.38",
          "form": "텐트 걷을라니 비오네;;;",
          "original_form": "텐트 걷을라니 비오네;;;",
          "speaker_id": "2",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100018663.1.39",
          "form": "아;;;",
          "original_form": "아;;;",
          "speaker_id": "1",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100018663.1.40",
          "form": "지금 비오네이 우짜노",
          "original_form": "지금 비오네이 우짜노",
          "speaker_id": "1",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100018663.1.41",
          "form": "우비입고 ㅋㅋㅋㅋㄱ",
          "original_form": "우비입고 ㅋㅋㅋㅋㄱ",
