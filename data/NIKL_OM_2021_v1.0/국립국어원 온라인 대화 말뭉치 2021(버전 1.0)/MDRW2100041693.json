      "utterance": [
        {
          "id": "MDRW2100041693.1.1",
          "form": "헐 좀 옅어진듯",
          "original_form": "헐 좀 옅어진듯",
          "speaker_id": "2",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100041693.1.2",
          "form": "여름이라 안나가는 김에 햇빛도 덜보고 있어",
          "original_form": "여름이라 안나가는 김에 햇빛도 덜보고 있어",
          "speaker_id": "1",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100041693.1.3",
          "form": "좋네요 굿굿 ㅎㅎ",
          "original_form": "좋네요 굿굿 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041693.1.4",
          "form": "딸이 이런것도 주고 좋다",
          "original_form": "딸이 이런것도 주고 좋다",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041693.1.5",
          "form": "아들은 하나도 안해주니까요!!",
          "original_form": "아들은 하나도 안해주니까요!!",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041693.1.6",
          "form": "딸이 예쁘지",
          "original_form": "딸이 예쁘지",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041693.1.7",
          "form": "근데 그거 비타민A인가 그러던데",
          "original_form": "근데 그거 비타민A인가 그러던데",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041693.1.8",
          "form": "피부로도 흡수되나보다",
          "original_form": "피부로도 흡수되나보다",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041693.1.9",
          "form": "잘됐지뭐",
          "original_form": "잘됐지뭐",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041693.1.10",
          "form": "ㅎㅎ 엄마 좋아하셔서 기쁘네요",
          "original_form": "ㅎㅎ 엄마 좋아하셔서 기쁘네요",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041693.1.11",
          "form": "너덕분이야 젊어지고",
          "original_form": "너덕분이야 젊어지고",
          "speaker_id": "1",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041693.1.12",
          "form": "이제 머리만 남았네요 염색 ㅋ",
          "original_form": "이제 머리만 남았네요 염색 ㅋ",
          "speaker_id": "2",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041693.1.13",
          "form": "염색은 엄마 머리 빠져서 안돼",
          "original_form": "염색은 엄마 머리 빠져서 안돼",
          "speaker_id": "1",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100041693.1.14",
          "form": "그거 말구 이번에ㅜ나온 샴푸가",
          "original_form": "그거 말구 이번에ㅜ나온 샴푸가",
          "speaker_id": "2",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100041693.1.15",
          "form": "머리 안빠지게 하고도 잘되거든요",
          "original_form": "머리 안빠지게 하고도 잘되거든요",
          "speaker_id": "2",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100041693.1.16",
          "form": "그런게 어딨냐",
          "original_form": "그런게 어딨냐",
          "speaker_id": "1",
