      "utterance": [
        {
          "id": "MDRW2100037637.1.1",
          "form": "앜ㅋㅋㅋㅋ네",
          "original_form": "앜ㅋㅋㅋㅋ네",
          "speaker_id": "2",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100037637.1.2",
          "form": "그거 한 10분 하나요?",
          "original_form": "그거 한 10분 하나요?",
          "speaker_id": "1",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100037637.1.3",
          "form": "후기 주세요",
          "original_form": "후기 주세요",
          "speaker_id": "2",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100037637.1.4",
          "form": "넼ㅋㅋ 집에서 도전입니다",
          "original_form": "넼ㅋㅋ 집에서 도전입니다",
          "speaker_id": "1",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100037637.1.5",
          "form": "그랬던거 같아요",
          "original_form": "그랬던거 같아요",
          "speaker_id": "2",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100037637.1.6",
          "form": "오 딱 좋군요 ㅎㅎ",
          "original_form": "오 딱 좋군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 14:00"
        },
        {
          "id": "MDRW2100037637.1.7",
          "form": "그쵸, 사실 힘들다고 했지만",
          "original_form": "그쵸, 사실 힘들다고 했지만",
          "speaker_id": "2",
          "time": "20210901 14:00"
        },
        {
          "id": "MDRW2100037637.1.8",
          "form": "걍 숨이 좀 찬다 정도긴 해요",
          "original_form": "걍 숨이 좀 찬다 정도긴 해요",
          "speaker_id": "2",
          "time": "20210901 14:00"
        },
        {
          "id": "MDRW2100037637.1.9",
          "form": "아 숨이 차는게 힘든거죠 ㅋㅋㅋ",
          "original_form": "아 숨이 차는게 힘든거죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100037637.1.10",
          "form": "그것만으로 충격이긴 했지만요 ㅋㅋㅋ",
          "original_form": "그것만으로 충격이긴 했지만요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100037637.1.11",
          "form": "그렇긴 하죠 ㅋㅋㅋ",
          "original_form": "그렇긴 하죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100037637.1.12",
          "form": "어렸을때는 안힘들었는데",
          "original_form": "어렸을때는 안힘들었는데",
          "speaker_id": "1",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100037637.1.13",
          "form": "그래도 완주할만 합니다",
          "original_form": "그래도 완주할만 합니다",
          "speaker_id": "2",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100037637.1.14",
          "form": "체력의 차이가 ㅋㅋ",
          "original_form": "체력의 차이가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100037637.1.15",
          "form": "검색하니까",
          "original_form": "검색하니까",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100037637.1.16",
          "form": "3회 연속버전이 있네요..?",
          "original_form": "3회 연속버전이 있네요..?",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100037637.1.17",
          "form": "오, 그건 진짜 운동되겠다",
          "original_form": "오, 그건 진짜 운동되겠다",
          "speaker_id": "2",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100037637.1.18",
          "form": "가능할까욬ㅋㅋ",
          "original_form": "가능할까욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100037637.1.19",
          "form": "한번 다 하고 멈출듯한.. 예감",
          "original_form": "한번 다 하고 멈출듯한.. 예감",
          "speaker_id": "1",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100037637.1.20",
          "form": "ㅋㅋㅋㅋ 한번 트라이~트라이~",
          "original_form": "ㅋㅋㅋㅋ 한번 트라이~트라이~",
