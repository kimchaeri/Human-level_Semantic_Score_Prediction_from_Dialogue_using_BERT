      "utterance": [
        {
          "id": "MDRW2100033701.1.1",
          "form": "코로나 백신 맞으셨어요?",
          "original_form": "코로나 백신 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100033701.1.2",
          "form": "네 저 2차 기다려요 금요일",
          "original_form": "네 저 2차 기다려요 금요일",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100033701.1.3",
          "form": "와 맞은지 꽤 되셨네요ㅠㅠ",
          "original_form": "와 맞은지 꽤 되셨네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100033701.1.4",
          "form": "두달기다렸어요",
          "original_form": "두달기다렸어요",
          "speaker_id": "2",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100033701.1.5",
          "form": "저는 다음주 1찬데",
          "original_form": "저는 다음주 1찬데",
          "speaker_id": "1",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100033701.1.6",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100033701.1.7",
          "form": "곧이네요",
          "original_form": "곧이네요",
          "speaker_id": "2",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100033701.1.8",
          "form": "네~~ 무서워요ㅠ1차 뭐맞으셨어요?",
          "original_form": "네~~ 무서워요ㅠ1차 뭐맞으셨어요?",
          "speaker_id": "1",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100033701.1.9",
          "form": "아스트라제네카요",
          "original_form": "아스트라제네카요",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033701.1.10",
          "form": "그럼2차도 az세요?",
          "original_form": "그럼2차도 az세요?",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033701.1.11",
          "form": "모더나래요  저는 부작용 없었어서 괜찮았어요",
          "original_form": "모더나래요  저는 부작용 없었어서 괜찮았어요",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033701.1.12",
          "form": "아 아까뉴스기사에서",
          "original_form": "아 아까뉴스기사에서",
          "speaker_id": "1",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100033701.1.13",
          "form": "1차 아제맞고 2차 모더나 안된다고",
          "original_form": "1차 아제맞고 2차 모더나 안된다고",
          "speaker_id": "1",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100033701.1.14",
          "form": "봤는데요?",
          "original_form": "봤는데요?",
          "speaker_id": "1",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100033701.1.15",
          "form": "강릉 백신 오접종ㅠ기사요",
          "original_form": "강릉 백신 오접종ㅠ기사요",
          "speaker_id": "1",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100033701.1.16",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "2",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100033701.1.17",
          "form": "첨알았네요",
          "original_form": "첨알았네요",
          "speaker_id": "2",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100033701.1.18",
          "form": "근데그게 바꿔지나요?",
          "original_form": "근데그게 바꿔지나요?",
          "speaker_id": "2",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100033701.1.19",
          "form": "모르겠어요ㅠ그래서 지금 검색해보면 다들 되냐고 묻고 난리던데ㅠ",
          "original_form": "모르겠어요ㅠ그래서 지금 검색해보면 다들 되냐고 묻고 난리던데ㅠ",
          "speaker_id": "1",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100033701.1.20",
          "form": "뭐가맞는지 모르겠어요ㅡㅜ교차접종은 화이자만 된다고봤는데",
          "original_form": "뭐가맞는지 모르겠어요ㅡㅜ교차접종은 화이자만 된다고봤는데",
          "speaker_id": "1",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100033701.1.21",
          "form": "어렵네요 접종이ㅜ",
          "original_form": "어렵네요 접종이ㅜ",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100033701.1.22",
          "form": "확연하게 아는게 아니니 카더라도 많고ㅜ",
          "original_form": "확연하게 아는게 아니니 카더라도 많고ㅜ",
          "speaker_id": "2",
          "time": "20210824 23:36"
        }
      ]
