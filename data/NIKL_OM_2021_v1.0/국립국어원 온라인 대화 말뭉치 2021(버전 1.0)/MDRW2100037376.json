      "utterance": [
        {
          "id": "MDRW2100037376.1.1",
          "form": "그만큼이나 오래됐어...??",
          "original_form": "그만큼이나 오래됐어...??",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037376.1.2",
          "form": "나는 한 일주일정도 지난쥴 알았어",
          "original_form": "나는 한 일주일정도 지난쥴 알았어",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037376.1.3",
          "form": "심지어 조립은 내가해야하는건데.",
          "original_form": "심지어 조립은 내가해야하는건데.",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037376.1.4",
          "form": "정말 해외직구상품운 다시 생각해봐야해",
          "original_form": "정말 해외직구상품운 다시 생각해봐야해",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037376.1.5",
          "form": "그건 드라이버가 필요하니?",
          "original_form": "그건 드라이버가 필요하니?",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037376.1.6",
          "form": "좀 큰 스토어같던데 영 별로다",
          "original_form": "좀 큰 스토어같던데 영 별로다",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037376.1.7",
          "form": "그런건 보통 미니 간이로 제공해줘",
          "original_form": "그런건 보통 미니 간이로 제공해줘",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037376.1.8",
          "form": "직구한거였구나",
          "original_form": "직구한거였구나",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037376.1.9",
          "form": "그럼 통관번호같은거도 적었어?",
          "original_form": "그럼 통관번호같은거도 적었어?",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037376.1.10",
          "form": "한국 제품은 잘 없어",
          "original_form": "한국 제품은 잘 없어",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037376.1.11",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037376.1.12",
          "form": "나는 직구마스터야",
          "original_form": "나는 직구마스터야",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037376.1.13",
          "form": "한국사람들이 책을 안읽어서 그론가",
          "original_form": "한국사람들이 책을 안읽어서 그론가",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037376.1.14",
          "form": "한번 발급해두면 편해!",
          "original_form": "한번 발급해두면 편해!",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037376.1.15",
          "form": "지금 한국에 도착은했대??",
          "original_form": "지금 한국에 도착은했대??",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037376.1.16",
          "form": "중국인보단 많이 읽을거야",
          "original_form": "중국인보단 많이 읽을거야",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037376.1.17",
          "form": "비율상",
          "original_form": "비율상",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037376.1.18",
          "form": "그것도 몰라",
          "original_form": "그것도 몰라",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037376.1.19",
          "form": "비율상 ㅠ",
          "original_form": "비율상 ㅠ",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037376.1.20",
          "form": "안내드린 송장으로 알려준다는데 안내해준적이 없어",
          "original_form": "안내드린 송장으로 알려준다는데 안내해준적이 없어",
          "speaker_id": "1",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037376.1.21",
          "form": "한국에 도착하면 아마 관세청에서 연락을 줄꺼야",
          "original_form": "한국에 도착하면 아마 관세청에서 연락을 줄꺼야",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037376.1.22",
          "form": "사기는 아니겠지?",
          "original_form": "사기는 아니겠지?",
          "speaker_id": "1",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037376.1.23",
          "form": "그런걸보면 아직 외국인가봐",
          "original_form": "그런걸보면 아직 외국인가봐",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037376.1.24",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037376.1.25",
          "form": "설마~~",
          "original_form": "설마~~",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037376.1.26",
          "form": "나 오늘 아침에 머ㅓ 잘못샀다가 식겁했어",
          "original_form": "나 오늘 아침에 머ㅓ 잘못샀다가 식겁했어",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037376.1.27",
          "form": "너는 구글에서 뭐 구입할 때 신중하게 해!",
          "original_form": "너는 구글에서 뭐 구입할 때 신중하게 해!",
          "speaker_id": "1",
          "time": "20210831 23:00"
        }
      ]
    }
