      "utterance": [
        {
          "id": "MDRW2100028832.1.1",
          "form": "여행 좋아하세요",
          "original_form": "여행 좋아하세요",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028832.1.2",
          "form": "여행 좋아합니다!",
          "original_form": "여행 좋아합니다!",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028832.1.3",
          "form": "여행 계획 있으세요",
          "original_form": "여행 계획 있으세요",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028832.1.4",
          "form": "국내는 부산 가고 싶어요! 겨울 바다 보고 싶네요",
          "original_form": "국내는 부산 가고 싶어요! 겨울 바다 보고 싶네요",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028832.1.5",
          "form": "계획 있으신가요?",
          "original_form": "계획 있으신가요?",
          "speaker_id": "2",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100028832.1.6",
          "form": "저도 바다 좋아해요~",
          "original_form": "저도 바다 좋아해요~",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028832.1.7",
          "form": "전 여수 밤바다로ㅋㅋㅋ",
          "original_form": "전 여수 밤바다로ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028832.1.8",
          "form": "여수 요즘 많이 가시더라구요",
          "original_form": "여수 요즘 많이 가시더라구요",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028832.1.9",
          "form": "여수 가보셨나요?",
          "original_form": "여수 가보셨나요?",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028832.1.10",
          "form": "예전에 갔었는데 아기가 어려서",
          "original_form": "예전에 갔었는데 아기가 어려서",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028832.1.11",
          "form": "거의 숙소에만 있었어요ㅠ",
          "original_form": "거의 숙소에만 있었어요ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028832.1.12",
          "form": "앗 그렇군요ㅠ",
          "original_form": "앗 그렇군요ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028832.1.13",
          "form": "최근에 가신 여행지 있으시나요?",
          "original_form": "최근에 가신 여행지 있으시나요?",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028832.1.14",
          "form": "서해 다녀왔어요",
          "original_form": "서해 다녀왔어요",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028832.1.15",
          "form": "갯벌 체험하러 갔는데",
          "original_form": "갯벌 체험하러 갔는데",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028832.1.16",
          "form": "갯벌이 다 죽었더라구요;;",
          "original_form": "갯벌이 다 죽었더라구요;;",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028832.1.17",
          "form": "날이 더워서 그런걸까요ㅠㅠ 아니 갯벌이 죽을 정도면...",
          "original_form": "날이 더워서 그런걸까요ㅠㅠ 아니 갯벌이 죽을 정도면...",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028832.1.18",
          "form": "조회가 하나도 없어요",
          "original_form": "조회가 하나도 없어요",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028832.1.19",
          "form": "다 껍데기만",
          "original_form": "다 껍데기만",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028832.1.20",
          "form": "6시 내 고향은",
          "original_form": "6시 내 고향은",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028832.1.21",
          "form": "조작이었던 건가요ㅋㅋㅋㅋㅋ",
          "original_form": "조작이었던 건가요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028832.1.22",
          "form": "앗ㅋㅋㅋ미리 조개를 뿌려서ㅠㅠㅋㅋ",
          "original_form": "앗ㅋㅋㅋ미리 조개를 뿌려서ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028832.1.23",
          "form": "그 생각을 했어요ㅋㅋㅋㅋ",
          "original_form": "그 생각을 했어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028832.1.24",
          "form": "아니면 저희가 기술이 없었을 수도..",
          "original_form": "아니면 저희가 기술이 없었을 수도..",
          "speaker_id": "1",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028832.1.25",
          "form": "고생 많이 하셨겠어요ㅠㅠ",
          "original_form": "고생 많이 하셨겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028832.1.26",
          "form": "갯벌 체험 안해봤는데 뭔가 힘들거 같아요...",
          "original_form": "갯벌 체험 안해봤는데 뭔가 힘들거 같아요...",
          "speaker_id": "2",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028832.1.27",
          "form": "애들 때문에 갔지,,",
          "original_form": "애들 때문에 갔지,,",
          "speaker_id": "1",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028832.1.28",
          "form": "추천 안해요ㅋㅋㅋ",
          "original_form": "추천 안해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:30"
        }
      ]
    }
  ]
