      "utterance": [
        {
          "id": "MDRW2100008503.1.1",
          "form": "언니랑 형부랑 백신 맞은거 괜찮아?",
          "original_form": "언니랑 형부랑 백신 맞은거 괜찮아?",
          "speaker_id": "1",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008503.1.2",
          "form": "응 낚시도 안가고 집콕하면서 잘 잇다 문제는 나지",
          "original_form": "응 낚시도 안가고 집콕하면서 잘 잇다 문제는 나지",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008503.1.3",
          "form": "어쩔 생각이야?",
          "original_form": "어쩔 생각이야?",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008503.1.4",
          "form": "모르겠다",
          "original_form": "모르겠다",
          "speaker_id": "2",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008503.1.5",
          "form": "나도 사실 그렇다",
          "original_form": "나도 사실 그렇다",
          "speaker_id": "1",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008503.1.6",
          "form": "청소는 잘 했어?",
          "original_form": "청소는 잘 했어?",
          "speaker_id": "2",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008503.1.7",
          "form": "아침에  일어나서 잠시 쉬고 청소했다",
          "original_form": "아침에  일어나서 잠시 쉬고 청소했다",
          "speaker_id": "1",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100008503.1.8",
          "form": "다 같이 대청소하는거지?",
          "original_form": "다 같이 대청소하는거지?",
          "speaker_id": "2",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100008503.1.9",
          "form": "난 청소가 제일 싫다",
          "original_form": "난 청소가 제일 싫다",
          "speaker_id": "1",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100008503.1.10",
          "form": "같이 하긴 하는데... 내가 거실 전담해서 함",
          "original_form": "같이 하긴 하는데... 내가 거실 전담해서 함",
          "speaker_id": "1",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100008503.1.11",
          "form": "설거지 보 다 는 괜찮던데",
          "original_form": "설거지 보 다 는 괜찮던데",
          "speaker_id": "2",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100008503.1.12",
          "form": "난 설거지가 낫다ㅋ",
          "original_form": "난 설거지가 낫다ㅋ",
          "speaker_id": "1",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008503.1.13",
          "form": "먼지털고 하는게 일이긴 하지",
          "original_form": "먼지털고 하는게 일이긴 하지",
          "speaker_id": "2",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008503.1.14",
          "form": "그래서 로봇청소기 사고 싶긴 해",
          "original_form": "그래서 로봇청소기 사고 싶긴 해",
          "speaker_id": "1",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008503.1.15",
          "form": "가전제품이 가사일 도워주면 좋지",
          "original_form": "가전제품이 가사일 도워주면 좋지",
          "speaker_id": "2",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008503.1.16",
          "form": "물 걸 레 달린 로봇청소기 사면 엄청 편하대",
          "original_form": "물 걸 레 달린 로봇청소기 사면 엄청 편하대",
          "speaker_id": "1",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008503.1.17",
          "form": "근데 그러면 고장 잘  난다",
          "original_form": "근데 그러면 고장 잘  난다",
          "speaker_id": "2",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008503.1.18",
          "form": "얼마 못쓴다",
          "original_form": "얼마 못쓴다",
          "speaker_id": "2",
          "time": "20210822 16:21"
        }
      ]
