      "utterance": [
        {
          "id": "MDRW2100029348.1.1",
          "form": "다행히 케이스는 저렴하게 구매한 ㅌ투명이라서 괜찮아요",
          "original_form": "다행히 케이스는 저렴하게 구매한 ㅌ투명이라서 괜찮아요",
          "speaker_id": "2",
          "time": "20210903 02:22"
        },
        {
          "id": "MDRW2100029348.1.2",
          "form": "아 그거 변색안됫어요 ?",
          "original_form": "아 그거 변색안됫어요 ?",
          "speaker_id": "1",
          "time": "20210903 02:22"
        },
        {
          "id": "MDRW2100029348.1.3",
          "form": "아직은요",
          "original_form": "아직은요",
          "speaker_id": "2",
          "time": "20210903 02:23"
        },
        {
          "id": "MDRW2100029348.1.4",
          "form": "1~2달 사이정도 된거 같아요",
          "original_form": "1~2달 사이정도 된거 같아요",
          "speaker_id": "2",
          "time": "20210903 02:23"
        },
        {
          "id": "MDRW2100029348.1.5",
          "form": "사용한지",
          "original_form": "사용한지",
          "speaker_id": "2",
          "time": "20210903 02:23"
        },
        {
          "id": "MDRW2100029348.1.6",
          "form": "그렇군요.\n몇개 샀어요 ?",
          "original_form": "그렇군요.\n몇개 샀어요 ?",
          "speaker_id": "1",
          "time": "20210903 02:24"
        },
        {
          "id": "MDRW2100029348.1.7",
          "form": "원플원이라서 두개요",
          "original_form": "원플원이라서 두개요",
          "speaker_id": "2",
          "time": "20210903 02:24"
        },
        {
          "id": "MDRW2100029348.1.8",
          "form": "그러면 이천원 ?",
          "original_form": "그러면 이천원 ?",
          "speaker_id": "1",
          "time": "20210903 02:25"
        },
        {
          "id": "MDRW2100029348.1.9",
          "form": "아니요 이천원보다 더 주고 샀던걸로 기억해요",
          "original_form": "아니요 이천원보다 더 주고 샀던걸로 기억해요",
          "speaker_id": "2",
          "time": "20210903 02:25"
        },
        {
          "id": "MDRW2100029348.1.10",
          "form": "그래도 가격이 좋네요.",
          "original_form": "그래도 가격이 좋네요.",
          "speaker_id": "1",
          "time": "20210903 02:25"
        },
        {
          "id": "MDRW2100029348.1.11",
          "form": "나쁘지는 않았어요",
          "original_form": "나쁘지는 않았어요",
          "speaker_id": "2",
          "time": "20210903 02:26"
        },
        {
          "id": "MDRW2100029348.1.12",
          "form": "다른건 안샀어요 ?",
          "original_form": "다른건 안샀어요 ?",
          "speaker_id": "1",
          "time": "20210903 02:26"
        },
        {
          "id": "MDRW2100029348.1.13",
          "form": "다이어리형을 쓰다가 자주 찢어지고 닳고 그래서 투명으로 바꾸게 되었죠",
          "original_form": "다이어리형을 쓰다가 자주 찢어지고 닳고 그래서 투명으로 바꾸게 되었죠",
          "speaker_id": "2",
          "time": "20210903 02:26"
        },
        {
          "id": "MDRW2100029348.1.14",
          "form": "오늘 아니 어제에요?",
          "original_form": "오늘 아니 어제에요?",
          "speaker_id": "2",
          "time": "20210903 02:26"
        },
        {
          "id": "MDRW2100029348.1.15",
          "form": "아 그렇군요. 다이어리형이 수명이 좀 짧아요.",
          "original_form": "아 그렇군요. 다이어리형이 수명이 좀 짧아요.",
          "speaker_id": "1",
          "time": "20210903 02:27"
        },
        {
          "id": "MDRW2100029348.1.16",
          "form": "맞아요ㅜㅜ 그래서 고민하다 처음으로 다이어리에서 벗어나봤어요",
          "original_form": "맞아요ㅜㅜ 그래서 고민하다 처음으로 다이어리에서 벗어나봤어요",
          "speaker_id": "2",
          "time": "20210903 02:27"
        },
        {
          "id": "MDRW2100029348.1.17",
          "form": "그리고 왜이렇게 비싼지\n가죽이 가죽같지도않던데",
          "original_form": "그리고 왜이렇게 비싼지\n가죽이 가죽같지도않던데",
          "speaker_id": "1",
          "time": "20210903 02:28"
        },
        {
          "id": "MDRW2100029348.1.18",
          "form": "맞아요 가격을 이해 할 수 없어요",
          "original_form": "맞아요 가격을 이해 할 수 없어요",
          "speaker_id": "2",
          "time": "20210903 02:28"
        },
        {
          "id": "MDRW2100029348.1.19",
          "form": "얼마전에 기존에 샀던 가죽 다이어리 제품을 알아보는데 2만원이 넘어서\n어이가없었어요.",
          "original_form": "얼마전에 기존에 샀던 가죽 다이어리 제품을 알아보는데 2만원이 넘어서\n어이가없었어요.",
          "speaker_id": "1",
          "time": "20210903 02:30"
        }
      ]
