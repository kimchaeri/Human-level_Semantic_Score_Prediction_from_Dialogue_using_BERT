      "utterance": [
        {
          "id": "MDRW2100033739.1.1",
          "form": "드디어 태풍이 갔어!",
          "original_form": "드디어 태풍이 갔어!",
          "speaker_id": "2",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100033739.1.2",
          "form": "응 많이 오더라",
          "original_form": "응 많이 오더라",
          "speaker_id": "2",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100033739.1.3",
          "form": "주위 하천이 범람해서 산책로가 잠겼대.",
          "original_form": "주위 하천이 범람해서 산책로가 잠겼대.",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100033739.1.4",
          "form": "태풍이 생각보다 빨리 소멸된 것 같아",
          "original_form": "태풍이 생각보다 빨리 소멸된 것 같아",
          "speaker_id": "1",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100033739.1.5",
          "form": "사진 봤어?",
          "original_form": "사진 봤어?",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100033739.1.6",
          "form": "세상에 너도 직접 봤니?",
          "original_form": "세상에 너도 직접 봤니?",
          "speaker_id": "1",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100033739.1.7",
          "form": "정말 어마어마하다",
          "original_form": "정말 어마어마하다",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100033739.1.8",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100033739.1.9",
          "form": "아니 나중에 보여주라!",
          "original_form": "아니 나중에 보여주라!",
          "speaker_id": "1",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100033739.1.10",
          "form": "아찔하더라.",
          "original_form": "아찔하더라.",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100033739.1.11",
          "form": "이제 날이 더 선선해지겠지?",
          "original_form": "이제 날이 더 선선해지겠지?",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100033739.1.12",
          "form": "나는 그리고 이번 장마에",
          "original_form": "나는 그리고 이번 장마에",
          "speaker_id": "1",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100033739.1.13",
          "form": "난 가을이 좋아.",
          "original_form": "난 가을이 좋아.",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100033739.1.14",
          "form": "태풍이 가서 정말 기뻐.",
          "original_form": "태풍이 가서 정말 기뻐.",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100033739.1.15",
          "form": "장화를 갖고싶다는 생각이 많이 들었어",
          "original_form": "장화를 갖고싶다는 생각이 많이 들었어",
          "speaker_id": "1",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100033739.1.16",
          "form": "가을에도 태풍이 오지않아?",
          "original_form": "가을에도 태풍이 오지않아?",
          "speaker_id": "1",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100033739.1.17",
          "form": "왜 그렇게 생각했습니까?",
          "original_form": "왜 그렇게 생각했습니까?",
          "speaker_id": "2",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100033739.1.18",
          "form": "가을이니 다 괜찮아.",
          "original_form": "가을이니 다 괜찮아.",
          "speaker_id": "2",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100033739.1.19",
          "form": "신발이 구정물에 들어가서",
          "original_form": "신발이 구정물에 들어가서",
          "speaker_id": "1",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100033739.1.20",
          "form": "선선한 공기가 좋기 때문이야.",
          "original_form": "선선한 공기가 좋기 때문이야.",
          "speaker_id": "2",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100033739.1.21",
          "form": "매우 불쾌했거든",
          "original_form": "매우 불쾌했거든",
          "speaker_id": "1",
          "time": "20210824 23:56"
        }
      ]
    }
