      "utterance": [
        {
          "id": "MDRW2100036814.1.1",
          "form": "피자 파파존스 좋아해요~ name2님은요?",
          "original_form": "피자 파파존스 좋아해요~ &name2&님은요?",
          "speaker_id": "1",
          "time": "20210831 12:47"
        },
        {
          "id": "MDRW2100036814.1.2",
          "form": "저는 도미노요!",
          "original_form": "저는 도미노요!",
          "speaker_id": "2",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100036814.1.3",
          "form": "메뉴는 어떤거 좋아하세요?",
          "original_form": "메뉴는 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 12:51"
        },
        {
          "id": "MDRW2100036814.1.4",
          "form": "슈퍼파파스 그게 무난하던데요",
          "original_form": "슈퍼파파스 그게 무난하던데요",
          "speaker_id": "1",
          "time": "20210831 12:51"
        },
        {
          "id": "MDRW2100036814.1.5",
          "form": "그렇구나 파인애플피자 좋아하세요?",
          "original_form": "그렇구나 파인애플피자 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100036814.1.6",
          "form": "아 네 그거 저는 맛있던데 호불호 갈린다면서요",
          "original_form": "아 네 그거 저는 맛있던데 호불호 갈린다면서요",
          "speaker_id": "1",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100036814.1.7",
          "form": "네 저는 불호라서요",
          "original_form": "네 저는 불호라서요",
          "speaker_id": "2",
          "time": "20210831 12:56"
        },
        {
          "id": "MDRW2100036814.1.8",
          "form": "전 디저트 같아서 식사로 애매하다고 느껴요",
          "original_form": "전 디저트 같아서 식사로 애매하다고 느껴요",
          "speaker_id": "2",
          "time": "20210831 12:56"
        },
        {
          "id": "MDRW2100036814.1.9",
          "form": "아.. 무슨 피자 좋아하세요?",
          "original_form": "아.. 무슨 피자 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100036814.1.10",
          "form": "저는 불고기피자 쉬림프피자 좋아해요",
          "original_form": "저는 불고기피자 쉬림프피자 좋아해요",
          "speaker_id": "2",
          "time": "20210831 13:00"
        },
        {
          "id": "MDRW2100036814.1.11",
          "form": "한식입맛이라 그런가봐요",
          "original_form": "한식입맛이라 그런가봐요",
          "speaker_id": "2",
          "time": "20210831 13:00"
        },
        {
          "id": "MDRW2100036814.1.12",
          "form": "야채피자 종류 드셔보셨어요? 이거 은근 맛있던데요",
          "original_form": "야채피자 종류 드셔보셨어요? 이거 은근 맛있던데요",
          "speaker_id": "1",
          "time": "20210831 13:01"
        },
        {
          "id": "MDRW2100036814.1.13",
          "form": "아니요! 어떤맛이예요?",
          "original_form": "아니요! 어떤맛이예요?",
          "speaker_id": "2",
          "time": "20210831 13:02"
        },
        {
          "id": "MDRW2100036814.1.14",
          "form": "깔끔한 맛이었어요 함 드셔보세요",
          "original_form": "깔끔한 맛이었어요 함 드셔보세요",
          "speaker_id": "1",
          "time": "20210831 13:05"
        },
        {
          "id": "MDRW2100036814.1.15",
          "form": "추천감사해요 먹어볼게요",
          "original_form": "추천감사해요 먹어볼게요",
          "speaker_id": "2",
          "time": "20210831 13:28"
        },
        {
          "id": "MDRW2100036814.1.16",
          "form": "네 입맛에 맞으셨음 좋겠어요",
          "original_form": "네 입맛에 맞으셨음 좋겠어요",
          "speaker_id": "1",
          "time": "20210831 13:28"
        }
