      "utterance": [
        {
          "id": "MDRW2100021761.1.1",
          "form": "닥터프로스트인가",
          "original_form": "닥터프로스트인가",
          "speaker_id": "2",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021761.1.2",
          "form": "웹툰그리신분이래요",
          "original_form": "웹툰그리신분이래요",
          "speaker_id": "2",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021761.1.3",
          "form": "아아아아 무슨 웹툰인지 알거같아요! 드라마도있었는데!",
          "original_form": "아아아아 무슨 웹툰인지 알거같아요! 드라마도있었는데!",
          "speaker_id": "1",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021761.1.4",
          "form": "암튼 그분이 그림이 젤 잘그리시는 듯",
          "original_form": "암튼 그분이 그림이 젤 잘그리시는 듯",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021761.1.5",
          "form": "다른 유튜브는 또 뭐보세요?",
          "original_form": "다른 유튜브는 또 뭐보세요?",
          "speaker_id": "1",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021761.1.6",
          "form": "노래나 영화제목을 10초인가",
          "original_form": "노래나 영화제목을 10초인가",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021761.1.7",
          "form": "이어그리기 하는건데 진짜 대박이었어요",
          "original_form": "이어그리기 하는건데 진짜 대박이었어요",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021761.1.8",
          "form": "추천합니다",
          "original_form": "추천합니다",
          "speaker_id": "2",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021761.1.9",
          "form": "아 저 여행유튜브 많이봐요",
          "original_form": "아 저 여행유튜브 많이봐요",
          "speaker_id": "2",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021761.1.10",
          "form": "빠니보틀이라구",
          "original_form": "빠니보틀이라구",
          "speaker_id": "2",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021761.1.11",
          "form": "아아 그것도 들어봤어요!!",
          "original_form": "아아 그것도 들어봤어요!!",
          "speaker_id": "1",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021761.1.12",
          "form": "많이들보시더라구요!",
          "original_form": "많이들보시더라구요!",
          "speaker_id": "1",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021761.1.13",
          "form": "코로나땜시 여행못다녀서",
          "original_form": "코로나땜시 여행못다녀서",
          "speaker_id": "2",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021761.1.14",
          "form": "그사이 좋좋소리는 명작을 연출하고",
          "original_form": "그사이 좋좋소리는 명작을 연출하고",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021761.1.15",
          "form": "지금 미국으로 또 여행가셨네요",
          "original_form": "지금 미국으로 또 여행가셨네요",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021761.1.16",
          "form": "여잔데 약간 병맛 콘텐츠를 좋아하네요 ;;",
          "original_form": "여잔데 약간 병맛 콘텐츠를 좋아하네요 ;;",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021761.1.17",
          "form": "좋좋소 그거 드라마 아닌가요..?",
          "original_form": "좋좋소 그거 드라마 아닌가요..?",
          "speaker_id": "1",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021761.1.18",
          "form": "네넵",
          "original_form": "네넵",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021761.1.19",
          "form": "빠니보틀님이 각본제작했는데",
          "original_form": "빠니보틀님이 각본제작했는데",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021761.1.20",
          "form": "대박이 났었죠",
          "original_form": "대박이 났었죠",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021761.1.21",
          "form": "헐 ㅋㅋㅋ 좋좋소 보려고 목록에 담아둔건데! 보셨어요?",
          "original_form": "헐 ㅋㅋㅋ 좋좋소 보려고 목록에 담아둔건데! 보셨어요?",
          "speaker_id": "1",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021761.1.22",
          "form": "네 다는 아니구",
          "original_form": "네 다는 아니구",
          "speaker_id": "2",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021761.1.23",
          "form": "그거도 중소기업 빗대서",
          "original_form": "그거도 중소기업 빗대서",
          "speaker_id": "2",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021761.1.24",
          "form": "뼈때리는 내용이",
          "original_form": "뼈때리는 내용이",
          "speaker_id": "2",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021761.1.25",
          "form": "ㄷㄷㄷㄷ",
          "original_form": "ㄷㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021761.1.26",
          "form": "근데 그분은 중소 다니다가 유튜버하시는거에요?",
          "original_form": "근데 그분은 중소 다니다가 유튜버하시는거에요?",
          "speaker_id": "1",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021761.1.27",
          "form": "그러신듯해요",
          "original_form": "그러신듯해요",
          "speaker_id": "2",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021761.1.28",
          "form": "영상쪽인가 디자인쪽인가",
          "original_form": "영상쪽인가 디자인쪽인가",
          "speaker_id": "2",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021761.1.29",
          "form": "관련분야일하시다가 여행유튜버로 전행하신듯해요",
          "original_form": "관련분야일하시다가 여행유튜버로 전행하신듯해요",
          "speaker_id": "2",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021761.1.30",
          "form": "경험담이라면 너무 안타까운일이네요...",
          "original_form": "경험담이라면 너무 안타까운일이네요...",
          "speaker_id": "1",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021761.1.31",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210828 22:29"
        },
        {
          "id": "MDRW2100021761.1.32",
          "form": "보시면 그런생각 안하실거예요",
          "original_form": "보시면 그런생각 안하실거예요",
          "speaker_id": "2",
          "time": "20210828 22:29"
        },
        {
          "id": "MDRW2100021761.1.33",
          "form": "왜냐면 다른사람들 킹받게 중간에 관두고",
          "original_form": "왜냐면 다른사람들 킹받게 중간에 관두고",
          "speaker_id": "2",
          "time": "20210828 22:29"
        },
        {
          "id": "MDRW2100021761.1.34",
          "form": "여러일이 일어나요",
          "original_form": "여러일이 일어나요",
          "speaker_id": "2",
          "time": "20210828 22:29"
        }
      ]
    }
