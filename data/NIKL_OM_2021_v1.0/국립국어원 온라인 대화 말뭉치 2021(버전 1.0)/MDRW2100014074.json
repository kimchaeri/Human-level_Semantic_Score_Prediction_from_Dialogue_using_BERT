      "utterance": [
        {
          "id": "MDRW2100014074.1.1",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014074.1.2",
          "form": "유퀴즈랑 그 무서운거",
          "original_form": "유퀴즈랑 그 무서운거",
          "speaker_id": "2",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014074.1.3",
          "form": "엠비씨에서 하는 그...그.. 심야괴담회!!!",
          "original_form": "엠비씨에서 하는 그...그.. 심야괴담회!!!",
          "speaker_id": "2",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014074.1.4",
          "form": "무서운 거?",
          "original_form": "무서운 거?",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014074.1.5",
          "form": "찾아봅니다",
          "original_form": "찾아봅니다",
          "speaker_id": "2",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014074.1.6",
          "form": "아 무서운거 잘보나요",
          "original_form": "아 무서운거 잘보나요",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014074.1.7",
          "form": "유퀴즈는 저도 봐요 유퀴즈 존잼",
          "original_form": "유퀴즈는 저도 봐요 유퀴즈 존잼",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014074.1.8",
          "form": "엇 무서운거 싫어하시는 군요?!",
          "original_form": "엇 무서운거 싫어하시는 군요?!",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014074.1.9",
          "form": "유퀴즈...크으으 유재석 씨 나오는 건 다 잼난거같아요",
          "original_form": "유퀴즈...크으으 유재석 씨 나오는 건 다 잼난거같아요",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014074.1.10",
          "form": "썩 좋아하는 편은 아닙니다",
          "original_form": "썩 좋아하는 편은 아닙니다",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014074.1.11",
          "form": "맞아요 놀뭐도 재밌고",
          "original_form": "맞아요 놀뭐도 재밌고",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014074.1.12",
          "form": "식스센스도 재밌고",
          "original_form": "식스센스도 재밌고",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014074.1.13",
          "form": "런닝맨도 재밌고",
          "original_form": "런닝맨도 재밌고",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014074.1.14",
          "form": "맞아요 재능 부럽다",
          "original_form": "맞아요 재능 부럽다",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014074.1.15",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014074.1.16",
          "form": "유퀴즈 최근 뭐보셨어요?",
          "original_form": "유퀴즈 최근 뭐보셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014074.1.17",
          "form": "국가대표편 봤어요",
          "original_form": "국가대표편 봤어요",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014074.1.18",
          "form": "name2 보셨나요",
          "original_form": "&name2& 보셨나요",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014074.1.19",
          "form": "엇 아직이요",
          "original_form": "엇 아직이요",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014074.1.20",
          "form": "아 그러고 보니 오늘도 유퀴즈하네",
          "original_form": "아 그러고 보니 오늘도 유퀴즈하네",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014074.1.21",
          "form": "저는 최근 샤론최...클립으로 본것이",
          "original_form": "저는 최근 샤론최...클립으로 본것이",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014074.1.22",
          "form": "아 근데 그 마무리는 봤어요",
          "original_form": "아 근데 그 마무리는 봤어요",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014074.1.23",
          "form": "눈물 좔좔",
          "original_form": "눈물 좔좔",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014074.1.24",
          "form": "샤론최 그분 너무 멋져",
          "original_form": "샤론최 그분 너무 멋져",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014074.1.25",
          "form": "맞아요 저 눈물 한방울 흘렸습니다",
          "original_form": "맞아요 저 눈물 한방울 흘렸습니다",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014074.1.26",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014074.1.27",
          "form": "어케 그런 편집을..",
          "original_form": "어케 그런 편집을..",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014074.1.28",
          "form": "유퀴즈 날 울리는 방송",
          "original_form": "유퀴즈 날 울리는 방송",
          "speaker_id": "1",
          "time": "20210825 17:15"
        }
      ]
    }
