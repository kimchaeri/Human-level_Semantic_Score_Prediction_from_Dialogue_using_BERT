        },
        {
          "id": "MDRW2100019158.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019158.1.3",
          "form": "이제 원신 얘기를 하면 되나요",
          "original_form": "이제 원신 얘기를 하면 되나요",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019158.1.4",
          "form": "네 그렇습니다 ㅋㅋㅋㅋㅋ",
          "original_form": "네 그렇습니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019158.1.5",
          "form": "이번에 2.2에서 주는 신규 캐릭터",
          "original_form": "이번에 2.2에서 주는 신규 캐릭터",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019158.1.6",
          "form": "에일로이 플레이 영상 나왓는데 생각보다 괜찮은듯?",
          "original_form": "에일로이 플레이 영상 나왓는데 생각보다 괜찮은듯?",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019158.1.7",
          "form": "오 그래?",
          "original_form": "오 그래?",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019158.1.8",
          "form": "차라리 걔를 키워볼까",
          "original_form": "차라리 걔를 키워볼까",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019158.1.9",
          "form": "원석은 좀 더 아껴놓고",
          "original_form": "원석은 좀 더 아껴놓고",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019158.1.10",
          "form": "근데 얼음 활캐야",
          "original_form": "근데 얼음 활캐야",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019158.1.11",
          "form": "그것도 나쁘진 않지",
          "original_form": "그것도 나쁘진 않지",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019158.1.12",
          "form": "어쨌든 5성이니까",
          "original_form": "어쨌든 5성이니까",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019158.1.13",
          "form": "얼음 묻히는것만 잘하면",
          "original_form": "얼음 묻히는것만 잘하면",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019158.1.14",
          "form": "케이아빼고 넣어도 될것같은데",
          "original_form": "케이아빼고 넣어도 될것같은데",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019158.1.15",
          "form": "스킬?인거같던데",
          "original_form": "스킬?인거같던데",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019158.1.16",
          "form": "홍보영상보니까",
          "original_form": "홍보영상보니까",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019158.1.17",
          "form": "근더 케이아가 진짜",
          "original_form": "근더 케이아가 진짜",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019158.1.18",
          "form": "일정 범주의 모든 적을 얼리는듯",
          "original_form": "일정 범주의 모든 적을 얼리는듯",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019158.1.19",
          "form": "얼음 묻히는거하나는",
          "original_form": "얼음 묻히는거하나는",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019158.1.20",
          "form": "잘하는편이라",
          "original_form": "잘하는편이라",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019158.1.21",
          "form": "좀 봐서?",
          "original_form": "좀 봐서?",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019158.1.22",
          "form": "난 초반부터 피슬을 주딜로 써서 그런가",
          "original_form": "난 초반부터 피슬을 주딜로 써서 그런가",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019158.1.23",
          "form": "활캐도 쓸만하더라구",
          "original_form": "활캐도 쓸만하더라구",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019158.1.24",
          "form": "2활 넣어도 될것같아",
          "original_form": "2활 넣어도 될것같아",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019158.1.25",
          "form": "케이아가 4성치고 효율은 진짜 좋지",
          "original_form": "케이아가 4성치고 효율은 진짜 좋지",
          "speaker_id": "1",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100019158.1.26",
          "form": "근데 궁이 뭔지 아직 못봤어",
          "original_form": "근데 궁이 뭔지 아직 못봤어",
          "speaker_id": "1",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100019158.1.27",
          "form": "ㅇㅇ.....일단 5성이니까",
          "original_form": "ㅇㅇ.....일단 5성이니까",
          "speaker_id": "2",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100019158.1.28",
          "form": "플레이 영상 한 번 봐봐",
          "original_form": "플레이 영상 한 번 봐봐",
          "speaker_id": "1",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100019158.1.29",
          "form": "ㅇㅏ",
          "original_form": "ㅇㅏ",
          "speaker_id": "2",
          "time": "20210827 15:50"
        },
        {
          "id": "MDRW2100019158.1.30",
          "form": "ㅆㅓ보아야지 뭐",
          "original_form": "ㅆㅓ보아야지 뭐",
