      "utterance": [
        {
          "id": "MDRW2100004010.1.1",
          "form": "동호회 하나요?",
          "original_form": "동호회 하나요?",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100004010.1.2",
          "form": "아니요. 혹시 동호회에 가입되어있나요?",
          "original_form": "아니요. 혹시 동호회에 가입되어있나요?",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100004010.1.3",
          "form": "저도 아니요 딱히 할 곳이 없어요",
          "original_form": "저도 아니요 딱히 할 곳이 없어요",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100004010.1.4",
          "form": "봉사는 해보셨나요?",
          "original_form": "봉사는 해보셨나요?",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100004010.1.5",
          "form": "20대 초반에 했었어요 요양원이었나",
          "original_form": "20대 초반에 했었어요 요양원이었나",
          "speaker_id": "1",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100004010.1.6",
          "form": "거제도로 간 적 있었어요",
          "original_form": "거제도로 간 적 있었어요",
          "speaker_id": "1",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100004010.1.7",
          "form": "요양원가면 어떤 일을 하나요?",
          "original_form": "요양원가면 어떤 일을 하나요?",
          "speaker_id": "2",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100004010.1.8",
          "form": "저는 보통 아이들있는 아동센터를 가서",
          "original_form": "저는 보통 아이들있는 아동센터를 가서",
          "speaker_id": "2",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100004010.1.9",
          "form": "꽤 오래전 일이라 기억이 가물가물한데",
          "original_form": "꽤 오래전 일이라 기억이 가물가물한데",
          "speaker_id": "1",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100004010.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100004010.1.11",
          "form": "청소하고 어르신 말동무 했어요",
          "original_form": "청소하고 어르신 말동무 했어요",
          "speaker_id": "1",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100004010.1.12",
          "form": "어르신들이 좋아하셨겠어요",
          "original_form": "어르신들이 좋아하셨겠어요",
          "speaker_id": "2",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100004010.1.13",
          "form": "적적하니까",
          "original_form": "적적하니까",
          "speaker_id": "2",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100004010.1.14",
          "form": "어떤계기로 가게됐나요?",
          "original_form": "어떤계기로 가게됐나요?",
          "speaker_id": "2",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100004010.1.15",
          "form": "아는 분이 주말에 시간 되냐고 물으셔서",
          "original_form": "아는 분이 주말에 시간 되냐고 물으셔서",
          "speaker_id": "1",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100004010.1.16",
          "form": "친구들이랑 후배들이랑 갔었어요",
          "original_form": "친구들이랑 후배들이랑 갔었어요",
          "speaker_id": "1",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100004010.1.17",
          "form": "아동센터는 어때요?",
          "original_form": "아동센터는 어때요?",
          "speaker_id": "1",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100004010.1.18",
          "form": "음 아이들 대하는거라 어른보다 어렵더라고요ㅎㅎ",
          "original_form": "음 아이들 대하는거라 어른보다 어렵더라고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004010.1.19",
          "form": "아무래도 사춘기 애들이 많으니까",
          "original_form": "아무래도 사춘기 애들이 많으니까",
          "speaker_id": "2",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004010.1.20",
          "form": "아이들이 힘들긴 하겠어요",
          "original_form": "아이들이 힘들긴 하겠어요",
          "speaker_id": "1",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004010.1.21",
          "form": "어르신들이라 말동무면 되니깐",
          "original_form": "어르신들이라 말동무면 되니깐",
          "speaker_id": "1",
