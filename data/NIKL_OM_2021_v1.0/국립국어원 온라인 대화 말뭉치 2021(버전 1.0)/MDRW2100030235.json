        {
          "id": "MDRW2100030235.1.1",
          "form": "맥주도같이파나요",
          "original_form": "맥주도같이파나요",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030235.1.2",
          "form": "닭껍질 튀김들어보셨나요",
          "original_form": "닭껍질 튀김들어보셨나요",
