      "utterance": [
        {
          "id": "MDRW2100030305.1.1",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030305.1.2",
          "form": "중국음식두 있구 이탈리아 음식두 있구 일본 음식두있구",
          "original_form": "중국음식두 있구 이탈리아 음식두 있구 일본 음식두있구",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030305.1.3",
          "form": "언니는 어떤나라가좋아? 전체적으로",
          "original_form": "언니는 어떤나라가좋아? 전체적으로",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030305.1.4",
          "form": "마자 다 다르징",
          "original_form": "마자 다 다르징",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030305.1.5",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030305.1.6",
          "form": "나는 우리나라 음식이",
          "original_form": "나는 우리나라 음식이",
          "speaker_id": "2",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030305.1.7",
          "form": "그래두 제일 좋은것같애 ㅎㅎ",
          "original_form": "그래두 제일 좋은것같애 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030305.1.8",
          "form": "그렇구나 ㅎㅎ 예를 들어서",
          "original_form": "그렇구나 ㅎㅎ 예를 들어서",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030305.1.9",
          "form": "무슨 음식?",
          "original_form": "무슨 음식?",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030305.1.10",
          "form": "삽겹살같은거 ㅋ",
          "original_form": "삽겹살같은거 ㅋ",
          "speaker_id": "2",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030305.1.11",
          "form": "아 맞아!! 삼겹살 맛있지 ㅎㅎ",
          "original_form": "아 맞아!! 삼겹살 맛있지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030305.1.12",
          "form": "삼겹살 조아",
          "original_form": "삼겹살 조아",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030305.1.13",
          "form": "소금기름장에 삼겹살 찍구 상추에 싸먹으면",
          "original_form": "소금기름장에 삼겹살 찍구 상추에 싸먹으면",
          "speaker_id": "2",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030305.1.14",
          "form": "나는 중국 음식이 제일 좋은거같아..",
          "original_form": "나는 중국 음식이 제일 좋은거같아..",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030305.1.15",
          "form": "맛잇지..",
          "original_form": "맛잇지..",
          "speaker_id": "2",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030305.1.16",
          "form": "맞아!!! 엄청좋아",
          "original_form": "맞아!!! 엄청좋아",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030305.1.17",
          "form": "왱??",
          "original_form": "왱??",
          "speaker_id": "2",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030305.1.18",
          "form": "중국은 식문화가 많이 발달된 나라래",
          "original_form": "중국은 식문화가 많이 발달된 나라래",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030305.1.19",
          "form": "오..들어봐써",
          "original_form": "오..들어봐써",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030305.1.20",
          "form": "왜냐면 땅이 넓어서",
          "original_form": "왜냐면 땅이 넓어서",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030305.1.21",
          "form": "내부에서 식재료를 많이 공급할수있구",
          "original_form": "내부에서 식재료를 많이 공급할수있구",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030305.1.22",
          "form": "사천음식같은거 유명하잖어",
          "original_form": "사천음식같은거 유명하잖어",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030305.1.23",
          "form": "예전부터 부유했자나 권력이 쎄고.",
          "original_form": "예전부터 부유했자나 권력이 쎄고.",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030305.1.24",
          "form": "맞어!",
          "original_form": "맞어!",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030305.1.25",
          "form": "그런가?",
          "original_form": "그런가?",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030305.1.26",
          "form": "아무튼 중국은 지역마다 음식 특색이 다른게",
          "original_form": "아무튼 중국은 지역마다 음식 특색이 다른게",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030305.1.27",
          "form": "신기해 ㅎㅎ",
          "original_form": "신기해 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030305.1.28",
          "form": "웅 그래서 많이 발전할수잇ㅅ엇다고 들었어",
          "original_form": "웅 그래서 많이 발전할수잇ㅅ엇다고 들었어",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030305.1.29",
          "form": "4대요리가있지",
          "original_form": "4대요리가있지",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030305.1.30",
          "form": "상하이요리랑",
          "original_form": "상하이요리랑",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030305.1.31",
          "form": "그런거 ㅎㅎ",
          "original_form": "그런거 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030305.1.32",
          "form": "그렇군 ㅋㅋ 유익하네",
          "original_form": "그렇군 ㅋㅋ 유익하네",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030305.1.33",
          "form": "중국 가면 먹어봐야겟당",
          "original_form": "중국 가면 먹어봐야겟당",
          "speaker_id": "2",
          "time": "20210903 23:41"
        }
      ]
