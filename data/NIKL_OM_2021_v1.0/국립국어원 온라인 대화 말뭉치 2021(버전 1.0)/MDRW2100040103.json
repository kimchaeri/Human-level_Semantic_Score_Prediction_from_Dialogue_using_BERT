      "utterance": [
        {
          "id": "MDRW2100040103.1.1",
          "form": "아이들이 있나보군요",
          "original_form": "아이들이 있나보군요",
          "speaker_id": "2",
          "time": "20210904 09:58"
        },
        {
          "id": "MDRW2100040103.1.2",
          "form": "저는 애들이커서",
          "original_form": "저는 애들이커서",
          "speaker_id": "2",
          "time": "20210904 09:58"
        },
        {
          "id": "MDRW2100040103.1.3",
          "form": "학원가네요..",
          "original_form": "학원가네요..",
          "speaker_id": "2",
          "time": "20210904 09:58"
        },
        {
          "id": "MDRW2100040103.1.4",
          "form": "네~ 저희는 좀 이따 자전거 타러 나가려고요",
          "original_form": "네~ 저희는 좀 이따 자전거 타러 나가려고요",
          "speaker_id": "1",
          "time": "20210904 09:58"
        },
        {
          "id": "MDRW2100040103.1.5",
          "form": "어렸을땐 놀러많이다녔는데",
          "original_form": "어렸을땐 놀러많이다녔는데",
          "speaker_id": "2",
          "time": "20210904 09:58"
        },
        {
          "id": "MDRW2100040103.1.6",
          "form": "악!! 학원.. ㅠ 토요일인데요?",
          "original_form": "악!! 학원.. ㅠ 토요일인데요?",
          "speaker_id": "1",
          "time": "20210904 09:58"
        },
        {
          "id": "MDRW2100040103.1.7",
          "form": "셋다 학원 가나요?",
          "original_form": "셋다 학원 가나요?",
          "speaker_id": "1",
          "time": "20210904 09:58"
        },
        {
          "id": "MDRW2100040103.1.8",
          "form": "중고생들은 주말이없어요",
          "original_form": "중고생들은 주말이없어요",
          "speaker_id": "2",
          "time": "20210904 09:58"
        },
        {
          "id": "MDRW2100040103.1.9",
          "form": "애들 학원 가느라 놀러다니는것도 초등 저학년이 마지막인가요?",
          "original_form": "애들 학원 가느라 놀러다니는것도 초등 저학년이 마지막인가요?",
          "speaker_id": "1",
          "time": "20210904 09:59"
        },
        {
          "id": "MDRW2100040103.1.10",
          "form": "고학년되면 아직시험이없으니 가도되는것같아요",
          "original_form": "고학년되면 아직시험이없으니 가도되는것같아요",
          "speaker_id": "2",
          "time": "20210904 09:59"
        },
        {
          "id": "MDRW2100040103.1.11",
          "form": "옛날엔 시험있었거든요",
          "original_form": "옛날엔 시험있었거든요",
          "speaker_id": "2",
          "time": "20210904 09:59"
        },
        {
          "id": "MDRW2100040103.1.12",
          "form": "지금은 모두폐지된것같아요",
          "original_form": "지금은 모두폐지된것같아요",
          "speaker_id": "2",
          "time": "20210904 09:59"
        },
        {
          "id": "MDRW2100040103.1.13",
          "form": "아~ 시험보기 시작하는 중학교부터 주말에도 학교 가는군요",
          "original_form": "아~ 시험보기 시작하는 중학교부터 주말에도 학교 가는군요",
          "speaker_id": "1",
          "time": "20210904 09:59"
        },
        {
          "id": "MDRW2100040103.1.14",
          "form": "같이 다닐 수 있을때 얼른 다녀야겠엉ㅅ",
          "original_form": "같이 다닐 수 있을때 얼른 다녀야겠엉ㅅ",
          "speaker_id": "1",
          "time": "20210904 10:00"
        },
        {
          "id": "MDRW2100040103.1.15",
          "form": "토요일은 학교는안가고",
          "original_form": "토요일은 학교는안가고",
          "speaker_id": "2",
          "time": "20210904 10:00"
        },
        {
          "id": "MDRW2100040103.1.16",
          "form": "학원들을 가네요",
          "original_form": "학원들을 가네요",
          "speaker_id": "2",
          "time": "20210904 10:00"
        },
        {
          "id": "MDRW2100040103.1.17",
          "form": "그럼 학원은 애들이 가고 싶다고 해서 보내주시나요?",
          "original_form": "그럼 학원은 애들이 가고 싶다고 해서 보내주시나요?",
          "speaker_id": "1",
          "time": "20210904 10:00"
        },
        {
          "id": "MDRW2100040103.1.18",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "2",
          "time": "20210904 10:00"
        },
        {
          "id": "MDRW2100040103.1.19",
          "form": "안배우고가면 쫓아가기 힘들다고하더군요",
          "original_form": "안배우고가면 쫓아가기 힘들다고하더군요",
          "speaker_id": "2",
          "time": "20210904 10:00"
        },
        {
          "id": "MDRW2100040103.1.20",
          "form": "애들이 엄마 나 수학이 부족한것 같은데 수학학원 보내줘~ 이렇게 말하나요?",
          "original_form": "애들이 엄마 나 수학이 부족한것 같은데 수학학원 보내줘~ 이렇게 말하나요?",
          "speaker_id": "1",
          "time": "20210904 10:00"
        },
        {
          "id": "MDRW2100040103.1.21",
          "form": "예습이 중요해요",
          "original_form": "예습이 중요해요",
          "speaker_id": "2",
          "time": "20210904 10:01"
        },
        {
          "id": "MDRW2100040103.1.22",
          "form": "아~ 예습하고 가야 하는군요 ㅠ",
          "original_form": "아~ 예습하고 가야 하는군요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 10:01"
        },
        {
          "id": "MDRW2100040103.1.23",
          "form": "앗 이런 ㅠ",
          "original_form": "앗 이런 ㅠ",
          "speaker_id": "1",
          "time": "20210904 10:01"
        },
        {
          "id": "MDRW2100040103.1.24",
          "form": "아니요..",
          "original_form": "아니요..",
          "speaker_id": "2",
          "time": "20210904 10:01"
        }
      ]
    }
  ]
