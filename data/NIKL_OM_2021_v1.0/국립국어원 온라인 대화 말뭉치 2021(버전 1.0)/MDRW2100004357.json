      "utterance": [
        {
          "id": "MDRW2100004357.1.1",
          "form": "주제 바꿔봤어요",
          "original_form": "주제 바꿔봤어요",
          "speaker_id": "1",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004357.1.2",
          "form": "가장 좋아하는 음식은?!",
          "original_form": "가장 좋아하는 음식은?!",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004357.1.3",
          "form": "햄버거.!",
          "original_form": "햄버거.!",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004357.1.4",
          "form": "햄버거 좋아하세요?",
          "original_form": "햄버거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004357.1.5",
          "form": "엄청!!좋아해요",
          "original_form": "엄청!!좋아해요",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004357.1.6",
          "form": "ㅋㅋㅋㅋ저도 그래요",
          "original_form": "ㅋㅋㅋㅋ저도 그래요",
          "speaker_id": "1",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004357.1.7",
          "form": "어디 햄버거 좋아하세요?",
          "original_form": "어디 햄버거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004357.1.8",
          "form": "맥날? 롯데?",
          "original_form": "맥날? 롯데?",
          "speaker_id": "1",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004357.1.9",
          "form": "아니면 수제버거 파신가",
          "original_form": "아니면 수제버거 파신가",
          "speaker_id": "1",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004357.1.10",
          "form": "맥도날드!!",
          "original_form": "맥도날드!!",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004357.1.11",
          "form": "아 저도 맥날파에요",
          "original_form": "아 저도 맥날파에요",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004357.1.12",
          "form": "진짜 너무 맛있어",
          "original_form": "진짜 너무 맛있어",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004357.1.13",
          "form": "친구들이 빅맥만 보면 제 생각 난대요",
          "original_form": "친구들이 빅맥만 보면 제 생각 난대요",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004357.1.14",
          "form": "그쵸 뭘좀 아시군요!!",
          "original_form": "그쵸 뭘좀 아시군요!!",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004357.1.15",
          "form": "그쪽도 빅맥 제일 좋아하시나요?",
          "original_form": "그쪽도 빅맥 제일 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004357.1.16",
          "form": "치즈 낭낭 ㅠㅠ 너무 좋은",
          "original_form": "치즈 낭낭 ㅠㅠ 너무 좋은",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004357.1.17",
          "form": "전 불고기 ㅋㅋ",
          "original_form": "전 불고기 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004357.1.18",
          "form": "불고기도 훌륭하죠",
          "original_form": "불고기도 훌륭하죠",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004357.1.19",
          "form": "예전에는 정말 많이 먹었는데",
          "original_form": "예전에는 정말 많이 먹었는데",
          "speaker_id": "1",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004357.1.20",
          "form": "지금은 치즈에 환장하는 사람이 돼서 ㅎ",
          "original_form": "지금은 치즈에 환장하는 사람이 돼서 ㅎ",
          "speaker_id": "1",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004357.1.21",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004357.1.22",
          "form": "마라탕은 좋아하시나요",
          "original_form": "마라탕은 좋아하시나요",
          "speaker_id": "1",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004357.1.23",
          "form": "제 최애 음식인대",
          "original_form": "제 최애 음식인대",
          "speaker_id": "1",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004357.1.24",
          "form": "마라탕? 먹어본적 없어요",
          "original_form": "마라탕? 먹어본적 없어요",
          "speaker_id": "2",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004357.1.25",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004357.1.26",
          "form": "엄청 맛있는데",
          "original_form": "엄청 맛있는데",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004357.1.27",
          "form": "ㅇㅅㅇ?",
          "original_form": "ㅇㅅㅇ?",
          "speaker_id": "2",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004357.1.28",
          "form": "매운거 좋아하시면 나중에 한번",
          "original_form": "매운거 좋아하시면 나중에 한번",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004357.1.29",
          "form": "드셔보세요 최고",
          "original_form": "드셔보세요 최고",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004357.1.30",
          "form": "닭발!!!",
          "original_form": "닭발!!!",
          "speaker_id": "2",
          "time": "20210821 16:35"
        },
        {
          "id": "MDRW2100004357.1.31",
          "form": "닭발도 넘 맛있죠",
          "original_form": "닭발도 넘 맛있죠",
          "speaker_id": "1",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004357.1.32",
          "form": "안주로 딱인데 ㅎㅎ",
          "original_form": "안주로 딱인데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004357.1.33",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210821 16:36"
        }
      ]
    }
