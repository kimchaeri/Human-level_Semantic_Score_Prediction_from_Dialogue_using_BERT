      "utterance": [
        {
          "id": "MDRW2100030426.1.1",
          "form": "로봇청소기는 어디거가 좋아요",
          "original_form": "로봇청소기는 어디거가 좋아요",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030426.1.2",
          "form": "샤오미요 ㅋㅋ",
          "original_form": "샤오미요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100030426.1.3",
          "form": "대륙의 실수",
          "original_form": "대륙의 실수",
          "speaker_id": "2",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100030426.1.4",
          "form": "우와 대륙의실수 ㅋㅋㅋ",
          "original_form": "우와 대륙의실수 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 06:32"
        },
        {
          "id": "MDRW2100030426.1.5",
          "form": "가성비가 좋은가보군요",
          "original_form": "가성비가 좋은가보군요",
          "speaker_id": "1",
          "time": "20210904 06:32"
        },
        {
          "id": "MDRW2100030426.1.6",
          "form": "아니 ㅋㅋ왜케 일찍 일어나셨어요",
          "original_form": "아니 ㅋㅋ왜케 일찍 일어나셨어요",
          "speaker_id": "2",
          "time": "20210904 06:33"
        },
        {
          "id": "MDRW2100030426.1.7",
          "form": "저 오늘 당직이라 ㅋㅋ name2님은 왜케 일찍 ㅋㅋ",
          "original_form": "저 오늘 당직이라 ㅋㅋ &name2&님은 왜케 일찍 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 06:33"
        },
        {
          "id": "MDRW2100030426.1.8",
          "form": "저도 근무중인뎨!",
          "original_form": "저도 근무중인뎨!",
          "speaker_id": "2",
          "time": "20210904 06:54"
        },
        {
          "id": "MDRW2100030426.1.9",
          "form": "오 무슨 근무중이세여",
          "original_form": "오 무슨 근무중이세여",
          "speaker_id": "1",
          "time": "20210904 07:09"
        },
        {
          "id": "MDRW2100030426.1.10",
          "form": "지하철 교대근무입니다ㅋㅋ",
          "original_form": "지하철 교대근무입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 07:09"
        },
        {
          "id": "MDRW2100030426.1.11",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210904 07:10"
        },
        {
          "id": "MDRW2100030426.1.12",
          "form": "언제집에가세요ㅠㅠ",
          "original_form": "언제집에가세요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 07:10"
        },
        {
          "id": "MDRW2100030426.1.13",
          "form": "9시에요ㅋㅋ",
          "original_form": "9시에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 07:40"
        },
        {
          "id": "MDRW2100030426.1.14",
          "form": "name1님은 언제 가세여",
          "original_form": "&name1&님은 언제 가세여",
          "speaker_id": "2",
          "time": "20210904 07:40"
        },
        {
          "id": "MDRW2100030426.1.15",
          "form": "지금 다와가는중이에여",
          "original_form": "지금 다와가는중이에여",
          "speaker_id": "1",
          "time": "20210904 07:59"
        },
        {
          "id": "MDRW2100030426.1.16",
          "form": "오후 5시 30분에 해제입니다ㅜㅜ",
          "original_form": "오후 5시 30분에 해제입니다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 07:59"
        },
        {
          "id": "MDRW2100030426.1.17",
          "form": "아 이제 출근이시구나ㅠ",
          "original_form": "아 이제 출근이시구나ㅠ",
          "speaker_id": "2",
          "time": "20210904 08:00"
        },
        {
          "id": "MDRW2100030426.1.18",
          "form": "그래도 돈벌어야",
          "original_form": "그래도 돈벌어야",
          "speaker_id": "2",
          "time": "20210904 08:00"
        },
        {
          "id": "MDRW2100030426.1.19",
          "form": "비스포크 살 수 있으니까",
          "original_form": "비스포크 살 수 있으니까",
          "speaker_id": "2",
          "time": "20210904 08:00"
        },
        {
