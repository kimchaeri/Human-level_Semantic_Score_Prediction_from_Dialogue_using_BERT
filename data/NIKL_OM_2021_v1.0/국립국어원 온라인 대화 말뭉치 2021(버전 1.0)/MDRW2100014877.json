      "utterance": [
        {
          "id": "MDRW2100014877.1.1",
          "form": "요즘 날씨가 선선해져서",
          "original_form": "요즘 날씨가 선선해져서",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014877.1.2",
          "form": "긴바지입었는데",
          "original_form": "긴바지입었는데",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014877.1.3",
          "form": "습해서 괜히입엇어요",
          "original_form": "습해서 괜히입엇어요",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014877.1.4",
          "form": "아 마장요 저도 긴바지 입고나갔는데",
          "original_form": "아 마장요 저도 긴바지 입고나갔는데",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014877.1.5",
          "form": "바지 기장 어디까지인지 다 느껴지더라고요",
          "original_form": "바지 기장 어디까지인지 다 느껴지더라고요",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014877.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014877.1.7",
          "form": "몇부입으셧는데요??",
          "original_form": "몇부입으셧는데요??",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014877.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014877.1.9",
          "form": "쓰읍..몇..부요? 발목까지 오기는 했는데",
          "original_form": "쓰읍..몇..부요? 발목까지 오기는 했는데",
          "speaker_id": "2",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014877.1.10",
          "form": "가을옷 사실건가요",
          "original_form": "가을옷 사실건가요",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014877.1.11",
          "form": "후훟후후후 이미 샀답니다",
          "original_form": "후훟후후후 이미 샀답니다",
          "speaker_id": "2",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014877.1.12",
          "form": "뭐사셧나요",
          "original_form": "뭐사셧나요",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014877.1.13",
          "form": "가디건 셔츠",
          "original_form": "가디건 셔츠",
          "speaker_id": "2",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014877.1.14",
          "form": "치마",
          "original_form": "치마",
          "speaker_id": "2",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014877.1.15",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014877.1.16",
          "form": "이미 준비 다하셨네요!",
          "original_form": "이미 준비 다하셨네요!",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014877.1.17",
          "form": "요즘 무슨 치마가 유행인가요??",
          "original_form": "요즘 무슨 치마가 유행인가요??",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014877.1.18",
          "form": "가을에 입을만한 치마 추천좀해주세여!",
          "original_form": "가을에 입을만한 치마 추천좀해주세여!",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014877.1.19",
          "form": "엇 저는 저는...저는...",
          "original_form": "엇 저는 저는...저는...",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014877.1.20",
          "form": "저는 롱으로 사기는 했습니다",
          "original_form": "저는 롱으로 사기는 했습니다",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014877.1.21",
          "form": "ㅇㄴ 저 패고인데 추천해도되나요..?",
          "original_form": "ㅇㄴ 저 패고인데 추천해도되나요..?",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014877.1.22",
          "form": "아앗 그렇다면",
          "original_form": "아앗 그렇다면",
          "speaker_id": "1",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014877.1.23",
          "form": "제가 알아서 잘 찾아입어볼게요",
          "original_form": "제가 알아서 잘 찾아입어볼게요",
          "speaker_id": "1",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014877.1.24",
          "form": "ㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014877.1.25",
          "form": "바로 커트하시다니",
          "original_form": "바로 커트하시다니",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014877.1.26",
          "form": "역시 T다",
          "original_form": "역시 T다",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014877.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ아앗",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아앗",
          "speaker_id": "1",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014877.1.28",
          "form": "이런면에서 들켜버리는거구나",
          "original_form": "이런면에서 들켜버리는거구나",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014877.1.29",
          "form": "f들은 어떻게 대답하나요??",
          "original_form": "f들은 어떻게 대답하나요??",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014877.1.30",
          "form": "글..쎄요?",
          "original_form": "글..쎄요?",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014877.1.31",
          "form": "'아냐~ 그래도 함 추천해봐봐' (입진않음)",
          "original_form": "'아냐~ 그래도 함 추천해봐봐' (입진않음)",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014877.1.32",
          "form": "약간 말로만 따듯한 느낌",
          "original_form": "약간 말로만 따듯한 느낌",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014877.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014877.1.34",
          "form": "아 정말 f스러운 대답이네요",
          "original_form": "아 정말 f스러운 대답이네요",
          "speaker_id": "1",
          "time": "20210825 21:13"
        }
      ]
    }
