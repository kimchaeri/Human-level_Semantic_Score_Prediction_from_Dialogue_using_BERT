        },
        {
          "id": "MDRW2100010206.1.2",
          "form": "아 의외네?",
          "original_form": "아 의외네?",
          "speaker_id": "1",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100010206.1.3",
          "form": "음료수 뭘  좋아해",
          "original_form": "음료수 뭘  좋아해",
          "speaker_id": "2",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100010206.1.4",
          "form": "나는 약콩두유",
          "original_form": "나는 약콩두유",
          "speaker_id": "1",
          "time": "20210823 15:35"
        },
        {
          "id": "MDRW2100010206.1.5",
          "form": "그게 젤 맛있어?",
          "original_form": "그게 젤 맛있어?",
          "speaker_id": "2",
          "time": "20210823 15:35"
        },
        {
          "id": "MDRW2100010206.1.6",
          "form": "응 난 그게 제일 맛있던데",
          "original_form": "응 난 그게 제일 맛있던데",
          "speaker_id": "1",
          "time": "20210823 15:35"
        },
        {
          "id": "MDRW2100010206.1.7",
          "form": "밀키스도 좋아해",
          "original_form": "밀키스도 좋아해",
          "speaker_id": "2",
          "time": "20210823 15:35"
        },
        {
          "id": "MDRW2100010206.1.8",
          "form": "밀키스 좋아해?",
          "original_form": "밀키스 좋아해?",
          "speaker_id": "1",
          "time": "20210823 15:35"
        },
        {
          "id": "MDRW2100010206.1.9",
          "form": "응 좋아해",
          "original_form": "응 좋아해",
          "speaker_id": "2",
          "time": "20210823 15:36"
        },
        {
          "id": "MDRW2100010206.1.10",
          "form": "잘 안마시잖아",
          "original_form": "잘 안마시잖아",
          "speaker_id": "1",
          "time": "20210823 15:36"
        },
        {
          "id": "MDRW2100010206.1.11",
          "form": "있으면 먹고 없으면 안먹고",
          "original_form": "있으면 먹고 없으면 안먹고",
          "speaker_id": "2",
          "time": "20210823 15:36"
        },
        {
          "id": "MDRW2100010206.1.12",
          "form": "근데 요즘 거의 못본 거 같아",
          "original_form": "근데 요즘 거의 못본 거 같아",
          "speaker_id": "1",
          "time": "20210823 15:36"
        },
        {
          "id": "MDRW2100010206.1.13",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210823 15:37"
        },
        {
          "id": "MDRW2100010206.1.14",
          "form": "나도 예전에 마셔봤는데 이젠 맛이 기억이 잘 안나",
          "original_form": "나도 예전에 마셔봤는데 이젠 맛이 기억이 잘 안나",
