      "utterance": [
        {
          "id": "MDRW2100001815.1.1",
          "form": "name2님은 학교 성실히 다니셨나요?",
          "original_form": "&name2&님은 학교 성실히 다니셨나요?",
          "speaker_id": "1",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001815.1.2",
          "form": "저는 나름대로 성실하게 다닌 거 같아요",
          "original_form": "저는 나름대로 성실하게 다닌 거 같아요",
          "speaker_id": "2",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001815.1.3",
          "form": "지금 생각하면 왜 그렇게 열심히 다녔지? 싶은...ㅋㅋㅋㅋ",
          "original_form": "지금 생각하면 왜 그렇게 열심히 다녔지? 싶은...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001815.1.4",
          "form": "name1님은 어떠셨어요?",
          "original_form": "&name1&님은 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001815.1.5",
          "form": "저도...공부를 잘한건 아닌데 성실히 다녔어요ㅎㅎ",
          "original_form": "저도...공부를 잘한건 아닌데 성실히 다녔어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:26"
        },
        {
          "id": "MDRW2100001815.1.6",
          "form": "대학때는 결석 한 번 하는게 큰일인것처럼 출석 꼬박꼬박 했더라구요ㅠㅠ",
          "original_form": "대학때는 결석 한 번 하는게 큰일인것처럼 출석 꼬박꼬박 했더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 21:26"
        },
        {
          "id": "MDRW2100001815.1.7",
          "form": "어 저도요ㅋㅋㅋ자체휴강 해본 적 거의 없어요",
          "original_form": "어 저도요ㅋㅋㅋ자체휴강 해본 적 거의 없어요",
          "speaker_id": "2",
          "time": "20210819 21:27"
        },
        {
          "id": "MDRW2100001815.1.8",
          "form": "지금 생각하면 그 때 열심히 놀았어야 하는데",
          "original_form": "지금 생각하면 그 때 열심히 놀았어야 하는데",
          "speaker_id": "2",
          "time": "20210819 21:27"
        },
        {
          "id": "MDRW2100001815.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100001815.1.10",
          "form": "지금보다 대학때 더 열심히 산거같네요ㅎㅎ",
          "original_form": "지금보다 대학때 더 열심히 산거같네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100001815.1.11",
          "form": "오 저도요ㅋㅋㅋ",
          "original_form": "오 저도요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100001815.1.12",
          "form": "그땐 진짜 지각도 안하려고 하고",
          "original_form": "그땐 진짜 지각도 안하려고 하고",
          "speaker_id": "2",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100001815.1.13",
          "form": "공부는 열심히 안했지만..",
          "original_form": "공부는 열심히 안했지만..",
          "speaker_id": "2",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100001815.1.14",
          "form": "수업만 열심히 들었어요ㅋㅋㅋ",
          "original_form": "수업만 열심히 들었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100001815.1.15",
          "form": "동아리 활동은 하셨나요?",
          "original_form": "동아리 활동은 하셨나요?",
          "speaker_id": "1",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100001815.1.16",
          "form": "어 저 리코더부 해봤어요",
          "original_form": "어 저 리코더부 해봤어요",
          "speaker_id": "2",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100001815.1.17",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100001815.1.18",
          "form": "저는 동아리 활동을 안했어요ㅠㅠ",
          "original_form": "저는 동아리 활동을 안했어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001815.1.19",
          "form": "이것저것 해볼걸 후회되네요ㅠㅠ",
          "original_form": "이것저것 해볼걸 후회되네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001815.1.20",
          "form": "리코더부면....리코더 잘부시나요??!!",
          "original_form": "리코더부면....리코더 잘부시나요??!!",
          "speaker_id": "1",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001815.1.21",
          "form": "그땐 잘 불었던 거 같은데",
          "original_form": "그땐 잘 불었던 거 같은데",
          "speaker_id": "2",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001815.1.22",
          "form": "지금은 다 잊어버렸어요",
          "original_form": "지금은 다 잊어버렸어요",
          "speaker_id": "2",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001815.1.23",
          "form": "저도 다른거 해볼걸 그랬어요ㅠㅠ",
          "original_form": "저도 다른거 해볼걸 그랬어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001815.1.24",
          "form": "저는 리코더 초딩때 음악 시간에 배운게 끝이더라구요",
          "original_form": "저는 리코더 초딩때 음악 시간에 배운게 끝이더라구요",
          "speaker_id": "1",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001815.1.25",
          "form": "침 튀기면서 참 열심히 했는데ㅎㅎ",
          "original_form": "침 튀기면서 참 열심히 했는데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001815.1.26",
          "form": "name2님은 어느 과목 좋아하셨나요?",
          "original_form": "&name2&님은 어느 과목 좋아하셨나요?",
          "speaker_id": "1",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001815.1.27",
          "form": "저는 국어 좋아했어요!",
          "original_form": "저는 국어 좋아했어요!",
          "speaker_id": "2",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001815.1.28",
          "form": "그리고 과학이 정말정말 어려웠어요ㅠㅠ",
          "original_form": "그리고 과학이 정말정말 어려웠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001815.1.29",
          "form": "저도 과학ㅠㅠㅠ",
          "original_form": "저도 과학ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 21:50"
        },
        {
          "id": "MDRW2100001815.1.30",
          "form": "아무리 봐도 뭐라는지 모르겠는 느낌ㅠㅠㅠ",
          "original_form": "아무리 봐도 뭐라는지 모르겠는 느낌ㅠㅠㅠ",
