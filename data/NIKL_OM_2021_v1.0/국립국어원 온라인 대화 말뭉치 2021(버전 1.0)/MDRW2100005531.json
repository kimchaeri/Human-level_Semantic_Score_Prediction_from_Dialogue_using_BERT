      "utterance": [
        {
          "id": "MDRW2100005531.1.1",
          "form": "마",
          "original_form": "마",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005531.1.2",
          "form": "오버워치 요즘 다 접었는데",
          "original_form": "오버워치 요즘 다 접었는데",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005531.1.3",
          "form": "아직도 하는놈",
          "original_form": "아직도 하는놈",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005531.1.4",
          "form": "이번에 오버워치 리그는 어캐됐노",
          "original_form": "이번에 오버워치 리그는 어캐됐노",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005531.1.5",
          "form": "이번에 옵치리그?",
          "original_form": "이번에 옵치리그?",
          "speaker_id": "2",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005531.1.6",
          "form": "중국팀이랑 외국팀이 결승올라감",
          "original_form": "중국팀이랑 외국팀이 결승올라감",
          "speaker_id": "2",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005531.1.7",
          "form": "한국인들이 다 해먹는 시대는 끝났다",
          "original_form": "한국인들이 다 해먹는 시대는 끝났다",
          "speaker_id": "2",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005531.1.8",
          "form": "옵치 프로게이머는 놀라운게",
          "original_form": "옵치 프로게이머는 놀라운게",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005531.1.9",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005531.1.10",
          "form": "이제 한국인들",
          "original_form": "이제 한국인들",
          "speaker_id": "1",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005531.1.11",
          "form": "아무도 못올라갔나?",
          "original_form": "아무도 못올라갔나?",
          "speaker_id": "1",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005531.1.12",
          "form": "노노",
          "original_form": "노노",
          "speaker_id": "2",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005531.1.13",
          "form": "중국팀 하나",
          "original_form": "중국팀 하나",
          "speaker_id": "2",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005531.1.14",
          "form": "다국적팀 하나",
          "original_form": "다국적팀 하나",
          "speaker_id": "2",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005531.1.15",
          "form": "다국적팀 대부분이 한국인",
          "original_form": "다국적팀 대부분이 한국인",
          "speaker_id": "2",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005531.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005531.1.17",
          "form": "그래도 한국인들이",
          "original_form": "그래도 한국인들이",
          "speaker_id": "1",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005531.1.18",
          "form": "올라가긴했내!",
          "original_form": "올라가긴했내!",
          "speaker_id": "1",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005531.1.19",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005531.1.20",
          "form": "그러하다",
          "original_form": "그러하다",
          "speaker_id": "2",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005531.1.21",
          "form": "옵치리그는 근데 이제 내년에 끝날듯",
          "original_form": "옵치리그는 근데 이제 내년에 끝날듯",
          "speaker_id": "2",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005531.1.22",
          "form": "인기도 별로 없고",
          "original_form": "인기도 별로 없고",
          "speaker_id": "2",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005531.1.23",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "1",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005531.1.24",
          "form": "오버워치가 인기가 예전같지가않지",
          "original_form": "오버워치가 인기가 예전같지가않지",
          "speaker_id": "1",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005531.1.25",
          "form": "유명한 프로게이머도 요즘 없고",
          "original_form": "유명한 프로게이머도 요즘 없고",
          "speaker_id": "2",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005531.1.26",
          "form": "롤 이길때도 있었는데",
          "original_form": "롤 이길때도 있었는데",
          "speaker_id": "2",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005531.1.27",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005531.1.28",
          "form": "페이커 이런애들은",
          "original_form": "페이커 이런애들은",
          "speaker_id": "1",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005531.1.29",
          "form": "이제 다 은퇴했제?",
          "original_form": "이제 다 은퇴했제?",
          "speaker_id": "1",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005531.1.30",
          "form": "페이커는 롤",
          "original_form": "페이커는 롤",
          "speaker_id": "2",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005531.1.31",
          "form": "아직 놀랍게도 현역이다",
          "original_form": "아직 놀랍게도 현역이다",
          "speaker_id": "2",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005531.1.32",
          "form": "지금도 중딱은 함",
          "original_form": "지금도 중딱은 함",
          "speaker_id": "2",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005531.1.33",
          "form": "아??",
          "original_form": "아??",
          "speaker_id": "1",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005531.1.34",
          "form": "페이커는 롤이구나",
          "original_form": "페이커는 롤이구나",
          "speaker_id": "1",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005531.1.35",
          "form": "오버워치 유명한 프로게이머는 누군데",
          "original_form": "오버워치 유명한 프로게이머는 누군데",
          "speaker_id": "1",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005531.1.36",
          "form": "세최였던 때에 비하면 못하지만",
          "original_form": "세최였던 때에 비하면 못하지만",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005531.1.37",
          "form": "류제홍?",
          "original_form": "류제홍?",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005531.1.38",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005531.1.39",
          "form": "근데 사실 그런것도",
          "original_form": "근데 사실 그런것도",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005531.1.40",
          "form": "쪼낙도 있고",
          "original_form": "쪼낙도 있고",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005531.1.41",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005531.1.42",
          "form": "나이들면 더 하기 힘드니까",
          "original_form": "나이들면 더 하기 힘드니까",
          "speaker_id": "1",
          "time": "20210821 23:05"
        }
      ]
    }
