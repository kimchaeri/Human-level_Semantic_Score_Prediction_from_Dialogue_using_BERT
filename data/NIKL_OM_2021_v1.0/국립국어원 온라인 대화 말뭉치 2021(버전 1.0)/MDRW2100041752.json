      "utterance": [
        {
          "id": "MDRW2100041752.1.1",
          "form": "금요일부터 일요일까지 아주 화창하니 좋은 날씨였는데",
          "original_form": "금요일부터 일요일까지 아주 화창하니 좋은 날씨였는데",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041752.1.2",
          "form": "오늘은 흐리네요 내일 비온대요",
          "original_form": "오늘은 흐리네요 내일 비온대요",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041752.1.3",
          "form": "오늘 해가쨍쨍한데 비가오더라구요..? 허허ㅋㅋㅋㅋ",
          "original_form": "오늘 해가쨍쨍한데 비가오더라구요..? 허허ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041752.1.4",
          "form": "당황했습니다",
          "original_form": "당황했습니다",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041752.1.5",
          "form": "아 그런 날 있죠",
          "original_form": "아 그런 날 있죠",
          "speaker_id": "2",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041752.1.6",
          "form": "네 우양산이어서",
          "original_form": "네 우양산이어서",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041752.1.7",
          "form": "급하게썼습니다ㅋㅋㅋ",
          "original_form": "급하게썼습니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041752.1.8",
          "form": "옛말에 그런 날은 호랑이 장가가는 날이라고 했었는데",
          "original_form": "옛말에 그런 날은 호랑이 장가가는 날이라고 했었는데",
          "speaker_id": "2",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041752.1.9",
          "form": "아ㅋㅋㅋ이런날을 모라고하더라 혼자 생각하고있었어요ㅋㅋㄱㄲ",
          "original_form": "아ㅋㅋㅋ이런날을 모라고하더라 혼자 생각하고있었어요ㅋㅋㄱㄲ",
          "speaker_id": "1",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100041752.1.10",
          "form": "흔치 않은 날씨인데 이번 여름에도 그러고 종종 그런 날이 있었어요",
          "original_form": "흔치 않은 날씨인데 이번 여름에도 그러고 종종 그런 날이 있었어요",
          "speaker_id": "2",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100041752.1.11",
          "form": "오늘진짜 오락가락하네요 우산을접었다폈다ㅋㅋㅋ",
          "original_form": "오늘진짜 오락가락하네요 우산을접었다폈다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:20"
        },
        {
          "id": "MDRW2100041752.1.12",
          "form": "여기는 계속 흐리다가 비가 오기 시작했어요",
          "original_form": "여기는 계속 흐리다가 비가 오기 시작했어요",
          "speaker_id": "2",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100041752.1.13",
          "form": "내일까지 온대요",
          "original_form": "내일까지 온대요",
          "speaker_id": "2",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100041752.1.14",
          "form": "빨래는 또 안마르겠죠",
          "original_form": "빨래는 또 안마르겠죠",
          "speaker_id": "2",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100041752.1.15",
          "form": "ㅜㅜ 하..진짜날씨ㅜㅜ..",
          "original_form": "ㅜㅜ 하..진짜날씨ㅜㅜ..",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041752.1.16",
          "form": "꿉꿉한데이러면..",
          "original_form": "꿉꿉한데이러면..",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041752.1.17",
          "form": "내일까지 오고 다시 해가 쨍하고 나왔으면  좋겠네요",
          "original_form": "내일까지 오고 다시 해가 쨍하고 나왔으면  좋겠네요",
          "speaker_id": "2",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100041752.1.18",
          "form": "건조기가.. 또.. 생각이..나네요",
          "original_form": "건조기가.. 또.. 생각이..나네요",
          "speaker_id": "2",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100041752.1.19",
          "form": "맞아요 늘 아쉬운건조기...",
          "original_form": "맞아요 늘 아쉬운건조기...",
          "speaker_id": "1",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100041752.1.20",
          "form": "지난 장마때 사람들과 이야기하면서 엄청 건조기 타령했엇거든요",
          "original_form": "지난 장마때 사람들과 이야기하면서 엄청 건조기 타령했엇거든요",
          "speaker_id": "2",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100041752.1.21",
          "form": "그때 거의 2주를 비가 왔으니까요",
          "original_form": "그때 거의 2주를 비가 왔으니까요",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100041752.1.22",
          "form": "주말동안 해가 떠서 좋았는데 또 비가오니 생각나네요",
          "original_form": "주말동안 해가 떠서 좋았는데 또 비가오니 생각나네요",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100041752.1.23",
          "form": "집만넓음 당장샀을거예요ㅜㅜ흑흑",
          "original_form": "집만넓음 당장샀을거예요ㅜㅜ흑흑",
          "speaker_id": "1",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100041752.1.24",
          "form": "가을비가 오고 있어요",
          "original_form": "가을비가 오고 있어요",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100041752.1.25",
          "form": "이번비는 가을장마는 아닌 듯하고 아마 태풍이 지나가나 싶게 바람도 꽤 부네요",
          "original_form": "이번비는 가을장마는 아닌 듯하고 아마 태풍이 지나가나 싶게 바람도 꽤 부네요",
          "speaker_id": "2",
          "time": "20210906 21:16"
        }
      ]
    }
