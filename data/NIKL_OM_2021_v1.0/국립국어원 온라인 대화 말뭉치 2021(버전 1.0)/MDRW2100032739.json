      "utterance": [
        {
          "id": "MDRW2100032739.1.1",
          "form": "형제 있으신가요?",
          "original_form": "형제 있으신가요?",
          "speaker_id": "1",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100032739.1.2",
          "form": "자매나?!",
          "original_form": "자매나?!",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032739.1.3",
          "form": "3형재에요",
          "original_form": "3형재에요",
          "speaker_id": "2",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032739.1.4",
          "form": "name1은요",
          "original_form": "&name1&은요",
          "speaker_id": "2",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032739.1.5",
          "form": "우와 저는 오빠 한 명 있어요",
          "original_form": "우와 저는 오빠 한 명 있어요",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032739.1.6",
          "form": "나이차이는 얼마나 나나요?",
          "original_form": "나이차이는 얼마나 나나요?",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032739.1.7",
          "form": "저는 오빠랑 2살 차이랍니다 ㅎ.ㅎ",
          "original_form": "저는 오빠랑 2살 차이랍니다 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032739.1.8",
          "form": "네 전 오빠 둘이 있어요",
          "original_form": "네 전 오빠 둘이 있어요",
          "speaker_id": "2",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032739.1.9",
          "form": "오빠랑 나이차이가 많이 나는 편인가요?",
          "original_form": "오빠랑 나이차이가 많이 나는 편인가요?",
          "speaker_id": "1",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032739.1.10",
          "form": "전 4살 7살 차이랒니다",
          "original_form": "전 4살 7살 차이랒니다",
          "speaker_id": "2",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032739.1.11",
          "form": "우와 많이나네요!",
          "original_form": "우와 많이나네요!",
          "speaker_id": "1",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032739.1.12",
          "form": "나이차이가 많이나면 좋은 점 있나요?",
          "original_form": "나이차이가 많이나면 좋은 점 있나요?",
          "speaker_id": "1",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032739.1.13",
          "form": "주위사람들 보면 잘해준다고 하던데요",
          "original_form": "주위사람들 보면 잘해준다고 하던데요",
          "speaker_id": "1",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032739.1.14",
          "form": "네  정말 잘 챙겨줘요",
          "original_form": "네  정말 잘 챙겨줘요",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100032739.1.15",
          "form": "부럽네요~~~!",
          "original_form": "부럽네요~~~!",
          "speaker_id": "1",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100032739.1.16",
          "form": "나한테 왜 이렇게 잘해주나 고마울때가 많아요",
          "original_form": "나한테 왜 이렇게 잘해주나 고마울때가 많아요",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100032739.1.17",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100032739.1.18",
          "form": "그만큼 name2님이 잘하셔서 그런게 아닌가요?!",
          "original_form": "그만큼 &name2&님이 잘하셔서 그런게 아닌가요?!",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100032739.1.19",
          "form": "음...저희오빠는...그저그래요 ㅎ.ㅎ",
          "original_form": "음...저희오빠는...그저그래요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100032739.1.20",
          "form": "저도 비슷하게 관심이 없어서요 ㅜㅜㅋㅋㅋ",
          "original_form": "저도 비슷하게 관심이 없어서요 ㅜㅜㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100032739.1.21",
          "form": "전 별로 한게 없어요",
          "original_form": "전 별로 한게 없어요",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100032739.1.22",
          "form": "받기만한듯해서요",
          "original_form": "받기만한듯해서요",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100032739.1.23",
          "form": "아니에요!오빠분들은 다르게 느끼셨을 수도 있죠!",
          "original_form": "아니에요!오빠분들은 다르게 느끼셨을 수도 있죠!",
