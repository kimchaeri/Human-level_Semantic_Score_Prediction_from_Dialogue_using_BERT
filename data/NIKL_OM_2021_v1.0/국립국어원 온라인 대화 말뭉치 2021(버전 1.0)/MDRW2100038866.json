      "utterance": [
        {
          "id": "MDRW2100038866.1.1",
          "form": "못먹어요ㅋㅋ",
          "original_form": "못먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100038866.1.2",
          "form": "오기로 먹는거죠",
          "original_form": "오기로 먹는거죠",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100038866.1.3",
          "form": "얘기하니 먹고싶네요\r\n매운맛은 중독이에요",
          "original_form": "얘기하니 먹고싶네요\r\n매운맛은 중독이에요",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100038866.1.4",
          "form": "맞아요 퍽하면 땡기고 그렇죠",
          "original_form": "맞아요 퍽하면 땡기고 그렇죠",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100038866.1.5",
          "form": "배달앱 할인있을땐 안시킬수가 없어요",
          "original_form": "배달앱 할인있을땐 안시킬수가 없어요",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100038866.1.6",
          "form": "저는 배달앱을 안써서요...ㅎㅎ",
          "original_form": "저는 배달앱을 안써서요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100038866.1.7",
          "form": "만원 할인 이런건 버릴수 없잖아요ㅋ",
          "original_form": "만원 할인 이런건 버릴수 없잖아요ㅋ",
          "speaker_id": "1",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100038866.1.8",
          "form": "와 그건 진짜 크네요",
          "original_form": "와 그건 진짜 크네요",
          "speaker_id": "2",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038866.1.9",
          "form": "네 개미지옥이요ㅋ",
          "original_form": "네 개미지옥이요ㅋ",
          "speaker_id": "1",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100038866.1.10",
          "form": "이참에 배달앱 깔까봐요",
          "original_form": "이참에 배달앱 깔까봐요",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100038866.1.11",
          "form": "가끔 파바 마논이상 오처논주거든요\r\n매일삽니다",
          "original_form": "가끔 파바 마논이상 오처논주거든요\r\n매일삽니다",
          "speaker_id": "1",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100038866.1.12",
          "form": "엄청 뿌리는군요",
          "original_form": "엄청 뿌리는군요",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038866.1.13",
          "form": "장사가 되나 모르겠네요",
          "original_form": "장사가 되나 모르겠네요",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038866.1.14",
          "form": "저도 그게궁금해요",
          "original_form": "저도 그게궁금해요",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038866.1.15",
          "form": "마케팅인걸까요",
          "original_form": "마케팅인걸까요",
          "speaker_id": "2",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100038866.1.16",
          "form": "파바가 마케팅할 짬밥은 아닌데...",
          "original_form": "파바가 마케팅할 짬밥은 아닌데...",
          "speaker_id": "2",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100038866.1.17",
          "form": "요기요가 그만큼 대주는건지..",
          "original_form": "요기요가 그만큼 대주는건지..",
          "speaker_id": "1",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100038866.1.18",
          "form": "그러면서 이익나는거 보면 신기해요",
          "original_form": "그러면서 이익나는거 보면 신기해요",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100038866.1.19",
          "form": "그래서 쿠폰나오면 무조건 먹어요ㅋ",
          "original_form": "그래서 쿠폰나오면 무조건 먹어요ㅋ",
          "speaker_id": "1",
          "time": "20210902 22:59"
        }
