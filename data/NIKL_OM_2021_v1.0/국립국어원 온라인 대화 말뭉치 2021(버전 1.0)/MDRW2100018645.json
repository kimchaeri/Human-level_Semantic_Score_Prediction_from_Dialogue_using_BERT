      "utterance": [
        {
          "id": "MDRW2100018645.1.1",
          "form": "동물 키우시나요?",
          "original_form": "동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210827 09:49"
        },
        {
          "id": "MDRW2100018645.1.2",
          "form": "아니욥",
          "original_form": "아니욥",
          "speaker_id": "2",
          "time": "20210827 09:53"
        },
        {
          "id": "MDRW2100018645.1.3",
          "form": "동물 좋아하시나요?",
          "original_form": "동물 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 09:58"
        },
        {
          "id": "MDRW2100018645.1.4",
          "form": "좋아합니다",
          "original_form": "좋아합니다",
          "speaker_id": "2",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018645.1.5",
          "form": "어떤 동물을 제일 좋아하세요?",
          "original_form": "어떤 동물을 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 10:00"
        },
        {
          "id": "MDRW2100018645.1.6",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "2",
          "time": "20210827 10:05"
        },
        {
          "id": "MDRW2100018645.1.7",
          "form": "반려견키우셧나요?",
          "original_form": "반려견키우셧나요?",
          "speaker_id": "2",
          "time": "20210827 10:05"
        },
        {
          "id": "MDRW2100018645.1.8",
          "form": "네 예전에 키웠어요!",
          "original_form": "네 예전에 키웠어요!",
          "speaker_id": "1",
          "time": "20210827 10:06"
        },
        {
          "id": "MDRW2100018645.1.9",
          "form": "지금은 고양이를 키우고 있어요",
          "original_form": "지금은 고양이를 키우고 있어요",
          "speaker_id": "1",
          "time": "20210827 10:06"
        },
        {
          "id": "MDRW2100018645.1.10",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210827 10:07"
        },
        {
          "id": "MDRW2100018645.1.11",
          "form": "나이더들고 시간 많이 보낼수잇을때",
          "original_form": "나이더들고 시간 많이 보낼수잇을때",
          "speaker_id": "2",
          "time": "20210827 10:08"
        },
        {
          "id": "MDRW2100018645.1.12",
          "form": "강아지한마리 키우고싶어요",
          "original_form": "강아지한마리 키우고싶어요",
          "speaker_id": "2",
          "time": "20210827 10:08"
        },
        {
          "id": "MDRW2100018645.1.13",
          "form": "확실히 그게 좋은거 같아요",
          "original_form": "확실히 그게 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210827 10:21"
        },
        {
          "id": "MDRW2100018645.1.14",
          "form": "같이 보내지 못하는 시간들이 너무 아쉬워요",
          "original_form": "같이 보내지 못하는 시간들이 너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210827 10:21"
        },
        {
          "id": "MDRW2100018645.1.15",
          "form": "어떤 강아지키우셧나욤?",
          "original_form": "어떤 강아지키우셧나욤?",
          "speaker_id": "2",
          "time": "20210827 10:22"
        },
        {
          "id": "MDRW2100018645.1.16",
          "form": "치와와 키웠어요!",
          "original_form": "치와와 키웠어요!",
          "speaker_id": "1",
          "time": "20210827 10:23"
        },
        {
          "id": "MDRW2100018645.1.17",
          "form": "지금 고양이는 몇살인가요?",
          "original_form": "지금 고양이는 몇살인가요?",
          "speaker_id": "2",
          "time": "20210827 10:23"
        },
        {
          "id": "MDRW2100018645.1.18",
          "form": "어릴때 차 밑에 누가 두고 가서 만나게 되었어요",
          "original_form": "어릴때 차 밑에 누가 두고 가서 만나게 되었어요",
          "speaker_id": "1",
          "time": "20210827 10:24"
        },
        {
          "id": "MDRW2100018645.1.19",
          "form": "이제 3살 됐어요!",
          "original_form": "이제 3살 됐어요!",
          "speaker_id": "1",
          "time": "20210827 10:24"
        },
        {
          "id": "MDRW2100018645.1.20",
          "form": "우와ㅠㅠ",
          "original_form": "우와ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 10:24"
        },
        {
          "id": "MDRW2100018645.1.21",
          "form": "맘씨좋은분이세요ㅜㅜ",
          "original_form": "맘씨좋은분이세요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 10:24"
        },
        {
          "id": "MDRW2100018645.1.22",
          "form": "아니예요ㅠ 어렸을때라 부모님이 받아주셨다는게 감사해요",
          "original_form": "아니예요ㅠ 어렸을때라 부모님이 받아주셨다는게 감사해요",
          "speaker_id": "1",
          "time": "20210827 10:24"
        }
      ]
