      "utterance": [
        {
          "id": "MDRW2100011663.1.1",
          "form": "일하는게 싫어요",
          "original_form": "일하는게 싫어요",
          "speaker_id": "1",
          "time": "20210824 14:57"
        },
        {
          "id": "MDRW2100011663.1.2",
          "form": "맞아요. 이상한 사람은 어찌나 많은지ㅡㅡ",
          "original_form": "맞아요. 이상한 사람은 어찌나 많은지ㅡㅡ",
          "speaker_id": "2",
          "time": "20210824 14:59"
        },
        {
          "id": "MDRW2100011663.1.3",
          "form": "오늘도 난 일개미로 열심히 살아갑니다",
          "original_form": "오늘도 난 일개미로 열심히 살아갑니다",
          "speaker_id": "1",
          "time": "20210824 14:59"
        },
        {
          "id": "MDRW2100011663.1.4",
          "form": "네 월급날은 오니까요",
          "original_form": "네 월급날은 오니까요",
          "speaker_id": "2",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011663.1.5",
          "form": "직장 옮기고 싶어요",
          "original_form": "직장 옮기고 싶어요",
          "speaker_id": "1",
          "time": "20210824 15:02"
        },
        {
          "id": "MDRW2100011663.1.6",
          "form": "무슨 스트레스가 크신가요",
          "original_form": "무슨 스트레스가 크신가요",
          "speaker_id": "2",
          "time": "20210824 15:02"
        },
        {
          "id": "MDRW2100011663.1.7",
          "form": "사람들과 안맞아요",
          "original_form": "사람들과 안맞아요",
          "speaker_id": "1",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011663.1.8",
          "form": "사람들이 진짜 제일 스트레스예요",
          "original_form": "사람들이 진짜 제일 스트레스예요",
          "speaker_id": "2",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011663.1.9",
          "form": "저도 맞는 사람 없어요 ㅜㄴ",
          "original_form": "저도 맞는 사람 없어요 ㅜㄴ",
          "speaker_id": "2",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011663.1.10",
          "form": "그러게 근데 나도ㅜ이제 약간 질렸다고해야하나",
          "original_form": "그러게 근데 나도ㅜ이제 약간 질렸다고해야하나",
          "speaker_id": "1",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011663.1.11",
          "form": "근데 집이 가까워서",
          "original_form": "근데 집이 가까워서",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011663.1.12",
          "form": "직장이 가깝다고요?",
          "original_form": "직장이 가깝다고요?",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011663.1.13",
          "form": "난 멀기까지합니다",
          "original_form": "난 멀기까지합니다",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011663.1.14",
          "form": "네 직장이요",
          "original_form": "네 직장이요",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011663.1.15",
          "form": "아이구",
          "original_form": "아이구",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011663.1.16",
          "form": "가까운게 좋지",
          "original_form": "가까운게 좋지",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011663.1.17",
          "form": "운전하면 좀 낫지 않으세요?",
          "original_form": "운전하면 좀 낫지 않으세요?",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011663.1.18",
          "form": "맞아 운전하는 시간이 젤로 좋음",
          "original_form": "맞아 운전하는 시간이 젤로 좋음",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011663.1.19",
          "form": "승진 못하면 여기서 더 있을까봐오",
          "original_form": "승진 못하면 여기서 더 있을까봐오",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011663.1.20",
          "form": "직장 스트레스 운전으로 훠이",
          "original_form": "직장 스트레스 운전으로 훠이",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011663.1.21",
          "form": "일단 돈을 써야",
          "original_form": "일단 돈을 써야",
          "speaker_id": "2",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011663.1.22",
          "form": "애사심이 생긴데요",
          "original_form": "애사심이 생긴데요",
          "speaker_id": "2",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011663.1.23",
          "form": "너무 알뜰하신건 아닌지",
          "original_form": "너무 알뜰하신건 아닌지",
          "speaker_id": "2",
          "time": "20210824 15:21"
        }
      ]
