      "utterance": [
        {
          "id": "MDRW2100005840.1.1",
          "form": "집놀이족 이거 나잖어",
          "original_form": "집놀이족 이거 나잖어",
          "speaker_id": "2",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100005840.1.2",
          "form": "전 이북리더(김정은x)기요",
          "original_form": "전 이북리더(김정은x)기요",
          "speaker_id": "2",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100005840.1.3",
          "form": "저는.. 닌텐도 스위치요",
          "original_form": "저는.. 닌텐도 스위치요",
          "speaker_id": "1",
          "time": "20210907 13:58"
        },
        {
          "id": "MDRW2100005840.1.4",
          "form": "저 아이패드 추가합니다",
          "original_form": "저 아이패드 추가합니다",
          "speaker_id": "2",
          "time": "20210907 13:59"
        },
        {
          "id": "MDRW2100005840.1.5",
          "form": "스마트폰도...",
          "original_form": "스마트폰도...",
          "speaker_id": "2",
          "time": "20210907 13:59"
        },
        {
          "id": "MDRW2100005840.1.6",
          "form": "걍.. 사실 우리의 삶임",
          "original_form": "걍.. 사실 우리의 삶임",
          "speaker_id": "1",
          "time": "20210907 13:59"
        },
        {
          "id": "MDRW2100005840.1.7",
          "form": "그머지 거치대?",
          "original_form": "그머지 거치대?",
          "speaker_id": "1",
          "time": "20210907 13:59"
        },
        {
          "id": "MDRW2100005840.1.8",
          "form": "태블릿거치대????",
          "original_form": "태블릿거치대????",
          "speaker_id": "1",
          "time": "20210907 13:59"
        },
        {
          "id": "MDRW2100005840.1.9",
          "form": "그거있으면 진짜 더이상 인간이 아닌 무언가가된다는데",
          "original_form": "그거있으면 진짜 더이상 인간이 아닌 무언가가된다는데",
          "speaker_id": "1",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100005840.1.10",
          "form": "침대인가요?",
          "original_form": "침대인가요?",
          "speaker_id": "2",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100005840.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100005840.1.12",
          "form": "공부하려고 산 독서대에 주로 태블릿 거치해두긴 합니다만...",
          "original_form": "공부하려고 산 독서대에 주로 태블릿 거치해두긴 합니다만...",
          "speaker_id": "2",
          "time": "20210907 14:03"
        },
        {
          "id": "MDRW2100005840.1.13",
          "form": "난 애초에 그 뒤에 삼각형으로 접히는? 케이스사가지고ㅋㅋㅋㅋ",
          "original_form": "난 애초에 그 뒤에 삼각형으로 접히는? 케이스사가지고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:05"
        },
        {
          "id": "MDRW2100005840.1.14",
          "form": "기본 아닌가요",
          "original_form": "기본 아닌가요",
          "speaker_id": "2",
          "time": "20210907 14:05"
        },
        {
          "id": "MDRW2100005840.1.15",
          "form": "집 어디서든 누워서 보기 ㅁ삽가능",
          "original_form": "집 어디서든 누워서 보기 ㅁ삽가능",
          "speaker_id": "2",
          "time": "20210907 14:05"
        },
        {
          "id": "MDRW2100005840.1.16",
          "form": "이북리더기가 가벼워서 집에 누워서 보기 딱이랍니다",
          "original_form": "이북리더기가 가벼워서 집에 누워서 보기 딱이랍니다",
          "speaker_id": "2",
          "time": "20210907 14:05"
        },
        {
          "id": "MDRW2100005840.1.17",
          "form": "액정이 깨져서 유감이군요",
          "original_form": "액정이 깨져서 유감이군요",
          "speaker_id": "2",
          "time": "20210907 14:05"
        },
        {
          "id": "MDRW2100005840.1.18",
          "form": "울지않아 난 얼은이니까",
          "original_form": "울지않아 난 얼은이니까",
          "speaker_id": "1",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100005840.1.19",
          "form": "근데 이게 자리가 사람을만든다고",
          "original_form": "근데 이게 자리가 사람을만든다고",
          "speaker_id": "1",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100005840.1.20",
          "form": "이거 말고 다른 사람들은 집에서 뭐 가지고 놀까???",
          "original_form": "이거 말고 다른 사람들은 집에서 뭐 가지고 놀까???",
          "speaker_id": "2",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100005840.1.21",
          "form": "나 영상 별로안보는편인데 태블릿이랑 스위치생기고 유튜브 많이보게됨",
          "original_form": "나 영상 별로안보는편인데 태블릿이랑 스위치생기고 유튜브 많이보게됨",
          "speaker_id": "1",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100005840.1.22",
          "form": "인정합니다",
          "original_form": "인정합니다",
          "speaker_id": "2",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100005840.1.23",
          "form": "다른사람들은 집에서 안놀죠",
          "original_form": "다른사람들은 집에서 안놀죠",
          "speaker_id": "1",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100005840.1.24",
          "form": "아무래도 유튜브 시청시간이 제일 많이 늘었음",
          "original_form": "아무래도 유튜브 시청시간이 제일 많이 늘었음",
          "speaker_id": "2",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100005840.1.25",
          "form": "넷플릭스는 생각보다 안 봐지더라고",
          "original_form": "넷플릭스는 생각보다 안 봐지더라고",
          "speaker_id": "2",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100005840.1.26",
          "form": "다른사람들은 나가서 논답니다",
          "original_form": "다른사람들은 나가서 논답니다",
          "speaker_id": "1",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100005840.1.27",
          "form": "난 내가 평균이고 동생이 이상한건줄알았거든",
          "original_form": "난 내가 평균이고 동생이 이상한건줄알았거든",
          "speaker_id": "1",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100005840.1.28",
          "form": "내가 이상한거더라",
          "original_form": "내가 이상한거더라",
          "speaker_id": "1",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100005840.1.29",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210907 14:09"
        },
        {
          "id": "MDRW2100005840.1.30",
          "form": "어라라",
          "original_form": "어라라",
          "speaker_id": "2",
          "time": "20210907 14:09"
        },
        {
          "id": "MDRW2100005840.1.31",
          "form": "집에서 놀 게 얼마나 많은데",
          "original_form": "집에서 놀 게 얼마나 많은데",
          "speaker_id": "2",
          "time": "20210907 14:09"
        },
        {
          "id": "MDRW2100005840.1.32",
          "form": "요새는 홈트도 하니까 ㅋㅋㅋㅋ",
          "original_form": "요새는 홈트도 하니까 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:11"
        },
        {
          "id": "MDRW2100005840.1.33",
          "form": "집에서하는게 훨씬 효율적인거같아",
          "original_form": "집에서하는게 훨씬 효율적인거같아",
          "speaker_id": "1",
          "time": "20210907 14:11"
        },
        {
          "id": "MDRW2100005840.1.34",
          "form": "싸이클같은건 집에서도 탈수있교 ㅋㅋㅋㅋㅋ",
          "original_form": "싸이클같은건 집에서도 탈수있교 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:11"
        },
        {
          "id": "MDRW2100005840.1.35",
          "form": "요가매트 많이 사시더라고요",
          "original_form": "요가매트 많이 사시더라고요",
          "speaker_id": "2",
          "time": "20210907 14:12"
        },
        {
          "id": "MDRW2100005840.1.36",
          "form": "요가매트 있으면 바닥에 눕기 용이할 것 같아요",
          "original_form": "요가매트 있으면 바닥에 눕기 용이할 것 같아요",
          "speaker_id": "2",
          "time": "20210907 14:12"
        },
        {
          "id": "MDRW2100005840.1.37",
          "form": "ㅁㅈㅁㅈ 요가매트나 덤벨 이런것만 있어도 충분함",
          "original_form": "ㅁㅈㅁㅈ 요가매트나 덤벨 이런것만 있어도 충분함",
          "speaker_id": "1",
          "time": "20210907 14:12"
        },
        {
          "id": "MDRW2100005840.1.38",
          "form": "침대가 있어 굳이 사지 않아요",
          "original_form": "침대가 있어 굳이 사지 않아요",
          "speaker_id": "2",
          "time": "20210907 14:13"
        },
        {
          "id": "MDRW2100005840.1.39",
          "form": "헉 어케알앗지",
          "original_form": "헉 어케알앗지",
          "speaker_id": "1",
          "time": "20210907 14:13"
        },
        {
          "id": "MDRW2100005840.1.40",
          "form": "방바닥에 ㄷ엎드릴때 쌉유용",
          "original_form": "방바닥에 ㄷ엎드릴때 쌉유용",
          "speaker_id": "1",
          "time": "20210907 14:13"
        },
        {
          "id": "MDRW2100005840.1.41",
          "form": "집놀이 천재거든요",
          "original_form": "집놀이 천재거든요",
          "speaker_id": "2",
          "time": "20210907 14:13"
        },
        {
          "id": "MDRW2100005840.1.42",
