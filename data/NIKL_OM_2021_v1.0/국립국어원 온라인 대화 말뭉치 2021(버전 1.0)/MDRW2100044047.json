      "utterance": [
        {
          "id": "MDRW2100044047.1.1",
          "form": "오 그것도 들어볼게요!",
          "original_form": "오 그것도 들어볼게요!",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044047.1.2",
          "form": "남돌은 아예 관심없으신가요",
          "original_form": "남돌은 아예 관심없으신가요",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044047.1.3",
          "form": "아뇨 없진 않아요 ㅋㅋㅋㅋ많이 덜할뿐",
          "original_form": "아뇨 없진 않아요 ㅋㅋㅋㅋ많이 덜할뿐",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044047.1.4",
          "form": "아이돌 자체를 좋아해수 ㅋㅋㅋㅋ",
          "original_form": "아이돌 자체를 좋아해수 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044047.1.5",
          "form": "아악ㅋㅋㅋ남돌중에서는 누굴좋아하시나요",
          "original_form": "아악ㅋㅋㅋ남돌중에서는 누굴좋아하시나요",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044047.1.6",
          "form": "젘ㅋㅋㅋㅋ근데 요즘 애들은 잘 모르고....",
          "original_form": "젘ㅋㅋㅋㅋ근데 요즘 애들은 잘 모르고....",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044047.1.7",
          "form": "몬스타엑스 좋아했어욬ㅋㅋㅋㅋㅋ",
          "original_form": "몬스타엑스 좋아했어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044047.1.8",
          "form": "헐 대박ㅋㅋㅋㅋ몬엑정도면 요즘애들아닌가요?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐 대박ㅋㅋㅋㅋ몬엑정도면 요즘애들아닌가요?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044047.1.9",
          "form": "아그래요? ㅋㅋㅋㅋㅋ",
          "original_form": "아그래요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044047.1.10",
          "form": "휴 다행이다....ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "휴 다행이다....ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044047.1.11",
          "form": "ㅋㅋㅋㅋ몬엑이 또 노래좋자나요ㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ몬엑이 또 노래좋자나요ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044047.1.12",
          "form": "그게되나 적당히좋아하는게",
          "original_form": "그게되나 적당히좋아하는게",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044047.1.13",
          "form": "크",
          "original_form": "크",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044047.1.14",
          "form": "아니 진짜",
          "original_form": "아니 진짜",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044047.1.15",
          "form": "수록곡들이 더 좋은거아세요?",
          "original_form": "수록곡들이 더 좋은거아세요?",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044047.1.16",
          "form": "저 한동안 몬엑 엄청 빠져서 노래 다들었는데",
          "original_form": "저 한동안 몬엑 엄청 빠져서 노래 다들었는데",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044047.1.17",
          "form": "헐 수록곡 추천해주세요",
          "original_form": "헐 수록곡 추천해주세요",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044047.1.18",
          "form": "수록곡 맛집입니다",
          "original_form": "수록곡 맛집입니다",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044047.1.19",
          "form": "저 숨겨진 수록곡명곡조아해요",
          "original_form": "저 숨겨진 수록곡명곡조아해요",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044047.1.20",
          "form": "하 노래 다외웠었는데",
          "original_form": "하 노래 다외웠었는데",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044047.1.21",
          "form": "벌써 까먹은거 실화인가요",
          "original_form": "벌써 까먹은거 실화인가요",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044047.1.22",
          "form": "안들은지 2년됏나  ..",
          "original_form": "안들은지 2년됏나  ..",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044047.1.23",
          "form": "근데 지금 기억나는건",
          "original_form": "근데 지금 기억나는건",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044047.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ두근두근",
