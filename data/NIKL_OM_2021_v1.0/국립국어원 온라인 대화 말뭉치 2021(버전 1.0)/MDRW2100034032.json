      "utterance": [
        {
          "id": "MDRW2100034032.1.1",
          "form": "좋아하는 연예인 있으세요?",
          "original_form": "좋아하는 연예인 있으세요?",
          "speaker_id": "1",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100034032.1.2",
          "form": "딱히 완전 좋아하는 사람은 없어요",
          "original_form": "딱히 완전 좋아하는 사람은 없어요",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100034032.1.3",
          "form": "이동욱이요",
          "original_form": "이동욱이요",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100034032.1.4",
          "form": "예전부터 잘생기고 ㅋㅋ좋긴했러요",
          "original_form": "예전부터 잘생기고 ㅋㅋ좋긴했러요",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100034032.1.5",
          "form": "뮤비에서 애절한 연기가 기억에도 남고",
          "original_form": "뮤비에서 애절한 연기가 기억에도 남고",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100034032.1.6",
          "form": "오 나이들수록 멋있어지는거 같아요",
          "original_form": "오 나이들수록 멋있어지는거 같아요",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100034032.1.7",
          "form": "맞아요 그리고 피부가 와",
          "original_form": "맞아요 그리고 피부가 와",
          "speaker_id": "2",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100034032.1.8",
          "form": "여자보다 투명 ㅋㅋ",
          "original_form": "여자보다 투명 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100034032.1.9",
          "form": "맞아요ㅋㅋ 그리 하얀지 몰랐는데 엄청 뽀얗더라구요",
          "original_form": "맞아요ㅋㅋ 그리 하얀지 몰랐는데 엄청 뽀얗더라구요",
          "speaker_id": "1",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100034032.1.10",
          "form": "입술색도 예쁘고ㅋㅋ",
          "original_form": "입술색도 예쁘고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100034032.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100034032.1.12",
          "form": "입술이 ㅋㅋ참나",
          "original_form": "입술이 ㅋㅋ참나",
          "speaker_id": "2",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100034032.1.13",
          "form": "이러고보니 좋아하는거 맞네요 ㅋㅋ",
          "original_form": "이러고보니 좋아하는거 맞네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100034032.1.14",
          "form": "누구 좋아하세요?",
          "original_form": "누구 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100034032.1.15",
          "form": "ㅋㅋㅋ저는 송중기요!",
          "original_form": "ㅋㅋㅋ저는 송중기요!",
          "speaker_id": "1",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100034032.1.16",
          "form": "어머 ㅋㅋ",
          "original_form": "어머 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100034032.1.17",
          "form": "피부도 좋고 나이 안먹는거같아욬ㅋㅋ",
          "original_form": "피부도 좋고 나이 안먹는거같아욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100034032.1.18",
          "form": "송중기 안좋아하기 힘들죠",
          "original_form": "송중기 안좋아하기 힘들죠",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100034032.1.19",
          "form": "빈센죠 다 보셨겠네요",
          "original_form": "빈센죠 다 보셨겠네요",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100034032.1.20",
          "form": "그러게요 나이를 안먹어요 피부짱",
          "original_form": "그러게요 나이를 안먹어요 피부짱",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100034032.1.21",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100034032.1.22",
          "form": "어쩜 그리 멋지게 나왔는지ㅋㅋ",
          "original_form": "어쩜 그리 멋지게 나왔는지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100034032.1.23",
          "form": "ㅋㅋ다들 송중기 앓이 했죠",
          "original_form": "ㅋㅋ다들 송중기 앓이 했죠",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100034032.1.24",
          "form": "안할 수 없는 역할을 잘 고르더라구요ㅋㅋ",
          "original_form": "안할 수 없는 역할을 잘 고르더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:30"
        }
      ]
