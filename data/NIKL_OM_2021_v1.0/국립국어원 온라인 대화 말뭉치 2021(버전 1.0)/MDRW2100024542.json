      "utterance": [
        {
          "id": "MDRW2100024542.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 13:37"
        },
        {
          "id": "MDRW2100024542.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210831 13:38"
        },
        {
          "id": "MDRW2100024542.1.3",
          "form": "쇼핑 좋아하세요...?",
          "original_form": "쇼핑 좋아하세요...?",
          "speaker_id": "2",
          "time": "20210831 13:38"
        },
        {
          "id": "MDRW2100024542.1.4",
          "form": "구경하는 거 좋아해요",
          "original_form": "구경하는 거 좋아해요",
          "speaker_id": "1",
          "time": "20210831 13:38"
        },
        {
          "id": "MDRW2100024542.1.5",
          "form": "저도 너무 좋아해요!",
          "original_form": "저도 너무 좋아해요!",
          "speaker_id": "2",
          "time": "20210831 13:41"
        },
        {
          "id": "MDRW2100024542.1.6",
          "form": "온라인 쇼핑도 시간 가는 줄 몰라요!",
          "original_form": "온라인 쇼핑도 시간 가는 줄 몰라요!",
          "speaker_id": "2",
          "time": "20210831 13:41"
        },
        {
          "id": "MDRW2100024542.1.7",
          "form": "맞아요 저는 요즘 생활한복 구경하고 있어요",
          "original_form": "맞아요 저는 요즘 생활한복 구경하고 있어요",
          "speaker_id": "1",
          "time": "20210831 13:55"
        },
        {
          "id": "MDRW2100024542.1.8",
          "form": "오 좋죠! 저도 한때 철릭원피스 같은거 사고싶었아요!",
          "original_form": "오 좋죠! 저도 한때 철릭원피스 같은거 사고싶었아요!",
          "speaker_id": "2",
          "time": "20210831 14:07"
        },
        {
          "id": "MDRW2100024542.1.9",
          "form": "근데 너무 비싼데 입고 나갈 곳이 없어서 ㅋㅋㅋㅋ 포기했지만요",
          "original_form": "근데 너무 비싼데 입고 나갈 곳이 없어서 ㅋㅋㅋㅋ 포기했지만요",
          "speaker_id": "2",
          "time": "20210831 14:08"
        },
        {
          "id": "MDRW2100024542.1.10",
          "form": "맞아요 리슬이 좋다고 추천 받았는데 가격대가 ㅎㄷㄷ..",
          "original_form": "맞아요 리슬이 좋다고 추천 받았는데 가격대가 ㅎㄷㄷ..",
          "speaker_id": "1",
          "time": "20210831 14:11"
        },
        {
          "id": "MDRW2100024542.1.11",
          "form": "오..가격보니 반드시 좋아야겠네요...",
          "original_form": "오..가격보니 반드시 좋아야겠네요...",
          "speaker_id": "2",
          "time": "20210831 14:12"
        },
        {
          "id": "MDRW2100024542.1.12",
          "form": "ㅋㅋㅋㅋㅋ 저는 요즘 와이드 핀턱 슬랙스가 갖고싶어서 열심히 찾아보는 중이에요",
          "original_form": "ㅋㅋㅋㅋㅋ 저는 요즘 와이드 핀턱 슬랙스가 갖고싶어서 열심히 찾아보는 중이에요",
          "speaker_id": "2",
          "time": "20210831 14:13"
        },
        {
          "id": "MDRW2100024542.1.13",
          "form": "오 좋죠",
          "original_form": "오 좋죠",
          "speaker_id": "1",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100024542.1.14",
          "form": "보통 인터넷으로 구매하세요?",
          "original_form": "보통 인터넷으로 구매하세요?",
          "speaker_id": "1",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100024542.1.15",
          "form": "입어보고 싶어서 매장도 돌았었는데 역시 여름에는 밖에 나가는 거 아니더라구요",
          "original_form": "입어보고 싶어서 매장도 돌았었는데 역시 여름에는 밖에 나가는 거 아니더라구요",
          "speaker_id": "2",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100024542.1.16",
          "form": "인터넷이 짱입니다",
          "original_form": "인터넷이 짱입니다",
          "speaker_id": "2",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100024542.1.17",
          "form": "저는 서울스토어 자주봐요",
          "original_form": "저는 서울스토어 자주봐요",
          "speaker_id": "2",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100024542.1.18",
          "form": "저는 집에만 자주 있었더니 살이 너무 쪄서 사이즈 애매하더라구요ㅠㅠ",
          "original_form": "저는 집에만 자주 있었더니 살이 너무 쪄서 사이즈 애매하더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100024542.1.19",
          "form": "맞아요 저도 살 찌고나서 고무줄 바지 많이사요",
          "original_form": "맞아요 저도 살 찌고나서 고무줄 바지 많이사요",
          "speaker_id": "2",
          "time": "20210831 14:21"
        },
        {
          "id": "MDRW2100024542.1.20",
          "form": "요즘 밴딩 바지 많이 나와요!",
          "original_form": "요즘 밴딩 바지 많이 나와요!",
          "speaker_id": "2",
          "time": "20210831 14:21"
        },
        {
          "id": "MDRW2100024542.1.21",
          "form": "저 얼마전에 산 슬랙스도 밴딩이에요ㅜㅜ",
          "original_form": "저 얼마전에 산 슬랙스도 밴딩이에요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 14:21"
        },
        {
          "id": "MDRW2100024542.1.22",
          "form": "ㅋㅋㅋ큐ㅠㅠㅠ 그래서 요즘 다시 운동하려고요",
          "original_form": "ㅋㅋㅋ큐ㅠㅠㅠ 그래서 요즘 다시 운동하려고요",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100024542.1.23",
          "form": "이번에 요가매트도 샀어요 ㅋㅋ",
          "original_form": "이번에 요가매트도 샀어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100024542.1.24",
          "form": "저도 운동할거에요! 저는 저스트댄스 하려구요",
          "original_form": "저도 운동할거에요! 저는 저스트댄스 하려구요",
          "speaker_id": "2",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100024542.1.25",
          "form": "요가매트는 이미 있어서 ㅎㅎ",
          "original_form": "요가매트는 이미 있어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100024542.1.26",
          "form": "그거 재밌죠 체험판 했었는데 동생이랑 하다가 웃겨 죽는 줄 알았어요",
          "original_form": "그거 재밌죠 체험판 했었는데 동생이랑 하다가 웃겨 죽는 줄 알았어요",
          "speaker_id": "1",
          "time": "20210831 14:26"
        },
        {
          "id": "MDRW2100024542.1.27",
          "form": "ㅋㅋㅋㅋㅋ 그쵸 저는 운동 재미없으면 안해서 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 그쵸 저는 운동 재미없으면 안해서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:30"
        },
        {
          "id": "MDRW2100024542.1.28",
          "form": "닌텐도 스위치도 갖고싶네요ㅜㅜ 링피트도 해보고싶아요",
          "original_form": "닌텐도 스위치도 갖고싶네요ㅜㅜ 링피트도 해보고싶아요",
          "speaker_id": "2",
          "time": "20210831 14:30"
        },
        {
          "id": "MDRW2100024542.1.29",
          "form": "저 그거 가지고 있어요! 최근에 샀어요 ㅋㅋㅋ",
          "original_form": "저 그거 가지고 있어요! 최근에 샀어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:34"
        },
        {
          "id": "MDRW2100024542.1.30",
          "form": "링피트요?? 대박",
          "original_form": "링피트요?? 대박",
          "speaker_id": "2",
          "time": "20210831 14:37"
        },
        {
          "id": "MDRW2100024542.1.31",
          "form": "어떠세요...",
          "original_form": "어떠세요...",
          "speaker_id": "2",
          "time": "20210831 14:37"
        },
        {
          "id": "MDRW2100024542.1.32",
          "form": "새거 사셨나요...?? 저는 중고도 알아보고 있아서ㅠㅠ",
          "original_form": "새거 사셨나요...?? 저는 중고도 알아보고 있아서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 14:37"
        },
        {
          "id": "MDRW2100024542.1.33",
          "form": "새거 샀어요 요즘은 구하기 어렵지 않아서요",
          "original_form": "새거 샀어요 요즘은 구하기 어렵지 않아서요",
          "speaker_id": "1",
          "time": "20210831 16:00"
        },
        {
          "id": "MDRW2100024542.1.34",
          "form": "그렇군요!!",
          "original_form": "그렇군요!!",
          "speaker_id": "2",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024542.1.35",
          "form": "운동은 많이 되나요..?",
          "original_form": "운동은 많이 되나요..?",
          "speaker_id": "2",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024542.1.36",
          "form": "네 운동 진짜 많이 되요 한 번 하고 나면 땀 범벅이에요",
          "original_form": "네 운동 진짜 많이 되요 한 번 하고 나면 땀 범벅이에요",
          "speaker_id": "1",
          "time": "20210831 16:29"
        },
        {
          "id": "MDRW2100024542.1.37",
          "form": "재미도 있나요?!?!",
          "original_form": "재미도 있나요?!?!",
          "speaker_id": "2",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100024542.1.38",
          "form": "네 몬스터가 잡히는게 보이니까 성취를 바로바로 보여줘서 재밌어요",
          "original_form": "네 몬스터가 잡히는게 보이니까 성취를 바로바로 보여줘서 재밌어요",
          "speaker_id": "1",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100024542.1.39",
          "form": "우와ㅜㅜ 저도 갑자기 뽐뿌오네요",
          "original_form": "우와ㅜㅜ 저도 갑자기 뽐뿌오네요",
          "speaker_id": "2",
          "time": "20210831 18:36"
        },
        {
          "id": "MDRW2100024542.1.40",
          "form": "할인 안하려나요... ㅎ 다 좋은데 너무 비싼..게임기 ...",
          "original_form": "할인 안하려나요... ㅎ 다 좋은데 너무 비싼..게임기 ...",
          "speaker_id": "2",
          "time": "20210831 18:36"
        }
      ]
