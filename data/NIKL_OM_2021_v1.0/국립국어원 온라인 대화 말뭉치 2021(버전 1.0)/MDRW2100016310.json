        {
          "id": "MDRW2100016310.1.1",
          "form": "세계사 공부하면 좋지~너희집 아들",
          "original_form": "세계사 공부하면 좋지~너희집 아들",
          "speaker_id": "2",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100016310.1.2",
          "form": "그런거보는거보면..이과인거같다",
          "original_form": "그런거보는거보면..이과인거같다",
          "speaker_id": "1",
          "time": "20210826 11:51"
        },
        {
          "id": "MDRW2100016310.1.3",
          "form": "세계사는 문과인데",
          "original_form": "세계사는 문과인데",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100016310.1.4",
          "form": "아 그렇나 ㅋㅋ",
          "original_form": "아 그렇나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016310.1.5",
          "form": "사실적인거라서 이과인줄",
          "original_form": "사실적인거라서 이과인줄",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016310.1.6",
          "form": "응~ㅋㅋㅋㅋ",
          "original_form": "응~ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016310.1.7",
          "form": "영어 세계사면 문관데~~",
          "original_form": "영어 세계사면 문관데~~",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016310.1.8",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016310.1.9",
          "form": "과학도 좋아한다 곤충 같은거랑",
          "original_form": "과학도 좋아한다 곤충 같은거랑",
          "speaker_id": "1",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016310.1.10",
          "form": "다 잘하네~너거집 아들",
          "original_form": "다 잘하네~너거집 아들",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016310.1.11",
          "form": "그냥 수학이 힘든것뿐 수학을 제일 잘할껄",
          "original_form": "그냥 수학이 힘든것뿐 수학을 제일 잘할껄",
          "speaker_id": "2",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016310.1.12",
          "form": "어 제일잘하는건 수학인데..맨날칸다",
          "original_form": "어 제일잘하는건 수학인데..맨날칸다",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016310.1.13",
          "form": "카니 엄마가 맨날못한다고 하잖아 하데",
          "original_form": "카니 엄마가 맨날못한다고 하잖아 하데",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016310.1.14",
          "form": "어려운거하니깐 힘들겠지",
          "original_form": "어려운거하니깐 힘들겠지",
          "speaker_id": "2",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016310.1.15",
          "form": "어 그런거같다 심화가 힘들지",
          "original_form": "어 그런거같다 심화가 힘들지",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100016310.1.16",
          "form": "그래서 그런듯",
          "original_form": "그래서 그런듯",
          "speaker_id": "2",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100016310.1.17",
          "form": "앞으로 영어교육도 걱정이다\r\n과외는 내년에나 하던지 할라고",
          "original_form": "앞으로 영어교육도 걱정이다\r\n과외는 내년에나 하던지 할라고",
          "speaker_id": "1",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100016310.1.18",
          "form": "다 걱정이지 자식 키우는건",
          "original_form": "다 걱정이지 자식 키우는건",
