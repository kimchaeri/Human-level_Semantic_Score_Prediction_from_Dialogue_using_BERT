      "utterance": [
        {
          "id": "MDRW2100009088.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210822 21:05"
        },
        {
          "id": "MDRW2100009088.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210822 21:05"
        },
        {
          "id": "MDRW2100009088.1.3",
          "form": "저는 취준입니당ㅠ",
          "original_form": "저는 취준입니당ㅠ",
          "speaker_id": "1",
          "time": "20210822 21:08"
        },
        {
          "id": "MDRW2100009088.1.4",
          "form": "알바 해보신적 있으신가요?",
          "original_form": "알바 해보신적 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 21:08"
        },
        {
          "id": "MDRW2100009088.1.5",
          "form": "네 알바 해봤죠ㅎㅎ",
          "original_form": "네 알바 해봤죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 21:09"
        },
        {
          "id": "MDRW2100009088.1.6",
          "form": "저는 카페알바해봤습니다!",
          "original_form": "저는 카페알바해봤습니다!",
          "speaker_id": "1",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009088.1.7",
          "form": "레나님은 어떤거 해보셨나요?",
          "original_form": "레나님은 어떤거 해보셨나요?",
          "speaker_id": "1",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009088.1.8",
          "form": "저도 카페알바랑 시청알바 공공기관 인턴했었어요ㅎㅎ",
          "original_form": "저도 카페알바랑 시청알바 공공기관 인턴했었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009088.1.9",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210822 21:11"
        },
        {
          "id": "MDRW2100009088.1.10",
          "form": "공공기관 인턴은 엄청 들어가기 어렵지 않나요?",
          "original_form": "공공기관 인턴은 엄청 들어가기 어렵지 않나요?",
          "speaker_id": "1",
          "time": "20210822 21:11"
        },
        {
          "id": "MDRW2100009088.1.11",
          "form": "대단하시네요!",
          "original_form": "대단하시네요!",
          "speaker_id": "1",
          "time": "20210822 21:11"
        },
        {
          "id": "MDRW2100009088.1.12",
          "form": "음 아뇨 과가 연관되어있어서 그렇지 않았던거같아요 운이 좋았어요ㅎㅎ",
          "original_form": "음 아뇨 과가 연관되어있어서 그렇지 않았던거같아요 운이 좋았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 21:12"
        },
        {
          "id": "MDRW2100009088.1.13",
          "form": "그래도 너무 멋져요!",
          "original_form": "그래도 너무 멋져요!",
          "speaker_id": "1",
          "time": "20210822 21:12"
        },
        {
          "id": "MDRW2100009088.1.14",
          "form": "ㅎㅎ취업이면 더 좋았겠지만요",
          "original_form": "ㅎㅎ취업이면 더 좋았겠지만요",
          "speaker_id": "2",
          "time": "20210822 21:13"
        },
        {
          "id": "MDRW2100009088.1.15",
          "form": "아ㅠㅠ",
          "original_form": "아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:14"
        },
        {
          "id": "MDRW2100009088.1.16",
          "form": "그래도 인턴 경력이 있으면 좀 더 잘되지 않을까요?",
          "original_form": "그래도 인턴 경력이 있으면 좀 더 잘되지 않을까요?",
          "speaker_id": "1",
          "time": "20210822 21:14"
        },
        {
          "id": "MDRW2100009088.1.17",
          "form": "인턴은 근데 크게 도움 안되던데ㅎㅎ 그렇지않나요?",
          "original_form": "인턴은 근데 크게 도움 안되던데ㅎㅎ 그렇지않나요?",
          "speaker_id": "2",
          "time": "20210822 21:15"
        },
        {
          "id": "MDRW2100009088.1.18",
          "form": "제가 공공기관 뭐 알아볼때 인턴했으면 가산점붙는다고 들었던거 같은데 확실하지가 않아서요ㅠ",
          "original_form": "제가 공공기관 뭐 알아볼때 인턴했으면 가산점붙는다고 들었던거 같은데 확실하지가 않아서요ㅠ",
          "speaker_id": "1",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009088.1.19",
          "form": "그런기관도 있군요..",
          "original_form": "그런기관도 있군요..",
          "speaker_id": "2",
          "time": "20210822 21:17"
        }
      ]
