      "utterance": [
        {
          "id": "MDRW2100022923.1.1",
          "form": "식사하셨나용?",
          "original_form": "식사하셨나용?",
          "speaker_id": "2",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100022923.1.2",
          "form": "방금 과자 꺼냇어요.. ㅋㅋㅋ",
          "original_form": "방금 과자 꺼냇어요.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022923.1.3",
          "form": "뭐드시나요?",
          "original_form": "뭐드시나요?",
          "speaker_id": "1",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022923.1.4",
          "form": "저슨 케이크요ㅋㅋㅋ",
          "original_form": "저슨 케이크요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100022923.1.5",
          "form": "아.. 케익은 자기전에 먹는게 꿀맛이져..",
          "original_form": "아.. 케익은 자기전에 먹는게 꿀맛이져..",
          "speaker_id": "1",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100022923.1.6",
          "form": "무슨맛이에여?",
          "original_form": "무슨맛이에여?",
          "speaker_id": "1",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100022923.1.7",
          "form": "그쳐",
          "original_form": "그쳐",
          "speaker_id": "2",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022923.1.8",
          "form": "치즈랑 고구마용ㅋㅋㅋ",
          "original_form": "치즈랑 고구마용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022923.1.9",
          "form": "어떤 케익좋아하세요?",
          "original_form": "어떤 케익좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022923.1.10",
          "form": "질문이 너무 무례하시네요...",
          "original_form": "질문이 너무 무례하시네요...",
          "speaker_id": "1",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022923.1.11",
          "form": "케익님을 고를수가 있어요?",
          "original_form": "케익님을 고를수가 있어요?",
          "speaker_id": "1",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022923.1.12",
          "form": "모든게 다 사랑이고 기쁨이고 은총입니다.. ㅋㅋㅋ",
          "original_form": "모든게 다 사랑이고 기쁨이고 은총입니다.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100022923.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ혹시나 했던거였는데..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ혹시나 했던거였는데..",
          "speaker_id": "2",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022923.1.14",
          "form": "건방져버렸네요ㅋㅋㅋ",
          "original_form": "건방져버렸네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022923.1.15",
          "form": "아 또 먹고싶슴다",
          "original_form": "아 또 먹고싶슴다",
          "speaker_id": "2",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022923.1.16",
          "form": "설마 다 드셧어요? 조각느님이셧나여?",
          "original_form": "설마 다 드셧어요? 조각느님이셧나여?",
          "speaker_id": "1",
          "time": "20210829 19:25"
        },
        {
          "id": "MDRW2100022923.1.17",
          "form": "조각이였어요ㅜㅠ",
          "original_form": "조각이였어요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022923.1.18",
          "form": "아.. 가난이 우릴 이렇게 비참하게 만드네요.. \r\n케익은 본디 둥근것을 케익이라 하는것인데..\r\n조각으로 만족해야하다니..",
          "original_form": "아.. 가난이 우릴 이렇게 비참하게 만드네요.. \r\n케익은 본디 둥근것을 케익이라 하는것인데..\r\n조각으로 만족해야하다니..",
          "speaker_id": "1",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100022923.1.19",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100022923.1.20",
          "form": "케익이랑 뭐 마셧어여? 콜라?",
          "original_form": "케익이랑 뭐 마셧어여? 콜라?",
          "speaker_id": "1",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022923.1.21",
          "form": "우유요!",
          "original_form": "우유요!",
          "speaker_id": "2",
          "time": "20210829 19:51"
        },
        {
          "id": "MDRW2100022923.1.22",
          "form": "우유밖에 없더라구요ㅜ",
          "original_form": "우유밖에 없더라구요ㅜ",
          "speaker_id": "2",
          "time": "20210829 19:51"
        }
      ]
