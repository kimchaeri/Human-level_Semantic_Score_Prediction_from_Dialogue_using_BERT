      "utterance": [
        {
          "id": "MDRW2100038818.1.1",
          "form": "마자요 연출이 기발하고 흡입력있고..",
          "original_form": "마자요 연출이 기발하고 흡입력있고..",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100038818.1.2",
          "form": "얘기하니까 또 보고싶네용 ㅎㅎ 넷플릭스에 있어서 자주 틀어봐요",
          "original_form": "얘기하니까 또 보고싶네용 ㅎㅎ 넷플릭스에 있어서 자주 틀어봐요",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100038818.1.3",
          "form": "헐 넷플릭스에 잇어요!?!?!?!??!?!?!??!?!?",
          "original_form": "헐 넷플릭스에 잇어요!?!?!?!??!?!?!??!?!?",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100038818.1.4",
          "form": "전 왓챠밖에 없는 줄 알았거든요",
          "original_form": "전 왓챠밖에 없는 줄 알았거든요",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100038818.1.5",
          "form": "있어요!!",
          "original_form": "있어요!!",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100038818.1.6",
          "form": "제가 요즘은 넷플만 쓰거든요 ㅎㅎㅎ",
          "original_form": "제가 요즘은 넷플만 쓰거든요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100038818.1.7",
          "form": "왓챠랑 둘 다 있는 콘텐츠도 은근 많더라구요",
          "original_form": "왓챠랑 둘 다 있는 콘텐츠도 은근 많더라구요",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100038818.1.8",
          "form": "헛!!!! 그렇구나!! 전 번갈아가며 써요!",
          "original_form": "헛!!!! 그렇구나!! 전 번갈아가며 써요!",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100038818.1.9",
          "form": "둘 다 쓰다가 통장에 빵구날 뻔한 적이 잏아서 ㅎㅎ",
          "original_form": "둘 다 쓰다가 통장에 빵구날 뻔한 적이 잏아서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100038818.1.10",
          "form": "저도 가끔 왓챠 이벤트할땐 보기도하고 그래요 ㅎㅎㅎㅎ",
          "original_form": "저도 가끔 왓챠 이벤트할땐 보기도하고 그래요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100038818.1.11",
          "form": "왓챠 드라마 넘 재밌어서...",
          "original_form": "왓챠 드라마 넘 재밌어서...",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100038818.1.12",
          "form": "아예안볼순없겜ㅅ더라구요",
          "original_form": "아예안볼순없겜ㅅ더라구요",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100038818.1.13",
          "form": "체르노빌, 이어즈앤이어즈, 더리틀드러머걸",
          "original_form": "체르노빌, 이어즈앤이어즈, 더리틀드러머걸",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100038818.1.14",
          "form": "그냥 드러머걸인가..ㅎㅎ 암튼",
          "original_form": "그냥 드러머걸인가..ㅎㅎ 암튼",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100038818.1.15",
          "form": "헐 다리틀드리미걸 그 박찬욱 감독 드라마 맞나요??!",
          "original_form": "헐 다리틀드리미걸 그 박찬욱 감독 드라마 맞나요??!",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100038818.1.16",
          "form": "마자요!!!",
          "original_form": "마자요!!!",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100038818.1.17",
          "form": "연출 중요하게 보시는 분이라면",
          "original_form": "연출 중요하게 보시는 분이라면",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100038818.1.18",
          "form": "꼭 보시길...ㅠ추천",
          "original_form": "꼭 보시길...ㅠ추천",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100038818.1.19",
          "form": "넘 좋아요",
          "original_form": "넘 좋아요",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100038818.1.20",
          "form": "박찬욱 특유의.. 이성간의 긴장감",
          "original_form": "박찬욱 특유의.. 이성간의 긴장감",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100038818.1.21",
          "form": "헐!!! 안 봤는데 기대되네요!!!!!!",
          "original_form": "헐!!! 안 봤는데 기대되네요!!!!!!",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100038818.1.22",
          "form": "작품이 좀 긴가요?",
          "original_form": "작품이 좀 긴가요?",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100038818.1.23",
          "form": "표현너무 잘하고.. 색감이나 의상에도 신경 많이 썼더라고요",
          "original_form": "표현너무 잘하고.. 색감이나 의상에도 신경 많이 썼더라고요",
          "speaker_id": "2",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100038818.1.24",
          "form": "스토리나 연기는 말할것도없고ㅠㅠㅠ",
          "original_form": "스토리나 연기는 말할것도없고ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100038818.1.25",
          "form": "시즌은 하나였던거강다요",
          "original_form": "시즌은 하나였던거강다요",
          "speaker_id": "2",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100038818.1.26",
          "form": "5회? 정도 짧았던걸로 기억해요",
          "original_form": "5회? 정도 짧았던걸로 기억해요",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100038818.1.27",
          "form": "허얼 완전 짧네요?? 내일 백신 맞고 할 것도 없는데",
          "original_form": "허얼 완전 짧네요?? 내일 백신 맞고 할 것도 없는데",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
