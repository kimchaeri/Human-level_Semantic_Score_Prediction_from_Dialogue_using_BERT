      "utterance": [
        {
          "id": "MDRW2100038677.1.1",
          "form": "네네 일본 베트남이용",
          "original_form": "네네 일본 베트남이용",
          "speaker_id": "2",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100038677.1.2",
          "form": "어디 가보셨너요",
          "original_form": "어디 가보셨너요",
          "speaker_id": "2",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100038677.1.3",
          "form": "전 아직 한번도 못 가봤어요ㅠㅠ",
          "original_form": "전 아직 한번도 못 가봤어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100038677.1.4",
          "form": "국내는 어디 가뷰셨나용",
          "original_form": "국내는 어디 가뷰셨나용",
          "speaker_id": "2",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100038677.1.5",
          "form": "어릴 때 부산이랑 춘천 정도 가봤어요",
          "original_form": "어릴 때 부산이랑 춘천 정도 가봤어요",
          "speaker_id": "1",
          "time": "20210902 19:57"
        },
        {
          "id": "MDRW2100038677.1.6",
          "form": "여행 다니는 것보다는 놀이공원 같은 데를 더 많이 다녔어요ㅋㅋㅠㅠ 이제 시간은 많은데 코로나 때문에 어딜 못 가네요",
          "original_form": "여행 다니는 것보다는 놀이공원 같은 데를 더 많이 다녔어요ㅋㅋㅠㅠ 이제 시간은 많은데 코로나 때문에 어딜 못 가네요",
          "speaker_id": "1",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100038677.1.7",
          "form": "오 저도 춘천가봤어요",
          "original_form": "오 저도 춘천가봤어요",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100038677.1.8",
          "form": "참 좋은 시절인데 코로나라니ㅜㅠ",
          "original_form": "참 좋은 시절인데 코로나라니ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100038677.1.9",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100038677.1.10",
          "form": "일본에서 맛있는 거 많이 드셨나요?",
          "original_form": "일본에서 맛있는 거 많이 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100038677.1.11",
          "form": "네네 우동맛집 모츠나베",
          "original_form": "네네 우동맛집 모츠나베",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100038677.1.12",
          "form": "후쿠오카 가고싶어요 또ㅠ",
          "original_form": "후쿠오카 가고싶어요 또ㅠ",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100038677.1.13",
          "form": "맛있는 거 너무 많죠ㅠㅠ 일본으로 식도락 여행 가는 게 꿈이에요ㅋㅋㅋ",
          "original_form": "맛있는 거 너무 많죠ㅠㅠ 일본으로 식도락 여행 가는 게 꿈이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100038677.1.14",
          "form": "맞아요ㅠ 료칸도좋았고",
          "original_form": "맞아요ㅠ 료칸도좋았고",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100038677.1.15",
          "form": "온천에 부모님 모시고가고팠는데",
          "original_form": "온천에 부모님 모시고가고팠는데",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100038677.1.16",
          "form": "코로나 때문에 너무 아쉽네요ㅠㅠ",
          "original_form": "코로나 때문에 너무 아쉽네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100038677.1.17",
          "form": "언제 끝날지도 모르겠고ㅠㅠ",
          "original_form": "언제 끝날지도 모르겠고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100038677.1.18",
          "form": "그니까요 지긋지긋",
          "original_form": "그니까요 지긋지긋",
          "speaker_id": "2",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100038677.1.19",
          "form": "내년에는 갈 수 있을지...",
          "original_form": "내년에는 갈 수 있을지...",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100038677.1.20",
          "form": "제발그랬음좋겠어요",
          "original_form": "제발그랬음좋겠어요",
          "speaker_id": "2",
          "time": "20210902 20:45"
        }
      ]
