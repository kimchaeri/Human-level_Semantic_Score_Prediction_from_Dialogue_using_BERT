      "utterance": [
        {
          "id": "MDRW2100042311.1.1",
          "form": "아뇨 삼남매요ㅎㅎ",
          "original_form": "아뇨 삼남매요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042311.1.2",
          "form": "부모님이랑",
          "original_form": "부모님이랑",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042311.1.3",
          "form": "그럼 5가족이 같이 살고 있나보네요",
          "original_form": "그럼 5가족이 같이 살고 있나보네요",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042311.1.4",
          "form": "자녀분이 2분인가요?",
          "original_form": "자녀분이 2분인가요?",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042311.1.5",
          "form": "저희는 3가족이에요 저희 부부랑 큰딸",
          "original_form": "저희는 3가족이에요 저희 부부랑 큰딸",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042311.1.6",
          "form": "작은 딸은 결혼해서 분가했고",
          "original_form": "작은 딸은 결혼해서 분가했고",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042311.1.7",
          "form": "아 있다 없음 쓸쓸하시죠?",
          "original_form": "아 있다 없음 쓸쓸하시죠?",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042311.1.8",
          "form": "매주 와요",
          "original_form": "매주 와요",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042311.1.9",
          "form": "육아 힘들다고 친정 찬스 쓰네요",
          "original_form": "육아 힘들다고 친정 찬스 쓰네요",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042311.1.10",
          "form": "주말마다 와서 매주 보니까 좋네요 ㅎㅎ",
          "original_form": "주말마다 와서 매주 보니까 좋네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042311.1.11",
          "form": "아ㅋㅋ좋으시겠다",
          "original_form": "아ㅋㅋ좋으시겠다",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042311.1.12",
          "form": "엄마가 제일 부러워하십니다",
          "original_form": "엄마가 제일 부러워하십니다",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042311.1.13",
          "form": "뭐 부모는 그렇긴 한데",
          "original_form": "뭐 부모는 그렇긴 한데",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042311.1.14",
          "form": "그렇다고 자녀한테 강요는 할수 없으니까요",
          "original_form": "그렇다고 자녀한테 강요는 할수 없으니까요",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042311.1.15",
          "form": "매번 납득하시고 또 속상해하시고 반복하세요",
          "original_form": "매번 납득하시고 또 속상해하시고 반복하세요",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042311.1.16",
          "form": "남동생은 가능성 있으니 다섯정도 조카낳아줬음 좋겠어요",
          "original_form": "남동생은 가능성 있으니 다섯정도 조카낳아줬음 좋겠어요",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042311.1.17",
          "form": "아이들을 좋아하나봐요",
          "original_form": "아이들을 좋아하나봐요",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042311.1.18",
          "form": "조카 생기면 잘 돌봐줄 수 있을 거 같나요?^^",
          "original_form": "조카 생기면 잘 돌봐줄 수 있을 거 같나요?^^",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042311.1.19",
          "form": "네! 용돈도!",
          "original_form": "네! 용돈도!",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042311.1.20",
          "form": "저는 좋아하는데 애들이 별로 안좋아하더라고요ㅎㅎ",
          "original_form": "저는 좋아하는데 애들이 별로 안좋아하더라고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042311.1.21",
          "form": "그렇군요..ㅎㅎ 느낌으로는 애들 잘 놀아줄거 같은데",
          "original_form": "그렇군요..ㅎㅎ 느낌으로는 애들 잘 놀아줄거 같은데",
          "speaker_id": "1",
          "time": "20210906 21:48"
        }
