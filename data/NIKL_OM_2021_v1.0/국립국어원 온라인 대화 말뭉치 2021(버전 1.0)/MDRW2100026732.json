      "utterance": [
        {
          "id": "MDRW2100026732.1.1",
          "form": "아 저요?ㅋㅋ",
          "original_form": "아 저요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026732.1.2",
          "form": "affiliation에서 일합니당ㅋㅋ",
          "original_form": "&affiliation&에서 일합니당ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026732.1.3",
          "form": "오오 그래서 야근이 그랬군요!",
          "original_form": "오오 그래서 야근이 그랬군요!",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026732.1.4",
          "form": "네 ㅋㅋ 교대근무라서요 ㅎㅎ",
          "original_form": "네 ㅋㅋ 교대근무라서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026732.1.5",
          "form": "주야비휴 사람들 잘 몰라요",
          "original_form": "주야비휴 사람들 잘 몰라요",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026732.1.6",
          "form": "어쩐지 어디 공무원이신가 했는데",
          "original_form": "어쩐지 어디 공무원이신가 했는데",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026732.1.7",
          "form": "생각하니 딱 거기네요!",
          "original_form": "생각하니 딱 거기네요!",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026732.1.8",
          "form": "오늘 하루 푹 쉬셨나요",
          "original_form": "오늘 하루 푹 쉬셨나요",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026732.1.9",
          "form": "아니요 ㅎㅎ",
          "original_form": "아니요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026732.1.10",
          "form": "오늘 라이언키링 사러",
          "original_form": "오늘 라이언키링 사러",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026732.1.11",
          "form": "나갔다와서 ㅠ",
          "original_form": "나갔다와서 ㅠ",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026732.1.12",
          "form": "아ㅜ 주야비휴면 찐 야근이라 낮밤 바뀌어서 피곤할텐데",
          "original_form": "아ㅜ 주야비휴면 찐 야근이라 낮밤 바뀌어서 피곤할텐데",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026732.1.13",
          "form": "라이언키링은 또 왜요ㅎㅎ",
          "original_form": "라이언키링은 또 왜요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026732.1.14",
          "form": "카카오 라이언 키링 나왔대서",
          "original_form": "카카오 라이언 키링 나왔대서",
          "speaker_id": "2",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026732.1.15",
          "form": "줍줍해두려 오늘 역 돌아댕겼어요ㅋㅋ",
          "original_form": "줍줍해두려 오늘 역 돌아댕겼어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026732.1.16",
          "form": "아 누구 심부름이 아니고 그냥 취향이시군요ㅋㅋㅋ",
          "original_form": "아 누구 심부름이 아니고 그냥 취향이시군요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026732.1.17",
          "form": "아니 근데 라이언키링이 언제 나온거죠!!!",
          "original_form": "아니 근데 라이언키링이 언제 나온거죠!!!",
          "speaker_id": "1",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026732.1.18",
          "form": "네이버에 라이언역장님",
          "original_form": "네이버에 라이언역장님",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026732.1.19",
          "form": "검색해보세요ㅋㅋㅋ",
          "original_form": "검색해보세요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026732.1.20",
          "form": "존예ㅜㅜㅜㅜㅜ",
          "original_form": "존예ㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026732.1.21",
          "form": "뭐죶이 귀여운 생명체는ㅜㅜㅜ",
          "original_form": "뭐죶이 귀여운 생명체는ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026732.1.22",
          "form": "쉬는날 도는 게 이해가 갑니다ㅜ",
          "original_form": "쉬는날 도는 게 이해가 갑니다ㅜ",
          "speaker_id": "1",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026732.1.23",
          "form": "곧 품절 각 봅니다ㅋㅋㅋ",
          "original_form": "곧 품절 각 봅니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100026732.1.24",
          "form": "ㅜㅇㅜ 안돼!",
          "original_form": "ㅜㅇㅜ 안돼!",
          "speaker_id": "1",
          "time": "20210902 00:58"
        },
        {
          "id": "MDRW2100026732.1.25",
          "form": "제가 쟁여뒀습죠",
          "original_form": "제가 쟁여뒀습죠",
          "speaker_id": "2",
          "time": "20210902 00:58"
        },
        {
          "id": "MDRW2100026732.1.26",
          "form": "오늘은 백신만 맞고 쉬려했는데 안되겠네용",
          "original_form": "오늘은 백신만 맞고 쉬려했는데 안되겠네용",
