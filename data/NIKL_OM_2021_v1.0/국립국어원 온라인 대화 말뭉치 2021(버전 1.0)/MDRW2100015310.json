      "utterance": [
        {
          "id": "MDRW2100015310.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015310.1.2",
          "form": "가끔씩 해요ㅋㅋㅋ",
          "original_form": "가끔씩 해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015310.1.3",
          "form": "어떤거 좋아하세요?",
          "original_form": "어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015310.1.4",
          "form": "롤 했었어요",
          "original_form": "롤 했었어요",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015310.1.5",
          "form": "롤 배그?",
          "original_form": "롤 배그?",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015310.1.6",
          "form": "저는 롤은 안하는데 배그는 좀 많이 해요!",
          "original_form": "저는 롤은 안하는데 배그는 좀 많이 해요!",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015310.1.7",
          "form": "오... 배그 평균 몇길이신가요",
          "original_form": "오... 배그 평균 몇길이신가요",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015310.1.8",
          "form": "몇킬",
          "original_form": "몇킬",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015310.1.9",
          "form": "제기 겜을 좀 못해가지구...",
          "original_form": "제기 겜을 좀 못해가지구...",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015310.1.10",
          "form": "한판에 편균 1~3킬정도 하는거같아요ㅋㅋㅋ",
          "original_form": "한판에 편균 1~3킬정도 하는거같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015310.1.11",
          "form": "저는 총게임 진짜 자신있었는데",
          "original_form": "저는 총게임 진짜 자신있었는데",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015310.1.12",
          "form": "배그는 매일 떨어지면 죽어요ㅋㅋㅋㅋ",
          "original_form": "배그는 매일 떨어지면 죽어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015310.1.13",
          "form": "ㅋㅋㅋㅋㅋ배그가 은근 어렵죠",
          "original_form": "ㅋㅋㅋㅋㅋ배그가 은근 어렵죠",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015310.1.14",
          "form": "총만 잘썬다고 되는게 아니더라구요!",
          "original_form": "총만 잘썬다고 되는게 아니더라구요!",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015310.1.15",
          "form": "지형지물도 써야되고",
          "original_form": "지형지물도 써야되고",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015310.1.16",
          "form": "자기장 운도 좋아야되고",
          "original_form": "자기장 운도 좋아야되고",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015310.1.17",
          "form": "그러게요! 혼자서는 진짜 힘들더라구요",
          "original_form": "그러게요! 혼자서는 진짜 힘들더라구요",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015310.1.18",
          "form": "친구들이랑 같이하면 그래도 할만해서 좋아요ㅋㅋㅋ",
          "original_form": "친구들이랑 같이하면 그래도 할만해서 좋아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015310.1.19",
          "form": "요즘도 친구들이랑 종종 하시나요?",
          "original_form": "요즘도 친구들이랑 종종 하시나요?",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015310.1.20",
          "form": "네 요즘에도 종종해요!",
          "original_form": "네 요즘에도 종종해요!",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015310.1.21",
          "form": "제가 롤을 안하다보나니까 모이면 거의 배그를 해요ㅋㅋㅋ",
          "original_form": "제가 롤을 안하다보나니까 모이면 거의 배그를 해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015310.1.22",
          "form": "저는 요즘 같이 할 친구가 없어서 못하고있어요...",
          "original_form": "저는 요즘 같이 할 친구가 없어서 못하고있어요...",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015310.1.23",
          "form": "아 그러시구나 웃프네여ㅋ큐ㅠㅠㅠ",
          "original_form": "아 그러시구나 웃프네여ㅋ큐ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015310.1.24",
          "form": "배그 솔로는 너무 무서워요...!",
          "original_form": "배그 솔로는 너무 무서워요...!",
          "speaker_id": "1",
          "time": "20210825 22:52"
        }
      ]
    }
  ]
