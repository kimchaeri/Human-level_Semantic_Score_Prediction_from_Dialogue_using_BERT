      "utterance": [
        {
          "id": "MDRW2100029762.1.1",
          "form": "답장이 빨라서 좋네요",
          "original_form": "답장이 빨라서 좋네요",
          "speaker_id": "1",
          "time": "20210903 13:12"
        },
        {
          "id": "MDRW2100029762.1.2",
          "form": "넹 제가 좀 빨라요",
          "original_form": "넹 제가 좀 빨라요",
          "speaker_id": "2",
          "time": "20210903 13:12"
        },
        {
          "id": "MDRW2100029762.1.3",
          "form": "애 있으세요?",
          "original_form": "애 있으세요?",
          "speaker_id": "1",
          "time": "20210903 13:12"
        },
        {
          "id": "MDRW2100029762.1.4",
          "form": "무슨 소리시죠?? 기분이 나쁘네요 미혼인데",
          "original_form": "무슨 소리시죠?? 기분이 나쁘네요 미혼인데",
          "speaker_id": "2",
          "time": "20210903 13:13"
        },
        {
          "id": "MDRW2100029762.1.5",
          "form": "미혼도 애 있을 수 있죠;",
          "original_form": "미혼도 애 있을 수 있죠;",
          "speaker_id": "1",
          "time": "20210903 13:13"
        },
        {
          "id": "MDRW2100029762.1.6",
          "form": "닫힌 사고를 하시네요",
          "original_form": "닫힌 사고를 하시네요",
          "speaker_id": "1",
          "time": "20210903 13:13"
        },
        {
          "id": "MDRW2100029762.1.7",
          "form": "아 미안합니다 제가 좀 보수적이라서",
          "original_form": "아 미안합니다 제가 좀 보수적이라서",
          "speaker_id": "2",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100029762.1.8",
          "form": "애 있으세요?",
          "original_form": "애 있으세요?",
          "speaker_id": "2",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100029762.1.9",
          "form": "미팅해보셨나요?",
          "original_form": "미팅해보셨나요?",
          "speaker_id": "1",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100029762.1.10",
          "form": "애는 없습니다",
          "original_form": "애는 없습니다",
          "speaker_id": "1",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100029762.1.11",
          "form": "안해봤어요",
          "original_form": "안해봤어요",
          "speaker_id": "2",
          "time": "20210903 13:17"
        },
        {
          "id": "MDRW2100029762.1.12",
          "form": "자만추라서요",
          "original_form": "자만추라서요",
          "speaker_id": "2",
          "time": "20210903 13:17"
        },
        {
          "id": "MDRW2100029762.1.13",
          "form": "오늘 남친 만나세요?",
          "original_form": "오늘 남친 만나세요?",
          "speaker_id": "1",
          "time": "20210903 13:27"
        },
        {
          "id": "MDRW2100029762.1.14",
          "form": "안 만나고 싶은데요",
          "original_form": "안 만나고 싶은데요",
          "speaker_id": "2",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100029762.1.15",
          "form": "제가 내일 중요한 발표가 있거든요",
          "original_form": "제가 내일 중요한 발표가 있거든요",
          "speaker_id": "2",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100029762.1.16",
          "form": "그래서 바쁘다니까",
          "original_form": "그래서 바쁘다니까",
          "speaker_id": "2",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100029762.1.17",
          "form": "그럼 헤어지세요",
          "original_form": "그럼 헤어지세요",
          "speaker_id": "1",
          "time": "20210903 13:37"
        },
        {
          "id": "MDRW2100029762.1.18",
          "form": "발표 준비를 자기 집에 와서 하라네요?",
          "original_form": "발표 준비를 자기 집에 와서 하라네요?",
          "speaker_id": "2",
          "time": "20210903 13:37"
        },
        {
          "id": "MDRW2100029762.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ문란하네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ문란하네요",
          "speaker_id": "1",
          "time": "20210903 13:37"
        },
        {
          "id": "MDRW2100029762.1.20",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210903 13:37"
        },
        {
          "id": "MDRW2100029762.1.21",
