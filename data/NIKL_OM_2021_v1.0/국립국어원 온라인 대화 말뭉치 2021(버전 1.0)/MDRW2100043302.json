      "utterance": [
        {
          "id": "MDRW2100043302.1.1",
          "form": "당근 애용하세요?",
          "original_form": "당근 애용하세요?",
          "speaker_id": "2",
          "time": "20210907 17:18"
        },
        {
          "id": "MDRW2100043302.1.2",
          "form": "저 당근 안해봣어요",
          "original_form": "저 당근 안해봣어요",
          "speaker_id": "2",
          "time": "20210907 17:18"
        },
        {
          "id": "MDRW2100043302.1.3",
          "form": "아 네 제주와서는 동네생활보고",
          "original_form": "아 네 제주와서는 동네생활보고",
          "speaker_id": "1",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100043302.1.4",
          "form": "무선청소기 중고로 샀어요",
          "original_form": "무선청소기 중고로 샀어요",
          "speaker_id": "1",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100043302.1.5",
          "form": "그쵸 지금처럼 잠깐살기엔 중고가 최고져",
          "original_form": "그쵸 지금처럼 잠깐살기엔 중고가 최고져",
          "speaker_id": "2",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100043302.1.6",
          "form": "당근 시작하면 온갖거 다 팔아버릴까봐 ㅋㅋㅋ",
          "original_form": "당근 시작하면 온갖거 다 팔아버릴까봐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100043302.1.7",
          "form": "ㅎㅎ 배송료때문에 아까워서 말 살수가 없어요",
          "original_form": "ㅎㅎ 배송료때문에 아까워서 말 살수가 없어요",
          "speaker_id": "1",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100043302.1.8",
          "form": "아 맞네요 ㅋㅋㅋ 제주산간지역",
          "original_form": "아 맞네요 ㅋㅋㅋ 제주산간지역",
          "speaker_id": "2",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100043302.1.9",
          "form": "배송비 ㅋㅋㅋㅋ",
          "original_form": "배송비 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100043302.1.10",
          "form": "얼마정도예요?",
          "original_form": "얼마정도예요?",
          "speaker_id": "2",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100043302.1.11",
          "form": "1인용 쇼파 조명 가고싶은데 ㅠ",
          "original_form": "1인용 쇼파 조명 가고싶은데 ㅠ",
          "speaker_id": "1",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100043302.1.12",
          "form": "가지구 싶은데",
          "original_form": "가지구 싶은데",
          "speaker_id": "1",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100043302.1.13",
          "form": "보통 3000원 더 붙어요",
          "original_form": "보통 3000원 더 붙어요",
          "speaker_id": "1",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100043302.1.14",
          "form": "홀 그럼 오육천원이군요",
          "original_form": "홀 그럼 오육천원이군요",
          "speaker_id": "2",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100043302.1.15",
          "form": "아 그런 단점이..",
          "original_form": "아 그런 단점이..",
          "speaker_id": "2",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100043302.1.16",
          "form": "가구는 몇만원도 더 붙어요",
          "original_form": "가구는 몇만원도 더 붙어요",
          "speaker_id": "1",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100043302.1.17",
          "form": "오늘의집을 그만봐야된다눈..",
          "original_form": "오늘의집을 그만봐야된다눈..",
          "speaker_id": "1",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100043302.1.18",
          "form": "몇만원...털썩",
          "original_form": "몇만원...털썩",
          "speaker_id": "2",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043302.1.19",
          "form": "ㅋㅋㅋㅋ먖아요 그거보면 진짜 끝도없어진다던뎈ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ먖아요 그거보면 진짜 끝도없어진다던뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043302.1.20",
          "form": "오늘의지름",
          "original_form": "오늘의지름",
          "speaker_id": "1",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043302.1.21",
          "form": "근데 어쩜 잘꾸미고 사는 분들이 많는지",
          "original_form": "근데 어쩜 잘꾸미고 사는 분들이 많는지",
          "speaker_id": "2",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043302.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ집x 지름ㅇ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ집x 지름ㅇ",
          "speaker_id": "2",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043302.1.23",
          "form": "지금은 좌식인데 허리가 점점 아파서",
          "original_form": "지금은 좌식인데 허리가 점점 아파서",
          "speaker_id": "1",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043302.1.24",
          "form": "입식으로 가려구요",
          "original_form": "입식으로 가려구요",
          "speaker_id": "1",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043302.1.25",
          "form": "맞아요 한국인이라 좌식편하긴한데",
          "original_form": "맞아요 한국인이라 좌식편하긴한데",
          "speaker_id": "2",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043302.1.26",
          "form": "오래앉아있기가 너무 불편해요",
          "original_form": "오래앉아있기가 너무 불편해요",
          "speaker_id": "2",
          "time": "20210907 17:58"
        }
      ]
