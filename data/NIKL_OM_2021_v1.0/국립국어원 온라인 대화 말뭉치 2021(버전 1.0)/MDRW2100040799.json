      "utterance": [
        {
          "id": "MDRW2100040799.1.1",
          "form": "날씨 화창하고 좋네요",
          "original_form": "날씨 화창하고 좋네요",
          "speaker_id": "1",
          "time": "20210905 07:41"
        },
        {
          "id": "MDRW2100040799.1.2",
          "form": "그쵸 가을이에요~",
          "original_form": "그쵸 가을이에요~",
          "speaker_id": "2",
          "time": "20210905 07:41"
        },
        {
          "id": "MDRW2100040799.1.3",
          "form": "가을이라 아침저녁으로는 쌀쌀하네요",
          "original_form": "가을이라 아침저녁으로는 쌀쌀하네요",
          "speaker_id": "1",
          "time": "20210905 07:41"
        },
        {
          "id": "MDRW2100040799.1.4",
          "form": "맞아요~ 찬바람기온차도 요새 비염땜에 괴롭네요ㅠ",
          "original_form": "맞아요~ 찬바람기온차도 요새 비염땜에 괴롭네요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:42"
        },
        {
          "id": "MDRW2100040799.1.5",
          "form": "약드세요?",
          "original_form": "약드세요?",
          "speaker_id": "1",
          "time": "20210905 07:42"
        },
        {
          "id": "MDRW2100040799.1.6",
          "form": "비염약?",
          "original_form": "비염약?",
          "speaker_id": "1",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100040799.1.7",
          "form": "네ㅠ 약국가서 사왓네요ㅠ",
          "original_form": "네ㅠ 약국가서 사왓네요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100040799.1.8",
          "form": "저희아들도 비염땜에 계속킁킁거려 병원을가야되나 그러는중이예요",
          "original_form": "저희아들도 비염땜에 계속킁킁거려 병원을가야되나 그러는중이예요",
          "speaker_id": "1",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100040799.1.9",
          "form": "보통 먹으면 바로 효과가잇는데 이번엔 약이 영 잘안듣네요ㅠ",
          "original_form": "보통 먹으면 바로 효과가잇는데 이번엔 약이 영 잘안듣네요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100040799.1.10",
          "form": "약이라도사와야겠네요",
          "original_form": "약이라도사와야겠네요",
          "speaker_id": "1",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100040799.1.11",
          "form": "코가막히면 머리도 아프니ㅜ 어리면 병원을 가보세요ㅠ",
          "original_form": "코가막히면 머리도 아프니ㅜ 어리면 병원을 가보세요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100040799.1.12",
          "form": "전 중학교때 비염시작했는데",
          "original_form": "전 중학교때 비염시작했는데",
          "speaker_id": "2",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100040799.1.13",
          "form": "그래야겠네요..",
          "original_form": "그래야겠네요..",
          "speaker_id": "1",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100040799.1.14",
          "form": "요 몇년은 괜찮은거 같았다는데 올해는 좀 힘드네여ㅠㅠ",
          "original_form": "요 몇년은 괜찮은거 같았다는데 올해는 좀 힘드네여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100040799.1.15",
          "form": "저희아들도 중학생요",
          "original_form": "저희아들도 중학생요",
          "speaker_id": "1",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100040799.1.16",
          "form": "그렇군요ㅠ 그땐 집중력도 중요한시기니 잘 잡아야할듯해요",
          "original_form": "그렇군요ㅠ 그땐 집중력도 중요한시기니 잘 잡아야할듯해요",
          "speaker_id": "2",
          "time": "20210905 07:46"
        },
        {
          "id": "MDRW2100040799.1.17",
          "form": "병원가야겠군요..날씨가 가을오면 비염이 생겨요",
          "original_form": "병원가야겠군요..날씨가 가을오면 비염이 생겨요",
          "speaker_id": "1",
          "time": "20210905 07:46"
        },
        {
          "id": "MDRW2100040799.1.18",
          "form": "코로숨쉬기힘들면 ㅠㅠ 진짜 괴롭죠",
          "original_form": "코로숨쉬기힘들면 ㅠㅠ 진짜 괴롭죠",
          "speaker_id": "2",
          "time": "20210905 07:46"
        }
      ]
