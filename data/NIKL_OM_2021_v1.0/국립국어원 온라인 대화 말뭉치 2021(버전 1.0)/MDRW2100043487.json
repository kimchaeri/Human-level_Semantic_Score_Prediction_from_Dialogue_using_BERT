      "utterance": [
        {
          "id": "MDRW2100043487.1.1",
          "form": "최근에 산 거 있냥",
          "original_form": "최근에 산 거 있냥",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043487.1.2",
          "form": "나 믹서기 샀어!",
          "original_form": "나 믹서기 샀어!",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043487.1.3",
          "form": "자취방에서 프라페 해먹으려구 ㅋㅋㅋ",
          "original_form": "자취방에서 프라페 해먹으려구 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043487.1.4",
          "form": "헐, 호화로운 식생활이다",
          "original_form": "헐, 호화로운 식생활이다",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043487.1.5",
          "form": "나는 컴퓨터 새로 장만함",
          "original_form": "나는 컴퓨터 새로 장만함",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043487.1.6",
          "form": "헉 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "헉 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043487.1.7",
          "form": "그쪽이 훨씬...",
          "original_form": "그쪽이 훨씬...",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043487.1.8",
          "form": "저는 호화랑은 거리가 멉니다용ㅋㅋ",
          "original_form": "저는 호화랑은 거리가 멉니다용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043487.1.9",
          "form": "어느 부랜드 노트뷱 샀오?",
          "original_form": "어느 부랜드 노트뷱 샀오?",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043487.1.10",
          "form": "노트북에서 데스크톱으로 갈아탔수",
          "original_form": "노트북에서 데스크톱으로 갈아탔수",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043487.1.11",
          "form": "모니터는 삼성, 본체는 HP",
          "original_form": "모니터는 삼성, 본체는 HP",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043487.1.12",
          "form": "오오ㅋㅋㅋ",
          "original_form": "오오ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043487.1.13",
          "form": "어차피 원래 작업을 거의 집에서하니까",
          "original_form": "어차피 원래 작업을 거의 집에서하니까",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043487.1.14",
          "form": "잘 샀수 ㅋㅋㅋ",
          "original_form": "잘 샀수 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043487.1.15",
