        {
          "id": "MDRW2100025061.1.1",
          "form": "영화좋아하세요?",
          "original_form": "영화좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025061.1.2",
          "form": "네 좋아해요! 어떤 장르 좋아하세요?",
          "original_form": "네 좋아해요! 어떤 장르 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025061.1.3",
          "form": "류코요",
          "original_form": "류코요",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025061.1.4",
          "form": "로코요",
          "original_form": "로코요",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025061.1.5",
          "form": "저도 로코나 뮤지컬영화 좋아해요!",
          "original_form": "저도 로코나 뮤지컬영화 좋아해요!",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025061.1.6",
          "form": "멜로가 짱이뵤2",
          "original_form": "멜로가 짱이뵤2",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025061.1.7",
          "form": "추천영화 있으세요?",
          "original_form": "추천영화 있으세요?",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025061.1.8",
          "form": "첫키스만50번째요",
          "original_form": "첫키스만50번째요",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025061.1.9",
          "form": "아 그거 기억상실이죠??",
          "original_form": "아 그거 기억상실이죠??",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100025061.1.10",
          "form": "전 어바웃타임좋아해요",
          "original_form": "전 어바웃타임좋아해요",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025061.1.11",
          "form": "네.맞아요",
          "original_form": "네.맞아요",
          "speaker_id": "1",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100025061.1.12",
          "form": "오에스티도 좋나요?",
          "original_form": "오에스티도 좋나요?",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025061.1.13",
          "form": "네 감미로운목소리진짜좋다요",
          "original_form": "네 감미로운목소리진짜좋다요",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
