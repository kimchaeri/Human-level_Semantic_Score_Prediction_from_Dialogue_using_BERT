      "utterance": [
        {
          "id": "MDRW2100005020.1.1",
          "form": "나요즘 양꼬치도 먹고싶고 마라탕도 먹고싶다",
          "original_form": "나요즘 양꼬치도 먹고싶고 마라탕도 먹고싶다",
          "speaker_id": "1",
          "time": "20210821 19:58"
        },
        {
          "id": "MDRW2100005020.1.2",
          "form": "울집근처 양꼬치집 없어졌어ㅜ 개슬퍼",
          "original_form": "울집근처 양꼬치집 없어졌어ㅜ 개슬퍼",
          "speaker_id": "2",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100005020.1.3",
          "form": "다음 수업때 선생님한테 양꼬치 맛집 여쭤볼라고",
          "original_form": "다음 수업때 선생님한테 양꼬치 맛집 여쭤볼라고",
          "speaker_id": "2",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100005020.1.4",
          "form": "같이 가보자",
          "original_form": "같이 가보자",
          "speaker_id": "2",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100005020.1.5",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005020.1.6",
          "form": "맛집까지 가긴 귀찮은데 ㅎ",
          "original_form": "맛집까지 가긴 귀찮은데 ㅎ",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005020.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005020.1.8",
          "form": "(뭐야,,)",
          "original_form": "(뭐야,,)",
          "speaker_id": "2",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005020.1.9",
          "form": "아 진짜? ㅎㅎ",
          "original_form": "아 진짜? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005020.1.10",
          "form": "근데 솔직히 맛집이라고 엄청나게 다르고 그런건 못느끼겠어",
          "original_form": "근데 솔직히 맛집이라고 엄청나게 다르고 그런건 못느끼겠어",
          "speaker_id": "2",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005020.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아",
          "speaker_id": "1",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005020.1.12",
          "form": "저번에 코엑스 인도음식점도",
          "original_form": "저번에 코엑스 인도음식점도",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005020.1.13",
          "form": "맛집이라해서 기대했는데",
          "original_form": "맛집이라해서 기대했는데",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005020.1.14",
          "form": "그냥저냥.. 먹을만~",
          "original_form": "그냥저냥.. 먹을만~",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005020.1.15",
          "form": "난.. 좋았어.. 인도커리..",
          "original_form": "난.. 좋았어.. 인도커리..",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005020.1.16",
          "form": "하지만 그 녹색 커리는.. 안맞아.. 강렬하긴 했어",
          "original_form": "하지만 그 녹색 커리는.. 안맞아.. 강렬하긴 했어",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005020.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005020.1.18",
          "form": "나중에 맛있는 커리집 가보고싶다",
          "original_form": "나중에 맛있는 커리집 가보고싶다",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005020.1.19",
          "form": "나중에 커리 맛집 찾아 본토로 떠나자",
          "original_form": "나중에 커리 맛집 찾아 본토로 떠나자",
          "speaker_id": "2",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005020.1.20",
          "form": "맛집 찾는 법 아는 포카?",
          "original_form": "맛집 찾는 법 아는 포카?",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005020.1.21",
          "form": "모릅니다",
          "original_form": "모릅니다",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005020.1.22",
          "form": "님 저녁 뭐먹었삼",
          "original_form": "님 저녁 뭐먹었삼",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005020.1.23",
          "form": "맘스터치",
          "original_form": "맘스터치",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005020.1.24",
          "form": "난 맘스땃쥐가 젤 맛있더라 근데 요즘 질려",
          "original_form": "난 맘스땃쥐가 젤 맛있더라 근데 요즘 질려",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005020.1.25",
          "form": "결별예정",
          "original_form": "결별예정",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005020.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005020.1.27",
          "form": "난 만두먹었어 왕교자",
          "original_form": "난 만두먹었어 왕교자",
          "speaker_id": "1",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005020.1.28",
          "form": "오랜만에 먹으니까 맛있더라",
          "original_form": "오랜만에 먹으니까 맛있더라",
          "speaker_id": "1",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005020.1.29",
          "form": "나 갈비만두 먹고싶음",
          "original_form": "나 갈비만두 먹고싶음",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005020.1.30",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100005020.1.31",
          "form": "갈비만두 먹어봄?",
          "original_form": "갈비만두 먹어봄?",
          "speaker_id": "1",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100005020.1.32",
          "form": "개맛있어 감칠만 오짐",
          "original_form": "개맛있어 감칠만 오짐",
          "speaker_id": "2",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005020.1.33",
          "form": "나중에 갈비만두 먹으러 가자",
          "original_form": "나중에 갈비만두 먹으러 가자",
          "speaker_id": "2",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005020.1.34",
          "form": "난 그냥 그렇던데",
          "original_form": "난 그냥 그렇던데",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005020.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005020.1.36",
          "form": "난 김치만두가 제일 좋아",
          "original_form": "난 김치만두가 제일 좋아",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005020.1.37",
          "form": "매콤한맛 최고",
          "original_form": "매콤한맛 최고",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005020.1.38",
          "form": "참나 갈비만두가 최고존엄임",
          "original_form": "참나 갈비만두가 최고존엄임",
          "speaker_id": "2",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005020.1.39",
          "form": "네네.. 그러세요..",
          "original_form": "네네.. 그러세요..",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005020.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005020.1.41",
          "form": "오늘 낮에는 볶음밥 먹었는데",
          "original_form": "오늘 낮에는 볶음밥 먹었는데",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005020.1.42",
          "form": "이번에 볶음밥 잘돼서 밥먹는데 기분 좋았사",
          "original_form": "이번에 볶음밥 잘돼서 밥먹는데 기분 좋았사",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005020.1.43",
          "form": "좋았삼",
          "original_form": "좋았삼",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005020.1.44",
          "form": "무슨 볶음밥 해먹었어?",
          "original_form": "무슨 볶음밥 해먹었어?",
          "speaker_id": "2",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005020.1.45",
          "form": "파계란볶음밥에 스팸 넣고 라면스프 쬠 넣었어",
          "original_form": "파계란볶음밥에 스팸 넣고 라면스프 쬠 넣었어",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005020.1.46",
          "form": "라면스프 넣으면 먼맛이지",
          "original_form": "라면스프 넣으면 먼맛이지",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005020.1.47",
          "form": "라면스프맛",
          "original_form": "라면스프맛",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005020.1.48",
          "form": "약간 감칠맛내기용인가",
          "original_form": "약간 감칠맛내기용인가",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005020.1.49",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005020.1.50",
          "form": "어 알랴줘서 고마워 ㅎ;",
          "original_form": "어 알랴줘서 고마워 ㅎ;",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005020.1.51",
          "form": "많이넣는건 별론데 조금넣으면 ㄱㅊ",
          "original_form": "많이넣는건 별론데 조금넣으면 ㄱㅊ",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005020.1.52",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005020.1.53",
          "form": "라면스프에서 라면스프맛 날 줄 몰랐음;ㅎ",
          "original_form": "라면스프에서 라면스프맛 날 줄 몰랐음;ㅎ",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005020.1.54",
          "form": "그치 나도 이번에 깜짝놀람",
          "original_form": "그치 나도 이번에 깜짝놀람",
          "speaker_id": "1",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005020.1.55",
          "form": "이게 무슨 대화람",
          "original_form": "이게 무슨 대화람",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005020.1.56",
          "form": "떡볶이 만들때 살짝 넣어주면 맛있어",
          "original_form": "떡볶이 만들때 살짝 넣어주면 맛있어",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005020.1.57",
          "form": "내 부족한 맛 나는 떡볶이를 완성시캬줌,,",
          "original_form": "내 부족한 맛 나는 떡볶이를 완성시캬줌,,",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005020.1.58",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005020.1.59",
          "form": "난 떡볶이는 사먹는게 맛있는듯 먼가 집에서 만들면 잘 안되더라구..",
          "original_form": "난 떡볶이는 사먹는게 맛있는듯 먼가 집에서 만들면 잘 안되더라구..",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005020.1.60",
          "form": "아니 마자가 왜 나쁜말이야",
          "original_form": "아니 마자가 왜 나쁜말이야",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005020.1.61",
          "form": "맞.아.",
          "original_form": "맞.아.",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005020.1.62",
          "form": "그렇기는 해.",
          "original_form": "그렇기는 해.",
          "speaker_id": "2",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005020.1.63",
          "form": "나 어제 마라탕 먹었다",
          "original_form": "나 어제 마라탕 먹었다",
