      "utterance": [
        {
          "id": "MDRW2100028377.1.1",
          "form": "기계 그런거 무섭지 않니",
          "original_form": "기계 그런거 무섭지 않니",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028377.1.2",
          "form": "공정하게 잘 판단되는 기계면 좋죠!! 아마 괜찮을거예요",
          "original_form": "공정하게 잘 판단되는 기계면 좋죠!! 아마 괜찮을거예요",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028377.1.3",
          "form": "아직 운전같은건 사람이 해야겠더라",
          "original_form": "아직 운전같은건 사람이 해야겠더라",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028377.1.4",
          "form": "테슬라에서 나온 시스템도 그렇고 아직 미흡하긴 해요",
          "original_form": "테슬라에서 나온 시스템도 그렇고 아직 미흡하긴 해요",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028377.1.5",
          "form": "저번에 봤는데 트럭에 실려가는 신호등을 자동차가 인식해버렸대",
          "original_form": "저번에 봤는데 트럭에 실려가는 신호등을 자동차가 인식해버렸대",
          "speaker_id": "2",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028377.1.6",
          "form": "그런건 아무래도 ㅋㅋㅋㅋ 판단하기 힘들겠어요 ㅠㅠ",
          "original_form": "그런건 아무래도 ㅋㅋㅋㅋ 판단하기 힘들겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028377.1.7",
          "form": "이렇게 보면 인간이 고차원 생각 하는거구나 싶고...",
          "original_form": "이렇게 보면 인간이 고차원 생각 하는거구나 싶고...",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028377.1.8",
          "form": "학과 많이 생기면 그래도 발전하겠지",
          "original_form": "학과 많이 생기면 그래도 발전하겠지",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028377.1.9",
          "form": "맞아요 인재들 많이 생기겠죠!!",
          "original_form": "맞아요 인재들 많이 생기겠죠!!",
          "speaker_id": "1",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028377.1.10",
          "form": "너는 그럴 생각 없니?",
          "original_form": "너는 그럴 생각 없니?",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028377.1.11",
          "form": "ㅎㅎ 저도 나름 열심히 하고 있는데...",
          "original_form": "ㅎㅎ 저도 나름 열심히 하고 있는데...",
          "speaker_id": "1",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028377.1.12",
          "form": "공부 엸ㅁ히 해서 사회에 이바지하는 인재가 되거라",
          "original_form": "공부 엸ㅁ히 해서 사회에 이바지하는 인재가 되거라",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028377.1.13",
          "form": "넵 저도 제 진로 싫어하지 않아서",
          "original_form": "넵 저도 제 진로 싫어하지 않아서",
          "speaker_id": "1",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100028377.1.14",
          "form": "열심히 할게요 ㅎㅎ..",
          "original_form": "열심히 할게요 ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100028377.1.15",
          "form": "과학 기술 발전하면 편해질거야",
          "original_form": "과학 기술 발전하면 편해질거야",
          "speaker_id": "2",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100028377.1.16",
          "form": "근데 AI는 문제도 아직 많으니까",
          "original_form": "근데 AI는 문제도 아직 많으니까",
          "speaker_id": "1",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100028377.1.17",
          "form": "그부분도 어떻게 해야할지 봐야할거같아요",
          "original_form": "그부분도 어떻게 해야할지 봐야할거같아요",
