      "utterance": [
        {
          "id": "MDRW2100016794.1.1",
          "form": "개강하믄",
          "original_form": "개강하믄",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016794.1.2",
          "form": "시간표 어케 됑",
          "original_form": "시간표 어케 됑",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016794.1.3",
          "form": "난 안해도 돼?",
          "original_form": "난 안해도 돼?",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016794.1.4",
          "form": "나 주4일등교쓰",
          "original_form": "나 주4일등교쓰",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016794.1.5",
          "form": "아침에 일찍...,,",
          "original_form": "아침에 일찍...,,",
          "speaker_id": "2",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016794.1.6",
          "form": "니네도 캡스톤과목있어?",
          "original_form": "니네도 캡스톤과목있어?",
          "speaker_id": "2",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016794.1.7",
          "form": "캡스톤수업을 따로 신청해야 들을수잇을걸",
          "original_form": "캡스톤수업을 따로 신청해야 들을수잇을걸",
          "speaker_id": "1",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016794.1.8",
          "form": "난 안햇지ㅎㅅㅎ",
          "original_form": "난 안햇지ㅎㅅㅎ",
          "speaker_id": "1",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016794.1.9",
          "form": "우린 졸업필수더라",
          "original_form": "우린 졸업필수더라",
          "speaker_id": "2",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016794.1.10",
          "form": "자격증 다 때려쳐",
          "original_form": "자격증 다 때려쳐",
          "speaker_id": "2",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016794.1.11",
          "form": "Aㅏ...",
          "original_form": "Aㅏ...",
          "speaker_id": "1",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016794.1.12",
          "form": "일본어공부는 잘 하고 잇서?",
          "original_form": "일본어공부는 잘 하고 잇서?",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016794.1.13",
          "form": "ㅎ.. 접수 취소각아닌지 싶고ㅠ",
          "original_form": "ㅎ.. 접수 취소각아닌지 싶고ㅠ",
          "speaker_id": "1",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016794.1.14",
          "form": "웃을일이 아닌데",
          "original_form": "웃을일이 아닌데",
          "speaker_id": "1",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016794.1.15",
          "form": "웃음이 나온다",
          "original_form": "웃음이 나온다",
          "speaker_id": "1",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016794.1.16",
          "form": "따흑",
          "original_form": "따흑",
          "speaker_id": "1",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016794.1.17",
          "form": "9월초 시험이였나",
          "original_form": "9월초 시험이였나",
          "speaker_id": "2",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016794.1.18",
          "form": "미루면 언제보게되는겨..",
          "original_form": "미루면 언제보게되는겨..",
          "speaker_id": "2",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016794.1.19",
          "form": "졸업..",
          "original_form": "졸업..",
          "speaker_id": "2",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016794.1.20",
          "form": "쉽지않다",
          "original_form": "쉽지않다",
          "speaker_id": "2",
          "time": "20210826 15:54"
        },
        {
          "id": "MDRW2100016794.1.21",
          "form": "걍 졸업시험 보고",
          "original_form": "걍 졸업시험 보고",
          "speaker_id": "1",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016794.1.22",
          "form": "그럼 되는거야?",
          "original_form": "그럼 되는거야?",
          "speaker_id": "2",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016794.1.23",
          "form": "끝끝?",
          "original_form": "끝끝?",
          "speaker_id": "2",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016794.1.24",
          "form": "12월에 자격증 따야지,,",
          "original_form": "12월에 자격증 따야지,,",
          "speaker_id": "1",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016794.1.25",
          "form": "취업생각해서ㅜ",
          "original_form": "취업생각해서ㅜ",
          "speaker_id": "1",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016794.1.26",
          "form": "한의원은 끗낫고",
          "original_form": "한의원은 끗낫고",
          "speaker_id": "1",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016794.1.27",
          "form": "엔투?",
          "original_form": "엔투?",
          "speaker_id": "2",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016794.1.28",
          "form": "피부과 간다",
          "original_form": "피부과 간다",
          "speaker_id": "1",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016794.1.29",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016794.1.30",
          "form": "바쁘다바빠",
          "original_form": "바쁘다바빠",
          "speaker_id": "2",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016794.1.31",
          "form": "아니 치료받는게 왜 나쁜말인데요",
          "original_form": "아니 치료받는게 왜 나쁜말인데요",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016794.1.32",
          "form": "여기 조선시대야?ㅋㅋㅋㅋㄲㄲ",
          "original_form": "여기 조선시대야?ㅋㅋㅋㅋㄲㄲ",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016794.1.33",
          "form": "나도 졸업시험 줄여서 쓰다가",
          "original_form": "나도 졸업시험 줄여서 쓰다가",
          "speaker_id": "1",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016794.1.34",
          "form": "그럼 대학은 안다녀도 되겠네",
          "original_form": "그럼 대학은 안다녀도 되겠네",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016794.1.35",
          "form": "개강하고",
          "original_form": "개강하고",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016794.1.36",
          "form": "나는 한달있다",
          "original_form": "나는 한달있다",
          "speaker_id": "2",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016794.1.37",
          "form": "바로 또 실습",
          "original_form": "바로 또 실습",
          "speaker_id": "2",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016794.1.38",
          "form": "모의수업할듯 대면 때",
          "original_form": "모의수업할듯 대면 때",
          "speaker_id": "2",
          "time": "20210826 15:59"
        }
      ]
    }
