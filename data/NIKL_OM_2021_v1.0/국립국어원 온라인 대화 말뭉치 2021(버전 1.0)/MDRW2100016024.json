          "time": "20210826 08:07"
        },
        {
          "id": "MDRW2100016024.1.16",
          "form": "그거말씀하시는건가",
          "original_form": "그거말씀하시는건가",
          "speaker_id": "1",
          "time": "20210826 08:07"
