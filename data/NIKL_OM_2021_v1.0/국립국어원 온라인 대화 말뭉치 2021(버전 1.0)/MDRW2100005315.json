      "utterance": [
        {
          "id": "MDRW2100005315.1.1",
          "form": "지금 태풍올라오나봐요",
          "original_form": "지금 태풍올라오나봐요",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005315.1.2",
          "form": "장마아니고 태풍이예요?",
          "original_form": "장마아니고 태풍이예요?",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005315.1.3",
          "form": "뉴스에서 가을장마라더니 태풍도오나봐요",
          "original_form": "뉴스에서 가을장마라더니 태풍도오나봐요",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005315.1.4",
          "form": "무섭네요 ㅜㅠ",
          "original_form": "무섭네요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005315.1.5",
          "form": "우린 태풍오면대기하고 난린데",
          "original_form": "우린 태풍오면대기하고 난린데",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005315.1.6",
          "form": "일기예보 확인해보세요",
          "original_form": "일기예보 확인해보세요",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005315.1.7",
          "form": "뉴스보니 부산 난리났던데요",
          "original_form": "뉴스보니 부산 난리났던데요",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005315.1.8",
          "form": "진짜요? 오늘 하루 뉴스를 못봤네요",
          "original_form": "진짜요? 오늘 하루 뉴스를 못봤네요",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005315.1.9",
          "form": "저도 잠깐봐서 장맛비인지 태풍때문인지 확실치않아요ㅎ",
          "original_form": "저도 잠깐봐서 장맛비인지 태풍때문인지 확실치않아요ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005315.1.10",
          "form": "부산에 물난리났나요?",
          "original_form": "부산에 물난리났나요?",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005315.1.11",
          "form": "그런가봐요 어디지역은 배뒤집히고",
          "original_form": "그런가봐요 어디지역은 배뒤집히고",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005315.1.12",
          "form": "헉 배뒤집힐정도로 바람이 센가보네요",
          "original_form": "헉 배뒤집힐정도로 바람이 센가보네요",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005315.1.13",
          "form": "그런가봐요 피해가 없어야 할텐데",
          "original_form": "그런가봐요 피해가 없어야 할텐데",
          "speaker_id": "1",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005315.1.14",
          "form": "저는 회사대기하라고 통보가 안와야할텐데요 ㅜㅠ",
          "original_form": "저는 회사대기하라고 통보가 안와야할텐데요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005315.1.15",
          "form": "대구는 괜찮지 않을까요?",
          "original_form": "대구는 괜찮지 않을까요?",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005315.1.16",
          "form": "안오길 바래봐야쥬",
          "original_form": "안오길 바래봐야쥬",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005315.1.17",
          "form": "이따 일기예보 다시봐야겠어요",
          "original_form": "이따 일기예보 다시봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005315.1.18",
          "form": "서울은 많이안오는거 같은데",
          "original_form": "서울은 많이안오는거 같은데",
          "speaker_id": "1",
          "time": "20210821 22:03"
        }
      ]
