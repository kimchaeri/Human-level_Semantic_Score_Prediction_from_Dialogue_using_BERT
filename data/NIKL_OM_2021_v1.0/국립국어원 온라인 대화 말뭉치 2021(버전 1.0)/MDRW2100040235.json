      "utterance": [
        {
          "id": "MDRW2100040235.1.1",
          "form": "저는 후라이드 안매운거 좋아해요",
          "original_form": "저는 후라이드 안매운거 좋아해요",
          "speaker_id": "2",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100040235.1.2",
          "form": "아하~ 평소 음식 드실때도요?",
          "original_form": "아하~ 평소 음식 드실때도요?",
          "speaker_id": "1",
          "time": "20210904 14:33"
        },
        {
          "id": "MDRW2100040235.1.3",
          "form": "네 저 매운거 잘 못먹어서",
          "original_form": "네 저 매운거 잘 못먹어서",
          "speaker_id": "2",
          "time": "20210904 14:33"
        },
        {
          "id": "MDRW2100040235.1.4",
          "form": "짬뽕도 제 음식으로 시켜본적이 없네요",
          "original_form": "짬뽕도 제 음식으로 시켜본적이 없네요",
          "speaker_id": "2",
          "time": "20210904 14:33"
        },
        {
          "id": "MDRW2100040235.1.5",
          "form": "아하..! 그럼 짜장면 좋아하세요?",
          "original_form": "아하..! 그럼 짜장면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 14:34"
        },
        {
          "id": "MDRW2100040235.1.6",
          "form": "짜장면 아니면 볶음밥이요",
          "original_form": "짜장면 아니면 볶음밥이요",
          "speaker_id": "2",
          "time": "20210904 14:34"
        },
        {
          "id": "MDRW2100040235.1.7",
          "form": "또봉이는 얼마정도 해요?",
          "original_form": "또봉이는 얼마정도 해요?",
          "speaker_id": "2",
          "time": "20210904 14:36"
        },
        {
          "id": "MDRW2100040235.1.8",
          "form": "또봉이는 두마리에 22000원 정도요!",
          "original_form": "또봉이는 두마리에 22000원 정도요!",
          "speaker_id": "1",
          "time": "20210904 14:37"
        },
        {
          "id": "MDRW2100040235.1.9",
          "form": "아 볶음밥 너무 맛있죠...",
          "original_form": "아 볶음밥 너무 맛있죠...",
          "speaker_id": "1",
          "time": "20210904 14:37"
        },
        {
          "id": "MDRW2100040235.1.10",
          "form": "두마리에요??",
          "original_form": "두마리에요??",
          "speaker_id": "2",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100040235.1.11",
          "form": "싸네요?",
          "original_form": "싸네요?",
          "speaker_id": "2",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100040235.1.12",
          "form": "또봉이두마리치킨이라 두 마리예요",
          "original_form": "또봉이두마리치킨이라 두 마리예요",
          "speaker_id": "1",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100040235.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맛있어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맛있어요",
          "speaker_id": "1",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100040235.1.14",
          "form": "아...그렇네요 가게 이름부터 두마리네요 ㅎ",
          "original_form": "아...그렇네요 가게 이름부터 두마리네요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100040235.1.15",
          "form": "다음에 기회되면 꼭 드셔보세요!",
          "original_form": "다음에 기회되면 꼭 드셔보세요!",
          "speaker_id": "1",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100040235.1.16",
          "form": "네 그래야 겟네요 ㅎㅎ",
          "original_form": "네 그래야 겟네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100040235.1.17",
          "form": "후라이드 매운건 아니죠?",
          "original_form": "후라이드 매운건 아니죠?",
          "speaker_id": "2",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100040235.1.18",
          "form": "네! 그냥 후라이드입니다 ㅎㅎ",
          "original_form": "네! 그냥 후라이드입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100040235.1.19",
