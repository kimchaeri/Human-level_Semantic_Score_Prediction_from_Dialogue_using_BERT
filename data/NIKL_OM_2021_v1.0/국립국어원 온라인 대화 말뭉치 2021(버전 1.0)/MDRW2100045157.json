      "utterance": [
        {
          "id": "MDRW2100045157.1.1",
          "form": "식혜 좋아하세요??",
          "original_form": "식혜 좋아하세요??",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045157.1.2",
          "form": "아 좋아해영 !",
          "original_form": "아 좋아해영 !",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045157.1.3",
          "form": "식혜는어디꺼 드세여",
          "original_form": "식혜는어디꺼 드세여",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045157.1.4",
          "form": "저 얼마전에 호박식혜 먹었는데",
          "original_form": "저 얼마전에 호박식혜 먹었는데",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045157.1.5",
          "form": "엄청 달더라고요",
          "original_form": "엄청 달더라고요",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045157.1.6",
          "form": "맛이 없어서 추천 못해드려요ㅋㅋ",
          "original_form": "맛이 없어서 추천 못해드려요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045157.1.7",
          "form": "호박자체가 달아서 그런가봐요ㅠ",
          "original_form": "호박자체가 달아서 그런가봐요ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045157.1.8",
          "form": "전 밥알 없는거 좋아하는데",
          "original_form": "전 밥알 없는거 좋아하는데",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045157.1.9",
          "form": "그렇다면 더더욱 안좋아하실 맛이었어요 ㅎㅎ",
          "original_form": "그렇다면 더더욱 안좋아하실 맛이었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045157.1.10",
          "form": "밥알 엄청 많았거든요",
          "original_form": "밥알 엄청 많았거든요",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045157.1.11",
          "form": "켁 ㅠㅠㅠ",
          "original_form": "켁 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045157.1.12",
          "form": "느린머시기 많이 사던데 식혜가 맞았나",
          "original_form": "느린머시기 많이 사던데 식혜가 맞았나",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045157.1.13",
          "form": "느린마을.",
          "original_form": "느린마을.",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045157.1.14",
          "form": "느린마을 이라는 식혜도 있나보네요??",
          "original_form": "느린마을 이라는 식혜도 있나보네요??",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045157.1.15",
          "form": "저는 처음 들었어요",
          "original_form": "저는 처음 들었어요",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045157.1.16",
          "form": "느린마을은 막걸리네요 ㅎ 그냥 느린 식헤ㅋㅋㅋ",
          "original_form": "느린마을은 막걸리네요 ㅎ 그냥 느린 식헤ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045157.1.17",
          "form": "아 그것도 처음들었어요 ㅎㅎ",
          "original_form": "아 그것도 처음들었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045157.1.18",
          "form": "ㅋㅋㅋ 그런게 있더라구요 맛있다고 글을.ㅋ 코스트코에도 파나봐요",
          "original_form": "ㅋㅋㅋ 그런게 있더라구요 맛있다고 글을.ㅋ 코스트코에도 파나봐요",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045157.1.19",
          "form": "오 그렇군요!! 식혜 맛있는거 사먹고 싶어요",
          "original_form": "오 그렇군요!! 식혜 맛있는거 사먹고 싶어요",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045157.1.20",
          "form": "비락은 너무 뭔가 맛이",
          "original_form": "비락은 너무 뭔가 맛이",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045157.1.21",
          "form": "아쉽..",
          "original_form": "아쉽..",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045157.1.22",
          "form": "해먹어야 맘에 들거 같기도 한데",
          "original_form": "해먹어야 맘에 들거 같기도 한데",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045157.1.23",
          "form": "번거로워서요",
          "original_form": "번거로워서요",
          "speaker_id": "2",
          "time": "20210908 01:12"
        }
      ]
    }
