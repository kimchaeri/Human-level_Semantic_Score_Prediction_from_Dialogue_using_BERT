      "utterance": [
        {
          "id": "MDRW2100019827.1.1",
          "form": "여행가고싶다",
          "original_form": "여행가고싶다",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019827.1.2",
          "form": "강릉 다녀오셨자나여",
          "original_form": "강릉 다녀오셨자나여",
          "speaker_id": "1",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019827.1.3",
          "form": "ㅋㅋㅋ 여긴 일하러 간건가",
          "original_form": "ㅋㅋㅋ 여긴 일하러 간건가",
          "speaker_id": "1",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019827.1.4",
          "form": "이건 여행이 아니야...",
          "original_form": "이건 여행이 아니야...",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019827.1.5",
          "form": "노동",
          "original_form": "노동",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019827.1.6",
          "form": "여행으로 강릉을 가면 좋을텐데",
          "original_form": "여행으로 강릉을 가면 좋을텐데",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019827.1.7",
          "form": "다른 곳을 가겠지",
          "original_form": "다른 곳을 가겠지",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019827.1.8",
          "form": "정말 좋지~ 그래도 바닷가 보니까 좋더라고",
          "original_form": "정말 좋지~ 그래도 바닷가 보니까 좋더라고",
          "speaker_id": "2",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019827.1.9",
          "form": "맨날 가평 시골에서 답답했는데",
          "original_form": "맨날 가평 시골에서 답답했는데",
          "speaker_id": "2",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019827.1.10",
          "form": "조금만 올라가면 진짜 물 더 맑잖아",
          "original_form": "조금만 올라가면 진짜 물 더 맑잖아",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019827.1.11",
          "form": "캬 바다",
          "original_form": "캬 바다",
          "speaker_id": "1",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100019827.1.12",
          "form": "바다 색 예쁘더라고 우리 올해 여행한번 가면 좋을것같아",
          "original_form": "바다 색 예쁘더라고 우리 올해 여행한번 가면 좋을것같아",
          "speaker_id": "2",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019827.1.13",
          "form": "강릉까지 기차로2시간 갈만하더라",
          "original_form": "강릉까지 기차로2시간 갈만하더라",
          "speaker_id": "2",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100019827.1.14",
          "form": "강릉여행 가?ㅋㅋㅋ",
          "original_form": "강릉여행 가?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100019827.1.15",
          "form": "오늘 난 오전근무하고 오후에 혼자 기차타고 간거야",
          "original_form": "오늘 난 오전근무하고 오후에 혼자 기차타고 간거야",
          "speaker_id": "2",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100019827.1.16",
          "form": "아 근무하고 갔구나 안그래도 쉬고 갔나 했어",
          "original_form": "아 근무하고 갔구나 안그래도 쉬고 갔나 했어",
          "speaker_id": "1",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100019827.1.17",
          "form": "괜찮더라고 기차 매진이었어",
          "original_form": "괜찮더라고 기차 매진이었어",
          "speaker_id": "2",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100019827.1.18",
          "form": "여행가도 잠만 자는거 아니야? 피곤해서",
          "original_form": "여행가도 잠만 자는거 아니야? 피곤해서",
          "speaker_id": "1",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100019827.1.19",
          "form": "너 제주도 갔을때 잠만 잤다며ㅋㅋㅋ",
          "original_form": "너 제주도 갔을때 잠만 잤다며ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100019827.1.20",
          "form": "아니라고 말을 못하겠네 ㅋㅋㅋㅋㅋ",
          "original_form": "아니라고 말을 못하겠네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100019827.1.21",
          "form": "ㅋㅋㅋㅋㅋ 들은게 있어서",
          "original_form": "ㅋㅋㅋㅋㅋ 들은게 있어서",
          "speaker_id": "1",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100019827.1.22",
          "form": "강릉도 카페가 많이생겨서 카페에서 디저트 먹고 바다보면서 자야지",
          "original_form": "강릉도 카페가 많이생겨서 카페에서 디저트 먹고 바다보면서 자야지",
          "speaker_id": "2",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100019827.1.23",
          "form": "생각만 해도 시원하네",
          "original_form": "생각만 해도 시원하네",
          "speaker_id": "1",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100019827.1.24",
          "form": "여행은 역시 휴식이잖아",
          "original_form": "여행은 역시 휴식이잖아",
          "speaker_id": "2",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100019827.1.25",
          "form": "그렇지",
