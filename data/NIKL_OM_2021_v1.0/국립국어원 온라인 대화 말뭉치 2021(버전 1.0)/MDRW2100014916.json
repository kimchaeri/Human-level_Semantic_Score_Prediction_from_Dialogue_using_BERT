      "utterance": [
        {
          "id": "MDRW2100014916.1.1",
          "form": "여행가고싶다",
          "original_form": "여행가고싶다",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014916.1.2",
          "form": "어디로?",
          "original_form": "어디로?",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014916.1.3",
          "form": "제주도!",
          "original_form": "제주도!",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014916.1.4",
          "form": "아 맞아 저번에 갔었지",
          "original_form": "아 맞아 저번에 갔었지",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014916.1.5",
          "form": "응 너무 좋았어",
          "original_form": "응 너무 좋았어",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014916.1.6",
          "form": "날씨는 더웠어",
          "original_form": "날씨는 더웠어",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014916.1.7",
          "form": "코로나 끝나면 또 가기로했잖아",
          "original_form": "코로나 끝나면 또 가기로했잖아",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014916.1.8",
          "form": "그때 먹은 피자가 가끔 생각나",
          "original_form": "그때 먹은 피자가 가끔 생각나",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014916.1.9",
          "form": "나도 근데 차없으면 힘들겠더라",
          "original_form": "나도 근데 차없으면 힘들겠더라",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014916.1.10",
          "form": "그건그래 렌트해야지",
          "original_form": "그건그래 렌트해야지",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014916.1.11",
          "form": "에코랜드도 좋았어",
          "original_form": "에코랜드도 좋았어",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014916.1.12",
          "form": "풍경이 예뻤어",
          "original_form": "풍경이 예뻤어",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014916.1.13",
          "form": "맞아 비와서 운치있었지",
          "original_form": "맞아 비와서 운치있었지",
          "speaker_id": "1",
          "time": "20210825 21:13"
        }
