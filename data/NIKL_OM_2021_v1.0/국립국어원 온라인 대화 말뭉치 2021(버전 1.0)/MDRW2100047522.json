      "utterance": [
        {
          "id": "MDRW2100047522.1.1",
          "form": "저녁 뭐드셨나요",
          "original_form": "저녁 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100047522.1.2",
          "form": "양장피를 시켜먹으려 했으나",
          "original_form": "양장피를 시켜먹으려 했으나",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100047522.1.3",
          "form": "마음을 바꿔 안주상을 차리고 있어요",
          "original_form": "마음을 바꿔 안주상을 차리고 있어요",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100047522.1.4",
          "form": "어떤 안주인가요?",
          "original_form": "어떤 안주인가요?",
          "speaker_id": "2",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100047522.1.5",
          "form": "닭근위 닭발 곱창볶음이요",
          "original_form": "닭근위 닭발 곱창볶음이요",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100047522.1.6",
          "form": "와 3종 세트네요^^",
          "original_form": "와 3종 세트네요^^",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100047522.1.7",
          "form": "^^ 다 인스턴트예요ㅎㅎ",
          "original_form": "^^ 다 인스턴트예요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100047522.1.8",
          "form": "전자레인지 1분컷",
          "original_form": "전자레인지 1분컷",
          "speaker_id": "1",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100047522.1.9",
          "form": "ㅋㅋ 안주야 이런건가요?",
          "original_form": "ㅋㅋ 안주야 이런건가요?",
          "speaker_id": "2",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100047522.1.10",
          "form": "네 하하와 김종국이 포장에 있길래 한번 믿어보려고요ㅎ",
          "original_form": "네 하하와 김종국이 포장에 있길래 한번 믿어보려고요ㅎ",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100047522.1.11",
          "form": "인스턴트도 드시나요?",
          "original_form": "인스턴트도 드시나요?",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100047522.1.12",
          "form": "그럼요~ㅎㅎ 만두가 한끼 후루룩 먹기 편한듯요",
          "original_form": "그럼요~ㅎㅎ 만두가 한끼 후루룩 먹기 편한듯요",
          "speaker_id": "2",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100047522.1.13",
          "form": "라면도 안 드실 줄 알았어요ㅎㅎ",
          "original_form": "라면도 안 드실 줄 알았어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100047522.1.14",
          "form": "만두는 어느 브랜드 사세요?",
          "original_form": "만두는 어느 브랜드 사세요?",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100047522.1.15",
          "form": "비비고가 맛있더라구요~! 풀무원 얇은피도!",
          "original_form": "비비고가 맛있더라구요~! 풀무원 얇은피도!",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100047522.1.16",
          "form": "역시 유명한데는 이유가 있네요ㅎ",
          "original_form": "역시 유명한데는 이유가 있네요ㅎ",
          "speaker_id": "1",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100047522.1.17",
          "form": "저도 비비고 픽에 김치는 풀무원이요",
          "original_form": "저도 비비고 픽에 김치는 풀무원이요",
