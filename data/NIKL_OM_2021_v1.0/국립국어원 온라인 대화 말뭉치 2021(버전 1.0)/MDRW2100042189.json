      "utterance": [
        {
          "id": "MDRW2100042189.1.1",
          "form": "저희 동네는 비가 많이 와요",
          "original_form": "저희 동네는 비가 많이 와요",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042189.1.2",
          "form": "저희도 비가 많이 내려요",
          "original_form": "저희도 비가 많이 내려요",
          "speaker_id": "2",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042189.1.3",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042189.1.4",
          "form": "실내에 계신거죠?",
          "original_form": "실내에 계신거죠?",
          "speaker_id": "1",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042189.1.5",
          "form": "네 집이에요 밖에 있으세요?",
          "original_form": "네 집이에요 밖에 있으세요?",
          "speaker_id": "2",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042189.1.6",
          "form": "아뇨 저도 집이요",
          "original_form": "아뇨 저도 집이요",
          "speaker_id": "1",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100042189.1.7",
          "form": "비올 때 밖에 있음 난감하니",
          "original_form": "비올 때 밖에 있음 난감하니",
          "speaker_id": "1",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100042189.1.8",
          "form": "혹시 밖이신가 했어요",
          "original_form": "혹시 밖이신가 했어요",
          "speaker_id": "1",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100042189.1.9",
          "form": "맞아요 비 많이 오면 밖에 있는 자체가 힘들죠",
          "original_form": "맞아요 비 많이 오면 밖에 있는 자체가 힘들죠",
          "speaker_id": "2",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042189.1.10",
          "form": "네 우산 있어도 젖고",
          "original_form": "네 우산 있어도 젖고",
          "speaker_id": "1",
          "time": "20210906 20:38"
        },
        {
          "id": "MDRW2100042189.1.11",
          "form": "비 많이 내리면 어떻게 집에 가나도 걱정이고요",
          "original_form": "비 많이 내리면 어떻게 집에 가나도 걱정이고요",
          "speaker_id": "1",
          "time": "20210906 20:41"
        },
        {
          "id": "MDRW2100042189.1.12",
          "form": "요즘 추워져서 감기에 걸리기 더 쉽고요 ㅠ",
          "original_form": "요즘 추워져서 감기에 걸리기 더 쉽고요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 20:44"
        },
        {
          "id": "MDRW2100042189.1.13",
          "form": "참 이번주 초까지 가을장마래요",
          "original_form": "참 이번주 초까지 가을장마래요",
          "speaker_id": "2",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100042189.1.14",
          "form": "앗 가을장마 아직 안끝났군요",
          "original_form": "앗 가을장마 아직 안끝났군요",
          "speaker_id": "1",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100042189.1.15",
          "form": "네 지금 오는 비는 마지막으로 오는 거라고 하더라고요",
          "original_form": "네 지금 오는 비는 마지막으로 오는 거라고 하더라고요",
          "speaker_id": "2",
          "time": "20210906 20:47"
        },
        {
          "id": "MDRW2100042189.1.16",
          "form": "아 그럼 다시 맑은 날이 오겠네요",
          "original_form": "아 그럼 다시 맑은 날이 오겠네요",
          "speaker_id": "1",
          "time": "20210906 20:49"
        },
        {
          "id": "MDRW2100042189.1.17",
          "form": "네 살짝 기대하고 있어요 기상청이 맞춰야는데 말이죠 ㅠㅋㅋ",
          "original_form": "네 살짝 기대하고 있어요 기상청이 맞춰야는데 말이죠 ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042189.1.18",
          "form": "네 일기예보가 맞길!!",
          "original_form": "네 일기예보가 맞길!!",
          "speaker_id": "1",
          "time": "20210906 20:51"
