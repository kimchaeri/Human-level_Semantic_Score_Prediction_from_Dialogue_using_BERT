      "utterance": [
        {
          "id": "MDRW2100003468.1.1",
          "form": "요즘 사고싶은거 있어?",
          "original_form": "요즘 사고싶은거 있어?",
          "speaker_id": "1",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003468.1.2",
          "form": "컴퓨터 사고 싶어",
          "original_form": "컴퓨터 사고 싶어",
          "speaker_id": "2",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100003468.1.3",
          "form": "컴퓨터 고장났어?",
          "original_form": "컴퓨터 고장났어?",
          "speaker_id": "1",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100003468.1.4",
          "form": "우리집 데스크톱 고장난지 오래되어서",
          "original_form": "우리집 데스크톱 고장난지 오래되어서",
          "speaker_id": "2",
          "time": "20210820 23:27"
        },
        {
          "id": "MDRW2100003468.1.5",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210820 23:27"
        },
        {
          "id": "MDRW2100003468.1.6",
          "form": "사양 높은 게임 하고 싶어",
          "original_form": "사양 높은 게임 하고 싶어",
          "speaker_id": "2",
          "time": "20210820 23:27"
        },
        {
          "id": "MDRW2100003468.1.7",
          "form": "돈 모아서 바꿔야겠네",
          "original_form": "돈 모아서 바꿔야겠네",
          "speaker_id": "1",
          "time": "20210820 23:27"
        },
        {
          "id": "MDRW2100003468.1.8",
          "form": "컴퓨터 바꾸면 삶의 질이 나아지더라",
          "original_form": "컴퓨터 바꾸면 삶의 질이 나아지더라",
          "speaker_id": "1",
          "time": "20210820 23:28"
        },
        {
          "id": "MDRW2100003468.1.9",
          "form": "진짜 사고싶다ㅠ",
          "original_form": "진짜 사고싶다ㅠ",
          "speaker_id": "2",
          "time": "20210820 23:29"
        },
        {
          "id": "MDRW2100003468.1.10",
          "form": "너는 사고 싶은거 있어?",
          "original_form": "너는 사고 싶은거 있어?",
          "speaker_id": "2",
          "time": "20210820 23:29"
        },
        {
          "id": "MDRW2100003468.1.11",
          "form": "나는 최근에 사고싶은걸 많이 샀어",
          "original_form": "나는 최근에 사고싶은걸 많이 샀어",
          "speaker_id": "1",
          "time": "20210820 23:30"
        },
        {
          "id": "MDRW2100003468.1.12",
          "form": "아직 남은건... 흠",
          "original_form": "아직 남은건... 흠",
          "speaker_id": "1",
          "time": "20210820 23:30"
        },
        {
          "id": "MDRW2100003468.1.13",
          "form": "나도 조립 pc 사고싶다",
          "original_form": "나도 조립 pc 사고싶다",
          "speaker_id": "1",
          "time": "20210820 23:30"
        },
        {
          "id": "MDRW2100003468.1.14",
          "form": "노트북은 사양이 딸리네",
          "original_form": "노트북은 사양이 딸리네",
          "speaker_id": "1",
          "time": "20210820 23:31"
        },
        {
          "id": "MDRW2100003468.1.15",
          "form": "나도 조립 pc 원하는데 구성 어떻게 해야 좋은지 몰라ㅋㅋ",
          "original_form": "나도 조립 pc 원하는데 구성 어떻게 해야 좋은지 몰라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100003468.1.16",
          "form": "그건 컴퓨터 잘 아는 친구한테 맡기는게 좋은거같아",
          "original_form": "그건 컴퓨터 잘 아는 친구한테 맡기는게 좋은거같아",
          "speaker_id": "1",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100003468.1.17",
          "form": "응 나 혼자서는 뭐가 뭔지 잘 몰라서",
          "original_form": "응 나 혼자서는 뭐가 뭔지 잘 몰라서",
          "speaker_id": "2",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100003468.1.18",
          "form": "그게 좋지",
          "original_form": "그게 좋지",
          "speaker_id": "2",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100003468.1.19",
          "form": "저번에 잘 아는 친구한테 본가 pc 조립 맡겼는데",
          "original_form": "저번에 잘 아는 친구한테 본가 pc 조립 맡겼는데",
          "speaker_id": "1",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003468.1.20",
          "form": "아주 기깔나게 만들어와서 아직까지 만족스러워",
          "original_form": "아주 기깔나게 만들어와서 아직까지 만족스러워",
          "speaker_id": "1",
          "time": "20210820 23:36"
        },
