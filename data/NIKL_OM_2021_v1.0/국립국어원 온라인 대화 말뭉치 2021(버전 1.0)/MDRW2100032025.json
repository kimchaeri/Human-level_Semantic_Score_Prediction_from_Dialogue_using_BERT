          "speaker_id": "1",
          "time": "20210905 12:25"
        },
        {
          "id": "MDRW2100032025.1.8",
          "form": "첨성대 이쁜거 같아요",
          "original_form": "첨성대 이쁜거 같아요",
          "speaker_id": "2",
          "time": "20210905 12:45"
