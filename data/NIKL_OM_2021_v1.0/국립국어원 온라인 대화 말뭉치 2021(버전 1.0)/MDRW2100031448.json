      "utterance": [
        {
          "id": "MDRW2100031448.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210904 21:35"
        },
        {
          "id": "MDRW2100031448.1.2",
          "form": "요즘 이용하는 ott 있어요?",
          "original_form": "요즘 이용하는 ott 있어요?",
          "speaker_id": "1",
          "time": "20210904 21:35"
        },
        {
          "id": "MDRW2100031448.1.3",
          "form": "티빙 하나 구독하고 있어요",
          "original_form": "티빙 하나 구독하고 있어요",
          "speaker_id": "2",
          "time": "20210904 21:36"
        },
        {
          "id": "MDRW2100031448.1.4",
          "form": "티빙으로 최근에 뭐 봤어요?",
          "original_form": "티빙으로 최근에 뭐 봤어요?",
          "speaker_id": "1",
          "time": "20210904 21:37"
        },
        {
          "id": "MDRW2100031448.1.5",
          "form": "놀라운 토요일이랑 대탈출이요",
          "original_form": "놀라운 토요일이랑 대탈출이요",
          "speaker_id": "2",
          "time": "20210904 21:37"
        },
        {
          "id": "MDRW2100031448.1.6",
          "form": "놀라운 토요일이 노래듣고 가사맞추고 퀴즈 맞추는거죠?",
          "original_form": "놀라운 토요일이 노래듣고 가사맞추고 퀴즈 맞추는거죠?",
          "speaker_id": "1",
          "time": "20210904 21:43"
        },
        {
          "id": "MDRW2100031448.1.7",
          "form": "네, 맞아요. 보시나요?",
          "original_form": "네, 맞아요. 보시나요?",
          "speaker_id": "2",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100031448.1.8",
          "form": "종종 티비 틀어서 하면 봐요",
          "original_form": "종종 티비 틀어서 하면 봐요",
          "speaker_id": "1",
          "time": "20210904 21:51"
        },
        {
          "id": "MDRW2100031448.1.9",
          "form": "그거 재방송도 엄청해주더라고요",
          "original_form": "그거 재방송도 엄청해주더라고요",
          "speaker_id": "1",
          "time": "20210904 21:51"
        },
        {
          "id": "MDRW2100031448.1.10",
          "form": "티비엔에서 꽤 인기있는 예능이니까요",
          "original_form": "티비엔에서 꽤 인기있는 예능이니까요",
          "speaker_id": "2",
          "time": "20210904 21:52"
        },
        {
          "id": "MDRW2100031448.1.11",
          "form": "중간부터 봐도 이해가 안가는 포맷도 아니구요",
          "original_form": "중간부터 봐도 이해가 안가는 포맷도 아니구요",
          "speaker_id": "2",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100031448.1.12",
          "form": "맞아요 잠깐잠깐 볼때 시청자들도 같이 풀 수 있는 퀴즈라서 더 좋은거 같아요",
          "original_form": "맞아요 잠깐잠깐 볼때 시청자들도 같이 풀 수 있는 퀴즈라서 더 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100031448.1.13",
          "form": "님은 어떤 ott 이용하나요?",
          "original_form": "님은 어떤 ott 이용하나요?",
          "speaker_id": "2",
          "time": "20210904 21:56"
        },
        {
          "id": "MDRW2100031448.1.14",
          "form": "저는 넷플이랑 티빙 써요!",
          "original_form": "저는 넷플이랑 티빙 써요!",
          "speaker_id": "1",
          "time": "20210904 21:57"
        },
        {
          "id": "MDRW2100031448.1.15",
          "form": "오, 님은 티빙에서 뭘보시나요?",
          "original_form": "오, 님은 티빙에서 뭘보시나요?",
          "speaker_id": "2",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100031448.1.16",
          "form": "저는 라이프온 마스봤어요",
          "original_form": "저는 라이프온 마스봤어요",
          "speaker_id": "1",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100031448.1.17",
          "form": "라이프 온 마스 아세요?",
          "original_form": "라이프 온 마스 아세요?",
          "speaker_id": "1",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100031448.1.18",
          "form": "오, 알아요. 아직 보진 않았지만",
          "original_form": "오, 알아요. 아직 보진 않았지만",
          "speaker_id": "2",
          "time": "20210904 22:12"
        },
        {
          "id": "MDRW2100031448.1.19",
          "form": "저도 추천받아서 봤는데 이거 디게 재미있더라고요",
          "original_form": "저도 추천받아서 봤는데 이거 디게 재미있더라고요",
          "speaker_id": "1",
          "time": "20210904 22:13"
        }
