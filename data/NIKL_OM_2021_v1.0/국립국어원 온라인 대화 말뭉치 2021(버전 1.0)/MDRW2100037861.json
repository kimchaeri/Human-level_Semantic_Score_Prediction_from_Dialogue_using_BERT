      "utterance": [
        {
          "id": "MDRW2100037861.1.1",
          "form": "고소한 냄새",
          "original_form": "고소한 냄새",
          "speaker_id": "1",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100037861.1.2",
          "form": "사람을 행복하게 하는",
          "original_form": "사람을 행복하게 하는",
          "speaker_id": "1",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100037861.1.3",
          "form": "결혼생활보고 깨소금 볶는다",
          "original_form": "결혼생활보고 깨소금 볶는다",
          "speaker_id": "1",
          "time": "20210901 20:15"
        },
        {
          "id": "MDRW2100037861.1.4",
          "form": "기름냄새 폴폴",
          "original_form": "기름냄새 폴폴",
          "speaker_id": "1",
          "time": "20210901 20:15"
        },
        {
          "id": "MDRW2100037861.1.5",
          "form": "이렇게 얘기하는 이유가 다 있는듯요",
          "original_form": "이렇게 얘기하는 이유가 다 있는듯요",
          "speaker_id": "1",
          "time": "20210901 20:15"
        },
        {
          "id": "MDRW2100037861.1.6",
          "form": "오! 결혼 하셨나요??",
          "original_form": "오! 결혼 하셨나요??",
          "speaker_id": "2",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100037861.1.7",
          "form": "아니요 저는 미혼이에요",
          "original_form": "아니요 저는 미혼이에요",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100037861.1.8",
          "form": "결혼하셨어요?",
          "original_form": "결혼하셨어요?",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100037861.1.9",
          "form": "저도 미혼이에요!",
          "original_form": "저도 미혼이에요!",
          "speaker_id": "2",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100037861.1.10",
          "form": "결혼생활이 깨소금 같다 하셔서 경험담인가 했어요 ㅎㅎ",
          "original_form": "결혼생활이 깨소금 같다 하셔서 경험담인가 했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100037861.1.11",
          "form": "그냥 들은 얘기였어요ㅎㅎㅎ",
          "original_form": "그냥 들은 얘기였어요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037861.1.12",
          "form": "전 사실 비혼주의에 가까워서",
          "original_form": "전 사실 비혼주의에 가까워서",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037861.1.13",
          "form": "저도 그래요 ㅎㅎ",
          "original_form": "저도 그래요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100037861.1.14",
          "form": "아직 확고한 신념은 없는데, 결혼에 대한 필요성은 못 느껴서 생각이 없어요 ㅎㅎ",
          "original_form": "아직 확고한 신념은 없는데, 결혼에 대한 필요성은 못 느껴서 생각이 없어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100037861.1.15",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100037861.1.16",
          "form": "막 굳이 꼭 해야한다는 의무감은 없어요",
          "original_form": "막 굳이 꼭 해야한다는 의무감은 없어요",
          "speaker_id": "1",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100037861.1.17",
          "form": "얼른 인식들이 외국처럼 좋아지면 좋겠네요",
          "original_form": "얼른 인식들이 외국처럼 좋아지면 좋겠네요",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100037861.1.18",
          "form": "신경 끄고 사는 삶으로!",
          "original_form": "신경 끄고 사는 삶으로!",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100037861.1.19",
          "form": "맞아요 개인의 선택을 존중하는",
          "original_form": "맞아요 개인의 선택을 존중하는",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100037861.1.20",
          "form": "분위기가 제대로 자리잡았으면 좋겠어요",
          "original_form": "분위기가 제대로 자리잡았으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100037861.1.21",
          "form": "저희 다음 세대에는 그런 분위기가 자리 잡겠죠 ㅎㅎ",
          "original_form": "저희 다음 세대에는 그런 분위기가 자리 잡겠죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100037861.1.22",
          "form": "그러길빌어요",
          "original_form": "그러길빌어요",
          "speaker_id": "1",
          "time": "20210902 09:52"
        },
        {
          "id": "MDRW2100037861.1.23",
          "form": "저희 세대는 고통 받겠네요",
          "original_form": "저희 세대는 고통 받겠네요",
          "speaker_id": "2",
          "time": "20210902 09:53"
        },
