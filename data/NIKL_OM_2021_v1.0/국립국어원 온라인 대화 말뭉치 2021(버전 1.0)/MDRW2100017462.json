      "utterance": [
        {
          "id": "MDRW2100017462.1.1",
          "form": "네! 집에 나물 많아서 비벼 먹었네요 ㅎ",
          "original_form": "네! 집에 나물 많아서 비벼 먹었네요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017462.1.2",
          "form": "무슨 나물 좋아하세요?",
          "original_form": "무슨 나물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017462.1.3",
          "form": "저는 고사리요! 국에 넣어 먹어도 맛있고 그냥 먹어도 좋네요ㅋㅋ",
          "original_form": "저는 고사리요! 국에 넣어 먹어도 맛있고 그냥 먹어도 좋네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017462.1.4",
          "form": "매운거 좋아하세요?",
          "original_form": "매운거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017462.1.5",
          "form": "저 매운거 너무 못 먹어요\r\n맵찔이 입니다 ㅠㅋㅋㅋㅋㅋ",
          "original_form": "저 매운거 너무 못 먹어요\r\n맵찔이 입니다 ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017462.1.6",
          "form": "ㅋㅋㅋㅋ아이고 민트초코는요?",
          "original_form": "ㅋㅋㅋㅋ아이고 민트초코는요?",
          "speaker_id": "1",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017462.1.7",
          "form": "그건 치약 아닌가요?ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그건 치약 아닌가요?ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017462.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ하와이안피자는요??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ하와이안피자는요??",
          "speaker_id": "1",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017462.1.9",
          "form": "파인애플 피자 완전 맛있는데 왜 논란인지 이해할 수 없어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "파인애플 피자 완전 맛있는데 왜 논란인지 이해할 수 없어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017462.1.10",
          "form": "저랑 입맛 비슷하신거같아요...",
          "original_form": "저랑 입맛 비슷하신거같아요...",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017462.1.11",
          "form": "배스킨라빈스에서 무슨맛 좋아하세요?",
          "original_form": "배스킨라빈스에서 무슨맛 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017462.1.12",
          "form": "저 레인보우 샤베트요ㅋㅋㅋㅋ 다들 싫어하던데 ㅠ",
          "original_form": "저 레인보우 샤베트요ㅋㅋㅋㅋ 다들 싫어하던데 ㅠ",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017462.1.13",
          "form": "헙 31요거트는 안좋아하세요?",
          "original_form": "헙 31요거트는 안좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017462.1.14",
          "form": "그것도 좋아해욬ㅋㅋㅋㅋ",
          "original_form": "그것도 좋아해욬ㅋㅋㅋㅋ",
