          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016054.1.17",
          "form": "아이고 그런 어려움이ㅠㅠ",
          "original_form": "아이고 그런 어려움이ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016054.1.18",
          "form": "그럼요 다행이 좋데요 제품이",
          "original_form": "그럼요 다행이 좋데요 제품이",
          "speaker_id": "2",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016054.1.19",
          "form": "네 찔려요 맘이 ㅡㅡ",
          "original_form": "네 찔려요 맘이 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016054.1.20",
          "form": "아 제껀 언제 사나요",
