      "utterance": [
        {
          "id": "MDRW2100014136.1.1",
          "form": "반려동물키우시나용?",
          "original_form": "반려동물키우시나용?",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014136.1.2",
          "form": "아뇨 ㅠㅠ 키우시나요?",
          "original_form": "아뇨 ㅠㅠ 키우시나요?",
          "speaker_id": "2",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014136.1.3",
          "form": "아뇹 ㅠㅠ 궁금해서 해봤는데 아쉽네용",
          "original_form": "아뇹 ㅠㅠ 궁금해서 해봤는데 아쉽네용",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014136.1.4",
          "form": "키우고싶은 동물있으세요?",
          "original_form": "키우고싶은 동물있으세요?",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014136.1.5",
          "form": "강아지 키워보고 싶긴해요!",
          "original_form": "강아지 키워보고 싶긴해요!",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014136.1.6",
          "form": "털 부슝뷰슝한 애들로 ㅎㅎㅎ",
          "original_form": "털 부슝뷰슝한 애들로 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014136.1.7",
          "form": "어떤 동물 키우고 싶으세요?",
          "original_form": "어떤 동물 키우고 싶으세요?",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014136.1.8",
          "form": "앗 저두요 ㅎㅎㅎ 강아지 너무 기엽죵",
          "original_form": "앗 저두요 ㅎㅎㅎ 강아지 너무 기엽죵",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014136.1.9",
          "form": "근데 저는 아직 동물들이 조금 무서운거같아요 흐극",
          "original_form": "근데 저는 아직 동물들이 조금 무서운거같아요 흐극",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014136.1.10",
          "form": "애견카페가보셧나요?",
          "original_form": "애견카페가보셧나요?",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014136.1.11",
          "form": "네네! 한번 가봤어요!",
          "original_form": "네네! 한번 가봤어요!",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014136.1.12",
          "form": "근데 저도 안 키워본 사람이라 동물 앞네서 뚝딱이게(?) 되더라라구요 ㅋㅋㅋ",
          "original_form": "근데 저도 안 키워본 사람이라 동물 앞네서 뚝딱이게(?) 되더라라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014136.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 뚝딱 인정이요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 뚝딱 인정이요",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014136.1.14",
          "form": "앵무새도 신기한것같아요!!",
          "original_form": "앵무새도 신기한것같아요!!",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014136.1.15",
          "form": "사람처럼 말하는 애들보면",
          "original_form": "사람처럼 말하는 애들보면",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014136.1.16",
          "form": "진짜 알고서 그러는 건가 신기해요",
          "original_form": "진짜 알고서 그러는 건가 신기해요",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014136.1.17",
          "form": "그러니깐요!!! 유투브에 엄청 뜨는게 신기해용",
          "original_form": "그러니깐요!!! 유투브에 엄청 뜨는게 신기해용",
          "speaker_id": "1",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014136.1.18",
          "form": "목소리 진짜 사람같은 애등도 잇자나요",
          "original_form": "목소리 진짜 사람같은 애등도 잇자나요",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014136.1.19",
          "form": "진짜 신기해요 ㅋㅋㅋㅋ",
          "original_form": "진짜 신기해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014136.1.20",
          "form": "마자요마자욬ㅋㅋㅋㅋ 유정이..? 아세요? 그집에서 키우는 앵무새 신기햐용",
          "original_form": "마자요마자욬ㅋㅋㅋㅋ 유정이..? 아세요? 그집에서 키우는 앵무새 신기햐용",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014136.1.21",
          "form": "검색해보고 왔는데 진짜 신기하네요 ㅋㅋㅋㅋ",
          "original_form": "검색해보고 왔는데 진짜 신기하네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:26"
        },
        {
          "id": "MDRW2100014136.1.22",
          "form": "진짜 엄마 목소리 같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 엄마 목소리 같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100014136.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ 그쵸그쵸 신기해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그쵸그쵸 신기해요",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014136.1.24",
          "form": "막 햄스터 집 짓는 사람들도 신기해요",
          "original_form": "막 햄스터 집 짓는 사람들도 신기해요",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014136.1.25",
          "form": "그 햄스터한테 이층집 맘들어주는 사람 본 거 갘아요",
          "original_form": "그 햄스터한테 이층집 맘들어주는 사람 본 거 갘아요",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014136.1.26",
