      "utterance": [
        {
          "id": "MDRW2100026687.1.1",
          "form": "기독교이신가요?",
          "original_form": "기독교이신가요?",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026687.1.2",
          "form": "저는 무교입니다",
          "original_form": "저는 무교입니다",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026687.1.3",
          "form": "가족들은 불교",
          "original_form": "가족들은 불교",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026687.1.4",
          "form": "그런데 종교를 가리진 않습니다.",
          "original_form": "그런데 종교를 가리진 않습니다.",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026687.1.5",
          "form": "저도 뮤교",
          "original_form": "저도 뮤교",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026687.1.6",
          "form": "무교",
          "original_form": "무교",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026687.1.7",
          "form": "최근..교회는 좀 싫더라구요..\r\n코로나의 영향으로...?",
          "original_form": "최근..교회는 좀 싫더라구요..\r\n코로나의 영향으로...?",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026687.1.8",
          "form": "엄마는 불교",
          "original_form": "엄마는 불교",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026687.1.9",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026687.1.10",
          "form": "아니 모이지 마라고 하면 적당히 하시지..",
          "original_form": "아니 모이지 마라고 하면 적당히 하시지..",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026687.1.11",
          "form": "신천지...",
          "original_form": "신천지...",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026687.1.12",
          "form": "기겁했습니다..",
          "original_form": "기겁했습니다..",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026687.1.13",
          "form": "엑스들",
          "original_form": "엑스들",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026687.1.14",
          "form": "신천지 확진자..무섭게 나오더군요",
          "original_form": "신천지 확진자..무섭게 나오더군요",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026687.1.15",
          "form": "진짜 욕나와요",
          "original_form": "진짜 욕나와요",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026687.1.16",
          "form": "모이지 말라면 말아야지",
          "original_form": "모이지 말라면 말아야지",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026687.1.17",
          "form": "행사는 왜 해서",
          "original_form": "행사는 왜 해서",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026687.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026687.1.19",
          "form": "아주 전국적인",
          "original_form": "아주 전국적인",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026687.1.20",
          "form": "종교망신입니다",
          "original_form": "종교망신입니다",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026687.1.21",
          "form": "종교활동도 좋지만 민폐는 안되죠",
          "original_form": "종교활동도 좋지만 민폐는 안되죠",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026687.1.22",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026687.1.23",
          "form": "주변에 피해없이 즐기고 믿는건",
          "original_form": "주변에 피해없이 즐기고 믿는건",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026687.1.24",
          "form": "덕분에 종교단체, 종교인들 모두 싫어하는 사람들이 늘어났 지요",
          "original_form": "덕분에 종교단체, 종교인들 모두 싫어하는 사람들이 늘어났 지요",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026687.1.25",
          "form": "좋은데.",
          "original_form": "좋은데.",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026687.1.26",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210901 23:40"
        }
      ]
    }
