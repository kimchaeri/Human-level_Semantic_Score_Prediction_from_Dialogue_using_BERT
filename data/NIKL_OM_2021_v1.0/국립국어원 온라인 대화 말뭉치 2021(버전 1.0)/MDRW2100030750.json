        },
        {
          "id": "MDRW2100030750.1.4",
          "form": "가서",
          "original_form": "가서",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100030750.1.5",
          "form": "빵 사올ㅋ대",
          "original_form": "빵 사올ㅋ대",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100030750.1.6",
          "form": "투썸에서",
          "original_form": "투썸에서",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100030750.1.7",
          "form": "사와요",
          "original_form": "사와요",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100030750.1.8",
          "form": "마실 거 라던지",
          "original_form": "마실 거 라던지",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100030750.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100030750.1.10",
          "form": "빵집에서 빵 다 사와야지",
          "original_form": "빵집에서 빵 다 사와야지",
          "speaker_id": "2",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100030750.1.11",
          "form": "아침 9시에 가야지",
          "original_form": "아침 9시에 가야지",
          "speaker_id": "2",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100030750.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:18"
        },
        {
          "id": "MDRW2100030750.1.13",
          "form": "장발장",
          "original_form": "장발장",
          "speaker_id": "1",
          "time": "20210904 12:18"
        },
        {
          "id": "MDRW2100030750.1.14",
          "form": "가게 이름 뭐냐",
          "original_form": "가게 이름 뭐냐",
          "speaker_id": "1",
          "time": "20210904 12:18"
        },
        {
          "id": "MDRW2100030750.1.15",
          "form": "달콤 크렘",
          "original_form": "달콤 크렘",
          "speaker_id": "2",
          "time": "20210904 12:18"
        },
        {
          "id": "MDRW2100030750.1.16",
          "form": "진짜 장발장이 되겠다",
          "original_form": "진짜 장발장이 되겠다",
          "speaker_id": "2",
          "time": "20210904 12:18"
        },
        {
          "id": "MDRW2100030750.1.17",
          "form": "크뤰브렐레",
          "original_form": "크뤰브렐레",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100030750.1.18",
          "form": "팔려나",
          "original_form": "팔려나",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100030750.1.19",
          "form": "그거 먹고 싶다",
          "original_form": "그거 먹고 싶다",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100030750.1.20",
          "form": "하지만 살 쪄",
          "original_form": "하지만 살 쪄",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100030750.1.21",
          "form": "그건 안 팔 듯",
          "original_form": "그건 안 팔 듯",
          "speaker_id": "2",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100030750.1.22",
          "form": "비싸고 만들기 힘들어서...",
          "original_form": "비싸고 만들기 힘들어서...",
          "speaker_id": "2",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100030750.1.23",
          "form": "그럼 맛있는 거 사와",
          "original_form": "그럼 맛있는 거 사와",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100030750.1.24",
          "form": "궁금하네",
          "original_form": "궁금하네",
          "speaker_id": "1",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100030750.1.25",
          "form": "종류별로 다 사와야지",
          "original_form": "종류별로 다 사와야지",
          "speaker_id": "2",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030750.1.26",
          "form": "달걀",
          "original_form": "달걀",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030750.1.27",
          "form": "20개 주겠네",
          "original_form": "20개 주겠네",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030750.1.28",
          "form": "고마워서",
          "original_form": "고마워서",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030750.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030750.1.30",
          "form": "달걀 1인 당 10개인데",
          "original_form": "달걀 1인 당 10개인데",
          "speaker_id": "2",
          "time": "20210904 12:25"
        },
        {
          "id": "MDRW2100030750.1.31",
          "form": "줄 안 길면 좋겠다",
          "original_form": "줄 안 길면 좋겠다",
          "speaker_id": "2",
          "time": "20210904 12:25"
        },
        {
          "id": "MDRW2100030750.1.32",
          "form": "줄 길까?",
          "original_form": "줄 길까?",
          "speaker_id": "1",
          "time": "20210904 12:26"
        },
        {
          "id": "MDRW2100030750.1.33",
          "form": "안 길 것 같ㅇ ㄴ데",
          "original_form": "안 길 것 같ㅇ ㄴ데",
          "speaker_id": "1",
          "time": "20210904 12:26"
        },
        {
          "id": "MDRW2100030750.1.34",
          "form": "유부초밥 엄제 만들지",
          "original_form": "유부초밥 엄제 만들지",
          "speaker_id": "1",
          "time": "20210904 12:26"
        },
        {
          "id": "MDRW2100030750.1.35",
          "form": "엄청 길 걸",
          "original_form": "엄청 길 걸",
          "speaker_id": "2",
          "time": "20210904 12:26"
        },
        {
          "id": "MDRW2100030750.1.36",
          "form": "마스크 살 때도 사람들 많았잖아",
          "original_form": "마스크 살 때도 사람들 많았잖아",
          "speaker_id": "2",
          "time": "20210904 12:27"
        },
        {
          "id": "MDRW2100030750.1.37",
          "form": "부지런한 행신동 사람들",
          "original_form": "부지런한 행신동 사람들",
          "speaker_id": "1",
          "time": "20210904 12:28"
        }
