      "utterance": [
        {
          "id": "MDRW2100016413.1.1",
          "form": "그러게요 입양도있는데",
          "original_form": "그러게요 입양도있는데",
          "speaker_id": "1",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100016413.1.2",
          "form": "남의 신체를 훼손하면서까지 욕심을 채우고싶나",
          "original_form": "남의 신체를 훼손하면서까지 욕심을 채우고싶나",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016413.1.3",
          "form": "굳이 남의 장기를 빌려서..",
          "original_form": "굳이 남의 장기를 빌려서..",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016413.1.4",
          "form": "너무 징그럽지않나요 그 욕망",
          "original_form": "너무 징그럽지않나요 그 욕망",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016413.1.5",
          "form": "결국 돈 주고 사는거잖아요",
          "original_form": "결국 돈 주고 사는거잖아요",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016413.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016413.1.7",
          "form": "역겨워요",
          "original_form": "역겨워요",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016413.1.8",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016413.1.9",
          "form": "인신매매랑 같은거같아요",
          "original_form": "인신매매랑 같은거같아요",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016413.1.10",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016413.1.11",
          "form": "진짜 이해안되고",
          "original_form": "진짜 이해안되고",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016413.1.12",
          "form": "나중에 남자들은",
          "original_form": "나중에 남자들은",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016413.1.13",
          "form": "이것도 혜택이다",
          "original_form": "이것도 혜택이다",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016413.1.14",
          "form": "여자는 대리모해서 돈 벌잖아ㅋ",
          "original_form": "여자는 대리모해서 돈 벌잖아ㅋ",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016413.1.15",
          "form": "이럴것 같아서 더 짜증나욬ㅋㅋㅋ",
          "original_form": "이럴것 같아서 더 짜증나욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016413.1.16",
          "form": "왘ㅋㅋㅋㅋㅋ 그걸 혜택이라고..",
          "original_form": "왘ㅋㅋㅋㅋㅋ 그걸 혜택이라고..",
          "speaker_id": "1",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100016413.1.17",
          "form": "정말 징그럽네요 모든걸 자본화하고",
          "original_form": "정말 징그럽네요 모든걸 자본화하고",
          "speaker_id": "1",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100016413.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016413.1.19",
          "form": "지금 성매매처럼",
          "original_form": "지금 성매매처럼",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016413.1.20",
          "form": "자본화하지 말아야할 영역이있는데..",
          "original_form": "자본화하지 말아야할 영역이있는데..",
          "speaker_id": "1",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016413.1.21",
          "form": "여성들이 벗어나지 못할 그런 악순환이 될까봐",
          "original_form": "여성들이 벗어나지 못할 그런 악순환이 될까봐",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016413.1.22",
          "form": "걱정돼요",
          "original_form": "걱정돼요",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016413.1.23",
          "form": "맞아요.. 돈벌려고 억지로 그 업에 뛰어들어야하고",
          "original_form": "맞아요.. 돈벌려고 억지로 그 업에 뛰어들어야하고",
          "speaker_id": "1",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100016413.1.24",
          "form": "독일은 돈없으면 성 ㅇ매매 하라고 한대요",
          "original_form": "독일은 돈없으면 성 ㅇ매매 하라고 한대요",
          "speaker_id": "1",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100016413.1.25",
          "form": "와 미쳤나봐요",
          "original_form": "와 미쳤나봐요",
          "speaker_id": "2",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100016413.1.26",
          "form": "독일 강대국이고",
          "original_form": "독일 강대국이고",
          "speaker_id": "2",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100016413.1.27",
          "form": "여성인권 높은편 아닌가요",
          "original_form": "여성인권 높은편 아닌가요",
          "speaker_id": "2",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100016413.1.28",
          "form": "충격이네요",
          "original_form": "충격이네요",
          "speaker_id": "2",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100016413.1.29",
          "form": "맞아요 근데 독일 성ㅇ매매 다 합법이라",
          "original_form": "맞아요 근데 독일 성ㅇ매매 다 합법이라",
          "speaker_id": "1",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100016413.1.30",
          "form": "기업적 성ㅇ매매도 합법이에요",
          "original_form": "기업적 성ㅇ매매도 합법이에요",
          "speaker_id": "1",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100016413.1.31",
          "form": "거대한 성ㅇ매매 빌딩이 있고",
          "original_form": "거대한 성ㅇ매매 빌딩이 있고",
          "speaker_id": "1",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100016413.1.32",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100016413.1.33",
          "form": "몰랐네요",
          "original_form": "몰랐네요",
          "speaker_id": "2",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100016413.1.34",
          "form": "동네 버스 정거장에 성ㅇ매매 광고 있고 그렇대요",
          "original_form": "동네 버스 정거장에 성ㅇ매매 광고 있고 그렇대요",
          "speaker_id": "1",
          "time": "20210826 13:02"
        }
      ]
