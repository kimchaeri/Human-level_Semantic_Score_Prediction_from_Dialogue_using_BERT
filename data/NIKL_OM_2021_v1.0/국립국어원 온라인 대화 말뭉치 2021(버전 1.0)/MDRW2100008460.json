          "id": "MDRW2100008460.1.1",
          "form": "습해요",
          "original_form": "습해요",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008460.1.2",
          "form": "장마라 그런가봐요",
          "original_form": "장마라 그런가봐요",
          "speaker_id": "1",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008460.1.3",
          "form": "늦장마네요",
          "original_form": "늦장마네요",
          "speaker_id": "2",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008460.1.4",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008460.1.5",
          "form": "내일은 비올까요?",
          "original_form": "내일은 비올까요?",
          "speaker_id": "1",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008460.1.6",
          "form": "태풍이 오니까",
          "original_form": "태풍이 오니까",
          "speaker_id": "2",
          "time": "20210822 16:01"
        },
        {
          "id": "MDRW2100008460.1.7",
          "form": "오겠네요",
          "original_form": "오겠네요",
          "speaker_id": "1",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100008460.1.8",
          "form": "출근할 때 우산 챙겨야겠어요",
          "original_form": "출근할 때 우산 챙겨야겠어요",
          "speaker_id": "1",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100008460.1.9",
          "form": "네 그래여",
          "original_form": "네 그래여",
          "speaker_id": "2",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100008460.1.10",
          "form": "출퇴근 할 때는 안 왔으면 좋겠네요",
          "original_form": "출퇴근 할 때는 안 왔으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100008460.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008460.1.12",
          "form": "일주일 정도는 오겠죠",
          "original_form": "일주일 정도는 오겠죠",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008460.1.13",
          "form": "네 태풍때문에",
          "original_form": "네 태풍때문에",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008460.1.14",
          "form": "걱정이네요",
          "original_form": "걱정이네요",
          "speaker_id": "1",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008460.1.15",
          "form": "피해없이지나갔으면",
          "original_form": "피해없이지나갔으면",
