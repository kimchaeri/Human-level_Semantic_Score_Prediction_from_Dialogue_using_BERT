      "utterance": [
        {
          "id": "MDRW2100024691.1.1",
          "form": "이제 저녁인데 메뉴 정하셨나요?",
          "original_form": "이제 저녁인데 메뉴 정하셨나요?",
          "speaker_id": "1",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024691.1.2",
          "form": "막걸리한잔하려고요",
          "original_form": "막걸리한잔하려고요",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024691.1.3",
          "form": "전이랑요?",
          "original_form": "전이랑요?",
          "speaker_id": "1",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024691.1.4",
          "form": "오늘처러 비오는 날엔 전에 막걸리가 딱이죠",
          "original_form": "오늘처러 비오는 날엔 전에 막걸리가 딱이죠",
          "speaker_id": "1",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024691.1.5",
          "form": "귀찮아서 막걸리만 ...",
          "original_form": "귀찮아서 막걸리만 ...",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024691.1.6",
          "form": "빈속에 막걸리만요?",
          "original_form": "빈속에 막걸리만요?",
          "speaker_id": "1",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024691.1.7",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210831 17:19"
        },
        {
          "id": "MDRW2100024691.1.8",
          "form": "빈속은 아니라서.. ㅋㅋ",
          "original_form": "빈속은 아니라서.. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:19"
        },
        {
          "id": "MDRW2100024691.1.9",
          "form": "에고.. 안주거리 있으면 좋은데",
          "original_form": "에고.. 안주거리 있으면 좋은데",
          "speaker_id": "1",
          "time": "20210831 17:19"
        },
        {
          "id": "MDRW2100024691.1.10",
          "form": "아.. 빈속 아니에요?ㅎㅎ",
          "original_form": "아.. 빈속 아니에요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 17:19"
        },
        {
          "id": "MDRW2100024691.1.11",
          "form": "계속 간식먹어서..  ㅋㅋ",
          "original_form": "계속 간식먹어서..  ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100024691.1.12",
          "form": "아항..",
          "original_form": "아항..",
          "speaker_id": "1",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024691.1.13",
          "form": "오늘 하루종일 비오길래 부침개를 해야하나 고민중인데",
          "original_form": "오늘 하루종일 비오길래 부침개를 해야하나 고민중인데",
          "speaker_id": "1",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024691.1.14",
          "form": "집에 막걸리가 없어요",
          "original_form": "집에 막걸리가 없어요",
          "speaker_id": "1",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024691.1.15",
          "form": "비와서 사러나가긴 싫고",
          "original_form": "비와서 사러나가긴 싫고",
          "speaker_id": "1",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024691.1.16",
          "form": "그래서 냉장고에는 항상 막걸리 맥주가 상비되어야해요",
          "original_form": "그래서 냉장고에는 항상 막걸리 맥주가 상비되어야해요",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024691.1.17",
          "form": "그렇군요.. 저희 집은 있을때도 있고 없을 때도 있어서요 이럴때 아쉽네요",
          "original_form": "그렇군요.. 저희 집은 있을때도 있고 없을 때도 있어서요 이럴때 아쉽네요",
          "speaker_id": "1",
          "time": "20210831 17:37"
        },
        {
          "id": "MDRW2100024691.1.18",
          "form": "ㅋㅋ 미리미리 준비하는 자세",
          "original_form": "ㅋㅋ 미리미리 준비하는 자세",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100024691.1.19",
          "form": "ㅎㅎ 좋네요",
          "original_form": "ㅎㅎ 좋네요",
          "speaker_id": "1",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100024691.1.20",
          "form": "배달도 종종 시켜드시나요?",
          "original_form": "배달도 종종 시켜드시나요?",
          "speaker_id": "1",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100024691.1.21",
          "form": "배달은 중식정도",
          "original_form": "배달은 중식정도",
          "speaker_id": "2",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100024691.1.22",
          "form": "최근에 스벅시켜봤어요",
          "original_form": "최근에 스벅시켜봤어요",
          "speaker_id": "2",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100024691.1.23",
          "form": "자주 사용하시진 않는군요.. 저도 그래요",
          "original_form": "자주 사용하시진 않는군요.. 저도 그래요",
          "speaker_id": "1",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100024691.1.24",
          "form": "저는 먹고 싶은게 있으면 가게 가서 시키고 픽업해오지 배달은 별로더라구요",
          "original_form": "저는 먹고 싶은게 있으면 가게 가서 시키고 픽업해오지 배달은 별로더라구요",
          "speaker_id": "1",
          "time": "20210831 17:42"
        },
        {
          "id": "MDRW2100024691.1.25",
          "form": "저도 픽업파요",
          "original_form": "저도 픽업파요",
          "speaker_id": "2",
          "time": "20210831 17:42"
        },
        {
          "id": "MDRW2100024691.1.26",
          "form": "은근 배달비 아까워요",
          "original_form": "은근 배달비 아까워요",
          "speaker_id": "2",
          "time": "20210831 17:43"
        },
        {
          "id": "MDRW2100024691.1.27",
          "form": "배달시 음식값도 다르구요",
          "original_form": "배달시 음식값도 다르구요",
          "speaker_id": "2",
          "time": "20210831 17:43"
        },
        {
          "id": "MDRW2100024691.1.28",
          "form": "배달비 너무 올랐어요",
          "original_form": "배달비 너무 올랐어요",
          "speaker_id": "1",
          "time": "20210831 17:47"
        },
        {
          "id": "MDRW2100024691.1.29",
          "form": "또 바로 오는 게 아니라 여러집 들렸다 오는 것도 맘에 안들구요",
          "original_form": "또 바로 오는 게 아니라 여러집 들렸다 오는 것도 맘에 안들구요",
          "speaker_id": "1",
          "time": "20210831 17:47"
        }
      ]
    }
