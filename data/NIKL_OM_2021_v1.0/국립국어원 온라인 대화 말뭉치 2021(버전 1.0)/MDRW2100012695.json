      "utterance": [
        {
          "id": "MDRW2100012695.1.1",
          "form": "저녁 드셨나요",
          "original_form": "저녁 드셨나요",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100012695.1.2",
          "form": "네~~!!!! 저는 고등어찌개를 먹었습니다 ㅎㅎㅎㅎ",
          "original_form": "네~~!!!! 저는 고등어찌개를 먹었습니다 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100012695.1.3",
          "form": "들어있는 무가 담백하니 맛있더라구요!!!",
          "original_form": "들어있는 무가 담백하니 맛있더라구요!!!",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100012695.1.4",
          "form": "저녁드셨어요?",
          "original_form": "저녁드셨어요?",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100012695.1.5",
          "form": "저는 조촐하게 안주상을 차렸어요",
          "original_form": "저는 조촐하게 안주상을 차렸어요",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100012695.1.6",
          "form": "오 안주상에 올라간 메뉴는 뭔가요..!",
          "original_form": "오 안주상에 올라간 메뉴는 뭔가요..!",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100012695.1.7",
          "form": "닭근위볶음 닭발 곱창볶음 몬도가네 삼종 세트요ㅎㅎ",
          "original_form": "닭근위볶음 닭발 곱창볶음 몬도가네 삼종 세트요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100012695.1.8",
          "form": "인스턴트가 잘 나오네요",
          "original_form": "인스턴트가 잘 나오네요",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100012695.1.9",
          "form": "와 ㅌㅋㅋㅋㅌㅋ",
          "original_form": "와 ㅌㅋㅋㅋㅌㅋ",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100012695.1.10",
          "form": "몬도가네... 가 뭐죠?",
          "original_form": "몬도가네... 가 뭐죠?",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100012695.1.11",
          "form": "닭발에.. 소주드시나요",
          "original_form": "닭발에.. 소주드시나요",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100012695.1.12",
          "form": "닭발 매콤하니 맛있을것 같아요ㅜㅜ",
          "original_form": "닭발 매콤하니 맛있을것 같아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100012695.1.13",
          "form": "누군가는 혐오성 식품이라 그래서요ㅜ 그래서 몬도가네라 했죠",
          "original_form": "누군가는 혐오성 식품이라 그래서요ㅜ 그래서 몬도가네라 했죠",
          "speaker_id": "1",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100012695.1.14",
          "form": "네 소주요",
          "original_form": "네 소주요",
          "speaker_id": "1",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100012695.1.15",
          "form": "혹시 튤립 닭발 아세요?",
          "original_form": "혹시 튤립 닭발 아세요?",
          "speaker_id": "1",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100012695.1.16",
          "form": "아니용 튤립닭발은 뭐죠? 뼈없는닭발??",
          "original_form": "아니용 튤립닭발은 뭐죠? 뼈없는닭발??",
          "speaker_id": "2",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100012695.1.17",
          "form": "발가락뼈는 발라내고 발목 뼈대만 남긴 닭발이요",
          "original_form": "발가락뼈는 발라내고 발목 뼈대만 남긴 닭발이요",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100012695.1.18",
          "form": "무뼈닭발은 아쉽고 뼈닭발은 번거로웠는데 둘다 절충한 닭발이 있네요ㅎ",
          "original_form": "무뼈닭발은 아쉽고 뼈닭발은 번거로웠는데 둘다 절충한 닭발이 있네요ㅎ",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100012695.1.19",
          "form": "모양이 튤립닮아서 튤립닭발이래요ㅎ",
          "original_form": "모양이 튤립닮아서 튤립닭발이래요ㅎ",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100012695.1.20",
          "form": "그거 뜯고 있어요",
          "original_form": "그거 뜯고 있어요",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100012695.1.21",
          "form": "추천해요!",
          "original_form": "추천해요!",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100012695.1.22",
          "form": "오오!!!!!!!",
          "original_form": "오오!!!!!!!",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100012695.1.23",
          "form": "완전 신세계 인데요?",
          "original_form": "완전 신세계 인데요?",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100012695.1.24",
          "form": "무뼈는 먹는맛이 안나고 뼈닭발은 진짜 번거롭긴했거든요",
          "original_form": "무뼈는 먹는맛이 안나고 뼈닭발은 진짜 번거롭긴했거든요",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100012695.1.25",
          "form": "대박 ㅋㅋㅋㅋ",
          "original_form": "대박 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100012695.1.26",
          "form": "ㅎㅎ 홈플러스에서 원플원으로 사서 만족도가 더 높네요",
          "original_form": "ㅎㅎ 홈플러스에서 원플원으로 사서 만족도가 더 높네요",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100012695.1.27",
          "form": "연중행사처럼 원플원하나봐요",
          "original_form": "연중행사처럼 원플원하나봐요",
          "speaker_id": "1",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100012695.1.28",
          "form": "아아 홈플러스를 자주 안가다보니 몰랐어요! 다른곳에서도 팔겠쥬?",
          "original_form": "아아 홈플러스를 자주 안가다보니 몰랐어요! 다른곳에서도 팔겠쥬?",
          "speaker_id": "2",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100012695.1.29",
          "form": "어;; 비슷한 거라도 팔지 않을까요ㅎ",
          "original_form": "어;; 비슷한 거라도 팔지 않을까요ㅎ",
          "speaker_id": "1",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100012695.1.30",
          "form": "전 글램쿡거 마성의 튤립불닭발 샀어요",
          "original_form": "전 글램쿡거 마성의 튤립불닭발 샀어요",
          "speaker_id": "1",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100012695.1.31",
          "form": "근데 좀 매워요!",
          "original_form": "근데 좀 매워요!",
          "speaker_id": "1",
          "time": "20210826 22:13"
        }
      ]
    }
