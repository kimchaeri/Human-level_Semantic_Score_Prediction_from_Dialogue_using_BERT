        {
          "id": "MDRW2100019069.1.1",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100019069.1.2",
          "form": "큰 아들 학교 다녀왔잖아",
          "original_form": "큰 아들 학교 다녀왔잖아",
          "speaker_id": "1",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100019069.1.3",
          "form": "ㅇㅇ 아까 교과서 받으러 갔다며",
          "original_form": "ㅇㅇ 아까 교과서 받으러 갔다며",
          "speaker_id": "2",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100019069.1.4",
          "form": "근데 다녀와서는 실내화가 너무 작대.. 발 아프다고 ㅋㅋㅋ",
          "original_form": "근데 다녀와서는 실내화가 너무 작대.. 발 아프다고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019069.1.5",
          "form": "훌 그거 올해 초에 새로 산거잖아",
          "original_form": "훌 그거 올해 초에 새로 산거잖아",
          "speaker_id": "2",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019069.1.6",
          "form": "ㅇㅇ 심지어 그 때 엄청 커서 헐렁거렸거든",
          "original_form": "ㅇㅇ 심지어 그 때 엄청 커서 헐렁거렸거든",
          "speaker_id": "1",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019069.1.7",
          "form": "발이 많이 자랐나보네",
          "original_form": "발이 많이 자랐나보네",
          "speaker_id": "2",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019069.1.8",
          "form": "1cm는 더 늘려서 실내화 사야할 것 같아 ㅋㅋ",
          "original_form": "1cm는 더 늘려서 실내화 사야할 것 같아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019069.1.9",
          "form": "몇 번 신지도 못했는데 아깝다",
          "original_form": "몇 번 신지도 못했는데 아깝다",
          "speaker_id": "2",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100019069.1.10",
          "form": "동생 주래 ㅎㅎ",
          "original_form": "동생 주래 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100019069.1.11",
          "form": "누가? 큰아들이? ㅋㅋ",
          "original_form": "누가? 큰아들이? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100019069.1.12",
          "form": "ㅇㅇ 작아져서 아깝다니까 동생주라고 ㅎㅎ",
          "original_form": "ㅇㅇ 작아져서 아깝다니까 동생주라고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100019069.1.13",
          "form": "실내화 빨리 알아봐야겠네",
          "original_form": "실내화 빨리 알아봐야겠네",
          "speaker_id": "2",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100019069.1.14",
          "form": "이번엔 발사이즈 잰 다음에 사야겠어",
          "original_form": "이번엔 발사이즈 잰 다음에 사야겠어",
          "speaker_id": "1",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100019069.1.15",
          "form": "그랴요~ 아들이 너무 빨리 크네",
