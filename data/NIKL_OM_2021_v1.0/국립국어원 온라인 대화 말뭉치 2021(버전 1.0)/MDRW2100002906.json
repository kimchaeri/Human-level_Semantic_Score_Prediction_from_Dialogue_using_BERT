      "utterance": [
        {
          "id": "MDRW2100002906.1.1",
          "form": "그니까... 어떻게 피드백을 저렇게 줄 수가 잇지??",
          "original_form": "그니까... 어떻게 피드백을 저렇게 줄 수가 잇지??",
          "speaker_id": "2",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002906.1.2",
          "form": "이럴꺼면 너희들이 만들어봐라 쉽나 ㅠㅠ",
          "original_form": "이럴꺼면 너희들이 만들어봐라 쉽나 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002906.1.3",
          "form": "어이없을무...",
          "original_form": "어이없을무...",
          "speaker_id": "2",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002906.1.4",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002906.1.5",
          "form": "너무 어이없다",
          "original_form": "너무 어이없다",
          "speaker_id": "1",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002906.1.6",
          "form": "징짜.말만하면 다 되는 쥴 알아..",
          "original_form": "징짜.말만하면 다 되는 쥴 알아..",
          "speaker_id": "2",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002906.1.7",
          "form": "야근을 얼마나 해야하는건지",
          "original_form": "야근을 얼마나 해야하는건지",
          "speaker_id": "1",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002906.1.8",
          "form": "재택이라 더 힘들게 일해도 티도안나는데",
          "original_form": "재택이라 더 힘들게 일해도 티도안나는데",
          "speaker_id": "1",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002906.1.9",
          "form": "그러게.....",
          "original_form": "그러게.....",
          "speaker_id": "2",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002906.1.10",
          "form": "다음주부터 하루에 5개씩..",
          "original_form": "다음주부터 하루에 5개씩..",
          "speaker_id": "1",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002906.1.11",
          "form": "개과천선 시켜야겠네 ㅠ",
          "original_form": "개과천선 시켜야겠네 ㅠ",
          "speaker_id": "1",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002906.1.12",
          "form": "좀 평탄히 가나 햇는데....",
          "original_form": "좀 평탄히 가나 햇는데....",
          "speaker_id": "2",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002906.1.13",
          "form": "와우.. 5개씩....",
          "original_form": "와우.. 5개씩....",
          "speaker_id": "2",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002906.1.14",
          "form": "글게말이여",
          "original_form": "글게말이여",
          "speaker_id": "1",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002906.1.15",
          "form": "하 ..",
          "original_form": "하 ..",
          "speaker_id": "1",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002906.1.16",
          "form": "양도 많고 수정사항도 밑도끝도없으니",
          "original_form": "양도 많고 수정사항도 밑도끝도없으니",
          "speaker_id": "1",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002906.1.17",
          "form": "괴롭ㅋㅋ",
          "original_form": "괴롭ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002906.1.18",
          "form": "게다가 일할시간은 없고...",
          "original_form": "게다가 일할시간은 없고...",
          "speaker_id": "2",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002906.1.19",
          "form": "일정촉박하고..",
          "original_form": "일정촉박하고..",
          "speaker_id": "2",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002906.1.20",
          "form": "그러니까 ㅠ",
          "original_form": "그러니까 ㅠ",
          "speaker_id": "1",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002906.1.21",
          "form": "왜 그러냐고~",
          "original_form": "왜 그러냐고~",
          "speaker_id": "1",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002906.1.22",
