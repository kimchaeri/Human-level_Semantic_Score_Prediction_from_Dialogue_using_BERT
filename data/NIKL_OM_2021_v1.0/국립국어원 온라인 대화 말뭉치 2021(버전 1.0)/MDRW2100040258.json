      "utterance": [
        {
          "id": "MDRW2100040258.1.1",
          "form": "식사하셨어요??",
          "original_form": "식사하셨어요??",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100040258.1.2",
          "form": "대강 아점으로 짜파게티요^^",
          "original_form": "대강 아점으로 짜파게티요^^",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100040258.1.3",
          "form": "ㅎㅎ 저는 불고기에 밥먹었어요 ㅎ",
          "original_form": "ㅎㅎ 저는 불고기에 밥먹었어요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100040258.1.4",
          "form": "저녁엔 지난번 쏜살치킨 먹으려구요",
          "original_form": "저녁엔 지난번 쏜살치킨 먹으려구요",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040258.1.5",
          "form": "쉽게 쉽게 ㅎㅎㅎ",
          "original_form": "쉽게 쉽게 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040258.1.6",
          "form": "쏜살 집에서 해먹어도 맛있어요",
          "original_form": "쏜살 집에서 해먹어도 맛있어요",
          "speaker_id": "2",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100040258.1.7",
          "form": "도서관갔다 스벅들려서 음료먹고있어요",
          "original_form": "도서관갔다 스벅들려서 음료먹고있어요",
          "speaker_id": "2",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100040258.1.8",
          "form": "지금 하고있는데 약간 매콤하네요",
          "original_form": "지금 하고있는데 약간 매콤하네요",
          "speaker_id": "1",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100040258.1.9",
          "form": "애들 줄랬는데 ㅜㅠ",
          "original_form": "애들 줄랬는데 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100040258.1.10",
          "form": "도서관 스벅 코스로 좋네요 ㅎ",
          "original_form": "도서관 스벅 코스로 좋네요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100040258.1.11",
          "form": "안매운것도 살짝 맵긴하더라구요",
          "original_form": "안매운것도 살짝 맵긴하더라구요",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100040258.1.12",
          "form": "네 그래서 지금 고민이네요",
          "original_form": "네 그래서 지금 고민이네요",
          "speaker_id": "1",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100040258.1.13",
          "form": "다시 다른반찬을 해줘야할지 ㅋ",
          "original_form": "다시 다른반찬을 해줘야할지 ㅋ",
          "speaker_id": "1",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100040258.1.14",
          "form": "라면 먹음 먹을텐데...울딸램은 맵다하면서 먹거든요",
          "original_form": "라면 먹음 먹을텐데...울딸램은 맵다하면서 먹거든요",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100040258.1.15",
          "form": "아직 라면은 짜파게티만 ㅋ",
          "original_form": "아직 라면은 짜파게티만 ㅋ",
          "speaker_id": "1",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100040258.1.16",
          "form": "둘째는 껍데기 다 짤라내고 주니 먹네요",
          "original_form": "둘째는 껍데기 다 짤라내고 주니 먹네요",
          "speaker_id": "1",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100040258.1.17",
          "form": "약간 맵다면서 무우랑 같이 잘먹네요 ㅎ",
          "original_form": "약간 맵다면서 무우랑 같이 잘먹네요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100040258.1.18",
          "form": "다행이네요. 전 아직도 2봉지나 남았어요",
          "original_form": "다행이네요. 전 아직도 2봉지나 남았어요",
          "speaker_id": "2",
          "time": "20210905 08:49"
        },
        {
          "id": "MDRW2100040258.1.19",
          "form": "와 많으시네요 ㅎ",
          "original_form": "와 많으시네요 ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:58"
        },
        {
          "id": "MDRW2100040258.1.20",
          "form": "하나씩 언능 냉파를 ㅎ",
          "original_form": "하나씩 언능 냉파를 ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:59"
        },
        {
          "id": "MDRW2100040258.1.21",
          "form": "오늘 냉장고좀 비워야겠어요",
          "original_form": "오늘 냉장고좀 비워야겠어요",
          "speaker_id": "2",
          "time": "20210905 09:19"
        },
        {
          "id": "MDRW2100040258.1.22",
          "form": "지금 누룽지끌여서 김치만해서 대강 아침먹으려구요",
          "original_form": "지금 누룽지끌여서 김치만해서 대강 아침먹으려구요",
          "speaker_id": "2",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100040258.1.23",
          "form": "저도 어제 냉파 좀 했어요 ㅎ",
          "original_form": "저도 어제 냉파 좀 했어요 ㅎ",
          "speaker_id": "1",
          "time": "20210905 10:22"
        },
        {
          "id": "MDRW2100040258.1.24",
          "form": "치킨 비우고 베라도 파인트 쬐메 남은거",
          "original_form": "치킨 비우고 베라도 파인트 쬐메 남은거",
          "speaker_id": "1",
          "time": "20210905 10:22"
        },
        {
          "id": "MDRW2100040258.1.25",
          "form": "처리하고",
          "original_form": "처리하고",
          "speaker_id": "1",
          "time": "20210905 10:22"
        },
        {
          "id": "MDRW2100040258.1.26",
          "form": "불고기도 처리하고 ㅎㅎ",
          "original_form": "불고기도 처리하고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:22"
        },
        {
          "id": "MDRW2100040258.1.27",
          "form": "ㅎ 냉파 많이 하셨네요",
          "original_form": "ㅎ 냉파 많이 하셨네요",
          "speaker_id": "2",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100040258.1.28",
          "form": "막둥이만 누룽지주고 전 미역국먹었어요",
          "original_form": "막둥이만 누룽지주고 전 미역국먹었어요",
          "speaker_id": "2",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100040258.1.29",
          "form": "네 냉파하니 공간이 생기니 마음의 여유도 ㅋ",
          "original_form": "네 냉파하니 공간이 생기니 마음의 여유도 ㅋ",
          "speaker_id": "1",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100040258.1.30",
          "form": "저는 아침에 국종류 처리요 ㅋ",
          "original_form": "저는 아침에 국종류 처리요 ㅋ",
          "speaker_id": "1",
          "time": "20210905 10:24"
        }
      ]
