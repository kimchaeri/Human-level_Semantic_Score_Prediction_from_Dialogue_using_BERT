      "utterance": [
        {
          "id": "MDRW2100029719.1.1",
          "form": "저캡쳐를 많이해서",
          "original_form": "저캡쳐를 많이해서",
          "speaker_id": "2",
          "time": "20210903 12:12"
        },
        {
          "id": "MDRW2100029719.1.2",
          "form": "ㅋㅋㅋㅋ 먹어보지는않앗다는 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ 먹어보지는않앗다는 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 12:12"
        },
        {
          "id": "MDRW2100029719.1.3",
          "form": "요즘 맛집 추천 많이 뜨더라구요",
          "original_form": "요즘 맛집 추천 많이 뜨더라구요",
          "speaker_id": "1",
          "time": "20210903 12:12"
        },
        {
          "id": "MDRW2100029719.1.4",
          "form": "근데 갔던데만가요",
          "original_form": "근데 갔던데만가요",
          "speaker_id": "1",
          "time": "20210903 12:12"
        },
        {
          "id": "MDRW2100029719.1.5",
          "form": "저는다가볼려고하는편",
          "original_form": "저는다가볼려고하는편",
          "speaker_id": "2",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100029719.1.6",
          "form": "미식가시네요 ㅎㅎ",
          "original_form": "미식가시네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100029719.1.7",
          "form": "ㅡ맛있는건 언제나 좋아요",
          "original_form": "ㅡ맛있는건 언제나 좋아요",
          "speaker_id": "2",
          "time": "20210903 13:01"
        },
        {
          "id": "MDRW2100029719.1.8",
          "form": "저는 소고기 초밥 조아요",
          "original_form": "저는 소고기 초밥 조아요",
          "speaker_id": "1",
          "time": "20210903 13:07"
        },
        {
          "id": "MDRW2100029719.1.9",
          "form": "헐 맛있어요",
          "original_form": "헐 맛있어요",
          "speaker_id": "2",
          "time": "20210903 13:07"
        },
        {
          "id": "MDRW2100029719.1.10",
          "form": "손가락 데였어요 ㅠ",
          "original_form": "손가락 데였어요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 13:07"
        },
        {
          "id": "MDRW2100029719.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 13:07"
        },
        {
          "id": "MDRW2100029719.1.12",
          "form": "ㅠㅠ 무슨 짓을 하셨길래",
          "original_form": "ㅠㅠ 무슨 짓을 하셨길래",
          "speaker_id": "1",
          "time": "20210903 13:07"
        },
        {
          "id": "MDRW2100029719.1.13",
          "form": "그냥뜨신물 젓다가요",
          "original_form": "그냥뜨신물 젓다가요",
          "speaker_id": "2",
          "time": "20210903 13:10"
        },
        {
          "id": "MDRW2100029719.1.14",
          "form": "ㅠㅠ 뭐든 조심해야한다는",
          "original_form": "ㅠㅠ 뭐든 조심해야한다는",
          "speaker_id": "1",
          "time": "20210903 13:12"
        },
        {
          "id": "MDRW2100029719.1.15",
          "form": "맞어요ㅏㅈ아요",
          "original_form": "맞어요ㅏㅈ아요",
          "speaker_id": "2",
          "time": "20210903 13:23"
        },
        {
          "id": "MDRW2100029719.1.16",
          "form": "던벙쟁이라서",
          "original_form": "던벙쟁이라서",
          "speaker_id": "2",
          "time": "20210903 13:23"
        },
        {
          "id": "MDRW2100029719.1.17",
          "form": "부산에 2층버스 타보셨나요?",
          "original_form": "부산에 2층버스 타보셨나요?",
          "speaker_id": "1",
          "time": "20210903 13:25"
        },
        {
          "id": "MDRW2100029719.1.18",
          "form": "넹넹엄청 오래전에 타봤는데",
          "original_form": "넹넹엄청 오래전에 타봤는데",
          "speaker_id": "2",
          "time": "20210903 14:59"
        },
        {
          "id": "MDRW2100029719.1.19",
          "form": "좀 재미있더라규요",
          "original_form": "좀 재미있더라규요",
          "speaker_id": "2",
          "time": "20210903 14:59"
        },
        {
          "id": "MDRW2100029719.1.20",
