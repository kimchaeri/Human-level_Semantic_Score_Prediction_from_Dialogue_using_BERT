      "utterance": [
        {
          "id": "MDRW2100021327.1.1",
          "form": "ㅇㅇ 방금 주문했어",
          "original_form": "ㅇㅇ 방금 주문했어",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100021327.1.2",
          "form": "몇 분 정도 걸린대?",
          "original_form": "몇 분 정도 걸린대?",
          "speaker_id": "1",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100021327.1.3",
          "form": "40분 걸린다고 써있긴 한데 더 빨리 올듯",
          "original_form": "40분 걸린다고 써있긴 한데 더 빨리 올듯",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021327.1.4",
          "form": "ㅎㅎ 내가 가지러 갈 걸 그랬나",
          "original_form": "ㅎㅎ 내가 가지러 갈 걸 그랬나",
          "speaker_id": "1",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021327.1.5",
          "form": "직접 가지러 가도 가격은 똑같아",
          "original_form": "직접 가지러 가도 가격은 똑같아",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021327.1.6",
          "form": "엥? 배달비가 있는데?",
          "original_form": "엥? 배달비가 있는데?",
          "speaker_id": "1",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021327.1.7",
          "form": "지마켓에서 배달 2천원 쿠폰 주거든~ 스마일페이도 되공 ㅎㅎ",
          "original_form": "지마켓에서 배달 2천원 쿠폰 주거든~ 스마일페이도 되공 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021327.1.8",
          "form": "아.. 이래저래 그냥 배달이 낫겠네",
          "original_form": "아.. 이래저래 그냥 배달이 낫겠네",
          "speaker_id": "1",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100021327.1.9",
          "form": "글치~ 괜히 기운 뺄 필요 없어",
          "original_form": "글치~ 괜히 기운 뺄 필요 없어",
          "speaker_id": "2",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100021327.1.10",
          "form": "ㅎㅎ 그럼 난 기다리기만 하면 되는겨?",
          "original_form": "ㅎㅎ 그럼 난 기다리기만 하면 되는겨?",
          "speaker_id": "1",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100021327.1.11",
          "form": "ㅇㅇ 맛있게 촵촵 먹기만 하면 돼",
          "original_form": "ㅇㅇ 맛있게 촵촵 먹기만 하면 돼",
          "speaker_id": "2",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100021327.1.12",
          "form": "아.. 생각하니 배고프다",
          "original_form": "아.. 생각하니 배고프다",
          "speaker_id": "1",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100021327.1.13",
          "form": "조금만 기다려 ㅎㅎ",
          "original_form": "조금만 기다려 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100021327.1.14",
          "form": "아 배불러 ㅎㅎ 엽기 떡볶이 너무 맛있다",
          "original_form": "아 배불러 ㅎㅎ 엽기 떡볶이 너무 맛있다",
