        {
          "id": "MDRW2100030586.1.1",
          "form": "곰표랑 두꺼비 넘귀엽죠",
          "original_form": "곰표랑 두꺼비 넘귀엽죠",
          "speaker_id": "2",
          "time": "20210904 09:32"
        },
        {
          "id": "MDRW2100030586.1.2",
          "form": "옛날느낌도나고 귀엽더라구요",
          "original_form": "옛날느낌도나고 귀엽더라구요",
          "speaker_id": "1",
          "time": "20210904 09:33"
        },
        {
          "id": "MDRW2100030586.1.3",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210904 09:33"
        },
        {
          "id": "MDRW2100030586.1.4",
          "form": "전 곰표 맥주요",
          "original_form": "전 곰표 맥주요",
          "speaker_id": "1",
          "time": "20210904 09:33"
        },
        {
          "id": "MDRW2100030586.1.5",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "2",
          "time": "20210904 09:33"
        },
        {
          "id": "MDRW2100030586.1.6",
          "form": "술맛을 몰라서 맛은 잘모르겠고",
          "original_form": "술맛을 몰라서 맛은 잘모르겠고",
          "speaker_id": "1",
          "time": "20210904 09:34"
        },
        {
          "id": "MDRW2100030586.1.7",
          "form": "아..그렇군요",
          "original_form": "아..그렇군요",
          "speaker_id": "2",
          "time": "20210904 09:34"
        },
        {
          "id": "MDRW2100030586.1.8",
          "form": "전 병따개선물받았는데 귀엽더군요",
          "original_form": "전 병따개선물받았는데 귀엽더군요",
          "speaker_id": "2",
          "time": "20210904 09:34"
        },
        {
          "id": "MDRW2100030586.1.9",
          "form": "무슨병따개요?",
          "original_form": "무슨병따개요?",
          "speaker_id": "1",
          "time": "20210904 09:34"
        },
        {
          "id": "MDRW2100030586.1.10",
          "form": "진로세서 주는",
          "original_form": "진로세서 주는",
          "speaker_id": "2",
          "time": "20210904 09:34"
        },
        {
          "id": "MDRW2100030586.1.11",
          "form": "그런것도 있군요",
          "original_form": "그런것도 있군요",
          "speaker_id": "1",
          "time": "20210904 09:35"
        },
        {
          "id": "MDRW2100030586.1.12",
          "form": "네..아는분이 주셨어요",
          "original_form": "네..아는분이 주셨어요",
          "speaker_id": "2",
          "time": "20210904 09:35"
        },
        {
          "id": "MDRW2100030586.1.13",
          "form": "ㅈ두꺼비 넘 귀여워요",
          "original_form": "ㅈ두꺼비 넘 귀여워요",
          "speaker_id": "1",
          "time": "20210904 09:35"
        },
        {
          "id": "MDRW2100030586.1.14",
          "form": "저희집엔 옛날소주잔에 두꺼비 그려진게있더군요",
          "original_form": "저희집엔 옛날소주잔에 두꺼비 그려진게있더군요",
