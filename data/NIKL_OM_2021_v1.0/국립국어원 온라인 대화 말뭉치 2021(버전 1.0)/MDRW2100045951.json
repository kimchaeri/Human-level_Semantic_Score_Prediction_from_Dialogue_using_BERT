      "utterance": [
        {
          "id": "MDRW2100045951.1.1",
          "form": "안녕하세요 아침드세요?",
          "original_form": "안녕하세요 아침드세요?",
          "speaker_id": "1",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100045951.1.2",
          "form": "저 어제 먹다 남은 치킨 조금 먹고 왔어요 ㅎㅎ",
          "original_form": "저 어제 먹다 남은 치킨 조금 먹고 왔어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100045951.1.3",
          "form": "뭐드셨나요??",
          "original_form": "뭐드셨나요??",
          "speaker_id": "2",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100045951.1.4",
          "form": "식은치킨도맛있죠~~",
          "original_form": "식은치킨도맛있죠~~",
          "speaker_id": "1",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100045951.1.5",
          "form": "아뇨 전 아침은 안먹어요ㅠ",
          "original_form": "아뇨 전 아침은 안먹어요ㅠ",
          "speaker_id": "1",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100045951.1.6",
          "form": "아하 그런분들 많죠ㅠㅜ 배 안고프세요??",
          "original_form": "아하 그런분들 많죠ㅠㅜ 배 안고프세요??",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100045951.1.7",
          "form": "아침엔 고프단느낌은 안나는거같아요!",
          "original_form": "아침엔 고프단느낌은 안나는거같아요!",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045951.1.8",
          "form": "전 커피러버라 커피가 먼저 땡겨요ㅠㅠ",
          "original_form": "전 커피러버라 커피가 먼저 땡겨요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045951.1.9",
          "form": "아 빈속에 커피 드세요??",
          "original_form": "아 빈속에 커피 드세요??",
          "speaker_id": "2",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045951.1.10",
          "form": "저는 배부르게 먹어야 커피가 그나마 조금 들어가요ㅜㅜ",
          "original_form": "저는 배부르게 먹어야 커피가 그나마 조금 들어가요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045951.1.11",
          "form": "원래 안그랬는데 카페인이 잘 안받네요ㅠㅠ",
          "original_form": "원래 안그랬는데 카페인이 잘 안받네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045951.1.12",
          "form": "안좋운데",
          "original_form": "안좋운데",
          "speaker_id": "1",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045951.1.13",
          "form": "매일 그러고잇네요ㅠㅠ",
          "original_form": "매일 그러고잇네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045951.1.14",
          "form": "그러시구나 ㅜㅜ",
          "original_form": "그러시구나 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045951.1.15",
          "form": "드라마에서 보면 그런 케이스 꽤 많이나오는데 볼때마다 헉해요 속 괜찮나...",
          "original_form": "드라마에서 보면 그런 케이스 꽤 많이나오는데 볼때마다 헉해요 속 괜찮나...",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045951.1.16",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045951.1.17",
          "form": "근데 제 주변에도 꽤 있어요 ㅋㅋㅋ",
          "original_form": "근데 제 주변에도 꽤 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045951.1.18",
          "form": "전 아메 파에요~",
          "original_form": "전 아메 파에요~",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045951.1.19",
          "form": "맞아요 아메 ㅋㅋㅋ",
          "original_form": "맞아요 아메 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045951.1.20",
          "form": "눈뜨고도 ㅋㅋㅋ",
          "original_form": "눈뜨고도 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045951.1.21",
          "form": "밥먹고도 항상 땡지요ㅠ",
          "original_form": "밥먹고도 항상 땡지요ㅠ",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045951.1.22",
          "form": "땡겨요ㅠ",
          "original_form": "땡겨요ㅠ",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045951.1.23",
          "form": "그냥 커피를 엄청 좋아하시는구나...ㅁㅁ",
          "original_form": "그냥 커피를 엄청 좋아하시는구나...ㅁㅁ",
          "speaker_id": "2",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045951.1.24",
          "form": "저는 단 디저트 먹을때 좀 땡겨요",
          "original_form": "저는 단 디저트 먹을때 좀 땡겨요",
          "speaker_id": "2",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045951.1.25",
          "form": "네ㅠㅠ",
          "original_form": "네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045951.1.26",
          "form": "근데 그것도 마음대로 못먹어요 ㅋㅋㅋ 심장이 빨리뛰고 손 떨리거든요ㅠㅠㅠ",
          "original_form": "근데 그것도 마음대로 못먹어요 ㅋㅋㅋ 심장이 빨리뛰고 손 떨리거든요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045951.1.27",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "1",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045951.1.28",
          "form": "언제부터 이런 카페인 개복치가 된건지",
