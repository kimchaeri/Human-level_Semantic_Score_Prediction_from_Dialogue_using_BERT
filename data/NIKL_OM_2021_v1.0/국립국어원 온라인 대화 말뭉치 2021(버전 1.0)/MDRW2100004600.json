      "utterance": [
        {
          "id": "MDRW2100004600.1.1",
          "form": "여행 가고 싶다",
          "original_form": "여행 가고 싶다",
          "speaker_id": "2",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004600.1.2",
          "form": "여행자금 마련도 다 했는데",
          "original_form": "여행자금 마련도 다 했는데",
          "speaker_id": "2",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004600.1.3",
          "form": "가질 못해",
          "original_form": "가질 못해",
          "speaker_id": "2",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004600.1.4",
          "form": "나도 여행 가고싶어",
          "original_form": "나도 여행 가고싶어",
          "speaker_id": "1",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004600.1.5",
          "form": "우리 모임도 계만들어서 코로나 종식기원 여행자금모으기 해볼래??",
          "original_form": "우리 모임도 계만들어서 코로나 종식기원 여행자금모으기 해볼래??",
          "speaker_id": "1",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004600.1.6",
          "form": "아주 좋은 생각이야",
          "original_form": "아주 좋은 생각이야",
          "speaker_id": "2",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004600.1.7",
          "form": "카카오뱅크 저금통으로 돈모으기",
          "original_form": "카카오뱅크 저금통으로 돈모으기",
          "speaker_id": "1",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004600.1.8",
          "form": "1년 안엔 절대 안 끝날테니까",
          "original_form": "1년 안엔 절대 안 끝날테니까",
          "speaker_id": "2",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004600.1.9",
          "form": "저금통이 아닌가 그 어쨌든 여러명이서 돈모으는 기능있던데",
          "original_form": "저금통이 아닌가 그 어쨌든 여러명이서 돈모으는 기능있던데",
          "speaker_id": "1",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004600.1.10",
          "form": "모임통장!",
          "original_form": "모임통장!",
          "speaker_id": "2",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004600.1.11",
          "form": "한달에 얼마나 모아야 하려나",
          "original_form": "한달에 얼마나 모아야 하려나",
          "speaker_id": "2",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004600.1.12",
          "form": "ㅇㅇㅇ 맞아 그걸로 한달에 2만원씩만해도 달에8만원 5개월만모으면 40만원이다",
          "original_form": "ㅇㅇㅇ 맞아 그걸로 한달에 2만원씩만해도 달에8만원 5개월만모으면 40만원이다",
          "speaker_id": "1",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004600.1.13",
          "form": "숙박비 뚝딱",
          "original_form": "숙박비 뚝딱",
          "speaker_id": "1",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004600.1.14",
          "form": "근데 우리가 겨울에 갈것도 아닐것 같고",
          "original_form": "근데 우리가 겨울에 갈것도 아닐것 같고",
          "speaker_id": "1",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004600.1.15",
          "form": "최소 8개월은 모을것 같은데",
          "original_form": "최소 8개월은 모을것 같은데",
          "speaker_id": "1",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004600.1.16",
          "form": "적당히 쌀쌀할 때 가는거면",
          "original_form": "적당히 쌀쌀할 때 가는거면",
          "speaker_id": "2",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004600.1.17",
          "form": "어떨까",
          "original_form": "어떨까",
          "speaker_id": "2",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004600.1.18",
          "form": "34월??",
          "original_form": "34월??",
          "speaker_id": "1",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004600.1.19",
          "form": "코로나가 언제 종식되느냐에 따라 달려있긴한데",
          "original_form": "코로나가 언제 종식되느냐에 따라 달려있긴한데",
          "speaker_id": "1",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004600.1.20",
          "form": "당장 내년 34월은 무리려나??",
          "original_form": "당장 내년 34월은 무리려나??",
          "speaker_id": "1",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004600.1.21",
          "form": "3월 좋다",
          "original_form": "3월 좋다",
          "speaker_id": "2",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004600.1.22",
          "form": "모두가... 학교에서 벗어난 첫 3월",
          "original_form": "모두가... 학교에서 벗어난 첫 3월",
          "speaker_id": "2",
          "time": "20210821 17:31"
        },
        {
          "id": "MDRW2100004600.1.23",
          "form": "당장 오늘 만들어서 8월분부터 모은다치면 7개월,8개월치 모을수 있네",
          "original_form": "당장 오늘 만들어서 8월분부터 모은다치면 7개월,8개월치 모을수 있네",
          "speaker_id": "1",
          "time": "20210821 17:31"
        },
        {
          "id": "MDRW2100004600.1.24",
          "form": "숙소 아예 빌려가는 것도 나쁘지 않을듯!",
          "original_form": "숙소 아예 빌려가는 것도 나쁘지 않을듯!",
          "speaker_id": "2",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004600.1.25",
          "form": "펜션 이런 곳.... 시끄러워도 괜찮은!",
          "original_form": "펜션 이런 곳.... 시끄러워도 괜찮은!",
          "speaker_id": "2",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004600.1.26",
          "form": "와 그것도 좋지~~~ 일단 다른애등한테도 말해볼까",
          "original_form": "와 그것도 좋지~~~ 일단 다른애등한테도 말해볼까",
          "speaker_id": "1",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004600.1.27",
          "form": "아 팬션하니깐 생각난건데",
          "original_form": "아 팬션하니깐 생각난건데",
          "speaker_id": "1",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004600.1.28",
          "form": "팬션으로 가면 우리언니통해서 예약할수 있는게 좋을듯",
          "original_form": "팬션으로 가면 우리언니통해서 예약할수 있는게 좋을듯",
          "speaker_id": "1",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004600.1.29",
          "form": "언니회사랑 제휴되있는 팬션은 우선예약가능하데",
          "original_form": "언니회사랑 제휴되있는 팬션은 우선예약가능하데",
          "speaker_id": "1",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004600.1.30",
          "form": "꺄 너무좋아",
          "original_form": "꺄 너무좋아",
          "speaker_id": "2",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004600.1.31",
          "form": "일단 여행계획이 확실히 잡히면 이건 그때가서 다시 생각해보는걸루",
          "original_form": "일단 여행계획이 확실히 잡히면 이건 그때가서 다시 생각해보는걸루",
          "speaker_id": "1",
          "time": "20210821 17:34"
        }
      ]
    }
