      "utterance": [
        {
          "id": "MDRW2100028031.1.1",
          "form": "저녁 뭐드실거에요?",
          "original_form": "저녁 뭐드실거에요?",
          "speaker_id": "1",
          "time": "20210902 17:04"
        },
        {
          "id": "MDRW2100028031.1.2",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "2",
          "time": "20210902 17:15"
        },
        {
          "id": "MDRW2100028031.1.3",
          "form": "어디 치킨 좋아하세요?",
          "original_form": "어디 치킨 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 17:16"
        },
        {
          "id": "MDRW2100028031.1.4",
          "form": "네네여",
          "original_form": "네네여",
          "speaker_id": "2",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028031.1.5",
          "form": "네네 어떤치킨 드실거에요?",
          "original_form": "네네 어떤치킨 드실거에요?",
          "speaker_id": "1",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028031.1.6",
          "form": "후라이드요",
          "original_form": "후라이드요",
          "speaker_id": "2",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028031.1.7",
          "form": "오 저는 굽네좋아해요",
          "original_form": "오 저는 굽네좋아해요",
          "speaker_id": "1",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028031.1.8",
          "form": "굽네드셔보셨나요",
          "original_form": "굽네드셔보셨나요",
          "speaker_id": "1",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028031.1.9",
          "form": "것도 좋죠",
          "original_form": "것도 좋죠",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100028031.1.10",
          "form": "굽네에선 어떤 메뉴 좋아하세요",
          "original_form": "굽네에선 어떤 메뉴 좋아하세요",
          "speaker_id": "1",
          "time": "20210902 17:29"
        },
        {
          "id": "MDRW2100028031.1.11",
          "form": "최애 메뉴는요 아직",
          "original_form": "최애 메뉴는요 아직",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028031.1.12",
          "form": "네네에사 파닭은.안좋아하시나요?",
          "original_form": "네네에사 파닭은.안좋아하시나요?",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100028031.1.13",
          "form": "후라이드가 젤 좋아요",
          "original_form": "후라이드가 젤 좋아요",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028031.1.14",
          "form": "축구보면서 드실건가요?",
          "original_form": "축구보면서 드실건가요?",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028031.1.15",
          "form": "오늘 하나요?",
          "original_form": "오늘 하나요?",
          "speaker_id": "2",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100028031.1.16",
          "form": "네.오늘 축구한대요",
          "original_form": "네.오늘 축구한대요",
          "speaker_id": "1",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100028031.1.17",
          "form": "그래서 저도 치킨시키려구요",
          "original_form": "그래서 저도 치킨시키려구요",
