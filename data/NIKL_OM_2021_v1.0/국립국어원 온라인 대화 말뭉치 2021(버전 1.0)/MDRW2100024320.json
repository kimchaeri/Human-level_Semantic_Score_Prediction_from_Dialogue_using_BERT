      "utterance": [
        {
          "id": "MDRW2100024320.1.1",
          "form": "스타벅스 좋지요?",
          "original_form": "스타벅스 좋지요?",
          "speaker_id": "1",
          "time": "20210831 00:45"
        },
        {
          "id": "MDRW2100024320.1.2",
          "form": "많이요",
          "original_form": "많이요",
          "speaker_id": "2",
          "time": "20210831 00:45"
        },
        {
          "id": "MDRW2100024320.1.3",
          "form": "아이스아메리카노",
          "original_form": "아이스아메리카노",
          "speaker_id": "2",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100024320.1.4",
          "form": "스벅 좋은이유중 하나는 드라이브투루지요",
          "original_form": "스벅 좋은이유중 하나는 드라이브투루지요",
          "speaker_id": "1",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100024320.1.5",
          "form": "정말 유용해요~~^^",
          "original_form": "정말 유용해요~~^^",
          "speaker_id": "1",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100024320.1.6",
          "form": "이제 뜨아의계절이 왔어요",
          "original_form": "이제 뜨아의계절이 왔어요",
          "speaker_id": "2",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100024320.1.7",
          "form": "저 스벅 자주가서ㅋㅋ별쿠폰있어여",
          "original_form": "저 스벅 자주가서ㅋㅋ별쿠폰있어여",
          "speaker_id": "1",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100024320.1.8",
          "form": "와 정말 신여성이군요",
          "original_form": "와 정말 신여성이군요",
          "speaker_id": "2",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100024320.1.9",
          "form": "벌써 뜨아의 계절인가요?ㅋㅋㄱ",
          "original_form": "벌써 뜨아의 계절인가요?ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100024320.1.10",
          "form": "뜨아를식혀먹으면 맛있어요",
          "original_form": "뜨아를식혀먹으면 맛있어요",
          "speaker_id": "2",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100024320.1.11",
          "form": "별쿠폰으로는ㅋㅋㅋ프라푸치노한잔해야시요ㅋㅋㅋ",
          "original_form": "별쿠폰으로는ㅋㅋㅋ프라푸치노한잔해야시요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100024320.1.12",
          "form": "그래야 오래먹거든요",
          "original_form": "그래야 오래먹거든요",
          "speaker_id": "2",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100024320.1.13",
          "form": "프라프치노에 액상과당이 얼마나들어있는줄알면",
          "original_form": "프라프치노에 액상과당이 얼마나들어있는줄알면",
          "speaker_id": "2",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100024320.1.14",
          "form": "못마실꺼같아요",
          "original_form": "못마실꺼같아요",
          "speaker_id": "2",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100024320.1.15",
          "form": "뜨아 식기 전에 다 드실듯",
          "original_form": "뜨아 식기 전에 다 드실듯",
          "speaker_id": "1",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100024320.1.16",
          "form": "액상과당ㅠㅠㅠㅇ",
          "original_form": "액상과당ㅠㅠㅠㅇ",
          "speaker_id": "1",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100024320.1.17",
          "form": "다이어트의 적이래요",
          "original_form": "다이어트의 적이래요",
          "speaker_id": "2",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100024320.1.18",
          "form": "요즘엔그래서 프라프치노용 라이트시럽이 있더라구요ㅋㅋㅋ",
          "original_form": "요즘엔그래서 프라프치노용 라이트시럽이 있더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:48"
        },
        {
          "id": "MDRW2100024320.1.19",
          "form": "세상 참  좋아졌네요",
          "original_form": "세상 참  좋아졌네요",
          "speaker_id": "2",
          "time": "20210831 00:48"
        },
        {
          "id": "MDRW2100024320.1.20",
          "form": "최소한의 양심같은것이죠ㅋㅋㅋ휘핑크림먹어야하니까여",
          "original_form": "최소한의 양심같은것이죠ㅋㅋㅋ휘핑크림먹어야하니까여",
          "speaker_id": "1",
          "time": "20210831 00:48"
        },
        {
          "id": "MDRW2100024320.1.21",
          "form": "저지방우유는필수죠",
          "original_form": "저지방우유는필수죠",
          "speaker_id": "2",
          "time": "20210831 00:48"
        },
        {
          "id": "MDRW2100024320.1.22",
          "form": "종근당 단백질쉐이크",
          "original_form": "종근당 단백질쉐이크",
          "speaker_id": "2",
          "time": "20210831 00:48"
        },
        {
          "id": "MDRW2100024320.1.23",
          "form": "저번에 저지방우유 라이트시럽 휘핑먹었거든요?",
          "original_form": "저번에 저지방우유 라이트시럽 휘핑먹었거든요?",
          "speaker_id": "1",
          "time": "20210831 00:48"
        },
        {
          "id": "MDRW2100024320.1.24",
          "form": "너무맛있어요 미숫가루맛",
          "original_form": "너무맛있어요 미숫가루맛",
          "speaker_id": "2",
          "time": "20210831 00:48"
        },
        {
          "id": "MDRW2100024320.1.25",
          "form": "먹으면서도 이게 뭔가...현타왔어여",
          "original_form": "먹으면서도 이게 뭔가...현타왔어여",
          "speaker_id": "1",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024320.1.26",
          "form": "단백질쉐이크 좋네요ㅋㅋ",
          "original_form": "단백질쉐이크 좋네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024320.1.27",
          "form": "휘핑은죄가없어요",
          "original_form": "휘핑은죄가없어요",
          "speaker_id": "2",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024320.1.28",
          "form": "당함류 낮나요?",
          "original_form": "당함류 낮나요?",
          "speaker_id": "1",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024320.1.29",
          "form": "높아요",
          "original_form": "높아요",
          "speaker_id": "2",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024320.1.30",
          "form": "미국꺼에비해ㄷㄷㄷ",
          "original_form": "미국꺼에비해ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024320.1.31",
          "form": "맛없을수가 없네요ㅋㅋㅋ",
          "original_form": "맛없을수가 없네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024320.1.32",
          "form": "에리스리톨 비싸서못넣나봐요",
