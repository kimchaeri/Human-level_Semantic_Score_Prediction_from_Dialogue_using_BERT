      "utterance": [
        {
          "id": "MDRW2100019581.1.1",
          "form": "여행좋아하시나요?",
          "original_form": "여행좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019581.1.2",
          "form": "네! 좋아해요",
          "original_form": "네! 좋아해요",
          "speaker_id": "2",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100019581.1.3",
          "form": "최근에 여행어디로 가셨나요?",
          "original_form": "최근에 여행어디로 가셨나요?",
          "speaker_id": "1",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100019581.1.4",
          "form": "한 세달전쯤에 제주도 다녀왔어요:)",
          "original_form": "한 세달전쯤에 제주도 다녀왔어요:)",
          "speaker_id": "2",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100019581.1.5",
          "form": "제주도는 어디가 좋으셨어요?",
          "original_form": "제주도는 어디가 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100019581.1.6",
          "form": "아무래도 역시 바다가 좋죠ㅋㅋㅋ",
          "original_form": "아무래도 역시 바다가 좋죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019581.1.7",
          "form": "제주도 가보셨나요?",
          "original_form": "제주도 가보셨나요?",
          "speaker_id": "2",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019581.1.8",
          "form": "수학여행때 가보고 안가봤어요",
          "original_form": "수학여행때 가보고 안가봤어요",
          "speaker_id": "1",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019581.1.9",
          "form": "한 10년되었네요",
          "original_form": "한 10년되었네요",
          "speaker_id": "1",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019581.1.10",
          "form": "아앗 저두요!!ㅋㅋㅋ 그러다가 이번에 두번째로 가본거에요ㅋㅋㅋ",
          "original_form": "아앗 저두요!!ㅋㅋㅋ 그러다가 이번에 두번째로 가본거에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019581.1.11",
          "form": "많이 변했던가요?ㅋㅋㅋㅋㅋ",
          "original_form": "많이 변했던가요?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019581.1.12",
          "form": "렌트하셨어요?",
          "original_form": "렌트하셨어요?",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019581.1.13",
          "form": "사실 수학여행때 기억에 남는거라곤 소인국 테마파크랑 흑돼지밖에 없어서....",
          "original_form": "사실 수학여행때 기억에 남는거라곤 소인국 테마파크랑 흑돼지밖에 없어서....",
          "speaker_id": "2",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019581.1.14",
          "form": "바뀐건지 어떤지도 잘 모르겠어요ㅋㅋㅋ",
          "original_form": "바뀐건지 어떤지도 잘 모르겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019581.1.15",
          "form": "아니요ㅠㅠ 면허는 있는데 운전을 못해서 뚜벅이로 다녔어요",
          "original_form": "아니요ㅠㅠ 면허는 있는데 운전을 못해서 뚜벅이로 다녔어요",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019581.1.16",
          "form": "헉 고생하셨겠다",
          "original_form": "헉 고생하셨겠다",
          "speaker_id": "1",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100019581.1.17",
          "form": "렌트안해도 갈만한가요?",
          "original_form": "렌트안해도 갈만한가요?",
          "speaker_id": "1",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100019581.1.18",
          "form": "나쁘진 않은데 렌트하는게 역시 좋을거같아요ㅋㅋㅋ",
          "original_form": "나쁘진 않은데 렌트하는게 역시 좋을거같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019581.1.19",
          "form": "운전하는친구를 꼬셔야겠어요",
          "original_form": "운전하는친구를 꼬셔야겠어요",
          "speaker_id": "1",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019581.1.20",
          "form": "ㅋㅋㅋ면허 없으세요??",
          "original_form": "ㅋㅋㅋ면허 없으세요??",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019581.1.21",
          "form": "면허는 있는데 장롱면허라...",
          "original_form": "면허는 있는데 장롱면허라...",
          "speaker_id": "1",
