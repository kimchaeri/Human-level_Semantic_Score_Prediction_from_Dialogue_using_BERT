      "utterance": [
        {
          "id": "MDRW2100016199.1.1",
          "form": "유튭 좋아하시나요",
          "original_form": "유튭 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100016199.1.2",
          "form": "요새는 시간이 없어서 못보네요",
          "original_form": "요새는 시간이 없어서 못보네요",
          "speaker_id": "2",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100016199.1.3",
          "form": "넷플도 보시나요?",
          "original_form": "넷플도 보시나요?",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100016199.1.4",
          "form": "아도 안봐요 애키우느라 시간이없네요 보시는거있으세요?",
          "original_form": "아도 안봐요 애키우느라 시간이없네요 보시는거있으세요?",
          "speaker_id": "2",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016199.1.5",
          "form": "네 킹덤이요!",
          "original_form": "네 킹덤이요!",
          "speaker_id": "1",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016199.1.6",
          "form": "티비도 잘 못보시나요",
          "original_form": "티비도 잘 못보시나요",
          "speaker_id": "1",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016199.1.7",
          "form": "티비는 예능으로 봐요",
          "original_form": "티비는 예능으로 봐요",
          "speaker_id": "2",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016199.1.8",
          "form": "순풍산부인과 재방 아침마다 꼭보네요",
          "original_form": "순풍산부인과 재방 아침마다 꼭보네요",
          "speaker_id": "2",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016199.1.9",
          "form": "와 진짜추억",
          "original_form": "와 진짜추억",
          "speaker_id": "1",
          "time": "20210826 11:53"
        },
        {
          "id": "MDRW2100016199.1.10",
          "form": "안유치하고 재밌더라고요",
          "original_form": "안유치하고 재밌더라고요",
          "speaker_id": "2",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100016199.1.11",
          "form": "요즘도 재방을 하는군요",
          "original_form": "요즘도 재방을 하는군요",
          "speaker_id": "1",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100016199.1.12",
          "form": "야인시대 하듯이 하더라고요",
          "original_form": "야인시대 하듯이 하더라고요",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016199.1.13",
          "form": "와 야인시대 대박이네요",
          "original_form": "와 야인시대 대박이네요",
          "speaker_id": "1",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016199.1.14",
          "form": "ㅎㅎ 요새 다시 돌고 돌아요 유튜브에서도",
          "original_form": "ㅎㅎ 요새 다시 돌고 돌아요 유튜브에서도",
          "speaker_id": "2",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100016199.1.15",
          "form": "어렸을때 진짜좋아했는데",
          "original_form": "어렸을때 진짜좋아했는데",
          "speaker_id": "1",
