      "utterance": [
        {
          "id": "MDRW2100047152.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100047152.1.2",
          "form": "음 보통이요",
          "original_form": "음 보통이요",
          "speaker_id": "2",
          "time": "20210829 17:19"
        },
        {
          "id": "MDRW2100047152.1.3",
          "form": "자주가진 않아요",
          "original_form": "자주가진 않아요",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100047152.1.4",
          "form": "바다 좋아요",
          "original_form": "바다 좋아요",
          "speaker_id": "1",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100047152.1.5",
          "form": "오 바다 좋지요",
          "original_form": "오 바다 좋지요",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100047152.1.6",
          "form": "바다 자주가세요?",
          "original_form": "바다 자주가세요?",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100047152.1.7",
          "form": "가끔이요",
          "original_form": "가끔이요",
          "speaker_id": "1",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100047152.1.8",
          "form": "바다 어디 가보셨어요",
          "original_form": "바다 어디 가보셨어요",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100047152.1.9",
          "form": "남해요",
          "original_form": "남해요",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100047152.1.10",
          "form": "오 남해면 부산 그쪽이요?",
          "original_form": "오 남해면 부산 그쪽이요?",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100047152.1.11",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100047152.1.12",
          "form": "오 좋았겠네요",
          "original_form": "오 좋았겠네요",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100047152.1.13",
          "form": "저도 바다 별로 안가봣는데",
          "original_form": "저도 바다 별로 안가봣는데",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100047152.1.14",
          "form": "부산은 가봤어요",
          "original_form": "부산은 가봤어요",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100047152.1.15",
          "form": "맘이 편안해집니다",
          "original_form": "맘이 편안해집니다",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100047152.1.16",
          "form": "맞아요 안들어가도 좋더라고요",
          "original_form": "맞아요 안들어가도 좋더라고요",
          "speaker_id": "2",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100047152.1.17",
          "form": "보는것만 해더",
          "original_form": "보는것만 해더",
          "speaker_id": "1",
          "time": "20210829 17:28"
        }
