      "utterance": [
        {
          "id": "MDRW2100005607.1.1",
          "form": "name3이 아까 고집 부리는거 봤제?",
          "original_form": "&name3&이 아까 고집 부리는거 봤제?",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005607.1.2",
          "form": "이제 눈 똑바로 뜨고 대든다",
          "original_form": "이제 눈 똑바로 뜨고 대든다",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005607.1.3",
          "form": "응 그렇더라",
          "original_form": "응 그렇더라",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005607.1.4",
          "form": "벌써저래가 사춘기오믄 우얄라나 몰라",
          "original_form": "벌써저래가 사춘기오믄 우얄라나 몰라",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005607.1.5",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005607.1.6",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005607.1.7",
          "form": "우리집도",
          "original_form": "우리집도",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005607.1.8",
          "form": "마찬가지지",
          "original_form": "마찬가지지",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005607.1.9",
          "form": "특히 폰 때문에 더 그렇더라",
          "original_form": "특히 폰 때문에 더 그렇더라",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005607.1.10",
          "form": "사춘기 오면 난리날듯",
          "original_form": "사춘기 오면 난리날듯",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005607.1.11",
          "form": "그래 그놈의 폰",
          "original_form": "그래 그놈의 폰",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005607.1.12",
          "form": "자기전에도 닌텐도 하다가 한판 혼나고 잠",
          "original_form": "자기전에도 닌텐도 하다가 한판 혼나고 잠",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005607.1.13",
          "form": "실컷 폰게임하고 유튜브보다가",
          "original_form": "실컷 폰게임하고 유튜브보다가",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005607.1.14",
          "form": "다 씻고 잘라니까 닌텐도 꺼내서",
          "original_form": "다 씻고 잘라니까 닌텐도 꺼내서",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005607.1.15",
          "form": "name4이 책 30분 읽을동안 계속 닌텐도 하다가 \r\n자자니까 책읽어달라고....아놔....",
          "original_form": "&name4&이 책 30분 읽을동안 계속 닌텐도 하다가 \r\n자자니까 책읽어달라고....아놔....",
          "speaker_id": "2",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005607.1.16",
          "form": "아 그랬구나ㅜㅠ",
          "original_form": "아 그랬구나ㅜㅠ",
          "speaker_id": "1",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005607.1.17",
          "form": "아고ㅋㅋㅋㅋ오늘 하루가 길었네ㅋㅋ",
          "original_form": "아고ㅋㅋㅋㅋ오늘 하루가 길었네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005607.1.18",
          "form": "우리 첫째도",
          "original_form": "우리 첫째도",
          "speaker_id": "1",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005607.1.19",
          "form": "자기전까지 게임하다가",
          "original_form": "자기전까지 게임하다가",
          "speaker_id": "1",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005607.1.20",
          "form": "자는데",
          "original_form": "자는데",
          "speaker_id": "1",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005607.1.21",
          "form": "참 쉽지않네",
          "original_form": "참 쉽지않네",
          "speaker_id": "1",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005607.1.22",
          "form": "글타 게임...",
          "original_form": "글타 게임...",
          "speaker_id": "2",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005607.1.23",
          "form": "벌써 이래가",
          "original_form": "벌써 이래가",
          "speaker_id": "2",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005607.1.24",
          "form": "얼마나 더 싸울라나 몰라",
          "original_form": "얼마나 더 싸울라나 몰라",
          "speaker_id": "2",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005607.1.25",
          "form": "그렇지. ㅋㅋ",
          "original_form": "그렇지. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005607.1.26",
          "form": "다 자나?? 신랑은 뭐하농",
          "original_form": "다 자나?? 신랑은 뭐하농",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005607.1.27",
          "form": "그래도 name3이 보니깐 큰느낌드는게 좀 반항기보다는 자기생각, 자기 입장을 똑부러지게 얘기하는 느낌도 들더라",
          "original_form": "그래도 &name3&이 보니깐 큰느낌드는게 좀 반항기보다는 자기생각, 자기 입장을 똑부러지게 얘기하는 느낌도 들더라",
          "speaker_id": "1",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005607.1.28",
          "form": "응 우리식구들은 다 잔다.",
          "original_form": "응 우리식구들은 다 잔다.",
          "speaker_id": "1",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005607.1.29",
          "form": "언니네는?",
          "original_form": "언니네는?",
          "speaker_id": "1",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005607.1.30",
          "form": "우리신랑은 친구만나러 잠깐 나갔음",
          "original_form": "우리신랑은 친구만나러 잠깐 나갔음",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005607.1.31",
          "form": "ㅋㅋㄱㅋㅋ바쁘다ㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋ바쁘다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005607.1.32",
          "form": "그래 요새는 한마디도 안지고 본인 생각을 얘기함...",
          "original_form": "그래 요새는 한마디도 안지고 본인 생각을 얘기함...",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005607.1.33",
          "form": "존중해줘야되는데",
          "original_form": "존중해줘야되는데",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005607.1.34",
          "form": "성질이 나네....ㅋㅋㅋㅋㄱ",
          "original_form": "성질이 나네....ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005607.1.35",
          "form": "맞어 존중해줘야되는데",
          "original_form": "맞어 존중해줘야되는데",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005607.1.36",
          "form": "화가나지ㅜㅜㅋㅋ",
          "original_form": "화가나지ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005607.1.37",
          "form": "대드는것같고ㅜㅋ",
          "original_form": "대드는것같고ㅜㅋ",
          "speaker_id": "1",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005607.1.38",
          "form": "그래 ㅋㅋㅋㅋㅋ 화가 참....",
          "original_form": "그래 ㅋㅋㅋㅋㅋ 화가 참....",
