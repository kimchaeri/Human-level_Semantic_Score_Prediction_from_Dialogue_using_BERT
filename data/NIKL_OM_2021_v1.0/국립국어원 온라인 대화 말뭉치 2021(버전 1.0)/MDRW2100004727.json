      "utterance": [
        {
          "id": "MDRW2100004727.1.1",
          "form": "너 결혼을 안할꺼야?",
          "original_form": "너 결혼을 안할꺼야?",
          "speaker_id": "1",
          "time": "20210821 18:12"
        },
        {
          "id": "MDRW2100004727.1.2",
          "form": "응 이 나이에 무슨",
          "original_form": "응 이 나이에 무슨",
          "speaker_id": "2",
          "time": "20210821 18:12"
        },
        {
          "id": "MDRW2100004727.1.3",
          "form": "왜 아직 안늦었다",
          "original_form": "왜 아직 안늦었다",
          "speaker_id": "1",
          "time": "20210821 18:12"
        },
        {
          "id": "MDRW2100004727.1.4",
          "form": "내 한몸 건사하기 힘들다 난",
          "original_form": "내 한몸 건사하기 힘들다 난",
          "speaker_id": "2",
          "time": "20210821 18:12"
        },
        {
          "id": "MDRW2100004727.1.5",
          "form": "체력이 그렇게 약해서 어쩌노",
          "original_form": "체력이 그렇게 약해서 어쩌노",
          "speaker_id": "1",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004727.1.6",
          "form": "체력도 있어야 연애를 하는데 그것도 없다",
          "original_form": "체력도 있어야 연애를 하는데 그것도 없다",
          "speaker_id": "2",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004727.1.7",
          "form": "안타깝네",
          "original_form": "안타깝네",
          "speaker_id": "1",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004727.1.8",
          "form": "혼자라서 편하게 사는거지뭐",
          "original_form": "혼자라서 편하게 사는거지뭐",
          "speaker_id": "2",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004727.1.9",
          "form": "그건 어쩔땐 부럽기도 하다",
          "original_form": "그건 어쩔땐 부럽기도 하다",
          "speaker_id": "1",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004727.1.10",
          "form": "ㅎㅎ 형부랑 싸웠을때 말이지",
          "original_form": "ㅎㅎ 형부랑 싸웠을때 말이지",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004727.1.11",
          "form": "응 거기다가 얘들도 말 안들을때",
          "original_form": "응 거기다가 얘들도 말 안들을때",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004727.1.12",
          "form": "육아전쟁이 고달플듯",
          "original_form": "육아전쟁이 고달플듯",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004727.1.13",
          "form": "젤 힘들다 육아전쟁이다 차라리 일을 하겠다",
          "original_form": "젤 힘들다 육아전쟁이다 차라리 일을 하겠다",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004727.1.14",
          "form": "ㅋㅋ다들 그렇다고 하더라",
          "original_form": "ㅋㅋ다들 그렇다고 하더라",
          "speaker_id": "2",
