      "utterance": [
        {
          "id": "MDRW2100015632.1.1",
          "form": "저도 끊어야 하는데 습관처럼 먹고있어요ㅜㅜ",
          "original_form": "저도 끊어야 하는데 습관처럼 먹고있어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015632.1.2",
          "form": "지금도 복숭아 먹고있어요ㅋㅋ",
          "original_form": "지금도 복숭아 먹고있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015632.1.3",
          "form": "아 맛있겠네요",
          "original_form": "아 맛있겠네요",
          "speaker_id": "1",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015632.1.4",
          "form": "딱딱한 복숭아요?",
          "original_form": "딱딱한 복숭아요?",
          "speaker_id": "1",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015632.1.5",
          "form": "넵ㅎㅎ  전 딱복만 먹어요",
          "original_form": "넵ㅎㅎ  전 딱복만 먹어요",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015632.1.6",
          "form": "혹시 물복 좋아하세요?",
          "original_form": "혹시 물복 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015632.1.7",
          "form": "아 딱딱이 맛나죠 향긋하니",
          "original_form": "아 딱딱이 맛나죠 향긋하니",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015632.1.8",
          "form": "황도도 좋아하긴해요",
          "original_form": "황도도 좋아하긴해요",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015632.1.9",
          "form": "딱복이 씹을때 은은하게 퍼지는 단맛때문에 진짜 맛있는것같아요ㅎㅎ",
          "original_form": "딱복이 씹을때 은은하게 퍼지는 단맛때문에 진짜 맛있는것같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015632.1.10",
          "form": "요즘 유행하는 복숭아 레시피 아세요?",
          "original_form": "요즘 유행하는 복숭아 레시피 아세요?",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015632.1.11",
          "form": "맞아요 강하지않은....\r\n은은한 향긋함요~",
          "original_form": "맞아요 강하지않은....\r\n은은한 향긋함요~",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015632.1.12",
          "form": "뭐에요???",
          "original_form": "뭐에요???",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015632.1.13",
          "form": "복숭아를 파내서 그릭요거트를 넣고 살짝얼린다음 그래놀라랑 꿀을 뿌려서 먹는거에요ㅎㅎ",
          "original_form": "복숭아를 파내서 그릭요거트를 넣고 살짝얼린다음 그래놀라랑 꿀을 뿌려서 먹는거에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015632.1.14",
          "form": "진짜맛있으니깐 해보세용",
          "original_form": "진짜맛있으니깐 해보세용",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015632.1.15",
          "form": "아 맛있나요?상상이 안가네요",
          "original_form": "아 맛있나요?상상이 안가네요",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015632.1.16",
          "form": "해볼께요 ㅎㅎ",
          "original_form": "해볼께요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015632.1.17",
          "form": "카페 디저트 수준인데요",
          "original_form": "카페 디저트 수준인데요",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015632.1.18",
          "form": "돈주고 사먹을 맛이에요ㅎㅎ",
          "original_form": "돈주고 사먹을 맛이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 06:54"
        },
        {
          "id": "MDRW2100015632.1.19",
          "form": "오늘 점심은 뭐드실 예정이세요?",
          "original_form": "오늘 점심은 뭐드실 예정이세요?",
          "speaker_id": "2",
          "time": "20210826 06:54"
        },
        {
          "id": "MDRW2100015632.1.20",
          "form": "점심..그냥 빵먹을까해요",
          "original_form": "점심..그냥 빵먹을까해요",
          "speaker_id": "1",
          "time": "20210826 08:18"
        },
        {
          "id": "MDRW2100015632.1.21",
          "form": "점심까지는 밥을챙기기 힘들어요",
          "original_form": "점심까지는 밥을챙기기 힘들어요",
          "speaker_id": "1",
          "time": "20210826 08:19"
        },
        {
          "id": "MDRW2100015632.1.22",
          "form": "저도 귀찮아서 삼각김밥으로 떼웠어요ㅎㅎ",
          "original_form": "저도 귀찮아서 삼각김밥으로 떼웠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:30"
        },
        {
          "id": "MDRW2100015632.1.23",
          "form": "저녁은 뭐드실거에요?",
          "original_form": "저녁은 뭐드실거에요?",
          "speaker_id": "2",
          "time": "20210826 15:30"
        },
        {
          "id": "MDRW2100015632.1.24",
          "form": "김치볶음밥하려고\r\n카레용 돼지고기 꺼내놨어요",
          "original_form": "김치볶음밥하려고\r\n카레용 돼지고기 꺼내놨어요",
          "speaker_id": "1",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100015632.1.25",
          "form": "우와 볶음밥에 돼지고기도 넣어요?",
          "original_form": "우와 볶음밥에 돼지고기도 넣어요?",
          "speaker_id": "2",
          "time": "20210826 15:33"
        },
        {
          "id": "MDRW2100015632.1.26",
          "form": "저는 햄이나 참치만 넣어봤어요ㅎㅎ",
          "original_form": "저는 햄이나 참치만 넣어봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100015632.1.27",
          "form": "넣어서 해볼려구요..ㅎ 영양가가 나으니",
          "original_form": "넣어서 해볼려구요..ㅎ 영양가가 나으니",
