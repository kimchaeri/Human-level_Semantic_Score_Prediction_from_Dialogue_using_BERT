      "utterance": [
        {
          "id": "MDRW2100004901.1.1",
          "form": "여름 휴가 다녀오셨어요?",
          "original_form": "여름 휴가 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004901.1.2",
          "form": "휴가갔다오긴 했는데 숙박은 안잡고 대부분 당일치기갔다왔어요ㅎ",
          "original_form": "휴가갔다오긴 했는데 숙박은 안잡고 대부분 당일치기갔다왔어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004901.1.3",
          "form": "고흥으로ㅎㅎ",
          "original_form": "고흥으로ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004901.1.4",
          "form": "아 가족여행으로요?",
          "original_form": "아 가족여행으로요?",
          "speaker_id": "1",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004901.1.5",
          "form": "사는데가 전남이라ㅎ 가족들하고가고 딸하고만 둘이가기도 하구요ㅎ",
          "original_form": "사는데가 전남이라ㅎ 가족들하고가고 딸하고만 둘이가기도 하구요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004901.1.6",
          "form": "고흥에 뭐가 유명해요?",
          "original_form": "고흥에 뭐가 유명해요?",
          "speaker_id": "1",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004901.1.7",
          "form": "오 좋은 아빠시네요",
          "original_form": "오 좋은 아빠시네요",
          "speaker_id": "1",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004901.1.8",
          "form": "전 아빠랑 둘이 뭐 해본기억이 없는데",
          "original_form": "전 아빠랑 둘이 뭐 해본기억이 없는데",
          "speaker_id": "1",
          "time": "20210821 19:16"
        },
        {
          "id": "MDRW2100004901.1.9",
          "form": "고흥에는 나로호 우수센터가있어서ㅎㅎㅎㅎ",
          "original_form": "고흥에는 나로호 우수센터가있어서ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004901.1.10",
          "form": "아이가 많이 좋아해요ㅎ 해수욕장도 유명해서",
          "original_form": "아이가 많이 좋아해요ㅎ 해수욕장도 유명해서",
          "speaker_id": "2",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004901.1.11",
          "form": "가족들여행하기좋더라구요ㅎ",
          "original_form": "가족들여행하기좋더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004901.1.12",
          "form": "아 볼거리 많은가요? 우주센터~",
          "original_form": "아 볼거리 많은가요? 우주센터~",
          "speaker_id": "1",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004901.1.13",
          "form": "그리고 횟감이 좋죠ㅎ",
          "original_form": "그리고 횟감이 좋죠ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004901.1.14",
          "form": "솔직히 어른들은 볼만한게없어요ㅎㅎ",
          "original_form": "솔직히 어른들은 볼만한게없어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004901.1.15",
          "form": "먹는 회요? 고흥이 바닷가인가요?",
          "original_form": "먹는 회요? 고흥이 바닷가인가요?",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004901.1.16",
          "form": "지리도 잘몰라가지고",
          "original_form": "지리도 잘몰라가지고",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004901.1.17",
          "form": "ㅎㅎ 잘모르시군요ㅎ 바닷가에요ㅎ",
          "original_form": "ㅎㅎ 잘모르시군요ㅎ 바닷가에요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004901.1.18",
          "form": "고흥에 회가 유명하고 싸요 ㅎ",
          "original_form": "고흥에 회가 유명하고 싸요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004901.1.19",
          "form": "회는 별로 않좋아하세요?",
          "original_form": "회는 별로 않좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004901.1.20",
          "form": "전남이죠?ㅎ",
          "original_form": "전남이죠?ㅎ",
          "speaker_id": "1",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004901.1.21",
          "form": "영광옆에있는건가요?",
          "original_form": "영광옆에있는건가요?",
          "speaker_id": "1",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004901.1.22",
          "form": "회 없어서 못먹어요ㅋㅋ",
          "original_form": "회 없어서 못먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004901.1.23",
          "form": "네네ㅎ 전남이에요ㅎ",
          "original_form": "네네ㅎ 전남이에요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004901.1.24",
          "form": "한번 나중에 코로나 잠잠해지면 들리셔요ㅎㅎ",
          "original_form": "한번 나중에 코로나 잠잠해지면 들리셔요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004901.1.25",
          "form": "전남은 먹거리는 아주 기가막힙니다ㅎ",
          "original_form": "전남은 먹거리는 아주 기가막힙니다ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004901.1.26",
          "form": "영광은 출장가느라 가봤는데",
          "original_form": "영광은 출장가느라 가봤는데",
          "speaker_id": "1",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004901.1.27",
          "form": "고흥도 가봐야겠네요",
          "original_form": "고흥도 가봐야겠네요",
          "speaker_id": "1",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004901.1.28",
          "form": "고흥들렀다가 순천 여수 이렇게도시면 괜찮겟네요 ㅣㅎㅎ",
          "original_form": "고흥들렀다가 순천 여수 이렇게도시면 괜찮겟네요 ㅣㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004901.1.29",
          "form": "여수요즘에 핫하다고하던데ㅎㅎ",
          "original_form": "여수요즘에 핫하다고하던데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004901.1.30",
          "form": "순천 여수 광양은 가봤어요",
          "original_form": "순천 여수 광양은 가봤어요",
          "speaker_id": "1",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004901.1.31",
          "form": "아맞네  요즘 코로나 때문에ㅠ",
          "original_form": "아맞네  요즘 코로나 때문에ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004901.1.32",
          "form": "친구가 광양에있어서 ㅎ",
          "original_form": "친구가 광양에있어서 ㅎ",
          "speaker_id": "1",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004901.1.33",
          "form": "여수 좋더라구요",
          "original_form": "여수 좋더라구요",
          "speaker_id": "1",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004901.1.34",
          "form": "그렇군요ㅎㅎ 전 순천에살아서ㅎㅎ",
          "original_form": "그렇군요ㅎㅎ 전 순천에살아서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004901.1.35",
          "form": "광양이 직장이라ㅎㅎ",
          "original_form": "광양이 직장이라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004901.1.36",
          "form": "포스코 다니시는건가요ㅎ",
          "original_form": "포스코 다니시는건가요ㅎ",
          "speaker_id": "1",
          "time": "20210821 19:30"
        },
        {
          "id": "MDRW2100004901.1.37",
          "form": "살기 좋은동네에요ㅎ 저도 출장많이가는데 다른데는 못살겟더라구요ㅎ 특히 수도권은ㅠ",
          "original_form": "살기 좋은동네에요ㅎ 저도 출장많이가는데 다른데는 못살겟더라구요ㅎ 특히 수도권은ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:30"
        },
        {
          "id": "MDRW2100004901.1.38",
          "form": "아니요ㅎㅎ 선박회사다녀요ㅎㅎ",
          "original_form": "아니요ㅎㅎ 선박회사다녀요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:30"
        }
      ]
    }
