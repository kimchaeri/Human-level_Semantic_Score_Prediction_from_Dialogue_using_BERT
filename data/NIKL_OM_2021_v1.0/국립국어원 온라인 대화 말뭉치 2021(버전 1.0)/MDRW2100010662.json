      "utterance": [
        {
          "id": "MDRW2100010662.1.1",
          "form": "영화나 뮤지컬 좋아하세요??",
          "original_form": "영화나 뮤지컬 좋아하세요??",
          "speaker_id": "1",
          "time": "20210823 20:24"
        },
        {
          "id": "MDRW2100010662.1.2",
          "form": "영화는 가끔 보러 가요",
          "original_form": "영화는 가끔 보러 가요",
          "speaker_id": "2",
          "time": "20210823 20:25"
        },
        {
          "id": "MDRW2100010662.1.3",
          "form": "어떤 영화 좋아하세요??",
          "original_form": "어떤 영화 좋아하세요??",
          "speaker_id": "1",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100010662.1.4",
          "form": "액션을 잘 보는 편이에요",
          "original_form": "액션을 잘 보는 편이에요",
          "speaker_id": "2",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100010662.1.5",
          "form": "어떤 영화 재미있게 보셨어요??",
          "original_form": "어떤 영화 재미있게 보셨어요??",
          "speaker_id": "1",
          "time": "20210823 20:37"
        },
        {
          "id": "MDRW2100010662.1.6",
          "form": "안본지 오래되서ㅋㅋ 마블 영화를 잘 보긴 했어요",
          "original_form": "안본지 오래되서ㅋㅋ 마블 영화를 잘 보긴 했어요",
          "speaker_id": "2",
          "time": "20210823 20:38"
        },
        {
          "id": "MDRW2100010662.1.7",
          "form": "좋아하는 영화가 뭐에요?",
          "original_form": "좋아하는 영화가 뭐에요?",
          "speaker_id": "2",
          "time": "20210823 20:39"
        },
        {
          "id": "MDRW2100010662.1.8",
          "form": "저는 마블 영화는 진짜 다 아이맥스에서 봤어요 ㅋㅋ",
          "original_form": "저는 마블 영화는 진짜 다 아이맥스에서 봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:41"
        },
        {
          "id": "MDRW2100010662.1.9",
          "form": "좀 신박한거 좋아하는거 같아요",
          "original_form": "좀 신박한거 좋아하는거 같아요",
          "speaker_id": "1",
          "time": "20210823 20:41"
        },
        {
          "id": "MDRW2100010662.1.10",
          "form": "우주에 관한거나 신선한 주제?? 인셉션같은",
          "original_form": "우주에 관한거나 신선한 주제?? 인셉션같은",
          "speaker_id": "1",
          "time": "20210823 20:41"
        },
        {
          "id": "MDRW2100010662.1.11",
          "form": "전쟁영화도 재밌고",
          "original_form": "전쟁영화도 재밌고",
          "speaker_id": "1",
          "time": "20210823 20:42"
        },
        {
          "id": "MDRW2100010662.1.12",
          "form": "인셉션 재밌져 약간 머리 아파지긴 하는데ㅋㅋ",
          "original_form": "인셉션 재밌져 약간 머리 아파지긴 하는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:42"
        },
        {
          "id": "MDRW2100010662.1.13",
          "form": "저는 해석하는 영화를 좀 좋아하는거 같네요",
          "original_form": "저는 해석하는 영화를 좀 좋아하는거 같네요",
          "speaker_id": "1",
          "time": "20210823 20:45"
        },
        {
          "id": "MDRW2100010662.1.14",
          "form": "인셉션 나중에 재개봉 했을때 용산에서 아이맥스로 봤었는데",
          "original_form": "인셉션 나중에 재개봉 했을때 용산에서 아이맥스로 봤었는데",
          "speaker_id": "1",
          "time": "20210823 20:45"
        },
        {
          "id": "MDRW2100010662.1.15",
          "form": "스토리를 너무 세세하게 알아서 잘 안와닿더라구요 근데 ㅋㅋ",
          "original_form": "스토리를 너무 세세하게 알아서 잘 안와닿더라구요 근데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:46"
        },
        {
          "id": "MDRW2100010662.1.16",
          "form": "아이맥스로 본 적 없어서 궁금하네요",
          "original_form": "아이맥스로 본 적 없어서 궁금하네요",
          "speaker_id": "2",
          "time": "20210823 20:47"
        },
        {
          "id": "MDRW2100010662.1.17",
          "form": "많이 다른가요?",
          "original_form": "많이 다른가요?",
          "speaker_id": "2",
          "time": "20210823 20:47"
        },
        {
          "id": "MDRW2100010662.1.18",
          "form": "일반을 제가 못봤어서 ㅋㅋ",
          "original_form": "일반을 제가 못봤어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100010662.1.19",
          "form": "제가 고3때 개봉했어서",
          "original_form": "제가 고3때 개봉했어서",
          "speaker_id": "1",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100010662.1.20",
          "form": "인터스텔라는 확실히 대박이었어요",
          "original_form": "인터스텔라는 확실히 대박이었어요",
          "speaker_id": "1",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100010662.1.21",
          "form": "어젠저스 엔드게임이랑",
          "original_form": "어젠저스 엔드게임이랑",
          "speaker_id": "1",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100010662.1.22",
          "form": "우주 영화는 확실히 더 멋있을거 같네요",
          "original_form": "우주 영화는 확실히 더 멋있을거 같네요",
          "speaker_id": "2",
          "time": "20210823 20:51"
        },
        {
          "id": "MDRW2100010662.1.23",
          "form": "우주영화가 스케일이 진짜 크죠",
          "original_form": "우주영화가 스케일이 진짜 크죠",
          "speaker_id": "1",
          "time": "20210824 07:51"
        },
        {
          "id": "MDRW2100010662.1.24",
          "form": "우주관련된 영화는 무조건 아이맥스 ㅋㅋ",
          "original_form": "우주관련된 영화는 무조건 아이맥스 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 07:51"
        },
        {
          "id": "MDRW2100010662.1.25",
          "form": "용산 아이맥스가 진짜 대박이에요",
          "original_form": "용산 아이맥스가 진짜 대박이에요",
          "speaker_id": "1",
          "time": "20210824 07:51"
        }
      ]
