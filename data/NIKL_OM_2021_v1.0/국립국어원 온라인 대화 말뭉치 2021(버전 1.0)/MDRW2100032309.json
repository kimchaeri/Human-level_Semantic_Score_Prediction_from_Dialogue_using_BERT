        },
        {
          "id": "MDRW2100032309.1.2",
          "form": "맞다 요즘엔 뭐 덕질해??",
          "original_form": "맞다 요즘엔 뭐 덕질해??",
          "speaker_id": "1",
          "time": "20210905 16:10"
