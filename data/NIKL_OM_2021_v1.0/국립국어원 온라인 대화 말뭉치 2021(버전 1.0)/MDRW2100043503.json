      "utterance": [
        {
          "id": "MDRW2100043503.1.1",
          "form": "두부중에 좀 부드러운 두부잇지",
          "original_form": "두부중에 좀 부드러운 두부잇지",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043503.1.2",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043503.1.3",
          "form": "그거 이름 먼지알아?",
          "original_form": "그거 이름 먼지알아?",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043503.1.4",
          "form": "연두부아냐?",
          "original_form": "연두부아냐?",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043503.1.5",
          "form": "오대박 알고잇네",
          "original_form": "오대박 알고잇네",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043503.1.6",
          "form": "당연히알쥬",
          "original_form": "당연히알쥬",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043503.1.7",
          "form": "초두부도 비슷한건가",
          "original_form": "초두부도 비슷한건가",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043503.1.8",
          "form": "그건머시기여",
          "original_form": "그건머시기여",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043503.1.9",
          "form": "나도 잘 몰라",
          "original_form": "나도 잘 몰라",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043503.1.10",
          "form": "연두부가 그 부드러운건데",
          "original_form": "연두부가 그 부드러운건데",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043503.1.11",
          "form": "맞는거같애 그게",
          "original_form": "맞는거같애 그게",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043503.1.12",
          "form": "먹어본거야?",
          "original_form": "먹어본거야?",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043503.1.13",
          "form": "헐 왜유",
          "original_form": "헐 왜유",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043503.1.14",
          "form": "급식",
          "original_form": "급식",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043503.1.15",
          "form": "에서 나옴",
          "original_form": "에서 나옴",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043503.1.16",
          "form": "근데 안먹엇다",
          "original_form": "근데 안먹엇다",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043503.1.17",
          "form": "왜냐면 너무 간 약해보여",
          "original_form": "왜냐면 너무 간 약해보여",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043503.1.18",
          "form": "아그래???",
          "original_form": "아그래???",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043503.1.19",
          "form": "싱거워보여서",
          "original_form": "싱거워보여서",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043503.1.20",
          "form": "아그건 그래..",
          "original_form": "아그건 그래..",
          "speaker_id": "1",
