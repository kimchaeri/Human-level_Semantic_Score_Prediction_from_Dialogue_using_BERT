      "utterance": [
        {
          "id": "MDRW2100011814.1.1",
          "form": "단 디저트를 잘 못먹어서요",
          "original_form": "단 디저트를 잘 못먹어서요",
          "speaker_id": "1",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011814.1.2",
          "form": "ㅋㅋㅋ닉네임이 파르페셔서요",
          "original_form": "ㅋㅋㅋ닉네임이 파르페셔서요",
          "speaker_id": "2",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011814.1.3",
          "form": "ㅋㅋ파르페 요새는 잘 안 팔더라고요",
          "original_form": "ㅋㅋ파르페 요새는 잘 안 팔더라고요",
          "speaker_id": "2",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011814.1.4",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210824 16:22"
        },
        {
          "id": "MDRW2100011814.1.5",
          "form": "생쥐 님은 디저트 좋아하시나요?",
          "original_form": "생쥐 님은 디저트 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 16:22"
        },
        {
          "id": "MDRW2100011814.1.6",
          "form": "짠게 좋아요",
          "original_form": "짠게 좋아요",
          "speaker_id": "2",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011814.1.7",
          "form": "짠 디저트에는 어떤 게 있나요?",
          "original_form": "짠 디저트에는 어떤 게 있나요?",
          "speaker_id": "1",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011814.1.8",
          "form": "음 걍 짠 과자를 먹습니다ㅋㅋ",
          "original_form": "음 걍 짠 과자를 먹습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011814.1.9",
          "form": "프레첼 과자 아세요?",
          "original_form": "프레첼 과자 아세요?",
          "speaker_id": "2",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011814.1.10",
          "form": "아 알아요!",
          "original_form": "아 알아요!",
          "speaker_id": "1",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011814.1.11",
          "form": "맛있죠",
          "original_form": "맛있죠",
          "speaker_id": "1",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011814.1.12",
          "form": "과자 자주 드세요?",
          "original_form": "과자 자주 드세요?",
          "speaker_id": "1",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100011814.1.13",
          "form": "자제하긴 하는데 주 2회 정도는 먹는거 같아요ㅋㅋ",
          "original_form": "자제하긴 하는데 주 2회 정도는 먹는거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100011814.1.14",
          "form": "파르페님은요?",
          "original_form": "파르페님은요?",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100011814.1.15",
          "form": "저는 있으면 먹는데 막 사서 먹진 않아요",
          "original_form": "저는 있으면 먹는데 막 사서 먹진 않아요",
          "speaker_id": "1",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100011814.1.16",
          "form": "감자칩 같은 것도 좋아하시나요?",
          "original_form": "감자칩 같은 것도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011814.1.17",
          "form": "네ㅋㅋㅋ사실 아직 허니버터칩 좋아합니다",
          "original_form": "네ㅋㅋㅋ사실 아직 허니버터칩 좋아합니다",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011814.1.18",
          "form": "맛있더라고요",
          "original_form": "맛있더라고요",
          "speaker_id": "1",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011814.1.19",
          "form": "단짠단짠의 정석",
          "original_form": "단짠단짠의 정석",
          "speaker_id": "1",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011814.1.20",
          "form": "ㅋㅋ저녁은 드셨나요",
          "original_form": "ㅋㅋ저녁은 드셨나요",
          "speaker_id": "2",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011814.1.21",
          "form": "지금 순두부찌개랑 먹으려고요!",
          "original_form": "지금 순두부찌개랑 먹으려고요!",
          "speaker_id": "1",
          "time": "20210824 17:54"
        }
      ]
