      "utterance": [
        {
          "id": "MDRW2100002623.1.1",
          "form": "날씨어때요?",
          "original_form": "날씨어때요?",
          "speaker_id": "1",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002623.1.2",
          "form": "아깐 흐렸는데 지금은 맑아졌네요!",
          "original_form": "아깐 흐렸는데 지금은 맑아졌네요!",
          "speaker_id": "2",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002623.1.3",
          "form": "변덕스럽네요",
          "original_form": "변덕스럽네요",
          "speaker_id": "1",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002623.1.4",
          "form": "요즘 날씨가 참 변덕스럽긴하죠 ㅠㅠ",
          "original_form": "요즘 날씨가 참 변덕스럽긴하죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002623.1.5",
          "form": "맑았다가 흐렸다가 소나기내리고",
          "original_form": "맑았다가 흐렸다가 소나기내리고",
          "speaker_id": "2",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002623.1.6",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002623.1.7",
          "form": "이게 날씨가 좀 괜찮아져야될텐데",
          "original_form": "이게 날씨가 좀 괜찮아져야될텐데",
          "speaker_id": "2",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002623.1.8",
          "form": "나갈일이 생기면 맑은데도 우산챙겨야하나",
          "original_form": "나갈일이 생기면 맑은데도 우산챙겨야하나",
          "speaker_id": "2",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002623.1.9",
          "form": "고민될정도라니까요 ㅠㅠ",
          "original_form": "고민될정도라니까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002623.1.10",
          "form": "맞아요 대비는 해야 하니까",
          "original_form": "맞아요 대비는 해야 하니까",
          "speaker_id": "1",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002623.1.11",
          "form": "가을이 되면 좀 괜찮아질련지",
          "original_form": "가을이 되면 좀 괜찮아질련지",
          "speaker_id": "2",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002623.1.12",
          "form": "모르겠네여 ㅠㅠ",
          "original_form": "모르겠네여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002623.1.13",
          "form": "그래요",
          "original_form": "그래요",
          "speaker_id": "1",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002623.1.14",
          "form": "그쪽 날씨는 어떤가용?",
          "original_form": "그쪽 날씨는 어떤가용?",
          "speaker_id": "2",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002623.1.15",
          "form": "맑아요",
          "original_form": "맑아요",
          "speaker_id": "1",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002623.1.16",
          "form": "부럽네요! 덥진 않으세여?",
          "original_form": "부럽네요! 덥진 않으세여?",
          "speaker_id": "2",
          "time": "20210820 14:37"
        },
        {
          "id": "MDRW2100002623.1.17",
          "form": "덥기도 해요",
          "original_form": "덥기도 해요",
          "speaker_id": "1",
          "time": "20210820 14:38"
        }
      ]
    }
