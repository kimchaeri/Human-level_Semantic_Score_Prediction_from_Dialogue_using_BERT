        {
          "id": "MDRW2100034394.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210828 03:16"
        },
        {
          "id": "MDRW2100034394.1.2",
          "form": "나 수강신청 망했어",
          "original_form": "나 수강신청 망했어",
          "speaker_id": "1",
          "time": "20210828 03:16"
        },
        {
          "id": "MDRW2100034394.1.3",
          "form": "어떻게했길래ㅜㅜ",
          "original_form": "어떻게했길래ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 03:16"
        },
        {
          "id": "MDRW2100034394.1.4",
          "form": "아침9시 수업이 3개나 돼",
          "original_form": "아침9시 수업이 3개나 돼",
          "speaker_id": "1",
          "time": "20210828 03:16"
        },
        {
          "id": "MDRW2100034394.1.5",
          "form": "어이쿠 너무 힘들겠다",
          "original_form": "어이쿠 너무 힘들겠다",
          "speaker_id": "2",
          "time": "20210828 03:17"
        },
        {
          "id": "MDRW2100034394.1.6",
          "form": "나 아침잠이 많아서 걱정이야",
          "original_form": "나 아침잠이 많아서 걱정이야",
          "speaker_id": "1",
          "time": "20210828 03:17"
        },
        {
          "id": "MDRW2100034394.1.7",
          "form": "아침 수업 진짜 힘든데ㅜ",
          "original_form": "아침 수업 진짜 힘든데ㅜ",
          "speaker_id": "2",
          "time": "20210828 03:17"
        },
        {
          "id": "MDRW2100034394.1.8",
          "form": "pc방을 하필 잘못갔어",
          "original_form": "pc방을 하필 잘못갔어",
          "speaker_id": "1",
          "time": "20210828 03:17"
        },
        {
          "id": "MDRW2100034394.1.9",
          "form": "지각하면 안되잖아",
          "original_form": "지각하면 안되잖아",
          "speaker_id": "2",
          "time": "20210828 03:18"
        },
        {
          "id": "MDRW2100034394.1.10",
          "form": "원래가던 pc방이 공사중이지 뭐야",
          "original_form": "원래가던 pc방이 공사중이지 뭐야",
          "speaker_id": "1",
          "time": "20210828 03:18"
        },
        {
          "id": "MDRW2100034394.1.11",
          "form": "그래로 요즘 인터넷 수멉이지",
          "original_form": "그래로 요즘 인터넷 수멉이지",
          "speaker_id": "2",
          "time": "20210828 03:19"
        },
        {
          "id": "MDRW2100034394.1.12",
          "form": "맞아 인터넷수업 편해",
          "original_form": "맞아 인터넷수업 편해",
          "speaker_id": "1",
          "time": "20210828 03:20"
        },
        {
          "id": "MDRW2100034394.1.13",
          "form": "카메라 켜고 들어?",
          "original_form": "카메라 켜고 들어?",
          "speaker_id": "2",
          "time": "20210828 03:20"
        },
        {
          "id": "MDRW2100034394.1.14",
          "form": "보통은? 그래서 잠옷을 갈아입어",
          "original_form": "보통은? 그래서 잠옷을 갈아입어",
