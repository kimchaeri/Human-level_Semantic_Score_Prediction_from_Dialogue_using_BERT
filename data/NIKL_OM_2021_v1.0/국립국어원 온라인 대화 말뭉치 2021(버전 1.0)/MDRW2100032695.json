      "utterance": [
        {
          "id": "MDRW2100032695.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032695.1.2",
          "form": "네 하이요",
          "original_form": "네 하이요",
          "speaker_id": "2",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032695.1.3",
          "form": "요즘 ott 보는거 있어요?",
          "original_form": "요즘 ott 보는거 있어요?",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032695.1.4",
          "form": "넷플릭스봐요",
          "original_form": "넷플릭스봐요",
          "speaker_id": "2",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032695.1.5",
          "form": "오 저도요!",
          "original_form": "오 저도요!",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032695.1.6",
          "form": "최근에 재미있게 본거 있어요?",
          "original_form": "최근에 재미있게 본거 있어요?",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032695.1.7",
          "form": "저 종이의집이랑 셜록 봤어요",
          "original_form": "저 종이의집이랑 셜록 봤어요",
          "speaker_id": "2",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100032695.1.8",
          "form": "좀 된 것 들이네요",
          "original_form": "좀 된 것 들이네요",
          "speaker_id": "2",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100032695.1.9",
          "form": "님은 뭐 추천하실 거 있으세여",
          "original_form": "님은 뭐 추천하실 거 있으세여",
          "speaker_id": "2",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100032695.1.10",
          "form": "아니에요 그정도면 최근이죠!",
          "original_form": "아니에요 그정도면 최근이죠!",
          "speaker_id": "1",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100032695.1.11",
          "form": "음... 한국드라마는 쌈마이웨이나 비밀의 숲 재미있더라고요!",
          "original_form": "음... 한국드라마는 쌈마이웨이나 비밀의 숲 재미있더라고요!",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100032695.1.12",
          "form": "외국영화는 너무 이것저것 다봐서 기억도 안나네요ㅋㅋ",
          "original_form": "외국영화는 너무 이것저것 다봐서 기억도 안나네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100032695.1.13",
          "form": "오 비숲 ㅌㅋㅌ 명작이죠",
          "original_form": "오 비숲 ㅌㅋㅌ 명작이죠",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100032695.1.14",
          "form": "비숲 너무 재미있었어요ㅠㅠㅠ",
          "original_form": "비숲 너무 재미있었어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032695.1.15",
          "form": "조승우 배두나 커플케미도 좋규",
          "original_form": "조승우 배두나 커플케미도 좋규",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032695.1.16",
          "form": "비숲2도 보셨나여",
          "original_form": "비숲2도 보셨나여",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032695.1.17",
          "form": "네!! 봤어요!!",
          "original_form": "네!! 봤어요!!",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032695.1.18",
          "form": "아니 둘이 케미 너무 좋아요!",
          "original_form": "아니 둘이 케미 너무 좋아요!",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032695.1.19",
          "form": "1편이 아무래도 더 재미있었던 듯",
          "original_form": "1편이 아무래도 더 재미있었던 듯",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032695.1.20",
          "form": "같은 작가님이 라이프도 쓰셨다는데",
          "original_form": "같은 작가님이 라이프도 쓰셨다는데",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032695.1.21",
          "form": "아직 그건 안봤어요",
          "original_form": "아직 그건 안봤어요",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032695.1.22",
          "form": "맞아요 1편이 비교적으로 더 촘촘했던거 같아요",
          "original_form": "맞아요 1편이 비교적으로 더 촘촘했던거 같아요",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032695.1.23",
          "form": "그 작가님 이번에 새 드라마 들어가시더라고요",
          "original_form": "그 작가님 이번에 새 드라마 들어가시더라고요",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032695.1.24",
          "form": "오 벌써여",
          "original_form": "오 벌써여",
          "speaker_id": "2",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032695.1.25",
          "form": "비숲3 기다리고 있는데 나올까 싶네요ㅠ",
          "original_form": "비숲3 기다리고 있는데 나올까 싶네요ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032695.1.26",
          "form": "이제.더 연결할 스토리가 있을까요",
          "original_form": "이제.더 연결할 스토리가 있을까요",
          "speaker_id": "2",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100032695.1.27",
          "form": "셜록이 죽다가 살아나는 전개같이 안허믄",
          "original_form": "셜록이 죽다가 살아나는 전개같이 안허믄",
          "speaker_id": "2",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100032695.1.28",
          "form": "그러게요 여진이가 본청에 들어가버려서",
          "original_form": "그러게요 여진이가 본청에 들어가버려서",
          "speaker_id": "1",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032695.1.29",
          "form": "공조 하려면 시즌2 끝에서 다시 용산서로 돌아왔어야 싶네요...",
          "original_form": "공조 하려면 시즌2 끝에서 다시 용산서로 돌아왔어야 싶네요...",
          "speaker_id": "1",
          "time": "20210905 22:37"
        }
      ]
    }
