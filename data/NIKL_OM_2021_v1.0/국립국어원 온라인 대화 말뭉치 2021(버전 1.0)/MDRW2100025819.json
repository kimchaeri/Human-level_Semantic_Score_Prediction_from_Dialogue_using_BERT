      "utterance": [
        {
          "id": "MDRW2100025819.1.1",
          "form": "쇼핑좋아하시나요",
          "original_form": "쇼핑좋아하시나요",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025819.1.2",
          "form": "네네 방금도 편의점에서 뭘 사고 나오던 길이에요",
          "original_form": "네네 방금도 편의점에서 뭘 사고 나오던 길이에요",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025819.1.3",
          "form": "오우 편의점좋죠",
          "original_form": "오우 편의점좋죠",
          "speaker_id": "1",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025819.1.4",
          "form": "뭐사셨나됴?!",
          "original_form": "뭐사셨나됴?!",
          "speaker_id": "1",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025819.1.5",
          "form": "전 요즘 쿠팡을 주로 사용해요",
          "original_form": "전 요즘 쿠팡을 주로 사용해요",
          "speaker_id": "1",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025819.1.6",
          "form": "음료수 원플원이용ㅋㅋㅋㅋ",
          "original_form": "음료수 원플원이용ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100025819.1.7",
          "form": "ㅋㅋㅋ 원플원 너무 좋지요!!!",
          "original_form": "ㅋㅋㅋ 원플원 너무 좋지요!!!",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100025819.1.8",
          "form": "오늘 기프트콘 부자가 되어서 ㅋㅋㅋㅋ",
          "original_form": "오늘 기프트콘 부자가 되어서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100025819.1.9",
          "form": "오 ㅎㅎ 좋았겠네요 ㅋㅋ",
          "original_form": "오 ㅎㅎ 좋았겠네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 06:53"
        },
        {
          "id": "MDRW2100025819.1.10",
          "form": "쿠팡에선 주로 뭐 사시나요",
          "original_form": "쿠팡에선 주로 뭐 사시나요",
          "speaker_id": "2",
          "time": "20210902 08:08"
        },
        {
          "id": "MDRW2100025819.1.11",
          "form": "전 너무 충동구매 많이해서 쿠팡 지웠어요ㅋㅋㅋ",
          "original_form": "전 너무 충동구매 많이해서 쿠팡 지웠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 08:08"
        },
        {
          "id": "MDRW2100025819.1.12",
          "form": "쿠팡에선 보통 낼 꼭 받아야하는 급한제품 사요ㅋ",
          "original_form": "쿠팡에선 보통 낼 꼭 받아야하는 급한제품 사요ㅋ",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100025819.1.13",
          "form": "만원 이하 제품(?)",
          "original_form": "만원 이하 제품(?)",
          "speaker_id": "1",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100025819.1.14",
          "form": "마자요ㅋㅋㅋ 좀 쿠팡맨에게 죄송할때도 있지만",
          "original_form": "마자요ㅋㅋㅋ 좀 쿠팡맨에게 죄송할때도 있지만",
          "speaker_id": "2",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100025819.1.15",
          "form": "맞습니다...월급받으실거에용",
          "original_form": "맞습니다...월급받으실거에용",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100025819.1.16",
          "form": "전 막 질러뒀던",
          "original_form": "전 막 질러뒀던",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100025819.1.17",
          "form": "가을 옷들이 오늘 한꺼번에 도착해서",
          "original_form": "가을 옷들이 오늘 한꺼번에 도착해서",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100025819.1.18",
          "form": "즐거운 언박싱 할거에용",
          "original_form": "즐거운 언박싱 할거에용",
          "speaker_id": "2",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100025819.1.19",
          "form": "우와 너무 좋겠네요 ㅜ 가을옷",
          "original_form": "우와 너무 좋겠네요 ㅜ 가을옷",
          "speaker_id": "1",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100025819.1.20",
          "form": "전 코로나덕에 살쪄서 옷이 다 안맞 ㅜ",
          "original_form": "전 코로나덕에 살쪄서 옷이 다 안맞 ㅜ",
          "speaker_id": "1",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100025819.1.21",
          "form": "저는 살을 좀 뺏는데 유지가능할지 모르겠어요 ㅋㅋㅋ",
          "original_form": "저는 살을 좀 뺏는데 유지가능할지 모르겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 07:54"
        },
        {
          "id": "MDRW2100025819.1.22",
          "form": "다시 찌기전에 맘껏 예쁜옷 입어보게요...",
          "original_form": "다시 찌기전에 맘껏 예쁜옷 입어보게요...",
          "speaker_id": "2",
          "time": "20210903 07:54"
        },
        {
          "id": "MDRW2100025819.1.23",
          "form": "우와!! 대단하신대요ㅠㅎ",
          "original_form": "우와!! 대단하신대요ㅠㅎ",
          "speaker_id": "1",
          "time": "20210904 05:53"
        },
        {
          "id": "MDRW2100025819.1.24",
          "form": "그럴때 쇼핑할맛나죠",
          "original_form": "그럴때 쇼핑할맛나죠",
          "speaker_id": "1",
          "time": "20210904 05:53"
        },
        {
          "id": "MDRW2100025819.1.25",
          "form": "저도 근데 코로나로 많이 쪄서ㅋㅋㅋ 옷이 안맞기 시작해서 빼기 시작햇어요",
          "original_form": "저도 근데 코로나로 많이 쪄서ㅋㅋㅋ 옷이 안맞기 시작해서 빼기 시작햇어요",
