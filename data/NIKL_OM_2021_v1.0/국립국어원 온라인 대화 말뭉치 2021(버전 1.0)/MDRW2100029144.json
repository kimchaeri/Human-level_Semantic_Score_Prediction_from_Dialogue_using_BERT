        {
          "id": "MDRW2100029144.1.1",
          "form": "오늘 선선하군요",
          "original_form": "오늘 선선하군요",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029144.1.2",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029144.1.3",
          "form": "전오늘 좀 추웠어요",
          "original_form": "전오늘 좀 추웠어요",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029144.1.4",
          "form": "오늘은 그냥 자도 될듯요",
          "original_form": "오늘은 그냥 자도 될듯요",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029144.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029144.1.6",
          "form": "창문열고!",
          "original_form": "창문열고!",
          "speaker_id": "1",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029144.1.7",
          "form": "ㅋㅋ맞아요 어느새 가을이",
          "original_form": "ㅋㅋ맞아요 어느새 가을이",
          "speaker_id": "2",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029144.1.8",
          "form": "이제 슬슬 두꺼운 이불 꺼낼때가 온거같아요",
          "original_form": "이제 슬슬 두꺼운 이불 꺼낼때가 온거같아요",
          "speaker_id": "1",
          "time": "20210904 13:59"
        },
        {
          "id": "MDRW2100029144.1.9",
          "form": "그러겡요 아침에 춥더라고요",
          "original_form": "그러겡요 아침에 춥더라고요",
          "speaker_id": "2",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100029144.1.10",
          "form": "전 가을이제일좋아요",
          "original_form": "전 가을이제일좋아요",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100029144.1.11",
          "form": "겨울.여름싫어요ㅜ",
          "original_form": "겨울.여름싫어요ㅜ",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100029144.1.12",
          "form": "전 겨울에",
          "original_form": "전 겨울에",
          "speaker_id": "2",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100029144.1.13",
          "form": "스키탈수 있어서 ㅋㅋ",
          "original_form": "스키탈수 있어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100029144.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100029144.1.15",
          "form": "저는 스키 탈줄 모르는데",
          "original_form": "저는 스키 탈줄 모르는데",
          "speaker_id": "1",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100029144.1.16",
          "form": "잘타세요??",
          "original_form": "잘타세요??",
          "speaker_id": "1",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100029144.1.17",
          "form": "저요?핵고수죠",
          "original_form": "저요?핵고수죠",
          "speaker_id": "2",
          "time": "20210904 21:39"
        },
        {
          "id": "MDRW2100029144.1.18",
          "form": "오ㅋㅋㅋ 궁금하다",
          "original_form": "오ㅋㅋㅋ 궁금하다",
          "speaker_id": "1",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100029144.1.19",
          "form": "그 스키 상급코스경사보니까",
          "original_form": "그 스키 상급코스경사보니까",
          "speaker_id": "1",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100029144.1.20",
          "form": "장난아시던데",
          "original_form": "장난아시던데",
          "speaker_id": "1",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100029144.1.21",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100029144.1.22",
          "form": "상급 기본 아닙니까",
          "original_form": "상급 기본 아닙니까",
