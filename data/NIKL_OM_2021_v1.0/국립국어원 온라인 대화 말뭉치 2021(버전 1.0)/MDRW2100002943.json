      "utterance": [
        {
          "id": "MDRW2100002943.1.1",
          "form": "안녕~ 이상형이 어떻게돼?",
          "original_form": "안녕~ 이상형이 어떻게돼?",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002943.1.2",
          "form": "음 대화가 잘되는 사람?",
          "original_form": "음 대화가 잘되는 사람?",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002943.1.3",
          "form": "name1씨는?",
          "original_form": "&name1&씨는?",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002943.1.4",
          "form": "내적인건 다 똑같은거같아서 나는 외적인거 말하는편이야 ㅋㅋㅋ",
          "original_form": "내적인건 다 똑같은거같아서 나는 외적인거 말하는편이야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002943.1.5",
          "form": "웃을때 예뻐야돼,,,그리고 강아지상,,",
          "original_form": "웃을때 예뻐야돼,,,그리고 강아지상,,",
          "speaker_id": "1",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002943.1.6",
          "form": "강아지상이라..",
          "original_form": "강아지상이라..",
          "speaker_id": "2",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002943.1.7",
          "form": "연예인으로 치면?",
          "original_form": "연예인으로 치면?",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002943.1.8",
          "form": "약간 박형식같은 얼굴 좋아하는건가",
          "original_form": "약간 박형식같은 얼굴 좋아하는건가",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002943.1.9",
          "form": "웃을 때 이쁜 사람이 누가있지",
          "original_form": "웃을 때 이쁜 사람이 누가있지",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002943.1.10",
          "form": "정해인!! ㅋㅋㅋㅋㅋ 완전내이상형이야",
          "original_form": "정해인!! ㅋㅋㅋㅋㅋ 완전내이상형이야",
          "speaker_id": "1",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002943.1.11",
          "form": "아..!",
          "original_form": "아..!",
          "speaker_id": "2",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002943.1.12",
          "form": "다른 조건은 보는거 없어?",
          "original_form": "다른 조건은 보는거 없어?",
          "speaker_id": "2",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002943.1.13",
          "form": "난 외적인  요소 보면 키가 컸음 좋겠어",
          "original_form": "난 외적인  요소 보면 키가 컸음 좋겠어",
          "speaker_id": "2",
          "time": "20210820 16:58"
        },
        {
          "id": "MDRW2100002943.1.14",
          "form": "내가지금 키가 나랑 비슷한 사람 만나고있는데 나도 키가 중요한줄알았는데 딱히 중요할건없드라구",
          "original_form": "내가지금 키가 나랑 비슷한 사람 만나고있는데 나도 키가 중요한줄알았는데 딱히 중요할건없드라구",
          "speaker_id": "1",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002943.1.15",
          "form": "그런가?",
          "original_form": "그런가?",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002943.1.16",
          "form": "난 좀 나랑 비슷하거나 작은 건 별로던데",
          "original_form": "난 좀 나랑 비슷하거나 작은 건 별로던데",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002943.1.17",
          "form": "몸은 좀 좋아야하는거같아,",
          "original_form": "몸은 좀 좋아야하는거같아,",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002943.1.18",
          "form": "그건 맞아",
          "original_form": "그건 맞아",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002943.1.19",
          "form": "마르거나 살찐건 싫어",
          "original_form": "마르거나 살찐건 싫어",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002943.1.20",
          "form": "관리안하는 사람 별로",
          "original_form": "관리안하는 사람 별로",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002943.1.21",
          "form": "호불호가 강하구나",
          "original_form": "호불호가 강하구나",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002943.1.22",
          "form": "그런가?",
          "original_form": "그런가?",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002943.1.23",
          "form": "보통 다들 그러지않아?",
          "original_form": "보통 다들 그러지않아?",
          "speaker_id": "2",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002943.1.24",
          "form": "나도 마른사람 별로엿거든? 근데 또 만나서 재밋으면 만나더라ㅜㅋㅋㅋㅋㅋ",
          "original_form": "나도 마른사람 별로엿거든? 근데 또 만나서 재밋으면 만나더라ㅜㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002943.1.25",
          "form": "오 그래?",
          "original_form": "오 그래?",
          "speaker_id": "2",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002943.1.26",
          "form": "외적인것 좀 달려도 성격좋으면 아무래도 커버치니까",
          "original_form": "외적인것 좀 달려도 성격좋으면 아무래도 커버치니까",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002943.1.27",
          "form": "맞아 성격이 진짜중요해",
          "original_form": "맞아 성격이 진짜중요해",
          "speaker_id": "1",
          "time": "20210820 17:17"
        }
      ]
