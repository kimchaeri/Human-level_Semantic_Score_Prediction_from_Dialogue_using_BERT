      "utterance": [
        {
          "id": "MDRW2100007944.1.1",
          "form": "코로나 끝나고 나면 어디 가고싶은데 있어?",
          "original_form": "코로나 끝나고 나면 어디 가고싶은데 있어?",
          "speaker_id": "1",
          "time": "20210822 12:10"
        },
        {
          "id": "MDRW2100007944.1.2",
          "form": "음.. 지금 당장 생각나는 곳은 없는데",
          "original_form": "음.. 지금 당장 생각나는 곳은 없는데",
          "speaker_id": "2",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007944.1.3",
          "form": "좀 한적한 곳에 가서 쉬고싶음",
          "original_form": "좀 한적한 곳에 가서 쉬고싶음",
          "speaker_id": "2",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007944.1.4",
          "form": "바닷가나 휴양림",
          "original_form": "바닷가나 휴양림",
          "speaker_id": "2",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007944.1.5",
          "form": "국내? 국외?",
          "original_form": "국내? 국외?",
          "speaker_id": "1",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007944.1.6",
          "form": "당분간 국외는 힘들 것 같고",
          "original_form": "당분간 국외는 힘들 것 같고",
          "speaker_id": "2",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007944.1.7",
          "form": "국내로",
          "original_form": "국내로",
          "speaker_id": "2",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007944.1.8",
          "form": "요새 사람들이 다 있어서",
          "original_form": "요새 사람들이 다 있어서",
          "speaker_id": "1",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007944.1.9",
          "form": "한적한 데가 있으려나",
          "original_form": "한적한 데가 있으려나",
          "speaker_id": "1",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007944.1.10",
          "form": "내집이 제일 한적한 여행지인듯 ㅋㅋ",
          "original_form": "내집이 제일 한적한 여행지인듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007944.1.11",
          "form": "ㅋㅋㅋ그건 그래",
          "original_form": "ㅋㅋㅋ그건 그래",
          "speaker_id": "2",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007944.1.12",
          "form": "일단 코로나가 좀 잠잠해져야 여행계획도 세울듯",
          "original_form": "일단 코로나가 좀 잠잠해져야 여행계획도 세울듯",
          "speaker_id": "2",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007944.1.13",
          "form": "언니는? 어디 여행가고 싶은 곳 있어?",
          "original_form": "언니는? 어디 여행가고 싶은 곳 있어?",
          "speaker_id": "2",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007944.1.14",
          "form": "나는 국내 여행을 좀 다니고 싶어",
          "original_form": "나는 국내 여행을 좀 다니고 싶어",
          "speaker_id": "1",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007944.1.15",
          "form": "코로나가 잦아들고 나면",
          "original_form": "코로나가 잦아들고 나면",
          "speaker_id": "1",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007944.1.16",
          "form": "유명하지 않은 동네에서 방하나 빌려서 일주일이고 이주일이고",
          "original_form": "유명하지 않은 동네에서 방하나 빌려서 일주일이고 이주일이고",
          "speaker_id": "1",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007944.1.17",
          "form": "동네 맛집투어",
          "original_form": "동네 맛집투어",
          "speaker_id": "1",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007944.1.18",
          "form": "ㅋㅋㅋ역시 여행의 핵심은 맛집투어!!",
          "original_form": "ㅋㅋㅋ역시 여행의 핵심은 맛집투어!!",
          "speaker_id": "2",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007944.1.19",
          "form": "그치 맛집투어가 없으면 여행이 아니다",
          "original_form": "그치 맛집투어가 없으면 여행이 아니다",
          "speaker_id": "1",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007944.1.20",
          "form": "엄마아빠는 역시 해외파인듯하지?",
          "original_form": "엄마아빠는 역시 해외파인듯하지?",
          "speaker_id": "1",
          "time": "20210822 12:23"
        },
        {
          "id": "MDRW2100007944.1.21",
          "form": "그럼그럼!! 맛집투어는 여행의 핵심이지",
          "original_form": "그럼그럼!! 맛집투어는 여행의 핵심이지",
          "speaker_id": "2",
          "time": "20210822 12:23"
        },
        {
          "id": "MDRW2100007944.1.22",
          "form": "엄마아빠는 기운 있을 때 한번이라도 더 해외 다녀오자 싶은 마음이 있으신듯",
          "original_form": "엄마아빠는 기운 있을 때 한번이라도 더 해외 다녀오자 싶은 마음이 있으신듯",
          "speaker_id": "2",
          "time": "20210822 12:23"
        },
        {
          "id": "MDRW2100007944.1.23",
          "form": "해외 살다 온 사람들은 여행 안갔어도 해외 자체에 염증이 생기고",
          "original_form": "해외 살다 온 사람들은 여행 안갔어도 해외 자체에 염증이 생기고",
          "speaker_id": "1",
          "time": "20210822 12:23"
        },
        {
          "id": "MDRW2100007944.1.24",
          "form": "국내에 있던 사람들은 나가고 싶고 그런가벼",
          "original_form": "국내에 있던 사람들은 나가고 싶고 그런가벼",
          "speaker_id": "1",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007944.1.25",
          "form": "퇴직하고 나면 돈도 있겠다  다녀오실 만하지",
          "original_form": "퇴직하고 나면 돈도 있겠다  다녀오실 만하지",
          "speaker_id": "2",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007944.1.26",
          "form": "두 분이 잘 다녀오심 좋지 뭐~",
          "original_form": "두 분이 잘 다녀오심 좋지 뭐~",
          "speaker_id": "2",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007944.1.27",
          "form": "나는 좀 충전하는 여행을 다녀오고 싶어",
          "original_form": "나는 좀 충전하는 여행을 다녀오고 싶어",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007944.1.28",
          "form": "근데 이것도 충전 계획을 잘 세워야할듯",
          "original_form": "근데 이것도 충전 계획을 잘 세워야할듯",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007944.1.29",
          "form": "그러게 충전이 아니라 소비하는 여행이 될 수 있으므로",
          "original_form": "그러게 충전이 아니라 소비하는 여행이 될 수 있으므로",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007944.1.30",
          "form": "충전하는 여행이면 맛있는걸 먹어줘야지",
          "original_form": "충전하는 여행이면 맛있는걸 먹어줘야지",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007944.1.31",
          "form": "응 맛있는 것 먹고 마음도 채우고!!",
          "original_form": "응 맛있는 것 먹고 마음도 채우고!!",
          "speaker_id": "2",
          "time": "20210822 12:34"
        }
      ]
