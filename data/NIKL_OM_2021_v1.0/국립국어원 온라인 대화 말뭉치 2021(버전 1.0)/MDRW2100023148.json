      "utterance": [
        {
          "id": "MDRW2100023148.1.1",
          "form": "아ㅋㅋㅋ힘들긴 힘들겟지만...",
          "original_form": "아ㅋㅋㅋ힘들긴 힘들겟지만...",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100023148.1.2",
          "form": "미용 챙길랴 건강챙길랴 그게 더 힘드네요 ㅠ",
          "original_form": "미용 챙길랴 건강챙길랴 그게 더 힘드네요 ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100023148.1.3",
          "form": "되게 열심히 사시네요 미용과 건강 모두 신경쓰며 ㅋㅋㅋㅋ",
          "original_form": "되게 열심히 사시네요 미용과 건강 모두 신경쓰며 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100023148.1.4",
          "form": "그니까요.. 이렇게 해야 인기를 유지할 수 있네요",
          "original_form": "그니까요.. 이렇게 해야 인기를 유지할 수 있네요",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100023148.1.5",
          "form": "전 맨날 내면의 아름다움이 중요하지 하면서 내려놓고 살다가",
          "original_form": "전 맨날 내면의 아름다움이 중요하지 하면서 내려놓고 살다가",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100023148.1.6",
          "form": "ㅋㅋㅋㅋ내면의 아름다움...",
          "original_form": "ㅋㅋㅋㅋ내면의 아름다움...",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100023148.1.7",
          "form": "미용 건강 30대 되서야 신경쓰기 시작햇는데",
          "original_form": "미용 건강 30대 되서야 신경쓰기 시작햇는데",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023148.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023148.1.9",
          "form": "그럼 지금은 둘다 충족하셨나요?",
          "original_form": "그럼 지금은 둘다 충족하셨나요?",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023148.1.10",
          "form": "인기 유지의 비결 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "인기 유지의 비결 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023148.1.11",
          "form": "저요 어우 아직 갈길이 멀죠",
          "original_form": "저요 어우 아직 갈길이 멀죠",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023148.1.12",
          "form": "진짜 30대 후반인데 아직도 20대로 보이시는 분 보면",
          "original_form": "진짜 30대 후반인데 아직도 20대로 보이시는 분 보면",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023148.1.13",
          "form": "더더욱 불타올라요",
          "original_form": "더더욱 불타올라요",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023148.1.14",
          "form": "그래도 작년보단 좀 사람된 거 같아요",
          "original_form": "그래도 작년보단 좀 사람된 거 같아요",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023148.1.15",
          "form": "예뻐졌다 슬림해졌다 이런 소리 올해 마니 들엇어요",
          "original_form": "예뻐졌다 슬림해졌다 이런 소리 올해 마니 들엇어요",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023148.1.16",
          "form": "오 진짜 대단하신 것 같아요",
          "original_form": "오 진짜 대단하신 것 같아요",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023148.1.17",
          "form": "제 주변엔 살 쪘다 확쪘다 앓는 소리만 하는데...",
          "original_form": "제 주변엔 살 쪘다 확쪘다 앓는 소리만 하는데...",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023148.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023148.1.19",
          "form": "요즘 동료들이 저한테 비결 묻네요",
          "original_form": "요즘 동료들이 저한테 비결 묻네요",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023148.1.20",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023148.1.21",
          "form": "어떻게 하면 살이 그리 빠지냐면서^^",
          "original_form": "어떻게 하면 살이 그리 빠지냐면서^^",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023148.1.22",
          "form": "비결 뭐라고 해여",
          "original_form": "비결 뭐라고 해여",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023148.1.23",
          "form": "그냥 아무것도 안했다 했어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 아무것도 안했다 했어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023148.1.24",
          "form": "식단관리하구 운동 꾸준히 했다는 그런 뻔한 이야기들만..",
          "original_form": "식단관리하구 운동 꾸준히 했다는 그런 뻔한 이야기들만..",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023148.1.25",
          "form": "하긴 젤 뻔한데 젤 하기 힘든",
          "original_form": "하긴 젤 뻔한데 젤 하기 힘든",
