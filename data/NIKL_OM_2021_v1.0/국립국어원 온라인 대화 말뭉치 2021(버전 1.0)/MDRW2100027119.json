      "utterance": [
        {
          "id": "MDRW2100027119.1.1",
          "form": "전에 회사다닐때도",
          "original_form": "전에 회사다닐때도",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100027119.1.2",
          "form": "불닭볶음면 먹고 와서",
          "original_form": "불닭볶음면 먹고 와서",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100027119.1.3",
          "form": "화장실 들락거리던 모습이",
          "original_form": "화장실 들락거리던 모습이",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100027119.1.4",
          "form": "아직도 눈에 선하네염ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아직도 눈에 선하네염ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100027119.1.5",
          "form": "그때도 제가그랬나여???ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그때도 제가그랬나여???ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:59"
        },
        {
          "id": "MDRW2100027119.1.6",
          "form": "허헣ㅎ",
          "original_form": "허헣ㅎ",
          "speaker_id": "2",
          "time": "20210902 09:59"
        },
        {
          "id": "MDRW2100027119.1.7",
          "form": "넴ㅋㅋ 제가 그래서\n불닭볶음면은 금요일에만\n잡수라고 했었어여 ㅋㅋㅋ",
          "original_form": "넴ㅋㅋ 제가 그래서\n불닭볶음면은 금요일에만\n잡수라고 했었어여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:00"
        },
        {
          "id": "MDRW2100027119.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:00"
        },
        {
          "id": "MDRW2100027119.1.9",
          "form": "왜난 기억이가물하지",
          "original_form": "왜난 기억이가물하지",
          "speaker_id": "2",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100027119.1.10",
          "form": "일상이라그런가",
          "original_form": "일상이라그런가",
          "speaker_id": "2",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100027119.1.11",
          "form": "요샌 불닭은 끊으셨어염?ㅋㅋ",
          "original_form": "요샌 불닭은 끊으셨어염?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100027119.1.12",
          "form": "조금씩.먹고있어염 ㅋㅋㅋㅋㅋ",
          "original_form": "조금씩.먹고있어염 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100027119.1.13",
          "form": "요즘엔",
          "original_form": "요즘엔",
          "speaker_id": "2",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100027119.1.14",
          "form": "비빔면이",
          "original_form": "비빔면이",
          "speaker_id": "2",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100027119.1.15",
          "form": "글캐맛있드라구얌",
          "original_form": "글캐맛있드라구얌",
          "speaker_id": "2",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100027119.1.16",
          "form": "물론 비빔면 매운맛",
          "original_form": "물론 비빔면 매운맛",
          "speaker_id": "2",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100027119.1.17",
          "form": "비빔면 매운맛이 있나염?",
          "original_form": "비빔면 매운맛이 있나염?",
          "speaker_id": "1",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027119.1.18",
          "form": "어디꺼에요?",
          "original_form": "어디꺼에요?",
          "speaker_id": "1",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027119.1.19",
          "form": "팔도비빔면",
          "original_form": "팔도비빔면",
          "speaker_id": "2",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100027119.1.20",
          "form": "매운맛나왔옹 ㅋㅋ",
          "original_form": "매운맛나왔옹 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100027119.1.21",
          "form": "아 팔도군요ㅋㅋ\n맵찔이는 못먹겠지? ㅜㅜ",
          "original_form": "아 팔도군요ㅋㅋ\n맵찔이는 못먹겠지? ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100027119.1.22",
          "form": "조꼼..",
          "original_form": "조꼼..",
          "speaker_id": "2",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100027119.1.23",
          "form": "맵긴해 ㅋㅋㅋ",
          "original_form": "맵긴해 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100027119.1.24",
          "form": "불닭보단 아닌데..",
          "original_form": "불닭보단 아닌데..",
          "speaker_id": "2",
          "time": "20210902 12:42"
        },
        {
          "id": "MDRW2100027119.1.25",
          "form": "비빔면만 먹어?",
          "original_form": "비빔면만 먹어?",
          "speaker_id": "1",
          "time": "20210902 12:42"
        },
        {
          "id": "MDRW2100027119.1.26",
          "form": "계란? 오이? 골뱅이",
          "original_form": "계란? 오이? 골뱅이",
          "speaker_id": "1",
          "time": "20210902 12:42"
        },
        {
          "id": "MDRW2100027119.1.27",
          "form": "이런거 넣어 먹엉?",
          "original_form": "이런거 넣어 먹엉?",
          "speaker_id": "1",
          "time": "20210902 12:42"
        },
        {
          "id": "MDRW2100027119.1.28",
          "form": "난 그냥 비빔면에",
          "original_form": "난 그냥 비빔면에",
          "speaker_id": "2",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100027119.1.29",
          "form": "군만두만 구워먹어욤ㅋㅋㅋ",
          "original_form": "군만두만 구워먹어욤ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100027119.1.30",
          "form": "오 맛있겠다 그것도 ㅋㅋ",
          "original_form": "오 맛있겠다 그것도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100027119.1.31",
          "form": "비비고? 드시나염?ㅋㅋ",
          "original_form": "비비고? 드시나염?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:44"
        }
      ]
    }
