      "utterance": [
        {
          "id": "MDRW2100038094.1.1",
          "form": "그런 소식도 되게 빠르구나",
          "original_form": "그런 소식도 되게 빠르구나",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038094.1.2",
          "form": "나는 베이컨을 샀어",
          "original_form": "나는 베이컨을 샀어",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038094.1.3",
          "form": "당연하지. 난 인싸잖아",
          "original_form": "당연하지. 난 인싸잖아",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038094.1.4",
          "form": "베이컨?",
          "original_form": "베이컨?",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038094.1.5",
          "form": "그리고 버터를 살껀데 추천하는거있어??",
          "original_form": "그리고 버터를 살껀데 추천하는거있어??",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038094.1.6",
          "form": "너 베이콘 좋아해?",
          "original_form": "너 베이콘 좋아해?",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038094.1.7",
          "form": "버터 흠",
          "original_form": "버터 흠",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038094.1.8",
          "form": "당연하지 밥도둑이잖아",
          "original_form": "당연하지 밥도둑이잖아",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038094.1.9",
          "form": "페이쟝브레통이 맛있어",
          "original_form": "페이쟝브레통이 맛있어",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038094.1.10",
          "form": "네가 이용하는 마트에 있어?",
          "original_form": "네가 이용하는 마트에 있어?",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038094.1.11",
          "form": "음 너무 생소한이름이다",
          "original_form": "음 너무 생소한이름이다",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038094.1.12",
          "form": "흠.. 서울우유버터는 그저 그래",
          "original_form": "흠.. 서울우유버터는 그저 그래",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038094.1.13",
          "form": "패이쟝브레통 무염도 맛있고",
          "original_form": "패이쟝브레통 무염도 맛있고",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038094.1.14",
          "form": "그럼 그건 내 목록에서 빼둘께",
          "original_form": "그럼 그건 내 목록에서 빼둘께",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038094.1.15",
          "form": "앵커버터도 맛있어",
          "original_form": "앵커버터도 맛있어",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038094.1.16",
          "form": "앵커버터는 있어?",
          "original_form": "앵커버터는 있어?",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038094.1.17",
          "form": "그럼 너 추천하는 마요네즈는 있어?",
          "original_form": "그럼 너 추천하는 마요네즈는 있어?",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038094.1.18",
          "form": "응 그건있어 그럼 앵커버터로 사야겠다",
          "original_form": "응 그건있어 그럼 앵커버터로 사야겠다",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038094.1.19",
          "form": "그냥 오뚜기 거 먹어",
          "original_form": "그냥 오뚜기 거 먹어",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038094.1.20",
          "form": "어머 잘됐다!",
          "original_form": "어머 잘됐다!",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038094.1.21",
          "form": "그래 ㅠㅠ 너무 차갑다",
          "original_form": "그래 ㅠㅠ 너무 차갑다",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038094.1.22",
          "form": "나는 아일랜드드레싱도 좋아해",
          "original_form": "나는 아일랜드드레싱도 좋아해",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038094.1.23",
          "form": "싸우전드아일랜드드레싱",
          "original_form": "싸우전드아일랜드드레싱",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038094.1.24",
          "form": "너 마켓컬리 이용한 적 있어?",
          "original_form": "너 마켓컬리 이용한 적 있어?",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038094.1.25",
          "form": "좋은 상품 하나 추천해줄게.",
          "original_form": "좋은 상품 하나 추천해줄게.",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038094.1.26",
          "form": "이용한 적 없으면 혜택이 더 좋을거야",
          "original_form": "이용한 적 없으면 혜택이 더 좋을거야",
          "speaker_id": "1",
          "time": "20210901 23:16"
        }
      ]
    }
  ]
