      "utterance": [
        {
          "id": "MDRW2100032649.1.1",
          "form": "name2님은 요리잘하세요?",
          "original_form": "&name2&님은 요리잘하세요?",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032649.1.2",
          "form": "조금해요 ㅎㅎ",
          "original_form": "조금해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032649.1.3",
          "form": "브러워요!",
          "original_form": "브러워요!",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032649.1.4",
          "form": "유튜브같은거 보고 따라하면",
          "original_form": "유튜브같은거 보고 따라하면",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032649.1.5",
          "form": "할만해요ㅛ",
          "original_form": "할만해요ㅛ",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032649.1.6",
          "form": "저는 유툰브봐도 좀어렵더라고요ㅠ",
          "original_form": "저는 유툰브봐도 좀어렵더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032649.1.7",
          "form": "ㅎㅎ 쉽지않죠 ㅎㅎ",
          "original_form": "ㅎㅎ 쉽지않죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032649.1.8",
          "form": "맛집찾아다니는거",
          "original_form": "맛집찾아다니는거",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032649.1.9",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032649.1.10",
          "form": "딱히, 찾아서 가진 않네요",
          "original_form": "딱히, 찾아서 가진 않네요",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032649.1.11",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032649.1.12",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032649.1.13",
          "form": "엄청찾아다니는건아니고",
          "original_form": "엄청찾아다니는건아니고",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032649.1.14",
          "form": "기회되면 먹는것같아요",
          "original_form": "기회되면 먹는것같아요",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032649.1.15",
          "form": "어느 가게가 기억에 남나요 ?",
          "original_form": "어느 가게가 기억에 남나요 ?",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032649.1.16",
          "form": "칼국수집!",
          "original_form": "칼국수집!",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032649.1.17",
          "form": "맛있다는데갔었는데",
          "original_form": "맛있다는데갔었는데",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032649.1.18",
          "form": "맛있더라고요ㅎㅎ",
          "original_form": "맛있더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032649.1.19",
          "form": "역시 유명한데는 이유가 있나보네요 ㅎㅎ",
          "original_form": "역시 유명한데는 이유가 있나보네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032649.1.20",
          "form": "역시 이름값을 하더라고요ㅋㅋ",
          "original_form": "역시 이름값을 하더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:52"
        }
      ]
