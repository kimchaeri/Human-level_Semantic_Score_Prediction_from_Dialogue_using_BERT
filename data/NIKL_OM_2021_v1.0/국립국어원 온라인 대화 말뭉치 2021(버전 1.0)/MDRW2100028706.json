      "utterance": [
        {
          "id": "MDRW2100028706.1.1",
          "form": "동물 키우시나요",
          "original_form": "동물 키우시나요",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028706.1.2",
          "form": "동물 키웠었습니다",
          "original_form": "동물 키웠었습니다",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028706.1.3",
          "form": "강아지요ㅠㅎㅎ",
          "original_form": "강아지요ㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028706.1.4",
          "form": "오 저도 예전에요",
          "original_form": "오 저도 예전에요",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028706.1.5",
          "form": "이젠 안키우세요?",
          "original_form": "이젠 안키우세요?",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028706.1.6",
          "form": "네 아직 생각이 나서ㅎㅎ",
          "original_form": "네 아직 생각이 나서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028706.1.7",
          "form": "대신 길고양이 밥 챙겨주고 있어요",
          "original_form": "대신 길고양이 밥 챙겨주고 있어요",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028706.1.8",
          "form": "좋은 일 하시네여!!",
          "original_form": "좋은 일 하시네여!!",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028706.1.9",
          "form": "저도 고양이 엄청 좋아합니다",
          "original_form": "저도 고양이 엄청 좋아합니다",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028706.1.10",
          "form": "고양이 넘 귀여워요ㅎ",
          "original_form": "고양이 넘 귀여워요ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028706.1.11",
          "form": "예전에 어떤 동물 키우셨나요?",
          "original_form": "예전에 어떤 동물 키우셨나요?",
          "speaker_id": "1",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028706.1.12",
          "form": "저도 강아지요 ㅎ",
          "original_form": "저도 강아지요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100028706.1.13",
          "form": "요즘은 고양이 키우고싶어요 ㅠㅜ",
          "original_form": "요즘은 고양이 키우고싶어요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100028706.1.14",
          "form": "고양이 넘 귀여워요ㅎㅎ 인스타로 고양이 덕질 하고 있어요ㅠㅠ유툽도 그렇고",
          "original_form": "고양이 넘 귀여워요ㅎㅎ 인스타로 고양이 덕질 하고 있어요ㅠㅠ유툽도 그렇고",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028706.1.15",
          "form": "ㅠ 아 진짜요",
          "original_form": "ㅠ 아 진짜요",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028706.1.16",
          "form": "영상 누구꺼 보세여?",
          "original_form": "영상 누구꺼 보세여?",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028706.1.17",
          "form": "크집사도 좋아하고",
          "original_form": "크집사도 좋아하고",
          "speaker_id": "1",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100028706.1.18",
          "form": "순무도 좋아해요",
          "original_form": "순무도 좋아해요",
          "speaker_id": "1",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100028706.1.19",
          "form": "메탈남이랑 하삼님도 좋아해요",
          "original_form": "메탈남이랑 하삼님도 좋아해요",
          "speaker_id": "1",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100028706.1.20",
          "form": "오 하하하님은요??!",
          "original_form": "오 하하하님은요??!",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100028706.1.21",
          "form": "하삼님이 하하하 님이요! 사람들이 하가 3개라 하삼님 이라 부르더라구요ㅋㅋ",
          "original_form": "하삼님이 하하하 님이요! 사람들이 하가 3개라 하삼님 이라 부르더라구요ㅋㅋ",
