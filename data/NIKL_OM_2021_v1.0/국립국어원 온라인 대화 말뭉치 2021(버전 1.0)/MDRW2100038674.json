      "utterance": [
        {
          "id": "MDRW2100038674.1.1",
          "form": "음 어정쩡한 여름옷상태요 ㅋㅋ",
          "original_form": "음 어정쩡한 여름옷상태요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100038674.1.2",
          "form": "저도 ㅋㅋㅋ",
          "original_form": "저도 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100038674.1.3",
          "form": "건강 위해 챙겨드시는게 있나요",
          "original_form": "건강 위해 챙겨드시는게 있나요",
          "speaker_id": "1",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100038674.1.4",
          "form": "아 얼마전에 엄마 기억력영양제 사드렸어요",
          "original_form": "아 얼마전에 엄마 기억력영양제 사드렸어요",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100038674.1.5",
          "form": "이름이복잡했는데",
          "original_form": "이름이복잡했는데",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100038674.1.6",
          "form": "포스파티딜세린?",
          "original_form": "포스파티딜세린?",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100038674.1.7",
          "form": "일거에요",
          "original_form": "일거에요",
          "speaker_id": "2",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100038674.1.8",
          "form": "오오 기억력",
          "original_form": "오오 기억력",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100038674.1.9",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100038674.1.10",
          "form": "기억력 증진 영양제인가보네요",
          "original_form": "기억력 증진 영양제인가보네요",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100038674.1.11",
          "form": "네네 주재료는 콩이고요",
          "original_form": "네네 주재료는 콩이고요",
          "speaker_id": "2",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100038674.1.12",
          "form": "저희 어머니께서는 60대셔서",
          "original_form": "저희 어머니께서는 60대셔서",
          "speaker_id": "2",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100038674.1.13",
          "form": "좀치매걱정 하셔서",
          "original_form": "좀치매걱정 하셔서",
          "speaker_id": "2",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100038674.1.14",
          "form": "기억력영양제 선물로 추천합니다 ㅋㅋ",
          "original_form": "기억력영양제 선물로 추천합니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100038674.1.15",
          "form": "오 콩이군요ㅠㅠ 추천감사해요",
          "original_form": "오 콩이군요ㅠㅠ 추천감사해요",
          "speaker_id": "1",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100038674.1.16",
          "form": "갱년기시기도해서 ㅠㅠ 걱정이에요",
          "original_form": "갱년기시기도해서 ㅠㅠ 걱정이에요",
          "speaker_id": "1",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100038674.1.17",
          "form": "기억력 영양제 그거말고도 있거든요",
          "original_form": "기억력 영양제 그거말고도 있거든요",
          "speaker_id": "2",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100038674.1.18",
          "form": "천마 라는 성분도 기억력에 좋다고해서 요새 뜨더라고요 ㅎㅎ",
          "original_form": "천마 라는 성분도 기억력에 좋다고해서 요새 뜨더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100038674.1.19",
          "form": "아정말요 저도먹고부모님두드려야겠다",
          "original_form": "아정말요 저도먹고부모님두드려야겠다",
          "speaker_id": "1",
          "time": "20210902 19:34"
        },
        {
          "id": "MDRW2100038674.1.20",
          "form": "근데 가격은 좀 있어요",
          "original_form": "근데 가격은 좀 있어요",
          "speaker_id": "2",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100038674.1.21",
          "form": "저는 음 바디닥터스 꺼 3+1해서 대략 한박스 오만원주고 샀어요",
          "original_form": "저는 음 바디닥터스 꺼 3+1해서 대략 한박스 오만원주고 샀어요",
          "speaker_id": "2",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100038674.1.22",
          "form": "오 바디닥터스 메모메모",
          "original_form": "오 바디닥터스 메모메모",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100038674.1.23",
          "form": "건강이최고에요",
          "original_form": "건강이최고에요",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100038674.1.24",
          "form": "그리고 그 홍삼인가 그것도 엄마 꾸준히드시는게",
          "original_form": "그리고 그 홍삼인가 그것도 엄마 꾸준히드시는게",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100038674.1.25",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100038674.1.26",
          "form": "오 화애락 같은건가요",
          "original_form": "오 화애락 같은건가요",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100038674.1.27",
          "form": "그 팩에들었다해야하나",
          "original_form": "그 팩에들었다해야하나",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100038674.1.28",
          "form": "쪽 짜먹는거요 ㅋㅋ",
          "original_form": "쪽 짜먹는거요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100038674.1.29",
          "form": "편의점에도 팔고하는거 ㅋㅋ",
          "original_form": "편의점에도 팔고하는거 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100038674.1.30",
          "form": "아하 애브리타임",
          "original_form": "아하 애브리타임",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100038674.1.31",
          "form": "저는 그때 바디닥터스파는 네이버스토어 에 여러브랜드팔거든요 거기꺼 가성비좋아요",
          "original_form": "저는 그때 바디닥터스파는 네이버스토어 에 여러브랜드팔거든요 거기꺼 가성비좋아요",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100038674.1.32",
          "form": "아 네이버스토어가 요즘 간편하고 좋아요",
          "original_form": "아 네이버스토어가 요즘 간편하고 좋아요",
          "speaker_id": "1",
          "time": "20210902 21:43"
        }
      ]
