      "utterance": [
        {
          "id": "MDRW2100014001.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100014001.1.2",
          "form": "요즘 읽은 책 뭐있어?",
          "original_form": "요즘 읽은 책 뭐있어?",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100014001.1.3",
          "form": "달러구트의 꿈 백화점 !",
          "original_form": "달러구트의 꿈 백화점 !",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014001.1.4",
          "form": "오 그거 나도 1편 읽었어",
          "original_form": "오 그거 나도 1편 읽었어",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014001.1.5",
          "form": "나도나도 어젠 2편 읽었어",
          "original_form": "나도나도 어젠 2편 읽었어",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014001.1.6",
          "form": "오 아직 2편은 안읽었는데 어때?",
          "original_form": "오 아직 2편은 안읽었는데 어때?",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014001.1.7",
          "form": "1편만큼 재밌었어",
          "original_form": "1편만큼 재밌었어",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014001.1.8",
          "form": "오 기대된다 나도 읽어야겠어",
          "original_form": "오 기대된다 나도 읽어야겠어",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100014001.1.9",
          "form": "재밌어 올해의 도서일만 해 ㅋㅋㅋ",
          "original_form": "재밌어 올해의 도서일만 해 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100014001.1.10",
          "form": "요즘 책 사서 읽어?",
          "original_form": "요즘 책 사서 읽어?",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100014001.1.11",
          "form": "아니 앱으로 읽어",
          "original_form": "아니 앱으로 읽어",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100014001.1.12",
          "form": "아 밀리의 서재 같은?",
          "original_form": "아 밀리의 서재 같은?",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100014001.1.13",
          "form": "응응 기간구독해서 읽는건데 좋아",
          "original_form": "응응 기간구독해서 읽는건데 좋아",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100014001.1.14",
          "form": "나는 요즘 도서관에서 빌려 읽고있어",
          "original_form": "나는 요즘 도서관에서 빌려 읽고있어",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100014001.1.15",
          "form": "도서관 다니는것도 좋지",
          "original_form": "도서관 다니는것도 좋지",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100014001.1.16",
          "form": "맞아 도서관 분위기가 좋아",
          "original_form": "맞아 도서관 분위기가 좋아",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100014001.1.17",
          "form": "요즘 잡지 읽은거 있니?",
          "original_form": "요즘 잡지 읽은거 있니?",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100014001.1.18",
          "form": "월간지는 가끔씩 보는거 같아",
          "original_form": "월간지는 가끔씩 보는거 같아",
          "speaker_id": "2",
          "time": "20210825 16:50"
        }
      ]
