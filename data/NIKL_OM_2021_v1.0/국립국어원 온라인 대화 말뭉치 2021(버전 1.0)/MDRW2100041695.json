        {
          "id": "MDRW2100041695.1.1",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041695.1.2",
          "form": "지방은 늘 작게하는거 같아요",
          "original_form": "지방은 늘 작게하는거 같아요",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041695.1.3",
          "form": "화장실 모래는 머쓰시나요",
          "original_form": "화장실 모래는 머쓰시나요",
          "speaker_id": "1",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041695.1.4",
          "form": "저는 국룰의 벤토요",
          "original_form": "저는 국룰의 벤토요",
          "speaker_id": "2",
          "time": "20210906 19:12"
        },
        {
          "id": "MDRW2100041695.1.5",
          "form": "두부 쓰다가 바꿨다는요",
          "original_form": "두부 쓰다가 바꿨다는요",
          "speaker_id": "2",
          "time": "20210906 19:12"
        },
        {
          "id": "MDRW2100041695.1.6",
          "form": "저도 벤토...ㅎㅎ",
          "original_form": "저도 벤토...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100041695.1.7",
          "form": "사막화가 쫌있네요",
          "original_form": "사막화가 쫌있네요",
          "speaker_id": "1",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100041695.1.8",
          "form": "아크로펫 모래 추천합니다",
          "original_form": "아크로펫 모래 추천합니다",
          "speaker_id": "2",
          "time": "20210906 19:15"
        },
        {
          "id": "MDRW2100041695.1.9",
          "form": "생각 보다 저렴하네요??",
          "original_form": "생각 보다 저렴하네요??",
          "speaker_id": "1",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100041695.1.10",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100041695.1.11",
          "form": "잘뭉치고 좋더라구요",
          "original_form": "잘뭉치고 좋더라구요",
          "speaker_id": "2",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100041695.1.12",
          "form": "오 후기한번 찾아봐야겠네욥",
          "original_form": "오 후기한번 찾아봐야겠네욥",
          "speaker_id": "1",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100041695.1.13",
          "form": "화장실앞에 모래 매트는 사셨나용",
          "original_form": "화장실앞에 모래 매트는 사셨나용",
          "speaker_id": "1",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100041695.1.14",
          "form": "매트는 아직읎는데",
          "original_form": "매트는 아직읎는데",
          "speaker_id": "2",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100041695.1.15",
          "form": "좋나요??",
          "original_form": "좋나요??",
          "speaker_id": "2",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100041695.1.16",
          "form": "네네 매트 있기 전후가 완전달라요",
          "original_form": "네네 매트 있기 전후가 완전달라요",
          "speaker_id": "1",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100041695.1.17",
          "form": "저도 함 찾아봐야겠네요 매트라니",
          "original_form": "저도 함 찾아봐야겠네요 매트라니",
          "speaker_id": "2",
          "time": "20210906 19:23"
        },
        {
          "id": "MDRW2100041695.1.18",
          "form": "그나마 사막화 덜 해요 ㅜㅡㅋ",
          "original_form": "그나마 사막화 덜 해요 ㅜㅡㅋ",
