        },
        {
          "id": "MDRW2100013993.1.2",
          "form": "쓰읍..오타쿠였답니다",
          "original_form": "쓰읍..오타쿠였답니다",
          "speaker_id": "2",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100013993.1.3",
          "form": "아이돌 덕후",
          "original_form": "아이돌 덕후",
          "speaker_id": "2",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100013993.1.4",
          "form": "name1은요",
          "original_form": "&name1&은요",
          "speaker_id": "2",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100013993.1.5",
          "form": "아이돌 누구 좋아했어요",
          "original_form": "아이돌 누구 좋아했어요",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100013993.1.6",
          "form": "전 그냥 평범?",
          "original_form": "전 그냥 평범?",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100013993.1.7",
          "form": "남들 하는 만큼만 하고다녔어요",
          "original_form": "남들 하는 만큼만 하고다녔어요",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100013993.1.8",
          "form": "ㅇㄴ 나만",
          "original_form": "ㅇㄴ 나만",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013993.1.9",
          "form": "나만 들켜버렦네",
          "original_form": "나만 들켜버렦네",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013993.1.10",
          "form": "뭔가 약간의 부끄러움이 있지만",
          "original_form": "뭔가 약간의 부끄러움이 있지만",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013993.1.11",
          "form": "아나 전 엑소요",
          "original_form": "아나 전 엑소요",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013993.1.12",
          "form": "EXO",
          "original_form": "EXO",
          "speaker_id": "1",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013993.1.13",
          "form": "누가 제일 좋던가요",
          "original_form": "누가 제일 좋던가요",
          "speaker_id": "1",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013993.1.14",
          "form": "막 사물함에 사진 붙혀놓고 그러셨나",
          "original_form": "막 사물함에 사진 붙혀놓고 그러셨나",
          "speaker_id": "1",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013993.1.15",
          "form": "아뇨 사물함에 사진은 안붙혀놓앆고",
          "original_form": "아뇨 사물함에 사진은 안붙혀놓앆고",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100013993.1.16",
          "form": "그 애들한테 편지 받아서(애들 데뷔초때ㅖ",
          "original_form": "그 애들한테 편지 받아서(애들 데뷔초때ㅖ",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100013993.1.17",
          "form": "스엠에 선물 같이 보내고 그랬어요",
          "original_form": "스엠에 선물 같이 보내고 그랬어요",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100013993.1.18",
          "form": "열성팬이셨구나",
          "original_form": "열성팬이셨구나",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100013993.1.19",
          "form": "전 경수로 입덕핶어요",
          "original_form": "전 경수로 입덕핶어요",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100013993.1.20",
          "form": "감자.",
          "original_form": "감자.",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100013993.1.21",
          "form": "감자?",
          "original_form": "감자?",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100013993.1.22",
          "form": "경수 감자같다고",
          "original_form": "경수 감자같다고",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100013993.1.23",
          "form": "안티나 팬이나 감자로 부르더라고요",
          "original_form": "안티나 팬이나 감자로 부르더라고요",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100013993.1.24",
          "form": "아.. 동글동글한건가",
          "original_form": "아.. 동글동글한건가",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100013993.1.25",
          "form": "너무 귀엽죠",
          "original_form": "너무 귀엽죠",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100013993.1.26",
          "form": "ㅋㅋㅋㅋ네",
          "original_form": "ㅋㅋㅋㅋ네",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100013993.1.27",
          "form": "아니 저는 엑소 때문에",
