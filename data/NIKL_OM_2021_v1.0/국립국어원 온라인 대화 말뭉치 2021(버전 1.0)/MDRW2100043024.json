      "utterance": [
        {
          "id": "MDRW2100043024.1.1",
          "form": "오늘 설거지랑 욕실청소...",
          "original_form": "오늘 설거지랑 욕실청소...",
          "speaker_id": "2",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100043024.1.2",
          "form": "니가 싫어하는거...",
          "original_form": "니가 싫어하는거...",
          "speaker_id": "2",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100043024.1.3",
          "form": "으....싫다",
          "original_form": "으....싫다",
          "speaker_id": "1",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100043024.1.4",
          "form": "나는 빨래하고 반찬해둬야함",
          "original_form": "나는 빨래하고 반찬해둬야함",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100043024.1.5",
          "form": "오늘 빨래 안마르겠는데. ..",
          "original_form": "오늘 빨래 안마르겠는데. ..",
          "speaker_id": "2",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100043024.1.6",
          "form": "그니까.. 걱정임 속옷정도는 마르겠지...",
          "original_form": "그니까.. 걱정임 속옷정도는 마르겠지...",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043024.1.7",
          "form": "선풍기 틀어놔",
          "original_form": "선풍기 틀어놔",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043024.1.8",
          "form": "그래야겠다",
          "original_form": "그래야겠다",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043024.1.9",
          "form": "욕실도 잘 안마르는거 아니가",
          "original_form": "욕실도 잘 안마르는거 아니가",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100043024.1.10",
          "form": "청소끝나고 이케아 유리닦이로 다 닦아내면",
          "original_form": "청소끝나고 이케아 유리닦이로 다 닦아내면",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100043024.1.11",
          "form": "괜찮더라",
          "original_form": "괜찮더라",
          "speaker_id": "2",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043024.1.12",
          "form": "오 꿀팁...",
          "original_form": "오 꿀팁...",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043024.1.13",
          "form": "나는 세차할때 쓰는 닦는제품 쓰는데",
          "original_form": "나는 세차할때 쓰는 닦는제품 쓰는데",
          "speaker_id": "1",
          "time": "20210907 13:21"
        },
        {
          "id": "MDRW2100043024.1.14",
          "form": "그거괜찮아",
          "original_form": "그거괜찮아",
          "speaker_id": "1",
          "time": "20210907 13:21"
        },
        {
          "id": "MDRW2100043024.1.15",
          "form": "오....감사 하나 사둬야겠다",
          "original_form": "오....감사 하나 사둬야겠다",
          "speaker_id": "2",
          "time": "20210907 13:21"
        },
        {
          "id": "MDRW2100043024.1.16",
          "form": "확실히 세차용품이 잘나오더라",
          "original_form": "확실히 세차용품이 잘나오더라",
          "speaker_id": "2",
          "time": "20210907 13:21"
        },
        {
          "id": "MDRW2100043024.1.17",
          "form": "세차에 꽂힌 사람이 많아서 그런가봐",
          "original_form": "세차에 꽂힌 사람이 많아서 그런가봐",
          "speaker_id": "1",
          "time": "20210907 13:21"
        },
        {
          "id": "MDRW2100043024.1.18",
          "form": "유리닦이도 세차용이 확실히 더좋더라",
          "original_form": "유리닦이도 세차용이 확실히 더좋더라",
          "speaker_id": "2",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100043024.1.19",
          "form": "그 간단한것도 차이가 나나보네",
          "original_form": "그 간단한것도 차이가 나나보네",
          "speaker_id": "1",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100043024.1.20",
          "form": "틈새를 닦을수있게 해뒀더라고",
          "original_form": "틈새를 닦을수있게 해뒀더라고",
          "speaker_id": "2",
          "time": "20210907 13:23"
        }
