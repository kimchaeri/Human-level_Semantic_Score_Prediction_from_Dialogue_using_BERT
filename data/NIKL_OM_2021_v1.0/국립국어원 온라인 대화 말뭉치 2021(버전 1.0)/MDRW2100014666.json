      "utterance": [
        {
          "id": "MDRW2100014666.1.1",
          "form": "스토아 두툼다쓰셨어요?",
          "original_form": "스토아 두툼다쓰셨어요?",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100014666.1.2",
          "form": "네 어제 다털어였어요ㅎ",
          "original_form": "네 어제 다털어였어요ㅎ",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100014666.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100014666.1.4",
          "form": "저도 어제 만포받은것들 털었어요",
          "original_form": "저도 어제 만포받은것들 털었어요",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100014666.1.5",
          "form": "너비아니하나사고",
          "original_form": "너비아니하나사고",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100014666.1.6",
          "form": "식용유 쟁이고 ㅎㅎ",
          "original_form": "식용유 쟁이고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100014666.1.7",
          "form": "무배예요?",
          "original_form": "무배예요?",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100014666.1.8",
          "form": "머사셨어요?",
          "original_form": "머사셨어요?",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100014666.1.9",
          "form": "저 10만포 막 쓰고 3천정도남아서",
          "original_form": "저 10만포 막 쓰고 3천정도남아서",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100014666.1.10",
          "form": "네 금액이 크니 무배더라구요",
          "original_form": "네 금액이 크니 무배더라구요",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100014666.1.11",
          "form": "아 만포있으셨죠?",
          "original_form": "아 만포있으셨죠?",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100014666.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100014666.1.13",
          "form": "남은건 속바지샀어요",
          "original_form": "남은건 속바지샀어요",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100014666.1.14",
          "form": "신랑꺼는 스토아 출첵 만포",
          "original_form": "신랑꺼는 스토아 출첵 만포",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100014666.1.15",
          "form": "스토아 두툼 짱이예요",
          "original_form": "스토아 두툼 짱이예요",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100014666.1.16",
          "form": "무배라 살거없을땐 그게좋더라구요",
          "original_form": "무배라 살거없을땐 그게좋더라구요",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100014666.1.17",
          "form": "오 또요?",
          "original_form": "오 또요?",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100014666.1.18",
          "form": "고구마큐브사고싶었는데",
          "original_form": "고구마큐브사고싶었는데",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100014666.1.19",
          "form": "엄청잘되시네요ㅎ부럽",
          "original_form": "엄청잘되시네요ㅎ부럽",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100014666.1.20",
          "form": "무배가 아니던가? 가격이 안맞던가 그랬어요",
          "original_form": "무배가 아니던가? 가격이 안맞던가 그랬어요",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100014666.1.21",
          "form": "허닭 무배아닌듯요",
          "original_form": "허닭 무배아닌듯요",
