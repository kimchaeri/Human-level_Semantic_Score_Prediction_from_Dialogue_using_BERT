      "utterance": [
        {
          "id": "MDRW2100033196.1.1",
          "form": "온라인 커뮤 활동 하세여?",
          "original_form": "온라인 커뮤 활동 하세여?",
          "speaker_id": "1",
          "time": "20210906 18:35"
        },
        {
          "id": "MDRW2100033196.1.2",
          "form": "아니요. 하시는거 있나요?",
          "original_form": "아니요. 하시는거 있나요?",
          "speaker_id": "2",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100033196.1.3",
          "form": "저는 심심이랑 인스타그램이요",
          "original_form": "저는 심심이랑 인스타그램이요",
          "speaker_id": "1",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100033196.1.4",
          "form": "저도 몇개 가입은 되있는데 열심히 활동하진 않네요",
          "original_form": "저도 몇개 가입은 되있는데 열심히 활동하진 않네요",
          "speaker_id": "2",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100033196.1.5",
          "form": "저는 인스타는 매일하고",
          "original_form": "저는 인스타는 매일하고",
          "speaker_id": "1",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100033196.1.6",
          "form": "카페나 그런 커뮤는 잘 안해요",
          "original_form": "카페나 그런 커뮤는 잘 안해요",
          "speaker_id": "1",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100033196.1.7",
          "form": "인스타 업로드 많이 하시나봐요",
          "original_form": "인스타 업로드 많이 하시나봐요",
          "speaker_id": "2",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100033196.1.8",
          "form": "네네 추억용으로 많이 올려요",
          "original_form": "네네 추억용으로 많이 올려요",
          "speaker_id": "1",
          "time": "20210906 19:12"
        },
        {
          "id": "MDRW2100033196.1.9",
          "form": "사진 보기 좋더라고요",
          "original_form": "사진 보기 좋더라고요",
          "speaker_id": "1",
          "time": "20210906 19:12"
        },
        {
          "id": "MDRW2100033196.1.10",
          "form": "그쵸. 인스타는 필터도 많더라구요.",
          "original_form": "그쵸. 인스타는 필터도 많더라구요.",
          "speaker_id": "2",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100033196.1.11",
          "form": "네네 필터는 잘 안써요ㅋㅋㅋ",
          "original_form": "네네 필터는 잘 안써요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:15"
        },
        {
          "id": "MDRW2100033196.1.12",
          "form": "전 업로드는 잘 안하는 편인데 보는건 재밌어요ㅋㅋ",
          "original_form": "전 업로드는 잘 안하는 편인데 보는건 재밌어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100033196.1.13",
          "form": "인스타에 예쁜 물건이 넘 마나서",
          "original_form": "인스타에 예쁜 물건이 넘 마나서",
          "speaker_id": "1",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100033196.1.14",
          "form": "돈을 마니 쓰는고같아요",
          "original_form": "돈을 마니 쓰는고같아요",
          "speaker_id": "1",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100033196.1.15",
          "form": "광고가 많기도 하더라구요.",
          "original_form": "광고가 많기도 하더라구요.",
          "speaker_id": "2",
          "time": "20210906 19:29"
        },
        {
          "id": "MDRW2100033196.1.16",
          "form": "구쵸 광고도 많고",
