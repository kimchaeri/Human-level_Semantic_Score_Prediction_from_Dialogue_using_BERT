        },
        {
          "id": "MDRW2100010412.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010412.1.3",
          "form": "요즘 즐겨보는 프로그램이 있으신가요?",
          "original_form": "요즘 즐겨보는 프로그램이 있으신가요?",
          "speaker_id": "2",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010412.1.4",
          "form": "저늑",
          "original_form": "저늑",
          "speaker_id": "1",
          "time": "20210824 10:55"
        },
        {
          "id": "MDRW2100010412.1.5",
          "form": "저는 해방타운이요",
          "original_form": "저는 해방타운이요",
          "speaker_id": "1",
          "time": "20210824 10:55"
        },
        {
          "id": "MDRW2100010412.1.6",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210824 10:55"
        },
        {
          "id": "MDRW2100010412.1.7",
          "form": "전 집에 티비가 없어서 잘 챙겨보진 못해요 ㅎ ㅠㅠ",
          "original_form": "전 집에 티비가 없어서 잘 챙겨보진 못해요 ㅎ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100010412.1.8",
          "form": "해방타운 재밌나요?",
          "original_form": "해방타운 재밌나요?",
          "speaker_id": "2",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100010412.1.9",
          "form": "네네 볼만해요",
          "original_form": "네네 볼만해요",
          "speaker_id": "1",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100010412.1.10",
          "form": "아예안보나요?",
          "original_form": "아예안보나요?",
          "speaker_id": "1",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100010412.1.11",
          "form": "네 ㅠㅠㅋㅋ 지난달에 넷플릭스 입문해서 보고있어요",
          "original_form": "네 ㅠㅠㅋㅋ 지난달에 넷플릭스 입문해서 보고있어요",
          "speaker_id": "2",
          "time": "20210825 10:29"
        },
        {
          "id": "MDRW2100010412.1.12",
          "form": "브리저튼 재밌더라구요",
          "original_form": "브리저튼 재밌더라구요",
          "speaker_id": "2",
          "time": "20210825 10:29"
        },
        {
          "id": "MDRW2100010412.1.13",
          "form": "넷플릭스",
          "original_form": "넷플릭스",
          "speaker_id": "1",
          "time": "20210825 11:29"
        },
        {
          "id": "MDRW2100010412.1.14",
          "form": "한달결제비가 얼마죠?",
          "original_form": "한달결제비가 얼마죠?",
          "speaker_id": "1",
          "time": "20210825 11:29"
        },
        {
          "id": "MDRW2100010412.1.15",
          "form": "2만원인가..로 알고있어요",
          "original_form": "2만원인가..로 알고있어요",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100010412.1.16",
          "form": "아버지 결제하신걸로 온가족이 봐서 정확히는 모르겠네요 ㅠ",
          "original_form": "아버지 결제하신걸로 온가족이 봐서 정확히는 모르겠네요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100010412.1.17",
          "form": "그렇군오",
          "original_form": "그렇군오",
          "speaker_id": "1",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100010412.1.18",
          "form": "잼난거있나요? 브리저튼말구",
          "original_form": "잼난거있나요? 브리저튼말구",
          "speaker_id": "1",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100010412.1.19",
          "form": "지정생존자?",
          "original_form": "지정생존자?",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100010412.1.20",
          "form": "몇년전 드라마인데 재밌다더라구요",
          "original_form": "몇년전 드라마인데 재밌다더라구요",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100010412.1.21",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100010412.1.22",
          "form": "넷플결제가 안되어있어서잘모르겠네요",
          "original_form": "넷플결제가 안되어있어서잘모르겠네요",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100010412.1.23",
          "form": "싱크홀 영화 재밌대요",
          "original_form": "싱크홀 영화 재밌대요",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100010412.1.24",
          "form": "생각없이 웃으면서 볼만한",
          "original_form": "생각없이 웃으면서 볼만한",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100010412.1.25",
          "form": "오 맞아요ㅋㅋㅋ",
          "original_form": "오 맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100010412.1.26",
          "form": "저보고싶어요",
          "original_form": "저보고싶어요",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100010412.1.27",
          "form": "마블시리즈 좋아하는 분들은",
          "original_form": "마블시리즈 좋아하는 분들은",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100010412.1.28",
          "form": "블랙위도우도 재밌었다 하시더라구요",
          "original_form": "블랙위도우도 재밌었다 하시더라구요",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100010412.1.29",
          "form": "웃기고 그렇다기보단 뭔가 찡하고 슬픈?",
          "original_form": "웃기고 그렇다기보단 뭔가 찡하고 슬픈?",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100010412.1.30",
          "form": "저는 참",
          "original_form": "저는 참",
          "speaker_id": "1",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100010412.1.31",
          "form": "그쪽 시리즈?",
          "original_form": "그쪽 시리즈?",
          "speaker_id": "1",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100010412.1.32",
          "form": "하다도안본것같네요ㅎㅎ",
          "original_form": "하다도안본것같네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100010412.1.33",
          "form": "좀 취향 갈릴만해요 ㅋㅋ 전 좋아해서 여러번 보고 그랬는데 블랙위도우는 아직 못봤네욤 ㅠ",
          "original_form": "좀 취향 갈릴만해요 ㅋㅋ 전 좋아해서 여러번 보고 그랬는데 블랙위도우는 아직 못봤네욤 ㅠ",
          "speaker_id": "2",
          "time": "20210826 17:31"
        }
      ]
