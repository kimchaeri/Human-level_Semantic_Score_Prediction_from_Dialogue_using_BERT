        {
          "id": "MDRW2100044058.1.1",
          "form": "커피도 좋아하세요?",
          "original_form": "커피도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044058.1.2",
          "form": "좋아하죠!",
          "original_form": "좋아하죠!",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044058.1.3",
          "form": "아메리카노?",
          "original_form": "아메리카노?",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044058.1.4",
          "form": "라떼!",
          "original_form": "라떼!",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044058.1.5",
          "form": "오호 라떼는 어떤 ?",
          "original_form": "오호 라떼는 어떤 ?",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044058.1.6",
          "form": "종류 많잖아요",
          "original_form": "종류 많잖아요",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044058.1.7",
          "form": "그냥 카페라떼 좋아해요",
          "original_form": "그냥 카페라떼 좋아해요",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044058.1.8",
          "form": "바닐라라떼 맛나더라구영",
          "original_form": "바닐라라떼 맛나더라구영",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044058.1.9",
          "form": "어디꺼 맛난가요?",
          "original_form": "어디꺼 맛난가요?",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044058.1.10",
          "form": "저는 스벅좋아해용",
          "original_form": "저는 스벅좋아해용",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044058.1.11",
          "form": "아항 스벅에도 여러가지 바닐라라떼가있잖아요 ㅇ",
          "original_form": "아항 스벅에도 여러가지 바닐라라떼가있잖아요 ㅇ",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044058.1.12",
          "form": "오리지날요?",
          "original_form": "오리지날요?",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044058.1.13",
          "form": "네네 오리지날요",
          "original_form": "네네 오리지날요",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044058.1.14",
          "form": "바닐라 콜그브루도 맛있대요 ㅋㅋㅋ",
          "original_form": "바닐라 콜그브루도 맛있대요 ㅋㅋㅋ",
