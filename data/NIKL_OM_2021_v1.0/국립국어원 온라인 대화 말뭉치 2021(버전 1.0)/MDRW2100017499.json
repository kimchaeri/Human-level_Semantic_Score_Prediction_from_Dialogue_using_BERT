      "utterance": [
        {
          "id": "MDRW2100017499.1.1",
          "form": "유튜브에서 뭐봐?",
          "original_form": "유튜브에서 뭐봐?",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017499.1.2",
          "form": "유튜브 안본다 뭐봐?",
          "original_form": "유튜브 안본다 뭐봐?",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017499.1.3",
          "form": "여행이나 동물같은거",
          "original_form": "여행이나 동물같은거",
          "speaker_id": "1",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017499.1.4",
          "form": "티비 뭐봐?",
          "original_form": "티비 뭐봐?",
          "speaker_id": "1",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017499.1.5",
          "form": "예능 봐 티비 뭐봐?",
          "original_form": "예능 봐 티비 뭐봐?",
          "speaker_id": "2",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017499.1.6",
          "form": "유퀴즈",
          "original_form": "유퀴즈",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017499.1.7",
          "form": "우도주막",
          "original_form": "우도주막",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017499.1.8",
          "form": "유퀴즈 유도주막 잼있어?",
          "original_form": "유퀴즈 유도주막 잼있어?",
          "speaker_id": "2",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017499.1.9",
          "form": "우도주막",
          "original_form": "우도주막",
          "speaker_id": "2",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017499.1.10",
          "form": "응 제주도 우도에서 촬영했대",
          "original_form": "응 제주도 우도에서 촬영했대",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017499.1.11",
          "form": "풍경구경도 하고 재밌던데",
          "original_form": "풍경구경도 하고 재밌던데",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017499.1.12",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017499.1.13",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017499.1.14",
          "form": "유퀴즈 이번주 누구나와?",
          "original_form": "유퀴즈 이번주 누구나와?",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017499.1.15",
          "form": "국가대표 나와",
          "original_form": "국가대표 나와",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017499.1.16",
          "form": "글쿤 요새 신곡 들어본거 있어?",
          "original_form": "글쿤 요새 신곡 들어본거 있어?",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017499.1.17",
          "form": "아니 들어본거 없어",
          "original_form": "아니 들어본거 없어",
          "speaker_id": "2",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017499.1.18",
          "form": "음악 최근에 뭐들었어?",
          "original_form": "음악 최근에 뭐들었어?",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017499.1.19",
          "form": "나는 레드벨벳 신곡 퀸덤 들었다",
          "original_form": "나는 레드벨벳 신곡 퀸덤 들었다",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017499.1.20",
          "form": "옛날노래 들었다",
          "original_form": "옛날노래 들었다",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017499.1.21",
          "form": "뭐?",
          "original_form": "뭐?",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017499.1.22",
          "form": "아무거나 다 좋아",
          "original_form": "아무거나 다 좋아",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017499.1.23",
          "form": "제목은 기억이 안나",
          "original_form": "제목은 기억이 안나",
