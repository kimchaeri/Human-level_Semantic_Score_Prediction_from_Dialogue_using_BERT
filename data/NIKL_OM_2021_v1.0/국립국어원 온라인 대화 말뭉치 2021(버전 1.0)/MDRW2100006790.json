      "utterance": [
        {
          "id": "MDRW2100006790.1.1",
          "form": "여행지가면 맛집 다니시나요",
          "original_form": "여행지가면 맛집 다니시나요",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100006790.1.2",
          "form": "네 맛집찾아다니는 묘미죠",
          "original_form": "네 맛집찾아다니는 묘미죠",
          "speaker_id": "2",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100006790.1.3",
          "form": "정보는 어디서 얻으세요?",
          "original_form": "정보는 어디서 얻으세요?",
          "speaker_id": "1",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100006790.1.4",
          "form": "주로 블로그죠 ㅎㅎ",
          "original_form": "주로 블로그죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100006790.1.5",
          "form": "어디서 찾으세요?",
          "original_form": "어디서 찾으세요?",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100006790.1.6",
          "form": "저는 네이버 방문자 리뷰요  ㅎ",
          "original_form": "저는 네이버 방문자 리뷰요  ㅎ",
          "speaker_id": "1",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100006790.1.7",
          "form": "그런곳이 있어요?",
          "original_form": "그런곳이 있어요?",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100006790.1.8",
          "form": "네 영수증 있어야만 인증가능해요 네이버 플레이스던가..",
          "original_form": "네 영수증 있어야만 인증가능해요 네이버 플레이스던가..",
          "speaker_id": "1",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100006790.1.9",
          "form": "아 마플이요",
          "original_form": "아 마플이요",
          "speaker_id": "2",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100006790.1.10",
          "form": "마이플레이스인가요?ㅎㅎ",
          "original_form": "마이플레이스인가요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100006790.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100006790.1.12",
          "form": "저도 해요",
          "original_form": "저도 해요",
          "speaker_id": "2",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100006790.1.13",
          "form": "영수증인증이라 믿고갔는데 실패한경우도 많아요 ㅠ",
          "original_form": "영수증인증이라 믿고갔는데 실패한경우도 많아요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 09:39"
        },
        {
          "id": "MDRW2100006790.1.14",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100006790.1.15",
          "form": "솔직히 쓰기 그러니",
          "original_form": "솔직히 쓰기 그러니",
          "speaker_id": "2",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100006790.1.16",
          "form": "100프로 믿진 못하겠어요",
          "original_form": "100프로 믿진 못하겠어요",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100006790.1.17",
          "form": "그래도 막연히 선택하기엔 어렵지만",
          "original_form": "그래도 막연히 선택하기엔 어렵지만",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100006790.1.18",
          "form": "참고자료로 도움되죠",
          "original_form": "참고자료로 도움되죠",
          "speaker_id": "2",
          "time": "20210908 10:42"
        }
      ]
