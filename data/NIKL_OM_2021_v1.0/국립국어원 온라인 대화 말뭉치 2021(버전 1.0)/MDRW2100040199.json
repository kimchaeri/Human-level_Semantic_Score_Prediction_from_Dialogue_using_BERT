      "utterance": [
        {
          "id": "MDRW2100040199.1.1",
          "form": "드라마 보새요?",
          "original_form": "드라마 보새요?",
          "speaker_id": "2",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100040199.1.2",
          "form": "아 종영 한 드라마 보고 있어요ㅎㄹ",
          "original_form": "아 종영 한 드라마 보고 있어요ㅎㄹ",
          "speaker_id": "1",
          "time": "20210904 19:22"
        },
        {
          "id": "MDRW2100040199.1.3",
          "form": "요즘 보는 드라마 있으세요?",
          "original_form": "요즘 보는 드라마 있으세요?",
          "speaker_id": "1",
          "time": "20210904 19:22"
        },
        {
          "id": "MDRW2100040199.1.4",
          "form": "저는 슬의생 챙겨봐요",
          "original_form": "저는 슬의생 챙겨봐요",
          "speaker_id": "2",
          "time": "20210906 14:23"
        },
        {
          "id": "MDRW2100040199.1.5",
          "form": "뭐보고계세요?",
          "original_form": "뭐보고계세요?",
          "speaker_id": "2",
          "time": "20210906 14:23"
        },
        {
          "id": "MDRW2100040199.1.6",
          "form": "저는 보이스4 보고 있어요!",
          "original_form": "저는 보이스4 보고 있어요!",
          "speaker_id": "1",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100040199.1.7",
          "form": "보신적 있으신가요?",
          "original_form": "보신적 있으신가요?",
          "speaker_id": "1",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100040199.1.8",
          "form": "오 들어는 봠ㅅ는대",
          "original_form": "오 들어는 봠ㅅ는대",
          "speaker_id": "2",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100040199.1.9",
          "form": "재밌다는 말 꽤 들었거든요",
          "original_form": "재밌다는 말 꽤 들었거든요",
          "speaker_id": "2",
          "time": "20210906 16:24"
        },
        {
          "id": "MDRW2100040199.1.10",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "2",
          "time": "20210906 16:24"
        },
        {
          "id": "MDRW2100040199.1.11",
          "form": "스릴 있어서 재밌어요!!",
          "original_form": "스릴 있어서 재밌어요!!",
          "speaker_id": "1",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100040199.1.12",
          "form": "시즌4 초반은 밤에 몰아서 봤는데 무섭더라구요ㅎㅎ",
          "original_form": "시즌4 초반은 밤에 몰아서 봤는데 무섭더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100040199.1.13",
          "form": "무서운거 잘보시나요?",
          "original_form": "무서운거 잘보시나요?",
          "speaker_id": "1",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100040199.1.14",
          "form": "그럭저럭 보는편이애요",
          "original_form": "그럭저럭 보는편이애요",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100040199.1.15",
          "form": "운동할때 맨날 넷플보는데 한번 달려봐야겠어요",
          "original_form": "운동할때 맨날 넷플보는데 한번 달려봐야겠어요",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100040199.1.16",
          "form": "저 오늘 영화보고왔어요!",
          "original_form": "저 오늘 영화보고왔어요!",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100040199.1.17",
          "form": "오 무슨 영화 보셨나요?",
          "original_form": "오 무슨 영화 보셨나요?",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100040199.1.18",
          "form": "코다라고 라라랜드 감독 신작일거예요",
          "original_form": "코다라고 라라랜드 감독 신작일거예요",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100040199.1.19",
          "form": "근데 진짜 찡 ㅠㅠㅠ",
          "original_form": "근데 진짜 찡 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100040199.1.20",
          "form": "오 슬픈 내용인가요??!@",
          "original_form": "오 슬픈 내용인가요??!@",
          "speaker_id": "1",
          "time": "20210906 23:44"
        },
        {
          "id": "MDRW2100040199.1.21",
          "form": "감동적이에요 ㅠㅠ",
          "original_form": "감동적이에요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 08:01"
        },
        {
          "id": "MDRW2100040199.1.22",
          "form": "마음이 울리는. ...그런...",
          "original_form": "마음이 울리는. ...그런...",
          "speaker_id": "2",
          "time": "20210907 08:01"
        },
        {
          "id": "MDRW2100040199.1.23",
          "form": "완전 로맨스 영환줄 알았는데 가족영화에 가깝더라고요",
          "original_form": "완전 로맨스 영환줄 알았는데 가족영화에 가깝더라고요",
          "speaker_id": "2",
          "time": "20210907 08:02"
        },
        {
          "id": "MDRW2100040199.1.24",
          "form": "오 추천 하시나요?!",
          "original_form": "오 추천 하시나요?!",
          "speaker_id": "1",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100040199.1.25",
          "form": "네 너무요 ㅠㅠ",
          "original_form": "네 너무요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100040199.1.26",
          "form": "제 인생영화됏어요..",
          "original_form": "제 인생영화됏어요..",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100040199.1.27",
          "form": "오 꼭 볼게요!!@",
          "original_form": "오 꼭 볼게요!!@",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100040199.1.28",
          "form": "코다!!!",
