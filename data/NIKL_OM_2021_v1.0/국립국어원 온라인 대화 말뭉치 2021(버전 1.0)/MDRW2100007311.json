      "utterance": [
        {
          "id": "MDRW2100007311.1.1",
          "form": "백신 신청하셨어요?",
          "original_form": "백신 신청하셨어요?",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007311.1.2",
          "form": "네 신청했어요",
          "original_form": "네 신청했어요",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007311.1.3",
          "form": "하셨어요?",
          "original_form": "하셨어요?",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007311.1.4",
          "form": "전 9월 30일로 예약했어요",
          "original_form": "전 9월 30일로 예약했어요",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007311.1.5",
          "form": "맞기 겁나요",
          "original_form": "맞기 겁나요",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007311.1.6",
          "form": "저는 9월 24일이요",
          "original_form": "저는 9월 24일이요",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007311.1.7",
          "form": "금욜인가요?",
          "original_form": "금욜인가요?",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007311.1.8",
          "form": "추석지나고 2일 연달아 휴가내고 맞으려구요",
          "original_form": "추석지나고 2일 연달아 휴가내고 맞으려구요",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007311.1.9",
          "form": "네 금요일이요",
          "original_form": "네 금요일이요",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007311.1.10",
          "form": "아 추석낀주군요",
          "original_form": "아 추석낀주군요",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007311.1.11",
          "form": "아직 화이자인지 모더나인지 알수없죠?",
          "original_form": "아직 화이자인지 모더나인지 알수없죠?",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007311.1.12",
          "form": "네일부러 신랑이랑 같이 맞으러가려구 그렇게했어요",
          "original_form": "네일부러 신랑이랑 같이 맞으러가려구 그렇게했어요",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007311.1.13",
          "form": "오늘 신랑말로는...",
          "original_form": "오늘 신랑말로는...",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007311.1.14",
          "form": "어디근거인지는 모르겠는데요",
          "original_form": "어디근거인지는 모르겠는데요",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007311.1.15",
          "form": "수도권은 화이자 지방은 모더나 라고 그러더라구요",
          "original_form": "수도권은 화이자 지방은 모더나 라고 그러더라구요",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007311.1.16",
          "form": "아 대전이 모더나 맞았다던데 그런가보네요",
          "original_form": "아 대전이 모더나 맞았다던데 그런가보네요",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007311.1.17",
          "form": "부작용이 모더나가 더많긴하던데 둘다 괜찮겠죠?",
          "original_form": "부작용이 모더나가 더많긴하던데 둘다 괜찮겠죠?",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007311.1.18",
          "form": "다들 모더나 맞으려고하던데요",
          "original_form": "다들 모더나 맞으려고하던데요",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007311.1.19",
          "form": "부작용이 작데요",
          "original_form": "부작용이 작데요",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007311.1.20",
          "form": "오그래요?",
          "original_form": "오그래요?",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007311.1.21",
          "form": "아직 맞은사람 없어서 그런거아니구요?",
          "original_form": "아직 맞은사람 없어서 그런거아니구요?",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007311.1.22",
          "form": "제주변에는 모더나맞고싶어하는사람들 뿐이예요",
          "original_form": "제주변에는 모더나맞고싶어하는사람들 뿐이예요",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007311.1.23",
          "form": "간호사언니도 모더나  맞고싶다더니",
          "original_form": "간호사언니도 모더나  맞고싶다더니",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007311.1.24",
          "form": "지방으로 가야되나요ㅎㅎ",
          "original_form": "지방으로 가야되나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007311.1.25",
          "form": "화이자였는데...아는병원에 모더나있다고 연락와서 취소하고 그병원가서 맞고왔어요",
          "original_form": "화이자였는데...아는병원에 모더나있다고 연락와서 취소하고 그병원가서 맞고왔어요",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007311.1.26",
          "form": "전 무서워서 그때가서 맞을지 고민을",
          "original_form": "전 무서워서 그때가서 맞을지 고민을",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007311.1.27",
          "form": "저는 일단 맞으려구요",
          "original_form": "저는 일단 맞으려구요",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007311.1.28",
          "form": "대부분은  이상없는거 같긴해요",
          "original_form": "대부분은  이상없는거 같긴해요",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007311.1.29",
          "form": "후기 들려주세요^^",
          "original_form": "후기 들려주세요^^",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007311.1.30",
          "form": "또 회사동기언니는 사이버상에서 알게된 간호사한테 이야기해서 모더나 오늘맞았다고 연락오더라구요",
          "original_form": "또 회사동기언니는 사이버상에서 알게된 간호사한테 이야기해서 모더나 오늘맞았다고 연락오더라구요",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007311.1.31",
          "form": "잔여백신 다들 있는데 안올리고",
          "original_form": "잔여백신 다들 있는데 안올리고",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007311.1.32",
          "form": "지인들 아님 이런 인맥으로 맞는 분위기예요",
          "original_form": "지인들 아님 이런 인맥으로 맞는 분위기예요",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007311.1.33",
          "form": "그런거 맞을거같아요",
          "original_form": "그런거 맞을거같아요",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007311.1.34",
          "form": "이젠 다예약되니 잔여안기다려도 되죠모ㅎ",
          "original_form": "이젠 다예약되니 잔여안기다려도 되죠모ㅎ",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007311.1.35",
          "form": "아제가 남아돈대요",
          "original_form": "아제가 남아돈대요",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007311.1.36",
          "form": "모더나인지 화이자인지 알수없는거보다",
          "original_form": "모더나인지 화이자인지 알수없는거보다",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007311.1.37",
          "form": "모더나 남았을때 맞고싶다는사람 많거든요",
          "original_form": "모더나 남았을때 맞고싶다는사람 많거든요",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007311.1.38",
          "form": "그럴수있겠네요",
          "original_form": "그럴수있겠네요",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007311.1.39",
          "form": "별증상 없대요?",
          "original_form": "별증상 없대요?",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007311.1.40",
          "form": "주변에 모더나는 없는거같아요",
          "original_form": "주변에 모더나는 없는거같아요",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007311.1.41",
          "form": "언니들은 다들 괜찮데요",
          "original_form": "언니들은 다들 괜찮데요",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007311.1.42",
          "form": "약도 안먹을만큼",
          "original_form": "약도 안먹을만큼",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007311.1.43",
          "form": "오  다행이네요",
          "original_form": "오  다행이네요",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007311.1.44",
          "form": "맞은 부위만 약간 얼얼하데요",
          "original_form": "맞은 부위만 약간 얼얼하데요",
          "speaker_id": "2",
          "time": "20210822 01:37"
        }
      ]
