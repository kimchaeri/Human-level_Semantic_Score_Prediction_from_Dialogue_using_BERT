      "utterance": [
        {
          "id": "MDRW2100002356.1.1",
          "form": "그러게요~",
          "original_form": "그러게요~",
          "speaker_id": "2",
          "time": "20210820 10:19"
        },
        {
          "id": "MDRW2100002356.1.2",
          "form": "요새는 비도 넘 갑자기 내려서 예측하기가 어렵네요",
          "original_form": "요새는 비도 넘 갑자기 내려서 예측하기가 어렵네요",
          "speaker_id": "2",
          "time": "20210820 10:19"
        },
        {
          "id": "MDRW2100002356.1.3",
          "form": "그렇죠 동남아같이 날씨가 바뀌는거 같아요",
          "original_form": "그렇죠 동남아같이 날씨가 바뀌는거 같아요",
          "speaker_id": "1",
          "time": "20210820 10:20"
        },
        {
          "id": "MDRW2100002356.1.4",
          "form": "그래도 요즘 미세먼지 이야기는 많이 안나오는거같아요",
          "original_form": "그래도 요즘 미세먼지 이야기는 많이 안나오는거같아요",
          "speaker_id": "1",
          "time": "20210820 10:20"
        },
        {
          "id": "MDRW2100002356.1.5",
          "form": "맞아요~! 그나마 미세먼지가 적어서 좋네요 ㅠ",
          "original_form": "맞아요~! 그나마 미세먼지가 적어서 좋네요 ㅠ",
          "speaker_id": "2",
          "time": "20210820 10:22"
        },
        {
          "id": "MDRW2100002356.1.6",
          "form": "진짜 넘 동남아 같아요~ 더운데 습하면서 덥고 ㅠㅠ",
          "original_form": "진짜 넘 동남아 같아요~ 더운데 습하면서 덥고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 10:22"
        },
        {
          "id": "MDRW2100002356.1.7",
          "form": "요즘 아이들은 안타까워요\r\n미세먼지땜에 못나가고 비와서 못나가고\r\n그러니 자꾸 게임 이런거에 현혹되는거같아요",
          "original_form": "요즘 아이들은 안타까워요\r\n미세먼지땜에 못나가고 비와서 못나가고\r\n그러니 자꾸 게임 이런거에 현혹되는거같아요",
          "speaker_id": "1",
          "time": "20210820 10:27"
        },
        {
          "id": "MDRW2100002356.1.8",
          "form": "이번에 비는 좀 시원하게 왔으면 좋겠어요",
          "original_form": "이번에 비는 좀 시원하게 왔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210820 10:29"
        },
        {
          "id": "MDRW2100002356.1.9",
          "form": "조금와서 습하게 만들지말고요",
          "original_form": "조금와서 습하게 만들지말고요",
          "speaker_id": "1",
          "time": "20210820 10:30"
        },
        {
          "id": "MDRW2100002356.1.10",
          "form": "마자요 ㅠㅠ 한번 올때 제대로  오고~ 시원하게 맑아지고~",
          "original_form": "마자요 ㅠㅠ 한번 올때 제대로  오고~ 시원하게 맑아지고~",
          "speaker_id": "2",
          "time": "20210820 10:50"
        },
        {
          "id": "MDRW2100002356.1.11",
          "form": "그러면 좋을텐데..!",
          "original_form": "그러면 좋을텐데..!",
          "speaker_id": "2",
          "time": "20210820 10:50"
        },
        {
          "id": "MDRW2100002356.1.12",
          "form": "가을도 좀 길면 좋겠어요 ㅠㅋㅋ 금방 겨울되지말고..",
          "original_form": "가을도 좀 길면 좋겠어요 ㅠㅋㅋ 금방 겨울되지말고..",
          "speaker_id": "2",
          "time": "20210820 10:50"
        },
        {
          "id": "MDRW2100002356.1.13",
          "form": "글쵸 가을 가을 가을 가을이였으면 좋겠어요",
          "original_form": "글쵸 가을 가을 가을 가을이였으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210820 10:52"
        },
        {
          "id": "MDRW2100002356.1.14",
          "form": "ㅋㅋ마자요! 가을 넘 좋아요 ㅠ 덥지도않고 춥지도 않고",
          "original_form": "ㅋㅋ마자요! 가을 넘 좋아요 ㅠ 덥지도않고 춥지도 않고",
          "speaker_id": "2",
          "time": "20210820 10:52"
        },
        {
          "id": "MDRW2100002356.1.15",
          "form": "어떤계절좋아하세요?",
          "original_form": "어떤계절좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 10:52"
        },
        {
          "id": "MDRW2100002356.1.16",
          "form": "저 봄/가을이요! ㅎㅎ 여름이 젤 싫어요 넘 더워서 ㅠㅋㅋ",
          "original_form": "저 봄/가을이요! ㅎㅎ 여름이 젤 싫어요 넘 더워서 ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 10:52"
        },
        {
          "id": "MDRW2100002356.1.17",
          "form": "저두요 봄가을이 딱 좋은거같아요",
          "original_form": "저두요 봄가을이 딱 좋은거같아요",
          "speaker_id": "1",
          "time": "20210820 10:53"
        },
        {
          "id": "MDRW2100002356.1.18",
          "form": "너무 덥고 추운거보다",
          "original_form": "너무 덥고 추운거보다",
          "speaker_id": "1",
          "time": "20210820 10:53"
        },
        {
          "id": "MDRW2100002356.1.19",
          "form": "그쵸~",
          "original_form": "그쵸~",
          "speaker_id": "2",
          "time": "20210820 10:54"
        },
        {
          "id": "MDRW2100002356.1.20",
          "form": "올 겨울 넘 추울까봐 걱정이네요",
          "original_form": "올 겨울 넘 추울까봐 걱정이네요",
          "speaker_id": "2",
          "time": "20210820 10:54"
        },
        {
          "id": "MDRW2100002356.1.21",
          "form": "올겨울 많이 춥데요?",
          "original_form": "올겨울 많이 춥데요?",
          "speaker_id": "1",
          "time": "20210820 10:55"
        },
        {
          "id": "MDRW2100002356.1.22",
          "form": "모르겠어요! ㅠ",
          "original_form": "모르겠어요! ㅠ",
          "speaker_id": "2",
          "time": "20210820 10:56"
        },
        {
          "id": "MDRW2100002356.1.23",
          "form": "안춥길 바라는 마음..~",
          "original_form": "안춥길 바라는 마음..~",
          "speaker_id": "2",
          "time": "20210820 10:56"
        }
      ]
    }
  ]
