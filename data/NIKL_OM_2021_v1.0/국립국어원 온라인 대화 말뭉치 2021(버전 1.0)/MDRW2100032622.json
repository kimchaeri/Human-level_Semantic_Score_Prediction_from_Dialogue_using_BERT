      "utterance": [
        {
          "id": "MDRW2100032622.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100032622.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100032622.1.3",
          "form": "운동하시는 편인가요?",
          "original_form": "운동하시는 편인가요?",
          "speaker_id": "2",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100032622.1.4",
          "form": "산책이랑 홈트정도요ㅎㅎ",
          "original_form": "산책이랑 홈트정도요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100032622.1.5",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 21:32"
        },
        {
          "id": "MDRW2100032622.1.6",
          "form": "전 달리기해요!",
          "original_form": "전 달리기해요!",
          "speaker_id": "2",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100032622.1.7",
          "form": "홈트면 유튜브보고 하세요?",
          "original_form": "홈트면 유튜브보고 하세요?",
          "speaker_id": "2",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100032622.1.8",
          "form": "오와 달리기 멋있어오ㅡ",
          "original_form": "오와 달리기 멋있어오ㅡ",
          "speaker_id": "1",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100032622.1.9",
          "form": "넵 짧게해요ㅎㅎ15분짜리",
          "original_form": "넵 짧게해요ㅎㅎ15분짜리",
          "speaker_id": "1",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100032622.1.10",
          "form": "어떤 채널보세요?",
          "original_form": "어떤 채널보세요?",
          "speaker_id": "2",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100032622.1.11",
          "form": "미서원 플랭크라고 15분짜리 영상이요!",
          "original_form": "미서원 플랭크라고 15분짜리 영상이요!",
          "speaker_id": "1",
          "time": "20210905 21:38"
        },
        {
          "id": "MDRW2100032622.1.12",
          "form": "한번 봐봐야겠어요 ㅎㅎ",
          "original_form": "한번 봐봐야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:39"
        },
        {
          "id": "MDRW2100032622.1.13",
          "form": "플랭크는 올바른 동작이 중요한 것 같더라고요",
          "original_form": "플랭크는 올바른 동작이 중요한 것 같더라고요",
          "speaker_id": "2",
          "time": "20210905 21:40"
        },
        {
          "id": "MDRW2100032622.1.14",
          "form": "안그럼 무릎에 안좋다고 ㄷㄷ",
          "original_form": "안그럼 무릎에 안좋다고 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210905 21:40"
        },
        {
          "id": "MDRW2100032622.1.15",
          "form": "와 맞아요",
          "original_form": "와 맞아요",
          "speaker_id": "1",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032622.1.16",
          "form": "손목무릎어깨 다 부담될수있다하더라고요",
          "original_form": "손목무릎어깨 다 부담될수있다하더라고요",
          "speaker_id": "1",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032622.1.17",
          "form": "배에힘빡주라고ㅠㅜ",
          "original_form": "배에힘빡주라고ㅠㅜ",
          "speaker_id": "1",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032622.1.18",
          "form": "플랭크하면 무릎에 무리가는게 느껴지던데 하기 겁나네요",
          "original_form": "플랭크하면 무릎에 무리가는게 느껴지던데 하기 겁나네요",
          "speaker_id": "2",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032622.1.19",
          "form": "운동이 자세 잘못하면 오히려 몸에 부담갈 수 있다니 참 쉽지않은 것같아요",
          "original_form": "운동이 자세 잘못하면 오히려 몸에 부담갈 수 있다니 참 쉽지않은 것같아요",
          "speaker_id": "2",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100032622.1.20",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032622.1.21",
          "form": "건강하려고하는거지만",
          "original_form": "건강하려고하는거지만",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032622.1.22",
          "form": "어렵죠",
          "original_form": "어렵죠",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032622.1.23",
          "form": "네... 실은 달리기도 무릎에 무리간다고 듣기도 하고요",
          "original_form": "네... 실은 달리기도 무릎에 무리간다고 듣기도 하고요",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032622.1.24",
          "form": "그래서 필라테스 같은게 좋다는 얘기도 들은 것같아요 ㅋㅋ",
          "original_form": "그래서 필라테스 같은게 좋다는 얘기도 들은 것같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032622.1.25",
          "form": "오..저는유연성이없어서",
          "original_form": "오..저는유연성이없어서",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032622.1.26",
          "form": "그거좋을것같아요",
          "original_form": "그거좋을것같아요",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032622.1.27",
          "form": "몸에 무리도 안가고 말이죠 ㅎㅎ",
          "original_form": "몸에 무리도 안가고 말이죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100032622.1.28",
          "form": "대신 홈트랑 달리기에 비해서 비싼 운동이에요",
          "original_form": "대신 홈트랑 달리기에 비해서 비싼 운동이에요",
          "speaker_id": "2",
          "time": "20210905 21:57"
        }
      ]
    }
