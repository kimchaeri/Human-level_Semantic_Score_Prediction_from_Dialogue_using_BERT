      "utterance": [
        {
          "id": "MDRW2100044483.1.1",
          "form": "식사하셨어요??",
          "original_form": "식사하셨어요??",
          "speaker_id": "2",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100044483.1.2",
          "form": "지금 먹고 있어요:)",
          "original_form": "지금 먹고 있어요:)",
          "speaker_id": "1",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100044483.1.3",
          "form": "아침 드셨어요?",
          "original_form": "아침 드셨어요?",
          "speaker_id": "1",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100044483.1.4",
          "form": "네 저도 아이들 보내고",
          "original_form": "네 저도 아이들 보내고",
          "speaker_id": "2",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100044483.1.5",
          "form": "미역국해서 밥 먹었어요",
          "original_form": "미역국해서 밥 먹었어요",
          "speaker_id": "2",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100044483.1.6",
          "form": "뭐 드세요??",
          "original_form": "뭐 드세요??",
          "speaker_id": "2",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100044483.1.7",
          "form": "엄마가 해 준 밑반찬에 먹구있어요:)",
          "original_form": "엄마가 해 준 밑반찬에 먹구있어요:)",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100044483.1.8",
          "form": "소고기 미역국인가요? ",
          "original_form": "소고기 미역국인가요? {emoji:♡}",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100044483.1.9",
          "form": "네 소고기 미역국이요 ㅎㅎ",
          "original_form": "네 소고기 미역국이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100044483.1.10",
          "form": "저도 친정엄마가 오셔서 끓여주신거예요",
          "original_form": "저도 친정엄마가 오셔서 끓여주신거예요",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100044483.1.11",
          "form": "오 맛있겠어요:)",
          "original_form": "오 맛있겠어요:)",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100044483.1.12",
          "form": "저도 내일 남편 생일이라\r\n미역국 준비해야해요 ㅋㅋ",
          "original_form": "저도 내일 남편 생일이라\r\n미역국 준비해야해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100044483.1.13",
          "form": "아 신랑 생일이시군요",
          "original_form": "아 신랑 생일이시군요",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100044483.1.14",
          "form": "요리는 뭐하세요??",
          "original_form": "요리는 뭐하세요??",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100044483.1.15",
          "form": "그러게요ㅠㅋㅋ\r\n지금 메뉴 검색중요",
          "original_form": "그러게요ㅠㅋㅋ\r\n지금 메뉴 검색중요",
          "speaker_id": "1",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100044483.1.16",
          "form": "생일때 뭐 해주세요?",
          "original_form": "생일때 뭐 해주세요?",
          "speaker_id": "1",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100044483.1.17",
          "form": "전요리를 못해서",
          "original_form": "전요리를 못해서",
          "speaker_id": "2",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100044483.1.18",
          "form": "정말 미역국 밖에 못 끓여요",
          "original_form": "정말 미역국 밖에 못 끓여요",
          "speaker_id": "2",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100044483.1.19",
          "form": "전에 신랑 생일이라고",
          "original_form": "전에 신랑 생일이라고",
          "speaker_id": "2",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100044483.1.20",
          "form": "뭐 이것저것 주려고 하다가 몸살이 나서 ㅋㅋㅋㅋㅋ",
          "original_form": "뭐 이것저것 주려고 하다가 몸살이 나서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100044483.1.21",
          "form": "저도 잘하는건 아닌데\r\n그래도 생일이니 뭐라도\r\n해야할거 같은 압박감이 ㅋㅋ",
          "original_form": "저도 잘하는건 아닌데\r\n그래도 생일이니 뭐라도\r\n해야할거 같은 압박감이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100044483.1.22",
          "form": "그렇죠 ㅋㅋㅋㅋㅋ",
          "original_form": "그렇죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100044483.1.23",
          "form": "저도 뭔가 압박감이 느껴져서 하다가",
          "original_form": "저도 뭔가 압박감이 느껴져서 하다가",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100044483.1.24",
          "form": "몸살나서 ㅋㅋㅋㅋ",
          "original_form": "몸살나서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100044483.1.25",
          "form": "그뒤로 다시는 신랑이\r\n하지마라고 하더라구요",
          "original_form": "그뒤로 다시는 신랑이\r\n하지마라고 하더라구요",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100044483.1.26",
          "form": "ㅋㅋ저희 남편은\r\n집밥에 진심인 편이라ㅜㅜ",
          "original_form": "ㅋㅋ저희 남편은\r\n집밥에 진심인 편이라ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100044483.1.27",
          "form": "안차릴수도 없네요",
          "original_form": "안차릴수도 없네요",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100044483.1.28",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100044483.1.29",
          "form": "집밥을 좋아하시는군요",
          "original_form": "집밥을 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100044483.1.30",
          "form": "name1님은",
          "original_form": "&name1&님은",
          "speaker_id": "2",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100044483.1.31",
          "form": "요리를 굉장히 잘 하실 거 같아요",
          "original_form": "요리를 굉장히 잘 하실 거 같아요",
          "speaker_id": "2",
