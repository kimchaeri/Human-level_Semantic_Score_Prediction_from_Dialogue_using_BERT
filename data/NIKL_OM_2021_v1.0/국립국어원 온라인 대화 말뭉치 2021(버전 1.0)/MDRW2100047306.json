      "utterance": [
        {
          "id": "MDRW2100047306.1.1",
          "form": "최근 잘샀다 생각하시는 꿀템있으세요?ㅎ",
          "original_form": "최근 잘샀다 생각하시는 꿀템있으세요?ㅎ",
          "speaker_id": "1",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100047306.1.2",
          "form": "꿀템은 없ㄱᆢ 책상바꿨네요",
          "original_form": "꿀템은 없ㄱᆢ 책상바꿨네요",
          "speaker_id": "2",
          "time": "20210826 10:30"
        },
        {
          "id": "MDRW2100047306.1.3",
          "form": "오 공부하는 책상 같은거 말이신가요?",
          "original_form": "오 공부하는 책상 같은거 말이신가요?",
          "speaker_id": "1",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100047306.1.4",
          "form": "ㅎ 네 요즘꿀템은 뭐있나요?",
          "original_form": "ㅎ 네 요즘꿀템은 뭐있나요?",
          "speaker_id": "2",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100047306.1.5",
          "form": "와 저도 바꾸고싶네요 얼마정도 주셨어요?",
          "original_form": "와 저도 바꾸고싶네요 얼마정도 주셨어요?",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100047306.1.6",
          "form": "저는 꿀템은 아니고 아쿠아슈즈 샀는데 실망했어요ㅜ",
          "original_form": "저는 꿀템은 아니고 아쿠아슈즈 샀는데 실망했어요ㅜ",
          "speaker_id": "1",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100047306.1.7",
          "form": "아쿠아슈즈 ㅎㅎ",
          "original_form": "아쿠아슈즈 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100047306.1.8",
          "form": "책상 은 제껀아니고ㅋㅋ",
          "original_form": "책상 은 제껀아니고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100047306.1.9",
          "form": "저는 화장품바꿔야되긴하네요",
          "original_form": "저는 화장품바꿔야되긴하네요",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100047306.1.10",
          "form": "화장품은 쓰시는 브랜드있으세요?",
          "original_form": "화장품은 쓰시는 브랜드있으세요?",
          "speaker_id": "1",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100047306.1.11",
          "form": "아쿠아슈즈는 오직 물에서만 신어야하더라고요 발아픔",
          "original_form": "아쿠아슈즈는 오직 물에서만 신어야하더라고요 발아픔",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100047306.1.12",
          "form": "싼거써요ㅋㅋ",
          "original_form": "싼거써요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100047306.1.13",
          "form": "이니스프리",
          "original_form": "이니스프리",
          "speaker_id": "2",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100047306.1.14",
          "form": "아하 그러시다면 시드물도 추천이요",
          "original_form": "아하 그러시다면 시드물도 추천이요",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100047306.1.15",
          "form": "처음들어보는데 어디팔아요?",
          "original_form": "처음들어보는데 어디팔아요?",
          "speaker_id": "2",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100047306.1.16",
          "form": "인터넷이요 성분착해요",
          "original_form": "인터넷이요 성분착해요",
          "speaker_id": "1",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100047306.1.17",
          "form": "아ㅎ 찾아봐야겠어요ㅎ",
          "original_form": "아ㅎ 찾아봐야겠어요ㅎ",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100047306.1.18",
          "form": "아쿠아슈즈는 기대가 컸는데",
          "original_form": "아쿠아슈즈는 기대가 컸는데",
          "speaker_id": "1",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100047306.1.19",
          "form": "용도를 잘못생각한거같아요ㅜ",
          "original_form": "용도를 잘못생각한거같아요ㅜ",
          "speaker_id": "1",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100047306.1.20",
          "form": "그렇군요 요즘당근도 핫하던데ㅎ",
          "original_form": "그렇군요 요즘당근도 핫하던데ㅎ",
          "speaker_id": "2",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100047306.1.21",
          "form": "당근ㅋㅋ 어제도 소소한거 팔았어요",
          "original_form": "당근ㅋㅋ 어제도 소소한거 팔았어요",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100047306.1.22",
          "form": "당근 활용하면좋은데",
          "original_form": "당근 활용하면좋은데",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100047306.1.23",
          "form": "근데 이상하게 팔건없고 살건 많아요 ㅋㅋ",
          "original_form": "근데 이상하게 팔건없고 살건 많아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:07"
        },
        {
          "id": "MDRW2100047306.1.24",
          "form": "물건 좋은거많이나오듸라고요",
          "original_form": "물건 좋은거많이나오듸라고요",
          "speaker_id": "2",
          "time": "20210826 14:14"
        },
        {
          "id": "MDRW2100047306.1.25",
          "form": "쇼핑 이니스프리 시드물 당근",
          "original_form": "쇼핑 이니스프리 시드물 당근",
          "speaker_id": "1",
          "time": "20210826 18:28"
        }
      ]
    }
