        },
        {
          "id": "MDRW2100047397.1.4",
          "form": "아니요 졸업한 지 꽤 되었어요",
          "original_form": "아니요 졸업한 지 꽤 되었어요",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100047397.1.5",
          "form": "학창시절에 어떤 학생이셨나요",
          "original_form": "학창시절에 어떤 학생이셨나요",
          "speaker_id": "1",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100047397.1.6",
          "form": "그냥 이것저것 열심히 하던 거 같네요",
          "original_form": "그냥 이것저것 열심히 하던 거 같네요",
          "speaker_id": "2",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100047397.1.7",
          "form": "name1님은 학생인가요?",
          "original_form": "&name1&님은 학생인가요?",
          "speaker_id": "2",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100047397.1.8",
          "form": "백수예어ㅡ",
          "original_form": "백수예어ㅡ",
          "speaker_id": "1",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100047397.1.9",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100047397.1.10",
          "form": "이직 준비 중인 백수예요",
          "original_form": "이직 준비 중인 백수예요",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100047397.1.11",
          "form": "이직 준비 어떻게하고계신가요?",
          "original_form": "이직 준비 어떻게하고계신가요?",
          "speaker_id": "1",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100047397.1.12",
          "form": "자격증이랑 영어 공부 해요",
          "original_form": "자격증이랑 영어 공부 해요",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100047397.1.13",
          "form": "name1님은 준비 하시는 게 있나요?",
          "original_form": "&name1&님은 준비 하시는 게 있나요?",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100047397.1.14",
          "form": "그냥 쉬고있어요",
          "original_form": "그냥 쉬고있어요",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100047397.1.15",
          "form": "이전에 하시던 일 있었나요?",
          "original_form": "이전에 하시던 일 있었나요?",
