      "utterance": [
        {
          "id": "MDRW2100008373.1.1",
          "form": "name2 님 게임 좋아하시나요?",
          "original_form": "&name2& 님 게임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008373.1.2",
          "form": "가끔씩 하고있어요",
          "original_form": "가끔씩 하고있어요",
          "speaker_id": "2",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008373.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008373.1.4",
          "form": "스팀 게임 위주로 가끔해요",
          "original_form": "스팀 게임 위주로 가끔해요",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008373.1.5",
          "form": "스팀 게임 하시나요?",
          "original_form": "스팀 게임 하시나요?",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008373.1.6",
          "form": "저도 스팀게임 자주해요!",
          "original_form": "저도 스팀게임 자주해요!",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008373.1.7",
          "form": "돈스타브 아새요?",
          "original_form": "돈스타브 아새요?",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008373.1.8",
          "form": "아 진짜 좋아해요",
          "original_form": "아 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008373.1.9",
          "form": "재밌죠",
          "original_form": "재밌죠",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008373.1.10",
          "form": "서바이벌이나 캐주얼 게임 많이 해요",
          "original_form": "서바이벌이나 캐주얼 게임 많이 해요",
          "speaker_id": "1",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008373.1.11",
          "form": "아 서바이벌 보다는 생존 게임이 맞겠네요",
          "original_form": "아 서바이벌 보다는 생존 게임이 맞겠네요",
          "speaker_id": "1",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008373.1.12",
          "form": "래프트 아세요?",
          "original_form": "래프트 아세요?",
          "speaker_id": "1",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008373.1.13",
          "form": "이름은 들어봤는데",
          "original_form": "이름은 들어봤는데",
          "speaker_id": "2",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008373.1.14",
          "form": "무슨 개임이죠?",
          "original_form": "무슨 개임이죠?",
          "speaker_id": "2",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008373.1.15",
          "form": "땟목 키우면서 바다에서 생존하는 게임인데 이것도",
          "original_form": "땟목 키우면서 바다에서 생존하는 게임인데 이것도",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008373.1.16",
          "form": "친구들이랑 하면 재밌어요",
          "original_form": "친구들이랑 하면 재밌어요",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008373.1.17",
          "form": "아아 유튜브애서 봤어요!",
          "original_form": "아아 유튜브애서 봤어요!",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008373.1.18",
          "form": "재밌어 보이더라고요",
          "original_form": "재밌어 보이더라고요",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008373.1.19",
          "form": "요즘은 뭐지 휴먼 폼플렛?",
          "original_form": "요즘은 뭐지 휴먼 폼플렛?",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008373.1.20",
          "form": "그거 했어요",
          "original_form": "그거 했어요",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008373.1.21",
          "form": "아 네네",
          "original_form": "아 네네",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008373.1.22",
          "form": "그것도 재밌죠",
          "original_form": "그것도 재밌죠",
          "speaker_id": "1",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008373.1.23",
          "form": "조작이 좀 어려운데 캐릭터들이 흐물텅 거리는게 너무 웃겨서",
          "original_form": "조작이 좀 어려운데 캐릭터들이 흐물텅 거리는게 너무 웃겨서",
          "speaker_id": "1",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008373.1.24",
          "form": "처음에 한참 웃었던 거 같아요",
          "original_form": "처음에 한참 웃었던 거 같아요",
          "speaker_id": "1",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008373.1.25",
          "form": "조작 괜찮으셨나요?",
          "original_form": "조작 괜찮으셨나요?",
          "speaker_id": "1",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008373.1.26",
          "form": "예전에는 너무 어지러워서",
          "original_form": "예전에는 너무 어지러워서",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008373.1.27",
          "form": "한두시간하고 그만뒀었어요 ㅋㅋㅋ",
          "original_form": "한두시간하고 그만뒀었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008373.1.28",
          "form": "지금은 좀 나아지긴했는데",
          "original_form": "지금은 좀 나아지긴했는데",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008373.1.29",
          "form": "역시 고수없이는 어렵더라고요",
          "original_form": "역시 고수없이는 어렵더라고요",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008373.1.30",
          "form": "시점이 진짜 어지럽더라고요",
          "original_form": "시점이 진짜 어지럽더라고요",
          "speaker_id": "1",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008373.1.31",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008373.1.32",
          "form": "맞아요 고수 친구 한 명은 필요해요",
          "original_form": "맞아요 고수 친구 한 명은 필요해요",
          "speaker_id": "1",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008373.1.33",
          "form": "그 고릴라 포즈 아세요?",
          "original_form": "그 고릴라 포즈 아세요?",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008373.1.34",
          "form": "아 구애하는 섹시한 고릴라 포즈(?",
          "original_form": "아 구애하는 섹시한 고릴라 포즈(?",
          "speaker_id": "1",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008373.1.35",
          "form": "그거 하면 친구들 끼리 맨날 웃음보거 터져거",
          "original_form": "그거 하면 친구들 끼리 맨날 웃음보거 터져거",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008373.1.36",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008373.1.37",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008373.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:37"
        },
