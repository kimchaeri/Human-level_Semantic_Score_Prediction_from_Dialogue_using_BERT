      "utterance": [
        {
          "id": "MDRW2100019907.1.1",
          "form": "기숙사에서 어땠어요",
          "original_form": "기숙사에서 어땠어요",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019907.1.2",
          "form": "같이 공부하고",
          "original_form": "같이 공부하고",
          "speaker_id": "1",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019907.1.3",
          "form": "치긴머코?",
          "original_form": "치긴머코?",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019907.1.4",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019907.1.5",
          "form": "배달파티였어요",
          "original_form": "배달파티였어요",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019907.1.6",
          "form": "생각만해도 재밌겠어요",
          "original_form": "생각만해도 재밌겠어요",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019907.1.7",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019907.1.8",
          "form": "그리워요 그 시절이",
          "original_form": "그리워요 그 시절이",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019907.1.9",
          "form": "요즘은 어떠세요",
          "original_form": "요즘은 어떠세요",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019907.1.10",
          "form": "공부할까하는데 학생때 어떻게했나몰라요",
          "original_form": "공부할까하는데 학생때 어떻게했나몰라요",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019907.1.11",
          "form": "환경이 바뀌다보니 힘들죠",
          "original_form": "환경이 바뀌다보니 힘들죠",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019907.1.12",
          "form": "공부하는 데로 돌아갈까봐요",
          "original_form": "공부하는 데로 돌아갈까봐요",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019907.1.13",
          "form": "그것도 좋을거같아요",
          "original_form": "그것도 좋을거같아요",
          "speaker_id": "2",
          "time": "20210827 22:52"
