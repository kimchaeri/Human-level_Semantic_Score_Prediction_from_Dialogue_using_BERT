      "utterance": [
        {
          "id": "MDRW2100009065.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 20:54"
        },
        {
          "id": "MDRW2100009065.1.2",
          "form": "음식 뭐 좋아하세요?",
          "original_form": "음식 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 20:54"
        },
        {
          "id": "MDRW2100009065.1.3",
          "form": "저는 해산물종류 좋아해요 카이센동 회 이런거요",
          "original_form": "저는 해산물종류 좋아해요 카이센동 회 이런거요",
          "speaker_id": "2",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009065.1.4",
          "form": "뭐좋아하세요?",
          "original_form": "뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009065.1.5",
          "form": "저도 회 엄청 좋아해요!",
          "original_form": "저도 회 엄청 좋아해요!",
          "speaker_id": "1",
          "time": "20210822 21:33"
        },
        {
          "id": "MDRW2100009065.1.6",
          "form": "초밥사랑합니다",
          "original_form": "초밥사랑합니다",
          "speaker_id": "1",
          "time": "20210822 21:33"
        },
        {
          "id": "MDRW2100009065.1.7",
          "form": "회 너무 좋죠 어떤회 좋아하세요?",
          "original_form": "회 너무 좋죠 어떤회 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 21:50"
        },
        {
          "id": "MDRW2100009065.1.8",
          "form": "광어랑 우럭이요!",
          "original_form": "광어랑 우럭이요!",
          "speaker_id": "1",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100009065.1.9",
          "form": "부모님이 주로 그걸로 사주셨어요ㅋㅋㅋ",
          "original_form": "부모님이 주로 그걸로 사주셨어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100009065.1.10",
          "form": "광어 우럭 맛있죠ㅠㅠㅠ",
          "original_form": "광어 우럭 맛있죠ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100009065.1.11",
          "form": "광어는 겨울이 더 맛있지만 사계절내내 맛있어서 좋아요",
          "original_form": "광어는 겨울이 더 맛있지만 사계절내내 맛있어서 좋아요",
          "speaker_id": "2",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100009065.1.12",
          "form": "아 정말요?? 제가 엄청 많이 회를 접한건 아니여서 제철은 몰랐는데 광어가 겨울이 제철이군요!",
          "original_form": "아 정말요?? 제가 엄청 많이 회를 접한건 아니여서 제철은 몰랐는데 광어가 겨울이 제철이군요!",
          "speaker_id": "1",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009065.1.13",
          "form": "회는 맛있는데 너무 비싸서 아쉬워요ㅠ",
          "original_form": "회는 맛있는데 너무 비싸서 아쉬워요ㅠ",
          "speaker_id": "1",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009065.1.14",
          "form": "이번겨울에 광어맛을 좀 더 음미하면서 먹어봐야겠어요ㅋㅋㅋ",
          "original_form": "이번겨울에 광어맛을 좀 더 음미하면서 먹어봐야겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009065.1.15",
          "form": "그죠 근데 광어는 대부분 양식이라 크게 차이 안날꺼예요ㅋㅋㅋ 겨울에 밀치추천드려요",
          "original_form": "그죠 근데 광어는 대부분 양식이라 크게 차이 안날꺼예요ㅋㅋㅋ 겨울에 밀치추천드려요",
          "speaker_id": "2",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009065.1.16",
          "form": "회진짜 비싸죠ㅠㅠ",
          "original_form": "회진짜 비싸죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009065.1.17",
          "form": "한번씩 먹고싶은데 먹었다하면 몇만원훌쩍이라 부담스러워요",
          "original_form": "한번씩 먹고싶은데 먹었다하면 몇만원훌쩍이라 부담스러워요",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009065.1.18",
          "form": "밀치 한번 도전해보겠습니다! 추천감사해요!",
          "original_form": "밀치 한번 도전해보겠습니다! 추천감사해요!",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009065.1.19",
          "form": "회는 각잡고 먹어야 할거같아요ㅠㅠ",
          "original_form": "회는 각잡고 먹어야 할거같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009065.1.20",
          "form": "그죠 회말고는 뭐 좋아하시나요?",
          "original_form": "그죠 회말고는 뭐 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009065.1.21",
          "form": "떡볶이 좋아합니다",
          "original_form": "떡볶이 좋아합니다",
          "speaker_id": "1",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009065.1.22",
          "form": "엽떡 신전 죠스 떡슐랭 다잘먹어요!",
          "original_form": "엽떡 신전 죠스 떡슐랭 다잘먹어요!",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009065.1.23",
          "form": "떡볶이 좋아하는 친구말로는 요즘 로제가 맛있다는데 로제떡볶이도 좋아하시나요?",
          "original_form": "떡볶이 좋아하는 친구말로는 요즘 로제가 맛있다는데 로제떡볶이도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009065.1.24",
          "form": "로제떡볶이 진짜 맛있어요",
          "original_form": "로제떡볶이 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100009065.1.25",
          "form": "살많이 찐다고 해서 가끔 먹는데 살찌는맛이에요",
          "original_form": "살많이 찐다고 해서 가끔 먹는데 살찌는맛이에요",
          "speaker_id": "1",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100009065.1.26",
          "form": "너무 맛있어서요ㅋㅋㅋ",
          "original_form": "너무 맛있어서요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100009065.1.27",
          "form": "호불호 안갈리는 맛있가봐요 다들 맛있다고 그러네요",
          "original_form": "호불호 안갈리는 맛있가봐요 다들 맛있다고 그러네요",
          "speaker_id": "2",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009065.1.28",
          "form": "만약에 느끼한거 싫어하시는 분이라면 매운맛으로 드시면 딱 적당해요",
          "original_form": "만약에 느끼한거 싫어하시는 분이라면 매운맛으로 드시면 딱 적당해요",
          "speaker_id": "1",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009065.1.29",
          "form": "뭔가 취향에 따라 느끼한걸 조절할 수 있어서 인기가 많나봐요",
          "original_form": "뭔가 취향에 따라 느끼한걸 조절할 수 있어서 인기가 많나봐요",
          "speaker_id": "1",
          "time": "20210822 22:46"
        }
      ]
    }
