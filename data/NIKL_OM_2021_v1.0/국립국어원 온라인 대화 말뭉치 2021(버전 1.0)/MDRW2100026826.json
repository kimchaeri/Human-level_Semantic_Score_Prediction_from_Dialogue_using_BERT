      "utterance": [
        {
          "id": "MDRW2100026826.1.1",
          "form": "나는 코난 기다리면서",
          "original_form": "나는 코난 기다리면서",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100026826.1.2",
          "form": "괴담레스토랑 봤거든",
          "original_form": "괴담레스토랑 봤거든",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100026826.1.3",
          "form": "근거 보면서 진짜 사람보다 귀신이 더 무서웠음",
          "original_form": "근거 보면서 진짜 사람보다 귀신이 더 무서웠음",
          "speaker_id": "2",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100026826.1.4",
          "form": "코난은 범인 잡히기라도 하지",
          "original_form": "코난은 범인 잡히기라도 하지",
          "speaker_id": "2",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100026826.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100026826.1.6",
          "form": "넌 어릴때 뭐 봤니",
          "original_form": "넌 어릴때 뭐 봤니",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100026826.1.7",
          "form": "어릴때부터 코난을봤구나...짱이더",
          "original_form": "어릴때부터 코난을봤구나...짱이더",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100026826.1.8",
          "form": "난 골고루 다봤지",
          "original_form": "난 골고루 다봤지",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100026826.1.9",
          "form": "아 코난 처음 봤을때가 11살때였지",
          "original_form": "아 코난 처음 봤을때가 11살때였지",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100026826.1.10",
          "form": "근데 난 애니보단 테런했음...ㅋ...ㅎ",
          "original_form": "근데 난 애니보단 테런했음...ㅋ...ㅎ",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100026826.1.11",
          "form": "딱 10년 됐네",
          "original_form": "딱 10년 됐네",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100026826.1.12",
          "form": "와...나도 10살때 테런해서 딱 10년됐는제",
          "original_form": "와...나도 10살때 테런해서 딱 10년됐는제",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100026826.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100026826.1.14",
          "form": "암튼 어릴때",
          "original_form": "암튼 어릴때",
          "speaker_id": "2",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100026826.1.15",
          "form": "투니버스 좋아했다 난",
          "original_form": "투니버스 좋아했다 난",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.16",
          "form": "헐나도",
          "original_form": "헐나도",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.17",
          "form": "맞제",
          "original_form": "맞제",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.18",
          "form": "나 캐캐체",
          "original_form": "나 캐캐체",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.19",
          "form": "광인이었음",
          "original_form": "광인이었음",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.20",
          "form": "투니버스가 진짜 재밌었음",
          "original_form": "투니버스가 진짜 재밌었음",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.21",
          "form": "와 나 그거 한번도 본적 없다",
          "original_form": "와 나 그거 한번도 본적 없다",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.22",
          "form": "캐캐체다이어리 캐캐체스티커북 다쓸어모으고",
          "original_form": "캐캐체다이어리 캐캐체스티커북 다쓸어모으고",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.23",
          "form": "그거 학종이(?)",
          "original_form": "그거 학종이(?)",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.24",
          "form": "아진짜ㅋㅋㅋㅋㅋ?",
          "original_form": "아진짜ㅋㅋㅋㅋㅋ?",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.25",
          "form": "그것만 알고 있음",
          "original_form": "그것만 알고 있음",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.26",
          "form": "뭐 쇼콜라 바닐라",
          "original_form": "뭐 쇼콜라 바닐라",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.28",
          "form": "이런애들 아님?",
          "original_form": "이런애들 아님?",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.29",
          "form": "아 그ㅓㄴ",
          "original_form": "아 그ㅓㄴ",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.30",
          "form": "꿈빛파티시엘아님?",
          "original_form": "꿈빛파티시엘아님?",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.31",
          "form": "꿈파도 많이봤지",
          "original_form": "꿈파도 많이봤지",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100026826.1.32",
          "form": "근데 저런 애니들",
          "original_form": "근데 저런 애니들",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100026826.1.33",
          "form": "엥 꿈파라고????",
          "original_form": "엥 꿈파라고????",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100026826.1.34",
          "form": "ㅇㅇㅇ감딸기 나오는거ㅓㅣ",
          "original_form": "ㅇㅇㅇ감딸기 나오는거ㅓㅣ",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100026826.1.35",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100026826.1.36",
          "form": "그게 뭐야",
          "original_form": "그게 뭐야",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100026826.1.37",
          "form": "꿈파 주인공이름ㅋㅋㅋㅋㅋ",
          "original_form": "꿈파 주인공이름ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100026826.1.38",
          "form": "난 오직 도라에몽-코난 루트였음",
          "original_form": "난 오직 도라에몽-코난 루트였음",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100026826.1.39",
          "form": "저런 애니들 일본에서는 시리즈 더많이 나와잇고",
          "original_form": "저런 애니들 일본에서는 시리즈 더많이 나와잇고",
          "speaker_id": "1",
          "time": "20210902 23:21"
        }
      ]
    }
