      "utterance": [
        {
          "id": "MDRW2100022747.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022747.1.2",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022747.1.3",
          "form": "아직 안했어요",
          "original_form": "아직 안했어요",
          "speaker_id": "2",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022747.1.4",
          "form": "요즘 탄산수를 너무 마셔서 좀 걱정이에요",
          "original_form": "요즘 탄산수를 너무 마셔서 좀 걱정이에요",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022747.1.5",
          "form": "메뉴는요?",
          "original_form": "메뉴는요?",
          "speaker_id": "1",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022747.1.6",
          "form": "생각중?",
          "original_form": "생각중?",
          "speaker_id": "1",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022747.1.7",
          "form": "저녁메뉴 글쎄요. 아침에 나물반찬 해놓은거 있어서 호박된장찌개 해서 먹으려구요.",
          "original_form": "저녁메뉴 글쎄요. 아침에 나물반찬 해놓은거 있어서 호박된장찌개 해서 먹으려구요.",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022747.1.8",
          "form": "맛나겠네녀",
          "original_form": "맛나겠네녀",
          "speaker_id": "1",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022747.1.9",
          "form": "ㅎㅎ 가지찜.죽순나물 두가지 해놨거든요.",
          "original_form": "ㅎㅎ 가지찜.죽순나물 두가지 해놨거든요.",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022747.1.10",
          "form": "님은 저녁메뉴가 뭐예요?",
          "original_form": "님은 저녁메뉴가 뭐예요?",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022747.1.11",
          "form": "만두오",
          "original_form": "만두오",
          "speaker_id": "1",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100022747.1.12",
          "form": "만두 맛있겠어요.",
          "original_form": "만두 맛있겠어요.",
          "speaker_id": "2",
          "time": "20210829 18:08"
        },
        {
          "id": "MDRW2100022747.1.13",
          "form": "저는 비비고만두 좋아하는데",
          "original_form": "저는 비비고만두 좋아하는데",
          "speaker_id": "2",
          "time": "20210829 18:08"
        },
        {
          "id": "MDRW2100022747.1.14",
          "form": "어디꺼 좋아하세요?",
          "original_form": "어디꺼 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 18:08"
        },
        {
          "id": "MDRW2100022747.1.15",
          "form": "저도 비비거요",
          "original_form": "저도 비비거요",
          "speaker_id": "1",
          "time": "20210829 18:09"
        },
        {
          "id": "MDRW2100022747.1.16",
          "form": "비비고 냉동식품 제 입맛엔 다 맛있는거 같아요.",
          "original_form": "비비고 냉동식품 제 입맛엔 다 맛있는거 같아요.",
          "speaker_id": "2",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100022747.1.17",
          "form": "비비고곤드레나물밥 드셔보셨어요?",
          "original_form": "비비고곤드레나물밥 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210829 18:16"
        },
        {
          "id": "MDRW2100022747.1.18",
          "form": "그것도 맛있더라구요.",
          "original_form": "그것도 맛있더라구요.",
          "speaker_id": "2",
          "time": "20210829 18:16"
        },
        {
          "id": "MDRW2100022747.1.19",
          "form": "요즘 꽃게가 제철인지 마트마다 꽃게 팔던데요. 꽃게 좋아하세요?",
          "original_form": "요즘 꽃게가 제철인지 마트마다 꽃게 팔던데요. 꽃게 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022747.1.20",
          "form": "엄청요",
          "original_form": "엄청요",
          "speaker_id": "1",
          "time": "20210831 18:57"
        },
        {
          "id": "MDRW2100022747.1.21",
          "form": "요즘 꽃게 완전 달고 맛있어요.",
          "original_form": "요즘 꽃게 완전 달고 맛있어요.",
          "speaker_id": "2",
          "time": "20210831 19:19"
        },
        {
          "id": "MDRW2100022747.1.22",
          "form": "요리를 잘 못해요",
          "original_form": "요리를 잘 못해요",
          "speaker_id": "1",
