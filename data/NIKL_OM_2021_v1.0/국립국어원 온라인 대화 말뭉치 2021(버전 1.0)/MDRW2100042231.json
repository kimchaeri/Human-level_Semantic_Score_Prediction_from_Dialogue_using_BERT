      "utterance": [
        {
          "id": "MDRW2100042231.1.1",
          "form": "저도 예전에 필라테스했었는데 쉬고있어요",
          "original_form": "저도 예전에 필라테스했었는데 쉬고있어요",
          "speaker_id": "2",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100042231.1.2",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100042231.1.3",
          "form": "필라테스가 은근히 건강에 좋더라고요",
          "original_form": "필라테스가 은근히 건강에 좋더라고요",
          "speaker_id": "1",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100042231.1.4",
          "form": "근육을 풀어줘서",
          "original_form": "근육을 풀어줘서",
          "speaker_id": "1",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100042231.1.5",
          "form": "키도 크고요!",
          "original_form": "키도 크고요!",
          "speaker_id": "2",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042231.1.6",
          "form": "속근육에 좋죠",
          "original_form": "속근육에 좋죠",
          "speaker_id": "2",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042231.1.7",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042231.1.8",
          "form": "이번에 건강검진 하는데",
          "original_form": "이번에 건강검진 하는데",
          "speaker_id": "1",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042231.1.9",
          "form": "키 컸나 확인해봐야겠어욧ㅎㅎ",
          "original_form": "키 컸나 확인해봐야겠어욧ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042231.1.10",
          "form": "네! 숨겨진 키가 뽀로롱!!!",
          "original_form": "네! 숨겨진 키가 뽀로롱!!!",
          "speaker_id": "2",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042231.1.11",
          "form": "요가도 해보셨어요?",
          "original_form": "요가도 해보셨어요?",
          "speaker_id": "2",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100042231.1.12",
          "form": "기대되네욯ㅎ",
          "original_form": "기대되네욯ㅎ",
          "speaker_id": "1",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100042231.1.13",
          "form": "요가는 그냥 기초정도요",
          "original_form": "요가는 그냥 기초정도요",
          "speaker_id": "1",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100042231.1.14",
          "form": "요가도 탄력받고 하면 재밌더라고요",
          "original_form": "요가도 탄력받고 하면 재밌더라고요",
          "speaker_id": "2",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100042231.1.15",
          "form": "아 요가도 좋죠",
          "original_form": "아 요가도 좋죠",
          "speaker_id": "1",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100042231.1.16",
          "form": "심신건강에 이롭잖아요",
          "original_form": "심신건강에 이롭잖아요",
          "speaker_id": "1",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100042231.1.17",
          "form": "네 근데 안하다보니 다 굳어버렸어요ㅜㅠ",
          "original_form": "네 근데 안하다보니 다 굳어버렸어요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210906 20:58"
        },
        {
          "id": "MDRW2100042231.1.18",
          "form": "아 그쵸ㅜ",
          "original_form": "아 그쵸ㅜ",
          "speaker_id": "1",
          "time": "20210906 20:58"
        },
        {
          "id": "MDRW2100042231.1.19",
          "form": "운동도 꾸준히 해야",
          "original_form": "운동도 꾸준히 해야",
          "speaker_id": "1",
          "time": "20210906 20:58"
        },
        {
          "id": "MDRW2100042231.1.20",
          "form": "맞아요 안되던 동작 하는 재미가 있어요",
          "original_form": "맞아요 안되던 동작 하는 재미가 있어요",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042231.1.21",
          "form": "늘더라고요",
          "original_form": "늘더라고요",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042231.1.22",
          "form": "네 조금씩 느는 것도 재미있고",
          "original_form": "네 조금씩 느는 것도 재미있고",
          "speaker_id": "1",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042231.1.23",
          "form": "건강해지는 느낌도 좋고요",
          "original_form": "건강해지는 느낌도 좋고요",
          "speaker_id": "1",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042231.1.24",
          "form": "맞아요 자기관리하는 내모습에 심취하게 돼요ㅋㅋ",
          "original_form": "맞아요 자기관리하는 내모습에 심취하게 돼요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:00"
        },
        {
          "id": "MDRW2100042231.1.25",
          "form": "ㅋㅋ맞아요",
          "original_form": "ㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210906 21:04"
        },
        {
          "id": "MDRW2100042231.1.26",
          "form": "건강을 챙기는 멋진 나자신",
          "original_form": "건강을 챙기는 멋진 나자신",
          "speaker_id": "1",
          "time": "20210906 21:04"
        },
