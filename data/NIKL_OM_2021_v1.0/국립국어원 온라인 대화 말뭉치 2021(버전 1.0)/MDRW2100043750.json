      "utterance": [
        {
          "id": "MDRW2100043750.1.1",
          "form": "아 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043750.1.2",
          "form": "둘이 별 차이 안나서 그랬군요",
          "original_form": "둘이 별 차이 안나서 그랬군요",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043750.1.3",
          "form": "작은애 수유쿠션에서 젖 먹으면 지도 수유쿠션에 누워야 했고요 ㅠ",
          "original_form": "작은애 수유쿠션에서 젖 먹으면 지도 수유쿠션에 누워야 했고요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043750.1.4",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043750.1.5",
          "form": "키우는동안 고생하셨겠어요",
          "original_form": "키우는동안 고생하셨겠어요",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043750.1.6",
          "form": "네 엄청요 ㅠ 이게 사람이 둘을 키울수 있는게 맞나 싶었어요",
          "original_form": "네 엄청요 ㅠ 이게 사람이 둘을 키울수 있는게 맞나 싶었어요",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043750.1.7",
          "form": "저는 지금 둘째 이상 있는 분들이 존경스럽답니다",
          "original_form": "저는 지금 둘째 이상 있는 분들이 존경스럽답니다",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043750.1.8",
          "form": "곧 그 존경스러운 분이 되시잖아요 ㅋㅋ",
          "original_form": "곧 그 존경스러운 분이 되시잖아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043750.1.9",
          "form": "당분간 제정신 아닌체 지내겠죠?...ㅠㅠ",
          "original_form": "당분간 제정신 아닌체 지내겠죠?...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043750.1.10",
          "form": "당분간 우리 연락 안되는건가요???",
          "original_form": "당분간 우리 연락 안되는건가요???",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043750.1.11",
          "form": "아니되옵니다",
          "original_form": "아니되옵니다",
          "speaker_id": "2",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043750.1.12",
          "form": "전 항상 대기중이니 name2님이 잘 나와주시면 됩니다 ㅎㄹ",
          "original_form": "전 항상 대기중이니 &name2&님이 잘 나와주시면 됩니다 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043750.1.13",
          "form": "세상과 소통할 수 있는 창구인데",
          "original_form": "세상과 소통할 수 있는 창구인데",
          "speaker_id": "2",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043750.1.14",
          "form": "애 키우면 어른 사람과 엄청 말하고 싶잖아요 ㅎ",
          "original_form": "애 키우면 어른 사람과 엄청 말하고 싶잖아요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043750.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
