      "utterance": [
        {
          "id": "MDRW2100040486.1.1",
          "form": "꿈님 저녁식사 하셨나용",
          "original_form": "꿈님 저녁식사 하셨나용",
          "speaker_id": "1",
          "time": "20210904 21:45"
        },
        {
          "id": "MDRW2100040486.1.2",
          "form": "네 먹었어요ㅎㅎ",
          "original_form": "네 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:46"
        },
        {
          "id": "MDRW2100040486.1.3",
          "form": "뭐 드셨어용?",
          "original_form": "뭐 드셨어용?",
          "speaker_id": "1",
          "time": "20210904 22:38"
        },
        {
          "id": "MDRW2100040486.1.4",
          "form": "저 찜닭 먹었어요",
          "original_form": "저 찜닭 먹었어요",
          "speaker_id": "2",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040486.1.5",
          "form": "우와! 매운거요? 아님 간장이요?",
          "original_form": "우와! 매운거요? 아님 간장이요?",
          "speaker_id": "1",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040486.1.6",
          "form": "저 로제랑 묵은지찜닭 먹었어요",
          "original_form": "저 로제랑 묵은지찜닭 먹었어요",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040486.1.7",
          "form": "앗 너무 틀렸는데요 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 너무 틀렸는데요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040486.1.8",
          "form": "로제는 상상이 가는데 묵은지 찜닭은 뭐예요?",
          "original_form": "로제는 상상이 가는데 묵은지 찜닭은 뭐예요?",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040486.1.9",
          "form": "묵은지찜닭은 김치찜 느낌이에요\r\n국물이 졸은 김치찌개 느낌도 나구요",
          "original_form": "묵은지찜닭은 김치찜 느낌이에요\r\n국물이 졸은 김치찌개 느낌도 나구요",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040486.1.10",
          "form": "오!! 로제랑 묵은지 중에 뭐가 더 맛있었어요?",
          "original_form": "오!! 로제랑 묵은지 중에 뭐가 더 맛있었어요?",
          "speaker_id": "1",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100040486.1.11",
          "form": "저는 오늘은 로제가 좀 더 입맛에 맞았어요ㅎㅎ",
          "original_form": "저는 오늘은 로제가 좀 더 입맛에 맞았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100040486.1.12",
          "form": "마싯겠다아아",
          "original_form": "마싯겠다아아",
          "speaker_id": "1",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040486.1.13",
          "form": "그럼 모든 찜닭중에 로제가 짱인가용?",
          "original_form": "그럼 모든 찜닭중에 로제가 짱인가용?",
          "speaker_id": "1",
          "time": "20210904 22:58"
        },
        {
          "id": "MDRW2100040486.1.14",
          "form": "그런건 아니에요",
          "original_form": "그런건 아니에요",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040486.1.15",
          "form": "사실 찜닭을 자주 먹는 편이 아니라서.. \r\n다른 찜닭은 잘 모르겠어요ㅜㅜ",
          "original_form": "사실 찜닭을 자주 먹는 편이 아니라서.. \r\n다른 찜닭은 잘 모르겠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100040486.1.16",
          "form": "아하 그러셨구나 저는 저번에 간장양념인거 시켜먹었는데",
          "original_form": "아하 그러셨구나 저는 저번에 간장양념인거 시켜먹었는데",
          "speaker_id": "1",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100040486.1.17",
          "form": "생각보다 맛없었어요..ㅠㅠㅠ",
          "original_form": "생각보다 맛없었어요..ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100040486.1.18",
          "form": "그리고 그 두꺼운 당면이 들어있었는데",
          "original_form": "그리고 그 두꺼운 당면이 들어있었는데",
          "speaker_id": "1",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100040486.1.19",
          "form": "먹기가 힘들었어요",
          "original_form": "먹기가 힘들었어요",
          "speaker_id": "1",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100040486.1.20",
          "form": "두꺼운 당면 좋아하세요?",
          "original_form": "두꺼운 당면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100040486.1.21",
          "form": "아니요 저는 얇은 당면이 더 좋더라구요",
          "original_form": "아니요 저는 얇은 당면이 더 좋더라구요",
          "speaker_id": "2",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100040486.1.22",
          "form": "저두요!ㅋㅋㅋㅋ",
          "original_form": "저두요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:09"
        }
      ]
