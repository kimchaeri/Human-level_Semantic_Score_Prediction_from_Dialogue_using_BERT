        {
          "id": "MDRW2100016162.1.1",
          "form": "쇼핑좋아하시나요?",
          "original_form": "쇼핑좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016162.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016162.1.3",
          "form": "보통 어떤거 사세요?",
          "original_form": "보통 어떤거 사세요?",
          "speaker_id": "2",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100016162.1.4",
          "form": "신발 옷 이요ㅎㅎ",
          "original_form": "신발 옷 이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100016162.1.5",
          "form": "저도 옷은 많이 사는거 같아요",
          "original_form": "저도 옷은 많이 사는거 같아요",
          "speaker_id": "2",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100016162.1.6",
          "form": "어떤 브랜드사시나요",
          "original_form": "어떤 브랜드사시나요",
          "speaker_id": "1",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100016162.1.7",
          "form": "저는 온라인쇼핑많이하는거같아요 이쁘면 브랜드안따지고 사서 ㅋㅋ",
          "original_form": "저는 온라인쇼핑많이하는거같아요 이쁘면 브랜드안따지고 사서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100016162.1.8",
          "form": "이제 가을옷 사야겠네요ㅎㅎ",
          "original_form": "이제 가을옷 사야겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100016162.1.9",
          "form": "그러게요 근데 가을이 왠지 짧을거같아서 조금만 사야겠어요ㅋㅋ",
          "original_form": "그러게요 근데 가을이 왠지 짧을거같아서 조금만 사야겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:46"
        },
        {
          "id": "MDRW2100016162.1.10",
          "form": "가을옷사셔야죠ㅋㅋ",
          "original_form": "가을옷사셔야죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:46"
        },
        {
          "id": "MDRW2100016162.1.11",
          "form": "아 간절기템 사야죠ㅎㅎ",
          "original_form": "아 간절기템 사야죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:03"
        },
        {
          "id": "MDRW2100016162.1.12",
          "form": "앗 보신거있으세요?",
          "original_form": "앗 보신거있으세요?",
          "speaker_id": "2",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100016162.1.13",
